{"textgrid.poem.38838": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siehst du nicht in T\u00f6nen Funken glimmen?", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du nicht in T\u00f6nen Funken glimmen?", "tokens": ["Siehst", "du", "nicht", "in", "T\u00f6\u00b7nen", "Fun\u00b7ken", "glim\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ja, es sind die s\u00fc\u00dfen Engelstimmen;", "tokens": ["Ja", ",", "es", "sind", "die", "s\u00fc\u00b7\u00dfen", "En\u00b7gel\u00b7stim\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In Form, Gestalt, wohin dein Auge sah,", "tokens": ["In", "Form", ",", "Ge\u00b7stalt", ",", "wo\u00b7hin", "dein", "Au\u00b7ge", "sah", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Farbenglanz ist dir der Ew'ge nah,", "tokens": ["In", "Far\u00b7ben\u00b7glanz", "ist", "dir", "der", "Ew'\u00b7ge", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wie ein R\u00e4thsel steht er vor dir da.", "tokens": ["Doch", "wie", "ein", "R\u00e4th\u00b7sel", "steht", "er", "vor", "dir", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er ist so nah' und wieder weit zur\u00fcck,", "tokens": ["Er", "ist", "so", "nah'", "und", "wie\u00b7der", "weit", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du siehst und f\u00fchlst, dann flieht er deinem Blick,", "tokens": ["Du", "siehst", "und", "f\u00fchlst", ",", "dann", "flieht", "er", "dei\u00b7nem", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem k\u00f6rperschweren Blick kann's nicht gelingen", "tokens": ["Dem", "k\u00f6r\u00b7per\u00b7schwe\u00b7ren", "Blick", "kann's", "nicht", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sich an den Unsichtbaren hinzudringen;", "tokens": ["Sich", "an", "den", "Un\u00b7sicht\u00b7ba\u00b7ren", "hin\u00b7zu\u00b7drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Entfernter noch, um mehr gesucht zu sein,", "tokens": ["Ent\u00b7fern\u00b7ter", "noch", ",", "um", "mehr", "ge\u00b7sucht", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUI", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verbarg er in die T\u00f6ne sich hinein;", "tokens": ["Ver\u00b7barg", "er", "in", "die", "T\u00f6\u00b7ne", "sich", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch freut es ihn, sich freyer dort zu regen,", "tokens": ["Doch", "freut", "es", "ihn", ",", "sich", "frey\u00b7er", "dort", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "PRF", "ADJD", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Liebe heller k\u00f6mmt dir dort entgegen. \u2013", "tokens": ["Die", "Lie\u00b7be", "hel\u00b7ler", "k\u00f6mmt", "dir", "dort", "ent\u00b7ge\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das war ich ehmals, ach! ich f\u00fchl' es tief,", "tokens": ["Das", "war", "ich", "eh\u00b7mals", ",", "ach", "!", "ich", "f\u00fchl'", "es", "tief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$,", "ITJ", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Eh' noch mein Geist in diesem K\u00f6rper schlief. \u2013", "tokens": ["Eh'", "noch", "mein", "Geist", "in", "die\u00b7sem", "K\u00f6r\u00b7per", "schlief", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word