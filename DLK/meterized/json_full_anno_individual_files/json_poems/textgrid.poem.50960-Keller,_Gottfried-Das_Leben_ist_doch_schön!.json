{"textgrid.poem.50960": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Leben ist doch sch\u00f6n!", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n, wie sch\u00f6n ist dieses kurze Leben,", "tokens": ["Wie", "sch\u00f6n", ",", "wie", "sch\u00f6n", "ist", "die\u00b7ses", "kur\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn es er\u00f6ffnet alle seine Quellen!", "tokens": ["Wenn", "es", "er\u00b7\u00f6ff\u00b7net", "al\u00b7le", "sei\u00b7ne", "Quel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Tage gleichen klaren Silberwellen,", "tokens": ["Die", "Ta\u00b7ge", "glei\u00b7chen", "kla\u00b7ren", "Sil\u00b7ber\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sich mit Macht zu \u00fcberholen streben.", "tokens": ["Die", "sich", "mit", "Macht", "zu", "\u00fc\u00b7berh\u00b7o\u00b7len", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was gestern freudig mocht mein Herz erheben,", "tokens": ["Was", "ge\u00b7stern", "freu\u00b7dig", "mocht", "mein", "Herz", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das mu\u00df ich l\u00e4chelnd heute r\u00fcckw\u00e4rts stellen;", "tokens": ["Das", "mu\u00df", "ich", "l\u00e4\u00b7chelnd", "heu\u00b7te", "r\u00fcck\u00b7w\u00e4rts", "stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn die Erfahrungen, sich dr\u00e4ngend, schwellen,", "tokens": ["Wenn", "die", "Er\u00b7fah\u00b7run\u00b7gen", ",", "sich", "dr\u00e4n\u00b7gend", ",", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRF", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Erlebnisse wie Blumen sie umgeben!", "tokens": ["Er\u00b7leb\u00b7nis\u00b7se", "wie", "Blu\u00b7men", "sie", "um\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So mu\u00df ich breiter stets den Strom erschauen,", "tokens": ["So", "mu\u00df", "ich", "brei\u00b7ter", "stets", "den", "Strom", "er\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch tiefer m\u00e4hlich seh den Grund ich winken,", "tokens": ["Auch", "tie\u00b7fer", "m\u00e4h\u00b7lich", "seh", "den", "Grund", "ich", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und t\u00e4glich lern ich mehr der Flut vertrauen.", "tokens": ["Und", "t\u00e4g\u00b7lich", "lern", "ich", "mehr", "der", "Flut", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun goldene Geschirre, sie zu trinken,", "tokens": ["Nun", "gol\u00b7de\u00b7ne", "Ge\u00b7schir\u00b7re", ",", "sie", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebt, G\u00f6tter! mir und Marmor, um zu bauen", "tokens": ["Gebt", ",", "G\u00f6t\u00b7ter", "!", "mir", "und", "Mar\u00b7mor", ",", "um", "zu", "bau\u00b7en"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "NN", "$.", "PPER", "KON", "NN", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den festen Damm zur Rechten wie zur Linken!", "tokens": ["Den", "fes\u00b7ten", "Damm", "zur", "Rech\u00b7ten", "wie", "zur", "Lin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie sch\u00f6n, wie sch\u00f6n ist dieses kurze Leben,", "tokens": ["Wie", "sch\u00f6n", ",", "wie", "sch\u00f6n", "ist", "die\u00b7ses", "kur\u00b7ze", "Le\u00b7ben", ","], "token_info": ["wor