{"dta.poem.4031": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Heimath .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Froh kehrt der Schiffer heim an den stillen Strom,", "tokens": ["Froh", "kehrt", "der", "Schif\u00b7fer", "heim", "an", "den", "stil\u00b7len", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Inseln fernher, wenn er geerntet hat;", "tokens": ["Von", "In\u00b7seln", "fern\u00b7her", ",", "wenn", "er", "ge\u00b7ern\u00b7tet", "hat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So k\u00e4m' auch ich zur Heimath, h\u00e4tt' ich", "tokens": ["So", "k\u00e4m'", "auch", "ich", "zur", "Hei\u00b7math", ",", "h\u00e4tt'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00fcter so viele, wie Leid, geerntet.", "tokens": ["G\u00fc\u00b7ter", "so", "vie\u00b7le", ",", "wie", "Leid", ",", "ge\u00b7ern\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "PIS", "$,", "PWAV", "NN", "$,", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ihr theuern Ufer, die mich erzogen einst,", "tokens": ["Ihr", "theu\u00b7ern", "U\u00b7fer", ",", "die", "mich", "er\u00b7zo\u00b7gen", "einst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stillt ihr der Liebe Leiden, versprecht ihr mir,", "tokens": ["Stillt", "ihr", "der", "Lie\u00b7be", "Lei\u00b7den", ",", "ver\u00b7sprecht", "ihr", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr W\u00e4lder meiner Jugend, wenn ich", "tokens": ["Ihr", "W\u00e4l\u00b7der", "mei\u00b7ner", "Ju\u00b7gend", ",", "wenn", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Komme, die Ruhe noch einmal wieder?", "tokens": ["Kom\u00b7me", ",", "die", "Ru\u00b7he", "noch", "ein\u00b7mal", "wie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Am k\u00fchlen Bache, wo ich der Wellen Spiel,", "tokens": ["Am", "k\u00fch\u00b7len", "Ba\u00b7che", ",", "wo", "ich", "der", "Wel\u00b7len", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Strome, wo ich gleiten die Schiffe sah,", "tokens": ["Am", "Stro\u00b7me", ",", "wo", "ich", "glei\u00b7ten", "die", "Schif\u00b7fe", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dort bin ich bald; euch traute Berge,", "tokens": ["Dort", "bin", "ich", "bald", ";", "euch", "trau\u00b7te", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mich beh\u00fcteten einst, der Heimath", "tokens": ["Die", "mich", "be\u00b7h\u00fc\u00b7te\u00b7ten", "einst", ",", "der", "Hei\u00b7math"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$,", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Verehrte sichre Grenzen, der Mutter Haus;", "tokens": ["Ver\u00b7ehr\u00b7te", "sich\u00b7re", "Gren\u00b7zen", ",", "der", "Mut\u00b7ter", "Haus", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und liebender Geschwister Umarmungen", "tokens": ["Und", "lie\u00b7ben\u00b7der", "Ge\u00b7schwis\u00b7ter", "Um\u00b7ar\u00b7mun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begr\u00fc\u00df' ich bald, und ihr umschlie\u00dft mich,", "tokens": ["Be\u00b7gr\u00fc\u00df'", "ich", "bald", ",", "und", "ihr", "um\u00b7schlie\u00dft", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df, wie in Banden, das Herz mir heile,", "tokens": ["Da\u00df", ",", "wie", "in", "Ban\u00b7den", ",", "das", "Herz", "mir", "hei\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "APPR", "NN", "$,", "ART", "NN", "PPER", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ihr treu geblieb'nen! aber ich wei\u00df, ich wei\u00df", "tokens": ["Ihr", "treu", "ge\u00b7blie\u00b7b'\u00b7nen", "!", "a\u00b7ber", "ich", "wei\u00df", ",", "ich", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "VVINF", "$.", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Liebe Leid, die\u00df heilet so bald mir nicht,", "tokens": ["Der", "Lie\u00b7be", "Leid", ",", "die\u00df", "hei\u00b7let", "so", "bald", "mir", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PDS", "VVFIN", "ADV", "ADV", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die\u00df singt kein Wiegensang, den tr\u00f6stend", "tokens": ["Die\u00df", "singt", "kein", "Wie\u00b7gen\u00b7sang", ",", "den", "tr\u00f6s\u00b7tend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sterbliche singen, mir aus dem Busen.", "tokens": ["Sterb\u00b7li\u00b7che", "sin\u00b7gen", ",", "mir", "aus", "dem", "Bu\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Denn sie, die uns das himmlische Feuer leihn,", "tokens": ["Denn", "sie", ",", "die", "uns", "das", "himm\u00b7li\u00b7sche", "Feu\u00b7er", "leihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die G\u00f6tter schenken heiliges Leid uns auch,", "tokens": ["Die", "G\u00f6t\u00b7ter", "schen\u00b7ken", "hei\u00b7li\u00b7ges", "Leid", "uns", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Drum bleibe die\u00df. Ein Sohn der Erde", "tokens": ["Drum", "blei\u00b7be", "die\u00df", ".", "Ein", "Sohn", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PDS", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich; zu lieben gemacht, zu leiden.", "tokens": ["Bin", "ich", ";", "zu", "lie\u00b7ben", "ge\u00b7macht", ",", "zu", "lei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$.", "PTKZU", "VVINF", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}