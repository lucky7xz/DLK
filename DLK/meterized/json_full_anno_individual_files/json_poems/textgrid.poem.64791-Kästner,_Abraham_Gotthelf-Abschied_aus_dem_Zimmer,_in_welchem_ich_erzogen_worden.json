{"textgrid.poem.64791": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "Abschied aus dem Zimmer, in welchem ich erzogen worden", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So seyst du noch f\u00fcr thr\u00e4nenvolle Blicke,", "tokens": ["So", "seyst", "du", "noch", "f\u00fcr", "thr\u00e4\u00b7nen\u00b7vol\u00b7le", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geliebter Ort, zuletzt mein Gegenstand!", "tokens": ["Ge\u00b7lieb\u00b7ter", "Ort", ",", "zu\u00b7letzt", "mein", "Ge\u00b7gen\u00b7stand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, welch Gef\u00fchl bringst du mir nicht zur\u00fccke!", "tokens": ["Ach", ",", "welch", "Ge\u00b7f\u00fchl", "bringst", "du", "mir", "nicht", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAT", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier lebt' ich erst, eh' ich es noch empfand;", "tokens": ["Hier", "lebt'", "ich", "erst", ",", "eh'", "ich", "es", "noch", "emp\u00b7fand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier hat f\u00fcr mich oft kummervolle N\u00e4chte,", "tokens": ["Hier", "hat", "f\u00fcr", "mich", "oft", "kum\u00b7mer\u00b7vol\u00b7le", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mir unbewu\u00dft, die Mutter durchgewacht;", "tokens": ["Mir", "un\u00b7be\u00b7wu\u00dft", ",", "die", "Mut\u00b7ter", "durch\u00b7ge\u00b7wacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wie das Kind zum Menschen werden m\u00f6chte,", "tokens": ["Und", "wie", "das", "Kind", "zum", "Men\u00b7schen", "wer\u00b7den", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Vaters Treu, des Vetters Huld", "tokens": ["Des", "Va\u00b7ters", "Treu", ",", "des", "Vet\u00b7ters", "Huld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Unschuld Zeit, des Lebens froher Morgen,", "tokens": ["Der", "Un\u00b7schuld", "Zeit", ",", "des", "Le\u00b7bens", "fro\u00b7her", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Verflog mir hier bey B\u00fcchern mehr als Spiel;", "tokens": ["Ver\u00b7flog", "mir", "hier", "bey", "B\u00fc\u00b7chern", "mehr", "als", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und ohne Zwang, noch l\u00e4ngst nicht reif zu sorgen,", "tokens": ["Und", "oh\u00b7ne", "Zwang", ",", "noch", "l\u00e4ngst", "nicht", "reif", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "War ich voll Flei\u00df, weil mir der Flei\u00df gefiel.", "tokens": ["War", "ich", "voll", "Flei\u00df", ",", "weil", "mir", "der", "Flei\u00df", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wenn hier zu ruhn mein Vater sich verweilte,", "tokens": ["Wenn", "hier", "zu", "ruhn", "mein", "Va\u00b7ter", "sich", "ver\u00b7weil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und den ihm gleich mein frommer Dank verehrt,", "tokens": ["Und", "den", "ihm", "gleich", "mein", "from\u00b7mer", "Dank", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Von Arbeit matt, wo ich die Frucht stets theilte,", "tokens": ["Von", "Ar\u00b7beit", "matt", ",", "wo", "ich", "die", "Frucht", "stets", "theil\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Hat Der mein Herz, Der meinen Geist gelehrt.", "tokens": ["Hat", "Der", "mein", "Herz", ",", "Der", "mei\u00b7nen", "Geist", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPOSAT", "NN", "$,", "ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ihr seyd dahin, ihr sch\u00f6nsten meiner Tage,", "tokens": ["Ihr", "seyd", "da\u00b7hin", ",", "ihr", "sch\u00f6ns\u00b7ten", "mei\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ihr seyd dahin, und kommt nicht mehr zur\u00fcck!", "tokens": ["Ihr", "seyd", "da\u00b7hin", ",", "und", "kommt", "nicht", "mehr", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Nur noch ein Lied, das euch zu sp\u00e4t beklage!", "tokens": ["Nur", "noch", "ein", "Lied", ",", "das", "euch", "zu", "sp\u00e4t", "be\u00b7kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Nur noch nach euch ein sehnsuchtsvoller Blick!", "tokens": ["Nur", "noch", "nach", "euch", "ein", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schnell flieh' mein Reim, gleich euch vergn\u00fcgten Jahren,", "tokens": ["Schnell", "flieh'", "mein", "Reim", ",", "gleich", "euch", "ver\u00b7gn\u00fcg\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu einer Zeit, die mir nicht mehr gef\u00e4llt.", "tokens": ["Zu", "ei\u00b7ner", "Zeit", ",", "die", "mir", "nicht", "mehr", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was mir auch einst, ach wann? mu\u00df widerfahren,", "tokens": ["Was", "mir", "auch", "einst", ",", "ach", "wann", "?", "mu\u00df", "wi\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "$,", "XY", "PWAV", "$.", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Trauerplatz, hast du mir dargestellt.", "tokens": ["O", "Trau\u00b7er\u00b7platz", ",", "hast", "du", "mir", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier zeigte mir der K\u00f6nig kalter Schrecken,", "tokens": ["Hier", "zeig\u00b7te", "mir", "der", "K\u00f6\u00b7nig", "kal\u00b7ter", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die strenge Macht, die nur den Leib besiegt,", "tokens": ["Die", "stren\u00b7ge", "Macht", ",", "die", "nur", "den", "Leib", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn Blicken nach, die sich zum Heiland strecken,", "tokens": ["Wenn", "Bli\u00b7cken", "nach", ",", "die", "sich", "zum", "Hei\u00b7land", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKVZ", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der frohe Geist mit Engelschwingen fliegt.", "tokens": ["Der", "fro\u00b7he", "Geist", "mit", "En\u00b7gel\u00b7schwin\u00b7gen", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir ", "tokens": ["Dir"], "token_info": ["word"], "pos": ["PPER"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Den Unterricht zum Leben nicht allein,", "tokens": ["Den", "Un\u00b7ter\u00b7richt", "zum", "Le\u00b7ben", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Lehrer mehr, als Vater mir gegeben,", "tokens": ["Zum", "Leh\u00b7rer", "mehr", ",", "als", "Va\u00b7ter", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War noch dein Amt, zum Sterben das zu seyn", "tokens": ["War", "noch", "dein", "Amt", ",", "zum", "Ster\u00b7ben", "das", "zu", "seyn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "APPRART", "NN", "ART", "PTKZU", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, w\u00e4r von dem, was ich allhier empfunden,", "tokens": ["Ach", ",", "w\u00e4r", "von", "dem", ",", "was", "ich", "all\u00b7hier", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "APPR", "ART", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Traurigste nur eines Vaters Tod!", "tokens": ["Das", "Trau\u00b7rigs\u00b7te", "nur", "ei\u00b7nes", "Va\u00b7ters", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier bluteten der treuen Mutter Wunden;", "tokens": ["Hier", "blu\u00b7te\u00b7ten", "der", "treu\u00b7en", "Mut\u00b7ter", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier f\u00fchlte sie, ach! mehr als Sterbensnoth.", "tokens": ["Hier", "f\u00fchl\u00b7te", "sie", ",", "ach", "!", "mehr", "als", "Ster\u00b7bens\u00b7noth", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ITJ", "$.", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Allwissender! der ihre Thr\u00e4nen z\u00e4hlte,", "tokens": ["All\u00b7wis\u00b7sen\u00b7der", "!", "der", "ih\u00b7re", "Thr\u00e4\u00b7nen", "z\u00e4hl\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die sie mir oft aus Z\u00e4rtlichkeit verbarg,", "tokens": ["Die", "sie", "mir", "oft", "aus", "Z\u00e4rt\u00b7lich\u00b7keit", "ver\u00b7barg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Helden gleich, die Wuth der Heiden qu\u00e4lte,", "tokens": ["Den", "Hel\u00b7den", "gleich", ",", "die", "Wuth", "der", "Hei\u00b7den", "qu\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "War sie durch dich bey gr\u00f6\u00dfern Leiden stark.", "tokens": ["War", "sie", "durch", "dich", "bey", "gr\u00f6\u00b7\u00dfern", "Lei\u00b7den", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "F\u00fcnf Jahre lang, bis alle Kraft versiegte,", "tokens": ["F\u00fcnf", "Jah\u00b7re", "lang", ",", "bis", "al\u00b7le", "Kraft", "ver\u00b7sieg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Lag deine Hand, Herr, Tag und Nacht auf ihr:", "tokens": ["Lag", "dei\u00b7ne", "Hand", ",", "Herr", ",", "Tag", "und", "Nacht", "auf", "ihr", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie hat gesiegt, hier, wo mein Vater siegte,", "tokens": ["Sie", "hat", "ge\u00b7siegt", ",", "hier", ",", "wo", "mein", "Va\u00b7ter", "sieg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ihr bessrer Theil lie\u00df Leib und Elend hier", "tokens": ["Ihr", "bess\u00b7rer", "Theil", "lie\u00df", "Leib", "und", "E\u00b7lend", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gott! deine Huld zeigt sich auch da am gr\u00f6\u00dften,", "tokens": ["Gott", "!", "dei\u00b7ne", "Huld", "zeigt", "sich", "auch", "da", "am", "gr\u00f6\u00df\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "PRF", "ADV", "ADV", "APPRART", "ADJA", "$,"], "meter": "++-+---+-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Wo Qual und Noth die Sterblichen beschwert:", "t