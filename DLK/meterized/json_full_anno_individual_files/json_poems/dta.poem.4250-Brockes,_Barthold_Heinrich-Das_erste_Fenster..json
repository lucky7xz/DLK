{"dta.poem.4250": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das erste Fenster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der ersten Tafel, linker Hand, bestrahlter Vorgrund ist", "tokens": ["Der", "ers\u00b7ten", "Ta\u00b7fel", ",", "lin\u00b7ker", "Hand", ",", "be\u00b7strahl\u00b7ter", "Vor\u00b7grund", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "der Wall,", "tokens": ["der", "Wall", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der mit dem Werkzeug der Bellonen, mit St\u00fccken, die zu", "tokens": ["Der", "mit", "dem", "Werk\u00b7zeug", "der", "Bel\u00b7lo\u00b7nen", ",", "mit", "St\u00fc\u00b7cken", ",", "die", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "NN", "$,", "PRELS", "APPR"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Freuden-Sch\u00fcssen,", "tokens": ["Freu\u00b7den\u00b7Sch\u00fcs\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und fremde Schiffe, welche gr\u00fcssen, hinwieder knallend", "tokens": ["Und", "frem\u00b7de", "Schif\u00b7fe", ",", "wel\u00b7che", "gr\u00fcs\u00b7sen", ",", "hin\u00b7wie\u00b7der", "knal\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "zu begr\u00fcssen,", "tokens": ["zu", "be\u00b7gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Gott Lob! bisher gebrauchet worden, wohl ausstaffirt, und", "tokens": ["Gott", "Lob", "!", "bis\u00b7her", "ge\u00b7brau\u00b7chet", "wor\u00b7den", ",", "wohl", "aus\u00b7staf\u00b7firt", ",", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "NN", "$.", "ADV", "VVPP", "VAPP", "$,", "ADV", "VVPP", "$,", "KON"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.8": {"text": "\u00fcberall", "tokens": ["\u00fc\u00b7be\u00b7rall"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Das Amt-Haus rings umher umgiebt. Ein zierlich rohtes", "tokens": ["Das", "Am\u00b7tHaus", "rings", "um\u00b7her", "um\u00b7giebt", ".", "Ein", "zier\u00b7lich", "roh\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKVZ", "VVFIN", "$.", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schilder-Haus", "tokens": ["Schil\u00b7der\u00b7Haus"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Steht mitten zwischen zwo Canonen. Die Brustwehr", "tokens": ["Steht", "mit\u00b7ten", "zwi\u00b7schen", "zwo", "Ca\u00b7no\u00b7nen", ".", "Die", "Brust\u00b7wehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "macht die Grenzen aus,", "tokens": ["macht", "die", "Gren\u00b7zen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Und l\u00e4sset uns, wo sie sich endet, der sogenannten K\u00e4lber-", "tokens": ["Und", "l\u00e4s\u00b7set", "uns", ",", "wo", "sie", "sich", "en\u00b7det", ",", "der", "so\u00b7ge\u00b7nann\u00b7ten", "K\u00e4l\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Weide,", "tokens": ["Wei\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Nachdem dieselbe aufgebrochen, das lieblich wallende Ge-", "tokens": ["Nach\u00b7dem", "die\u00b7sel\u00b7be", "auf\u00b7ge\u00b7bro\u00b7chen", ",", "das", "lieb\u00b7lich", "wal\u00b7len\u00b7de", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "VVPP", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "trayde", "tokens": ["tray\u00b7de"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "In einem bald smaragdnem Schmuck, und bald im g\u00fcld-", "tokens": ["In", "ei\u00b7nem", "bald", "sma\u00b7ragd\u00b7nem", "Schmuck", ",", "und", "bald", "im", "g\u00fcld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,", "KON", "ADV", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "nen Glanze, seh'n.", "tokens": ["nen", "Glan\u00b7ze", ",", "seh'", "n."], "token_info": ["word", "word", "punct", "word", "abbreviation"], "pos": ["ADJA", "NN", "$,", "VVFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Die Grenze dieser fetten Wiese ist die Krystallen- gleiche", "tokens": ["Die", "Gren\u00b7ze", "die\u00b7ser", "fet\u00b7ten", "Wie\u00b7se", "ist", "die", "Kry\u00b7stal\u00b7len", "glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "VAFIN", "ART", "TRUNC", "ADJA"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Fluht", "tokens": ["Fluht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Des \u00e4ussern tiefen Wasser-Grabens, wovon man hier ein", "tokens": ["Des", "\u00e4us\u00b7sern", "tie\u00b7fen", "Was\u00b7ser\u00b7Gra\u00b7bens", ",", "wo\u00b7von", "man", "hier", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PWAV", "PIS", "ADV", "ART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "St\u00fcck erblicket,", "tokens": ["St\u00fcck", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Das sich, von dem ", "tokens": ["Das", "sich", ",", "von", "dem"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "PRF", "$,", "APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Schatten schm\u00fccket,", "tokens": ["Schat\u00b7ten", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Der, nebst zwey hoher B\u00e4ume Schatten, auf seiner glatten", "tokens": ["Der", ",", "nebst", "zwey", "ho\u00b7her", "B\u00e4u\u00b7me", "Schat\u00b7ten", ",", "auf", "sei\u00b7ner", "glat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "CARD", "ADJA", "NN", "NN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Fl\u00e4che ruht,", "tokens": ["Fl\u00e4\u00b7che", "ruht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Bey welchen B\u00e4umen, linker Hand, noch ein Paar feiner", "tokens": ["Bey", "wel\u00b7chen", "B\u00e4u\u00b7men", ",", "lin\u00b7ker", "Hand", ",", "noch", "ein", "Paar", "fei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "ADJA", "NN", "$,", "ADV", "ART", "NN", "NN"], "meter": "+--+-+-+--++-", "measure": "iambic.hexa.invert"}, "line.28": {"text": "H\u00e4user stehet,", "tokens": ["H\u00e4u\u00b7ser", "ste\u00b7het", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Wozwischen der gebogne Weg zun S\u00fcd- und Wester-Wiesen", "tokens": ["Woz\u00b7wi\u00b7schen", "der", "ge\u00b7bog\u00b7ne", "Weg", "zun", "S\u00fcd", "und", "Wes\u00b7ter\u00b7Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "gehet,", "tokens": ["ge\u00b7het", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Und sich an diesem Orte theilt. Nach diesen H\u00e4usern siehet", "tokens": ["Und", "sich", "an", "die\u00b7sem", "Or\u00b7te", "theilt", ".", "Nach", "die\u00b7sen", "H\u00e4u\u00b7sern", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$.", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.32": {"text": "man", "tokens": ["man"], "token_info": ["word"], "pos": ["PIS"], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Ein fast nicht abzusehend Feld, von Aeckern eine solche Menge,", "tokens": ["Ein", "fast", "nicht", "ab\u00b7zu\u00b7se\u00b7hend", "Feld", ",", "von", "A\u00b7e\u00b7ckern", "ei\u00b7ne", "sol\u00b7che", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJD", "NN", "$,", "APPR", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.34": {"text": "Die, zwischen gr\u00fcn- bebl\u00fchmten Wiesen, in viele Strich\u2019 und", "tokens": ["Die", ",", "zwi\u00b7schen", "gr\u00fcn", "be\u00b7bl\u00fchm\u00b7ten", "Wie\u00b7sen", ",", "in", "vie\u00b7le", "Strich'", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "TRUNC", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "solcher L\u00e4nge,", "tokens": ["sol\u00b7cher", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.36": {"text": "Zumahl zur Sommers-Zeit, sich strecken, da\u00df, wenn gem\u00e4ht,", "tokens": ["Zu\u00b7mahl", "zur", "Som\u00b7mer\u00b7sZeit", ",", "sich", "stre\u00b7cken", ",", "da\u00df", ",", "wenn", "ge\u00b7m\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "PRF", "VVFIN", "$,", "KOUS", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "an vielen Orten,", "tokens": ["an", "vie\u00b7len", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "Ein unabsichtbar gr\u00fcn Gewand, besetzt mit breiten g\u00fcldnen", "tokens": ["Ein", "un\u00b7ab\u00b7sicht\u00b7bar", "gr\u00fcn", "Ge\u00b7wand", ",", "be\u00b7setzt", "mit", "brei\u00b7ten", "g\u00fcld\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.39": {"text": "Borten,", "tokens": ["Bor\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Sie unsern frohen Augen zeigen. Nachhero findet unser", "tokens": ["Sie", "un\u00b7sern", "fro\u00b7hen", "Au\u00b7gen", "zei\u00b7gen", ".", "Nach\u00b7he\u00b7ro", "fin\u00b7det", "un\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$.", "ADV", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.41": {"text": "Blick", "tokens": ["Blick"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Die Dorfschaft ", "tokens": ["Die", "Dorf\u00b7schaft"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.43": {"text": "D\u00f6s' ein St\u00fcck.", "tokens": ["D\u00f6s'", "ein", "St\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.44": {"text": "Zur linken sieht man ", "tokens": ["Zur", "lin\u00b7ken", "sieht", "man"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "mit Vergn\u00fcgen,", "tokens": ["mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.46": {"text": "Den hohen Thurm zum Neuen-Werk, recht mitten in der", "tokens": ["Den", "ho\u00b7hen", "Thurm", "zum", "Neu\u00b7en\u00b7Werk", ",", "recht", "mit\u00b7ten", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,", "ADV", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Elbe, liegen,", "tokens": ["El\u00b7be", ",", "lie\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.48": {"text": "Nebst seiner Bl\u00fcsen, die im Winter, mit der stets unter-", "tokens": ["Nebst", "sei\u00b7ner", "Bl\u00fc\u00b7sen", ",", "die", "im", "Win\u00b7ter", ",", "mit", "der", "stets", "un\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "$,", "APPR", "ART", "ADV", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.49": {"text": "haltnen Gluht,", "tokens": ["halt\u00b7nen", "Gluht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.50": {"text": "Den Schiffenden zum Pharus dient, und grosse Dienst\u2019 dem", "tokens": ["Den", "Schif\u00b7fen\u00b7den", "zum", "Pha\u00b7rus", "dient", ",", "und", "gros\u00b7se", "Dienst'", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Kaufmann thut.", "tokens": ["Kauf\u00b7mann", "thut", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.52": {"text": "Die\u00df ist der Augen Horizont, indem wir da den Luft-Kreis", "tokens": ["Die\u00df", "ist", "der", "Au\u00b7gen", "Ho\u00b7ri\u00b7zont", ",", "in\u00b7dem", "wir", "da", "den", "Luft\u00b7Kreis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-++", "measure": "unknown.measure.octa.plus"}, "line.53": {"text": "sehen", "tokens": ["se\u00b7hen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.54": {"text": "Sich gleichsam mit der Fluht verbinden. La\u00dft uns nunmehr", "tokens": ["Sich", "gleich\u00b7sam", "mit", "der", "Fluht", "ver\u00b7bin\u00b7den", ".", "La\u00dft", "uns", "nun\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$.", "VVIMP", "PPER", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.55": {"text": "denn weiter gehen,", "tokens": ["denn", "wei\u00b7ter", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.56": {"text": "Und schauen, was im andern Fenster, als in der andern", "tokens": ["Und", "schau\u00b7en", ",", "was", "im", "an\u00b7dern", "Fens\u00b7ter", ",", "als", "in", "der", "an\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "w