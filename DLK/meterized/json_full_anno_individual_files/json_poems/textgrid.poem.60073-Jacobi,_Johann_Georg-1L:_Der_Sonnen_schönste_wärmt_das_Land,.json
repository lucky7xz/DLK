"token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich unser Herz erfindet,", "tokens": ["Sich", "un\u00b7ser", "Herz", "er\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Liebe dort mit eigner Hand", "tokens": ["Wenn", "Lie\u00b7be", "dort", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns an ein M\u00e4dchen bindet:", "tokens": ["Uns", "an", "ein", "M\u00e4d\u00b7chen", "bin\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auf einmahl sehen wir, geweckt", "tokens": ["Auf", "ein\u00b7mahl", "se\u00b7hen", "wir", ",", "ge\u00b7weckt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus unsern Kindertr\u00e4umen,", "tokens": ["Aus", "un\u00b7sern", "Kin\u00b7der\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Baum, der Liebchens H\u00fctte deckt,", "tokens": ["Den", "Baum", ",", "der", "Lieb\u00b7chens", "H\u00fct\u00b7te", "deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allen andern B\u00e4umen.", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du kleines, v\u00e4terliches Land,", "tokens": ["Du", "klei\u00b7nes", ",", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo mir der Tag geschienen,", "tokens": ["Wo", "mir", "der", "Tag", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als mich die erste Muse fand", "tokens": ["Als", "mich", "die", "ers\u00b7te", "Mu\u00b7se", "fand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Weidenbach, im Gr\u00fcnen!", "tokens": ["Am", "Wei\u00b7den\u00b7bach", ",", "im", "Gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du gutes Land, wo Flur und Hain", "tokens": ["Du", "gu\u00b7tes", "Land", ",", "wo", "Flur", "und", "Hain"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sichrer Einfalt bl\u00fchen,", "tokens": ["In", "sich\u00b7rer", "Ein\u00b7falt", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo rings sich um den deutschen Rhein", "tokens": ["Wo", "rings", "sich", "um", "den", "deut\u00b7schen", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PRF", "APPR", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Trauben-H\u00fcgel ziehen!", "tokens": ["Die", "Trau\u00b7ben\u00b7H\u00fc\u00b7gel", "zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie liebt' ich, o, wie liebt' ich dich,", "tokens": ["Wie", "liebt'", "ich", ",", "o", ",", "wie", "liebt'", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "FM", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weinte dir entgegen!", "tokens": ["Und", "wein\u00b7te", "dir", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sehnt' ich, o, wie sehnt' ich mich", "tokens": ["Wie", "sehnt'", "ich", ",", "o", ",", "wie", "sehnt'", "ich", "mich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "FM", "$,", "PWAV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit lauten Herzens-Schl\u00e4gen!", "tokens": ["Mit", "lau\u00b7ten", "Her\u00b7zens\u00b7Schl\u00e4\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nun aber Lieb' im Busen wallt,", "tokens": ["Nun", "a\u00b7ber", "Lieb'", "im", "Bu\u00b7sen", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun geb' ich deine Freuden", "tokens": ["Nun", "geb'", "ich", "dei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um einen \u00f6den Tannen-Wald,", "tokens": ["Um", "ei\u00b7nen", "\u00f6\u00b7den", "Tan\u00b7nen\u00b7Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ungeschm\u00fcckten Heiden;", "tokens": ["Auf", "un\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Hei\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Weil auf der Heide Liebchen wohnt,", "tokens": ["Weil", "auf", "der", "Hei\u00b7de", "Lieb\u00b7chen", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umweht von Tannen-Hainen,", "tokens": ["Um\u00b7weht", "von", "Tan\u00b7nen\u00b7Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und freudenvoller Sonn' und Mond", "tokens": ["Und", "freu\u00b7den\u00b7vol\u00b7ler", "Sonn'", "und", "Mond"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wipfel da bescheinen