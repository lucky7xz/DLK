{"dta.poem.7714": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stets klarer wird es mir, und endlich wird es klar,", "tokens": ["Stets", "kla\u00b7rer", "wird", "es", "mir", ",", "und", "end\u00b7lich", "wird", "es", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PPER", "$,", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich nichts andres ward, als was ich anfangs war.", "tokens": ["Da\u00df", "ich", "nichts", "and\u00b7res", "ward", ",", "als", "was", "ich", "an\u00b7fangs", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PIS", "VAFIN", "$,", "KOUS", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Pflanzenkeim, der erst sich in zwei L\u00e4ppchen spaltet,", "tokens": ["Ein", "Pflan\u00b7zen\u00b7keim", ",", "der", "erst", "sich", "in", "zwei", "L\u00e4pp\u00b7chen", "spal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PRF", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann Stengel wird und Blatt, und sich als Blum' entfaltet.", "tokens": ["Dann", "Sten\u00b7gel", "wird", "und", "Blatt", ",", "und", "sich", "als", "Blum'", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "KON", "NN", "$,", "KON", "PRF", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Blume, die mit Licht schaut in sich selbst hinein,", "tokens": ["Die", "Blu\u00b7me", ",", "die", "mit", "Licht", "schaut", "in", "sich", "selbst", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "APPR", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erkennt die Pflanz' in sich, das wird ihr Saame seyn.", "tokens": ["Er\u00b7kennt", "die", "Pflanz'", "in", "sich", ",", "das", "wird", "ihr", "Saa\u00b7me", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PRF", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}