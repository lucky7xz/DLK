{"dta.poem.15912": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "117.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe kaum, und nun mu\u00df ich mich drum verklagen,", "tokens": ["Ich", "ha\u00b7be", "kaum", ",", "und", "nun", "mu\u00df", "ich", "mich", "drum", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KON", "ADV", "VMFIN", "PPER", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Rosen angesehn in diesen Sommertagen.", "tokens": ["Die", "Ro\u00b7sen", "an\u00b7ge\u00b7sehn", "in", "die\u00b7sen", "Som\u00b7mer\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was mir im Sinne lag, da\u00df dieses mir geschah?", "tokens": ["Was", "mir", "im", "Sin\u00b7ne", "lag", ",", "da\u00df", "die\u00b7ses", "mir", "ge\u00b7schah", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PDAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sch\u00f6n mu\u00dft' es seyn, weil ich davor nicht Rosen sah.", "tokens": ["Sch\u00f6n", "mu\u00dft'", "es", "seyn", ",", "weil", "ich", "da\u00b7vor", "nicht", "Ro\u00b7sen", "sah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$,", "KOUS", "PPER", "PAV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun sind die Rosen mit den Sommertagen hin,", "tokens": ["Nun", "sind", "die", "Ro\u00b7sen", "mit", "den", "Som\u00b7mer\u00b7ta\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht geblieben ist auch was mir lag im Sinn.", "tokens": ["Und", "nicht", "ge\u00b7blie\u00b7ben", "ist", "auch", "was", "mir", "lag", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "VAFIN", "ADV", "PWS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}