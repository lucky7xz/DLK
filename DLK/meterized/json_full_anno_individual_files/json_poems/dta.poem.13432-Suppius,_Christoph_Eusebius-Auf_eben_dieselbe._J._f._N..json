{"dta.poem.13432": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Auf eben dieselbe.  \n J. f. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Wo noch auf steilen Klippen, Um Naumburg.", "tokens": ["Wo", "noch", "auf", "stei\u00b7len", "Klip\u00b7pen", ",", "Um", "Naum\u00b7burg", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN", "$,", "KOUI", "NE", "$."], "meter": "-----+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Von frischem Most erhitzt,", "tokens": ["Von", "fri\u00b7schem", "Most", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit rothgef\u00e4rbten Lippen", "tokens": ["Mit", "roth\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Reben Vater sitzt;", "tokens": ["Der", "Re\u00b7ben", "Va\u00b7ter", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo er auf manchen H\u00f6hen,", "tokens": ["Wo", "er", "auf", "man\u00b7chen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mit h\u00f6nisch bitterm Schm\u00e4hen,", "tokens": ["Mit", "h\u00f6\u00b7nisch", "bit\u00b7term", "Schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Wassertrinker lacht,", "tokens": ["Der", "Was\u00b7ser\u00b7trin\u00b7ker", "lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die unter seinen F\u00fcssen", "tokens": ["Die", "un\u00b7ter", "sei\u00b7nen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Saale schmeicheln m\u00fcssen,", "tokens": ["Der", "Saa\u00b7le", "schmei\u00b7cheln", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Daher wird dieses Blatt gebracht.", "tokens": ["Da\u00b7her", "wird", "die\u00b7ses", "Blatt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von einem Ost getrieben", "tokens": ["Von", "ei\u00b7nem", "Ost", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kommt es in diese Fluhr,", "tokens": ["Kommt", "es", "in", "die\u00b7se", "Fluhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo sich zwey Seelen lieben,", "tokens": ["Wo", "sich", "zwey", "See\u00b7len", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und uns heisst die Natur,", "tokens": ["Und", "uns", "heisst", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da sie sich heut verbinden,", "tokens": ["Da", "sie", "sich", "heut", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Opfer anzuz\u00fcnden", "tokens": ["Ein", "Op\u00b7fer", "an\u00b7zu\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVIZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf diesem Feuerherd;", "tokens": ["Auf", "die\u00b7sem", "Feu\u00b7er\u00b7herd", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gott, Wahrheit, Wein und Tugend,", "tokens": ["Gott", ",", "Wahr\u00b7heit", ",", "Wein", "und", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die ewiggr\u00fcne Jugend", "tokens": ["Die", "e\u00b7wig\u00b7gr\u00fc\u00b7ne", "Ju\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der Freundschaft hei\u00dft uns liebenswerth.", "tokens": ["Der", "Freund\u00b7schaft", "hei\u00dft", "uns", "lie\u00b7bens\u00b7werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir stimmen unsre Leyer,", "tokens": ["Wir", "stim\u00b7men", "uns\u00b7re", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und singen dieses Lied", "tokens": ["Und", "sin\u00b7gen", "die\u00b7ses", "Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey dieser Hochzeitfeyer", "tokens": ["Bey", "die\u00b7ser", "Hoch\u00b7zeit\u00b7fe\u00b7yer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mit fr\u00f6lichem Gem\u00fcth;", "tokens": ["Mit", "fr\u00f6\u00b7li\u00b7chem", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du Gott der Dichtkunst, f\u00fchre", "tokens": ["Du", "Gott", "der", "Dicht\u00b7kunst", ",", "f\u00fch\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die\u00df Werk, komm bald und r\u00fchre", "tokens": ["Die\u00df", "Werk", ",", "komm", "bald", "und", "r\u00fch\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du selbst die Saiten wohl!", "tokens": ["Du", "selbst", "die", "Sai\u00b7ten", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Inhalt vom Gesange", "tokens": ["Der", "In\u00b7halt", "vom", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sey auch nach Laut und Klange", "tokens": ["Sey", "auch", "nach", "Laut", "und", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "APPR", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Entz\u00fcckend, rein, begeistrungsvoll!", "tokens": ["Ent\u00b7z\u00fc\u00b7ckend", ",", "rein", ",", "be\u00b7geis\u00b7trungs\u00b7voll", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie? sehn wir nicht die Liebe,", "tokens": ["Wie", "?", "sehn", "wir", "nicht", "die", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sie vom Himmel steigt,", "tokens": ["Wie", "sie", "vom", "Him\u00b7mel", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit auserw\u00e4hltem Triebe,", "tokens": ["Mit", "au\u00b7ser\u00b7w\u00e4hl\u00b7tem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihren Zepter neigt?", "tokens": ["Und", "ih\u00b7ren", "Zep\u00b7ter", "neigt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie kommt! mit Lust und Scherzen,", "tokens": ["Sie", "kommt", "!", "mit", "Lust", "und", "Scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die K\u00f6nigin der Herzen,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Mutter aller Welt,", "tokens": ["Die", "Mut\u00b7ter", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "O komm! die man gern schauet,", "tokens": ["O", "komm", "!", "die", "man", "gern", "schau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "ART", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Hier ist f\u00fcr dich gebauet,", "tokens": ["Hier", "ist", "f\u00fcr", "dich", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In Herzen, wo es dir gef\u00e4llt.", "tokens": ["In", "Her\u00b7zen", ",", "wo", "es", "dir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Besteige deine Thronen", "tokens": ["Be\u00b7stei\u00b7ge", "dei\u00b7ne", "Thro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit unumschr\u00e4nkter Macht,", "tokens": ["Mit", "un\u00b7um\u00b7schr\u00e4nk\u00b7ter", "Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey denen zwey Personen,", "tokens": ["Bey", "de\u00b7nen", "zwey", "Per\u00b7so\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "CARD", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die in dein Reich gebracht,", "tokens": ["Die", "in", "dein", "Reich", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bey den verbundnen Beyden,", "tokens": ["Bey", "den", "ver\u00b7bund\u00b7nen", "Bey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die keine Welt nun scheiden,", "tokens": ["Die", "kei\u00b7ne", "Welt", "nun", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kein Unfall trennen mag;", "tokens": ["Kein", "Un\u00b7fall", "tren\u00b7nen", "mag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So ist ihr k\u00fcnftig Leben", "tokens": ["So", "ist", "ihr", "k\u00fcnf\u00b7tig", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Von deinem Schutz umgeben", "tokens": ["Von", "dei\u00b7nem", "Schutz", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein immer neuer Hochzeittag.", "tokens": ["Ein", "im\u00b7mer", "neu\u00b7er", "Hoch\u00b7zeit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gesellschaft kann nicht fehlen,", "tokens": ["Ge\u00b7sell\u00b7schaft", "kann", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die deiner Hoheit werth,", "tokens": ["Die", "dei\u00b7ner", "Ho\u00b7heit", "werth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dieweil in ihren Seelen", "tokens": ["Die\u00b7weil", "in", "ih\u00b7ren", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel Tugend eingekehrt;", "tokens": ["Viel", "Tu\u00b7gend", "ein\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was manche Dichter preisen,", "tokens": ["Was", "man\u00b7che", "Dich\u00b7ter", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und uns als Muster weisen,", "tokens": ["Und", "uns", "als", "Mus\u00b7ter", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo es daran nicht fehlt,", "tokens": ["Wo", "es", "da\u00b7ran", "nicht", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PAV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ist kaum soviel zu nennen,", "tokens": ["Ist", "kaum", "so\u00b7viel", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Als du da wirst erkennen,", "tokens": ["Als", "du", "da", "wirst", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df hier bescheiden wird verhehlt.", "tokens": ["Da\u00df", "hier", "be\u00b7schei\u00b7den", "wird", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O! sch\u00fctte deine Segen", "tokens": ["O", "!", "sch\u00fct\u00b7te", "dei\u00b7ne", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch \u00fcber Jhnen aus!", "tokens": ["Doch", "\u00fc\u00b7ber", "Jh\u00b7nen", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sey sichtbarlich zugegen,", "tokens": ["Sey", "sicht\u00b7bar\u00b7lich", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und la\u00df um Beustens Haus,", "tokens": ["Und", "la\u00df", "um", "Beus\u00b7tens", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In seiner Seebach Leben,", "tokens": ["In", "sei\u00b7ner", "See\u00b7bach", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Weinstock voller Reben,", "tokens": ["Den", "Wein\u00b7stock", "vol\u00b7ler", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu unsrer Wonne sehn;", "tokens": ["Zu", "uns\u00b7rer", "Won\u00b7ne", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie wollen wir uns freuen!", "tokens": ["Wie", "wol\u00b7len", "wir", "uns", "freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und jedes Jahr von neuen", "tokens": ["Und", "je\u00b7des", "Jahr", "von", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Den Tag mit Herzenslust begehn.", "tokens": ["Den", "Tag", "mit", "Her\u00b7zens\u00b7lust", "be\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auch z\u00fcnde ihre Herzen", "tokens": ["Auch", "z\u00fcn\u00b7de", "ih\u00b7re", "Her\u00b7zen"], "toke