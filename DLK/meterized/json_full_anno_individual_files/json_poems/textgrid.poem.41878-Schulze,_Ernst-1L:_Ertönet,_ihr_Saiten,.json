{"textgrid.poem.41878": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ert\u00f6net, ihr Saiten,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ert\u00f6net, ihr Saiten,", "tokens": ["Er\u00b7t\u00f6\u00b7net", ",", "ihr", "Sai\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In n\u00e4chtlicher Ruh'", "tokens": ["In", "n\u00e4cht\u00b7li\u00b7cher", "Ruh'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und f\u00fchret von weiten", "tokens": ["Und", "f\u00fch\u00b7ret", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Tr\u00e4ume mir zu!", "tokens": ["Die", "Tr\u00e4u\u00b7me", "mir", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Schon h\u00f6r' ich sie schallen", "tokens": ["Schon", "h\u00f6r'", "ich", "sie", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im schwellenden Klang;", "tokens": ["Im", "schwel\u00b7len\u00b7den", "Klang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sie f\u00fcllen die Hallen", "tokens": ["Sie", "f\u00fcl\u00b7len", "die", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit Liebesgesang", "tokens": ["Mit", "Lie\u00b7bes\u00b7ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und wiegen und tragen", "tokens": ["Und", "wie\u00b7gen", "und", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Den sinkenden Muth", "tokens": ["Den", "sin\u00b7ken\u00b7den", "Muth"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Durch st\u00fcrmisches Zagen", "tokens": ["Durch", "st\u00fcr\u00b7mi\u00b7sches", "Za\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Auf t\u00f6nender Fluth.", "tokens": ["Auf", "t\u00f6\u00b7nen\u00b7der", "Fluth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die nimmer erklangen", "tokens": ["Die", "nim\u00b7mer", "er\u00b7klan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "F\u00fcr F\u00fcrsten und Gold,", "tokens": ["F\u00fcr", "F\u00fcrs\u00b7ten", "und", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Jetzt sind sie gefangen", "tokens": ["Jetzt", "sind", "sie", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Um bitteren Sold", "tokens": ["Um", "bit\u00b7te\u00b7ren", "Sold"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und geben mit Freuden", "tokens": ["Und", "ge\u00b7ben", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Um kargen Gewinn", "tokens": ["Um", "kar\u00b7gen", "Ge\u00b7winn"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und reichliche Leiden", "tokens": ["Und", "reich\u00b7li\u00b7che", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ihr K\u00f6stlichstes hin.", "tokens": ["Ihr", "K\u00f6st\u00b7lichs\u00b7tes", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Doch trifft auch die Lieder", "tokens": ["Doch", "trifft", "auch", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Manch finsterer Blick,", "tokens": ["Manch", "fins\u00b7te\u00b7rer", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Stets kehren sie wieder", "tokens": ["Stets", "keh\u00b7ren", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Zur Herrin zur\u00fcck.", "tokens": ["Zur", "Her\u00b7rin", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "O k\u00f6nnt' ich's ersingen,", "tokens": ["O", "k\u00f6nnt'", "ich's", "er\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "VVINF", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Das goldene Ziel!", "tokens": ["Das", "gol\u00b7de\u00b7ne", "Ziel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O k\u00f6nnt' ich's erringen", "tokens": ["O", "k\u00f6nnt'", "ich's", "er\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PIS", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im Schlachtengew\u00fchl!", "tokens": ["Im", "Schlach\u00b7ten\u00b7ge\u00b7w\u00fchl", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Vergebens begegnen", "tokens": ["Ver\u00b7ge\u00b7bens", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sich Leyer und Schwert;", "tokens": ["Sich", "Le\u00b7yer", "und", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sie h\u00e4lt den Verwegnen,", "tokens": ["Sie", "h\u00e4lt", "den", "Ver\u00b7weg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Den Milden nicht werth.", "tokens": ["Den", "Mil\u00b7den", "nicht", "werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und g\u00e4b' ich f\u00fcr Liebe", "tokens": ["Und", "g\u00e4b'", "ich", "f\u00fcr", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Das Leben auch gern,", "tokens": ["Das", "Le\u00b7ben", "auch", "gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Stets bleibt er mir tr\u00fcbe,", "tokens": ["Stets", "bleibt", "er", "mir", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Der freundliche Stern.", "tokens": ["Der", "freund\u00b7li\u00b7che", "Stern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Gewagt und gewonnen!", "tokens": ["Ge\u00b7wagt", "und", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schrieb Mancher auf's Schwert;", "tokens": ["Schrieb", "Man\u00b7cher", "auf's", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gewagt und zerronnen", "tokens": ["Ge\u00b7wagt", "und", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ist mir nur bescheert.", "tokens": ["Ist", "mir", "nur", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Doch la\u00df' ich es wallen,", "tokens": ["Doch", "la\u00df'", "ich", "es", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das edle Panier,", "tokens": ["Das", "ed\u00b7le", "Pa\u00b7nier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und soll es auch fallen,", "tokens": ["Und", "soll", "es", "auch", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "So fall' es mit mir!", "tokens": ["So", "fall'", "es", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Denn w\u00fcrdig der Beute", "tokens": ["Denn", "w\u00fcr\u00b7dig", "der", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ist nimmer der Mann,", "tokens": ["Ist", "nim\u00b7mer", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Der fliehend im Streite", "tokens": ["Der", "flie\u00b7hend", "im", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Sein Leben gewann.", "tokens": ["Sein", "Le\u00b7ben", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Mag schnell sich in Gluthen", "tokens": ["Mag", "schnell", "sich", "in", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PRF", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verzehren das Herz,", "tokens": ["Ver\u00b7zeh\u00b7ren", "das", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und mag es verbluten", "tokens": ["Und", "mag", "es", "ver\u00b7blu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im zaudernden Schmerz;", "tokens": ["Im", "zau\u00b7dern\u00b7den", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich n\u00e4hre die Wunde,", "tokens": ["Ich", "n\u00e4h\u00b7re", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ich liebe mein Leid", "tokens": ["Ich", "lie\u00b7be", "mein", "Leid"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und lasse die Kunde", "tokens": ["Und", "las\u00b7se", "die", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der kommenden Zeit:", "tokens": ["Der", "kom\u00b7men\u00b7den", "Zeit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Die immer auf's neue", "tokens": ["Die", "im\u00b7mer", "auf's", "neu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Das Herz ihm betr\u00fcbt,", "tokens": ["Das", "Herz", "ihm", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Die hat der Getreue", "tokens": ["Die", "hat", "der", "Ge\u00b7treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Noch sterbend geliebt.", "tokens": ["Noch", "ster\u00b7bend", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ert\u00f6net, ihr Saiten,", "tokens": ["Er\u00b7t\u00f6\u00b7net", ",", "ihr", "Sai\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In n\u00e4chtlicher Ruh'", "tokens": ["In", "n\u00e4cht\u00b7li\u00b7cher", "Ruh'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und f\u00fchret von weiten", "tokens": ["Und", "f\u00fch\u00b7ret", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA"], "meter": 