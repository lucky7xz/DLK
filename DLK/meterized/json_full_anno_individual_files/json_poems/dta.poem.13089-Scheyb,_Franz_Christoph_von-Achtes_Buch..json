{"dta.poem.13089": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.57", "en:0.14", "cy:0.14", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egeduld! was redest du? was soll der Einspruch seyn?", "tokens": ["\u201e", "ge\u00b7duld", "!", "was", "re\u00b7dest", "du", "?", "was", "soll", "der", "Ein\u00b7spruch", "seyn", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "PWS", "VVFIN", "PPER", "$.", "PWS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "215\u201dMit dem du mein Gespr\u00e4ch noch unbefragt verwirrest?", "tokens": ["\"", "Mit", "dem", "du", "mein", "Ge\u00b7spr\u00e4ch", "noch", "un\u00b7be\u00b7fragt", "ver\u00b7wir\u00b7rest", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eda du mit deinem Wei\u00df und Grau darunter girrest?", "tokens": ["\u201e", "da", "du", "mit", "dei\u00b7nem", "Wei\u00df", "und", "Grau", "da\u00b7run\u00b7ter", "gir\u00b7rest", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie schien uns fast erz\u00fcrnt: sie wiederhohlte das", "tokens": ["Sie", "schien", "uns", "fast", "er\u00b7z\u00fcrnt", ":", "sie", "wie\u00b7der\u00b7hohl\u00b7te", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was sie gesagt, und sprach: \u201eDu redest ohne Ma\u00df.", "tokens": ["Was", "sie", "ge\u00b7sagt", ",", "und", "sprach", ":", "\u201e", "Du", "re\u00b7dest", "oh\u00b7ne", "Ma\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "(es gieng die Bau-Kunst an, die wollte sie noch fragen:)", "tokens": ["(", "es", "gieng", "die", "Bau\u00b7Kunst", "an", ",", "die", "woll\u00b7te", "sie", "noch", "fra\u00b7gen", ":)"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "emoticon"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "220\u201dWie kannst du nun so frey von einem Vorzug sagen?", "tokens": ["\"", "Wie", "kannst", "du", "nun", "so", "frey", "von", "ei\u00b7nem", "Vor\u00b7zug", "sa\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201emit was du dich geprahlt, das kommt von oben her;", "tokens": ["\u201e", "mit", "was", "du", "dich", "ge\u00b7prahlt", ",", "das", "kommt", "von", "o\u00b7ben", "her", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$,", "PDS", "VVFIN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201enicht dein, des Himmels ist der Wercke Kunst und Ehr.", "tokens": ["\u201e", "nicht", "dein", ",", "des", "Him\u00b7mels", "ist", "der", "Wer\u00b7cke", "Kunst", "und", "Ehr", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PPOSAT", "$,", "ART", "NN", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edas was dein Winckelma\u00df und deine Richtschnur bauet,", "tokens": ["\u201e", "das", "was", "dein", "Win\u00b7ckel\u00b7ma\u00df", "und", "dei\u00b7ne", "Richt\u00b7schnur", "bau\u00b7et", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "PRELS", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eist stumm, wann man darauf nicht meine Zierden schauet.", "tokens": ["\u201e", "ist", "stumm", ",", "wann", "man", "da\u00b7rauf", "nicht", "mei\u00b7ne", "Zier\u00b7den", "schau\u00b7et", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$,", "PWAV", "PIS", "PAV", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "225\u201dDas Helden-Bild, das ich dir erst beschrieben hab,", "tokens": ["\"", "Das", "Hel\u00b7den\u00b7Bild", ",", "das", "ich", "dir", "erst", "be\u00b7schrie\u00b7ben", "hab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eg\u00e4b mehr als ein Geb\u00e4u von tausend S\u00e4ulen ab.", "tokens": ["\u201e", "g\u00e4b", "mehr", "als", "ein", "Ge\u00b7b\u00e4u", "von", "tau\u00b7send", "S\u00e4u\u00b7len", "ab", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "KOKOM", "ART", "NN", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ees w\u00fcrde von Metall, Gold oder Erz gegossen,", "tokens": ["\u201e", "es", "w\u00fcr\u00b7de", "von", "Me\u00b7tall", ",", "Gold", "o\u00b7der", "Erz", "ge\u00b7gos\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "\u201emit Sieges-R\u00fcstungen und Kriegs-Ger\u00e4th umschlossen.", "tokens": ["\u201e", "mit", "Sie\u00b7ges\u00b7R\u00fcs\u00b7tun\u00b7gen", "und", "Kriegs\u00b7Ge\u00b7r\u00e4th", "um\u00b7schlos\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}