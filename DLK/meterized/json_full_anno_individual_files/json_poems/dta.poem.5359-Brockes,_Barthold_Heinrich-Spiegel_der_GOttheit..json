{"dta.poem.5359": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Spiegel der GOttheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind die erschaffnen Creaturen, wenn man's erweget,", "tokens": ["Sind", "die", "er\u00b7schaff\u00b7nen", "Crea\u00b7tu\u00b7ren", ",", "wenn", "man's", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "anders was,", "tokens": ["an\u00b7ders", "was", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PIS", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Als ein des Sch\u00f6pfers wahres Wesen vor Augen stellend", "tokens": ["Als", "ein", "des", "Sch\u00f6p\u00b7fers", "wah\u00b7res", "We\u00b7sen", "vor", "Au\u00b7gen", "stel\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ART", "NN", "ADJA", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Spiegel-Glas?", "tokens": ["Spie\u00b7gel\u00b7Glas", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "In welchem, durch die, zu dem Zweck allein, erschaffne", "tokens": ["In", "wel\u00b7chem", ",", "durch", "die", ",", "zu", "dem", "Zweck", "al\u00b7lein", ",", "er\u00b7schaff\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PWAT", "$,", "APPR", "ART", "$,", "APPR", "ART", "NN", "ADV", "$,", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sonnen-Strahlen,", "tokens": ["Son\u00b7nen\u00b7Strah\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sich seine Weisheit, Lieb\u2019 und Macht uns allen \u00fcberzeug-", "tokens": ["Sich", "sei\u00b7ne", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht", "uns", "al\u00b7len", "\u00fc\u00b7ber\u00b7zeug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "PPER", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "lich mahlen,", "tokens": ["lich", "mah\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Und, durch die Sinnen, unsren Seelen empfindlich vor-", "tokens": ["Und", ",", "durch", "die", "Sin\u00b7nen", ",", "un\u00b7sren", "See\u00b7len", "emp\u00b7find\u00b7lich", "vor"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "gestellet werden?", "tokens": ["ge\u00b7stel\u00b7let", "wer\u00b7den", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Das Wasser, auch die glatten C\u00f6rper, worin vom Him-", "tokens": ["Das", "Was\u00b7ser", ",", "auch", "die", "glat\u00b7ten", "C\u00f6r\u00b7per", ",", "wo\u00b7rin", "vom", "Him"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,", "PWAV", "APPRART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "mel und der Erden", "tokens": ["mel", "und", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Von angestrahlten B\u00e4umen, Blumen, Geb\u00fcschen, W\u00e4l-", "tokens": ["Von", "an\u00b7ge\u00b7strahl\u00b7ten", "B\u00e4u\u00b7men", ",", "Blu\u00b7men", ",", "Ge\u00b7b\u00fc\u00b7schen", ",", "W\u00e4l"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "dern, Thal, und H\u00fcgel,", "tokens": ["dern", ",", "Thal", ",", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Die Sch\u00f6nheit sich verdoppelt zeigt, sind von dem Spie-", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "sich", "ver\u00b7dop\u00b7pelt", "zeigt", ",", "sind", "von", "dem", "Spie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVPP", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "gel Gegen-Spiegel,", "tokens": ["gel", "Ge\u00b7gen\u00b7Spie\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Und unser Aug\u2019 ein sinnlicher, lebend\u2019ger Spiegel, der", "tokens": ["Und", "un\u00b7ser", "Aug'", "ein", "sinn\u00b7li\u00b7cher", ",", "le\u00b7ben\u00b7d'\u00b7ger", "Spie\u00b7gel", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "den Geist,", "tokens": ["den", "Geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Auf eine sonderbahre Weise, durch C\u00f6rper etwas Geistigs", "tokens": ["Auf", "ei\u00b7ne", "son\u00b7der\u00b7bah\u00b7re", "Wei\u00b7se", ",", "durch", "C\u00f6r\u00b7per", "et\u00b7was", "Geis\u00b7tigs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NE", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "weis't.", "tokens": ["weis'", "t."], "token_info": ["word", "abbreviation"], "pos": ["VVFIN", "NE"], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Die Seel\u2019 erblicket von der GOttheit, in der Gesch\u00f6pfe", "tokens": ["Die", "Seel'", "er\u00b7bli\u00b7cket", "von", "der", "Got\u00b7theit", ",", "in", "der", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.22": {"text": "Wunder Pracht,", "tokens": ["Wun\u00b7der", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Ein gleichsam dreyfach-einigs Wesen, in seiner ", "tokens": ["Ein", "gleich\u00b7sam", "dreyfach\u00b7ei\u00b7nigs", "We\u00b7sen", ",", "in", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "APPR", "PPOSAT"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Liebe, Macht.", "tokens": ["Lie\u00b7be", ",", "Macht", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Erfordert es denn wenigstens vern\u00fcnft\u2019ger Menschen See-", "tokens": ["Er\u00b7for\u00b7dert", "es", "denn", "we\u00b7nigs\u00b7tens", "ver\u00b7n\u00fcnft'\u00b7ger", "Men\u00b7schen", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "len-Pflicht,", "tokens": ["len\u00b7Pflicht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Zu seines Nahmens Preis\u2019 und Ruhme, Lob, Herrlichkeit", "tokens": ["Zu", "sei\u00b7nes", "Nah\u00b7mens", "Preis'", "und", "Ruh\u00b7me", ",", "Lob", ",", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "und Ehre, nicht,", "tokens": ["und", "Eh\u00b7re", ",", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}}}}}