{"textgrid.poem.33346": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Gegenst\u00fcck zu B\u00fcrger's Lied:", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Bachus ist ein schlechter Mann,", "tokens": ["Herr", "Ba\u00b7chus", "ist", "ein", "schlech\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schmutz'ger, grober Bengel,", "tokens": ["Ein", "schmutz'\u00b7ger", ",", "gro\u00b7ber", "Ben\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Herr Apoll, der Leyermann,", "tokens": ["Und", "Herr", "A\u00b7poll", ",", "der", "Le\u00b7yer\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "$,", "ART", "NN", "$,"], "meter": "-++---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ist gegen ihn ein Engel.", "tokens": ["Ist", "ge\u00b7gen", "ihn", "ein", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar wei\u00df der Saufbold auf dem Fa\u00df", "tokens": ["Zwar", "wei\u00df", "der", "Sauf\u00b7bold", "auf", "dem", "Fa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar m\u00e4chtig sich zu br\u00fcsten,", "tokens": ["Gar", "m\u00e4ch\u00b7tig", "sich", "zu", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und thut, als wenn von seinem Na\u00df", "tokens": ["Und", "thut", ",", "als", "wenn", "von", "sei\u00b7nem", "Na\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOKOM", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir alle leben m\u00fc\u00dften.", "tokens": ["Wir", "al\u00b7le", "le\u00b7ben", "m\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allein guckt man in's Fa\u00df hinein,", "tokens": ["Al\u00b7lein", "guckt", "man", "in's", "Fa\u00df", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auf dem der Prahler reitet,", "tokens": ["Auf", "dem", "der", "Prah\u00b7ler", "rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So ist's nur saurer Apfelwein,", "tokens": ["So", "ist's", "nur", "sau\u00b7rer", "Ap\u00b7fel\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Hefen zubereitet.", "tokens": ["Mit", "He\u00b7fen", "zu\u00b7be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch sitzt er d'rauf, wie angepicht,", "tokens": ["Doch", "sitzt", "er", "d'\u00b7rauf", ",", "wie", "an\u00b7ge\u00b7picht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit immer vollem Glase,", "tokens": ["Mit", "im\u00b7mer", "vol\u00b7lem", "Gla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dickwanstig, Bausback' im Gesicht,", "tokens": ["Dick\u00b7wans\u00b7tig", ",", "Baus\u00b7back'", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rubinen auf der Nase.", "tokens": ["Ru\u00b7bi\u00b7nen", "auf", "der", "Na\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wird der Tummler ihm zu klein,", "tokens": ["Und", "wird", "der", "Tumm\u00b7ler", "ihm", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So legt er, wie von Sinnen,", "tokens": ["So", "legt", "er", ",", "wie", "von", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich unter'n Schlauch, und l\u00e4\u00dft den Wein", "tokens": ["Sich", "un\u00b7ter'n", "Schlauch", ",", "und", "l\u00e4\u00dft", "den", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich in die Gurgel rinnen.", "tokens": ["Sich", "in", "die", "Gur\u00b7gel", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bei Tische l\u00e4rmt und schreit und singt", "tokens": ["Bei", "Ti\u00b7sche", "l\u00e4rmt", "und", "schreit", "und", "singt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Bachus, wie von Sinnen,", "tokens": ["Herr", "Ba\u00b7chus", ",", "wie", "von", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Und l\u00e4\u00dft wohl gar oft, was er trinkt,", "tokens": ["Und", "l\u00e4\u00dft", "wohl", "gar", "oft", ",", "was", "er", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allen Leuten rinnen.", "tokens": ["Vor", "al\u00b7len", "Leu\u00b7ten", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im Rausch zertr\u00fcmmert und zerpufft", "tokens": ["Im", "Rausch", "zer\u00b7tr\u00fcm\u00b7mert", "und", "zer\u00b7pufft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er Sch\u00fcssel, Glas und Teller,", "tokens": ["Er", "Sch\u00fcs\u00b7sel", ",", "Glas", "und", "Tel\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "D'rum schmi\u00df man auch den groben Schuft", "tokens": ["D'\u00b7rum", "schmi\u00df", "man", "auch", "den", "gro\u00b7ben", "Schuft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zu Kutschern in den Keller.", "tokens": ["Zu", "Kut\u00b7schern", "in", "den", "Kel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nur pflegt er jetzt noch dann und wann", "tokens": ["Nur", "pflegt", "er", "jetzt", "noch", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Kl\u00f6ster zu gerathen,", "tokens": ["In", "Kl\u00f6s\u00b7ter", "zu", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und spielt, mit Seide angethan,", "tokens": ["Und", "spielt", ",", "mit", "Sei\u00b7de", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den trunkenen Pr\u00e4laten.", "tokens": ["Den", "trun\u00b7ke\u00b7nen", "Pr\u00e4\u00b7la\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Vor Zeiten lief er gar ohn' Hemd'", "tokens": ["Vor", "Zei\u00b7ten", "lief", "er", "gar", "ohn'", "Hemd'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herum auf allen Strassen,", "tokens": ["He\u00b7rum", "auf", "al\u00b7len", "Stras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lie\u00df die M\u00e4dchen unversch\u00e4mt", "tokens": ["Und", "lie\u00df", "die", "M\u00e4d\u00b7chen", "un\u00b7ver\u00b7sch\u00e4mt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Err\u00f6then und erblassen.", "tokens": ["Er\u00b7r\u00f6\u00b7then", "und", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dabei ist er nach altem Brauch", "tokens": ["Da\u00b7bei", "ist", "er", "nach", "al\u00b7tem", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Grobian von Sitten,", "tokens": ["Ein", "Gro\u00b7bi\u00b7an", "von", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "D'rum war er bei den M\u00e4dchen auch", "tokens": ["D'\u00b7rum", "war", "er", "bei", "den", "M\u00e4d\u00b7chen", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Von je so schlecht gelitten.", "tokens": ["Von", "je", "so", "schlecht", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dagegen wei\u00df gar wunders\u00fc\u00df", "tokens": ["Da\u00b7ge\u00b7gen", "wei\u00df", "gar", "wun\u00b7der\u00b7s\u00fc\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Apoll zu karessiren,", "tokens": ["A\u00b7poll", "zu", "ka\u00b7res\u00b7si\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ist artig, und l\u00e4\u00dft \u00fcberdies", "tokens": ["Ist", "ar\u00b7tig", ",", "und", "l\u00e4\u00dft", "\u00fc\u00b7ber\u00b7dies"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich alle Tag' frisiren.", "tokens": ["Sich", "al\u00b7le", "Tag'", "fri\u00b7si\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "An den Toiletten, auf dem Ball,", "tokens": ["An", "den", "To\u00b7i\u00b7let\u00b7ten", ",", "auf", "dem", "Ball", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bei Spiel und Assembleen,", "tokens": ["Bei", "Spiel", "und", "As\u00b7sem\u00b7bleen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei Serenaden \u2013 \u00fcberall", "tokens": ["Bei", "Se\u00b7re\u00b7na\u00b7den", "\u2013", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NE", "$(", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er recht gern gesehen.", "tokens": ["Ist", "er", "recht", "gern", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+----+-", "measure": "dactylic.init"}}, "stanza.13": {"line.1": {"text": "Er reicht die Papilloten dar", "tokens": ["Er", "reicht", "die", "Pa\u00b7pil\u00b7lo\u00b7ten", "dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Sch\u00f6nen, die sich putzen,", "tokens": ["Bei", "Sch\u00f6\u00b7nen", ",", "die", "sich", "put\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die erlauben ihm sogar", "tokens": ["Und", "die", "er\u00b7lau\u00b7ben", "ihm", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freiheit \u2013 sie zu dutzen.", "tokens": ["Die", "Frei\u00b7heit", "\u2013", "sie", "zu", "dut\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da mag Herr Bachus immerhin", "tokens": ["Da", "mag", "Herr", "Ba\u00b7chus", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die grossen Br\u00fcder schelten,", "tokens": ["Die", "gros\u00b7sen", "Br\u00fc\u00b7der", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Apollo hat es mehr Gewinn,", "tokens": ["A\u00b7pol\u00b7lo", "hat", "es", "mehr", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei M\u00e4dchen was zu gelten.", "tokens": ["Bei", "M\u00e4d\u00b7chen", "was", "zu", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Daf\u00fcr ist er auch ganz gemacht,", "tokens": ["Da\u00b7f\u00fcr", "ist", "er", "auch", "ganz", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Den Sch\u00f6nen zu gefallen:", "tokens": ["Den", "Sch\u00f6\u00b7nen", "zu", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geht Chapeau bas, tanzt, singt 