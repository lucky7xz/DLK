i"}}, "stanza.25": {"line.1": {"text": "Im Rausch zertr\u00fcmmert und zerpufft", "tokens": ["Im", "Rausch", "zer\u00b7tr\u00fcm\u00b7mert", "und", "zer\u00b7pufft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er Sch\u00fcssel, Glas und Teller,", "tokens": ["Er", "Sch\u00fcs\u00b7sel", ",", "Glas", "und", "Tel\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "D'rum schmi\u00df man auch den groben Schuft", "tokens": ["D'\u00b7rum", "schmi\u00df", "man", "auch", "den", "gro\u00b7ben", "Schuft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zu Kutschern in den Keller.", "tokens": ["Zu", "Kut\u00b7schern", "in", "den", "Kel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Nur pflegt er jetzt noch dann und wann", "tokens": ["Nur", "pflegt", "er", "jetzt", "noch", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Kl\u00f6ster zu gerathen,", "tokens": ["In", "Kl\u00f6s\u00b7ter", "zu", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und spielt, mit Seide angethan,", "tokens": ["Und", "spielt", ",", "mit", "Sei\u00b7de", "an\u00b