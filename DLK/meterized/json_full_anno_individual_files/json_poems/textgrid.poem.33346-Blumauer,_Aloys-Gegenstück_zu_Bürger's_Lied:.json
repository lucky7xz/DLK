 "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geht Chapeau bas, tanzt, singt und lacht,", "tokens": ["Geht", "Cha\u00b7pe\u00b7au", "bas", ",", "tanzt", ",", "singt", "und", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und kos't und scherzt mit allen.", "tokens": ["Und", "kos't", "und", "scherzt", "mit", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.34": {"line.1": {"text": "Den neu'sten Schnitt w\u00e4hlt sich der Mann", "tokens": ["Den", "neu'\u00b7sten", "Schnitt", "w\u00e4hlt", "sich", "der", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu jedem seiner Kleider,", "tokens": ["Zu", "je\u00b7dem", "sei\u00b7ner", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ist \u2013 wer s\u00e4h' ihm so was an?", "tokens": ["Und", "ist", "\u2013", "wer", "s\u00e4h'", "ihm", "so", "was", "an", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "PWS", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dabei sein eig'ner Schneider.", "tokens": ["Da\u00b7bei", "sein", "eig'\u00b7ner", "Schnei\u00b7der", "."], "token_info": ["word", "word", "w