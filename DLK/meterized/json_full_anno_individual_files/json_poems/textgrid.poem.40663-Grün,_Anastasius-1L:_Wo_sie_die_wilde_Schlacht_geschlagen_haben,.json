{"textgrid.poem.40663": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo sie die wilde Schlacht geschlagen haben,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sie die wilde Schlacht geschlagen haben,", "tokens": ["Wo", "sie", "die", "wil\u00b7de", "Schlacht", "ge\u00b7schla\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O lauscht nicht auf ", "tokens": ["O", "lauscht", "nicht", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da kreischt die Kr\u00e4he nur nach blankem Fange,", "tokens": ["Da", "kreischt", "die", "Kr\u00e4\u00b7he", "nur", "nach", "blan\u00b7kem", "Fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann kommen erst die Geier und die Raben;", "tokens": ["Dann", "kom\u00b7men", "erst", "die", "Gei\u00b7er", "und", "die", "Ra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie kommen zu beerben, zu begraben;", "tokens": ["Sie", "kom\u00b7men", "zu", "be\u00b7er\u00b7ben", ",", "zu", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "PTKZU", "VVINF", "$,", 