{"textgrid.poem.60178": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Freund! in deinem Blick seh' ich noch Klagen.", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freund! in deinem Blick seh' ich noch Klagen.", "tokens": ["O", "Freund", "!", "in", "dei\u00b7nem", "Blick", "seh'", "ich", "noch", "Kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So laut dein Herz auch dem Vergn\u00fcgen ruft,", "tokens": ["So", "laut", "dein", "Herz", "auch", "dem", "Ver\u00b7gn\u00fc\u00b7gen", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So laut h\u00f6rts noch in sich den Kummer sagen:", "tokens": ["So", "laut", "h\u00f6rts", "noch", "in", "sich", "den", "Kum\u00b7mer", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "APPR", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort liegt sie in der Gruft!", "tokens": ["Dort", "liegt", "sie", "in", "der", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach klage nur! ganz ist sie deiner Schmerzen", "tokens": ["Ach", "kla\u00b7ge", "nur", "!", "ganz", "ist", "sie", "dei\u00b7ner", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz deiner unumschr\u00e4nkten Trauer werth:", "tokens": ["Ganz", "dei\u00b7ner", "un\u00b7um\u00b7schr\u00e4nk\u00b7ten", "Trau\u00b7er", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Welch Antlitz! O! welch Bild vom besten Herzen!", "tokens": ["Welch", "Ant\u00b7litz", "!", "O", "!", "welch", "Bild", "vom", "bes\u00b7ten", "Her\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "NE", "$.", "PWAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das nun der Wurm verzehrt!", "tokens": ["Das", "nun", "der", "Wurm", "ver\u00b7zehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der heitre Tag, den keine Wolk umh\u00fcllet,", "tokens": ["Der", "heit\u00b7re", "Tag", ",", "den", "kei\u00b7ne", "Wolk", "um\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie l\u00e4chelt er von ihrer Stirn herab?", "tokens": ["Wie", "l\u00e4\u00b7chelt", "er", "von", "ih\u00b7rer", "Stirn", "her\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jeder Blick, wie mit Gef\u00fchl erf\u00fcllet", "tokens": ["Und", "je\u00b7der", "Blick", ",", "wie", "mit", "Ge\u00b7f\u00fchl", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe, die ihn gab!", "tokens": ["Der", "Lie\u00b7be", ",", "die", "ihn", "gab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr holder Reiz! der Tod nahm ihn zum Raube;", "tokens": ["Ihr", "hol\u00b7der", "Reiz", "!", "der", "Tod", "nahm", "ihn", "zum", "Rau\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6ne Mund! nicht mehr f\u00fcr deinen Ku\u00df!", "tokens": ["Der", "sch\u00f6\u00b7ne", "Mund", "!", "nicht", "mehr", "f\u00fcr", "dei\u00b7nen", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus ihm entfloh ihr sch\u00f6ner Geist dem Staube,", "tokens": ["Aus", "ihm", "ent\u00b7floh", "ihr", "sch\u00f6\u00b7ner", "Geist", "dem", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu himmlischem Genu\u00df!", "tokens": ["Zu", "himm\u00b7li\u00b7schem", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drey T\u00f6chter blieben nur, die durch ihr Lallen", "tokens": ["Drey", "T\u00f6ch\u00b7ter", "blie\u00b7ben", "nur", ",", "die", "durch", "ihr", "Lal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADV", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich tiefer ritzten in der bangen Brust,", "tokens": ["Dich", "tie\u00b7fer", "ritz\u00b7ten", "in", "der", "ban\u00b7gen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wie im Lenz die Rosenbl\u00e4tter fallen,", "tokens": ["So", "wie", "im", "Lenz", "die", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwelkte deine Lust!", "tokens": ["Ver\u00b7welk\u00b7te", "dei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zw\u00f6lfmal hat schon der Mond in vollem Lichte", "tokens": ["Zw\u00f6lf\u00b7mal", "hat", "schon", "der", "Mond", "in", "vol\u00b7lem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dir zugesehn, wenn schw\u00e4rzer, als die Nacht,", "tokens": ["Dir", "zu\u00b7ge\u00b7sehn", ",", "wenn", "schw\u00e4r\u00b7zer", ",", "als", "die", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "ADJA", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der tiefe Gram von deinem Angesichte", "tokens": ["Der", "tie\u00b7fe", "Gram", "von", "dei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Schlaf entfliehn gemacht!", "tokens": ["Den", "Schlaf", "ent\u00b7fliehn", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "H\u00f6r einmal auf, und wende deine Blicke", "tokens": ["H\u00f6r", "ein\u00b7mal", "auf", ",", "und", "wen\u00b7de", "dei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Grab, geneu\u00df des Lebens kurzen Traum!", "tokens": ["Vom", "Grab", ",", "ge\u00b7neu\u00df", "des", "Le\u00b7bens", "kur\u00b7zen", "Traum", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! ohne Liebe bleibt im gr\u00f6\u00dften Gl\u00fccke", "tokens": ["Ach", "!", "oh\u00b7ne", "Lie\u00b7be", "bleibt", "im", "gr\u00f6\u00df\u00b7ten", "Gl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Herz ein leerer Raum!", "tokens": ["Das", "Herz", "ein", "lee\u00b7rer", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Such unter allen Sch\u00f6nen, die dem Lande", "tokens": ["Such", "un\u00b7ter", "al\u00b7len", "Sch\u00f6\u00b7nen", ",", "die", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Liebe gab, dir eine Tochter aus,", "tokens": ["Die", "Lie\u00b7be", "gab", ",", "dir", "ei\u00b7ne", "Toch\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gezeichnet von der Tugend mit Verstande,", "tokens": ["Ge\u00b7zeich\u00b7net", "von", "der", "Tu\u00b7gend", "mit", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Zierde f\u00fcr dein Haus!", "tokens": ["Zur", "Zier\u00b7de", "f\u00fcr", "dein", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sanft, wie ein Lamm, das in der Mittagsstunde", "tokens": ["Sanft", ",", "wie", "ein", "Lamm", ",", "das", "in", "der", "Mit\u00b7tags\u00b7stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fromm auf dem Schoo\u00df der jungen Cloe spielt,", "tokens": ["Fromm", "auf", "dem", "Schoo\u00df", "der", "jun\u00b7gen", "Cloe", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sey sie, und trag ein Herz in ihrem Munde", "tokens": ["Sey", "sie", ",", "und", "trag", "ein", "Herz", "in", "ih\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das nur f\u00fcr dich gef\u00fchlt.", "tokens": ["Das", "nur", "f\u00fcr", "dich", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O Freund! in deinem Blick seh' ich noch Klagen.", "tokens": ["O", "Freund", "!", "in", "dei\u00b7nem", "Blick", "seh'", "ich", "noch", "Kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So laut dein Herz auch dem Vergn\u00fcgen ruft,", "tokens": ["So", "laut", "dein", "Herz", "auch", "dem", "Ver\u00b7gn\u00fc\u00b7gen", "ruft", ","], "tok