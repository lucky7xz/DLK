{"textgrid.poem.41961": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebchen, o komm zum l\u00e4ndlichen Fest, das ich heute bereitet,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, o komm zum l\u00e4ndlichen Fest, das ich heute bereitet,", "tokens": ["Lieb\u00b7chen", ",", "o", "komm", "zum", "l\u00e4nd\u00b7li\u00b7chen", "Fest", ",", "das", "ich", "heu\u00b7te", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "VVFIN", "APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wahrlich, im fr\u00f6hlichen Kreis fehlte die Grazie sonst.", "tokens": ["Wahr\u00b7lich", ",", "im", "fr\u00f6h\u00b7li\u00b7chen", "Kreis", "fehl\u00b7te", "die", "Gra\u00b7zie", "sonst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+--+---+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Sieh, zur Arkadischen Flur ward rings der ver\u00f6dete Harzwald,", "tokens": ["Sieh", ",", "zur", "Ar\u00b7ka\u00b7di\u00b7schen", "Flur", "ward", "rings", "der", "ver\u00b7\u00f6\u00b7de\u00b7te", "Harz\u00b7wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--++", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Hoch am schroffen Gebirg winket der Tempel der Lust.", "tokens": ["Hoch", "am", "schrof\u00b7fen", "Ge\u00b7birg", "win\u00b7ket", "der", "Tem\u00b7pel", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Fl\u00fcsterndes Laub w\u00f6lbt schattig den Dom, zum duftenden Altar", "tokens": ["Fl\u00fcs\u00b7tern\u00b7des", "Laub", "w\u00f6lbt", "schat\u00b7tig", "den", "Dom", ",", "zum", "duf\u00b7ten\u00b7den", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Schwellet der Rasen, es haucht s\u00e4uselnde Hymnen der West.", "tokens": ["Schwel\u00b7let", "der", "Ra\u00b7sen", ",", "es", "haucht", "s\u00e4u\u00b7seln\u00b7de", "Hym\u00b7nen", "der", "West", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Priesterin bist du selbst der rosigen Freude; f\u00fcr mich nur", "tokens": ["Pries\u00b7te\u00b7rin", "bist", "du", "selbst", "der", "ro\u00b7si\u00b7gen", "Freu\u00b7de", ";", "f\u00fcr", "mich", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "APPR", "PPER", "ADV"], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.8": {"text": "Sey, o h\u00f6re mein Flehn, Priesterin zarterer Gluth!", "tokens": ["Sey", ",", "o", "h\u00f6\u00b7re", "mein", "Flehn", ",", "Pries\u00b7te\u00b7rin", "zar\u00b7te\u00b7rer", "Gluth", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "VVFIN", "PPOSAT", "NN", "$,", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "K\u00f6nnt' ich mit dir allein, du Reizende, k\u00f6nnt ich die\u00df Fest doch", "tokens": ["K\u00f6nnt'", "ich", "mit", "dir", "al\u00b7lein", ",", "du", "Rei\u00b7zen\u00b7de", ",", "k\u00f6nnt", "ich", "die\u00df", "Fest", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ADV", "$,", "PPER", "NN", "$,", "VVFIN", "PPER", "PDS", "NN", "ADV"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Feyern, ohne da\u00df schlau lauscht' in der Ferne der Neid!", "tokens": ["Fey\u00b7ern", ",", "oh\u00b7ne", "da\u00df", "schlau", "lauscht'", "in", "der", "Fer\u00b7ne", "der", "Neid", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "KOUS", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Ach, mit schneidendem Schwert trennt Gl\u00fcck und Liebe der Volkswahn,", "tokens": ["Ach", ",", "mit", "schnei\u00b7den\u00b7dem", "Schwert", "trennt", "Gl\u00fcck", "und", "Lie\u00b7be", "der", "Volks\u00b7wahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Gr\u00e4mliche Sitte verbeut manche romantische Lust.", "tokens": ["Gr\u00e4m\u00b7li\u00b7che", "Sit\u00b7te", "ver\u00b7beut", "man\u00b7che", "ro\u00b7man\u00b7ti\u00b7sche", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Aber nur Muth! Schon ist alles bereit; der tappende Plutus", "tokens": ["A\u00b7ber", "nur", "Muth", "!", "Schon", "ist", "al\u00b7les", "be\u00b7reit", ";", "der", "tap\u00b7pen\u00b7de", "Plu\u00b7tus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "$.", "ADV", "VAFIN", "PIS", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "+--+-++--+-+--+-", "measure": "iambic.septa.invert"}, "line.14": {"text": "Fe\u00dfle, mit goldener Hand winkend, die M\u00e4nner an's Spiel,", "tokens": ["Fe\u00df\u00b7le", ",", "mit", "gol\u00b7de\u00b7ner", "Hand", "win\u00b7kend", ",", "die", "M\u00e4n\u00b7ner", "an's", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Und fest banne den weiblichen Kreis die Betr\u00fcgerin Fama,", "tokens": ["Und", "fest", "ban\u00b7ne", "den", "weib\u00b7li\u00b7chen", "Kreis", "die", "Be\u00b7tr\u00fc\u00b7ge\u00b7rin", "Fa\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-++--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Neue Ger\u00fcchte genug sammelt' ich heute f\u00fcr sie.", "tokens": ["Neu\u00b7e", "Ge\u00b7r\u00fcch\u00b7te", "ge\u00b7nug", "sam\u00b7melt'", "ich", "heu\u00b7te", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Manches stille Geb\u00fcsch hegt rings des verschwiegenen Hains Nacht,", "tokens": ["Man\u00b7ches", "stil\u00b7le", "Ge\u00b7b\u00fcsch", "hegt", "rings", "des", "ver\u00b7schwie\u00b7ge\u00b7nen", "Hains", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Manches heimliche Thal lockt zum verstohlenen Ku\u00df;", "tokens": ["Man\u00b7ches", "heim\u00b7li\u00b7che", "Thal", "lockt", "zum", "ver\u00b7stoh\u00b7le\u00b7nen", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Quellen rieseln umher, und weich ist der Rasen; der Westhauch", "tokens": ["Quel\u00b7len", "rie\u00b7seln", "um\u00b7her", ",", "und", "weich", "ist", "der", "Ra\u00b7sen", ";", "der", "West\u00b7hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Plaudert das s\u00fc\u00dfe Gespr\u00e4ch t\u00e4ndelnder Liebe nicht aus.", "tokens": ["Plau\u00b7dert", "das", "s\u00fc\u00b7\u00dfe", "Ge\u00b7spr\u00e4ch", "t\u00e4n\u00b7deln\u00b7der", "Lie\u00b7be", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Schm\u00fcckt mit dichterer D\u00e4mmerung euch, ihr schattigen Haine!", "tokens": ["Schm\u00fcckt", "mit", "dich\u00b7te\u00b7rer", "D\u00e4m\u00b7me\u00b7rung", "euch", ",", "ihr", "schat\u00b7ti\u00b7gen", "Hai\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Nur in der D\u00e4mmerung Schoo\u00df bl\u00fchet die Rose des Gl\u00fccks,", "tokens": ["Nur", "in", "der", "D\u00e4m\u00b7me\u00b7rung", "Schoo\u00df", "bl\u00fc\u00b7het", "die", "Ro\u00b7se", "des", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Ueppiger duftet, ihr Blumen, empor! vom Fittig des Dufthauchs", "tokens": ["Uep\u00b7pi\u00b7ger", "duf\u00b7tet", ",", "ihr", "Blu\u00b7men", ",", "em\u00b7por", "!", "vom", "Fit\u00b7tig", "des", "Duf\u00b7thauchs"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$.", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Tr\u00e4uft ein weicherer Sinn in das ge\u00f6ffnete Herz.", "tokens": ["Tr\u00e4uft", "ein", "wei\u00b7che\u00b7rer", "Sinn", "in", "das", "ge\u00b7\u00f6ff\u00b7ne\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Komm zum Fest, bald nahet der Trennung Stunde, zur\u00fcck bringt", "tokens": ["Komm", "zum", "Fest", ",", "bald", "na\u00b7het", "der", "Tren\u00b7nung", "Stun\u00b7de", ",", "zu\u00b7r\u00fcck", "bringt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Nimmer die Ewigkeit dir, was die Minute geraubt.", "tokens": ["Nim\u00b7mer", "die", "E\u00b7wig\u00b7keit", "dir", ",", "was", "die", "Mi\u00b7nu\u00b7te", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Liebchen, o komm zum l\u00e4ndlichen Fest, das ich heute bereitet,", "tokens": ["Lieb\u00b7chen", ",", "o", "komm", "zum", "l\u00e4nd\u00b7li\u00b7chen", "Fest", ",", "das", "ich", "heu\u00b7te", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "VVFIN", "APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wahrlich, im fr\u00f6hlichen Kreis fehlte die Grazie sonst.", "tokens": ["Wahr\u00b7lich", ",", "im", "fr\u00f6h\u00b7li\u00b7chen", "Kreis", "fehl\u00b7te", "die", "Gra\u00b7zie", "sonst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+--+---+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Sieh, zur Arkadischen Flur ward rings der ver\u00f6dete Harzwald,", "tokens": ["Sieh", ",", "zur", "Ar\u00b7ka\u00b7di\u00b7schen", "Flur", "ward", "rings", "der", "ver\u00b7\u00