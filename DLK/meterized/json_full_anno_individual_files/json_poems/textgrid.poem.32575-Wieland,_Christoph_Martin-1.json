{"textgrid.poem.32575": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder G\u00f6tterstand\u00ab \u2013 sprach einst von seinem Wolkenthron", "tokens": ["\u00bb", "der", "G\u00f6t\u00b7ter\u00b7stand", "\u00ab", "\u2013", "sprach", "einst", "von", "sei\u00b7nem", "Wol\u00b7ken\u00b7thron"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$(", "$(", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sultan im Olymp zu Majens sch\u00f6nem Sohn,", "tokens": ["Der", "Sul\u00b7tan", "im", "O\u00b7lymp", "zu", "Ma\u00b7jens", "sch\u00f6\u00b7nem", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbder G\u00f6tterstand, Herr Sohn, um ihm sein Recht zu geben,", "tokens": ["\u00bb", "der", "G\u00f6t\u00b7ter\u00b7stand", ",", "Herr", "Sohn", ",", "um", "ihm", "sein", "Recht", "zu", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "NN", "NN", "$,", "KOUI", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist (unter uns) beim Styx! ein schales Leben", "tokens": ["Ist", "(", "un\u00b7ter", "uns", ")", "beim", "Styx", "!", "ein", "scha\u00b7les", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$(", "APPR", "PPER", "$(", "APPRART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja, wer nur nicht dazu ", "tokens": ["Ja", ",", "wer", "nur", "nicht", "da\u00b7zu"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWS", "ADV", "PTKNEG", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und allenfalls auf acht bis vierzehn Tage,", "tokens": ["Und", "al\u00b7len\u00b7falls", "auf", "acht", "bis", "vier\u00b7zehn", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "CARD", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da lie\u00df ichs gelten! Aber mehr", "tokens": ["Da", "lie\u00df", "ichs", "gel\u00b7ten", "!", "A\u00b7ber", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVINF", "$.", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Man kriegt zuletzt des Weihrauchs so genug!", "tokens": ["Man", "kriegt", "zu\u00b7letzt", "des", "Weih\u00b7rauchs", "so", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und f\u00fcr und f\u00fcr zum ", "tokens": ["Und", "f\u00fcr", "und", "f\u00fcr", "zum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "KON", "APPR", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Die Grazien tanzen sehn, die Musen singen h\u00f6ren,", "tokens": ["Die", "Gra\u00b7zi\u00b7en", "tan\u00b7zen", "sehn", ",", "die", "Mu\u00b7sen", "sin\u00b7gen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$,", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Und immer Ganymed mit seinem Nektarkrug,", "tokens": ["Und", "im\u00b7mer", "Ga\u00b7ny\u00b7med", "mit", "sei\u00b7nem", "Nek\u00b7tar\u00b7krug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich sage dir, man kriegt's genug!", "tokens": ["Ich", "sa\u00b7ge", "dir", ",", "man", "kriegt's", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dann noch dazu den ewgen Litaneien", "tokens": ["Dann", "noch", "da\u00b7zu", "den", "ew\u00b7gen", "Li\u00b7ta\u00b7nei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Des Erdenvolks die Ohren herzuleihen!", "tokens": ["Des", "Er\u00b7den\u00b7volks", "die", "Oh\u00b7ren", "her\u00b7zu\u00b7lei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ein tolles ", "tokens": ["Ein", "tol\u00b7les"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Von Bitten ohne Sinn und Ma\u00df", "tokens": ["Von", "Bit\u00b7ten", "oh\u00b7ne", "Sinn", "und", "Ma\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Um nichts und wieder nichts, oft um Unm\u00f6glichkeiten!", "tokens": ["Um", "nichts", "und", "wie\u00b7der", "nichts", ",", "oft", "um", "Un\u00b7m\u00f6g\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "KON", "ADV", "PIS", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein wenig Sonnenschein zu meiner W\u00e4sche nur!'", "tokens": ["Ein", "we\u00b7nig", "Son\u00b7nen\u00b7schein", "zu", "mei\u00b7ner", "W\u00e4\u00b7sche", "nur", "!", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.20": {"text": "'zwei Regentage blo\u00df f\u00fcr meine trockne Flur!'", "tokens": ["'zwei", "Re\u00b7gen\u00b7ta\u00b7ge", "blo\u00df", "f\u00fcr", "mei\u00b7ne", "trock\u00b7ne", "Flur", "!", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ruft Mann und Frau aus hellem Munde", "tokens": ["Ruft", "Mann", "und", "Frau", "aus", "hel\u00b7lem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "In Einem Haus, in Einer Stunde.", "tokens": ["In", "Ei\u00b7nem", "Haus", ",", "in", "Ei\u00b7ner", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der Dedschial h\u00f6r alle das Gebr\u00fcll!", "tokens": ["Der", "Ded\u00b7schi\u00b7al", "h\u00f6r", "al\u00b7le", "das", "Ge\u00b7br\u00fcll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Tat ich ein einzigmal was jeder haben will,", "tokens": ["Tat", "ich", "ein", "ein\u00b7zig\u00b7mal", "was", "je\u00b7der", "ha\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADV", "PWS", "PIS", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es richtete die Welt und mich zu Grunde.", "tokens": ["Es", "rich\u00b7te\u00b7te", "die", "Welt", "und", "mich", "zu", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Kurz, trauter Sohn, die Stiefeln angeschn\u00fcrt!", "tokens": ["Kurz", ",", "trau\u00b7ter", "Sohn", ",", "die", "Stie\u00b7feln", "an\u00b7ge\u00b7schn\u00fcrt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Steig, eh ich hier des G\u00e4hnens m\u00fcde werde,", "tokens": ["Steig", ",", "eh", "ich", "hier", "des", "G\u00e4h\u00b7nens", "m\u00fc\u00b7de", "wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ein wenig nieder auf die Erde,", "tokens": ["Ein", "we\u00b7nig", "nie\u00b7der", "auf", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Zu sehen, ob man dort sich besser am\u00fcsiert!'", "tokens": ["Zu", "se\u00b7hen", ",", "ob", "man", "dort", "sich", "bes\u00b7ser", "a\u00b7m\u00fc\u00b7siert", "!", "'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PIS", "ADV", "PRF", "ADJD", "VVPP", "$.", "$("], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Merkur gehorcht, und ohne anzufragen,", "tokens": ["Mer\u00b7kur", "ge\u00b7horcht", ",", "und", "oh\u00b7ne", "an\u00b7zu\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "KON", "KOUI", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob Juno nach dem Erdenplan", "tokens": ["Ob", "Ju\u00b7no", "nach", "dem", "Er\u00b7den\u00b7plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was zu bestellen hat, und ohne Donnerwagen,", "tokens": ["Was", "zu", "be\u00b7stel\u00b7len", "hat", ",", "und", "oh\u00b7ne", "Don\u00b7ner\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "VAFIN", "$,", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schleicht Jupiter sich weg, und wird bei Leda \u2013 Schwan.", "to