{"textgrid.poem.60113": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihm, der an seinem Bothenstab", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihm, der an seinem Bothenstab", "tokens": ["Ihm", ",", "der", "an", "sei\u00b7nem", "Bo\u00b7then\u00b7stab"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So friedlich geht, so still vor\u00fcber", "tokens": ["So", "fried\u00b7lich", "geht", ",", "so", "still", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Nachtigallenhain und Grab;", "tokens": ["Vor", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7hain", "und", "Grab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem seiner Kinder Freude lieber", "tokens": ["Dem", "sei\u00b7ner", "Kin\u00b7der", "Freu\u00b7de", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Himmel und die Erde macht;", "tokens": ["Den", "Him\u00b7mel", "und", "die", "Er\u00b7de", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der jeden Weg, bey Tag und Nacht,", "tokens": ["Der", "je\u00b7den", "Weg", ",", "bey", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So rauh er ist, zu Ende singt,", "tokens": ["So", "rauh", "er", "ist", ",", "zu", "En\u00b7de", "singt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VAFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und, deutsch und wahr in That und Worten,", "tokens": ["Und", ",", "deutsch", "und", "wahr", "in", "That", "und", "Wor\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KON", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den guten Seelen aller Orten", "tokens": ["Den", "gu\u00b7ten", "See\u00b7len", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So manche gute Zeitung bringt \u2013", "tokens": ["So", "man\u00b7che", "gu\u00b7te", "Zei\u00b7tung", "bringt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ihm wollen wir zu Lieb' und Ehren", "tokens": ["Ihm", "wol\u00b7len", "wir", "zu", "Lieb'", "und", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den zweyten Freudenbecher leeren.", "tokens": ["Den", "zwey\u00b7ten", "Freu\u00b7den\u00b7be\u00b7cher", "lee\u00b7ren", "."], "token_info": ["word", "wor