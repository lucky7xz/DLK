{"dta.poem.16452": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "113.  Die Seelen Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Nimb hin der Sonnen Liecht: mein Jesus ist die Son\u0304e/", "tokens": ["Nimb", "hin", "der", "Son\u00b7nen", "Liecht", ":", "mein", "Je\u00b7sus", "ist", "die", "Son\u0304e", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$.", "PPOSAT", "NE", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die meine Seel erleucht/ und macht sie voller Wonne.", "tokens": ["Die", "mei\u00b7ne", "Seel", "er\u00b7leucht", "/", "und", "macht", "sie", "vol\u00b7ler", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}