{"dta.poem.13293": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verschiedne waren noch Gesellschafts-weis zertheilt,", "tokens": ["Ver\u00b7schied\u00b7ne", "wa\u00b7ren", "noch", "Ge\u00b7sell\u00b7schafts\u00b7weis", "zer\u00b7theilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich in dem Gespr\u00e4ch dort oder da verweilt;", "tokens": ["Die", "sich", "in", "dem", "Ge\u00b7spr\u00e4ch", "dort", "o\u00b7der", "da", "ver\u00b7weilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch trug die ", "tokens": ["Doch", "trug", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201ewas vor Verrichtungen ", "tokens": ["\u201e", "was", "vor", "Ver\u00b7rich\u00b7tun\u00b7gen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PWS", "APPR", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "145\u201dSo schreiten wir zum Schlu\u00df! vollenden wir den Streit!", "tokens": ["\"", "So", "schrei\u00b7ten", "wir", "zum", "Schlu\u00df", "!", "voll\u00b7en\u00b7den", "wir", "den", "Streit", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewir seynd nun allerdings der Hinderung befreyt;", "tokens": ["\u201e", "wir", "seynd", "nun", "al\u00b7ler\u00b7dings", "der", "Hin\u00b7de\u00b7rung", "be\u00b7freyt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201emir kommt in allem vor, es sey nicht schwer zu wissen,", "tokens": ["\u201e", "mir", "kommt", "in", "al\u00b7lem", "vor", ",", "es", "sey", "nicht", "schwer", "zu", "wis\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PIS", "PTKVZ", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edurch was vor einen Spruch der Zweifel aufzuschliessen.", "tokens": ["\u201e", "durch", "was", "vor", "ei\u00b7nen", "Spruch", "der", "Zwei\u00b7fel", "auf\u00b7zu\u00b7schlies\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}