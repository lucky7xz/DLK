{"dta.poem.14165": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte Gru\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sie aber schaute erschrocken", "tokens": ["Sie", "a\u00b7ber", "schau\u00b7te", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch lange Zeit nach mir hin,", "tokens": ["Noch", "lan\u00b7ge", "Zeit", "nach", "mir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und sch\u00fcttelte sinnend die Locken", "tokens": ["Und", "sch\u00fct\u00b7tel\u00b7te", "sin\u00b7nend", "die", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und wu\u00dfte nicht, wer ich bin. \u2014", "tokens": ["Und", "wu\u00df\u00b7te", "nicht", ",", "wer", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}