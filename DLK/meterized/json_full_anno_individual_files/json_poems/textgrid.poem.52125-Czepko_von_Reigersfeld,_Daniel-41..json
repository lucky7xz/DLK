{"textgrid.poem.52125": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zahl, ein' edle Kunst, die offenbahrt uns Gott,", "tokens": ["Die", "Zahl", ",", "ein'", "ed\u00b7le", "Kunst", ",", "die", "of\u00b7fen\u00b7bahrt", "uns", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und er wird offenbart durch sie aus weiser Noth:", "tokens": ["Und", "er", "wird", "of\u00b7fen\u00b7bart", "durch", "sie", "aus", "wei\u00b7ser", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr erster Ausflu\u00df ist das allgemeine Wesen,", "tokens": ["Ihr", "ers\u00b7ter", "Aus\u00b7flu\u00df", "ist", "das", "all\u00b7ge\u00b7mei\u00b7ne", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem hat die Geburth das Ein ihm auserlesen:", "tokens": ["In", "dem", "hat", "die", "Ge\u00b7burth", "das", "Ein", "ihm", "au\u00b7ser\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "ART", "NN", "ART", "ART", "PPER", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Du G\u00f6ttliche Gewalt, Du Thron F\u00fcrst dieser Krafft,", "tokens": ["Du", "G\u00f6tt\u00b7li\u00b7che", "Ge\u00b7walt", ",", "Du", "Thron", "F\u00fcrst", "die\u00b7ser", "Krafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "NN", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aus dem Ein entspringt, und w\u00fcrckt und alles schafft:", "tokens": ["Die", "aus", "dem", "Ein", "ent\u00b7springt", ",", "und", "w\u00fcrckt", "und", "al\u00b7les", "schafft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wir selbst\u00e4ndig sehn durch alle Zahlen fl\u00fc\u00dfen:", "tokens": ["Und", "wir", "selb\u00b7st\u00e4n\u00b7dig", "sehn", "durch", "al\u00b7le", "Zah\u00b7len", "fl\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Willkom'n Seriphiel. Mach uns die Kunst zu wi\u00dfen.", "tokens": ["Will\u00b7kom'n", "Se\u00b7ri\u00b7phiel", ".", "Mach", "uns", "die", "Kunst", "zu", "wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "So weit das gro\u00dfe Tieff: in dem die Sternen stehn,", "tokens": ["So", "weit", "das", "gro\u00b7\u00dfe", "Tieff", ":", "in", "dem", "die", "Ster\u00b7nen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$.", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ringrecht durch den Trieb des Grundbewegers gehn,", "tokens": ["Und", "rin\u00b7grecht", "durch", "den", "Trieb", "des", "Grund\u00b7be\u00b7we\u00b7gers", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich in der Ewigkeit in Ihm wei\u00df auszustrecken:", "tokens": ["Sich", "in", "der", "E\u00b7wig\u00b7keit", "in", "Ihm", "wei\u00df", "aus\u00b7zu\u00b7stre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So weit wil dir den Zweck der Herrschung Gott auffstecken.", "tokens": ["So", "weit", "wil", "dir", "den", "Zweck", "der", "Herr\u00b7schung", "Gott", "auffs\u00b7te\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du kanst der Sternen Zahl und sagst Sie aus der Hand,", "tokens": ["Du", "kanst", "der", "Ster\u00b7nen", "Zahl", "und", "sagst", "Sie", "aus", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ihrer mehr noch sind als Thetis grauer Sand:", "tokens": ["Ob", "ih\u00b7rer", "mehr", "noch", "sind", "als", "The\u00b7tis", "grau\u00b7er", "Sand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "ADV", "VAFIN", "KOKOM", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Vogel, Fisch und Thier im Himmel, Meer und Erden", "tokens": ["Kein", "Vo\u00b7gel", ",", "Fisch", "und", "Thier", "im", "Him\u00b7mel", ",", "Meer", "und", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "APPRART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Br\u00fct, streicht und wirfft. Ihm mu\u00df die Zahl erst von dir werden.", "tokens": ["Br\u00fct", ",", "streicht", "und", "wirfft", ".", "Ihm", "mu\u00df", "die", "Zahl", "erst", "von", "dir", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und aus der Zahl da fleust das Wesen ieden ein,", "tokens": ["Und", "aus", "der", "Zahl", "da", "fleust", "das", "We\u00b7sen", "ie\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "PIAT", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wo ein Einflu\u00df sich dem andern macht gemein:", "tokens": ["Da", "wo", "ein", "Ein\u00b7flu\u00df", "sich", "dem", "an\u00b7dern", "macht", "ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PRF", "ART", "ADJA", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du machst, da\u00df alle Ding' in dem sie durch dich gehen,", "tokens": ["Du", "machst", ",", "da\u00df", "al\u00b7le", "Ding'", "in", "dem", "sie", "durch", "dich", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN", "APPR", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort in der Algebra, hier Mercava bestehen.", "tokens": ["Dort", "in", "der", "Al\u00b7ge\u00b7bra", ",", "hier", "Mer\u00b7ca\u00b7va", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "$,", "ADV", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gott schleust die gantze Welt in deine Ziffern ein,", "tokens": ["Gott", "schleust", "die", "gant\u00b7ze", "Welt", "in", "dei\u00b7ne", "Zif\u00b7fern", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist gezehlt, wie lang sie sol seyn, und nicht seyn:", "tokens": ["Es", "ist", "ge\u00b7zehlt", ",", "wie", "lang", "sie", "sol", "seyn", ",", "und", "nicht", "seyn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PWAV", "ADJD", "PPER", "VMFIN", "VAINF", "$,", "KON", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist gezehlt, schau umb: Sie stehn dir im Gesichte,", "tokens": ["Es", "ist", "ge\u00b7zehlt", ",", "schau", "umb", ":", "Sie", "stehn", "dir", "im", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die S\u00fcndfluth und der Tag des Herrn und das Gerichte.", "tokens": ["Die", "S\u00fcnd\u00b7fluth", "und", "der", "Tag", "des", "Herrn", "und", "das", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Im ersten Ein da quillt das wesentliche Meer", "tokens": ["Im", "ers\u00b7ten", "Ein", "da", "quillt", "das", "we\u00b7sent\u00b7li\u00b7che", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ART", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unverg\u00e4nglichkeit ohn End' und Anfang her:", "tokens": ["Der", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "ohn", "End'", "und", "An\u00b7fang", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das rinnt von Zahl auff Zahl durch dieses gro\u00dfe Gantze,", "tokens": ["Das", "rinnt", "von", "Zahl", "auff", "Zahl", "durch", "die\u00b7ses", "gro\u00b7\u00dfe", "Gant\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPR", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wir sehn und nicht sehn in ungeendtem Glantze.", "tokens": ["So", "wir", "sehn", "und", "nicht", "sehn", "in", "un\u00b7ge\u00b7end\u00b7tem", "Glant\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "KON", "PTKNEG", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Da\u00df in den Zahlen uns, in uns die Zahlen wir", "tokens": ["Da\u00df", "in", "den", "Zah\u00b7len", "uns", ",", "in", "uns", "die", "Zah\u00b7len", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "$,", "APPR", "PPER", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umschaun, und in das Ein gekehrt seyn f\u00fcr und f\u00fcr:", "tokens": ["Um\u00b7schaun", ",", "und", "in", "das", "Ein", "ge\u00b7kehrt", "seyn", "f\u00fcr", "und", "f\u00fcr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "APPR", "ART", "NN", "VVPP", "VAINF", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bringt Herr Franckenberg ein geistlichs Licht getragen,", "tokens": ["So", "bringt", "Herr", "Fran\u00b7cken\u00b7berg", "ein", "geist\u00b7lichs", "Licht", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das untern Engeln Er im Himmel auffgeschlagen.", "tokens": ["Das", "un\u00b7tern", "En\u00b7geln", "Er", "im", "Him\u00b7mel", "auff\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Du Edler Franckenberg! Dein himmlischer Verstand", "tokens": ["Du", "Ed\u00b7ler", "Fran\u00b7cken\u00b7berg", "!", "Dein", "himm\u00b7li\u00b7scher", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O seelge Freundschafft! macht mit Engeln uns bekannt:", "tokens": ["O", "seel\u00b7ge", "Freund\u00b7schafft", "!", "macht", "mit", "En\u00b7geln", "uns", "be\u00b7kannt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir k\u00f6nnen diesen Dienst mit Zahlen nicht bezahlen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "die\u00b7sen", "Dienst", "mit", "Zah\u00b7len", "nicht", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Ein, das mu\u00df es thun: Drein sie gehn, draus sie strahlen.", "tokens": ["Das", "Ein", ",", "das", "mu\u00df", "es", "thun", ":", "Drein", "sie", "gehn", ",", "draus", "sie", "strah\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$.", "NN", "PPER", "VVINF", "$,", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Erweckte Seelen auff! Was vor kein Cabalist,", "tokens": ["Er\u00b7weck\u00b7te", "See\u00b7len", "auff", "!", "Was", "vor", "kein", "Ca\u00b7ba\u00b7list", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$.", "PWS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sey Pythagoras, es sey der Trismegist,", "tokens": ["Es", "sey", "Py\u00b7tha\u00b7go\u00b7ras", ",", "es", "sey", "der", "Tris\u00b7me\u00b7gist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Ziffern ausgezehlt durch Glauben und durch Schweigen,", "tokens": ["An", "Zif\u00b7fern", "aus\u00b7ge\u00b7zehlt", "durch", "Glau\u00b7ben", "und", "durch", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird itzt ein Engel Euch an Fingern zehln und zeigen.", "tokens": ["Wird", "itzt", "ein", "En\u0