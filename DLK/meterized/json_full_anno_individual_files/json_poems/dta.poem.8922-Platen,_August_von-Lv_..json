{"dta.poem.8922": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Lv .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich so wenig dir gefallen habe;", "tokens": ["Da\u00df", "ich", "so", "we\u00b7nig", "dir", "ge\u00b7fal\u00b7len", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn deine blonde Jugend, s\u00fc\u00dfer Knabe,", "tokens": ["Denn", "dei\u00b7ne", "blon\u00b7de", "Ju\u00b7gend", ",", "s\u00fc\u00b7\u00dfer", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verschm\u00e4ht den melancholischen Genossen.", "tokens": ["Ver\u00b7schm\u00e4ht", "den", "me\u00b7lan\u00b7cho\u00b7li\u00b7schen", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So will in Scherz ich mich ergehn, in Possen,", "tokens": ["So", "will", "in", "Scherz", "ich", "mich", "er\u00b7gehn", ",", "in", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NN", "PPER", "PRF", "VVINF", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anstatt ich jezt mich blos an Thr\u00e4nen labe,", "tokens": ["An\u00b7statt", "ich", "jezt", "mich", "blos", "an", "Thr\u00e4\u00b7nen", "la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und um der Fr\u00f6hlichkeit mir fremde Gabe", "tokens": ["Und", "um", "der", "Fr\u00f6h\u00b7lich\u00b7keit", "mir", "frem\u00b7de", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hab' ich den Himmel anzuflehn beschlossen.", "tokens": ["Hab'", "ich", "den", "Him\u00b7mel", "an\u00b7zu\u00b7flehn", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVIZU", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar dank' ich viel dem wohlgelaunten Gl\u00fccke,", "tokens": ["Zwar", "dank'", "ich", "viel", "dem", "wohl\u00b7ge\u00b7laun\u00b7ten", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dem ich mehr, als ich verdient, empfangen,", "tokens": ["Von", "dem", "ich", "mehr", ",", "als", "ich", "ver\u00b7dient", ",", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "$,", "KOUS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nichts, wodurch ich meinen Freund entz\u00fccke:", "tokens": ["Doch", "nichts", ",", "wo\u00b7durch", "ich", "mei\u00b7nen", "Freund", "ent\u00b7z\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer aber g\u00e4be mir die vollen Wangen", "tokens": ["Wer", "a\u00b7ber", "g\u00e4\u00b7be", "mir", "die", "vol\u00b7len", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ersten Jugend und den Glanz zur\u00fccke,", "tokens": ["Der", "ers\u00b7ten", "Ju\u00b7gend", "und", "den", "Glanz", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Woran allein der Menschen Blicke hangen?", "tokens": ["Wo\u00b7ran", "al\u00b7lein", "der", "Men\u00b7schen", "Bli\u00b7cke", "han\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}