{"textgrid.poem.31733": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das Hungerlied", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verehrter Herr und K\u00f6nig,", "tokens": ["Ver\u00b7ehr\u00b7ter", "Herr", "und", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00dft du die schlimme Geschicht?", "tokens": ["Wei\u00dft", "du", "die", "schlim\u00b7me", "Ge\u00b7schicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Am Montag a\u00dfen wir wenig,", "tokens": ["Am", "Mon\u00b7tag", "a\u00b7\u00dfen", "wir", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und am Dienstag a\u00dfen wir nicht.", "tokens": ["Und", "am", "Diens\u00b7tag", "a\u00b7\u00dfen", "wir", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und am Mittwoch mu\u00dften wir darben,", "tokens": ["Und", "am", "Mitt\u00b7woch", "mu\u00df\u00b7ten", "wir", "dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "PPER", "PAV", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und am Donnerstag litten wir Not;", "tokens": ["Und", "am", "Don\u00b7ners\u00b7tag", "lit\u00b7ten", "wir", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und ach, am Freitag starben", "tokens": ["Und", "ach", ",", "am", "Frei\u00b7tag", "star\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir fast den Hungertod!", "tokens": ["Wir", "fast", "den", "Hun\u00b7ger\u00b7tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum la\u00df am Samstag backen", "tokens": ["Drum", "la\u00df", "am", "Sams\u00b7tag", "ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Brot, fein s\u00e4uberlich \u2013", "tokens": ["Das", "Brot", ",", "fein", "s\u00e4u\u00b7ber\u00b7lich", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst werden wir sonntags packen", "tokens": ["Sonst", "wer\u00b7den", "wir", "sonn\u00b7tags", "pa\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und fressen, o K\u00f6nig, dich!", "tokens": ["Und", "fres\u00b7sen", ",", "o", "K\u00f6\u00b7nig", ",", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "FM", "NN", "$,", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"