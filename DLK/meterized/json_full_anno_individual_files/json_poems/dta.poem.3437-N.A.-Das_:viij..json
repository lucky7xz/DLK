{"dta.poem.3437": {"metadata": {"author": {"name": "N.A.", "birth": "N.A.", "death": "N.A."}, "title": "Das\u00a0:viij.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1497", "urn": "urn:nbn:de:kobv:b4-30121-8", "language": ["de:0.99"], "booktitle": "Des Edlen Ritter Morgeners Walfart. Erfurt, 1497."}, "poem": {"stanza.1": {"line.1": {"text": "Er sprach ach kammerer traut gesell", "tokens": ["Er", "sprach", "ach", "kam\u00b7me\u00b7rer", "traut", "ge\u00b7sell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVFIN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "du oller liebster diener mein", "tokens": ["du", "ol\u00b7ler", "liebs\u00b7ter", "die\u00b7ner", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wolst du nun thun auch was ich well.", "tokens": ["wolst", "du", "nun", "thun", "auch", "was", "ich", "well", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "ADV", "PWS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so pfleg du selbs der frawen dein", "tokens": ["so", "pfleg", "du", "selbs", "der", "fra\u00b7wen", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ich gelobe dir auch f\u00fcr war", "tokens": ["ich", "ge\u00b7lo\u00b7be", "dir", "auch", "f\u00fcr", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "VAFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "kum ich her wider heim zu land", "tokens": ["kum", "ich", "her", "wi\u00b7der", "heim", "zu", "land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ich begab dich wol sycherlichen zwar", "tokens": ["ich", "be\u00b7gab", "dich", "wol", "sy\u00b7cher\u00b7li\u00b7chen", "zwar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "ADV"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}