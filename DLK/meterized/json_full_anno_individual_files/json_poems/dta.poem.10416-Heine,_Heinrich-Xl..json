{"dta.poem.10416": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ein J\u00fcngling liebt ein M\u00e4dchen,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "liebt", "ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hat einen Andern erw\u00e4hlt;", "tokens": ["Die", "hat", "ei\u00b7nen", "An\u00b7dern", "er\u00b7w\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der Andre liebt eine Andre,", "tokens": ["Der", "And\u00b7re", "liebt", "ei\u00b7ne", "And\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und hat sich mit dieser verm\u00e4hlt.", "tokens": ["Und", "hat", "sich", "mit", "die\u00b7ser", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "APPR", "PDAT", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4dchen heurathet aus Aerger", "tokens": ["Das", "M\u00e4d\u00b7chen", "heu\u00b7ra\u00b7thet", "aus", "A\u00b7er\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den ersten besten Mann,", "tokens": ["Den", "ers\u00b7ten", "bes\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ihr in den Weg gelaufen;", "tokens": ["Der", "ihr", "in", "den", "Weg", "ge\u00b7lau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der J\u00fcngling ist \u00fcbel dran.", "tokens": ["Der", "J\u00fcng\u00b7ling", "ist", "\u00fc\u00b7bel", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist eine alte Geschichte,", "tokens": ["Es", "ist", "ei\u00b7ne", "al\u00b7te", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Doch bleibt sie immer neu;", "tokens": ["Doch", "bleibt", "sie", "im\u00b7mer", "neu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wem sie just passiret,", "tokens": ["Und", "wem", "sie", "just", "pas\u00b7si\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem bricht das Herz entzwei.", "tokens": ["Dem", "bricht", "das", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}