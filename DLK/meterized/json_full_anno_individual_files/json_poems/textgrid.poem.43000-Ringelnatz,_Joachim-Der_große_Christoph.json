{"textgrid.poem.43000": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der gro\u00dfe Christoph", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Rigas Hafen kennt,", "tokens": ["Wer", "Ri\u00b7gas", "Ha\u00b7fen", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kennt auch das Holzmonument,", "tokens": ["Kennt", "auch", "das", "Holz\u00b7mo\u00b7nu\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Das man den gro\u00dfen Christoph nennt.", "tokens": ["Das", "man", "den", "gro\u00b7\u00dfen", "Chris\u00b7toph", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Heilige mit seinem Wanderstabe.", "tokens": ["Der", "Hei\u00b7li\u00b7ge", "mit", "sei\u00b7nem", "Wan\u00b7der\u00b7sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf seiner Schulter sitzt der Jesusknabe.", "tokens": ["Auf", "sei\u00b7ner", "Schul\u00b7ter", "sitzt", "der", "Je\u00b7sus\u00b7kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den hat er, wie die Leute dort sagen,", "tokens": ["Den", "hat", "er", ",", "wie", "die", "Leu\u00b7te", "dort", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch die D\u00fcna getragen.", "tokens": ["Durch", "die", "D\u00fc\u00b7na", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Die Fl\u00f6\u00dfer und die Schiffersleute schenken", "tokens": ["Die", "Fl\u00f6\u00b7\u00dfer", "und", "die", "Schif\u00b7fers\u00b7leu\u00b7te", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm Blumen, B\u00e4nder hin und andrerlei", "tokens": ["Ihm", "Blu\u00b7men", ",", "B\u00e4n\u00b7der", "hin", "und", "an\u00b7drer\u00b7lei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "NN", "PTKVZ", "KON", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und bitten fr\u00f6mmig ihn dabei,", "tokens": ["Und", "bit\u00b7ten", "fr\u00f6m\u00b7mig", "ihn", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er m\u00f6ge dies und das zum Guten lenken.", "tokens": ["Er", "m\u00f6\u00b7ge", "dies", "und", "das", "zum", "Gu\u00b7ten", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "KON", "ART", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es kommen viele Leute so und gehn.", "tokens": ["Es", "kom\u00b7men", "vie\u00b7le", "Leu\u00b7te", "so", "und", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Christoph tr\u00e4gt um seine Lenden", "tokens": ["Der", "Chris\u00b7toph", "tr\u00e4gt", "um", "sei\u00b7ne", "Len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Hemd, vier Hemden, manchmal zehn,", "tokens": ["Ein", "Hemd", ",", "vier", "Hem\u00b7den", ",", "manch\u00b7mal", "zehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "NN", "$,", "ADV", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So je nachdem, was sie ihm spenden", "tokens": ["So", "je", "nach\u00b7dem", ",", "was", "sie", "ihm", "spen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "$,", "PRELS", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und andermal auch wieder stehlen.", "tokens": ["Und", "an\u00b7der\u00b7mal", "auch", "wie\u00b7der", "steh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er tr\u00e4gt und gibt das Gerngewollte.", "tokens": ["Er", "tr\u00e4gt", "und", "gibt", "das", "Gern\u00b7ge\u00b7woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Christus schweigt; er ist ja noch so klein,", "tokens": ["Und", "Chris\u00b7tus", "schweigt", ";", "er", "ist", "ja", "noch", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und beide l\u00e4cheln ob der simplen Seelen.", "tokens": ["Und", "bei\u00b7de", "l\u00e4\u00b7cheln", "ob", "der", "sim\u00b7plen", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn sie wirklich etwas wurmen sollte,", "tokens": ["Und", "wenn", "sie", "wirk\u00b7lich", "et\u00b7was", "wur\u00b7men", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann kann das nur ein Holzwurm sein.", "tokens": ["Dann", "kann", "das", "nur", "ein", "Holz\u00b7wurm", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDS", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer Rigas Hafen kennt,", "tokens": ["Wer", "Ri\u00b7gas", "Ha\u00b7fen", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kennt auch das Holzmonument,", "tokens": ["Kennt", "auch", "das", "Holz\u00b7mo\u00b7nu\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter