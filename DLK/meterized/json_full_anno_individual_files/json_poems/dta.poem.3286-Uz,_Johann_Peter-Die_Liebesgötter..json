{"dta.poem.3286": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebesg\u00f6tter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.71", "cy:0.28"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Cypris, meiner Phyllis gleich,", "tokens": ["Cyp\u00b7ris", ",", "mei\u00b7ner", "Phyl\u00b7lis", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sa\u00df von Grazien umgeben!", "tokens": ["Sa\u00df", "von", "Gra\u00b7zi\u00b7en", "um\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich sah ihr frohes Reich;", "tokens": ["Denn", "ich", "sah", "ihr", "fro\u00b7hes", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich berauschten Cyperns Reben.", "tokens": ["Mich", "be\u00b7rauschten", "Cy\u00b7perns", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ein geweihter Myrthenwald,", "tokens": ["Ein", "ge\u00b7weih\u00b7ter", "Myr\u00b7then\u00b7wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den geheime Schatten schw\u00e4rzten,", "tokens": ["Den", "ge\u00b7hei\u00b7me", "Schat\u00b7ten", "schw\u00e4rz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "War der G\u00f6ttinn Aufenthalt,", "tokens": ["War", "der", "G\u00f6t\u00b7tinn", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo die Liebesg\u00f6tter scherzten.", "tokens": ["Wo", "die", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "scherz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Viele giengen Paar bey Paar:", "tokens": ["Vie\u00b7le", "gien\u00b7gen", "Paar", "bey", "Paar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre sungen, die ich kannte,", "tokens": ["And\u00b7re", "sun\u00b7gen", ",", "die", "ich", "kann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Auge schalkhast war,", "tokens": ["De\u00b7ren", "Au\u00b7ge", "schalk\u00b7hast", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und voll schlauer Wollust brannte.", "tokens": ["Und", "voll", "schlau\u00b7er", "Wol\u00b7lust", "brann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Viele flogen r\u00fcstig aus,", "tokens": ["Vie\u00b7le", "flo\u00b7gen", "r\u00fcs\u00b7tig", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Bogen in der Rechten.", "tokens": ["Mit", "dem", "Bo\u00b7gen", "in", "der", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Viele waren nicht zu Haus;", "tokens": ["Vie\u00b7le", "wa\u00b7ren", "nicht", "zu", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil sie bey Ly\u00e4en zechten.", "tokens": ["Weil", "sie", "bey", "Ly\u00b7\u00e4\u00b7en", "zech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Der voll bl\u00f6der Unschuld schien,", "tokens": ["Der", "voll", "bl\u00f6\u00b7der", "Un\u00b7schuld", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrscht auf stillen Sch\u00e4ferauen.", "tokens": ["Herrscht", "auf", "stil\u00b7len", "Sch\u00e4\u00b7fe\u00b7rau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuerreich, verschwiegen, k\u00fchn", "tokens": ["Feu\u00b7er\u00b7reich", ",", "ver\u00b7schwie\u00b7gen", ",", "k\u00fchn"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADJD", "$,", "VVPP", "$,", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah der Liebling junger Frauen.", "tokens": ["Sah", "der", "Lieb\u00b7ling", "jun\u00b7ger", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, erm\u00fcdet hingekr\u00fcmmt,", "tokens": ["Doch", ",", "er\u00b7m\u00fc\u00b7det", "hin\u00b7ge\u00b7kr\u00fcmmt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlief der Liebesgott der Ehen:", "tokens": ["Schlief", "der", "Lie\u00b7bes\u00b7gott", "der", "E\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu Ly\u00e4en hie\u00df, ergrimmt,", "tokens": ["Zu", "Ly\u00b7\u00e4\u00b7en", "hie\u00df", ",", "er\u00b7grimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Venus diesen Schl\u00e4ser gehen.", "tokens": ["Ve\u00b7nus", "die\u00b7sen", "Schl\u00e4\u00b7ser", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unter gr\u00fcner B\u00fcsche Nacht,", "tokens": ["Un\u00b7ter", "gr\u00fc\u00b7ner", "B\u00fc\u00b7sche", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter abgelegnen Str\u00e4uchen,", "tokens": ["Un\u00b7ter", "ab\u00b7ge\u00b7leg\u00b7nen", "Str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo so manche Nymphe lacht,", "tokens": ["Wo", "so", "man\u00b7che", "Nym\u00b7phe", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah ich sie am liebsten schleichen.", "tokens": ["Sah", "ich", "sie", "am", "liebs\u00b7ten", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Viele flohn mit leichtem Fu\u00df", "tokens": ["Vie\u00b7le", "flohn", "mit", "leich\u00b7tem", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Allen Zwang bethr\u00e4nter Ketten,", "tokens": ["Al\u00b7len", "Zwang", "be\u00b7thr\u00e4n\u00b7ter", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Flatterten von Ku\u00df zu Ku\u00df", "tokens": ["Flat\u00b7ter\u00b7ten", "von", "Ku\u00df", "zu", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Und von Blonden zu Brunetten.", "tokens": ["Und", "von", "Blon\u00b7den", "zu", "Bru\u00b7net\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Kleine G\u00f6tter voller List,", "tokens": ["Klei\u00b7ne", "G\u00f6t\u00b7ter", "vol\u00b7ler", "List", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren Pfeil kein Herz verfehlet,", "tokens": ["De\u00b7ren", "Pfeil", "kein", "Herz", "ver\u00b7feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vom Necktar trunken ist,", "tokens": ["Und", "vom", "Neck\u00b7tar", "trun\u00b7ken", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob er gleich die Thoren qu\u00e4let:", "tokens": ["Ob", "er", "gleich", "die", "Tho\u00b7ren", "qu\u00e4\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleibt auf meinen Ruf bereit,", "tokens": ["Bleibt", "auf", "mei\u00b7nen", "Ruf", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Jugend froh zu machen!", "tokens": ["Mei\u00b7ne", "Ju\u00b7gend", "froh", "zu", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Jugend Fr\u00fchlingszeit", "tokens": ["In", "der", "Ju\u00b7gend", "Fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fcnsch ich unter euch zu lachen.", "tokens": ["W\u00fcnsch", "ich", "un\u00b7ter", "euch", "zu", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}