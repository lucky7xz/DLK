{"dta.poem.14996": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Abschied vom Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchl's an meines Herzens matterm Schlage,", "tokens": ["Ich", "f\u00fchl's", "an", "mei\u00b7nes", "Her\u00b7zens", "mat\u00b7term", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier steh ich an den Marken meiner Tage. \u2014", "tokens": ["Hier", "steh", "ich", "an", "den", "Mar\u00b7ken", "mei\u00b7ner", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gott, wie du willst, dir hab' ich mich ergeben. \u2014", "tokens": ["Gott", ",", "wie", "du", "willst", ",", "dir", "hab'", "ich", "mich", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "PPER", "PRF", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Viel goldne Bilder sah ich um mich schweben,", "tokens": ["Viel", "gold\u00b7ne", "Bil\u00b7der", "sah", "ich", "um", "mich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das sch\u00f6ne Traumlied wird zur Todtenklage! \u2014", "tokens": ["Das", "sch\u00f6\u00b7ne", "Traum\u00b7lied", "wird", "zur", "Tod\u00b7ten\u00b7kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Muth! Muth! \u2014 Was ich so treu im Herzen trage,", "tokens": ["Muth", "!", "Muth", "!", "Was", "ich", "so", "treu", "im", "Her\u00b7zen", "tra\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "$(", "PWS", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Das mu\u00df ja doch dort ewig mit mir leben! \u2014", "tokens": ["Das", "mu\u00df", "ja", "doch", "dort", "e\u00b7wig", "mit", "mir", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ADV", "ADJD", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und was ich hier als Heiligthum erkannte,", "tokens": ["Und", "was", "ich", "hier", "als", "Hei\u00b7lig\u00b7thum", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wof\u00fcr ich rasch und jugendlich entbrannte,", "tokens": ["Wo\u00b7f\u00fcr", "ich", "rasch", "und", "ju\u00b7gend\u00b7lich", "ent\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ob ich's nun Freiheit, ob ich's Liebe nannte,", "tokens": ["Ob", "ich's", "nun", "Frei\u00b7heit", ",", "ob", "ich's", "Lie\u00b7be", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "NN", "$,", "KOUS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als leichter Seraph seh ichs vor mir stehen, \u2014", "tokens": ["Als", "leich\u00b7ter", "Se\u00b7raph", "seh", "ichs", "vor", "mir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PIS", "APPR", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und wie die Sinne langsam mir vergehen,", "tokens": ["Und", "wie", "die", "Sin\u00b7ne", "lang\u00b7sam", "mir", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Tr\u00e4gt mich ein Hauch zu morgenrothen H\u00f6hen.", "tokens": ["Tr\u00e4gt", "mich", "ein", "Hauch", "zu", "mor\u00b7gen\u00b7ro\u00b7then", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}