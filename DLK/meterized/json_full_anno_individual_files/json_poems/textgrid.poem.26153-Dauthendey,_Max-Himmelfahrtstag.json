{"textgrid.poem.26153": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Himmelfahrtstag", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemals ich je in einen andern Himmel mag", "tokens": ["Nie\u00b7mals", "ich", "je", "in", "ei\u00b7nen", "an\u00b7dern", "Him\u00b7mel", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als den, in dem ich immer selig lag,", "tokens": ["Als", "den", ",", "in", "dem", "ich", "im\u00b7mer", "se\u00b7lig", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "APPR", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In deinem Arm, wo alle Erde still", "tokens": ["In", "dei\u00b7nem", "Arm", ",", "wo", "al\u00b7le", "Er\u00b7de", "still"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu deinen F\u00fc\u00dfen lehnt und nichts mehr will.", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "lehnt", "und", "nichts", "mehr", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "PIS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein Haar mit seiner wogenden Geb\u00e4rde,", "tokens": ["Dein", "Haar", "mit", "sei\u00b7ner", "wo\u00b7gen\u00b7den", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein Aug' mit seiner Lichterschaar", "tokens": ["Dein", "Aug'", "mit", "sei\u00b7ner", "Lich\u00b7ter\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und deine Brust, an der ich wunschlos werde,", "tokens": ["Und", "dei\u00b7ne", "Brust", ",", "an", "der", "ich", "wun\u00b7schlos", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie aller Himmel allerh\u00f6chste Lust mir sind.", "tokens": ["Sie", "al\u00b7ler", "Him\u00b7mel", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Lust", "mir", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJA", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Lieb' ist die Himmelfahrt f\u00fcr jedes Erdenkind.", "tokens": ["Lieb'", "ist", "die", "Him\u00b7mel\u00b7fahrt", "f\u00fcr", "je\u00b7des", "Er\u00b7den\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Niemals ich je in einen andern Himmel mag", "tokens": ["Nie\u00b7mals", "ich", "je", "in", "ei\u00b7nen", "an\u00b7dern", "Him\u00b7mel", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "VMFIN"]