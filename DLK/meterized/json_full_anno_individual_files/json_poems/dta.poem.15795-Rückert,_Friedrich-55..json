{"dta.poem.15795": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was du nicht lieben kannst, mu\u00dft du darum nicht hassen;", "tokens": ["Was", "du", "nicht", "lie\u00b7ben", "kannst", ",", "mu\u00dft", "du", "da\u00b7rum", "nicht", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erkl\u00e4ren wird es sich, entschuldigen sich lassen.", "tokens": ["Er\u00b7kl\u00e4\u00b7ren", "wird", "es", "sich", ",", "ent\u00b7schul\u00b7di\u00b7gen", "sich", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "$,", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Alter hats gethan, der Jugend Feuer d\u00e4mpfend;", "tokens": ["Das", "Al\u00b7ter", "hats", "ge\u00b7than", ",", "der", "Ju\u00b7gend", "Feu\u00b7er", "d\u00e4mp\u00b7fend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der \u00e4u\u00dfre Stand, mit Zwang den innern Schwung bek\u00e4mpfend;", "tokens": ["Der", "\u00e4u\u00df\u00b7re", "Stand", ",", "mit", "Zwang", "den", "in\u00b7nern", "Schwung", "be\u00b7k\u00e4mp\u00b7fend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein schwacher Augenblick, Homers, des Alten, Nicken.", "tokens": ["Ein", "schwa\u00b7cher", "Au\u00b7gen\u00b7blick", ",", "Ho\u00b7mers", ",", "des", "Al\u00b7ten", ",", "Ni\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "$,", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie? bist du sicher selbst vor schwachen Augenblicken?", "tokens": ["Wie", "?", "bist", "du", "si\u00b7cher", "selbst", "vor", "schwa\u00b7chen", "Au\u00b7gen\u00b7bli\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bist sicher, da\u00df nicht Stand und Umstand dich bedinge,", "tokens": ["Bist", "si\u00b7cher", ",", "da\u00df", "nicht", "Stand", "und", "Um\u00b7stand", "dich", "be\u00b7din\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PTKNEG", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch dir des Alters Frost ans innre Leben dringe?", "tokens": ["Auch", "dir", "des", "Al\u00b7ters", "Frost", "ans", "inn\u00b7re", "Le\u00b7ben", "drin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum, nicht fehlloser, halt' auch einen Fehl zu gut!", "tokens": ["Drum", ",", "nicht", "fehl\u00b7lo\u00b7ser", ",", "halt'", "auch", "ei\u00b7nen", "Fehl", "zu", "gut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PTKNEG", "ADJD", "$,", "VVFIN", "ADV", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Eigenliebe thuts, wer nicht aus Lieb' es thut.", "tokens": ["Aus", "Ei\u00b7gen\u00b7lie\u00b7be", "thuts", ",", "wer", "nicht", "aus", "Lieb'", "es", "thut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWS", "PTKNEG", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sich selbst entw\u00fcrdigt, wer ehrw\u00fcrdiges vernichtet,", "tokens": ["Sich", "selbst", "ent\u00b7w\u00fcr\u00b7digt", ",", "wer", "ehr\u00b7w\u00fcr\u00b7di\u00b7ges", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,", "PWS", "ADJA", "VVPP", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Der Menschheit Stolz und Lust mit Lust unmenschlich richtet.", "tokens": ["Der", "Menschheit", "Stolz", "und", "Lust", "mit", "Lust", "un\u00b7menschlich", "rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-++-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}