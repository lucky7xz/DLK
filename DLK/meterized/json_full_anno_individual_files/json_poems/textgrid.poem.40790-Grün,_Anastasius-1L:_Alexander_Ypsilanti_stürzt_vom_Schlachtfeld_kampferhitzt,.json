{"textgrid.poem.40790": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alexander Ypsilanti st\u00fcrzt vom Schlachtfeld kampferhitzt,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alexander Ypsilanti st\u00fcrzt vom Schlachtfeld kampferhitzt,", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "Yp\u00b7si\u00b7lan\u00b7ti", "st\u00fcrzt", "vom", "Schlacht\u00b7feld", "kamp\u00b7fer\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wo die Freiheit ihres Blutes letzten Tropfen hat verspritzt,", "tokens": ["Wo", "die", "Frei\u00b7heit", "ih\u00b7res", "Blu\u00b7tes", "letz\u00b7ten", "Trop\u00b7fen", "hat", "ver\u00b7spritzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wo er einen hohen Orden sich gewonnen, unbewu\u00dft,", "tokens": ["Wo", "er", "ei\u00b7nen", "ho\u00b7hen", "Or\u00b7den", "sich", "ge\u00b7won\u00b7nen", ",", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "PRF", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Eine sch\u00f6ne Heldenwunde, klaffend vorn an seiner Brust.", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Hel\u00b7den\u00b7wun\u00b7de", ",", "klaf\u00b7fend", "vorn", "an", "sei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "So mit stolzer Purpurrose seinen Busen ausgeschm\u00fcckt,", "tokens": ["So", "mit", "stol\u00b7zer", "Pur\u00b7pur\u00b7ro\u00b7se", "sei\u00b7nen", "Bu\u00b7sen", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "In der Hand den Stumpf des Schwertes, kampfzerbrochen und zerst\u00fcckt", "tokens": ["In", "der", "Hand", "den", "Stumpf", "des", "Schwer\u00b7tes", ",", "kampf\u00b7zer\u00b7bro\u00b7chen", "und", "zer\u00b7st\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Tritt der Held auf Oestreichs Boden, \u2013 o betr\u00e4t' er ihn doch nicht!", "tokens": ["Tritt", "der", "Held", "auf", "O\u00b7e\u00b7streichs", "Bo\u00b7den", ",", "\u2013", "o", "be\u00b7tr\u00e4t'", "er", "ihn", "doch", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,", "$(", "FM", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Beut vertrauend uns die H\u00e4nde, tritt an unsern Herd und spricht:", "tokens": ["Beut", "ver\u00b7trau\u00b7end", "uns", "die", "H\u00e4n\u00b7de", ",", "tritt", "an", "un\u00b7sern", "Herd", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbwenig ist's, darum ich flehe! Gebt mir Linnen zum Verband,", "tokens": ["\u00bb", "we\u00b7nig", "ist's", ",", "da\u00b7rum", "ich", "fle\u00b7he", "!", "Gebt", "mir", "Lin\u00b7nen", "zum", "Ver\u00b7band", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "PAV", "PPER", "VVFIN", "$.", "VVIMP", "PPER", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "La\u00dft an eurer Luft mich laben, und erfreu'n an eurem Land!\u00ab", "tokens": ["La\u00dft", "an", "eu\u00b7rer", "Luft", "mich", "la\u00b7ben", ",", "und", "er\u00b7freu'n", "an", "eu\u00b7rem", "Land", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "M\u00e4cht'ger als der Mund des Gastes spricht sein rinnend Heldenblut!", "tokens": ["M\u00e4cht'\u00b7ger", "als", "der", "Mund", "des", "Gas\u00b7tes", "spricht", "sein", "rin\u00b7nend", "Hel\u00b7den\u00b7blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und sie hei\u00dfen ihn willkommen, und zu bleiben wohlgemuth:", "tokens": ["Und", "sie", "hei\u00b7\u00dfen", "ihn", "will\u00b7kom\u00b7men", ",", "und", "zu", "blei\u00b7ben", "wohl\u00b7ge\u00b7muth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "PTKZU", "VVINF", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbmunkats ist ein h\u00fcbsches Schl\u00f6\u00dflein, Luft und Aussicht sch\u00f6n und rein!", "tokens": ["\u00bb", "mun\u00b7kats", "ist", "ein", "h\u00fcb\u00b7sches", "Schl\u00f6\u00df\u00b7lein", ",", "Luft", "und", "Aus\u00b7sicht", "sch\u00f6n", "und", "rein", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nur beschr\u00e4nkt euch noch einstweilen auf ein einz'ges Fensterlein;", "tokens": ["Nur", "be\u00b7schr\u00e4nkt", "euch", "noch", "einst\u00b7wei\u00b7len", "auf", "ein", "einz'\u00b7ges", "Fens\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "An Verband soll's auch nicht fehlen, der wohl fest und gut euch pa\u00dft,", "tokens": ["An", "Ver\u00b7band", "soll's", "auch", "nicht", "feh\u00b7len", ",", "der", "wohl", "fest", "und", "gut", "euch", "pa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,", "PRELS", "ADV", "PTKVZ", "KON", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Scheint er auch zu sein von Eisen, gleicht er auch den Ketten fast.\u00ab \u2013", "tokens": ["Scheint", "er", "auch", "zu", "sein", "von", "Ei\u00b7sen", ",", "gleicht", "er", "auch", "den", "Ket\u00b7ten", "fast", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VAINF", "APPR", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Durch sein Gitterfenster nieder blickt der Griechenheld aufs Land,", "tokens": ["Durch", "sein", "Git\u00b7ter\u00b7fens\u00b7ter", "nie\u00b7der", "blickt", "der", "Grie\u00b7chen\u00b7held", "aufs", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Das in schwelgerischer F\u00fclle zaubervollen Lenzes stand:", "tokens": ["Das", "in", "schwel\u00b7ge\u00b7ri\u00b7scher", "F\u00fcl\u00b7le", "zau\u00b7ber\u00b7vol\u00b7len", "Len\u00b7zes", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "\u00bbo wie k\u00f6nnen Rosen duften, Saat und Frucht noch schwellen dicht,", "tokens": ["\u00bb", "o", "wie", "k\u00f6n\u00b7nen", "Ro\u00b7sen", "duf\u00b7ten", ",", "Saat", "und", "Frucht", "noch", "schwel\u00b7len", "dicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "KON", "ADJA", "NN", "VVFIN", "$,", "NN", "KON", "NN", "ADV", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Saft'ge Reben lockend winken, wo des Gastes Recht man bricht?\u00ab \u2013", "tokens": ["Saft'\u00b7ge", "Re\u00b7ben", "lo\u00b7ckend", "win\u00b7ken", ",", "wo", "des", "Gas\u00b7tes", "Recht", "man", "bricht", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "VVPP", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "PIS", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Sieben lange Jahr' in Ketten dort der Leu aus Hellas lag.", "tokens": ["Sie\u00b7ben", "lan\u00b7ge", "Jahr'", "in", "Ket\u00b7ten", "dort", "der", "Leu", "aus", "Hel\u00b7las", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "APPR", "NN", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sieh, nun l\u00f6st man sie, da\u00df wieder frei mit uns er wandeln mag!", "tokens": ["Sieh", ",", "nun", "l\u00f6st", "man", "sie", ",", "da\u00df", "wie\u00b7der", "frei", "mit", "uns", "er", "wan\u00b7deln", "mag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PIS", "PPER", "$,", "KOUS", "ADV", "ADJD", "APPR", "PPER", "PPER", "VVFIN", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber kaum nach sieben Tagen brach der Tod sein Herz entzwei!", "tokens": ["A\u00b7ber", "kaum", "nach", "sie\u00b7ben", "Ta\u00b7gen", "brach", "der", "Tod", "sein", "Herz", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "CARD", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Traun, mich d\u00fcnkt, da\u00df er gestorben wohl an unsrer Freiheit sei!", "tokens": ["Traun", ",", "mich", "d\u00fcnkt", ",", "da\u00df", "er", "ge\u00b7stor\u00b7ben", "wohl", "an", "uns\u00b7rer", "Frei\u00b7heit", "sei", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Alexander Ypsilanti st\u00fcrzt vom Schlachtfeld kampferhitzt,", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "Yp\u00b7si\u00b7lan\u00b7ti", "st\u00fcrzt", "vom", "Schlacht\u00b7feld", "kamp\u00b7fer\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wo die Freiheit ihres Blutes letzten Tropfen hat verspritzt,", "tokens": ["Wo", "die", "Frei\u00b7heit", "ih\u00b7res", "Blu\u00b7tes", "letz\u00b7ten", "Trop\u00b7fen", "hat", "ver\u00b7spritzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wo er einen hohen Orden sich gewonnen, unbewu\u00dft,", "tokens": ["Wo", "er", "ei\u00b7nen", "ho\u00b7hen", "Or\u00b7den", "sich", "ge\u00b7won\u00b7nen", ",", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "pu