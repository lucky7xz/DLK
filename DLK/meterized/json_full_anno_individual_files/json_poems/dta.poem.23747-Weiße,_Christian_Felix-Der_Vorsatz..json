{"dta.poem.23747": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Vorsatz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich jung bin, soll mein Flei\u00df", "tokens": ["Weil", "ich", "jung", "bin", ",", "soll", "mein", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eifrig sich bestreben,", "tokens": ["Eif\u00b7rig", "sich", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ich m\u00f6g einst, als ein Greis,", "tokens": ["Da\u00df", "ich", "m\u00f6g", "einst", ",", "als", "ein", "Greis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Recht zufrieden leben.", "tokens": ["Recht", "zu\u00b7frie\u00b7den", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar will ich mich jugendlich", "tokens": ["Zwar", "will", "ich", "mich", "ju\u00b7gend\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Meiner Tage freuen;", "tokens": ["Mei\u00b7ner", "Ta\u00b7ge", "freu\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch nie also, da\u00df es mich", "tokens": ["Doch", "nie", "al\u00b7so", ",", "da\u00df", "es", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf im Alter reuen.", "tokens": ["Darf", "im", "Al\u00b7ter", "reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}