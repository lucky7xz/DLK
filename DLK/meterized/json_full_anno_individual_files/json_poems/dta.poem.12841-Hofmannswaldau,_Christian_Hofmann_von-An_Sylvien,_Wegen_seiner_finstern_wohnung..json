{"dta.poem.12841": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien,  \n Wegen seiner finstern wohnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gebe gerne zu, da\u00df in dein nettes zimmer,", "tokens": ["Ich", "ge\u00b7be", "ger\u00b7ne", "zu", ",", "da\u00df", "in", "dein", "net\u00b7tes", "zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Sylvia! mehr licht, als in das meine, f\u00e4llt;", "tokens": ["O", "Syl\u00b7via", "!", "mehr", "licht", ",", "als", "in", "das", "mei\u00b7ne", ",", "f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$.", "PIAT", "NN", "$,", "KOUS", "APPR", "PRELS", "PPOSAT", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Denn dieses kennt nicht mehr, als einer sonne schimmer,", "tokens": ["Denn", "die\u00b7ses", "kennt", "nicht", "mehr", ",", "als", "ei\u00b7ner", "son\u00b7ne", "schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da deines meistentheils drey sonnen in sich h\u00e4lt.", "tokens": ["Da", "dei\u00b7nes", "meis\u00b7ten\u00b7theils", "drey", "son\u00b7nen", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "CARD", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}