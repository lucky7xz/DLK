{"textgrid.poem.55642": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[das Leben ist ein schlechter Spa\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist ein schlechter Spa\u00df,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "schlech\u00b7ter", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem fehlt's an Dies, dem fehlt's an Das,", "tokens": ["Dem", "fehlt's", "an", "Dies", ",", "dem", "fehlt's", "an", "Das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PDS", "$,", "PRELS", "VVFIN", "APPR", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der will nicht wenig, der zuviel,", "tokens": ["Der", "will", "nicht", "we\u00b7nig", ",", "der", "zu\u00b7viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "ADV", "$,", "PRELS", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Kann und Gl\u00fcck kommt auch ins Spiel.", "tokens": ["Und", "Kann", "und", "Gl\u00fcck", "kommt", "auch", "ins", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KON", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hat sich's Ungl\u00fcck drein gelegt,", "tokens": ["Und", "hat", "sich's", "Un\u00b7gl\u00fcck", "drein", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jeder, wie er nicht wollte, tr\u00e4gt.", "tokens": ["Je\u00b7der", ",", "wie", "er", "nicht", "woll\u00b7te", ",", "tr\u00e4gt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "PTKNEG", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis endlich Erben mit Behagen", "tokens": ["Bis", "end\u00b7lich", "Er\u00b7ben", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Herrn Kannicht-Willnicht weiter tragen.", "tokens": ["Herrn", "Kan\u00b7nicht\u00b7Will\u00b7nicht", "wei\u00b7ter", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Leben ist ein schlechter Spa\u00df,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "schlech\u00b7ter", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem fehlt's an Dies, dem fehlt's an Das,", "tokens": ["Dem", "fehlt's", "an", "Dies", ",", "dem", "fehlt's", "an", "Das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PDS", "$,", "PRELS", "VVFIN", "APPR", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der will nicht wenig, der zuviel,", "tokens": ["Der", "will", "nicht", "we\u00b7nig", ",", "der", "zu\u00b7viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS",