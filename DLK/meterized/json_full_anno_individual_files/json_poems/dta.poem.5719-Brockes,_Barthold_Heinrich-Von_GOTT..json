{"dta.poem.5719": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Lasst uns die Geister nur, die nicht zu biegen seyn,", "tokens": ["Lasst", "uns", "die", "Geis\u00b7ter", "nur", ",", "die", "nicht", "zu", "bie\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PTKNEG", "PTKZU", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In ihrem sch\u00e4ndlichen und groben Jrrthum lassen!", "tokens": ["In", "ih\u00b7rem", "sch\u00e4nd\u00b7li\u00b7chen", "und", "gro\u00b7ben", "Jrr\u00b7thum", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir k\u00f6nnen unsern Grund durch andre Proben fassen.", "tokens": ["Wir", "k\u00f6n\u00b7nen", "un\u00b7sern", "Grund", "durch", "and\u00b7re", "Pro\u00b7ben", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Proben geben uns das ", "tokens": ["Die", "Pro\u00b7ben", "ge\u00b7ben", "uns", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "So durch die Sinne nicht zu fassen ist, zu lesen.", "tokens": ["So", "durch", "die", "Sin\u00b7ne", "nicht", "zu", "fas\u00b7sen", "ist", ",", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir wollen anfangs sehn in Einfalt einerley,", "tokens": ["Wir", "wol\u00b7len", "an\u00b7fangs", "sehn", "in", "Ein\u00b7falt", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was die ", "tokens": ["Was", "die"], "token_info": ["word", "word"], "pos": ["PWS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wenn wir versp\u00fcren,", "tokens": ["Wenn", "wir", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Da\u00df Vorw\u00fcrff\u2019 uns umgeben, die uns r\u00fchren;", "tokens": ["Da\u00df", "Vor\u00b7w\u00fcrff'", "uns", "um\u00b7ge\u00b7ben", ",", "die", "uns", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Was k\u00f6nnen doch f\u00fcr Eigenschafften,", "tokens": ["Was", "k\u00f6n\u00b7nen", "doch", "f\u00fcr", "Ei\u00b7gen\u00b7schaff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Nach unserem Begriff\u2019, an selben hafften,", "tokens": ["Nach", "un\u00b7se\u00b7rem", "Be\u00b7griff'", ",", "an", "sel\u00b7ben", "haff\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wodurch sich in dem Sinn so manche W\u00fcrckung pr\u00e4gt?", "tokens": ["Wo\u00b7durch", "sich", "in", "dem", "Sinn", "so", "man\u00b7che", "W\u00fcr\u00b7ckung", "pr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was k\u00f6nnen wir vom ", "tokens": ["Was", "k\u00f6n\u00b7nen", "wir", "vom"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPRART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "dencken?", "tokens": ["den\u00b7cken", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Als da\u00df sie sind ", "tokens": ["Als", "da\u00df", "sie", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}}}}}