{"textgrid.poem.32650": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei Nektar und Ambrosia", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Nektar und Ambrosia", "tokens": ["Bei", "Nek\u00b7tar", "und", "A\u00b7mbro\u00b7sia"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt Vater Zeus gefoltert da;", "tokens": ["Sitzt", "Va\u00b7ter", "Zeus", "ge\u00b7fol\u00b7tert", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "VVPP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Mutter Juno zankt.", "tokens": ["Denn", "Mut\u00b7ter", "Ju\u00b7no", "zankt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir aber sitzen hier und freun", "tokens": ["Wir", "a\u00b7ber", "sit\u00b7zen", "hier", "und", "freun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns ungest\u00f6rt beim s\u00fc\u00dfen Wein!", "tokens": ["Uns", "un\u00b7ge\u00b7st\u00f6rt", "beim", "s\u00fc\u00b7\u00dfen", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den G\u00f6ttern sei's gedankt!", "tokens": ["Den", "G\u00f6t\u00b7tern", "sei's", "ge\u00b7dankt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch wird durch keine Frau vom Haus", "tokens": ["Noch", "wird", "durch", "kei\u00b7ne", "Frau", "vom", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nektar und der laute Schmaus", "tokens": ["Der", "Nek\u00b7tar", "und", "der", "lau\u00b7te", "Schmaus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns Gl\u00fccklichen verg\u00e4llt.", "tokens": ["Uns", "Gl\u00fcck\u00b7li\u00b7chen", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vom Joch des Ehestandes frei,", "tokens": ["Vom", "Joch", "des", "E\u00b7hes\u00b7tan\u00b7des", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umflattert uns ein steter Mai,", "tokens": ["Um\u00b7flat\u00b7tert", "uns", "ein", "ste\u00b7ter", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und golden ist die Welt.", "tokens": ["Und", "gol\u00b7den", "ist", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer aber wei\u00df, wie bald's geschieht,", "tokens": ["Wer", "a\u00b7ber", "wei\u00df", ",", "wie", "bald's", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "punct", "word", "w