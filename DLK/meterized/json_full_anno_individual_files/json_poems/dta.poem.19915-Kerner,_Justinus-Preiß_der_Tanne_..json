{"dta.poem.19915": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Prei\u00df der Tanne .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngsthin h\u00f6rt' ich wie die Rebe", "tokens": ["J\u00fcng\u00b7sthin", "h\u00f6rt'", "ich", "wie", "die", "Re\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Tanne sprach und schalt:", "tokens": ["Mit", "der", "Tan\u00b7ne", "sprach", "und", "schalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolze! himmelw\u00e4rts dich hebe!", "tokens": ["Stol\u00b7ze", "!", "him\u00b7mel\u00b7w\u00e4rts", "dich", "he\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$.", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch bleibst du starr und kalt!", "tokens": ["Den\u00b7noch", "bleibst", "du", "starr", "und", "kalt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Spend' auch ich nur kargen Schatten", "tokens": ["Spend'", "auch", "ich", "nur", "kar\u00b7gen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wegem\u00fcden, gleich wie du,", "tokens": ["We\u00b7ge\u00b7m\u00fc\u00b7den", ",", "gleich", "wie", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchret doch mein Saft die Matten,", "tokens": ["F\u00fch\u00b7ret", "doch", "mein", "Saft", "die", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie leicht! der Heimat zu.", "tokens": ["O", "wie", "leicht", "!", "der", "Hei\u00b7mat", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$.", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und im Herbste, \u2014 welche Wonne", "tokens": ["Und", "im", "Herbs\u00b7te", ",", "wel\u00b7che", "Won\u00b7ne"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "$(", "PWAT", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Bring' ich in des Menschen Haus!", "tokens": ["Bring'", "ich", "in", "des", "Men\u00b7schen", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaff' ihm eine neue Sonne,", "tokens": ["Schaff'", "ihm", "ei\u00b7ne", "neu\u00b7e", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann die alte l\u00f6schet aus.", "tokens": ["Wann", "die", "al\u00b7te", "l\u00f6\u00b7schet", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So sich br\u00fcstend sprach die Rebe,", "tokens": ["So", "sich", "br\u00fcs\u00b7tend", "sprach", "die", "Re\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Tanne blieb nicht stumm,", "tokens": ["Doch", "die", "Tan\u00b7ne", "blieb", "nicht", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4uselnd sprach sie: gerne gebe", "tokens": ["S\u00e4u\u00b7selnd", "sprach", "sie", ":", "ger\u00b7ne", "ge\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich dir, Rebe, Prei\u00df und Ruhm.", "tokens": ["Ich", "dir", ",", "Re\u00b7be", ",", "Prei\u00df", "und", "Ruhm", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eines doch ist mir beschieden.", "tokens": ["Ei\u00b7nes", "doch", "ist", "mir", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mehr zu laben als dein Wein,", "tokens": ["Mehr", "zu", "la\u00b7ben", "als", "dein", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebensm\u00fcde; \u2014 welchen Frieden", "tokens": ["Le\u00b7bens\u00b7m\u00fc\u00b7de", ";", "wel\u00b7chen", "Frie\u00b7den"], "token_info": ["word", "punct", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "PWAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schlie\u00dfen meine Bretter ein!", "tokens": ["Schlie\u00b7\u00dfen", "mei\u00b7ne", "Bret\u00b7ter", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ob die Rebe sich gefangen", "tokens": ["Ob", "die", "Re\u00b7be", "sich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab der Tanne, wei\u00df ich nicht,", "tokens": ["Gab", "der", "Tan\u00b7ne", ",", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie schwieg, \u2014 und Thr\u00e4nen hangen", "tokens": ["Doch", "sie", "schwieg", ",", "und", "Thr\u00e4\u00b7nen", "han\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "$(", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sah ich ihr am Auge licht.", "tokens": ["Sah", "ich", "ihr", "am", "Au\u00b7ge", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}