{"dta.poem.10415": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Manch Bild vergessener Zeiten", "tokens": ["Manch", "Bild", "ver\u00b7ges\u00b7se\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Steigt auf aus seinem Grab,", "tokens": ["Steigt", "auf", "aus", "sei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zeigt wie in deiner N\u00e4he", "tokens": ["Und", "zeigt", "wie", "in", "dei\u00b7ner", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich einst gelebet hab'.", "tokens": ["Ich", "einst", "ge\u00b7le\u00b7bet", "hab'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Tage schwankte ich tr\u00e4umend", "tokens": ["Am", "Ta\u00b7ge", "schwank\u00b7te", "ich", "tr\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch alle Stra\u00dfen herum;", "tokens": ["Durch", "al\u00b7le", "Stra\u00b7\u00dfen", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Leute verwundert mich ansah'n,", "tokens": ["Die", "Leu\u00b7te", "ver\u00b7wun\u00b7dert", "mich", "an\u00b7sah'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich war so traurig und stumm.", "tokens": ["Ich", "war", "so", "trau\u00b7rig", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Des Nachts da war es besser,", "tokens": ["Des", "Nachts", "da", "war", "es", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da waren die Stra\u00dfen leer;", "tokens": ["Da", "wa\u00b7ren", "die", "Stra\u00b7\u00dfen", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich und mein Schatten selbander,", "tokens": ["Ich", "und", "mein", "Schat\u00b7ten", "sel\u00b7ban\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Wir wandelten schweigend einher.", "tokens": ["Wir", "wan\u00b7del\u00b7ten", "schwei\u00b7gend", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Mit wiederhallendem Fu\u00dftritt", "tokens": ["Mit", "wie\u00b7der\u00b7hal\u00b7len\u00b7dem", "Fu\u00df\u00b7tritt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wandelt ich \u00fcber die Br\u00fcck';", "tokens": ["Wan\u00b7delt", "ich", "\u00fc\u00b7ber", "die", "Br\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Der Mond brach aus den Wolken,", "tokens": ["Der", "Mond", "brach", "aus", "den", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gr\u00fc\u00dfte mit ernstem Blick'.", "tokens": ["Und", "gr\u00fc\u00df\u00b7te", "mit", "erns\u00b7tem", "Blick'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Steh'n blieb ich vor deinem Hause", "tokens": ["Steh'n", "blieb", "ich", "vor", "dei\u00b7nem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und starrte in die H\u00f6h',", "tokens": ["Und", "starr\u00b7te", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und starrte nach deinem Fenster, \u2014", "tokens": ["Und", "starr\u00b7te", "nach", "dei\u00b7nem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Herz that mir so weh'.", "tokens": ["Das", "Herz", "that", "mir", "so", "weh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich wei\u00df du hast aus dem Fenster", "tokens": ["Ich", "wei\u00df", "du", "hast", "aus", "dem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar oft herab geseh'n,", "tokens": ["Gar", "oft", "her\u00b7ab", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sah'st mich im Mondenlichte", "tokens": ["Und", "sah'st", "mich", "im", "Mon\u00b7den\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie eine S\u00e4ule steh'n.", "tokens": ["Wie", "ei\u00b7ne", "S\u00e4u\u00b7le", "steh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}