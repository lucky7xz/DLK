{"dta.poem.16323": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Donner rollt, und spricht, wenn ichs vergessen habe,", "tokens": ["Dein", "Don\u00b7ner", "rollt", ",", "und", "spricht", ",", "wenn", "ichs", "ver\u00b7ges\u00b7sen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "$,", "KOUS", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du seist mein Herr, und ich steh' unter deinem Stabe.", "tokens": ["Du", "seist", "mein", "Herr", ",", "und", "ich", "steh'", "un\u00b7ter", "dei\u00b7nem", "Sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du w\u00e4gst in deiner Hand best\u00e4ndig mein Geschick,", "tokens": ["Du", "w\u00e4gst", "in", "dei\u00b7ner", "Hand", "be\u00b7st\u00e4n\u00b7dig", "mein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch deutlicher f\u00fchl' ichs in diesem Augenblick.", "tokens": ["Doch", "deut\u00b7li\u00b7cher", "f\u00fchl'", "ichs", "in", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "APPR", "PDAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df nicht, was du, Herr, mit mir beschlossen habest,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "du", ",", "Herr", ",", "mit", "mir", "be\u00b7schlos\u00b7sen", "ha\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "$,", "NN", "$,", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann du r\u00fccknehmen willst das Pfand, das du mir gabest.", "tokens": ["Wann", "du", "r\u00fcck\u00b7neh\u00b7men", "willst", "das", "Pfand", ",", "das", "du", "mir", "ga\u00b7best", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bereit zur R\u00fcckgab' hier leg' ich es vor dir nieder,", "tokens": ["Be\u00b7reit", "zur", "R\u00fc\u00b7ck\u00b7gab'", "hier", "leg'", "ich", "es", "vor", "dir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und als dein neu Geschenk nehm' ich mit Dank es wieder.", "tokens": ["Und", "als", "dein", "neu", "Ge\u00b7schenk", "nehm'", "ich", "mit", "Dank", "es", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "APPR", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Leben ist mir werth, weil es ist eine Gabe,", "tokens": ["Das", "Le\u00b7ben", "ist", "mir", "werth", ",", "weil", "es", "ist", "ei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Die von der h\u00f6chsten Lieb' ich zum Andenken habe.", "tokens": ["Die", "von", "der", "h\u00f6chs\u00b7ten", "Lieb'", "ich", "zum", "An\u00b7den\u00b7ken", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}