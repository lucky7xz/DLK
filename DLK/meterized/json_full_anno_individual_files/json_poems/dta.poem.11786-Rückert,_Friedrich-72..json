{"dta.poem.11786": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleichg\u00fcltig findet mich der Lenz zum erstenmal,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "fin\u00b7det", "mich", "der", "Lenz", "zum", "ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob ich \u00e4lter sei als Wald und Berg und Thal.", "tokens": ["Al\u00b7sob", "ich", "\u00e4l\u00b7ter", "sei", "als", "Wald", "und", "Berg", "und", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KOKOM", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da Wald und Berg und Thal, die alten, sich erneun,", "tokens": ["Da", "Wald", "und", "Berg", "und", "Thal", ",", "die", "al\u00b7ten", ",", "sich", "er\u00b7neun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "$,", "ART", "ADJA", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sollte sich nicht neu das alte Herz auch freun?", "tokens": ["Wie", "soll\u00b7te", "sich", "nicht", "neu", "das", "al\u00b7te", "Herz", "auch", "freun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein halb Jahrhundert lang freut' ich mich Jahr um Jahr,", "tokens": ["Ein", "halb", "Jahr\u00b7hun\u00b7dert", "lang", "freut'", "ich", "mich", "Jahr", "um", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "VVFIN", "PPER", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wardst du nun so alt in diesem einz'gen gar?", "tokens": ["Und", "wardst", "du", "nun", "so", "alt", "in", "die\u00b7sem", "einz'\u00b7gen", "gar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nein! sondern weil ein Bild des Fr\u00fchlings in mir steht,", "tokens": ["Nein", "!", "son\u00b7dern", "weil", "ein", "Bild", "des", "Fr\u00fch\u00b7lings", "in", "mir", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "KOUS", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor welchem das zu Nichts, das drau\u00dfen steht, vergeht.", "tokens": ["Vor", "wel\u00b7chem", "das", "zu", "Nichts", ",", "das", "drau\u00b7\u00dfen", "steht", ",", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "APPR", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}