{"dta.poem.18081": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Auf den neuen Garten.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kanst du wohl gl\u00fcckseliger seyn,", "tokens": ["Kanst", "du", "wohl", "gl\u00fcck\u00b7se\u00b7li\u00b7ger", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.2": {"text": "O du stets gesegneter Garten?", "tokens": ["O", "du", "stets", "ge\u00b7se\u00b7gne\u00b7ter", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Du darffest auf den Sonnenschein", "tokens": ["Du", "darf\u00b7fest", "auf", "den", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht wie sonst ander G\u00e4rten warten.", "tokens": ["Nicht", "wie", "sonst", "an\u00b7der", "G\u00e4r\u00b7ten", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ADV", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann deiner eignen Sonnen Glantz", "tokens": ["Dann", "dei\u00b7ner", "eig\u00b7nen", "Son\u00b7nen", "Glantz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kan all deine Gew\u00e4chs erlaben,", "tokens": ["Kan", "all", "dei\u00b7ne", "Ge\u00b7w\u00e4chs", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und deine G\u00e4ng, St\u00f6ck, und B\u00e4um gantz", "tokens": ["Und", "dei\u00b7ne", "G\u00e4ng", ",", "St\u00f6ck", ",", "und", "B\u00e4um", "gantz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$,", "KON", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit bequemer Jahrs-Zeit begaben.", "tokens": ["Mit", "be\u00b7que\u00b7mer", "Jahr\u00b7sZeit", "be\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling ihres Angesichts", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ih\u00b7res", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kan dich mit Gilgen, Rosen, zieren,", "tokens": ["Kan", "dich", "mit", "Gil\u00b7gen", ",", "Ro\u00b7sen", ",", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dir an Blumen mangle nichts,", "tokens": ["Da\u00df", "dir", "an", "Blu\u00b7men", "mang\u00b7le", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann alle G\u00e4rten schon gefrieren.", "tokens": ["Wann", "al\u00b7le", "G\u00e4r\u00b7ten", "schon", "ge\u00b7frie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So wird dein Grund mit gr\u00fcnem Lust,", "tokens": ["So", "wird", "dein", "Grund", "mit", "gr\u00fc\u00b7nem", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sie nur ihren Fu\u00df hinsetzet,", "tokens": ["Wo", "sie", "nur", "ih\u00b7ren", "Fu\u00df", "hin\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie deine Zweig und Aest mit Blust", "tokens": ["Wie", "dei\u00b7ne", "Zweig", "und", "A\u00b7est", "mit", "Blust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und Frucht durch ihre Hand ergetzet.", "tokens": ["Und", "Frucht", "durch", "ih\u00b7re", "Hand", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sie in dir macht, da\u00df ab dir", "tokens": ["Und", "sie", "in", "dir", "macht", ",", "da\u00df", "ab", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPER", "VVFIN", "$,", "KOUS", "APPR", "PPER"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Sich Himmel und Erden erquicken,", "tokens": ["Sich", "Him\u00b7mel", "und", "Er\u00b7den", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und da\u00df du, aller G\u00e4rten Zier,", "tokens": ["Und", "da\u00df", "du", ",", "al\u00b7ler", "G\u00e4r\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie all mit Blumen kanst erquicken.", "tokens": ["Sie", "all", "mit", "Blu\u00b7men", "kanst", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja wann sie, (aller Blumen Ruhm,)", "tokens": ["Ja", "wann", "sie", ",", "(", "al\u00b7ler", "Blu\u00b7men", "Ruhm", ",", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "PWAV", "PPER", "$,", "$(", "PIAT", "NN", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dir deine Gew\u00e4chs betrachtet,", "tokens": ["In", "dir", "dei\u00b7ne", "Ge\u00b7w\u00e4chs", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Werden kaum gegen solcher Blum", "tokens": ["Wer\u00b7den", "kaum", "ge\u00b7gen", "sol\u00b7cher", "Blum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Deine Blumen wie Gra\u00df geachtet.", "tokens": ["Dei\u00b7ne", "Blu\u00b7men", "wie", "Gra\u00df", "ge\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}