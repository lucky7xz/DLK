{"textgrid.poem.37358": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Lache nicht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lache nicht, wenn mit den Jahren", "tokens": ["La\u00b7che", "nicht", ",", "wenn", "mit", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb und Freundlichkeit vergehen;", "tokens": ["Lieb", "und", "Freund\u00b7lich\u00b7keit", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was Paulinchen ist geschehen,", "tokens": ["Was", "Pau\u00b7lin\u00b7chen", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann auch dir mal widerfahren.", "tokens": ["Kann", "auch", "dir", "mal", "wi\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh nur, wie ver\u00e4ndert hat sich", "tokens": ["Sieh", "nur", ",", "wie", "ver\u00b7\u00e4n\u00b7dert", "hat", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "VVPP", "VAFIN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser guter K\u00fcchenbesen.", "tokens": ["Un\u00b7ser", "gu\u00b7ter", "K\u00fc\u00b7chen\u00b7be\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er, der sonst so weich gewesen,", "tokens": ["Er", ",", "der", "sonst", "so", "weich", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist jetzunder stumpf und kratzig.", "tokens": ["Ist", "je\u00b7tzun\u00b7der", "stumpf", "und", "krat\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Lache nicht, wenn mit den Jahren", "tokens": ["La\u00b7che", "nicht", ",", "wenn", "mit", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb und Freundlichkeit vergehen;", "tokens": ["Lieb", "und", "Freund\u00b7lich\u00b7keit", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was Paulinchen ist geschehen,", "tokens": ["Was", "Pau\u00b7lin\u00b7chen", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann auch dir mal widerfahren.", "tokens": ["Kann", "auch", "dir", "mal", "wi\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text