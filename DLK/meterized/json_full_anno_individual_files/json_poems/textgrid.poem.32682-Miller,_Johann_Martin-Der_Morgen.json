 {"text": "Und vor allen einst gl\u00e4nzest,", "tokens": ["Und", "vor", "al\u00b7len", "einst", "gl\u00e4n\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADV", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wenn dein Geist sich dem Staub entschwingt.", "tokens": ["Wenn", "dein", "Geist", "sich", "dem", "Staub", "ent\u00b7schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ruhig ist er; denn noch kennst du die Qualen nicht,", "tokens": ["Ru\u00b7hig", "ist", "er", ";", "denn", "noch", "kennst", "du", "die", "Qua\u00b7len", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$.", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die die Lieb' in das Herz ihrer Vertrauten geu\u00dft,", "tokens": ["Die", "die", "Lieb'", "in", "das", "Herz", "ih\u00b7rer", "Ver\u00b7trau\u00b7ten", "geu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nicht den Kummer, o Daphne,", "tokens": ["Nicht", "den", "Kum\u00b7mer", ",", "o", "Daph\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Den dein l\u00e4chelndes Auge schuf.", "tokens": ["Den", "dein", "l\u00e4\u00b7cheln\u00b7des", "Au\u00b7ge", "schuf", "