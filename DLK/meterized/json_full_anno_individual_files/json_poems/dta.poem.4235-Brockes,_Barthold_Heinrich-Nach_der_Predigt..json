{"dta.poem.4235": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nach der Predigt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt, la\u00dft uns an den mehr als g\u00fcldnen Sch\u00e4tzen", "tokens": ["Kommt", ",", "la\u00dft", "uns", "an", "den", "mehr", "als", "g\u00fcld\u00b7nen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "APPR", "ART", "PIAT", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gelben Aehren uns ergetzen!", "tokens": ["Der", "gel\u00b7ben", "A\u00b7eh\u00b7ren", "uns", "er\u00b7get\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "La\u00dft uns sie mit Vergn\u00fcgen m\u00e4h\u2019n", "tokens": ["La\u00dft", "uns", "sie", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "m\u00e4h'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in die Scheuren fahren seh\u2019n!", "tokens": ["Und", "in", "die", "Scheu\u00b7ren", "fah\u00b7ren", "seh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man sieht, mit Freuden, allbereit", "tokens": ["Man", "sieht", ",", "mit", "Freu\u00b7den", ",", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "$,", "APPR", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gelben Felder Schmuck und Segen,", "tokens": ["Der", "gel\u00b7ben", "Fel\u00b7der", "Schmuck", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch unsrer Schnitter Faust, in reger Emsigkeit,", "tokens": ["Durch", "uns\u00b7rer", "Schnit\u00b7ter", "Faust", ",", "in", "re\u00b7ger", "Em\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur blo\u00df allein um uns zu n\u00fctzen,", "tokens": ["Nur", "blo\u00df", "al\u00b7lein", "um", "uns", "zu", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich kr\u00fcmmen und in Schwaden legen.", "tokens": ["Sich", "kr\u00fcm\u00b7men", "und", "in", "Schwa\u00b7den", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man sieht, mit Lust, der Sensen Blitzen,", "tokens": ["Man", "sieht", ",", "mit", "Lust", ",", "der", "Sen\u00b7sen", "Blit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man sieht die Binder fertig binden,", "tokens": ["Man", "sieht", "die", "Bin\u00b7der", "fer\u00b7tig", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Kr\u00e4nz\u2019 aus Stroh um schwere Garben winden.", "tokens": ["Und", "Kr\u00e4nz'", "aus", "Stroh", "um", "schwe\u00b7re", "Gar\u00b7ben", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Seht, wie in zierlichen Alleen,", "tokens": ["Seht", ",", "wie", "in", "zier\u00b7li\u00b7chen", "Al\u00b7leen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und langen Linien, da\u00df sie kaum abzusehn,", "tokens": ["Und", "lan\u00b7gen", "Li\u00b7ni\u00b7en", ",", "da\u00df", "sie", "kaum", "ab\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die netten Hocken-H\u00fcgel stehen!", "tokens": ["Die", "net\u00b7ten", "Hocken\u00b7H\u00fc\u00b7gel", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Dort k\u00f6nnen kaum die grossen Leiter-Wagen", "tokens": ["Dort", "k\u00f6n\u00b7nen", "kaum", "die", "gros\u00b7sen", "Lei\u00b7ter\u00b7Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das raschelnde Gewicht gebundner Garben tragen.", "tokens": ["Das", "ra\u00b7scheln\u00b7de", "Ge\u00b7wicht", "ge\u00b7bund\u00b7ner", "Gar\u00b7ben", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Man f\u00e4hrt den Segen itzt in eure Scheuren ein,", "tokens": ["Man", "f\u00e4hrt", "den", "Se\u00b7gen", "itzt", "in", "eu\u00b7re", "Scheu\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihr verz\u00f6gert noch vergn\u00fcgt zu seyn?", "tokens": ["Und", "ihr", "ver\u00b7z\u00f6\u00b7gert", "noch", "ver\u00b7gn\u00fcgt", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lauter Anmuht, lauter Lust", "tokens": ["Lau\u00b7ter", "An\u00b7muht", ",", "lau\u00b7ter", "Lust"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Quill\u2019 aus eurer frohen Brust!", "tokens": ["Quill'", "aus", "eu\u00b7rer", "fro\u00b7hen", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Chor erk.", "tokens": ["Chor", "erk", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Seelen. Lauter Anmuht, lauter Lust", "tokens": ["See\u00b7len", ".", "Lau\u00b7ter", "An\u00b7muht", ",", "lau\u00b7ter", "Lust"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Quillt aus unsrer frohen Brust.", "tokens": ["Quillt", "aus", "uns\u00b7rer", "fro\u00b7hen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Es lobsing\u2019 itzt Jedermann!", "tokens": ["Es", "lob\u00b7sing'", "itzt", "Je\u00b7der\u00b7mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Jeder, der das Feld betrachtet,", "tokens": ["Je\u00b7der", ",", "der", "das", "Feld", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Preise GOttes weise St\u00e4rke!", "tokens": ["Prei\u00b7se", "Got\u00b7tes", "wei\u00b7se", "St\u00e4r\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rufe: ", "tokens": ["Ru\u00b7fe", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wer ihr achtet,", "tokens": ["Wer", "ihr", "ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Der hat eitel Lust daran.", "tokens": ["Der", "hat", "ei\u00b7tel", "Lust", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Betr. Du ewiger Gnaden allm\u00e4chtiger Wille rc.", "tokens": ["Be\u00b7tr.", "Du", "e\u00b7wi\u00b7ger", "Gna\u00b7den", "all\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Wil\u00b7le", "rc", "."], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "ADJA", "NN", "NE", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.8": {"line.1": {"text": "Herr des Sonnen-Scheins und Regens,", "tokens": ["Herr", "des", "Son\u00b7nen\u00b7Scheins", "und", "Re\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ew\u2019ge Brunnquell alles Segens,", "tokens": ["Ew'\u00b7ge", "Brunn\u00b7quell", "al\u00b7les", "Se\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ursprung aller Fruchtbarkeit!", "tokens": ["Ur\u00b7sprung", "al\u00b7ler", "Frucht\u00b7bar\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Du so viel Lieb\u2019 und G\u00fcte,", "tokens": ["Der", "Du", "so", "viel", "Lieb'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sonderlich zur Erndte-Zeit,", "tokens": ["Son\u00b7der\u00b7lich", "zur", "Ernd\u00b7te\u00b7Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner Creatur erwiesen,", "tokens": ["Dei\u00b7ner", "Crea\u00b7tur", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sey ger\u00fchmet und gepriesen,", "tokens": ["Sey", "ge\u00b7r\u00fch\u00b7met", "und", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit erkenntlichem Gem\u00fchte,", "tokens": ["Mit", "er\u00b7kennt\u00b7li\u00b7chem", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sonderlich zur Erndte-Zeit!", "tokens": ["Son\u00b7der\u00b7lich", "zur", "Ernd\u00b7te\u00b7Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Herr des rc.", "tokens": ["Herr", "des", "rc", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}