{"dta.poem.2741": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Beerdigung Fr. A. R. v. K. g. S. den  \n 6. Novembr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sey nunmehr da\u00df uns des Winters Zahn", "tokens": ["Es", "sey", "nun\u00b7mehr", "da\u00df", "uns", "des", "Win\u00b7ters", "Zahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der W\u00e4lder Zier/ der Felder Purpur raubet;", "tokens": ["Der", "W\u00e4l\u00b7der", "Zier", "/", "der", "Fel\u00b7der", "Pur\u00b7pur", "rau\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es steh verw\u00fcst der Flore Garten Plan", "tokens": ["Es", "steh", "ver\u00b7w\u00fcst", "der", "Flo\u00b7re", "Gar\u00b7ten", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "ART", "NN", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Baum gantz nackt/ entgipffelt/ unbelaubet/", "tokens": ["Der", "Baum", "gantz", "nackt", "/", "ent\u00b7gipf\u00b7felt", "/", "un\u00b7be\u00b7lau\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$(", "VVPP", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Lufft sey Schnee/ der Himmel kalt/", "tokens": ["Die", "Lufft", "sey", "Schnee", "/", "der", "Him\u00b7mel", "kalt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$(", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Brust der Erden ungestalt", "tokens": ["Die", "Brust", "der", "Er\u00b7den", "un\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wa\u00df vor lieblich hie\u00df/ ersterb itzt auff der Bahre/", "tokens": ["Und", "wa\u00df", "vor", "lieb\u00b7lich", "hie\u00df", "/", "ers\u00b7terb", "itzt", "auff", "der", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "VVFIN", "$(", "VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hiemit den Untergang zu zeigen von dem Jahre.", "tokens": ["Hie\u00b7mit", "den", "Un\u00b7ter\u00b7gang", "zu", "zei\u00b7gen", "von", "dem", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es hemmet sich der Sonnen g\u00fcldnes Rad/", "tokens": ["Es", "hem\u00b7met", "sich", "der", "Son\u00b7nen", "g\u00fcld\u00b7nes", "Rad", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und di\u00df was bleibt/ sind nichts als lange N\u00e4chte;", "tokens": ["Und", "di\u00df", "was", "bleibt", "/", "sind", "nichts", "als", "lan\u00b7ge", "N\u00e4ch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PWS", "VVFIN", "$(", "VAFIN", "PIS", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach da\u00df doch nicht von unsrem Lebens-Pfad/", "tokens": ["Ach", "da\u00df", "doch", "nicht", "von", "uns\u00b7rem", "Le\u00b7bens\u00b7Pfad", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der bleiche Tod ein gleiches Beyspiel br\u00e4chte!", "tokens": ["Der", "blei\u00b7che", "Tod", "ein", "glei\u00b7ches", "Bey\u00b7spiel", "br\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hoch-Edler Herr/ des Hauses Sonn", "tokens": ["Hoch\u00b7Ed\u00b7ler", "Herr", "/", "des", "Hau\u00b7ses", "Sonn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seiner Seelen Lust und Wonn/", "tokens": ["Und", "sei\u00b7ner", "See\u00b7len", "Lust", "und", "Wonn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sinck\u2019t bey gek\u00fcrtztem Tag jetzt in des Todes Schatten/", "tokens": ["Sin\u00b7ck't", "bey", "ge\u00b7k\u00fcrtz\u00b7tem", "Tag", "jetzt", "in", "des", "To\u00b7des", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Und Thr\u00e4nen bleiben nur dem Grabmahl abzustatten.", "tokens": ["Und", "Thr\u00e4\u00b7nen", "blei\u00b7ben", "nur", "dem", "Grab\u00b7mahl", "ab\u00b7zu\u00b7stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach schwartzer Tag! Ach aus der Tage Zahl", "tokens": ["Ach", "schwart\u00b7zer", "Tag", "!", "Ach", "aus", "der", "Ta\u00b7ge", "Zahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$.", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verworffner Tag auff ewig auszustreichen!", "tokens": ["Ver\u00b7worff\u00b7ner", "Tag", "auff", "e\u00b7wig", "aus\u00b7zu\u00b7strei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach Licht voll Angst! Ach Anblick voller Quahl!", "tokens": ["Ach", "Licht", "voll", "Angst", "!", "Ach", "An\u00b7blick", "vol\u00b7ler", "Quahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJD", "NN", "$.", "ITJ", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein ander Hertz und Leben wird zur Leichen.", "tokens": ["Sein", "an\u00b7der", "Hertz", "und", "Le\u00b7ben", "wird", "zur", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "KON", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Redner Kunst/ der W\u00f6rter Pracht", "tokens": ["Der", "Red\u00b7ner", "Kunst", "/", "der", "W\u00f6r\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was sonst Schmertzen linder macht", "tokens": ["Und", "was", "sonst", "Schmert\u00b7zen", "lin\u00b7der", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verschwindet hier/ da ist kein Pflaster mehr zu finden/", "tokens": ["Ver\u00b7schwin\u00b7det", "hier", "/", "da", "ist", "kein", "Pflas\u00b7ter", "mehr", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "ADV", "VAFIN", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df diese Wunde kan durch kluge Cur verbinden.", "tokens": ["Da\u00df", "die\u00b7se", "Wun\u00b7de", "kan", "durch", "klu\u00b7ge", "Cur", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich seh umb ihn so eine schwartze Nacht/", "tokens": ["Ich", "seh", "umb", "ihn", "so", "ei\u00b7ne", "schwart\u00b7ze", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als einsmahls nicht Egypten \u00fcberfallen/", "tokens": ["Als", "eins\u00b7mahls", "nicht", "E\u00b7gyp\u00b7ten", "\u00fc\u00b7berf\u00b7al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie jeder Stern sich zum Cometen macht", "tokens": ["Wie", "je\u00b7der", "Stern", "sich", "zum", "Co\u00b7me\u00b7ten", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Ach und Weh ohn Unterla\u00df erschallen!", "tokens": ["Wie", "Ach", "und", "Weh", "ohn", "Un\u00b7ter\u00b7la\u00df", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn der Verlust ist allzugro\u00df/", "tokens": ["Denn", "der", "Ver\u00b7lust", "ist", "all\u00b7zu\u00b7gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist zu schwer der Seelen Sto\u00df!", "tokens": ["Es", "ist", "zu", "schwer", "der", "See\u00b7len", "Sto\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es klage wer da wil bey hei\u00df entbrandten Schmertzen/", "tokens": ["Es", "kla\u00b7ge", "wer", "da", "wil", "bey", "hei\u00df", "ent\u00b7brand\u00b7ten", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADV", "VMFIN", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich wei\u00df die Schmertzen gehn ihm peinlicher zum Hertzen.", "tokens": ["Ich", "wei\u00df", "die", "Schmert\u00b7zen", "gehn", "ihm", "pein\u00b7li\u00b7cher", "zum", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Stand/ Gl\u00fcck und Gut ersetzet noch die Zeit.", "tokens": ["Stand", "/", "Gl\u00fcck", "und", "Gut", "er\u00b7set\u00b7zet", "noch", "die", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein solch Gemahl vom Himmel auserkohren/", "tokens": ["Ein", "solch", "Ge\u00b7mahl", "vom", "Him\u00b7mel", "au\u00b7ser\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die fromm und klug zu wandeln nur gebohren/", "tokens": ["Die", "fromm", "und", "klug", "zu", "wan\u00b7deln", "nur", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PTKZU", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die verkehrt in Zucker Weh und Leyd/", "tokens": ["Und", "die", "ver\u00b7kehrt", "in", "Zu\u00b7cker", "Weh", "und", "Leyd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "APPR", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Sorgen Last auff sich gelegt/", "tokens": ["Der", "Sor\u00b7gen", "Last", "auff", "sich", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PRF", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nichts als ihrem Mann gepflegt/", "tokens": ["Und", "nichts", "als", "ih\u00b7rem", "Mann", "ge\u00b7pflegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird wol Alcestens Treu und Porciens besch\u00e4men/", "tokens": ["Wird", "wol", "Al\u00b7ces\u00b7tens", "Treu", "und", "Por\u00b7ci\u00b7ens", "be\u00b7sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es k\u00f6nte noch die Welt hier ein Exempel nehmen.", "tokens": ["Es", "k\u00f6n\u00b7te", "noch", "die", "Welt", "hier", "ein", "Ex\u00b7em\u00b7pel", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Es zeigt uns auch der Zeiten grauer Mund/", "tokens": ["Es", "zeigt", "uns", "auch", "der", "Zei\u00b7ten", "grau\u00b7er", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wunderspiel von ungemeiner Liebe:", "tokens": ["Ein", "Wun\u00b7der\