{"dta.poem.331": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie flehet darwider.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein eintziger Trost/ es stirbet der Geist/", "tokens": ["Mein", "eint\u00b7zi\u00b7ger", "Trost", "/", "es", "stir\u00b7bet", "der", "Geist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Elend den Faden des Lebens zerreist", "tokens": ["Das", "E\u00b7lend", "den", "Fa\u00b7den", "des", "Le\u00b7bens", "zer\u00b7reist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wenn du auf mein Leben", "tokens": ["Wenn", "du", "auf", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dein Lieben wilt geben.", "tokens": ["Dein", "Lie\u00b7ben", "wilt", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ach! da\u00df sich verschleust", "tokens": ["Ach", "!", "da\u00df", "sich", "ver\u00b7schleust"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PRF", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Dein liebliches Hertz/", "tokens": ["Dein", "lieb\u00b7li\u00b7ches", "Hertz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und setzest die Wunden/", "tokens": ["Und", "set\u00b7zest", "die", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die ich schon empfunden", "tokens": ["Die", "ich", "schon", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "In gr\u00f6sseren Schmertz", "tokens": ["In", "gr\u00f6s\u00b7se\u00b7ren", "Schmertz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ich sterbe mein Licht/", "tokens": ["Ich", "ster\u00b7be", "mein", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Weil das mir gebricht", "tokens": ["Weil", "das", "mir", "ge\u00b7bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Wodurch ich die Sinnen", "tokens": ["Wo\u00b7durch", "ich", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Sonst k\u00f6nnen gewinnen", "tokens": ["Sonst", "k\u00f6n\u00b7nen", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Zur ewigen Pflicht.", "tokens": ["Zur", "e\u00b7wi\u00b7gen", "Pflicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Wilt du dich erweisen", "tokens": ["Wilt", "du", "dich", "er\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Denn h\u00e4rter wie Eysen?", "tokens": ["Denn", "h\u00e4r\u00b7ter", "wie", "Ey\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Ach thu es doch nicht.", "tokens": ["Ach", "thu", "es", "doch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}