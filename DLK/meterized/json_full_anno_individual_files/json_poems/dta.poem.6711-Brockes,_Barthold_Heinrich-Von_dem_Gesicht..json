{"dta.poem.6711": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der schnelle Blick verbreitet sich", "tokens": ["Der", "schnel\u00b7le", "Blick", "ver\u00b7brei\u00b7tet", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Osten an, bi\u00df gegen Westen.", "tokens": ["Von", "Os\u00b7ten", "an", ",", "bi\u00df", "ge\u00b7gen", "Wes\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KOUS", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Uns leuchten Sonn und Stern von der gew\u00f6lbten Festen", "tokens": ["Uns", "leuch\u00b7ten", "Sonn", "und", "Stern", "von", "der", "ge\u00b7w\u00f6lb\u00b7ten", "Fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf diesen Erden-Bau, der stets ver\u00e4nderlich", "tokens": ["Auf", "die\u00b7sen", "Er\u00b7den\u00b7Bau", ",", "der", "stets", "ver\u00b7\u00e4n\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch immer einerley.", "tokens": ["Doch", "im\u00b7mer", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dies Auge, das die Zier", "tokens": ["Dies", "Au\u00b7ge", ",", "das", "die", "Zier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des C\u00f6rpers und sein Licht, ein nimmer m\u00fcder Knecht", "tokens": ["Des", "C\u00f6r\u00b7pers", "und", "sein", "Licht", ",", "ein", "nim\u00b7mer", "m\u00fc\u00b7der", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des Geists, der voller Neu-Begier.", "tokens": ["Des", "Geists", ",", "der", "vol\u00b7ler", "Neu\u00b7Be\u00b7gier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein kluges Spiegel-Glas und eine Schilderey", "tokens": ["Ein", "klu\u00b7ges", "Spie\u00b7gel\u00b7Glas", "und", "ei\u00b7ne", "Schil\u00b7de\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die lebend, stellet uns des Himmels Heer,", "tokens": ["Die", "le\u00b7bend", ",", "stel\u00b7let", "uns", "des", "Him\u00b7mels", "Heer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Stellt uns das weite Meer", "tokens": ["Stellt", "uns", "das", "wei\u00b7te", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "In seinen kleinen Creisen f\u00fcr.", "tokens": ["In", "sei\u00b7nen", "klei\u00b7nen", "Crei\u00b7sen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}