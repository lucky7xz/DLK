{"dta.poem.8748": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wie seyd ihr froh und frisch und frank,", "tokens": ["Wie", "seyd", "ihr", "froh", "und", "frisch", "und", "frank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trillert Morgench\u00f6re:", "tokens": ["Und", "tril\u00b7lert", "Mor\u00b7genc\u00b7h\u00f6\u00b7re", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fchle mich im Herzen krank,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "im", "Her\u00b7zen", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich's von unten h\u00f6re.", "tokens": ["Wenn", "ich's", "von", "un\u00b7ten", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein St\u00fcndchen schleich' ich blos heraus", "tokens": ["Ein", "St\u00fcnd\u00b7chen", "schleich'", "ich", "blos", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In euer \u00e4stig Sommerhaus,", "tokens": ["In", "eu\u00b7er", "\u00e4s\u00b7tig", "Som\u00b7mer\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mu\u00df mich de\u00df beklagen:", "tokens": ["Und", "mu\u00df", "mich", "de\u00df", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr lebet stets in Saus und Braus,", "tokens": ["Ihr", "le\u00b7bet", "stets", "in", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seht's nachten hier und tagen.", "tokens": ["Seht's", "nach\u00b7ten", "hier", "und", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr sucht der B\u00e4ume gr\u00fcnes Dach,", "tokens": ["Ihr", "sucht", "der", "B\u00e4u\u00b7me", "gr\u00fc\u00b7nes", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wiese Schmelz, den Kieselbach,", "tokens": ["Der", "Wie\u00b7se", "Schmelz", ",", "den", "Kie\u00b7sel\u00b7bach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr flieht vor Stadt und Mauer,", "tokens": ["Ihr", "flieht", "vor", "Stadt", "und", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und la\u00dft die Menschen sagen Ach!", "tokens": ["Und", "la\u00dft", "die", "Men\u00b7schen", "sa\u00b7gen", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In ihrem Vogelbauer.", "tokens": ["In", "ih\u00b7rem", "Vo\u00b7gel\u00b7bau\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}