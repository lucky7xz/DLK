{"textgrid.poem.36479": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1. Der L\u00f6we und die drei Tiger", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein L\u00f6we schlummerte, die Sorge f\u00fcr sein Reich,", "tokens": ["Ein", "L\u00f6\u00b7we", "schlum\u00b7mer\u00b7te", ",", "die", "Sor\u00b7ge", "f\u00fcr", "sein", "Reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seiner V\u00f6lker Ruh, lie\u00df ihn nicht ruhig schlafen,", "tokens": ["Und", "sei\u00b7ner", "V\u00f6l\u00b7ker", "Ruh", ",", "lie\u00df", "ihn", "nicht", "ru\u00b7hig", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er lag, wie auf den Sprung, gefa\u00dft auf jeden Streich,", "tokens": ["Er", "lag", ",", "wie", "auf", "den", "Sprung", ",", "ge\u00b7fa\u00dft", "auf", "je\u00b7den", "Streich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "$,", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Feinde seines Reichs zu schrecken, und zu strafen.", "tokens": ["Die", "Fein\u00b7de", "sei\u00b7nes", "Reichs", "zu", "schre\u00b7cken", ",", "und", "zu", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drei Tiger sahen ihn. Der eine sprach. ", "tokens": ["Drei", "Ti\u00b7ger", "sa\u00b7hen", "ihn", ".", "Der", "ei\u00b7ne", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$.", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie machten einen festen Bund,", "tokens": ["Sie", "mach\u00b7ten", "ei\u00b7nen", "fes\u00b7ten", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beschworen ihn. \u2013 Der Schwur, so still des ersten Mund", "tokens": ["Be\u00b7schwo\u00b7ren", "ihn", ".", "\u2013", "Der", "Schwur", ",", "so", "still", "des", "ers\u00b7ten", "Mund"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "$(", "PDS", "VVFIN", "$,", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihn lispeln mochte, kam in des Monarchen Ohr,", "tokens": ["Ihn", "lis\u00b7peln", "moch\u00b7te", ",", "kam", "in", "des", "Mon\u00b7ar\u00b7chen", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der lauschend lag, kaum glaubte, was geschah. \u2013", "tokens": ["Der", "lau\u00b7schend", "lag", ",", "kaum", "glaub\u00b7te", ",", "was", "ge\u00b7schah", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "$,", "PWS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der zweite Tiger schwur. Was that der L\u00f6we da?", "tokens": ["Der", "zwei\u00b7te", "Ti\u00b7ger", "schwur", ".", "Was", "that", "der", "L\u00f6\u00b7we", "da", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PWS", "VVFIN", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er flog, als wie ein Strahl des Blitzes, schnell hervor,", "tokens": ["Er", "flog", ",", "als", "wie", "ein", "Strahl", "des", "Blit\u00b7zes", ",", "schnell", "her\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sa\u00df auf des dritten Tigers Nacken,", "tokens": ["Sa\u00df", "auf", "des", "drit\u00b7ten", "Ti\u00b7gers", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Schon eh' er schwur, hielt ihn,", "tokens": ["Schon", "eh'", "er", "schwur", ",", "hielt", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Bekam den ersten nur mit einer Klau zu packen;", "tokens": ["Be\u00b7kam", "den", "ers\u00b7ten", "nur", "mit", "e