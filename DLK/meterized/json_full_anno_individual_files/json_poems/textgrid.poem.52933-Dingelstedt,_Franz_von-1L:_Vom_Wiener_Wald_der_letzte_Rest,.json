{"textgrid.poem.52933": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Wiener Wald der letzte Rest,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Wiener Wald der letzte Rest,", "tokens": ["Vom", "Wie\u00b7ner", "Wald", "der", "letz\u00b7te", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer will ihn sehn verdorren?", "tokens": ["Wer", "will", "ihn", "sehn", "ver\u00b7dor\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist sonst ein rechter Baum gewest,", "tokens": ["Ist", "sonst", "ein", "rech\u00b7ter", "Baum", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist jetzt ein schlechter Knorren.", "tokens": ["Ist", "jetzt", "ein", "schlech\u00b7ter", "Knor\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es hei\u00dft: ein kluger Schlossersmann,", "tokens": ["Es", "hei\u00dft", ":", "ein", "klu\u00b7ger", "Schlos\u00b7sers\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um seine Kunst zu weisen,", "tokens": ["Um", "sei\u00b7ne", "Kunst", "zu", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der schwei\u00dfte in die Wand ihn an", "tokens": ["Der", "schwei\u00df\u00b7te", "in", "die", "Wand", "ihn", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hing ein Zauberschl\u00f6\u00dflein dran,", "tokens": ["Und", "hing", "ein", "Zau\u00b7ber\u00b7schl\u00f6\u00df\u00b7lein", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das ist der Stock im Eisen!", "tokens": ["Das", "ist", "der", "Stock", "im", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du Wiener Wald, du gr\u00fcner Wald,", "tokens": ["Du", "Wie\u00b7ner", "Wald", ",", "du", "gr\u00fc\u00b7ner", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bist du schlimm behandelt,", "tokens": ["Wie", "bist", "du", "schlimm", "be\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus freiem Waidmanns-Aufenthalt", "tokens": ["Aus", "frei\u00b7em", "Waid\u00b7manns\u00b7Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Tandlermarkt verwandelt!", "tokens": ["Zum", "Tand\u00b7ler\u00b7markt", "ver\u00b7wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deinem Laub spazieren ging", "tokens": ["In", "dei\u00b7nem", "Laub", "spa\u00b7zie\u00b7ren", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Hirschkuh mit den Gei\u00dfen,", "tokens": ["Die", "Hirschkuh", "mit", "den", "Gei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Jetzt steht von dir in Schlo\u00df und Ring", "tokens": ["Jetzt", "steht", "von", "dir", "in", "Schlo\u00df", "und", "Ring"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur noch ein zwerghaft Kr\u00fcppel-Ding,", "tokens": ["Nur", "noch", "ein", "zwerg\u00b7haft", "Kr\u00fcp\u00b7pel\u00b7Ding", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das ist der Stock im Eisen!", "tokens": ["Das", "ist", "der", "Stock", "im", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wer vom Handwerk lobesam", "tokens": ["Und", "wer", "vom", "Hand\u00b7werk", "lo\u00b7be\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPRART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wackrer Schmied-Geselle", "tokens": ["Als", "wack\u00b7rer", "Schmie\u00b7dGe\u00b7sel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur Kaiserstadt gezogen kam,", "tokens": ["Zur", "Kai\u00b7ser\u00b7stadt", "ge\u00b7zo\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besieht sich diese Stelle;", "tokens": ["Be\u00b7sieht", "sich", "die\u00b7se", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er dreht am Schlo\u00df wohl hin und her,", "tokens": ["Er", "dreht", "am", "Schlo\u00df", "wohl", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Versucht's auf alle Weisen,", "tokens": ["Ver\u00b7sucht's", "auf", "al\u00b7le", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch \u00f6ffnen kann er's nimmermehr,", "tokens": ["Doch", "\u00f6ff\u00b7nen", "kann", "er's", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ja, murrt er, das ist halt zu schwer,", "tokens": ["Ja", ",", "murrt", "er", ",", "das", "ist", "halt", "zu", "schwer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "VVFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das ist der Stock im Eisen!", "tokens": ["Das", "ist", "der", "Stock", "im", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Darauf in den gefeiten Baum", "tokens": ["Da\u00b7rauf", "in", "den", "ge\u00b7fei\u00b7ten", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt er als Gilde-Zeichen", "tokens": ["Schl\u00e4gt", "er", "als", "Gil\u00b7de\u00b7Zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein N\u00e4glein ein, wo just noch Raum", "tokens": ["Ein", "N\u00e4g\u00b7lein", "ein", ",", "wo", "just", "noch", "Raum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor N\u00e4glein seines Gleichen.", "tokens": ["Vor", "N\u00e4g\u00b7lein", "sei\u00b7nes", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ei, seht, der ist mir zugedeckt,", "tokens": ["Ei", ",", "seht", ",", "der", "ist", "mir", "zu\u00b7ge\u00b7deckt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PRELS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kaum noch ein Baum zu hei\u00dfen!", "tokens": ["Kaum", "noch", "ein", "Baum", "zu", "hei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und oben, links am Stamme, steckt", "tokens": ["Und", "o\u00b7ben", ",", "links", "am", "Stam\u00b7me", ",", "steckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "ADV", "APPRART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Schl\u00f6\u00dflein, das sie alle neckt,", "tokens": ["Das", "Schl\u00f6\u00df\u00b7lein", ",", "das", "sie", "al\u00b7le", "neckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das ist der Stock im Eisen!", "tokens": ["Das", "ist", "der", "Stock", "im", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und doch, Herr Meister, h\u00fcte dich!", "tokens": ["Und", "doch", ",", "Herr", "Meis\u00b7ter", ",", "h\u00fc\u00b7te", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "NN", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn nun die Burschen k\u00e4men", "tokens": ["Wenn", "nun", "die", "Bur\u00b7schen", "k\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und flugs statt Zang' und Dieterich", "tokens": ["Und", "flugs", "statt", "Zang'", "und", "Die\u00b7te\u00b7rich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NE", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die \u2013 Schmiedeh\u00e4mmer n\u00e4hmen!?", "tokens": ["Die", "\u2013", "Schmie\u00b7de\u00b7h\u00e4m\u00b7mer", "n\u00e4h\u00b7men", "!?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was nicht mit Kunst zu \u00f6ffnen ist", "tokens": ["Was", "nicht", "mit", "Kunst", "zu", "\u00f6ff\u00b7nen", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4\u00dft sich vielleicht \u2013 zerrei\u00dfen, \u2013", "tokens": ["L\u00e4\u00dft", "sich", "viel\u00b7leicht", "\u2013", "zer\u00b7rei\u00b7\u00dfen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$(", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und herrlich, wenn zu bessrer Frist", "tokens": ["Und", "herr\u00b7lich", ",", "wenn", "zu", "bess\u00b7rer", "Frist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Neu-gr\u00fcnend in die H\u00f6he schie\u00dft", "tokens": ["Neu\u00b7gr\u00fc\u00b7nend", "in", "die", "H\u00f6\u00b7he", "schie\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.t