{"textgrid.poem.48403": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17. Aus eines Andern seiner Erfindung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Setz' einen, der doch itzt nicht lebt auf dieser Erden,", "tokens": ["Setz'", "ei\u00b7nen", ",", "der", "doch", "itzt", "nicht", "lebt", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "PRELS", "ADV", "ADV", "PTKNEG", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "noch ie gefunden ist, noch wird gefunden werden,", "tokens": ["noch", "ie", "ge\u00b7fun\u00b7den", "ist", ",", "noch", "wird", "ge\u00b7fun\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$,", "ADV", "VAFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der alles hab' an sich, was einen r\u00fchmblich macht,", "tokens": ["der", "al\u00b7les", "hab'", "an", "sich", ",", "was", "ei\u00b7nen", "r\u00fchm\u00b7blich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "PRF", "$,", "PRELS", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "des Cr\u00f6si Geld und Gut, des C\u00e4sars Gl\u00fcck und Pracht,", "tokens": ["des", "Cr\u00f6\u00b7si", "Geld", "und", "Gut", ",", "des", "C\u00e4\u00b7sars", "Gl\u00fcck", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJD", "$,", "ART", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die Sch\u00f6nheit Absolons, die Weisheit Salomonis,", "tokens": ["die", "Sch\u00f6n\u00b7heit", "Ab\u00b7so\u00b7lons", ",", "die", "Weis\u00b7heit", "Sa\u00b7lo\u00b7mo\u00b7nis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Homers Beredsamkeit, den Eifer Ciceronis,", "tokens": ["Ho\u00b7mers", "Be\u00b7red\u00b7sam\u00b7keit", ",", "den", "Ei\u00b7fer", "Ci\u00b7ce\u00b7ro\u00b7nis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "das Leben des Augusts, des Simsons starke Kraft,", "tokens": ["das", "Le\u00b7ben", "des", "Au\u00b7gusts", ",", "des", "Sim\u00b7sons", "star\u00b7ke", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "des redlichen Traians gerechte B\u00fcrgerschaft,", "tokens": ["des", "red\u00b7li\u00b7chen", "Trai\u00b7ans", "ge\u00b7rech\u00b7te", "B\u00fcr\u00b7ger\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "des schnellen Azahels behende Hurtigkeiten,", "tokens": ["des", "schnel\u00b7len", "Az\u00b7a\u00b7hels", "be\u00b7hen\u00b7de", "Hur\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "des Hectors k\u00fchnen Mut, im Fall es k\u00f6mpt zum Streiten:", "tokens": ["des", "Hec\u00b7tors", "k\u00fch\u00b7nen", "Mut", ",", "im", "Fall", "es", "k\u00f6mpt", "zum", "Strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPRART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so ist er doch so hoch mit Gaben nicht geschm\u00fcckt,", "tokens": ["so", "ist", "er", "doch", "so", "hoch", "mit", "Ga\u00b7ben", "nicht", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als von der Neider Schaar verfolget und gedr\u00fcckt.", "tokens": ["als", "von", "der", "Nei\u00b7der", "Schaar", "ver\u00b7fol\u00b7get", "und", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Setz' einen, der doch itzt nicht lebt auf dieser Erden,", "tokens": ["Setz'", "ei\u00b7nen", ",", "der", "doch", "itzt", "nicht", "lebt", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "PRELS", "ADV", "ADV", "PTKNEG", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.i