{"dta.poem.11938": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man pflanzet einen Baum, damit er Fr\u00fcchte trage,", "tokens": ["Man", "pflan\u00b7zet", "ei\u00b7nen", "Baum", ",", "da\u00b7mit", "er", "Fr\u00fcch\u00b7te", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rennet einen Weg, da\u00df man ein Ziel erjage.", "tokens": ["Und", "ren\u00b7net", "ei\u00b7nen", "Weg", ",", "da\u00df", "man", "ein", "Ziel", "er\u00b7ja\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Und alle Segel wehn entgegen ihrem Port,", "tokens": ["Und", "al\u00b7le", "Se\u00b7gel", "wehn", "ent\u00b7ge\u00b7gen", "ih\u00b7rem", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle Str\u00f6me gehn zum Ozeane fort.", "tokens": ["Und", "al\u00b7le", "Str\u00f6\u00b7me", "gehn", "zum", "O\u00b7zea\u00b7ne", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir aber wissen nicht, wozu wir thun die Thaten,", "tokens": ["Wir", "a\u00b7ber", "wis\u00b7sen", "nicht", ",", "wo\u00b7zu", "wir", "thun", "die", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wir bezwecken, kaum, nie, wie es wird gerathen.", "tokens": ["Was", "wir", "be\u00b7zwe\u00b7cken", ",", "kaum", ",", "nie", ",", "wie", "es", "wird", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADV", "$,", "ADV", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das will mit Zweifeln uns bestricken und verwirren,", "tokens": ["Das", "will", "mit", "Zwei\u00b7feln", "uns", "be\u00b7stri\u00b7cken", "und", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Thatkraft l\u00e4hmen, und im Werkberuf uns irren.", "tokens": ["Die", "That\u00b7kraft", "l\u00e4h\u00b7men", ",", "und", "im", "Werk\u00b7be\u00b7ruf", "uns", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir aber wollen froh uns f\u00fchlen im Beruf,", "tokens": ["Wir", "a\u00b7ber", "wol\u00b7len", "froh", "uns", "f\u00fch\u00b7len", "im", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu wirken das wozu Gott Lust und Kraft uns schuf.", "tokens": ["Zu", "wir\u00b7ken", "das", "wo\u00b7zu", "Gott", "Lust", "und", "Kraft", "uns", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "PWAV", "NN", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer handelt oder denkt, wer herrschet oder schreibt,", "tokens": ["Wer", "han\u00b7delt", "o\u00b7der", "denkt", ",", "wer", "herr\u00b7schet", "o\u00b7der", "schreibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$,", "PWS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der thue nur mit Gott, wozu der Geist ihn treibt.", "tokens": ["Der", "thue", "nur", "mit", "Gott", ",", "wo\u00b7zu", "der", "Geist", "ihn", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Wen aber keiner treibt, mag wie er will es treiben,", "tokens": ["Wen", "a\u00b7ber", "kei\u00b7ner", "treibt", ",", "mag", "wie", "er", "will", "es", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "VVFIN", "$,", "VMFIN", "KOKOM", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt mit Gottes Geist wird doch im Gange bleiben.", "tokens": ["Die", "Welt", "mit", "Got\u00b7tes", "Geist", "wird", "doch", "im", "Gan\u00b7ge", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}