{"dta.poem.12853": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie zu einem spatziergange be-  \n reden wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Darff auch Leanders mund die k\u00fchne bitte wagen:", "tokens": ["Darff", "auch", "Le\u00b7an\u00b7ders", "mund", "die", "k\u00fch\u00b7ne", "bit\u00b7te", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "VVFIN", "ART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wolle Sylvia mit ihm spatzieren gehn?", "tokens": ["Es", "wol\u00b7le", "Syl\u00b7via", "mit", "ihm", "spat\u00b7zie\u00b7ren", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ach nein! es darff mein wunsch sich nicht so weit erh\u00f6hn;", "tokens": ["Ach", "nein", "!", "es", "darff", "mein", "wunsch", "sich", "nicht", "so", "weit", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPER", "VMFIN", "PPOSAT", "ADJD", "PRF", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die k\u00fchnheit ist zu gro\u00df, ich mu\u00df es selber sagen.", "tokens": ["Die", "k\u00fchn\u00b7heit", "ist", "zu", "gro\u00df", ",", "ich", "mu\u00df", "es", "sel\u00b7ber", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indessen h\u00f6r\u2019 ich doch die sch\u00f6ne Flora klagen,", "tokens": ["In\u00b7des\u00b7sen", "h\u00f6r'", "ich", "doch", "die", "sch\u00f6\u00b7ne", "Flo\u00b7ra", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df deine f\u00fc\u00dfe nicht in ihren blumen stehn.", "tokens": ["Da\u00df", "dei\u00b7ne", "f\u00fc\u00b7\u00dfe", "nicht", "in", "ih\u00b7ren", "blu\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die sonne scheinet auch nicht ohngefehr so sch\u00f6n;", "tokens": ["Die", "son\u00b7ne", "schei\u00b7net", "auch", "nicht", "ohn\u00b7ge\u00b7fehr", "so", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie w\u00fcnscht ihr reines licht dem deinen beyzutragen.", "tokens": ["Sie", "w\u00fcnscht", "ihr", "rei\u00b7nes", "licht", "dem", "dei\u00b7nen", "bey\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der sanffte Zephyrus kan ohne dich nicht seyn,", "tokens": ["Der", "sanff\u00b7te", "Ze\u00b7phy\u00b7rus", "kan", "oh\u00b7ne", "dich", "nicht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er bisamt seine lufft mit deinem othem ein.", "tokens": ["Er", "bi\u00b7samt", "sei\u00b7ne", "lufft", "mit", "dei\u00b7nem", "ot\u00b7hem", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die vogel singen nur, wo Sylvia spatzieret:", "tokens": ["Die", "vo\u00b7gel", "sin\u00b7gen", "nur", ",", "wo", "Syl\u00b7via", "spat\u00b7zie\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wo Flora, sonn\u2019 und lufft nur nichts erhalten kan,", "tokens": ["Wo", "Flo\u00b7ra", ",", "sonn'", "und", "lufft", "nur", "nichts", "er\u00b7hal\u00b7ten", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "XY", "KON", "VVFIN", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und keine nachtigall dich in das gr\u00fcne f\u00fchret,", "tokens": ["Und", "kei\u00b7ne", "nach\u00b7ti\u00b7gall", "dich", "in", "das", "gr\u00fc\u00b7ne", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So steh\u2019 ich billich itzt mit meinem bitten an.", "tokens": ["So", "steh'", "ich", "bil\u00b7lich", "itzt", "mit", "mei\u00b7nem", "bit\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}