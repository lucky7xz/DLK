{"dta.poem.3794": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Die verachtete und erz\u00fcrnte Liebe.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ungetreu und falsche Seele! ehemahls gelieb-", "tokens": ["Un\u00b7ge\u00b7treu", "und", "fal\u00b7sche", "See\u00b7le", "!", "e\u00b7he\u00b7mahls", "ge\u00b7lie\u00b7b"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "NN", "$.", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "ter Schatz!", "tokens": ["ter", "Schatz", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Gabst du meiner frohen Regung nicht in deinem", "tokens": ["Gabst", "du", "mei\u00b7ner", "fro\u00b7hen", "Re\u00b7gung", "nicht", "in", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Hertzen Platz,", "tokens": ["Hert\u00b7zen", "Platz", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Als ich es zum ersten mahl so mit Furcht als Zit-", "tokens": ["Als", "ich", "es", "zum", "ers\u00b7ten", "mahl", "so", "mit", "Furcht", "als", "Zit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "ADJA", "ADV", "ADV", "APPR", "NN", "KOKOM", "TRUNC"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "tern wagte,", "tokens": ["tern", "wag\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Als die fromme Redligkeit dir von meiner Treue", "tokens": ["Als", "die", "from\u00b7me", "Red\u00b7lig\u00b7keit", "dir", "von", "mei\u00b7ner", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "sagte?", "tokens": ["sag\u00b7te", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Dencke nur an unser B\u00fcndni\u00df, dencke an den er-", "tokens": ["Den\u00b7cke", "nur", "an", "un\u00b7ser", "B\u00fcn\u00b7dni\u00df", ",", "den\u00b7cke", "an", "den", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.10": {"text": "sten Ku\u00df,", "tokens": ["sten", "Ku\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Der dich deiner grossen Bo\u00dfheit st\u00fcndlich \u00fcber-", "tokens": ["Der", "dich", "dei\u00b7ner", "gros\u00b7sen", "Bo\u00df\u00b7heit", "st\u00fcnd\u00b7lich", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "zeugen mu\u00df.", "tokens": ["zeu\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Sinne etwas besser nach; war ich nicht dein Hertz", "tokens": ["Sin\u00b7ne", "et\u00b7was", "bes\u00b7ser", "nach", ";", "war", "ich", "nicht", "dein", "Hertz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "PTKVZ", "$.", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "und Leben?", "tokens": ["und", "Le\u00b7ben", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Hat nicht die verlogne Brust meiner Liebe Raum", "tokens": ["Hat", "nicht", "die", "ver\u00b7log\u00b7ne", "Brust", "mei\u00b7ner", "Lie\u00b7be", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "PPOSAT", "NN", "NN"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "gegeben?", "tokens": ["ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Siehe! bin ich gleich verlassen, nimmt sich nie-", "tokens": ["Sie\u00b7he", "!", "bin", "ich", "gleich", "ver\u00b7las\u00b7sen", ",", "nimmt", "sich", "nie"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "VAFIN", "PPER", "ADV", "VVINF", "$,", "VVFIN", "PRF", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "mand meiner an,", "tokens": ["mand", "mei\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Ey so glaube, da\u00df der Himmel deine Falschheit", "tokens": ["Ey", "so", "glau\u00b7be", ",", "da\u00df", "der", "Him\u00b7mel", "dei\u00b7ne", "Falschheit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "r\u00e4chen kan.", "tokens": ["r\u00e4\u00b7chen", "kan", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Glaube, wenn nicht meinen Geist Furcht, Angst,", "tokens": ["Glau\u00b7be", ",", "wenn", "nicht", "mei\u00b7nen", "Geist", "Furcht", ",", "Angst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PTKNEG", "PPOSAT", "NN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Raserey und Liebe", "tokens": ["Ra\u00b7se\u00b7rey", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Wenn nicht selbsten deine Untreu meinen Kiel zun", "tokens": ["Wenn", "nicht", "selbs\u00b7ten", "dei\u00b7ne", "Un\u00b7treu", "mei\u00b7nen", "Kiel", "zun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Dichten triebe,", "tokens": ["Dich\u00b7ten", "trie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Warlich, Kind! ich wolte schweigen; aber da", "tokens": ["War\u00b7lich", ",", "Kind", "!", "ich", "wol\u00b7te", "schwei\u00b7gen", ";", "a\u00b7ber", "da"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "NN", "$.", "PPER", "VMFIN", "VVINF", "$.", "ADV", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.26": {"text": "mein Auge thr\u00e4nt,", "tokens": ["mein", "Au\u00b7ge", "thr\u00e4nt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Da man dich mir nur zur Schande, und zur", "tokens": ["Da", "man", "dich", "mir", "nur", "zur", "Schan\u00b7de", ",", "und", "zur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "PPER", "ADV", "APPRART", "NN", "$,", "KON", "APPRART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Pein und Schmach erwehnt,", "tokens": ["Pein", "und", "Schmach", "er\u00b7wehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.29": {"text": "O! so ist mein Schmertz gerecht; lache nur bey", "tokens": ["O", "!", "so", "ist", "mein", "Schmertz", "ge\u00b7recht", ";", "la\u00b7che", "nur", "bey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$.", "VVFIN", "ADV", "APPR"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "meinen Schmertzen,", "tokens": ["mei\u00b7nen", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "Banne mich aus deiner Brust, banne mich aus", "tokens": ["Ban\u00b7ne", "mich", "aus", "dei\u00b7ner", "Brust", ",", "ban\u00b7ne", "mich", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PRF", "APPR"], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.32": {"text": "deinem Hertzen;", "tokens": ["dei\u00b7nem", "Hert\u00b7zen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "In mir gl\u00fcht und brennt die Rache. Dein ver-", "tokens": ["In", "mir", "gl\u00fcht", "und", "brennt", "die", "Ra\u00b7che", ".", "Dein", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$.", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "ha\u00dfter Ubermuth", "tokens": ["ha\u00df\u00b7ter", "U\u00b7ber\u00b7muth"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.35": {"text": "Bauet mir den Sarg zum sterben, wo die from-", "tokens": ["Bau\u00b7et", "mir", "den", "Sarg", "zum", "ster\u00b7ben", ",", "wo", "die", "from"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "VVINF", "$,", "PWAV", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.36": {"text": "me Liebe ruht.", "tokens": ["me", "Lie\u00b7be", "ruht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Auch selbst meine bange Grufft soll dein falsches", "tokens": ["Auch", "selbst", "mei\u00b7ne", "ban\u00b7ge", "Grufft", "soll", "dein", "fal\u00b7sches"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Hertze schrecken;", "tokens": ["Hert\u00b7ze", "schre\u00b7cken", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.39": {"text": "Mein verfaultes todten Bein soll dir Grau\u00df", "tokens": ["Mein", "ver\u00b7faul\u00b7tes", "tod\u00b7ten", "Bein", "soll", "dir", "Grau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VMFIN", "PPER", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.40": {"text": "und Angst erwecken.", "tokens": ["und", "Angst", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.41": {"text": "Ja, dir soll, zu deiner Kr\u00e4nckung, der gesetzte Lei-", "tokens": ["Ja", ",", "dir", "soll", ",", "zu", "dei\u00b7ner", "Kr\u00e4n\u00b7ckung", ",", "der", "ge\u00b7setz\u00b7te", "Lei"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.42": {"text": "chenstein", "tokens": ["chen\u00b7stein"], "token_info": ["word"], "pos": ["NE"], "meter": "-+", "measure": "iambic.single"}, "line.43": {"text": "Mit der Inschrifft voller Rache, deines Ehstands", "tokens": ["Mit", "der", "In\u00b7schrifft", "vol\u00b7ler", "Ra\u00b7che", ",", "dei\u00b7nes", "Eh\u00b7stands"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.44": {"text": "H\u00f6lle seyn.", "tokens": ["H\u00f6l\u00b7le", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "Gehe! thue was du wilt! la\u00df dich etwas bessers", "tokens": ["Ge\u00b7he", "!", "thue", "was", "du", "wilt", "!", "la\u00df", "dich", "et\u00b7was", "bes\u00b7sers"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PWS", "PPER", "VMFIN", "$.", "VVIMP", "PPER", "ADV", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.46": {"text": "k\u00fcssen,", "tokens": ["k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Meine Marter soll der Tod, meine Pein die", "tokens": ["Mei\u00b7ne", "Mar\u00b7ter", "soll", "der", "Tod", ",", "mei\u00b7ne", "Pein", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "$,", "PPOSAT", "NN", "ART"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.48": {"text": "Grufft vers\u00fcssen.", "tokens": ["Grufft", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.49": {"text": "Wie ein Pilgrim hier in Kedar sch\u00fcchtern und", "tokens": ["Wie", "ein", "Pil\u00b7grim", "hier", "in", "Ke\u00b7dar", "sch\u00fcch\u00b7tern", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "KON"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.50": {"text": "verlassen irrt,", "tokens": ["ver\u00b7las\u00b7sen", "irrt", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.51": {"text": "Wie er halb verzweiffelnde st\u00f6hnet, seuffzet, klagt", "tokens": ["Wie", "er", "halb", "ver\u00b7zweif\u00b7feln\u00b7de", "st\u00f6h\u00b7net", ",", "seuff\u00b7zet", ",", "klagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "PPER", "ADJD", "ADJA", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "und girrt,", "tokens": ["und", "girrt", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.53": {"text": "Eben so ists auch mit mir; Himmel! h\u00f6re doch", "tokens": ["E\u00b7ben", "so", "ists", "auch", "mit", "mir", ";", "Him\u00b7mel", "!", "h\u00f6\u00b7re", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "APPR", "PPER", "$.", "NN", "$.", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "mein Bitten!", "tokens": ["mein", "Bit\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.55": {"text": "Rei\u00dfe mich, so bald du wilt, aus den falschen", "tokens": ["Rei\u00b7\u00dfe", "mich", ",", "so", "bald", "du", "wilt", ",", "aus", "den", "fal\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.56": {"text": "Mesechs H\u00fctten!", "tokens": ["Me\u00b7sechs", "H\u00fct\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.57": {"text": "Brich den schwachen Bau des C\u00f6rpers! brich", "tokens": ["Brich", "den", "schwa\u00b7chen", "Bau", "des", "C\u00f6r\u00b7pers", "!", "brich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN", "$.", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.58": {"text": "ihn heute noch entzwey!", "tokens": ["ihn", "heu\u00b7te", "noch", "ent\u00b7zwey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "La\u00df es heute noch geschehen, alles gilt mir einer-", "tokens": ["La\u00df", "es", "heu\u00b7te", "noch", "ge\u00b7sche\u00b7hen", ",", "al\u00b7les", "gilt", "mir", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVPP", "$,", "PIS", "VVFIN", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.60": {"text": "ley.", "tokens": ["ley", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.61": {"text": "Nun ich sterbe. Wenn ich einst aus dem Stau-", "tokens": ["Nun", "ich", "ster\u00b7be", ".", "Wenn", "ich", "einst", "aus", "dem", "Stau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$.", "KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.62": {"text": "be auch erwache,", "tokens": ["be", "auch", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.63": {"text": "O! so ruff ich \u00fcber dir! Meineids-volle Rache!", "tokens": ["O", "!", "so", "ruff", "ich", "\u00fc\u00b7ber", "dir", "!", "Mein\u00b7eids\u00b7vol\u00b7le", "Ra\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$.", "NN", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.64": {"text": "Rache!", "tokens": ["Ra\u00b7che", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}