{"dta.poem.16066": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "120.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Thurme wird erz\u00e4hlt, den einst die Menschen bauten,", "tokens": ["Vom", "Thur\u00b7me", "wird", "er\u00b7z\u00e4hlt", ",", "den", "einst", "die", "Men\u00b7schen", "bau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sie auf eigne mehr dan Gottes Kraft vertrauten;", "tokens": ["Als", "sie", "auf", "eig\u00b7ne", "mehr", "dan", "Got\u00b7tes", "Kraft", "ver\u00b7trau\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADV", "ADV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie Gott, aufda\u00df er sie im k\u00fchnen Bauwerk irrte,", "tokens": ["Wie", "Gott", ",", "auf\u00b7da\u00df", "er", "sie", "im", "k\u00fch\u00b7nen", "Bau\u00b7werk", "irr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "KOUS", "PPER", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sprachen wunderbar der Bauenden verwirrte;", "tokens": ["Die", "Spra\u00b7chen", "wun\u00b7der\u00b7bar", "der", "Bau\u00b7en\u00b7den", "ver\u00b7wirr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Soda\u00df nach manchem Streit sie endlich r\u00e4thlich fanden,", "tokens": ["So\u00b7da\u00df", "nach", "man\u00b7chem", "Streit", "sie", "end\u00b7lich", "r\u00e4th\u00b7lich", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auseinander zu gehn, weil sie sich nicht verstanden:", "tokens": ["Aus\u00b7ein\u00b7an\u00b7der", "zu", "gehn", ",", "weil", "sie", "sich", "nicht", "ver\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Da griff zu guter Letz jeder nach seinem Sack,", "tokens": ["Da", "griff", "zu", "gu\u00b7ter", "Letz", "je\u00b7der", "nach", "sei\u00b7nem", "Sack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle zogen sie nun ab mit Sack und Pack;", "tokens": ["Und", "al\u00b7le", "zo\u00b7gen", "sie", "nun", "ab", "mit", "Sack", "und", "Pack", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Davon, wie vielfach nun gesprochen und geschrieben", "tokens": ["Da\u00b7von", ",", "wie", "viel\u00b7fach", "nun", "ge\u00b7spro\u00b7chen", "und", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "ADV", "ADV", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sprachen seien, ist in jeder Sack geblieben:", "tokens": ["Die", "Spra\u00b7chen", "sei\u00b7en", ",", "ist", "in", "je\u00b7der", "Sack", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn jeder hat, so gro\u00df ist Eigennutzes Macht,", "tokens": ["Denn", "je\u00b7der", "hat", ",", "so", "gro\u00df", "ist", "Ei\u00b7gen\u00b7nut\u00b7zes", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "ADV", "ADJD", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als alles er verga\u00df, an seinen Sack gedacht;", "tokens": ["Als", "al\u00b7les", "er", "ver\u00b7ga\u00df", ",", "an", "sei\u00b7nen", "Sack", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und keiner hat seitdem in seines Lebens Plack", "tokens": ["Und", "kei\u00b7ner", "hat", "seit\u00b7dem", "in", "sei\u00b7nes", "Le\u00b7bens", "Plack"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "PAV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergessen den vom Thurm mit heim gebrachten Sack.", "tokens": ["Ver\u00b7ges\u00b7sen", "den", "vom", "Thurm", "mit", "heim", "ge\u00b7brach\u00b7ten", "Sack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPRART", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}