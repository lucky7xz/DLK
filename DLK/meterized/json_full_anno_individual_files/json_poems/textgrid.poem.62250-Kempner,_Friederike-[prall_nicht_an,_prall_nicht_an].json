{"textgrid.poem.62250": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[prall nicht an, prall nicht an]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prall nicht an, prall nicht an,", "tokens": ["Prall", "nicht", "an", ",", "prall", "nicht", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Steine gibt es \u00fcberall,", "tokens": ["Stei\u00b7ne", "gibt", "es", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man kann, und man kann", "tokens": ["Und", "man", "kann", ",", "und", "man", "kann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "$,", "KON", "PIS", "VMFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Haben einen b\u00f6sen Fall;", "tokens": ["Ha\u00b7ben", "ei\u00b7nen", "b\u00f6\u00b7sen", "Fall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sto\u00df nicht an, sto\u00df nicht an,", "tokens": ["Sto\u00df", "nicht", "an", ",", "sto\u00df", "nicht", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "B\u00f6se Menschen gibt es viel,", "tokens": ["B\u00f6\u00b7se", "Men\u00b7schen", "gibt", "es", "viel", ","], "token_info": ["word