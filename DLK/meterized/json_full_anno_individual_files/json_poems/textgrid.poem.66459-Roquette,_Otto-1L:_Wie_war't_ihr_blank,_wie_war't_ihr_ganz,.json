fo": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Weh thut es, euch beim Scheidegru\u00df", "tokens": ["Weh", "thut", "es", ",", "euch", "beim", "Schei\u00b7de\u00b7gru\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So in mein Herz zu malen!", "tokens": ["So", "in", "mein", "Herz", "zu", "ma\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und was noch tr\u00fcber ist, ich mu\u00df", "tokens": ["Und", "was", "noch", "tr\u00fc\u00b7ber", "ist", ",", "ich", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADJD", "VAFIN", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch leider noch bezahlen.", "tokens": ["Euch", "lei\u00b7der", "noch", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So lebt denn wohl! Es soll mein Lied", "tokens": ["So", "lebt", "denn", "wohl", "!", "Es", "soll", "mein", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$.", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verk\u00fcnden, was in euch mir schied,", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", ",", "was", "in", "euch", "mir", "schied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie opfervoll im Dienen war", "tokens": ["Wie", "op\u00b7fer\u00b7voll", "im", "Die\u00b7nen", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein treues Stiefelpaar!", "tokens": ["Mein", "treu\u00b7es", "Stie\u00b7fel\u00b7paar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}