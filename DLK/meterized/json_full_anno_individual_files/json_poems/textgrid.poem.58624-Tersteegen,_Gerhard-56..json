{"textgrid.poem.58624": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gottesdienst im Eigensinn, Gutes tun mit Zwang und Klagen,", "tokens": ["Got\u00b7tes\u00b7dienst", "im", "Ei\u00b7gen\u00b7sinn", ",", "Gu\u00b7tes", "tun", "mit", "Zwang", "und", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "NN", "VVINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sabbat halten mit Verdru\u00df, kann dem Herren nicht behagen;", "tokens": ["Sab\u00b7bat", "hal\u00b7ten", "mit", "Ver\u00b7dru\u00df", ",", "kann", "dem", "Her\u00b7ren", "nicht", "be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Tu und rede nimmer nicht, was du willst und dir gef\u00e4llt,", "tokens": ["Tu", "und", "re\u00b7de", "nim\u00b7mer", "nicht", ",", "was", "du", "willst", "und", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "KON", "PPER", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Das ist Sabbat, wenn man sich innig Gott gelassen h\u00e4lt!", "tokens": ["Das", "ist", "Sab\u00b7bat", ",", "wenn", "man", "sich", "in\u00b7nig", "Gott", "ge\u00b7las\u00b7sen", "h\u00e4lt", "!"], "token_info"