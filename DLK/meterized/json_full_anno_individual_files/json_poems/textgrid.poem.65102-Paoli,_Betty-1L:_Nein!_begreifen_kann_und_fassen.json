{"textgrid.poem.65102": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein! begreifen kann und fassen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein! begreifen kann und fassen", "tokens": ["Nein", "!", "be\u00b7grei\u00b7fen", "kann", "und", "fas\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVINF", "VMFIN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich den eig'nen Wahnsinn nicht!", "tokens": ["Ich", "den", "eig'\u00b7nen", "Wahn\u00b7sinn", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum hab' ich dich verlassen,", "tokens": ["Wa\u00b7rum", "hab'", "ich", "dich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Seele Luft und Licht?", "tokens": ["Mei\u00b7ner", "See\u00b7le", "Luft", "und", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Strahlten deine Augensterne", "tokens": ["Strahl\u00b7ten", "dei\u00b7ne", "Au\u00b7gens\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich nicht an, voll milder Pracht?", "tokens": ["Mich", "nicht", "an", ",", "voll", "mil\u00b7der", "Pracht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos"