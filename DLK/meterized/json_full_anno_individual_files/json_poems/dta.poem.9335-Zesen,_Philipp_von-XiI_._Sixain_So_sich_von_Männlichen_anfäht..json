{"dta.poem.9335": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XiI .  \n Sixain/  \n So sich von M\u00e4nnlichen anf\u00e4ht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Frauenzimmer sonst ist ja so eckel nicht/", "tokens": ["Das", "Frau\u00b7en\u00b7zim\u00b7mer", "sonst", "ist", "ja", "so", "ec\u00b7kel", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wie k\u00f6m\u0303t es doch/ da\u00df du verh\u00fcllest dein gesicht/", "tokens": ["wie", "k\u00f6m\u0303t", "es", "doch", "/", "da\u00df", "du", "ver\u00b7h\u00fcl\u00b7lest", "dein", "ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "VVFIN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Schwartze/ wenn zu dier ein wohlgezierter Freyer", "tokens": ["O", "Schwart\u00b7ze", "/", "wenn", "zu", "dier", "ein", "wohl\u00b7ge\u00b7zier\u00b7ter", "Frey\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$(", "KOUS", "APPR", "PDAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in deinen zimmer k\u00f6m\u0303t? und weichest alsobald", "tokens": ["in", "dei\u00b7nen", "zim\u00b7mer", "k\u00f6m\u0303t", "?", "und", "wei\u00b7chest", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "aus seinen augen weg/ weil er von geldekalt?", "tokens": ["aus", "sei\u00b7nen", "au\u00b7gen", "weg", "/", "weil", "er", "von", "gel\u00b7de\u00b7kalt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$(", "KOUS", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ach sih/ das wust ich nicht/ der schneider macht Dich", "tokens": ["Ach", "sih", "/", "das", "wust", "ich", "nicht", "/", "der", "schnei\u00b7der", "macht", "Dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "$(", "ART", "ADJA", "VVFIN", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "theuer.", "tokens": ["theu\u00b7er", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}