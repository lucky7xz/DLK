{"textgrid.poem.52449": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im falben Schein des Westens wanken Schiffe", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im falben Schein des Westens wanken Schiffe", "tokens": ["Im", "fal\u00b7ben", "Schein", "des", "Wes\u00b7tens", "wan\u00b7ken", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur fernen H\u00f6h', ihr Segel rund geschwellt;", "tokens": ["Zur", "fer\u00b7nen", "H\u00f6h'", ",", "ihr", "Se\u00b7gel", "rund", "ge\u00b7schwellt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Brandung Wog' am weiten Kieselriffe", "tokens": ["Der", "Bran\u00b7dung", "Wo\u00b7g'", "am", "wei\u00b7ten", "Kie\u00b7sel\u00b7rif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Verrieselt und zerschellt.", "tokens": ["Ver\u00b7rie\u00b7selt", "und", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die golddurchflo\u00dfnen Wolkenlagen bla\u00dften:", "tokens": ["Die", "gold\u00b7durch\u00b7flo\u00df\u00b7nen", "Wol\u00b7ken\u00b7la\u00b7gen", "bla\u00df\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Ozean bepurpurt Zitterglut;", "tokens": ["Den", "O\u00b7ze\u00b7an", "be\u00b7pur\u00b7purt", "Zit\u00b7ter\u00b7glut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Scho\u00df der Fern' entragen kaum die Masten", "tokens": ["Dem", "Scho\u00df", "der", "Fern'", "ent\u00b7ra\u00b7gen", "kaum", "die", "Mas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tauchen in die Flut.", "tokens": ["Und", "tau\u00b7chen", "in", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zur H\u00fctte kehrt mit Sensen dort und Harken", "tokens": ["Zur", "H\u00fct\u00b7te", "kehrt", "mit", "Sen\u00b7sen", "dort", "und", "Har\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der frohe Landmann, der sein Feld gem\u00e4ht.", "tokens": ["Der", "fro\u00b7he", "Land\u00b7mann", ",", "der", "sein", "Feld", "ge\u00b7m\u00e4ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Rhede ruht, von braunen Fischerbarken", "tokens": ["Die", "Rhe\u00b7de", "ruht", ",", "von", "brau\u00b7nen", "Fi\u00b7scher\u00b7bar\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Nachen \u00fcbers\u00e4't.", "tokens": ["Und", "Na\u00b7chen", "\u00fc\u00b7ber\u00b7s\u00e4'", "t."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["KON", "NN", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die D\u00e4mmerung betuscht die Waldgestade", "tokens": ["Die", "D\u00e4m\u00b7me\u00b7rung", "be\u00b7tuscht", "die", "Wald\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit zartem Grau; die scheue M\u00f6ve pfeift", "tokens": ["Mit", "zar\u00b7tem", "Grau", ";", "die", "scheu\u00b7e", "M\u00f6\u00b7ve", "pfeift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Kreidenfels der luftigen Leukade,", "tokens": ["Am", "Krei\u00b7den\u00b7fels", "der", "luf\u00b7ti\u00b7gen", "Leu\u00b7ka\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Sehnsucht einsam schweift.", "tokens": ["Wo", "Sehn\u00b7sucht", "ein\u00b7sam", "schweift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des Hafens Markt verstummt; der Bootsmann l\u00e4utet", "tokens": ["Des", "Ha\u00b7fens", "Markt", "ver\u00b7stummt", ";", "der", "Boots\u00b7mann", "l\u00e4u\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Nachtgebet; des Leuchtturms Lampe blinkt.", "tokens": ["Zum", "Nacht\u00b7ge\u00b7bet", ";", "des", "Leucht\u00b7turms", "Lam\u00b7pe", "blinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch fern hinweg zum Morgenhimmel deutet", "tokens": ["Doch", "fern", "hin\u00b7weg", "zum", "Mor\u00b7gen\u00b7him\u00b7mel", "deu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Muse mir und winkt:", "tokens": ["Die", "Mu\u00b7se", "mir", "und", "winkt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort wandelt sie, wo gr\u00fcne Schimmer z\u00fccken,", "tokens": ["Dort", "wan\u00b7delt", "sie", ",", "wo", "gr\u00fc\u00b7ne", "Schim\u00b7mer", "z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie, welche nur mein Geistesflug erreicht.", "tokens": ["Sie", ",", "wel\u00b7che", "nur", "mein", "Geis\u00b7tes\u00b7flug", "er\u00b7reicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Stunde schl\u00e4gt, wenn mit gesenkten Blicken", "tokens": ["Die", "Stun\u00b7de", "schl\u00e4gt", ",", "wenn", "mit", "ge\u00b7senk\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie dem Gedr\u00e4ng' entweicht.", "tokens": ["Sie", "dem", "Ge\u00b7dr\u00e4ng'", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Fleuch hin, mein Geist, wo zu der Alpen Zinken", "tokens": ["Fleuch", "hin", ",", "mein", "Geist", ",", "wo", "zu", "der", "Al\u00b7pen", "Zin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Goldbeleuchtung steigend sich entzieht,", "tokens": ["Die", "Gold\u00b7be\u00b7leuch\u00b7tung", "stei\u00b7gend", "sich", "ent\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo feucht und k\u00fchl des Tobels Schatten sinken", "tokens": ["Wo", "feucht", "und", "k\u00fchl", "des", "To\u00b7bels", "Schat\u00b7ten", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hallt der Amsel Lied.", "tokens": ["Und", "hallt", "der", "Am\u00b7sel", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dort wandelt sie, umw\u00f6lbt von Lerchbaumsprossen,", "tokens": ["Dort", "wan\u00b7delt", "sie", ",", "um\u00b7w\u00f6lbt", "von", "Lerch\u00b7baum\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Staunt vorgelehnt am br\u00e4unlich klaren Teich;", "tokens": ["Staunt", "vor\u00b7ge\u00b7lehnt", "am", "br\u00e4un\u00b7lich", "kla\u00b7ren", "Teich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Spiegel gl\u00fcht, mit Gletscherlicht begossen;", "tokens": ["Sein", "Spie\u00b7gel", "gl\u00fcht", ",", "mit", "Glet\u00b7scher\u00b7licht", "be\u00b7gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Antlitz nur ist bleich.", "tokens": ["Ihr", "Ant\u00b7litz", "nur", "ist", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Des G\u00fcrtels Schleif' erheben laue Winde,", "tokens": ["Des", "G\u00fcr\u00b7tels", "Schleif'", "er\u00b7he\u00b7ben", "lau\u00b7e", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fl\u00fcsternd wallt das schwarze Seitenband,", "tokens": ["Und", "fl\u00fcs\u00b7ternd", "wallt", "das", "schwar\u00b7ze", "Sei\u00b7ten\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das seit der Trennung, statt der Rosenbinde,", "tokens": ["Das", "seit", "der", "Tren\u00b7nung", ",", "statt", "der", "Ro\u00b7sen\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie um die Locken wand.", "tokens": ["Sie", "um", "die", "Lo\u00b7cken", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Sprosser Largo t\u00f6nt in Wechselch\u00f6ren,", "tokens": ["Der", "Spros\u00b7ser", "Lar\u00b7go", "t\u00f6nt", "in", "Wech\u00b7selc\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Busch zu Busch; sie horcht und hemmt den Lauf,", "tokens": ["Von", "Busch", "zu", "Busch", ";", "sie", "horcht", "und", "hemmt", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dringt dann ins Dunkel gr\u00fcnverwachs'ner F\u00f6hren", "tokens": ["Dringt", "dann", "ins", "Dun\u00b7kel", "gr\u00fcn\u00b7ver\u00b7wachs'\u00b7ner", "F\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und blickt tiefatmend auf.", "tokens": ["Und", "blickt", "tie\u00b7fat\u00b7mend", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der letzte Hall der fernen Abendglocken", "tokens": ["Der", "letz\u00b7te", "Hall", "der", "fer\u00b7nen", "A\u00b7bend\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstummt und stirbt. Schau, wie sie ernster sinnt!", "tokens": ["Ver\u00b7stummt", "und", "stirbt", ".", "Schau", ",", "wie", "sie", "erns\u00b7ter", "sinnt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie neigt die Stirn auf die gel\u00f6sten Locken,", "tokens": ["Sie", "neigt", "die", "Stirn", "auf", "die", "ge\u00b7l\u00f6s\u00b7ten", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihre Thr\u00e4ne rinnt.", "tokens": ["Und", "ih\u00b7re", "Thr\u00e4\u00b7ne", "rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ihr tieflasurnen Fr\u00fchlingsenzianen", "tokens": ["Ihr", "tie\u00b7fla\u00b7sur\u00b7nen", "Fr\u00fch\u00b7lings\u00b7en\u00b7zi\u00b7a\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fa\u00dft auf die Thr\u00e4ne, welche sie vergo\u00df!", "tokens": ["Fa\u00dft", "auf", "die", "Thr\u00e4\u00b7ne", ",", "wel\u00b7che", "sie", "ver\u00b7go\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sink ein, o Nacht, und la\u00df nur mich es ahnen,", "tokens": ["Sink", "ein", ",", "o", "Nacht", ",", "und", "la\u00df", "nur", "mich", "es", "ah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "KON", "VVIMP", "ADV", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um welchen Freund sie flo\u00df!", "tokens": ["Um", "wel\u00b7chen", "Freund", "sie", "flo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Im falben Schein des Westens wanken Schiffe", "tokens": ["Im", "fal\u00b7ben", "Schein", "des", "Wes\u00b7tens", "wan\u00b7ken", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur fernen H\u00f6h', ihr Segel rund geschwellt;", "tokens": ["Zur", "fer\u00b7nen", "H\u00f6h'", ",", "ihr", "Se\u00b7gel", "rund", "ge\u00b7schwellt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Brandung