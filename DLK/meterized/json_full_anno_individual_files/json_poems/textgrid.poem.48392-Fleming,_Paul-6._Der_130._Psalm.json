INF", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und meine Seele harrt. Der frische Saft und Kern,", "tokens": ["und", "mei\u00b7ne", "See\u00b7le", "harrt", ".", "Der", "fri\u00b7sche", "Saft", "und", "Kern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "den sein Wort in sich hat, hei\u00dft so mich auf ihn hoffen.", "tokens": ["den", "sein", "Wort", "in", "sich", "hat", ",", "hei\u00dft", "so", "mich", "auf", "ihn", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PRF", "VAFIN", "$,", "VVFIN", "ADV", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Di\u00df Wohnhaus meiner Seel' halt' ich dem Herren offen", "tokens": ["Di\u00df", "Wohn\u00b7haus", "mei\u00b7ner", "Seel'", "halt'", "ich", "dem", "Her\u00b7ren", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "nicht an dem Tage nur. Wenn noch die dicke Nacht", "tokens": ["nicht", "an", "dem", "Ta\u00b7ge", "nur", ".", "Wenn", "noch", "die", "di\u00b7cke", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "$.", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "umb mein Gemach ist her und eh die Sonn' erwacht,", "tokens": ["umb", "mein", "Ge\u00b7mach", "ist", "her", "und", "eh", "die", "Sonn'", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VAFIN", "ADV", "KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "so denk ich schon an ihn und warte mit Verlangen", "tokens": ["so", "denk", "ich", "schon", "an", "ihn", "und", "war\u00b7te", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "auf ihn und seinen Trost. Ganz Israel sol hangen", "tokens": ["auf", "ihn", "und", "sei\u00b7nen", "Trost", ".", "Ganz", "Is\u00b7rael", "sol", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$.", "ADV", "NE", "VMFIN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "mit seinen Hoffnungen und Seufzen, Herr, an dir,", "tokens": ["mit", "sei\u00b7nen", "Hoff\u00b7nun\u00b7gen", "und", "Seuf\u00b7zen", ",", "Herr", ",", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "denn blos bei dir allein ist Gnade f\u00fcr und f\u00fcr.", "tokens": ["denn", "blos", "bei", "dir", "al\u00b7lein", "ist", "Gna\u00b7de", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ADV", "VAFIN", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du bist die Gnade selbst. Wol! hoffet all' ihr Frommen,", "tokens": ["Du", "bist", "die", "Gna\u00b7de", "selbst", ".", "Wol", "!", "hof\u00b7fet", "all'", "ihr", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$.", "ADV", "$.", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "wir wollen doch durch ihn zur alten Freiheit kommen!", "tokens": ["wir", "wol\u00b7len", "doch", "durch", "ihn", "zur", "al\u00b7ten", "Frei\u00b7heit", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Erl\u00f6sung hat er gnung. Und er, der treue Gott,", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "hat", "er", "gnung", ".", "Und", "er", ",", "der", "treu\u00b7e", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$.", "KON", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "wird Jacob machen los von aller Schuld und Not.", "tokens": ["wird", "Ja\u00b7cob", "ma\u00b7chen", "los", "von", "al\u00b7ler", "Schuld", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}