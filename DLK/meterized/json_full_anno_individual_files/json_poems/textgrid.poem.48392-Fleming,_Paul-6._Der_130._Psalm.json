{"textgrid.poem.48392": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Der 130. Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus diesem tiefen Schlund', aus dieser schwarzen Gruft,", "tokens": ["Aus", "die\u00b7sem", "tie\u00b7fen", "Schlund'", ",", "aus", "die\u00b7ser", "schwar\u00b7zen", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hab' ich so oft und oft, o Herr, zu dir geruft:", "tokens": ["hab'", "ich", "so", "oft", "und", "oft", ",", "o", "Herr", ",", "zu", "dir", "ge\u00b7ruft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "KON", "ADV", "$,", "FM", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach Vater, h\u00f6re mich! ach la\u00df dein' Ohren merken", "tokens": ["Ach", "Va\u00b7ter", ",", "h\u00f6\u00b7re", "mich", "!", "ach", "la\u00df", "dein'", "Oh\u00b7ren", "mer\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PPER", "$.", "XY", "XY", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf meines Flehens Stimm'. Herr, so du nach den Werken", "tokens": ["auf", "mei\u00b7nes", "Fle\u00b7hens", "Stimm'", ".", "Herr", ",", "so", "du", "nach", "den", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "NN", "$,", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "mit uns verfahren wilst, uns unsre Missetat", "tokens": ["mit", "uns", "ver\u00b7fah\u00b7ren", "wilst", ",", "uns", "uns\u00b7re", "Mis\u00b7se\u00b7tat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "$,", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und S\u00fcnde rechnen zu, so man ver\u00fcbet hat,", "tokens": ["und", "S\u00fcn\u00b7de", "rech\u00b7nen", "zu", ",", "so", "man", "ver\u00b7\u00fc\u00b7bet", "hat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "PIS", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Herr, Herr, wer wird vor dir in seinem Tun bestehen?", "tokens": ["Herr", ",", "Herr", ",", "wer", "wird", "vor", "dir", "in", "sei\u00b7nem", "Tun", "be\u00b7ste\u00b7hen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWS", "VAFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir m\u00fcssen allesampt auf eins zu scheitern gehen.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "al\u00b7le\u00b7sampt", "auf", "eins", "zu", "schei\u00b7tern", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PIS", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du aber, Gott, vergiebst, da\u00df man dich f\u00fcrchten sol,", "tokens": ["Du", "a\u00b7ber", ",", "Gott", ",", "ver\u00b7giebst", ",", "da\u00df", "man", "dich", "f\u00fcrch\u00b7ten", "sol", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "$,", "KOUS", "PIS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und so kan mancher noch vor dir bestehen wol,", "tokens": ["und", "so", "kan", "man\u00b7cher", "noch", "vor", "dir", "be\u00b7ste\u00b7hen", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "ADV", "APPR", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der nur frisch aus bekennt und Gnad' umb Recht begehret,", "tokens": ["der", "nur", "frisch", "aus", "be\u00b7kennt", "und", "Gnad'", "umb", "Recht", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "ADJD", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "das ihm denn, milder Herr, von dir stracks wird gewehret.", "tokens": ["das", "ihm", "denn", ",", "mil\u00b7der", "Herr", ",", "von", "dir", "stracks", "wird", "ge\u00b7weh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So kan man selig sein. Ich harre meines Herrn,", "tokens": ["So", "kan", "man", "se\u00b7lig", "sein", ".", "Ich", "har\u00b7re", "mei\u00b7nes", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VAINF", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und meine Seele harrt. Der frische Saft und Kern,", "tokens": ["und", "mei\u00b7ne", "See\u00b7le", "harrt", ".", "Der", "fri\u00b7sche", "Saft", "und", "Kern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "den sein Wort in sich hat, hei\u00dft so mich auf ihn hoffen.", "tokens": ["den", "sein", "Wort", "in", "sich", "hat", ",", "hei\u00dft", "so", "mich", "auf", "ihn", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PRF", "VAFIN", "$,", "VVFIN", "ADV", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Di\u00df Wohnhaus meiner Seel' halt' ich dem Herren offen", "tokens": ["Di\u00df", "Wohn\u00b7haus", "mei\u00b7ner", "Seel'", "halt'", "ich", "dem", "Her\u00b7ren", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "nicht an dem Tage nur. Wenn noch die dicke Nacht", "tokens": ["nicht", "an", "dem", "Ta\u00b7ge", "nur", ".", "Wenn", "noch", "die", "di\u00b7cke", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "$.", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "umb mein Gemach ist her und eh die Sonn' erwacht,", "tokens": ["umb", "mein", "Ge\u00b7mach", "ist", "her", "und", "eh", "die", "Sonn'", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VAFIN", "ADV", "KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "so denk ich schon an ihn und warte mit Verlangen", "tokens": ["so", "denk", "ich", "schon", "an", "ihn", "und", "war\u00b7te", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "auf ihn und seinen Trost. Ganz Israel sol hangen", "tokens": ["auf", "ihn", "und", "sei\u00b7nen", "Trost", ".", "Ganz", "Is\u00b7rael", "sol", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$.", "ADV", "NE", "VMFIN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "mit seinen Hoffnungen und Seufzen, Herr, an dir,", "tokens": ["mit", "sei\u00b7nen", "Hoff\u00b7nun\u00b7gen", "und", "Seuf\u00b7zen", ",", "Herr", ",", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "denn blos bei dir allein ist Gnade f\u00fcr und f\u00fcr.", "tokens": ["denn", "blos", "bei", "dir", "al\u00b7lein", "ist", "Gna\u00b7de", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ADV", "VAFIN", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du bist die Gnade selbst. Wol! hoffet all' ihr Frommen,", "tokens": ["Du", "bist", "die", "Gna\u00b7de", "selbst", ".", "Wol", "!", "hof\u00b7fet", "all'", "ihr", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$.", "ADV", "$.", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "wir wollen doch durch ihn zur alten Freiheit kommen!", "tokens": ["wir", "wol\u00b7len", "doch", "durch", "ihn", "zur", "al\u00b7ten", "Frei\u00b7heit", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Erl\u00f6sung hat er gnung. Und er, der treue Gott,", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "hat", "er", "gnung", ".", "Und", "er", ",", "der", "treu\u00b7e", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$.", "KON", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "wird Jacob machen los von aller Schuld und Not.", "tokens": ["wird", "Ja\u00b7cob", "ma\u00b7chen", "los", "von", "al\u00b7ler", "Schuld", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE"