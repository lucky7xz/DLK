{"dta.poem.19619": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Hussens Kerker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Es geht mit mir zu Ende,", "tokens": ["Es", "geht", "mit", "mir", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Sach und Spruch ist schon", "tokens": ["Mein", "Sach", "und", "Spruch", "ist", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch \u00fcber Menschenh\u00e4nde", "tokens": ["Hoch", "\u00fc\u00b7ber", "Men\u00b7schen\u00b7h\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ger\u00fcckt vor Gottes Thron,", "tokens": ["Ge\u00b7r\u00fcckt", "vor", "Got\u00b7tes", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon schwebt auf einer Wolke,", "tokens": ["Schon", "schwebt", "auf", "ei\u00b7ner", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Umringt von seinem Volke,", "tokens": ["Um\u00b7ringt", "von", "sei\u00b7nem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Entgegen mir des Menschen Sohn.", "tokens": ["Ent\u00b7ge\u00b7gen", "mir", "des", "Men\u00b7schen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Kerker will ich preisen,", "tokens": ["Den", "Ker\u00b7ker", "will", "ich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kerker, der ist gut!", "tokens": ["Der", "Ker\u00b7ker", ",", "der", "ist", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Fensterkreuz von Eisen", "tokens": ["Das", "Fens\u00b7ter\u00b7kreuz", "von", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Blickt auf die frische Flut", "tokens": ["Blickt", "auf", "die", "fri\u00b7sche", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und zwischen seinen St\u00e4ben", "tokens": ["Und", "zwi\u00b7schen", "sei\u00b7nen", "St\u00e4\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Seh' ich ein Segel schweben,", "tokens": ["Seh'", "ich", "ein", "Se\u00b7gel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Darob im Blau die Firne ruht.", "tokens": ["Da\u00b7rob", "im", "Blau", "die", "Fir\u00b7ne", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie nah die Flut ich f\u00fchle,", "tokens": ["Wie", "nah", "die", "Flut", "ich", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als l\u00e4g' ich drein versenkt,", "tokens": ["Als", "l\u00e4g'", "ich", "drein", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit wundersamer K\u00fchle", "tokens": ["Mit", "wun\u00b7der\u00b7sa\u00b7mer", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird mir der Leib getr\u00e4nkt \u2014", "tokens": ["Wird", "mir", "der", "Leib", "ge\u00b7tr\u00e4nkt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch seh' ich eine Traube", "tokens": ["Auch", "seh'", "ich", "ei\u00b7ne", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit einem rothen Laube,", "tokens": ["Mit", "ei\u00b7nem", "ro\u00b7then", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die tief herab ins Fenster h\u00e4ngt.", "tokens": ["Die", "tief", "her\u00b7ab", "ins", "Fens\u00b7ter", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es ist die Zeit zu feiern!", "tokens": ["Es", "ist", "die", "Zeit", "zu", "fei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es kommt die gro\u00dfe Ruh!", "tokens": ["Es", "kommt", "die", "gro\u00b7\u00dfe", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort lenkt ein Zug von Reihern", "tokens": ["Dort", "lenkt", "ein", "Zug", "von", "Rei\u00b7hern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem ew'gen Lenze zu,", "tokens": ["Dem", "ew'\u00b7gen", "Len\u00b7ze", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie wissen Pfad und Stege,", "tokens": ["Sie", "wis\u00b7sen", "Pfad", "und", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie kennen ihre Wege \u2014", "tokens": ["Sie", "ken\u00b7nen", "ih\u00b7re", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was, meine Seele, f\u00fcrchtest du?", "tokens": ["Was", ",", "mei\u00b7ne", "See\u00b7le", ",", "f\u00fcrch\u00b7test", "du", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}