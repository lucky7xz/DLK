{"dta.poem.11809": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So nebneinander gehn durchs Leben Menschen hin", "tokens": ["So", "neb\u00b7nein\u00b7an\u00b7der", "gehn", "durchs", "Le\u00b7ben", "Men\u00b7schen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df keiner wei\u00df noch fragt, wie ich gesinnt ihm bin.", "tokens": ["Da\u00df", "kei\u00b7ner", "wei\u00df", "noch", "fragt", ",", "wie", "ich", "ge\u00b7sinnt", "ihm", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol mancher ist dein Feind, und will es nur nicht zeigen,", "tokens": ["Wol", "man\u00b7cher", "ist", "dein", "Feind", ",", "und", "will", "es", "nur", "nicht", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wol mancher auch dein Freund, und will es nur verschweigen.", "tokens": ["Wol", "man\u00b7cher", "auch", "dein", "Freund", ",", "und", "will", "es", "nur", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verschweigen m\u00f6chten sie die Feindschaft, die sie hegen;", "tokens": ["Ver\u00b7schwei\u00b7gen", "m\u00f6ch\u00b7ten", "sie", "die", "Feind\u00b7schaft", ",", "die", "sie", "he\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auch die Freundlichkeit verschweigt ihr mir weswegen?", "tokens": ["Doch", "auch", "die", "Freund\u00b7lich\u00b7keit", "ver\u00b7schweigt", "ihr", "mir", "wes\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}