{"textgrid.poem.46833": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[freundesbrief, zu guter Stunde]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundesbrief, zu guter Stunde", "tokens": ["Freun\u00b7des\u00b7brief", ",", "zu", "gu\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommst du, ob, in der du kamest,", "tokens": ["Kommst", "du", ",", "ob", ",", "in", "der", "du", "ka\u00b7mest", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eine b\u00f6se Stund' auch sei.", "tokens": ["Ei\u00b7ne", "b\u00f6\u00b7se", "Stund'", "auch", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn ich f\u00fchlte mich verlassen", "tokens": ["Denn", "ich", "f\u00fchl\u00b7te", "mich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den Menschen; doch den Menschen", "tokens": ["Von", "den", "Men\u00b7schen", ";", "doch", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steht's, mich zu verlassen, frei.", "tokens": ["Steht's", ",", "mich", "zu", "ver\u00b7las\u