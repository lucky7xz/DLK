{"dta.poem.10550": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Sonnenuntergang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die gl\u00fchend rothe Sonne steigt", "tokens": ["Die", "gl\u00fc\u00b7hend", "ro\u00b7the", "Son\u00b7ne", "steigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinab in's weitaufschauernde,", "tokens": ["Hin\u00b7ab", "in's", "weit\u00b7auf\u00b7schau\u00b7ern\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Silbergraue Weltmeer;", "tokens": ["Sil\u00b7ber\u00b7grau\u00b7e", "Welt\u00b7meer", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Luftgebilde, rosig angehaucht,", "tokens": ["Luft\u00b7ge\u00b7bil\u00b7de", ",", "ro\u00b7sig", "an\u00b7ge\u00b7haucht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wallen ihr nach, und gegen\u00fcber,", "tokens": ["Wal\u00b7len", "ihr", "nach", ",", "und", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KON", "PTKVZ", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Aus herbstlich d\u00e4mmernden Wolkenschleiern,", "tokens": ["Aus", "herbst\u00b7lich", "d\u00e4m\u00b7mern\u00b7den", "Wol\u00b7ken\u00b7schlei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ein traurig todtblasses Antlitz,", "tokens": ["Ein", "trau\u00b7rig", "todt\u00b7blas\u00b7ses", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bricht hervor der Mond,", "tokens": ["Bricht", "her\u00b7vor", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Und hinter ihm, Lichtf\u00fcnkchen,", "tokens": ["Und", "hin\u00b7ter", "ihm", ",", "Licht\u00b7f\u00fcnk\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Nebelweit, schimmern die Sterne.", "tokens": ["Ne\u00b7bel\u00b7weit", ",", "schim\u00b7mern", "die", "Ster\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Einst am Himmel gl\u00e4nzten,", "tokens": ["Einst", "am", "Him\u00b7mel", "gl\u00e4nz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ehlich vereint,", "tokens": ["Eh\u00b7lich", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Luna, die G\u00f6ttin, und Sol, der Gott,", "tokens": ["Lu\u00b7na", ",", "die", "G\u00f6t\u00b7tin", ",", "und", "Sol", ",", "der", "Gott", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "KON", "VMFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und es wimmelten um sie her die Sterne,", "tokens": ["Und", "es", "wim\u00b7mel\u00b7ten", "um", "sie", "her", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die kleinen, unschuldigen Kinder.", "tokens": ["Die", "klei\u00b7nen", ",", "un\u00b7schul\u00b7di\u00b7gen", "Kin\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch b\u00f6se Zungen zischelten Zwiespalt,", "tokens": ["Doch", "b\u00f6\u00b7se", "Zun\u00b7gen", "zi\u00b7schel\u00b7ten", "Zwies\u00b7palt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und es trennte sich feindlich", "tokens": ["Und", "es", "trenn\u00b7te", "sich", "feind\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das hohe, leuchtende Eh'paar.", "tokens": ["Das", "ho\u00b7he", ",", "leuch\u00b7ten\u00b7de", "Eh'\u00b7paar", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt am Tage, in einsamer Pracht,", "tokens": ["Jetzt", "am", "Ta\u00b7ge", ",", "in", "ein\u00b7sa\u00b7mer", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ergeht sich dort oben der Sonnengott,", "tokens": ["Er\u00b7geht", "sich", "dort", "o\u00b7ben", "der", "Son\u00b7nen\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ob seiner Herrlichkeit", "tokens": ["Ob", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Angebetet und vielbesungen", "tokens": ["An\u00b7ge\u00b7be\u00b7tet", "und", "viel\u00b7be\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Von stolzen, gl\u00fcckgeh\u00e4rteten Menschen.", "tokens": ["Von", "stol\u00b7zen", ",", "gl\u00fcck\u00b7ge\u00b7h\u00e4r\u00b7te\u00b7ten", "Men\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aber des Nachts", "tokens": ["A\u00b7ber", "des", "Nachts"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Am Himmel wandelt Luna,", "tokens": ["Am", "Him\u00b7mel", "wan\u00b7delt", "Lu\u00b7na", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Die arme Mutter", "tokens": ["Die", "ar\u00b7me", "Mut\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Mit ihren verwaisten Sternenkindern,", "tokens": ["Mit", "ih\u00b7ren", "ver\u00b7wais\u00b7ten", "Ster\u00b7nen\u00b7kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und sie gl\u00e4nzt in stummer Wehmuth,", "tokens": ["Und", "sie", "gl\u00e4nzt", "in", "stum\u00b7mer", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und liebende M\u00e4dchen und sanfte Dichter", "tokens": ["Und", "lie\u00b7ben\u00b7de", "M\u00e4d\u00b7chen", "und", "sanf\u00b7te", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Weihen ihr Thr\u00e4nen und Lieder.", "tokens": ["Wei\u00b7hen", "ihr", "Thr\u00e4\u00b7nen", "und", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Die weiche Luna! Weiblich gesinnt", "tokens": ["Die", "wei\u00b7che", "Lu\u00b7na", "!", "Weib\u00b7lich", "ge\u00b7sinnt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Liebt sie noch immer den sch\u00f6nen Gemahl.", "tokens": ["Liebt", "sie", "noch", "im\u00b7mer", "den", "sch\u00f6\u00b7nen", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Gegen Abend, zitternd und bleich,", "tokens": ["Ge\u00b7gen", "A\u00b7bend", ",", "zit\u00b7ternd", "und", "bleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Lauscht sie hervor aus leichtem Gew\u00f6lk,", "tokens": ["Lauscht", "sie", "her\u00b7vor", "aus", "leich\u00b7tem", "Ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und schaut nach dem Scheidenden, schmerzlich,", "tokens": ["Und", "schaut", "nach", "dem", "Schei\u00b7den\u00b7den", ",", "schmerz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und m\u00f6chte ihm \u00e4ngstlich rufen: \u201eKomm!", "tokens": ["Und", "m\u00f6ch\u00b7te", "ihm", "\u00e4ngst\u00b7lich", "ru\u00b7fen", ":", "\u201e", "Komm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$.", "$(", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Komm! die Kinder verlangen nach Dir \u2014 \u201c", "tokens": ["Komm", "!", "die", "Kin\u00b7der", "ver\u00b7lan\u00b7gen", "nach", "Dir", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "ART", "NN", "VVFIN", "APPR", "PPER", "$(", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Aber der trotzige Sonnengott,", "tokens": ["A\u00b7ber", "der", "trot\u00b7zi\u00b7ge", "Son\u00b7nen\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Bei dem Anblick der Gattin ergl\u00fcht' er", "tokens": ["Bei", "dem", "An\u00b7blick", "der", "Gat\u00b7tin", "er\u00b7gl\u00fcht'", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "In doppeltem Purpur,", "tokens": ["In", "dop\u00b7pel\u00b7tem", "Pur\u00b7pur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vor Zorn und Schmerz,", "tokens": ["Vor", "Zorn", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und unerbittlich eilt er hinab", "tokens": ["Und", "un\u00b7er\u00b7bitt\u00b7lich", "eilt", "er", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "In sein fluthenkaltes Wittwerbett.", "tokens": ["In", "sein", "flut\u00b7hen\u00b7kal\u00b7tes", "Witt\u00b7wer\u00b7bett", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}