{"dta.poem.6246": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man mu\u00df demnach die Macht der Vorw\u00fcrff'unterscheiden", "tokens": ["Man", "mu\u00df", "dem\u00b7nach", "die", "Macht", "der", "Vor\u00b7w\u00fcrff'\u00b7un\u00b7ter\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit Bedacht wol \u00fcberlegen,", "tokens": ["Und", "mit", "Be\u00b7dacht", "wol", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was eigentlich f\u00fcr Art die C\u00f6rper in sich hegen,", "tokens": ["Was", "ei\u00b7gent\u00b7lich", "f\u00fcr", "Art", "die", "C\u00f6r\u00b7per", "in", "sich", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Wie sie verbunden sind, wenn sie den Leib bewegen.", "tokens": ["Wie", "sie", "ver\u00b7bun\u00b7den", "sind", ",", "wenn", "sie", "den", "Leib", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer fasset doch, wie mancherley", "tokens": ["Wer", "fas\u00b7set", "doch", ",", "wie", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "$,", "PWAV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Art der C\u00f6rperchen, so auf uns w\u00fcrcken, sey?", "tokens": ["Die", "Art", "der", "C\u00f6r\u00b7per\u00b7chen", ",", "so", "auf", "uns", "w\u00fcr\u00b7cken", ",", "sey", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "APPR", "PPER", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir wollen uns denn itzt dahin bestreben,", "tokens": ["Wir", "wol\u00b7len", "uns", "denn", "itzt", "da\u00b7hin", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Jhr innerstes zu sehn,", "tokens": ["Ihr", "in\u00b7ners\u00b7tes", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Wie mit geheim-unsichtbaren Geweben", "tokens": ["Wie", "mit", "ge\u00b7heim\u00b7un\u00b7sicht\u00b7ba\u00b7ren", "Ge\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sie durch einander gehn,", "tokens": ["Sie", "durch", "ein\u00b7an\u00b7der", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und in einander stecken.", "tokens": ["Und", "in", "ein\u00b7an\u00b7der", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}