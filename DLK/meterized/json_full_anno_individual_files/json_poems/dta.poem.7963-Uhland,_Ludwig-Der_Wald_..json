{"dta.poem.7963": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Wald .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Was je mir spielt\u2019 um Sinnen und Gem\u00fcthe", "tokens": ["Was", "je", "mir", "spielt'", "um", "Sin\u00b7nen", "und", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von frischem Gr\u00fcn, von k\u00fchlen D\u00e4mmerungen,", "tokens": ["Von", "fri\u00b7schem", "Gr\u00fcn", ",", "von", "k\u00fch\u00b7len", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das hat noch eben mich bedeckt, umschlungen,", "tokens": ["Das", "hat", "noch", "e\u00b7ben", "mich", "be\u00b7deckt", ",", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als eines Maienwaldes Lustgebiete.", "tokens": ["Als", "ei\u00b7nes", "Mai\u00b7en\u00b7wal\u00b7des", "Lust\u00b7ge\u00b7bie\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was je in Traum und Wachen mich umgl\u00fchte", "tokens": ["Was", "je", "in", "Traum", "und", "Wa\u00b7chen", "mich", "um\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Blumenschein, von Knospen, kaum gesprungen,", "tokens": ["Von", "Blu\u00b7men\u00b7schein", ",", "von", "Knos\u00b7pen", ",", "kaum", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das kam durch die Geb\u00fcsche hergedrungen,", "tokens": ["Das", "kam", "durch", "die", "Ge\u00b7b\u00fc\u00b7sche", "her\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als leichte J\u00e4gerin, des Waldes Bl\u00fcthe.", "tokens": ["Als", "leich\u00b7te", "J\u00e4\u00b7ge\u00b7rin", ",", "des", "Wal\u00b7des", "Bl\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie floh dahin, ich eilte nach, mit Flehen,", "tokens": ["Sie", "floh", "da\u00b7hin", ",", "ich", "eil\u00b7te", "nach", ",", "mit", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald h\u00e4tten meine Arme sie gebunden,", "tokens": ["Bald", "h\u00e4t\u00b7ten", "mei\u00b7ne", "Ar\u00b7me", "sie", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da mu\u00dfte schnell der Morgentraum verwehen.", "tokens": ["Da", "mu\u00df\u00b7te", "schnell", "der", "Mor\u00b7gen\u00b7traum", "ver\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Schicksal, das mir selbst nicht Hoffnung g\u00f6nnte!", "tokens": ["O", "Schick\u00b7sal", ",", "das", "mir", "selbst", "nicht", "Hoff\u00b7nung", "g\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir ist die Sch\u00f6nste nicht allein verschwunden,", "tokens": ["Mir", "ist", "die", "Sch\u00f6ns\u00b7te", "nicht", "al\u00b7lein", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wald sogar, drin ich sie suchen k\u00f6nnte.", "tokens": ["Der", "Wald", "so\u00b7gar", ",", "drin", "ich", "sie", "su\u00b7chen", "k\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}