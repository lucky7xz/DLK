{"dta.poem.1152": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "38. Die Sch\u00f6ne Einh\u00e4ndige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar kan ich nicht von dir/ mein Kind/ wie jener sagen: ", "tokens": ["Zwar", "kan", "ich", "nicht", "von", "dir", "/", "mein", "Kind", "/", "wie", "je\u00b7ner", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "$(", "PPOSAT", "NN", "$(", "KOKOM", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch sch\u00f6ner H\u00e4nde Band macht mich die Lieb ihr eigen.", "tokens": ["Durch", "sch\u00f6\u00b7ner", "H\u00e4n\u00b7de", "Band", "macht", "mich", "die", "Lieb", "ihr", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch werd ich mich auch nicht/ wie er betr\u00fcbt/ beklagen:", "tokens": ["Doch", "werd", "ich", "mich", "auch", "nicht", "/", "wie", "er", "be\u00b7tr\u00fcbt", "/", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADV", "PTKNEG", "$(", "PWAV", "PPER", "VVPP", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Darum mein bestes Los ist liebend sterb- und schweigen.", "tokens": ["Da\u00b7rum", "mein", "bes\u00b7tes", "Los", "ist", "lie\u00b7bend", "ster\u00b7b", "und", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "TRUNC", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du kanst mit einer Hand gnug Holtz zum Feuer tragen/", "tokens": ["Du", "kanst", "mit", "ei\u00b7ner", "Hand", "gnug", "Holtz", "zum", "Feu\u00b7er", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ADV", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Darffst unsre Flamme nicht mit beyden H\u00e4nden zeigen.", "tokens": ["Darffst", "uns\u00b7re", "Flam\u00b7me", "nicht", "mit", "bey\u00b7den", "H\u00e4n\u00b7den", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKNEG", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer will/ weil sie so gern uns mit Umfassen plagen/", "tokens": ["Wer", "will", "/", "weil", "sie", "so", "gern", "uns", "mit", "Um\u00b7fas\u00b7sen", "pla\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "KOUS", "PPER", "ADV", "ADV", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem Vielfu\u00df in der See/ den Affen Gunst zuneigen.", "tokens": ["Dem", "Viel\u00b7fu\u00df", "in", "der", "See", "/", "den", "Af\u00b7fen", "Gunst", "zu\u00b7nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "W\u00e4r Hercules wie du mit einer Hand versehn/", "tokens": ["W\u00e4r", "Her\u00b7cu\u00b7les", "wie", "du", "mit", "ei\u00b7ner", "Hand", "ver\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KOKOM", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und Bacchus Weiber auch/ k\u00f6nt Anteus noch leben/", "tokens": ["Und", "Bac\u00b7chus", "Wei\u00b7ber", "auch", "/", "k\u00f6nt", "An\u00b7teus", "noch", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "ADV", "$(", "VVFIN", "NE", "ADV", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Am Orfeus w\u00e4re nicht der schn\u00f6de Mord geschehn.", "tokens": ["Am", "Or\u00b7feus", "w\u00e4\u00b7re", "nicht", "der", "schn\u00f6\u00b7de", "Mord", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wofern der Sch\u00f6nheit Prei\u00df soll an den H\u00e4nden seyn/", "tokens": ["Wo\u00b7fern", "der", "Sch\u00f6n\u00b7heit", "Prei\u00df", "soll", "an", "den", "H\u00e4n\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VMFIN", "APPR", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So streicht ihn Briareus mit tausend Fingern ein:", "tokens": ["So", "streicht", "ihn", "Bri\u00b7a\u00b7reus", "mit", "tau\u00b7send", "Fin\u00b7gern", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Ausspruch/ ob di\u00df wahr/ m\u00f6gt ihr/ Verliebte/ geben.", "tokens": ["Den", "Aus\u00b7spruch", "/", "ob", "di\u00df", "wahr", "/", "m\u00f6gt", "ihr", "/", "Ver\u00b7lieb\u00b7te", "/", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PDS", "ADJD", "$(", "VMFIN", "PPER", "$(", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}