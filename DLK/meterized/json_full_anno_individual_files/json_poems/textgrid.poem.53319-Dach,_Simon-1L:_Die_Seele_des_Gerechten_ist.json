{"textgrid.poem.53319": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Seele des Gerechten ist", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Seele des Gerechten ist", "tokens": ["Die", "See\u00b7le", "des", "Ge\u00b7rech\u00b7ten", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Befreyt von angst, betrug vnd list,", "tokens": ["Be\u00b7freyt", "von", "angst", ",", "be\u00b7trug", "vnd", "list", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sp\u00e4ht vnd fr\u00fce sie m\u00f6ge sterben.", "tokens": ["Wie", "sp\u00e4ht", "vnd", "fr\u00fce", "sie", "m\u00f6\u00b7ge", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn wer hie Trew vnd Vnschuld liebt,", "tokens": ["Denn", "wer", "hie", "Trew", "vnd", "Vn\u00b7schuld", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd seinem Gotte stets sich giebt,", "tokens": ["Vnd", "sei\u00b7nem", "Got\u00b7te", "stets", "sich", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kan nicht verderben.", "tokens": ["Kan", "nicht", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nur da\u00df der \u00fcberbliebnen Zahl", "tokens": ["Nur", "da\u00df", "der", "\u00fc\u00b7berb\u00b7lieb\u00b7nen", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empfindet dessen Leid vnd Qual,", "tokens": ["Emp\u00b7fin\u00b7det", "des\u00b7sen", "Leid", "vnd", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd keinen Trost fast scheint zu haben:", "tokens": ["Vnd", "kei\u00b7nen", "Trost", "fast", "scheint", "zu", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie f\u00fchren hochbetr\u00fcbten Sinn,", "tokens": ["Sie", "f\u00fch\u00b7ren", "hoch\u00b7be\u00b7tr\u00fcb\u00b7ten", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd billich, denn jhr Theil ist hin", "tokens": ["Vnd", "bil\u00b7lich", ",", "denn", "jhr", "Theil", "ist", "hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd wird begraben.", "tokens": ["Vnd", "wird", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "GoTT, dies ist deiner Wei\u00dfheit Raht,", "tokens": ["GoTT", ",", "dies", "ist", "dei\u00b7ner", "Wei\u00df\u00b7heit", "Raht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der B\u00f6ses nie begangen hat,", "tokens": ["Der", "B\u00f6\u00b7ses", "nie", "be\u00b7gan\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du f\u00e4llest, was nur lebet, nieder,", "tokens": ["Du", "f\u00e4l\u00b7lest", ",", "was", "nur", "le\u00b7bet", ",", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jung, Alt, eh' als man sich besinnt,", "tokens": ["Jung", ",", "Alt", ",", "eh'", "als", "man", "sich", "be\u00b7sinnt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "KOUS", "KOUS", "PIS", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sprichst du, O du Menschen-Kind", "tokens": ["Dann", "sprichst", "du", ",", "O", "du", "Men\u00b7schen\u00b7Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Komm eilends wieder!", "tokens": ["Komm", "ei\u00b7lends", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Mein Leben, meine Lust vnd Zier", "tokens": ["Mein", "Le\u00b7ben", ",", "mei\u00b7ne", "Lust", "vnd", "Zier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist einer Handbreit nur bey dir,", "tokens": ["Ist", "ei\u00b7ner", "Hand\u00b7breit", "nur", "bey", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wie nichts sind alle Leuth' auff Erden!", "tokens": ["Wie", "nichts", "sind", "al\u00b7le", "Leuth'", "auff", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Creutz vnd Elend kr\u00e4nckt vns doch!", "tokens": ["Was", "Creutz", "vnd", "E\u00b7lend", "kr\u00e4nckt", "vns", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach GOTT, da\u00df wir so sicher noch", "tokens": ["Ach", "GoTT", ",", "da\u00df", "wir", "so", "si\u00b7cher", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Befunden werden!", "tokens": ["Be\u00b7fun\u00b7den", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "La\u00df deinen Willen, Herr, geschehn,", "tokens": ["La\u00df", "dei\u00b7nen", "Wil\u00b7len", ",", "Herr", ",", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thue, was du \u00fcber uns versehn,", "tokens": ["Thue", ",", "was", "du", "\u00fc\u00b7ber", "uns", "ver\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur gieb Gedult im Creutz vnd Leiden,", "tokens": ["Nur", "gieb", "Ge\u00b7dult", "im", "Creutz", "vnd", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir voraus in dieser Pein,", "tokens": ["Da\u00df", "wir", "vo\u00b7raus", "in", "die\u00b7ser", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als deinen Kindern zusteht, seyn", "tokens": ["Als", "dei\u00b7nen", "Kin\u00b7dern", "zu\u00b7steht", ",", "seyn"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Still vnd bescheiden!", "tokens": ["Still", "vnd", "be\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Waysen Pfl\u00e4g' vnd Schutz bist Du,", "tokens": ["Der", "Way\u00b7sen", "Pfl\u00e4g'", "vnd", "Schutz", "bist", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Einsam' heist Dich jhre Rhue,", "tokens": ["Die", "Ein\u00b7sam'", "heist", "Dich", "jhre", "Rhue", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auff Dich setzt alles sein Vertrawen,", "tokens": ["Auff", "Dich", "setzt", "al\u00b7les", "sein", "Ver\u00b7tra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedruckte Seelen sonderlich,", "tokens": ["Be\u00b7druck\u00b7te", "See\u00b7len", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd diesen l\u00e4sst vor andern sich", "tokens": ["Vnd", "die\u00b7sen", "l\u00e4sst", "vor", "an\u00b7dern", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PIS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Trost auch schawen.", "tokens": ["Dein", "Trost", "auch", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Reih' unsre Thr\u00e4nen fleissig auff,", "tokens": ["Reih'", "uns\u00b7re", "Thr\u00e4\u00b7nen", "fleis\u00b7sig", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hemm aber endlich jhren Lauff,", "tokens": ["Hemm", "a\u00b7ber", "end\u00b7lich", "jhren", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Stell unser kranckes Hertz zu frieden,", "tokens": ["Stell", "un\u00b7ser", "kran\u00b7ckes", "Hertz", "zu", "frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Vater, sey in keiner Noht,", "tokens": ["O", "Va\u00b7ter", ",", "sey", "in", "kei\u00b7ner", "Noht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es gelte Leben oder Todt,", "tokens": ["Es", "gel\u00b7te", "Le\u00b7ben", "o\u00b7der", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von uns geschieden!", "tokens": ["Von", "uns", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Die Seele des Gerechten ist", "tokens": ["Die", "See\u00b7le", "des", "Ge\u00b7rech\u00b7ten", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Befreyt von angst, betrug vnd list,", "tokens": ["Be\u00b7freyt", "von", "angst", ",", "be\u00b7trug", "vnd", "list", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos