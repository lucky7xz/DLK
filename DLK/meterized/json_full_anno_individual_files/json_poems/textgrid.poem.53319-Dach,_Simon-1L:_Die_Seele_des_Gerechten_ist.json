": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedruckte Seelen sonderlich,", "tokens": ["Be\u00b7druck\u00b7te", "See\u00b7len", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd diesen l\u00e4sst vor andern sich", "tokens": ["Vnd", "die\u00b7sen", "l\u00e4sst", "vor", "an\u00b7dern", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PIS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Trost auch schawen.", "tokens": ["Dein", "Trost", "auch", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "Reih' unsre Thr\u00e4nen fleissig auff,", "tokens": ["Reih'", "uns\u00b7re", "Thr\u00e4\u00b7nen", "fleis\u00b7sig", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hemm aber endlich jhren Lauff,", "tokens": ["Hemm", "a\u00b7ber", "end\u00b7lich", "jhren", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Stell unser kranckes Hertz zu frieden,", "tokens": ["Stell", "un\u00b7ser", "kran\u00b7ckes", "Hertz", "zu", "frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Vater, sey in keiner Noht,", "tokens": ["O", "Va\u00b7ter", ",", "sey", "in", "kei\u00b7ner", "Noht", ","], "token_info": ["word", "word"