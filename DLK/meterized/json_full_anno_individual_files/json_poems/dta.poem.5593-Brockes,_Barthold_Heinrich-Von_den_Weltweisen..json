{"dta.poem.5593": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchle, wie ein Thon mich r\u00fchrt, ein Strahl begeistert:", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "wie", "ein", "Thon", "mich", "r\u00fchrt", ",", "ein", "Strahl", "be\u00b7geis\u00b7tert", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch ein strenger Trieb sich meiner gantz bemeistert.", "tokens": ["Wo\u00b7durch", "ein", "stren\u00b7ger", "Trieb", "sich", "mei\u00b7ner", "gantz", "be\u00b7meis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich unterfange mich durch neue Th\u00f6n\u2019 und Lehren,", "tokens": ["Ich", "un\u00b7ter\u00b7fan\u00b7ge", "mich", "durch", "neu\u00b7e", "Th\u00f6n'", "und", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den unbekannten Grund und Inhalt zu erkl\u00e4ren", "tokens": ["Den", "un\u00b7be\u00b7kann\u00b7ten", "Grund", "und", "In\u00b7halt", "zu", "er\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Welt, die man vom Element,", "tokens": ["Der", "Welt", ",", "die", "man", "vom", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Elementarisch nennt:", "tokens": ["E\u00b7lem\u00b7en\u00b7ta\u00b7risch", "nennt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}