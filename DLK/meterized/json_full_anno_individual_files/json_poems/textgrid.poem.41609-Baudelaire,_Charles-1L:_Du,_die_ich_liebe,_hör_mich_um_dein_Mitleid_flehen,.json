{"textgrid.poem.41609": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, die ich liebe, h\u00f6r mich um dein Mitleid flehen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die ich liebe, h\u00f6r mich um dein Mitleid flehen,", "tokens": ["Du", ",", "die", "ich", "lie\u00b7be", ",", "h\u00f6r", "mich", "um", "dein", "Mit\u00b7leid", "fle\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Grund der finstren Schlucht, in die mein Herz versank.", "tokens": ["Vom", "Grund", "der", "finst\u00b7ren", "Schlucht", ",", "in", "die", "mein", "Herz", "ver\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Voll Gram ist diese Welt, ihr Himmel bleich und krank,", "tokens": ["Voll", "Gram", "ist", "die\u00b7se", "Welt", ",", "ihr", "Him\u00b7mel", "bleich", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "PDAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drin Schreck und L\u00e4sterung durch b\u00f6ses Dunkel wehen.", "tokens": ["Drin", "Schreck", "und", "L\u00e4s\u00b7te\u00b7rung", "durch", "b\u00f6\u00b7ses", "Dun\u00b7kel", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein kalter Sonnenball kreist dort sechs Monde lang,", "tokens": ["Ein", "kal\u00b7ter", "Son\u00b7nen\u00b7ball", "kreist", "dort", "sechs", "Mon\u00b7de", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die sechs andern deckt uns Nacht mit schwarzem Schilde.", "tokens": ["Und", "die", "sechs", "an\u00b7dern", "deckt", "uns", "Nacht", "mit", "schwar\u00b7zem", "Schil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "CARD", "PIS", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Land ist nackter als des Nordpols Eisgefilde,", "tokens": ["Das", "Land", "ist", "nack\u00b7ter", "als", "des", "Nord\u00b7pols", "Eis\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht B\u00e4che, Herden nicht, nicht Wald noch Wiesenhang.", "tokens": ["Nicht", "B\u00e4\u00b7che", ",", "Her\u00b7den", "nicht", ",", "nicht", "Wald", "noch", "Wie\u00b7sen\u00b7hang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "PTKNEG", "$,", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kein Grauen gibt es auf der Welt, das an die bleiche,", "tokens": ["Kein", "Grau\u00b7en", "gibt", "es", "auf", "der", "Welt", ",", "das", "an", "die", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erstarrte Grausamkeit der eisgen Sonne reiche,", "tokens": ["Er\u00b7starr\u00b7te", "Grau\u00b7sam\u00b7keit", "der", "eis\u00b7gen", "Son\u00b7ne", "rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und an dies Dunkel, wie das Chaos uferlos.", "tokens": ["Und", "an", "dies", "Dun\u00b7kel", ",", "wie", "das", "Chaos", "u\u00b7fer\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Mich f\u00fcllt mit hei\u00dfem Neid der \u00e4rmsten Tiere Los,", "tokens": ["Mich", "f\u00fcllt", "mit", "hei\u00b7\u00dfem", "Neid", "der", "\u00e4rms\u00b7ten", "Tie\u00b7re", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie im stumpfen Schlaf vergessen Schmerz und Plage;", "tokens": ["Weil", "sie", "im", "stump\u00b7fen", "Schlaf", "ver\u00b7ges\u00b7sen", "Schmerz", "und", "Pla\u00b7ge", ";"], "token_