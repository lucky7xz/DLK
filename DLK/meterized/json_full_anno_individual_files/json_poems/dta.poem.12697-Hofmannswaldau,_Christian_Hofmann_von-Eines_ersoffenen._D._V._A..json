{"dta.poem.12697": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines ersoffenen.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm wasser fieng ich an ein neu und Christlich leben;", "tokens": ["Jm", "was\u00b7ser", "fi\u00b7eng", "ich", "an", "ein", "neu", "und", "Christ\u00b7lich", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "APPR", "ART", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jm wasser hab ich auch mein leben aufgegeben:", "tokens": ["Jm", "was\u00b7ser", "hab", "ich", "auch", "mein", "le\u00b7ben", "auf\u00b7ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "PPOSAT", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jm wasser zog ich erst selbst meinen JEsus an,", "tokens": ["Jm", "was\u00b7ser", "zog", "ich", "erst", "selbst", "mei\u00b7nen", "Je\u00b7sus", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der aus dem wasser mich zuletzt auch ziehen kan.", "tokens": ["Der", "aus", "dem", "was\u00b7ser", "mich", "zu\u00b7letzt", "auch", "zie\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "PPER", "ADV", "ADV", "VVINF", "VMFIN"