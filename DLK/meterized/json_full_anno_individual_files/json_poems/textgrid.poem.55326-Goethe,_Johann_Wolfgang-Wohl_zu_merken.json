{"textgrid.poem.55326": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wohl zu merken", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn wir unterschieden haben,", "tokens": ["Und", "wenn", "wir", "un\u00b7ter\u00b7schie\u00b7den", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann m\u00fcssen wir lebendige Gaben", "tokens": ["Dann", "m\u00fcs\u00b7sen", "wir", "le\u00b7ben\u00b7di\u00b7ge", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem Abgesonderten wieder verleihn", "tokens": ["Dem", "Ab\u00b7ge\u00b7son\u00b7der\u00b7ten", "wie\u00b7der", "ver\u00b7leihn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und uns eines Folge-Lebens erfreun.", "tokens": ["Und", "uns", "ei\u00b7nes", "Fol\u00b7ge\u00b7Le\u00b7bens", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "So, wenn der Maler, der Poet,", "tokens": ["So", ",", "wenn", "der", "Ma\u00b7ler", ",", "der", "Po\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Howards Sondrung wohl vertraut,", "tokens": ["Mit", "Ho\u00b7wards", "Sond\u00b7rung", "wohl", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Morgens fr\u00fch, am Abend sp\u00e4t", "tokens": ["Des", "Mor\u00b7gens", "fr\u00fch", ",", "am", "A\u00b7bend", "sp\u00e4t"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Atmosph\u00e4re pr\u00fcfend schaut,", "tokens": ["Die", "At\u00b7mo\u00b7sph\u00e4\u00b7re", "pr\u00fc\u00b7fend", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da l\u00e4\u00dft er den Charakter gelten;", "tokens": ["Da", "l\u00e4\u00dft", "er", "den", "Cha\u00b7rak\u00b7ter", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ihm erteilen luftige Welten", "tokens": ["Doch", "ihm", "er\u00b7tei\u00b7len", "luf\u00b7ti\u00b7ge", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das \u00dcberg\u00e4ngliche, das Milde,", "tokens": ["Das", "\u00dc\u00b7ber\u00b7g\u00e4ng\u00b7li\u00b7che", ",", "das", "Mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er es fasse, f\u00fchle, bilde.", "tokens": ["Da\u00df", "er", "es", "fas\u00b7se", ",", "f\u00fch\u00b7le", ",", "bil\u00b