{"textgrid.poem.34982": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Ludewig von Bayerland", "tokens": ["Herr", "Lu\u00b7de\u00b7wig", "von", "Bay\u00b7er\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach seufzend zu sich selber:", "tokens": ["Sprach", "seuf\u00b7zend", "zu", "sich", "sel\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbder Sommer weicht, der Winter naht,", "tokens": ["\u00bb", "der", "Som\u00b7mer", "weicht", ",", "der", "Win\u00b7ter", "naht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Laub wird immer gelber.", "tokens": ["Das", "Laub", "wird", "im\u00b7mer", "gel\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Schelling und der Cornelius,", "tokens": ["Der", "Schel\u00b7ling", "und", "der", "Cor\u00b7ne\u00b7li\u00b7us", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie m\u00f6gen von dannen wandern;", "tokens": ["Sie", "m\u00f6\u00b7gen", "von", "dan\u00b7nen", "wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem einen erlosch im Kopf die Vernunft,", "tokens": ["Dem", "ei\u00b7nen", "er\u00b7losch", "im", "Kopf", "die", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Phantasie dem andern.", "tokens": ["Die", "Phan\u00b7ta\u00b7sie", "dem", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch da\u00df man aus meiner Krone stahl", "tokens": ["Doch", "da\u00df", "man", "aus", "mei\u00b7ner", "Kro\u00b7ne", "stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die beste Perle, da\u00df man", "tokens": ["Die", "bes\u00b7te", "Per\u00b7le", ",", "da\u00df", "man"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir meinen Turnkunstmeister geraubt,", "tokens": ["Mir", "mei\u00b7nen", "Turn\u00b7kunst\u00b7meis\u00b7ter", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das Menschenjuwel, den Ma\u00dfmann \u2013", "tokens": ["Das", "Men\u00b7schen\u00b7ju\u00b7wel", ",", "den", "Ma\u00df\u00b7mann", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Das hat mich gebeugt, das hat mich geknickt,", "tokens": ["Das", "hat", "mich", "ge\u00b7beugt", ",", "das", "hat", "mich", "ge\u00b7knickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$,", "PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das hat mir die Seele zerschmettert:", "tokens": ["Das", "hat", "mir", "die", "See\u00b7le", "zer\u00b7schmet\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mir fehlt jetzt der Mann, der in seiner Kunst", "tokens": ["Mir", "fehlt", "jetzt", "der", "Mann", ",", "der", "in", "sei\u00b7ner", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den h\u00f6chsten Pfahl erklettert.", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Pfahl", "er\u00b7klet\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sehe die kurzen Beinchen nicht mehr,", "tokens": ["Ich", "se\u00b7he", "die", "kur\u00b7zen", "Be\u00b7in\u00b7chen", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nicht mehr die platte Nase;", "tokens": ["Nicht", "mehr", "die", "plat\u00b7te", "Na\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schlug wie ein Pudel frisch-fromm-fr\u00f6hlich-frei", "tokens": ["Er", "schlug", "wie", "ein", "Pu\u00b7del", "frischfrom\u00b7mfr\u00f6h\u00b7lich\u00b7frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Purzelb\u00e4ume im Grase.", "tokens": ["Die", "Pur\u00b7zel\u00b7b\u00e4u\u00b7me", "im", "Gra\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Nur Altdeutsch verstand er, der Patriot,", "tokens": ["Nur", "Alt\u00b7deutsch", "ver\u00b7stand", "er", ",", "der", "Pat\u00b7ri\u00b7ot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nur Jakob-Grimmisch und Zeunisch;", "tokens": ["Nur", "Ja\u00b7kob\u00b7Grim\u00b7misch", "und", "Zeu\u00b7nisch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fremdw\u00f6rter blieben ihm immer fremd,", "tokens": ["Fremd\u00b7w\u00f6r\u00b7ter", "blie\u00b7ben", "ihm", "im\u00b7mer", "fremd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Griechisch zumal und Lateinisch.", "tokens": ["Grie\u00b7chisch", "zu\u00b7mal", "und", "La\u00b7tei\u00b7nisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Er hat, ein vaterl\u00e4ndisch Gem\u00fct,", "tokens": ["Er", "hat", ",", "ein", "va\u00b7ter\u00b7l\u00e4n\u00b7disch", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nur Eichelkaffee getrunken,", "tokens": ["Nur", "Ei\u00b7chel\u00b7kaf\u00b7fee", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Franzosen fra\u00df er und Limburger K\u00e4s',", "tokens": ["Fran\u00b7zo\u00b7sen", "fra\u00df", "er", "und", "Lim\u00b7bur\u00b7ger", "K\u00e4s'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NN", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach letzterm hat er gestunken.", "tokens": ["Nach", "letz\u00b7term", "hat", "er", "ge\u00b7stun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "O Schwager! gib mir den Ma\u00dfmann zur\u00fcck!", "tokens": ["O", "Schwa\u00b7ger", "!", "gib", "mir", "den", "Ma\u00df\u00b7mann", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn unter den Gesichtern", "tokens": ["Denn", "un\u00b7ter", "den", "Ge\u00b7sich\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist sein Gesicht, was ich selber bin,", "tokens": ["Ist", "sein", "Ge\u00b7sicht", ",", "was", "ich", "sel\u00b7ber", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Als Dichter unter den Dichtern.", "tokens": ["Als", "Dich\u00b7ter", "un\u00b7ter", "den", "Dich\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "O Schwager! behalt den Cornelius,", "tokens": ["O", "Schwa\u00b7ger", "!", "be\u00b7halt", "den", "Cor\u00b7ne\u00b7li\u00b7us", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ART", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch Schelling (da\u00df du den R\u00fcckert", "tokens": ["Auch", "Schel\u00b7ling", "(", "da\u00df", "du", "den", "R\u00fc\u00b7ckert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$(", "KOUS", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Behalten kannst, versteht sich von selbst) \u2013", "tokens": ["Be\u00b7hal\u00b7ten", "kannst", ",", "ver\u00b7steht", "sich", "von", "selbst", ")", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "$,", "VVFIN", "PRF", "APPR", "ADV", "$(", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wenn nur der Ma\u00dfmann zur\u00fcckkehrt!", "tokens": ["Wenn", "nur", "der", "Ma\u00df\u00b7mann", "zu\u00b7r\u00fcck\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O Schwager! begn\u00fcge dich mit dem Ruhm,", "tokens": ["O", "Schwa\u00b7ger", "!", "be\u00b7gn\u00fc\u00b7ge", "dich", "mit", "dem", "Ruhm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df du mich verdunkelt heute;", "tokens": ["Da\u00df", "du", "mich", "ver\u00b7dun\u00b7kelt", "heu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich, der in Deutschland der Erste war,", "tokens": ["Ich", ",", "der", "in", "Deutschland", "der", "Ers\u00b7te", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NE", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin nur noch der Zweite...\u00ab", "tokens": ["Ich", "bin", "nur", "noch", "der", "Zwei\u00b7te", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Herr Ludewig von Bayerland", "tokens": ["Herr", "Lu\u00b7de\u00b7wig", "von", "Bay\u00b7er\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach seufzend zu sich selber:", "tokens": ["Sprach", "seuf\u00b7zend", "zu", "sich", "sel\u00b7ber