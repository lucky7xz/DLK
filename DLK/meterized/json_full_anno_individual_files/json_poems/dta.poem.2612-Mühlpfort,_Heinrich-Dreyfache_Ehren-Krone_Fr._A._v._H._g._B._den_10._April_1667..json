{"dta.poem.2612": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dreyfache Ehren-Krone/  \n  Fr. A. v. H. g. B. den 10. April  1667.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend ist nie blo\u00df und ohne Glantz erschienen/", "tokens": ["Die", "Tu\u00b7gend", "ist", "nie", "blo\u00df", "und", "oh\u00b7ne", "Glantz", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "KON", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es hat die alte Welt ihr Kronen zugezehlt/", "tokens": ["Es", "hat", "die", "al\u00b7te", "Welt", "ihr", "Kro\u00b7nen", "zu\u00b7ge\u00b7zehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Lorbeern/ die noch heut in Zeiten-B\u00fcchern gr\u00fcnen/", "tokens": ["Und", "Lor\u00b7beern", "/", "die", "noch", "heut", "in", "Zei\u00b7ten\u00b7B\u00fc\u00b7chern", "gr\u00fc\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "ART", "ADV", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil Tugend ihr ja stets den Ruhm zum Zweck er-", "tokens": ["Weil", "Tu\u00b7gend", "ihr", "ja", "stets", "den", "Ruhm", "zum", "Zweck", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zwar wie ein freyes Volck in reinen Sitten lebte/", "tokens": ["Zwar", "wie", "ein", "frey\u00b7es", "Volck", "in", "rei\u00b7nen", "Sit\u00b7ten", "leb\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nicht ein G\u00f6tzen-Knecht des schn\u00f6den Goldes war/", "tokens": ["Und", "nicht", "ein", "G\u00f6t\u00b7zen\u00b7Knecht", "des", "schn\u00f6\u00b7den", "Gol\u00b7des", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als ein ermuntert Geist nach Helden-Thaten strebte/", "tokens": ["Als", "ein", "er\u00b7mun\u00b7tert", "Geist", "nach", "Hel\u00b7den\u00b7Tha\u00b7ten", "streb\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihm sein eignes Lob sein\u2019 eigne Faust gebahr;", "tokens": ["Und", "ihm", "sein", "eig\u00b7nes", "Lob", "sein'", "eig\u00b7ne", "Faust", "ge\u00b7bahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hat oft ein schlechtes Blat des Siegers Haupt bekr\u00e4ntzet;", "tokens": ["Hat", "oft", "ein", "schlech\u00b7tes", "Blat", "des", "Sie\u00b7gers", "Haupt", "be\u00b7kr\u00b7\u00e4nt\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Rom theilte frisches ", "tokens": ["Rom", "theil\u00b7te", "fri\u00b7sches"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "In welchem Ehrenschmuck sein B\u00fcrger mehr gegl\u00e4ntzet/", "tokens": ["In", "wel\u00b7chem", "Eh\u00b7ren\u00b7schmuck", "sein", "B\u00fcr\u00b7ger", "mehr", "ge\u00b7gl\u00e4nt\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als der so Perlen sucht itzt bey der Sonnen Hau\u00df.", "tokens": ["Als", "der", "so", "Per\u00b7len", "sucht", "itzt", "bey", "der", "Son\u00b7nen", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Minerva hat darumb den Oelzweig selbst gesetzet/", "tokens": ["Mi\u00b7ner\u00b7va", "hat", "da\u00b7rumb", "den", "O\u00b7el\u00b7zweig", "selbst", "ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PAV", "ART", "NN", "ADV", "VVPP", "$("], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Damit ihr j\u00e4hrlich Fest bekr\u00f6net konte seyn;", "tokens": ["Da\u00b7mit", "ihr", "j\u00e4hr\u00b7lich", "Fest", "be\u00b7kr\u00f6\u00b7net", "kon\u00b7te", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Eiche Jupiter zu Kronen werth gesch\u00e4tzet.", "tokens": ["Die", "Ei\u00b7che", "Ju\u00b7pi\u00b7ter", "zu", "Kro\u00b7nen", "werth", "ge\u00b7sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jm gr\u00fcnen Myrtenschmuck stellt sich die Venus ein.", "tokens": ["Jm", "gr\u00fc\u00b7nen", "Myr\u00b7ten\u00b7schmuck", "stellt", "sich", "die", "Ve\u00b7nus", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Heyden Priester ward belaubt mit Blum und Bl\u00e4ttern/", "tokens": ["Der", "Hey\u00b7den", "Pries\u00b7ter", "ward", "be\u00b7laubt", "mit", "Blum", "und", "Bl\u00e4t\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und ihm das Opffer Vieh in Kr\u00e4ntzen zugef\u00fchrt/", "tokens": ["Und", "ihm", "das", "Opf\u00b7fer", "Vieh", "in", "Kr\u00e4nt\u00b7zen", "zu\u00b7ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wenn die K\u00e4yser selbst sich wolten recht verg\u00f6ttern/", "tokens": ["Und", "wenn", "die", "K\u00e4y\u00b7ser", "selbst", "sich", "wol\u00b7ten", "recht", "ver\u00b7g\u00f6t\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PRF", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hat ihr geweyhtes Haupt der Lorbeer-strau\u00df geziert.", "tokens": ["Hat", "ihr", "ge\u00b7weyh\u00b7tes", "Haupt", "der", "Lor\u00b7beer\u00b7strau\u00df", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Zeiten Unschuld blieb in solchen guten Schrancken/", "tokens": ["Der", "Zei\u00b7ten", "Un\u00b7schuld", "blieb", "in", "sol\u00b7chen", "gu\u00b7ten", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bi\u00df da\u00df die Uppigkeit die Oberhand gewan:", "tokens": ["Bi\u00df", "da\u00df", "die", "Up\u00b7pig\u00b7keit", "die", "O\u00b7ber\u00b7hand", "ge\u00b7wan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dann fieng der V\u00e4ter Witz und Tugend an zukrancken/", "tokens": ["Dann", "fi\u00b7eng", "der", "V\u00e4\u00b7ter", "Witz", "und", "Tu\u00b7gend", "an", "zu\u00b7kran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "APPR", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Als ein gantz Morgenland zu Kleidern Seide span.", "tokens": ["Als", "ein", "gantz", "Mor\u00b7gen\u00b7land", "zu", "Klei\u00b7dern", "Sei\u00b7de", "span", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "NN", "APPR", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Augustus lie\u00df zu erst die goldnen Kronen blicken/", "tokens": ["Au\u00b7gus\u00b7tus", "lie\u00df", "zu", "erst", "die", "gold\u00b7nen", "Kro\u00b7nen", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.26": {"text": "Und seine Herrlichkeit im h\u00f6chsten Purpur sehn", "tokens": ["Und", "sei\u00b7ne", "Herr\u00b7lich\u00b7keit", "im", "h\u00f6chs\u00b7ten", "Pur\u00b7pur", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Drauf hat was Ost und West von Steinen k\u00f6nnen schicken/", "tokens": ["Drauf", "hat", "was", "Ost", "und", "West", "von", "Stei\u00b7nen", "k\u00f6n\u00b7nen", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "NN", "KON", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "De\u00df Phrygers kluge Hand auf Kronen m\u00fcssen nehn.", "tokens": ["De\u00df", "Phry\u00b7gers", "klu\u00b7ge", "Hand", "auf", "Kro\u00b7nen", "m\u00fcs\u00b7sen", "nehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bi\u00df der Tyrannen Stoltz und Hochmuth so gestiegen/", "tokens": ["Bi\u00df", "der", "Ty\u00b7ran\u00b7nen", "Stoltz", "und", "Hoch\u00b7muth", "so", "ge\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df ihre Scheitel stets von Demant tr\u00e4chtig stand/", "tokens": ["Da\u00df", "ih\u00b7re", "Schei\u00b7tel", "stets", "von", "De\u00b7mant", "tr\u00e4ch\u00b7tig", "stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df Strahlen umb ihr Haupt wie Sonnen musten fliegen/", "tokens": ["Da\u00df", "Strah\u00b7len", "umb", "ihr", "Haupt", "wie", "Son\u00b7nen", "mus\u00b7ten", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "KOKOM", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und diese Raserey verzehrt ein gantzes Land.", "tokens": ["Und", "die\u00b7se", "Ra\u00b7se\u00b7rey", "ver\u00b7zehrt", "ein", "gant\u00b7zes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es war das Capitol ein Schauplatz solcher Sch\u00e4tze/", "tokens": ["Es", "war", "das", "Ca\u00b7pi\u00b7tol", "ein", "Schau\u00b7platz", "sol\u00b7cher", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So von dem Kronen-Gold verschwendrisch beygelegt/", "tokens": ["So", "von", "dem", "Kro\u00b7nen\u00b7Gold", "ver\u00b7schwend\u00b7risch", "bey\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Bi\u00df da\u00df Vespasian entweyhte die Gesetze/", "tokens": ["Bi\u00df", "da\u00df", "Ves\u00b7pa\u00b7si\u00b7an", "ent\u00b7weyh\u00b7te", "die", "Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "NE", "VVFIN", "ART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Und hat das Heiligthum durch M\u00fcntzen umbgepr\u00e4gt.", "tokens": ["Und", "hat", "das", "Hei\u00b7lig\u00b7thum", "durch", "M\u00fcnt\u00b7zen", "umb\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Zwar Kronen bleiben noch der Majest\u00e4ten Zeichen/", "tokens": ["Zwar", "Kro\u00b7nen", "blei\u00b7ben", "noch", "der", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten", "Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und Spiegel/ welche recht die Hoheit stellen f\u00fcr;", "tokens": ["Und", "Spie\u00b7gel", "/", "wel\u00b7che", "recht", "die", "Ho\u00b7heit", "stel\u00b7len", "f\u00fcr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "PRELS", "ADV", "ART", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie sind Kleinodien bey den begl\u00fcckten Reichen/", "tokens": ["Sie", "sind", "Klein\u00b7o\u00b7di\u00b7en", "bey", "den", "be\u00b7gl\u00fcck\u00b7ten", "Rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Der Throne Morgenr\u00f6th und aller Zepter Zier.", "tokens": ["Der", "Thro\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6th", "und", "al\u00b7ler", "Zep\u00b7ter", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Hingegen sind sie auch ein Gauckelspiel der Zeiten/", "tokens": ["Hin\u00b7ge\u00b7gen", "sind", "sie", "auch", "ein", "Gau\u00b7ckel\u00b7spiel", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein Ziel worauf das Gl\u00fcck die sch\u00e4rffsten Augen hat:", "tokens": ["Ein", "Ziel", "wo\u00b7rauf", "das", "Gl\u00fcck", "die", "sch\u00e4rffs\u00b7ten", "Au\u00b7gen", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+