{"dta.poem.2299": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Helden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Helden! dingt mich nicht zum Dichter.", "tokens": ["Hel\u00b7den", "!", "dingt", "mich", "nicht", "zum", "Dich\u00b7ter."], "token_info": ["word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "$.", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Meine Laute will nicht schallen,", "tokens": ["Mei\u00b7ne", "Lau\u00b7te", "will", "nicht", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich euch ein Loblied singe.", "tokens": ["Wenn", "ich", "euch", "ein", "Lob\u00b7lied", "sin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer ist sie widerspenstig,", "tokens": ["Im\u00b7mer", "ist", "sie", "wi\u00b7der\u00b7spens\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer gibt sie falsche T\u00f6ne,", "tokens": ["Im\u00b7mer", "gibt", "sie", "fal\u00b7sche", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich euch ein Loblied singe.", "tokens": ["Wenn", "ich", "euch", "ein", "Lob\u00b7lied", "sin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich von der Liebe singe,", "tokens": ["Wenn", "ich", "von", "der", "Lie\u00b7be", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn ich Amors Waffen preise", "tokens": ["Wenn", "ich", "A\u00b7mors", "Waf\u00b7fen", "prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Oder wenn ich trinkend lalle:", "tokens": ["O\u00b7der", "wenn", "ich", "trin\u00b7kend", "lal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dann trift sie die sch\u00f6nsten T\u00f6ne,", "tokens": ["Dann", "trift", "sie", "die", "sch\u00f6ns\u00b7ten", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Dann so geht sie immer richtig.", "tokens": ["Dann", "so", "geht", "sie", "im\u00b7mer", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}