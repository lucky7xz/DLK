{"dta.poem.7090": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar, wenn man denckt, wie es auf Erden geht,", "tokens": ["Zwar", ",", "wenn", "man", "denckt", ",", "wie", "es", "auf", "Er\u00b7den", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "VVFIN", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Menschen insgemein,", "tokens": ["Da\u00df", "Men\u00b7schen", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus kalter Schl\u00e4frigkeit, fast unempfindlich seyn;", "tokens": ["Aus", "kal\u00b7ter", "Schl\u00e4f\u00b7rig\u00b7keit", ",", "fast", "un\u00b7emp\u00b7find\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie, von allen Sch\u00f6pffungs-Wercken,", "tokens": ["Da\u00df", "sie", ",", "von", "al\u00b7len", "Sch\u00f6pf\u00b7fungs\u00b7\u00b7Wer\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So viel als nichts, bemercken;", "tokens": ["So", "viel", "als", "nichts", ",", "be\u00b7mer\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PIS", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und da\u00df der Seelen Krafft,", "tokens": ["Und", "da\u00df", "der", "See\u00b7len", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Allein auf Ehr, und Lust, und Geld erpicht,", "tokens": ["Al\u00b7lein", "auf", "Ehr", ",", "und", "Lust", ",", "und", "Geld", "er\u00b7picht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von einer solchen Eigenschafft,", "tokens": ["Von", "ei\u00b7ner", "sol\u00b7chen", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als welche blo\u00df zu Ehr\u2019 und Reichthum f\u00fchret,", "tokens": ["Als", "wel\u00b7che", "blo\u00df", "zu", "Ehr'", "und", "Reicht\u00b7hum", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Fast das geringste nicht", "tokens": ["Fast", "das", "ge\u00b7rings\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Verlangt, empfindet, sp\u00fcret;", "tokens": ["Ver\u00b7langt", ",", "emp\u00b7fin\u00b7det", ",", "sp\u00fc\u00b7ret", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So folgt von selbst, da\u00df sie von solchen Sachen,", "tokens": ["So", "folgt", "von", "selbst", ",", "da\u00df", "sie", "von", "sol\u00b7chen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Von aller Freud und Lust, die aus so hohen Dingen,", "tokens": ["Von", "al\u00b7ler", "Freud", "und", "Lust", ",", "die", "aus", "so", "ho\u00b7hen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ihrer Wissenschafft Vermehrungen entspringen;", "tokens": ["Und", "ih\u00b7rer", "Wis\u00b7sen\u00b7schafft", "Ver\u00b7meh\u00b7run\u00b7gen", "ent\u00b7sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sich keinen w\u00fcrdigen Begriff und Eindruck machen.", "tokens": ["Sich", "kei\u00b7nen", "w\u00fcr\u00b7di\u00b7gen", "Be\u00b7griff", "und", "Ein\u00b7druck", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Allein, ein weiser Mann,", "tokens": ["Al\u00b7lein", ",", "ein", "wei\u00b7ser", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Der mit nie m\u00fcden Flei\u00df sich lange Zeit beflissen,", "tokens": ["Der", "mit", "nie", "m\u00fc\u00b7den", "Flei\u00df", "sich", "lan\u00b7ge", "Zeit", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADJA", "NN", "PRF", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein ihm verborgenes Geheimni\u00df recht zu wissen,", "tokens": ["Ein", "ihm", "ver\u00b7bor\u00b7ge\u00b7nes", "Ge\u00b7heim\u00b7ni\u00df", "recht", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und es zuletzt ergr\u00fcndet; der nur kann", "tokens": ["Und", "es", "zu\u00b7letzt", "er\u00b7gr\u00fcn\u00b7det", ";", "der", "nur", "kann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVPP", "$.", "ART", "ADV", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sich ein etwannigs Bild von jenen Freuden-Quellen,", "tokens": ["Sich", "ein", "et\u00b7wan\u00b7nigs", "Bild", "von", "je\u00b7nen", "Freu\u00b7den\u00b7Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So weit zu dencken ihm erlaubt, vor Augen stellen,", "tokens": ["So", "weit", "zu", "den\u00b7cken", "ihm", "er\u00b7laubt", ",", "vor", "Au\u00b7gen", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "PPER", "VVPP", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die dort der Seel\u2019gen Seelen-Heer,", "tokens": ["Die", "dort", "der", "Seel'\u00b7gen", "See\u00b7len\u00b7Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Aus einer Anmuths-See, aus einem Freuden-Meere,", "tokens": ["Aus", "ei\u00b7ner", "An\u00b7muths\u00b7See", ",", "aus", "ei\u00b7nem", "Freu\u00b7den\u00b7Mee\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn sie die Wunder-Werck des Sch\u00f6pffers \u00fcberdencken,", "tokens": ["Wenn", "sie", "die", "Wun\u00b7der\u00b7\u00b7Werck", "des", "Sch\u00f6pf\u00b7fers", "\u00fc\u00b7ber\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In stets vermehrter Lust und Anmuth, werden tr\u00e4ncken.", "tokens": ["In", "stets", "ver\u00b7mehr\u00b7ter", "Lust", "und", "An\u00b7muth", ",", "wer\u00b7den", "tr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "NN", "$,", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}