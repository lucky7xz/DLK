{"dta.poem.17510": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "131.  Der Mensch ist etwas grosses.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch mu\u00df doch was seyn! GOtt niembt sein we", "tokens": ["Der", "Mensch", "mu\u00df", "doch", "was", "seyn", "!", "Gott", "niembt", "sein", "we"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PIS", "VAINF", "$.", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sen an:", "tokens": ["sen", "an", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Umb aller Engel willn h\u00e4tt\u2019 er solchs nicht gethan.", "tokens": ["Umb", "al\u00b7ler", "En\u00b7gel", "willn", "h\u00e4tt'", "er", "solchs", "nicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "APZR", "VAFIN", "PPER", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}