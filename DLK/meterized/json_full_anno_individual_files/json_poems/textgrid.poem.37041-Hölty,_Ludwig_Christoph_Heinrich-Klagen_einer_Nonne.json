pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Augen, der die Luft", "tokens": ["Vor", "Au\u00b7gen", ",", "der", "die", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit lauten Trauert\u00f6nen f\u00fcllt,", "tokens": ["Mit", "lau\u00b7ten", "Trau\u00b7er\u00b7t\u00f6\u00b7nen", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meinen Namen ruft.", "tokens": ["Und", "mei\u00b7nen", "Na\u00b7men", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Vergebens ruft! Nie werd ich ihn,", "tokens": ["Ver\u00b7ge\u00b7bens", "ruft", "!", "Nie", "werd", "ich", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den treuen, wiedersehn,", "tokens": ["Den", "treu\u00b7en", ",", "wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie mit ihm, wenn die B\u00e4ume bl\u00fchn,", "tokens": ["Nie", "mit", "ihm", ",", "wenn", "die", "B\u00e4u\u00b7me", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Schattenhayne gehn.", "tokens": ["Durch", "Schat\u00b7ten\u00b7hay\u00b7ne", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Nein, trauern werd ich, bis der Arm", "tokens": ["Nein", ",", "trau\u00b7ern", "werd", "ich", ",", "bis", "der", "Arm"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVINF", "VAFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Grabes mich umfa\u00dft,", "tokens": ["Des", "Gra\u00b7bes", "mich", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn du o Schwermuth, und du Harm", "tokens": ["Wenn", "du", "o", "Schwer\u00b7muth", ",", "und", "du", "Harm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "FM", "NN", "$,", "KON", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich aufgezehret hast.", "tokens": ["Mich", "auf\u00b7ge\u00b7zeh\u00b7ret", "hast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}