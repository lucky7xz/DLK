{"dta.poem.3707": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wenn die Kinder \u00fcble Laune haben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Z\u00fcrnt und brummt der kleine Zwerg,               ", "tokens": ["Z\u00fcrnt", "und", "brummt", "der", "klei\u00b7ne", "Zwerg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm er alles \u00fcberzwerg,", "tokens": ["Nimm", "er", "al\u00b7les", "\u00fc\u00b7berz\u00b7werg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Backofen f\u00fcr ein Biergla\u00df,", "tokens": ["Ein", "Back\u00b7o\u00b7fen", "f\u00fcr", "ein", "Bier\u00b7gla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Mehlsack f\u00fcr ein Weinfa\u00df,", "tokens": ["Den", "Mehl\u00b7sack", "f\u00fcr", "ein", "Wein\u00b7fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Kirschbaum f\u00fcr ein Besenstiel,", "tokens": ["Den", "Kirschbaum", "f\u00fcr", "ein", "Be\u00b7sen\u00b7stiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Den Flederwisch f\u00fcr ein Windm\u00fchl,", "tokens": ["Den", "Fle\u00b7der\u00b7wisch", "f\u00f