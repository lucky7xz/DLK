{"dta.poem.20043": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf Franz  Kochs  Spiel auf der Maultrommel,  \n genannt die Mundharmonika.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer gab ihm dieses Zaubereisen!", "tokens": ["Wer", "gab", "ihm", "die\u00b7ses", "Zau\u00b7be\u00b7rei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer weihte seinen ird'schen Mund!", "tokens": ["Wer", "weih\u00b7te", "sei\u00b7nen", "ird'\u00b7schen", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Horcht! das sind ja des Himmels Weisen!", "tokens": ["Horcht", "!", "das", "sind", "ja", "des", "Him\u00b7mels", "Wei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Geistern steht ", "tokens": ["Mit", "Geis\u00b7tern", "steht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Seht ihr sie nicht in lichten Kreisen,", "tokens": ["Seht", "ihr", "sie", "nicht", "in", "lich\u00b7ten", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "R\u00fchrt er sein Spiel zur ernsten Stund'?", "tokens": ["R\u00fchrt", "er", "sein", "Spiel", "zur", "erns\u00b7ten", "Stund'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Ja! solche T\u00f6ne wohl h\u00f6rt klingen", "tokens": ["Ja", "!", "sol\u00b7che", "T\u00f6\u00b7ne", "wohl", "h\u00f6rt", "klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PIAT", "NN", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sterbende, der leise spricht:", "tokens": ["Der", "Ster\u00b7ben\u00b7de", ",", "der", "lei\u00b7se", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eihr Freunde! h\u00f6rt ihr auch die\u00df Singen?\u201c", "tokens": ["\u201e", "ihr", "Freun\u00b7de", "!", "h\u00f6rt", "ihr", "auch", "die\u00df", "Sin\u00b7gen", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "PDS", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freunde aber h\u00f6ren's nicht.", "tokens": ["Die", "Freun\u00b7de", "a\u00b7ber", "h\u00f6\u00b7ren's", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er spricht: es t\u00f6nt wie Engels Schwingen,", "tokens": ["Er", "spricht", ":", "es", "t\u00f6nt", "wie", "En\u00b7gels", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "KOKOM", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und stirbt, Verkl\u00e4rung im Gesicht.", "tokens": ["Und", "stirbt", ",", "Ver\u00b7kl\u00e4\u00b7rung", "im", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}