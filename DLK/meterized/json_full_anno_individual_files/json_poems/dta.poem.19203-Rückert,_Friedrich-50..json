{"dta.poem.19203": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Str\u00f6men t\u00e4glich tr\u00e4gt und st\u00fcndlich welch ein Heer", "tokens": ["Von", "Str\u00f6\u00b7men", "t\u00e4g\u00b7lich", "tr\u00e4gt", "und", "st\u00fcnd\u00b7lich", "welch", "ein", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "KON", "ADJD", "PWAT", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Meer s\u00fc\u00df Wasser zu, doch bitter bleibt das Meer.", "tokens": ["Dem", "Meer", "s\u00fc\u00df", "Was\u00b7ser", "zu", ",", "doch", "bit\u00b7ter", "bleibt", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "PTKVZ", "$,", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So t\u00e4glich, st\u00fcndlich bringt von Weisheit auch genug", "tokens": ["So", "t\u00e4g\u00b7lich", ",", "st\u00fcnd\u00b7lich", "bringt", "von", "Weis\u00b7heit", "auch", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVFIN", "APPR", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Welt der Weisen Zunft, doch wird die Welt nicht klug.", "tokens": ["Zur", "Welt", "der", "Wei\u00b7sen", "Zunft", ",", "doch", "wird", "die", "Welt", "nicht", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch lie\u00dfen dieses sich die Weisen wol verdrie\u00dfen,", "tokens": ["Doch", "lie\u00b7\u00dfen", "die\u00b7ses", "sich", "die", "Wei\u00b7sen", "wol", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da unverdrossen stets ins Meer die Str\u00f6me flie\u00dfen?", "tokens": ["Da", "un\u00b7ver\u00b7dros\u00b7sen", "stets", "ins", "Meer", "die", "Str\u00f6\u00b7me", "flie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da nie in ihrem Lauf die Str\u00f6me sich verbittern,", "tokens": ["Da", "nie", "in", "ih\u00b7rem", "Lauf", "die", "Str\u00f6\u00b7me", "sich", "ver\u00b7bit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sollten Weise sich im ihrigen erbittern?", "tokens": ["Wie", "soll\u00b7ten", "Wei\u00b7se", "sich", "im", "ih\u00b7ri\u00b7gen", "er\u00b7bit\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NN", "PRF", "APPRART", "PPOSAT", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Str\u00f6me s\u00fc\u00dfen nie das Meer, doch ziehen sie", "tokens": ["Die", "Str\u00f6\u00b7me", "s\u00fc\u00b7\u00dfen", "nie", "das", "Meer", ",", "doch", "zie\u00b7hen", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus ihm ihr S\u00fc\u00dfes selbst, und wissen selbst nicht wie,", "tokens": ["Aus", "ihm", "ihr", "S\u00fc\u00b7\u00dfes", "selbst", ",", "und", "wis\u00b7sen", "selbst", "nicht", "wie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "ADV", "$,", "KON", "VVFIN", "ADV", "PTKNEG", "KOKOM", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ob unterirdisch aufgedampft und ausgebraut,", "tokens": ["Ob", "un\u00b7ter\u00b7ir\u00b7disch", "auf\u00b7ge\u00b7dampft", "und", "aus\u00b7ge\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob \u00fcberirdisch abgekl\u00e4rt und angethaut;", "tokens": ["Ob", "\u00fc\u00b7be\u00b7rir\u00b7disch", "ab\u00b7ge\u00b7kl\u00e4rt", "und", "an\u00b7ge\u00b7thaut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Des Meeres bittre Flut wird s\u00fc\u00dfe Quelle wieder,", "tokens": ["Des", "Mee\u00b7res", "bitt\u00b7re", "Flut", "wird", "s\u00fc\u00b7\u00dfe", "Quel\u00b7le", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und billig str\u00f6mt der Quell darum zum Meere nieder.", "tokens": ["Und", "bil\u00b7lig", "str\u00f6mt", "der", "Quell", "da\u00b7rum", "zum", "Mee\u00b7re", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PAV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}