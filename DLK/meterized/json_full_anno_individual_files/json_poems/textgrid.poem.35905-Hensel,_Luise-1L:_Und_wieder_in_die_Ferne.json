{"textgrid.poem.35905": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wieder in die Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder in die Ferne", "tokens": ["Und", "wie\u00b7der", "in", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Seele sehnend zieht,", "tokens": ["Die", "See\u00b7le", "seh\u00b7nend", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder gr\u00fc\u00dft Dich gerne", "tokens": ["Und", "wie\u00b7der", "gr\u00fc\u00dft", "Dich", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz in Ton und Lied,", "tokens": ["Mein", "Herz", "in", "Ton", "und", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und will dann h\u00f6her schweben", "tokens": ["Und", "will", "dann", "h\u00f6\u00b7her", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinauf zum ew'gen Tag", "tokens": ["Hin\u00b7auf", "zum", "ew'\u00b7gen", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und danken f\u00fcr Dein Leben,", "tokens": ["Und", "dan\u00b7k