{"textgrid.poem.41116": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werd' ich einst gestorben sein,", "tokens": ["Werd'", "ich", "einst", "ge\u00b7stor\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden dies und das sie sagen,", "tokens": ["Wer\u00b7den", "dies", "und", "das", "sie", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "KON", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir doch ist bekannt allein,", "tokens": ["Dir", "doch", "ist", "be\u00b7kannt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wof\u00fcr hier mein Herz geschlagen.", "tokens": ["Wo\u00b7f\u00fcr", "hier", "mein", "Herz", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df sie schwatzen immerhin", "tokens": ["La\u00df", "sie", "schwat\u00b7zen", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber dem verscharrten Herzen,", "tokens": ["\u00dc\u00b7ber", "dem", "ver\u00b7scharr\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stumm, wie ich im Grabe bin,", "tokens": ["Stumm", ",", "wie", "ich", "im", "Gra\u00b7be", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei du stumm in deinen Schmerzen.", "tokens": ["Sei", "du", "stumm", "in", "dei\u00b7nen", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meinen Schatten sollen nicht", "tokens": ["Mei\u00b7nen", "Schat\u00b7ten", "sol\u00b7len", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6ren deines Auges Tr\u00e4nen,", "tokens": ["St\u00f6\u00b7ren", "dei\u00b7nes", "Au\u00b7ges", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er aus dem Sarge bricht,", "tokens": ["Wenn", "er", "aus", "dem", "Sar\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dir schwebt in seinem Sehnen.", "tokens": ["Zu", "dir", "schwebt", "in", "sei\u00b7nem", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn solang du lebest hier,", "tokens": ["Denn", "so\u00b7lang", "du", "le\u00b7best", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ich nicht die Erde lassen,", "tokens": ["Kann", "ich", "nicht", "die", "Er\u00b7de", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne dich, ich sag's nur dir,", "tokens": ["Oh\u00b7ne", "dich", ",", "ich", "sag's", "nur", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VVFIN", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrd' ich selbst den Himmel hassen.", "tokens": ["W\u00fcrd'", "ich", "selbst", "den", "Him\u00b7mel", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bis gebrochen auch dein Herz,", "tokens": ["Bis", "ge\u00b7bro\u00b7chen", "auch", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00f6st sich nicht mein Bann hienieden,", "tokens": ["L\u00f6st", "sich", "nicht", "mein", "Bann", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann erst schweb' ich himmelw\u00e4rts", "tokens": ["Dann", "erst", "schweb'", "ich", "him\u00b7mel\u