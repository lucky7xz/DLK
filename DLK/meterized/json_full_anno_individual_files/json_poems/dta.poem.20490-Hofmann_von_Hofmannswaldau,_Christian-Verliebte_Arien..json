{"dta.poem.20490": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll Solimene meine glut/", "tokens": ["Soll", "So\u00b7li\u00b7me\u00b7ne", "mei\u00b7ne", "glut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir versehret marck und blut/", "tokens": ["Die", "mir", "ver\u00b7seh\u00b7ret", "marck", "und", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gantz ohne deine rettung brennen?", "tokens": ["Gantz", "oh\u00b7ne", "dei\u00b7ne", "ret\u00b7tung", "bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du \u00e4scherst meine brust ja ein/", "tokens": ["Du", "\u00e4sc\u00b7herst", "mei\u00b7ne", "brust", "ja", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein hertze mu\u00df entz\u00fcndet seyn/", "tokens": ["Mein", "hert\u00b7ze", "mu\u00df", "ent\u00b7z\u00fcn\u00b7det", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du wilst keine flammen kennen.", "tokens": ["Und", "du", "wilst", "kei\u00b7ne", "flam\u00b7men", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Befrage selbst das heisse licht/", "tokens": ["Be\u00b7fra\u00b7ge", "selbst", "das", "heis\u00b7se", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das aus den hellen augen bricht/", "tokens": ["Das", "aus", "den", "hel\u00b7len", "au\u00b7gen", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was feuer es in mir erwecket.", "tokens": ["Was", "feu\u00b7er", "es", "in", "mir", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es schmeltzt der seelen hartes ei\u00df/", "tokens": ["Es", "schmeltzt", "der", "see\u00b7len", "har\u00b7tes", "ei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und machet Salamander hei\u00df/", "tokens": ["Und", "ma\u00b7chet", "Sa\u00b7la\u00b7man\u00b7der", "hei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich soll seyn unangestecket.", "tokens": ["Und", "ich", "soll", "seyn", "un\u00b7an\u00b7ge\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein auge hat mich so verf\u00fchrt/", "tokens": ["Dein", "au\u00b7ge", "hat", "mich", "so", "ver\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich hat dein plitz durchaus ger\u00fchrt/", "tokens": ["Mich", "hat", "dein", "plitz", "durc\u00b7haus", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den ich nicht konte sehen kommen/", "tokens": ["Den", "ich", "nicht", "kon\u00b7te", "se\u00b7hen", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich dachte wolcken anzusehn/", "tokens": ["Ich", "dach\u00b7te", "wol\u00b7cken", "an\u00b7zu\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVIZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit wasser-qvellen umzugehn/", "tokens": ["Mit", "was\u00b7ser\u00b7qvel\u00b7len", "um\u00b7zu\u00b7gehn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bin dadurch der ruh benommen.", "tokens": ["Und", "bin", "da\u00b7durch", "der", "ruh", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich kan nicht wider den betrug/", "tokens": ["Ich", "kan", "nicht", "wi\u00b7der", "den", "be\u00b7trug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer ist bey solchen flammen klug?", "tokens": ["Wer", "ist", "bey", "sol\u00b7chen", "flam\u00b7men", "klug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie k\u00f6nnen bienen nur vertreiben.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "bie\u00b7nen", "nur", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der mensch/ der leichte feuer f\u00e4ngt/", "tokens": ["Der", "mensch", "/", "der", "leich\u00b7te", "feu\u00b7er", "f\u00e4ngt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist wie die motte die sich sengt/", "tokens": ["Ist", "wie", "die", "mot\u00b7te", "die", "sich", "sengt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "PRELS", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und doch nicht kan vom feuer bleiben.", "tokens": ["Und", "doch", "nicht", "kan", "vom", "feu\u00b7er", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Verleugne nicht/ was du vollbracht/", "tokens": ["Ver\u00b7leug\u00b7ne", "nicht", "/", "was", "du", "voll\u00b7bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$(", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast durch feur mich arm gemacht/", "tokens": ["Du", "hast", "durch", "feur", "mich", "arm", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wilstu den mordbrand nicht bekennen?", "tokens": ["Wils\u00b7tu", "den", "mord\u00b7brand", "nicht", "be\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wisse doch/ da\u00df alle welt", "tokens": ["So", "wis\u00b7se", "doch", "/", "da\u00df", "al\u00b7le", "welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$(", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir das gerechte urthel f\u00e4llt:", "tokens": ["Dir", "das", "ge\u00b7rech\u00b7te", "ur\u00b7thel", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer andre brennt/ mu\u00df wieder brennen.", "tokens": ["Wer", "and\u00b7re", "brennt", "/", "mu\u00df", "wie\u00b7der", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$(", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}