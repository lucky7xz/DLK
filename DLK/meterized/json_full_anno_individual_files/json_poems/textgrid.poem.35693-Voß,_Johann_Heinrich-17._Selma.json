{"textgrid.poem.35693": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "17. Selma", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eil', o Mai, mit deinem Brautgesange!", "tokens": ["Eil'", ",", "o", "Mai", ",", "mit", "dei\u00b7nem", "Braut\u00b7ge\u00b7san\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eil' und r\u00f6te meines M\u00e4dchens Wange,", "tokens": ["Eil'", "und", "r\u00f6\u00b7te", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Rose f\u00fcr den Hochzeitkranz?", "tokens": ["Und", "die", "Ro\u00b7se", "f\u00fcr", "den", "Hoch\u00b7zeit\u00b7kranz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Alles taumelt; mir versiegt der Oden;", "tokens": ["Al\u00b7les", "tau\u00b7melt", ";", "mir", "ver\u00b7siegt", "der", "O\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unter meinem Fu\u00dfe brennt der Boden!", "tokens": ["Un\u00b7ter", "mei\u00b7nem", "Fu\u00b7\u00dfe", "brennt", "der", "Bo\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eil'! ich \u00fcberfliege deinen Glanz!", "tokens": ["Eil'", "!", "ich", "\u00fc\u00b7berf\u00b7lie\u00b7ge", "dei\u00b7nen", "Glanz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Unsre Seelen schuf im Myrtenthale", "tokens": ["Uns\u00b7re", "See\u00b7len", "schuf", "im", "Myr\u00b7ten\u00b7tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gott aus Einem morgenroten Strahle;", "tokens": ["Gott", "aus", "Ei\u00b7nem", "mor\u00b7gen\u00b7ro\u00b7ten", "Strah\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00c4hnlich sich, wie Wechselmelodieen;", "tokens": ["\u00c4hn\u00b7lich", "sich", ",", "wie", "Wech\u00b7sel\u00b7me\u00b7lo\u00b7di\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PRF", "$,", "PWAV", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Wie zwei K\u00fcsse, nach einander strebend,", "tokens": ["Wie", "zwei", "K\u00fcs\u00b7se", ",", "nach", "ein\u00b7an\u00b7der", "stre\u00b7bend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "$,", "APPR", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die auf hei\u00dfen Lippen, wonnebebend,", "tokens": ["Die", "auf", "hei\u00b7\u00dfen", "Lip\u00b7pen", ",", "won\u00b7ne\u00b7be\u00b7bend", ","], "token_info": ["word", "word", "word",