{"textgrid.poem.63653": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun gingen zwanzig Jahr dahin,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun gingen zwanzig Jahr dahin,", "tokens": ["Nun", "gin\u00b7gen", "zwan\u00b7zig", "Jahr", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit du uns fehlst, mein holder Sohn,", "tokens": ["Seit", "du", "uns", "fehlst", ",", "mein", "hol\u00b7der", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer noch in Ohr und Sinn", "tokens": ["Und", "im\u00b7mer", "noch", "in", "Ohr", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Klingt mir der lieben Stimme Ton,", "tokens": ["Klingt", "mir", "der", "lie\u00b7ben", "Stim\u00b7me", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und immer noch in N\u00e4chten klar,", "tokens": ["Und", "im\u00b7mer", "noch", "in", "N\u00e4ch\u00b7ten", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn mich geweckt die alte Wunde,", "tokens": ["Wenn", "mich", "ge\u00b7weckt", "die", "al\u00b7te", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Seh' ich dein ernstes Augenpaar,", "tokens": ["Seh'", "ich", "dein", "erns\u00b7tes", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das L\u00e4cheln an dem jungen Munde.", "tokens": ["Das", "L\u00e4\u00b7cheln", "an", "dem", "jun\u00b7gen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nein! das sind die Augen nicht", "tokens": ["Doch", "nein", "!", "das", "sind", "die", "Au\u00b7gen", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "PDS", "VAFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Knaben, wie in jener Zeit:", "tokens": ["Des", "Kna\u00b7ben", ",", "wie", "in", "je\u00b7ner", "Zeit", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich gr\u00fc\u00dft ein J\u00fcnglingsangesicht,", "tokens": ["Mich", "gr\u00fc\u00dft", "ein", "J\u00fcng\u00b7lings\u00b7an\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Lebensernst schon eingeweiht;", "tokens": ["In", "Le\u00b7ben\u00b7sernst", "schon", "ein\u00b7ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als ob an jenem dunklen Ort,", "tokens": ["Als", "ob", "an", "je\u00b7nem", "dunk\u00b7len", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der streng dich h\u00e4lt in seinem Banne,", "tokens": ["Der", "streng", "dich", "h\u00e4lt", "in", "sei\u00b7nem", "Ban\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du heimlich lebtest mit uns fort", "tokens": ["Du", "heim\u00b7lich", "leb\u00b7test", "mit", "uns", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und reiftest still heran zum Manne.", "tokens": ["Und", "reif\u00b7test", "still", "he\u00b7ran", "zum", "Man\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So w\u00e4rst du hier auf Erden auch", "tokens": ["So", "w\u00e4rst", "du", "hier", "auf", "Er\u00b7den", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu unserm Stolz herangebl\u00fcht,", "tokens": ["Zu", "un\u00b7serm", "Stolz", "her\u00b7an\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umweht von jenes Adels Hauch,", "tokens": ["Um\u00b7weht", "von", "je\u00b7nes", "A\u00b7dels", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der schon dein Knabenherz durchgl\u00fcht.", "tokens": ["Der", "schon", "dein", "Kna\u00b7ben\u00b7herz", "durch\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht wie der heut'gen jungen Welt", "tokens": ["Nicht", "wie", "der", "heut'\u00b7gen", "jun\u00b7gen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOKOM", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erschiene dir des Lebens Krone", "tokens": ["Er\u00b7schie\u00b7ne", "dir", "des", "Le\u00b7bens", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Genu\u00df und Macht, und treu gesellt", "tokens": ["Ge\u00b7nu\u00df", "und", "Macht", ",", "und", "treu", "ge\u00b7sellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00e4r' mir ein Freund in meinem Sohne.", "tokens": ["W\u00e4r'", "mir", "ein", "Freund", "in", "mei\u00b7nem", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun wir getrennt f\u00fcr immer sind,", "tokens": ["Nun", "wir", "ge\u00b7trennt", "f\u00fcr", "im\u00b7mer", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "APPR", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann ich im Geiste nur dir nahn,", "tokens": ["Kann", "ich", "im", "Geis\u00b7te", "nur", "dir", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch all mein Tagwerk, teures Kind,", "tokens": ["Doch", "all", "mein", "Tag\u00b7werk", ",", "teu\u00b7res", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist immer auch f\u00fcr dich getan.", "tokens": ["Ist", "im\u00b7mer", "auch", "f\u00fcr", "dich", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir bracht' ich stets das Beste dar", "tokens": ["Dir", "bracht'", "ich", "stets", "das", "Bes\u00b7te", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von meinen Lebensernten allen,", "tokens": ["Von", "mei\u00b7nen", "Le\u00b7ben\u00b7sern\u00b7ten", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn ein Werk vollendet war,", "tokens": ["Und", "wenn", "ein", "Werk", "voll\u00b7en\u00b7det", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Fragt' ich mich: w\u00fcrd' es ", "tokens": ["Fragt'", "ich", "mich", ":", "w\u00fcrd'", "es"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$.", "VAFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Umsonst! Es kehrt aus jenem Reich", "tokens": ["Um\u00b7sonst", "!", "Es", "kehrt", "aus", "je\u00b7nem", "Reich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Laut des Anteils je zur\u00fcck,", "tokens": ["Kein", "Laut", "des", "An\u00b7teils", "je", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einem blassen Schatten gleich", "tokens": ["Und", "ei\u00b7nem", "blas\u00b7sen", "Schat\u00b7ten", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist dieser Freundschaft Geistergl\u00fcck.", "tokens": ["Ist", "die\u00b7ser", "Freund\u00b7schaft", "Geis\u00b7ter\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch samml' ich heut die Herbstfrucht ein,", "tokens": ["Doch", "samml'", "ich", "heut", "die", "Herbst\u00b7frucht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gereift in Sonn' und Sturmeswettern,", "tokens": ["Ge\u00b7reift", "in", "Sonn'", "und", "Stur\u00b7mes\u00b7wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Toten soll zu eigen sein,", "tokens": ["Dem", "To\u00b7ten", "soll", "zu", "ei\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKA", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was leben wird in diesen Bl\u00e4ttern.", "tokens": ["Was", "le\u00b7ben", "wird", "in", "die\u00b7sen", "Bl\u00e4t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun gingen zwanzig Jahr dahin,", "tokens": ["Nun", "gin\u00b7gen", "zwan\u00b7zig", "Jahr", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit du uns fehlst, mein holder Sohn,", "tokens": ["Seit", "du", "uns", "fehlst", ",", "mein", "hol\u00b7der", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer noch in Ohr und Sinn", "tokens": ["Und", "im\u00b7mer", "noch", "in", "Ohr", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON"