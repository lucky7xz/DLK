{"textgrid.poem.41758": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbists wahr? ists m\u00f6glich? klangs von Mund zu Munde!", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbists wahr? ists m\u00f6glich? klangs von Mund zu Munde!", "tokens": ["\u00bb", "ists", "wahr", "?", "ists", "m\u00f6g\u00b7lich", "?", "klangs", "von", "Mund", "zu", "Mun\u00b7de", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$.", "VAFIN", "ADJD", "$.", "ADV", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie konnte solche schlimme That geschehen?\u00ab", "tokens": ["Wie", "konn\u00b7te", "sol\u00b7che", "schlim\u00b7me", "That", "ge\u00b7sche\u00b7hen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PIAT", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So fragend Tausende betroffen stehen", "tokens": ["So", "fra\u00b7gend", "Tau\u00b7sen\u00b7de", "be\u00b7trof\u00b7fen", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als man von Wien vernahm die Schreckenskunde.", "tokens": ["Als", "man", "von", "Wi\u00b7en", "ver\u00b7nahm", "die", "Schre\u00b7cken\u00b7skun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NE", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbach es ist wahr!\u00ab t\u00f6nt's jammernd in der Runde,", "tokens": ["\u00bb", "ach", "es", "ist", "wahr", "!", "\u00ab", "t\u00f6nt's", "jam\u00b7mernd", "in", "der", "Run\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "PPER", "VAFIN", "ADJD", "$.", "$(", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zum Opfer wurde Robert Blum ersehen,", "tokens": ["Zum", "Op\u00b7fer", "wur\u00b7de", "Ro\u00b7bert", "Blum", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NE", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als M\u00e4rtyrer zum blutgen Tod zu gehen", "tokens": ["Als", "M\u00e4r\u00b7ty\u00b7rer", "zum", "blut\u00b7gen", "Tod", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Volke treu bis zu der letzten Stunde.", "tokens": ["Dem", "Vol\u00b7ke", "treu", "bis", "zu", "der", "letz\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dem deutschen Volke, das ihm fest vertraute", "tokens": ["Dem", "deut\u00b7schen", "Vol\u00b7ke", ",", "das", "ihm", "fest", "ver\u00b7trau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ihn gew\u00e4hlt zu seinen Abgesandten", "tokens": ["Das", "ihn", "ge\u00b7w\u00e4hlt", "zu", "sei\u00b7nen", "Ab\u00b7ge\u00b7sand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil so wie er es keiner je verstanden.", "tokens": ["Weil", "so", "wie", "er", "es", "kei\u00b7ner", "je", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KOKOM", "PPER", "PPER", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und jedes Herz sein Hoffen auf ihm baute!", "tokens": ["Und", "je\u00b7des", "Herz", "sein", "Hof\u00b7fen", "auf", "ihm", "bau\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Allen, die wie ich ihn ganz erkannten", "tokens": ["Und", "Al\u00b7len", ",", "die", "wie", "ich", "ihn", "ganz", "er\u00b7kann\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "PRELS", "KOKOM", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verstummt der Schmerz im dumpfen Jammerlaute.", "tokens": ["Ver\u00b7stummt", "der", "Schmerz", "im", "dum\u00b7pfen", "Jam\u00b7mer\u00b7lau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbists wahr? ists m\u00f6glich? klangs von Mund zu Munde!", "tokens": ["\u00bb", "ists", "wahr", "?", "ists", "m\u00f6g\u00b7lich", "?", "klangs", "von", "Mund", "zu", "Mun\u00b7de", "!"], "token_in