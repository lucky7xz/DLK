{"dta.poem.6200": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein gar zu strenger Frost, ein'allzu strenge Hitze,", "tokens": ["Ein", "gar", "zu", "stren\u00b7ger", "Frost", ",", "ein'\u00b7all\u00b7zu", "stren\u00b7ge", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verheeren beyde Kraut und Bl\u00fcht\u2019.", "tokens": ["Ver\u00b7hee\u00b7ren", "bey\u00b7de", "Kraut", "und", "Bl\u00fcht'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Empfinden wir zu starcke Sonnen-Blitze;", "tokens": ["Emp\u00b7fin\u00b7den", "wir", "zu", "star\u00b7cke", "Son\u00b7nen\u00b7Blit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie, oder da\u00df der Nord, der weisse Flocken spr\u00fcht,", "tokens": ["Wie", ",", "o\u00b7der", "da\u00df", "der", "Nord", ",", "der", "weis\u00b7se", "Flo\u00b7cken", "spr\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "KOUS", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Morgen-Thau in Reiff verkehrt;", "tokens": ["Den", "Mor\u00b7gen\u00b7\u00b7T\u00b7hau", "in", "Reiff", "ver\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wird Flora buntes Reich, weil sie der Farben-Schein,", "tokens": ["Wird", "Flo\u00b7ra", "bun\u00b7tes", "Reich", ",", "weil", "sie", "der", "Fa\u00b7rben\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch feuchter D\u00fcnste H\u00fclff\u2019 und Zuflu\u00df blo\u00df allein", "tokens": ["Durch", "feuch\u00b7ter", "D\u00fcns\u00b7te", "H\u00fclff'", "und", "Zu\u00b7flu\u00df", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "KON", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In ihrem Glantz erh\u00e4lt, wann\u2019s Feld kein Na\u00df mehr n\u00e4hrt,", "tokens": ["In", "ih\u00b7rem", "Glantz", "er\u00b7h\u00e4lt", ",", "wann's", "Feld", "kein", "Na\u00df", "mehr", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADJA", "NN", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}