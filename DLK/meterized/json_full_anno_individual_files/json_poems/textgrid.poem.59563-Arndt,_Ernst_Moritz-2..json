{"textgrid.poem.59563": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein R\u00e4tsel tritt das Heilige ins Leben,", "tokens": ["Ein", "R\u00e4t\u00b7sel", "tritt", "das", "Hei\u00b7li\u00b7ge", "ins", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein R\u00e4tsel wohnt es in des Busens Gr\u00fcnden;", "tokens": ["Ein", "R\u00e4t\u00b7sel", "wohnt", "es", "in", "des", "Bu\u00b7sens", "Gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wandelt, wo die Blitze Wolken z\u00fcnden,", "tokens": ["Es", "wan\u00b7delt", ",", "wo", "die", "Blit\u00b7ze", "Wol\u00b7ken", "z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geahnet kaum dahin im leisen Schweben.", "tokens": ["Ge\u00b7ah\u00b7net", "kaum", "da\u00b7hin", "im", "lei\u00b7sen", "Schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df wir die Herzen und die H\u00e4nd' erheben", "tokens": ["Da\u00df", "wir", "die", "Her\u00b7zen", "und", "die", "H\u00e4nd'", "er\u00b7he\u00b7ben"], "toke