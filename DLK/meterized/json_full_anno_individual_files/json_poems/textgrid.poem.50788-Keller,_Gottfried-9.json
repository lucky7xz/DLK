{"textgrid.poem.50788": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "9", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Gehirne brennt mich diese Stille!", "tokens": ["Tief", "im", "Ge\u00b7hir\u00b7ne", "brennt", "mich", "die\u00b7se", "Stil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wenn ich verzweifelnd einen Augenblick", "tokens": ["Wenn", "ich", "ver\u00b7zwei\u00b7felnd", "ei\u00b7nen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geruht, wie mir befahl mein schwacher Wille,", "tokens": ["Ge\u00b7ruht", ",", "wie", "mir", "be\u00b7fahl", "mein", "schwa\u00b7cher", "Wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So kehrt die Angst verdoppelt mir zur\u00fcck.", "tokens": ["So", "kehrt", "die", "Angst", "ver\u00b7dop\u00b7pelt", "mir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und vor den Augen stets die schwarze H\u00fclle,", "tokens": ["Und", "vor", "den", "Au\u00b7gen", "stets", "die", "schwar\u00b7ze", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie tun mir weh, so offen starren sie.", "tokens": ["Sie", "tun", "mir", "weh", ",", "so", "of\u00b7fen", "star\u00b7ren", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie brennt mich im Gehirne diese Stille \u2013", "tokens": ["Wie", "brennt", "mich", "im", "Ge\u00b7hir\u00b7ne", "die\u00b7se", "Stil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Nachbarn! schreit ihr denn im Schlafe nie?", "tokens": ["Ihr", "Nach\u00b7barn", "!", "schreit", "ihr", "denn", "im", "Schla\u00b7fe", "nie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Horch \u2013 endlich zittert es an meine Bretter!", "tokens": ["Horch", "\u2013", "end\u00b7lich", "zit\u00b7tert", "es", "an", "mei\u00b7ne", "Bret\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was f\u00fcr ein geisterhaft metallner Klang,", "tokens": ["Was", "f\u00fcr", "ein", "geis\u00b7ter\u00b7haft", "me\u00b7tall\u00b7ner", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ist das f\u00fcr ein unterirdisch Wetter,", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "un\u00b7ter\u00b7ir\u00b7disch", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das mir ersch\u00fctternd in die Ohren drang?", "tokens": ["Das", "mir", "er\u00b7sch\u00fct\u00b7ternd", "in", "die", "Oh\u00b7ren", "drang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jach unterbrach es meine bangen Klagen,", "tokens": ["Jach", "un\u00b7ter\u00b7brach", "es", "mei\u00b7ne", "ban\u00b7gen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich fuhr zusammen, still, fast hoffnungsvoll \u2013", "tokens": ["Ich", "fuhr", "zu\u00b7sam\u00b7men", ",", "still", ",", "fast", "hoff\u00b7nungs\u00b7voll", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eilf \u2013 zw\u00f6lf \u2013 wahrhaftig, es hat zw\u00f6lf geschlagen,", "tokens": ["Eilf", "\u2013", "zw\u00f6lf", "\u2013", "wahr\u00b7haf\u00b7tig", ",", "es", "hat", "zw\u00f6lf", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "CARD", "$(", "ADJD", "$,", "PPER", "VAFIN", "CARD", "VVPP", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Das war die Turmuhr, die soeben scholl.", "tokens": ["Das", "war", "die", "Tur\u00b7muhr", ",", "die", "soe\u00b7ben", "scholl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die gro\u00dfe Glocke ist's im hohen Stuhle,", "tokens": ["Die", "gro\u00b7\u00dfe", "Glo\u00b7cke", "ist's", "im", "ho\u00b7hen", "Stuh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die klingt ins tiefste Fundament herab,", "tokens": ["Die", "klingt", "ins", "tiefs\u00b7te", "Fun\u00b7da\u00b7ment", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bahnt sich den Weg in diesem Leichenpfuhle", "tokens": ["Bahnt", "sich", "den", "Weg", "in", "die\u00b7sem", "Lei\u00b7chen\u00b7pfuh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Und singt ihr Lied in mein verla\u00dfnes Grab.", "tokens": ["Und", "singt", "ihr", "Lied", "in", "mein", "ver\u00b7la\u00df\u00b7nes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das ist gewi\u00df, gesteh's nur, armer Racker!", "tokens": ["Das", "ist", "ge\u00b7wi\u00df", ",", "ge\u00b7steh's", "nur", ",", "ar\u00b7mer", "Ra\u00b7cker", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "NE", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl so poetisch, wie wenn vordem ich", "tokens": ["Wohl", "so", "po\u00b7e\u00b7tisch", ",", "wie", "wenn", "vor\u00b7dem", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOKOM", "KOUS", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Mittag oft vom fernen Fr\u00fchlingsacker", "tokens": ["Am", "Mit\u00b7tag", "oft", "vom", "fer\u00b7nen", "Fr\u00fch\u00b7lings\u00b7a\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei diesem Klang vergn\u00fcgt nach Hause schlich.", "tokens": ["Bei", "die\u00b7sem", "Klang", "ver\u00b7gn\u00fcgt", "nach", "Hau\u00b7se", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Gewi\u00df sind jetzt die D\u00e4cher warm beschienen", "tokens": ["Ge\u00b7wi\u00df", "sind", "jetzt", "die", "D\u00e4\u00b7cher", "warm", "be\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom sonn'gen Lenz, vom jungen \u00c4therblau;", "tokens": ["Vom", "sonn'\u00b7gen", "Lenz", ",", "vom", "jun\u00b7gen", "\u00c4\u00b7ther\u00b7blau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun kr\u00e4uselt sich der Rauch aus den Kaminen,", "tokens": ["Nun", "kr\u00e4u\u00b7selt", "sich", "der", "Rauch", "aus", "den", "Ka\u00b7mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Leute lockend von der gr\u00fcnen Au.", "tokens": ["Die", "Leu\u00b7te", "lo\u00b7ckend", "von", "der", "gr\u00fc\u00b7nen", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Was h\u00f6hnst du mich, o Glockenton, im Grabe", "tokens": ["Was", "h\u00f6hnst", "du", "mich", ",", "o", "Glo\u00b7cken\u00b7ton", ",", "im", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$,", "FM", "NE", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mehrest meine namenlose Qual?", "tokens": ["Und", "meh\u00b7rest", "mei\u00b7ne", "na\u00b7men\u00b7lo\u00b7se", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entdeckst mir pl\u00f6tzlich, da\u00df ich Hunger habe", "tokens": ["Ent\u00b7deckst", "mir", "pl\u00f6tz\u00b7lich", ",", "da\u00df", "ich", "Hun\u00b7ger", "ha\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht kann hin zum \u00e4rmlich stillen Mahl?", "tokens": ["Und", "nicht", "kann", "hin", "zum", "\u00e4rm\u00b7lich", "stil\u00b7len", "Mahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "ADV", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich hab mein Teil gehungert doch dort oben,", "tokens": ["Ich", "hab", "mein", "Teil", "ge\u00b7hun\u00b7gert", "doch", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nun im Grabe wieder hungert's mich \u2013:", "tokens": ["Und", "nun", "im", "Gra\u00b7be", "wie\u00b7der", "hun\u00b7gert's", "mich", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV", "VVFIN", "PPER", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist dieser Stern aus Hunger denn gewoben,", "tokens": ["Ist", "die\u00b7ser", "Stern", "aus", "Hun\u00b7ger", "denn", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mehrt der Hunger mit der Tiefe sich?", "tokens": ["Und", "mehrt", "der", "Hun\u00b7ger", "mit", "der", "Tie\u00b7fe", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Halt aus, mein Herz! wir m\u00fcssen ihn bezwingen,", "tokens": ["Halt", "aus", ",", "mein", "Herz", "!", "wir", "m\u00fcs\u00b7sen", "ihn", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist ein feiger, schm\u00e4hlich gift'ger Feind!", "tokens": ["Es", "ist", "ein", "fei\u00b7ger", ",", "schm\u00e4h\u00b7lich", "gift'\u00b7ger", "Feind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf dem Geworfnen la\u00df uns grimmig ringen", "tokens": ["Auf", "dem", "Ge\u00b7worf\u00b7nen", "la\u00df", "uns", "grim\u00b7mig", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVIMP", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit andern, die sich gegen uns vereint!", "tokens": ["Mit", "an\u00b7dern", ",", "die", "sich", "ge\u00b7gen", "uns", "ver\u00b7eint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Tief im Gehirne brennt mich diese Stille!", "tokens": ["Tief", "im", "Ge\u00b7hir\u00b7ne", "brennt", "mich", "die\u00b7se", "Stil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+