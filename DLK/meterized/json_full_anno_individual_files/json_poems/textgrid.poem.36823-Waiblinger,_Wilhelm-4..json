{"textgrid.poem.36823": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe dich geliebt,", "tokens": ["Ich", "ha\u00b7be", "dich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und Treue bis zum Grabe dir geschworen,", "tokens": ["Und", "Treu\u00b7e", "bis", "zum", "Gra\u00b7be", "dir", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch hab' ich dein Herz so schwer betr\u00fcbt.", "tokens": ["Und", "doch", "hab'", "ich", "dein", "Herz", "so", "schwer", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So oft verga\u00df ich dein,", "tokens": ["So", "oft", "ver\u00b7ga\u00df", "ich", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Denn andre L\u00e4nder bringen andre Freuden,", "tokens": ["Denn", "and\u00b7re", "L\u00e4n\u00b7der", "brin\u00b7gen", "and\u00b7re", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch imm