{"dta.poem.10858": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Arion  \n Eine poetische Erzehlung.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die T\u00f6ne laufen auf, und fallen pl\u00f6tzlich nieder,", "tokens": ["Die", "T\u00f6\u00b7ne", "lau\u00b7fen", "auf", ",", "und", "fal\u00b7len", "pl\u00f6tz\u00b7lich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kr\u00e4useln sich im Lauf, und strecken sich dann wieder,", "tokens": ["Und", "kr\u00e4u\u00b7seln", "sich", "im", "Lauf", ",", "und", "stre\u00b7cken", "sich", "dann", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$,", "KON", "VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und werden rasch, und geil, und schnellen, springen, jauchzen;", "tokens": ["Und", "wer\u00b7den", "rasch", ",", "und", "geil", ",", "und", "schnel\u00b7len", ",", "sprin\u00b7gen", ",", "jauch\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,", "KON", "NN", "$,", "KON", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}