{"textgrid.poem.53875": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kleines Glockenspiel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Uhr schl\u00e4gt 12 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "12", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Gott allen braven Lesern helf!", "tokens": ["Gott", "al\u00b7len", "bra\u00b7ven", "Le\u00b7sern", "helf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Uhr schl\u00e4gt 1 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "1", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Das Zentrum handelt, Hilferding redet . . .", "tokens": ["Das", "Zent\u00b7rum", "han\u00b7delt", ",", "Hil\u00b7fer\u00b7ding", "re\u00b7det", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NE", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Die Uhr schl\u00e4gt 2 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "2", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wenn England einen Raufbold braucht: Deutschland ist allemal dabei.", "tokens": ["Wenn", "En\u00b7gland", "ei\u00b7nen", "Rauf\u00b7bold", "braucht", ":", "Deutschland", "ist", "al\u00b7le\u00b7mal", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "VVFIN", "$.", "NE", "VAFIN", "ADV", "PAV", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Uhr schl\u00e4gt 3 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "3", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "\u00bbes lebe die Republik!\u00ab ist ein zu nichts verpflichtendes Geschrei.", "tokens": ["\u00bb", "es", "le\u00b7be", "die", "Re\u00b7pub\u00b7lik", "!", "\u00ab", "ist", "ein", "zu", "nichts", "ver\u00b7pflich\u00b7ten\u00b7des", "Ge\u00b7schrei", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$.", "$(", "VAFIN", "ART", "APPR", "PIS", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Die Uhr schl\u00e4gt 4 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "4", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Es war einmal ein republikanischer Reichswehroffizier.", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "re\u00b7pub\u00b7li\u00b7ka\u00b7ni\u00b7scher", "Reichs\u00b7wehr\u00b7of\u00b7fi\u00b7zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Uhr schl\u00e4gt 5 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "5", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In Deutschland ist man viel zu vern\u00fcnf-", "tokens": ["In", "Deutschland", "ist", "man", "viel", "zu", "ver\u00b7n\u00fcn\u00b7f"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PIS", "ADV", "APPR", "TRUNC"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "tig, sich wegen jeden juristischen Drecks", "tokens": ["tig", ",", "sich", "we\u00b7gen", "je\u00b7den", "ju\u00b7ris\u00b7ti\u00b7schen", "Drecks"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PRF", "APPR", "PIAT", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u2013 die Uhr schl\u00e4gt 6 \u2013", "tokens": ["\u2013", "die", "Uhr", "schl\u00e4gt", "6", "\u2013"], "token_info": ["punct", "word", "word", "word", "number", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "zu erregen. La\u00dft uns lieber in die Zuchth\u00e4user abschieben", "tokens": ["zu", "er\u00b7re\u00b7gen", ".", "La\u00dft", "uns", "lie\u00b7ber", "in", "die", "Zucht\u00b7h\u00e4u\u00b7ser", "ab\u00b7schie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$.", "VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+--+-+-+-+--++-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "\u2013 die Uhr schl\u00e4gt 7 \u2013", "tokens": ["\u2013", "die", "Uhr", "schl\u00e4gt", "7", "\u2013"], "token_info": ["punct", "word", "word", "word", "number", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "wer etwas Kommunistisches in die Stra\u00dfen macht \u2013", "tokens": ["wer", "et\u00b7was", "Kom\u00b7mu\u00b7nis\u00b7ti\u00b7sches", "in", "die", "Stra\u00b7\u00dfen", "macht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "die Uhr schl\u00e4gt 8.", "tokens": ["die", "Uhr", "schl\u00e4gt", "8."], "token_info": ["word", "word", "word", "ordinal"], "pos": ["ART", "NN", "VVFIN", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Gleich darauf schl\u00e4gt die Uhr 9 \u2013", "tokens": ["Gleich", "da\u00b7rauf", "schl\u00e4gt", "die", "Uhr", "9", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "number", "punct"], "pos": ["ADV", "PAV", "VVFIN", "ART", "NN", "CARD", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sollte das ein Fehler im Uhrwerk seun?", "tokens": ["Soll\u00b7te", "das", "ein", "Feh\u00b7ler", "im", "Uhr\u00b7werk", "seun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "In Deutschland kann doch nichts vor\u2013, da mu\u00df alles zur\u00fccke gehn \u2013", "tokens": ["In", "Deutschland", "kann", "doch", "nichts", "vor", "\u2013", ",", "da", "mu\u00df", "al\u00b7les", "zu\u00b7r\u00fc\u00b7cke", "gehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "ADV", "PIS", "PTKVZ", "$(", "$,", "ADV", "VMFIN", "PIS", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "die Uhr schl\u00e4gt 10.", "tokens": ["die", "Uhr", "schl\u00e4gt", "10."], "token_info": ["word", "word", "word", "ordinal"], "pos": ["ART", "NN", "VVFIN", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Die Uhr schl\u00e4gt 11 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "11", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ausnahmezustand ist ein Zustand und ein sch\u00f6ner Notbeh-11.", "tokens": ["Aus\u00b7nah\u00b7me\u00b7zu\u00b7stand", "ist", "ein", "Zu\u00b7stand", "und", "ein", "sch\u00f6\u00b7ner", "Not\u00b7beh", "11."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "ordinal"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ART", "ADJA", "TRUNC", "ADJA"], "meter": "-+--+--+---+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Die Uhr schl\u00e4gt 12 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "12", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Gott immer noch allen Lesern helf!", "tokens": ["Gott", "im\u00b7mer", "noch", "al\u00b7len", "Le\u00b7sern", "helf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Es war einmal ein Sozialist, der t\u00e4t die Arbeitgeber reizen \u2013", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "So\u00b7zi\u00b7a\u00b7list", ",", "der", "t\u00e4t", "die", "Ar\u00b7beit\u00b7ge\u00b7ber", "rei\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "da schlug die Uhr 13!", "tokens": ["da", "schlug", "die", "Uhr", "13", "!"], "token_info": ["word", "word", "word", "word", "number", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "CARD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Die Uhr schlug 14, 15, 16, 17, 18, 19 und 20 . . .", "tokens": ["Die", "Uhr", "schlug", "14", ",", "15", ",", "16", ",", "17", ",", "18", ",", "19", "und", "20", ".", ".", "."], "token_info": ["word", "word", "word", "number", "punct", "number", "punct", "number", "punct", "number", "punct", "number", "punct", "number", "word", "number", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$,", "CARD", "$,", "CARD", "$,", "CARD", "$,", "CARD", "$,", "CARD", "KON", "CARD", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wir wollen die deutschen Br\u00fcder in Danzig befrein, wer aber nicht will, ist Danzig.", "tokens": ["Wir", "wol\u00b7len", "die", "deut\u00b7schen", "Br\u00fc\u00b7der", "in", "Dan\u00b7zig", "be\u00b7fr\u00b7ein", ",", "wer", "a\u00b7ber", "nicht", "will", ",", "ist", "Dan\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "NE", "PTKVZ", "$,", "PWS", "ADV", "PTKNEG", "VMFIN", "$,", "VAFIN", "NE", "$."], "meter": "-+--+-+--+--+--+-++-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "So schl\u00e4gt die Uhr bei Tage und bei Nacht,", "tokens": ["So", "schl\u00e4gt", "die", "Uhr", "bei", "Ta\u00b7ge", "und", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "denn daf\u00fcr ist sie Uhr und als solche auf dem Turm angebracht.", "tokens": ["denn", "da\u00b7f\u00fcr", "ist", "sie", "Uhr", "und", "als", "sol\u00b7che", "auf", "dem", "Turm", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "NN", "KON", "KOUS", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-++-+--+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Gro\u00dffressig nach au\u00dfen \u2013 nach innen verpr\u00fcgeltes Zivil \u2013:", "tokens": ["Gro\u00df\u00b7fres\u00b7sig", "nach", "au\u00b7\u00dfen", "\u2013", "nach", "in\u00b7nen", "ver\u00b7pr\u00fc\u00b7gel\u00b7tes", "Zi\u00b7vil", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ADV", "$(", "APPR", "ADV", "ADJA", "NN", "$(", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "das ist das deutsche Glockenspiel.", "tokens": ["das", "ist", "das", "deut\u00b7sche", "Glo\u00b7cken\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die Uhr schl\u00e4gt 12 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "12", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Gott allen braven Lesern helf!", "tokens": ["Gott", "al\u00b7len", "bra\u00b7ven", "Le\u00b7sern", "helf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Die Uhr schl\u00e4gt 1 \u2013", "tokens": ["Die", "Uhr", "schl\u00e4gt", "1", "\u2