{"dta.poem.11297": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das ver\u00e4nderte gl\u00fccke des K\u00f6nigs  \n in Franckreich.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor nahm der Frantzmann uns mit seinen moden ein,", "tokens": ["Vor", "nahm", "der", "Frantz\u00b7mann", "uns", "mit", "sei\u00b7nen", "mo\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun aber will er selbst der Teutschen affe seyn;", "tokens": ["Nun", "a\u00b7ber", "will", "er", "selbst", "der", "Teut\u00b7schen", "af\u00b7fe", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wie man vor der zeit das jahr-buch lie\u00df ver\u00e4ndern,", "tokens": ["Und", "wie", "man", "vor", "der", "zeit", "das", "jahr\u00b7buch", "lie\u00df", "ver\u00b7\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So pralet er itzund mit neuen staats-calendern.", "tokens": ["So", "pra\u00b7let", "er", "it\u00b7zund", "mit", "neu\u00b7en", "staats\u00b7ca\u00b7len\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch jene treibt ihr witz zu h\u00f6hern dingen auf.", "tokens": ["Doch", "je\u00b7ne", "treibt", "ihr", "witz", "zu", "h\u00f6\u00b7hern", "din\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir \u00e4nderten den tag, und er der sternen lauff.", "tokens": ["Wir", "\u00e4n\u00b7der\u00b7ten", "den", "tag", ",", "und", "er", "der", "ster\u00b7nen", "lauff", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Sein gantzer himmel mu\u00df verkehrt und r\u00fcckw\u00e4rts gehen,", "tokens": ["Sein", "gant\u00b7zer", "him\u00b7mel", "mu\u00df", "ver\u00b7kehrt", "und", "r\u00fcck\u00b7w\u00e4rts", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VVPP", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nachdem die sonne soll bey den planeten stehen.", "tokens": ["Nach\u00b7dem", "die", "son\u00b7ne", "soll", "bey", "den", "pla\u00b7ne\u00b7ten", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}}}}