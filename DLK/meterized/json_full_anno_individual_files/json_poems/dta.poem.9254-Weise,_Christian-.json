{"dta.poem.9254": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "OgOtt du reines wesen/", "tokens": ["O\u00b7gOtt", "du", "rei\u00b7nes", "we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du glantz der herrligkeit/", "tokens": ["Du", "glantz", "der", "herr\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil du mich hast erlesen", "tokens": ["Weil", "du", "mich", "hast", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu deiner Christenheit/", "tokens": ["Zu", "dei\u00b7ner", "Chris\u00b7ten\u00b7heit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So gib mir auch den sinn", "tokens": ["So", "gib", "mir", "auch", "den", "sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu aller reinen tugend/", "tokens": ["Zu", "al\u00b7ler", "rei\u00b7nen", "tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und lencke meine jugend", "tokens": ["Und", "len\u00b7cke", "mei\u00b7ne", "ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo ich zu sicher bin.", "tokens": ["Wo", "ich", "zu", "si\u00b7cher", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Erhalt mich in dem schrancken", "tokens": ["Er\u00b7halt", "mich", "in", "dem", "schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor aller b\u00f6sen lust/", "tokens": ["Vor", "al\u00b7ler", "b\u00f6\u00b7sen", "lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Regiere die gedancken", "tokens": ["Re\u00b7gie\u00b7re", "die", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mache mir bewust/", "tokens": ["Und", "ma\u00b7che", "mir", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du dreymahl heilger F\u00fcrst/", "tokens": ["Du", "drey\u00b7mahl", "heil\u00b7ger", "F\u00fcrst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df wir vor unser leben", "tokens": ["Da\u00df", "wir", "vor", "un\u00b7ser", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bald m\u00fcssen antwort geben/", "tokens": ["Bald", "m\u00fcs\u00b7sen", "ant\u00b7wort", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn du erscheinen wirst.", "tokens": ["Wenn", "du", "er\u00b7schei\u00b7nen", "wirst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Wenn ich was b\u00f6ses dichte/", "tokens": ["Wenn", "ich", "was", "b\u00f6\u00b7ses", "dich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So la\u00df es nicht geschehn/", "tokens": ["So", "la\u00df", "es", "nicht", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df ja mein gesichte", "tokens": ["Und", "la\u00df", "ja", "mein", "ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach keiner geilheit sehn/", "tokens": ["Nach", "kei\u00b7ner", "geil\u00b7heit", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Versucht mich fleisch und blut", "tokens": ["Ver\u00b7sucht", "mich", "fleisch", "und", "blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu unterschiednen s\u00fcnden/", "tokens": ["Zu", "un\u00b7ter\u00b7schied\u00b7nen", "s\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach HERR! so la\u00df dich finden", "tokens": ["Ach", "HeRR", "!", "so", "la\u00df", "dich", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und brich mir meinen muth.", "tokens": ["Und", "brich", "mir", "mei\u00b7nen", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Erhalt mein fa\u00df in ehren/", "tokens": ["Er\u00b7halt", "mein", "fa\u00df", "in", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "APPR", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In zucht und heiligkeit/", "tokens": ["In", "zucht", "und", "hei\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df mich nichts versehren", "tokens": ["Und", "la\u00df", "mich", "nichts", "ver\u00b7seh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bi\u00df auff die rechte zeit/", "tokens": ["Bi\u00df", "auff", "die", "rech\u00b7te", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und segne mich alsdann/", "tokens": ["Und", "seg\u00b7ne", "mich", "als\u00b7dann", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich ein liebes hertze", "tokens": ["Da\u00df", "ich", "ein", "lie\u00b7bes", "hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In unverbotnem schertze", "tokens": ["In", "un\u00b7ver\u00b7bot\u00b7nem", "schert\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach hause f\u00fchren kan.", "tokens": ["Nach", "hau\u00b7se", "f\u00fch\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Doch lencke meine sinnen", "tokens": ["Doch", "len\u00b7cke", "mei\u00b7ne", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zuvor auff dieses ziel/", "tokens": ["Zu\u00b7vor", "auff", "die\u00b7ses", "ziel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ich mein brod gewinnen", "tokens": ["Wie", "ich", "mein", "brod", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sie ernehren wil.", "tokens": ["Und", "sie", "er\u00b7neh\u00b7ren", "wil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach diesem zeuch mich hin", "tokens": ["Nach", "die\u00b7sem", "zeuch", "mich", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVIMP", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu einem frommen kinde/", "tokens": ["Zu", "ei\u00b7nem", "from\u00b7men", "kin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da ich mein labsal finde/", "tokens": ["Da", "ich", "mein", "lab\u00b7sal", "fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn ich gesch\u00e4fftig bin.", "tokens": ["Wenn", "ich", "ge\u00b7sch\u00e4ff\u00b7tig", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "6. La\u00df mich ein m\u00e4dgen lieben", "tokens": ["La\u00df", "mich", "ein", "m\u00e4d\u00b7gen", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mich von hertzen meynt/", "tokens": ["Das", "mich", "von", "hert\u00b7zen", "meynt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Freud und in betr\u00fcben/", "tokens": ["In", "Freud", "und", "in", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mit mir lacht und weint/", "tokens": ["Das", "mit", "mir", "lacht", "und", "weint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das sich in alles schickt/", "tokens": ["Das", "sich", "in", "al\u00b7les", "schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und das an meinem ende/", "tokens": ["Und", "das", "an", "mei\u00b7nem", "en\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn ich den lauff vollende/", "tokens": ["Wenn", "ich", "den", "lauff", "voll\u00b7en\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir noch die augen dr\u00fcckt.", "tokens": ["Mir", "noch", "die", "au\u00b7gen", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "7. Wiewohl du wirst es machen/", "tokens": ["Wie\u00b7wohl", "du", "wirst", "es", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil du mein Vater bist/", "tokens": ["Weil", "du", "mein", "Va\u00b7ter", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du weist was meinen sachen", "tokens": ["Du", "weist", "was", "mei\u00b7nen", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gut und ersprie\u00dflich ist.", "tokens": ["Gut", "und", "er\u00b7sprie\u00df\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dein wille soll geschehn/", "tokens": ["Dein", "wil\u00b7le", "soll", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und was du meinem leben", "tokens": ["Und", "was", "du", "mei\u00b7nem", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wilst zum geschencke geben/", "tokens": ["Wilst", "zum", "ge\u00b7schen\u00b7cke", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das will ich gerne sehn.", "tokens": ["Das", "will", "ich", "ger\u00b7ne", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}