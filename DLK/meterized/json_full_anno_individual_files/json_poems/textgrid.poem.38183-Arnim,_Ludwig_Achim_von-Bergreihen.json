{"textgrid.poem.38183": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Bergreihen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Gott, was wollen wir aber heben an,", "tokens": ["Ach", "Gott", ",", "was", "wol\u00b7len", "wir", "a\u00b7ber", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df wir das recht und wohl ersinnen,", "tokens": ["Da\u00df", "wir", "das", "recht", "und", "wohl", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollens auch nicht unterwegen lahn,", "tokens": ["Wir", "wol\u00b7lens", "auch", "nicht", "un\u00b7ter\u00b7we\u00b7gen", "lahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von den Hauern da wollen wir singen,", "tokens": ["Von", "den", "Hau\u00b7ern", "da", "wol\u00b7len", "wir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Sie hauen das Silber, das Gold mit Fleis,", "tokens": ["Sie", "hau\u00b7en", "das", "Sil\u00b7ber", ",", "das", "Gold", "mit", "Fleis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sogar aus festem Gesteine,", "tokens": ["So\u00b7gar", "aus", "fes\u00b7tem", "Ge\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gott wei\u00df wohl wo es liegt.", "tokens": ["Gott", "wei\u00df", "wohl", "wo", "es", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Hauer in die Grube fuhr,", "tokens": ["Ein", "Hau\u00b7er", "in", "die", "Gru\u00b7be", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus frischem freiem Muthe,", "tokens": ["Aus", "fri\u00b7schem", "frei\u00b7em", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man sieht wohl, da\u00df er Eisen f\u00fchrt,", "tokens": ["Man", "sieht", "wohl", ",", "da\u00df", "er", "Ei\u00b7sen", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darum gewinnt ihr das Gute,", "tokens": ["Da\u00b7rum", "ge\u00b7winnt", "ihr", "das", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn er k\u00f6mmt wieder wohl auf sein Ort,", "tokens": ["Wenn", "er", "k\u00f6mmt", "wie\u00b7der", "wohl", "auf", "sein", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+---+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wenn er an Gott gedenkt,", "tokens": ["Wenn", "er", "an", "Gott", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So ist er fein wohl beh\u00fct.", "tokens": ["So", "ist", "er", "fein", "wohl", "be\u00b7h\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wenn er nun sizt und will bestechen,", "tokens": ["Wenn", "er", "nun", "sizt", "und", "will", "be\u00b7ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Woche so sehr,", "tokens": ["Die", "gan\u00b7ze", "Wo\u00b7che", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hilf lieber Gott, das Erz woll brechen,", "tokens": ["Hilf", "lie\u00b7ber", "Gott", ",", "das", "Erz", "woll", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir ringen ja fast nach Erze,", "tokens": ["Wir", "rin\u00b7gen", "ja", "fast", "nach", "Er\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gott hat es nur selber verriegelt gar schon,", "tokens": ["Gott", "hat", "es", "nur", "sel\u00b7ber", "ver\u00b7rie\u00b7gelt", "gar", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "F\u00fcr manchen gar verschlossen,", "tokens": ["F\u00fcr", "man\u00b7chen", "gar", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es bauet mancher Mann \u00fcber das Jahr.", "tokens": ["Es", "bau\u00b7et", "man\u00b7cher", "Mann", "\u00fc\u00b7ber", "das", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.8": {"text": "Er wird des gar hart verdrossen,", "tokens": ["Er", "wird", "des", "gar", "hart", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Gott will es uns wieder aufriegeln gar sch\u00f6n,", "tokens": ["Gott", "will", "es", "uns", "wie\u00b7der", "auf\u00b7rie\u00b7geln", "gar", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADV", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+--+--+---+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Der Hauer mit seinen H\u00e4nden,", "tokens": ["Der", "Hau\u00b7er", "mit", "sei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Er getraut des zu genie\u00dfen zwar,", "tokens": ["Er", "ge\u00b7traut", "des", "zu", "ge\u00b7nie\u00b7\u00dfen", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PTKZU", "VVINF", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Wenn uns nun Gott ein Gl\u00fcck g\u00e4b,", "tokens": ["Wenn", "uns", "nun", "Gott", "ein", "Gl\u00fcck", "g\u00e4b", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Da\u00df wir das Erz begreifen,", "tokens": ["Da\u00df", "wir", "das", "Erz", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Unser Lob steht auf dem Bergwerk so breit,", "tokens": ["Un\u00b7ser", "Lob", "steht", "auf", "dem", "Berg\u00b7werk", "so", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Wir achten keines harten Reifes,", "tokens": ["Wir", "ach\u00b7ten", "kei\u00b7nes", "har\u00b7ten", "Rei\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wir sitzen gar gern bei gutem Getr\u00e4nk,", "tokens": ["Wir", "sit\u00b7zen", "gar", "gern", "bei", "gu\u00b7tem", "Ge\u00b7tr\u00e4nk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wies Menschenbild geneusset,", "tokens": ["Wies", "Men\u00b7schen\u00b7bild", "ge\u00b7neus\u00b7set", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Der Malvasier oder Rheinische Wein,", "tokens": ["Der", "Mal\u00b7va\u00b7sier", "o\u00b7der", "Rhei\u00b7ni\u00b7sche", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Den trinken gar gerne die Reichen.", "tokens": ["Den", "trin\u00b7ken", "gar", "ger\u00b7ne", "die", "Rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.20": {"text": "Wenn nun die Hauer kommen dar,", "tokens": ["Wenn", "nun", "die", "Hau\u00b7er", "kom\u00b7men", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wenn uns die Herren erkennen,", "tokens": ["Wenn", "uns", "die", "Her\u00b7ren", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Unser Lob, das preiset man gar,", "tokens": ["Un\u00b7ser", "Lob", ",", "das", "prei\u00b7set", "man", "gar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Herr Wirth, die Stube ist uns viel zu enge,", "tokens": ["Herr", "Wirth", ",", "die", "Stu\u00b7be", "ist", "uns", "viel", "zu", "en\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Auch r\u00fccken wir Tische und B\u00e4nke,", "tokens": ["Auch", "r\u00fc\u00b7cken", "wir", "Ti\u00b7sche", "und", "B\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.25": {"text": "Wir begie\u00dfen das Fl\u00f6tz recht \u00fcberall,", "tokens": ["Wir", "be\u00b7gie\u00b7\u00dfen", "das", "Fl\u00f6tz", "recht", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Sieht man die \u00c4uglein herwanken,", "tokens": ["Sieht", "man", "die", "\u00c4ug\u00b7lein", "her\u00b7wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.27": {"text": "Dazu kommen uns die Fiedler dar,", "tokens": ["Da\u00b7zu", "kom\u00b7men", "uns", "die", "Fied\u00b7ler", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Man h\u00f6rt die Saiten klingen,", "tokens": ["Man", "h\u00f6rt", "die", "Sai\u00b7ten", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Dazu die werthen Pfeifferlein gut,", "tokens": ["Da\u00b7zu", "die", "wert\u00b7hen", "Pfeif\u00b7fer\u00b7lein", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.30": {"text": "Man sieht die Hauer herspringen,", "tokens": ["Man", "sieht", "die", "Hau\u00b7er", "her\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Dazu die Jungfr\u00e4ulein s\u00e4uberlich,", "tokens": ["Da\u00b7zu", "die", "Jung\u00b7fr\u00e4u\u00b7lein", "s\u00e4u\u00b7ber\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Sie tragen rosenfarbe M\u00fcndlein,", "tokens": ["Sie", "tra\u00b7gen", "ro\u00b7sen\u00b7far\u00b7be", "M\u00fcnd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ihre W\u00e4nglein sind Lilien weis,", "tokens": ["Ih\u00b7re", "W\u00e4ng\u00b7lein", "sind", "Li\u00b7li\u00b7en", "weis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.34": {"text": "Der gewaltige M\u00fcnzerhof ist uns viel zu enge,", "tokens": ["Der", "ge\u00b7wal\u00b7ti\u00b7ge", "M\u00fcn\u00b7zer\u00b7hof", "ist", "uns", "viel", "zu", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Darauf wir das Silber bringen,", "tokens": ["Da\u00b7rauf", "wir", "das", "Sil\u00b7ber", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Merkt auf ihr werthen M\u00fcnzer gut,", "tokens": ["Merkt", "auf", "ihr", "wert\u00b7hen", "M\u00fcn\u00b7zer", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "VVFIN", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Helft mir die Hauer erkennen,", "tokens": ["Helft", "mir", "die", "Hau\u00b7er", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.38": {"text": "Sie hauen das Silber aus der alten Wand,", "tokens": ["Sie", "hau\u00b7en", "das", "Sil\u00b7ber", "aus", "der", "al\u00b7ten", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Die Gott der Herr selber gebauet hat,", "tokens": ["Die", "Gott", "der", "Herr", "sel\u00b7ber", "ge\u00b7bau\u00b7et", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "VA