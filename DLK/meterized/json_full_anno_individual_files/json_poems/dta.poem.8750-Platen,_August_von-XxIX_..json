{"dta.poem.8750": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIX .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Versank ich still und la\u00df,", "tokens": ["Ver\u00b7sank", "ich", "still", "und", "la\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVIMP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir war's, als h\u00e4tt' ich obgesiegt,", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "h\u00e4tt'", "ich", "ob\u00b7ge\u00b7siegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bezwungen Lieb' und Ha\u00df.", "tokens": ["Be\u00b7zwun\u00b7gen", "Lieb'", "und", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch f\u00fchl' ich, da\u00df zu jeder Frist", "tokens": ["Doch", "f\u00fchl'", "ich", ",", "da\u00df", "zu", "je\u00b7der", "Frist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz sich qu\u00e4lt und bangt,", "tokens": ["Das", "Herz", "sich", "qu\u00e4lt", "und", "bangt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df es nur gebrochen ist,", "tokens": ["Und", "da\u00df", "es", "nur", "ge\u00b7bro\u00b7chen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Anstatt zur Ruh' gelangt.", "tokens": ["An\u00b7statt", "zur", "Ruh'", "ge\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du hast zerst\u00fcckt mit Unbedacht", "tokens": ["Du", "hast", "zer\u00b7st\u00fcckt", "mit", "Un\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Spiegel dir, o Thor!", "tokens": ["Den", "Spie\u00b7gel", "dir", ",", "o", "Thor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun blickt der Schmerz verhundertfacht,", "tokens": ["Nun", "blickt", "der", "Schmerz", "ver\u00b7hun\u00b7dert\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertausendfacht hervor.", "tokens": ["Ver\u00b7tau\u00b7send\u00b7facht", "her\u00b7vor", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}