{"dta.poem.9275": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df wol/ da\u00df ich neider habe", "tokens": ["Ich", "wei\u00df", "wol", "/", "da\u00df", "ich", "nei\u00b7der", "ha\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir nach meinem gl\u00fccke stehn:", "tokens": ["Die", "mir", "nach", "mei\u00b7nem", "gl\u00fc\u00b7cke", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch alles ist des H\u00f6chsten gabe/", "tokens": ["Doch", "al\u00b7les", "ist", "des", "H\u00f6chs\u00b7ten", "ga\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daher entspringt mein wolergehn/", "tokens": ["Da\u00b7her", "ent\u00b7springt", "mein", "wo\u00b7ler\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum bleib' ich allzeit unbetr\u00fcbt", "tokens": ["Drum", "bleib'", "ich", "all\u00b7zeit", "un\u00b7be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nehme was der himmel giebt.", "tokens": ["Und", "neh\u00b7me", "was", "der", "him\u00b7mel", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich sehe da\u00df ich sicher bleibe/", "tokens": ["Ich", "se\u00b7he", "da\u00df", "ich", "si\u00b7cher", "blei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob gleich der neid sehr grimmig thut:", "tokens": ["Ob", "gleich", "der", "neid", "sehr", "grim\u00b7mig", "thut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchle nichts an meinem leibe/", "tokens": ["Ich", "f\u00fch\u00b7le", "nichts", "an", "mei\u00b7nem", "lei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das essen schmeckt mir gleich so gut/", "tokens": ["Das", "es\u00b7sen", "schmeckt", "mir", "gleich", "so", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jm schlaffe bin ich unverst\u00f6rt/", "tokens": ["Jm", "schlaf\u00b7fe", "bin", "ich", "un\u00b7ver\u00b7st\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und habe stets was mir geh\u00f6rt.", "tokens": ["Und", "ha\u00b7be", "stets", "was", "mir", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Hingegen wolt ich eh nicht leben", "tokens": ["Hin\u00b7ge\u00b7gen", "wolt", "ich", "eh", "nicht", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "KOUS", "PTKNEG", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Als an der neider stelle seyn/", "tokens": ["Als", "an", "der", "nei\u00b7der", "stel\u00b7le", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil sie nach meiner wohlfarth streben/", "tokens": ["Weil", "sie", "nach", "mei\u00b7ner", "wohl\u00b7farth", "stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So fressen sie viel kummer ein/", "tokens": ["So", "fres\u00b7sen", "sie", "viel", "kum\u00b7mer", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kr\u00e4ncken sich mit ihrem sinn/", "tokens": ["Und", "kr\u00e4n\u00b7cken", "sich", "mit", "ih\u00b7rem", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich so frisch und munter bin.", "tokens": ["Da\u00df", "ich", "so", "frisch", "und", "mun\u00b7ter", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ach w\u00e4r ich nur in noth und schmertzen/", "tokens": ["Ach", "w\u00e4r", "ich", "nur", "in", "noth", "und", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In angst/ gefahr und traurigkeit/", "tokens": ["In", "angst", "/", "ge\u00b7fahr", "und", "trau\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00e4men sie mit falschem hertzen/", "tokens": ["So", "k\u00e4\u00b7men", "sie", "mit", "fal\u00b7schem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herr sein zustand ist mir leid/", "tokens": ["Mein", "Herr", "sein", "zu\u00b7stand", "ist", "mir", "leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und der ist nicht ein ehrlich mann/", "tokens": ["Und", "der", "ist", "nicht", "ein", "ehr\u00b7lich", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "PTKNEG", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ihm nicht dienet/ wo er kan.", "tokens": ["Der", "ihm", "nicht", "die\u00b7net", "/", "wo", "er", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$(", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Wolan/ weil auff die sonnen-blicke", "tokens": ["Wo\u00b7lan", "/", "weil", "auff", "die", "son\u00b7nen\u00b7bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schwartzes schatten-bild entsteht:", "tokens": ["Ein", "schwart\u00b7zes", "schat\u00b7ten\u00b7bild", "ent\u00b7steht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und also auch bey allem gl\u00fccke", "tokens": ["Und", "al\u00b7so", "auch", "bey", "al\u00b7lem", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der blasse neid darneben geht:", "tokens": ["Der", "blas\u00b7se", "neid", "dar\u00b7ne\u00b7ben", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So g\u00e4b ich einen centner neid", "tokens": ["So", "g\u00e4b", "ich", "ei\u00b7nen", "cent\u00b7ner", "neid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht vor ein loth barmhertzigkeit.", "tokens": ["Nicht", "vor", "ein", "loth", "barm\u00b7hert\u00b7zig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Gott wolle mir barmhertzig seyn/", "tokens": ["Gott", "wol\u00b7le", "mir", "barm\u00b7hert\u00b7zig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So will ich mitten in dem leiden", "tokens": ["So", "will", "ich", "mit\u00b7ten", "in", "dem", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich \u00fcber mein gel\u00fccke freun.", "tokens": ["Mich", "\u00fc\u00b7ber", "mein", "ge\u00b7l\u00fc\u00b7cke", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott und die tugend ist mein ziel/", "tokens": ["Gott", "und", "die", "tu\u00b7gend", "ist", "mein", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "So hab ich was ich haben wil.", "tokens": ["So", "hab", "ich", "was", "ich", "ha\u00b7ben", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PWS", "PPER", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}