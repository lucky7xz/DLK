{"dta.poem.8243": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Zum Weihnachten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Tritt noch einmal mir behend!", "tokens": ["Tritt", "noch", "ein\u00b7mal", "mir", "be\u00b7hend", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Folg' mir durch des Abends Ruhe,", "tokens": ["Fol\u00b7g'", "mir", "durch", "des", "A\u00b7bends", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo der dunkle Taxus brennt.", "tokens": ["Wo", "der", "dunk\u00b7le", "Ta\u00b7xus", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Engel knieen an der Schwelle,", "tokens": ["En\u00b7gel", "kni\u00b7e\u00b7en", "an", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "H\u00fctend bei dem frommen Schein;", "tokens": ["H\u00fc\u00b7tend", "bei", "dem", "from\u00b7men", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Lippen klingt es helle:", "tokens": ["Von", "den", "Lip\u00b7pen", "klingt", "es", "hel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die Kindlein gehen ein!", "tokens": ["Nur", "die", "Kin\u00b7dlein", "ge\u00b7hen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch du schaust mich an verwundert,", "tokens": ["Doch", "du", "schaust", "mich", "an", "ver\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APZR", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprichst: \u201eVertreten sind die Schuh';", "tokens": ["Sprichst", ":", "\u201e", "Ver\u00b7tre\u00b7ten", "sind", "die", "Schuh'", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter alt' verge\u00dfnem Plunder", "tokens": ["Un\u00b7ter", "alt'", "ver\u00b7ge\u00df\u00b7nem", "Plun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt die Puppe in der Truh.\u201c", "tokens": ["Liegt", "die", "Pup\u00b7pe", "in", "der", "Truh", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Horch nur auf! Die alten M\u00e4rchen", "tokens": ["Horch", "nur", "auf", "!", "Die", "al\u00b7ten", "M\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn dich in die alte Pracht!", "tokens": ["Ziehn", "dich", "in", "die", "al\u00b7te", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie im Zauberwald das P\u00e4rchen", "tokens": ["Wie", "im", "Zau\u00b7ber\u00b7wald", "das", "P\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwatzen wir die ganze Nacht.", "tokens": ["Schwat\u00b7zen", "wir", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Von Schneewittchen bei den Zwergen,", "tokens": ["Von", "Schnee\u00b7witt\u00b7chen", "bei", "den", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo sie lebte unerkannt,", "tokens": ["Wo", "sie", "leb\u00b7te", "un\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und war hinter ihren Bergen", "tokens": ["Und", "war", "hin\u00b7ter", "ih\u00b7ren", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch die Sch\u00f6nst' im ganzen Land.", "tokens": ["Doch", "die", "Sch\u00f6nst'", "im", "gan\u00b7zen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von Hans B\u00e4rlein, der im Streite", "tokens": ["Von", "Hans", "B\u00e4rl\u00b7ein", ",", "der", "im", "Strei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einen Riesenritter schlug,", "tokens": ["Ei\u00b7nen", "Rie\u00b7sen\u00b7rit\u00b7ter", "schlug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die K\u00f6nigstochter freite,", "tokens": ["Der", "die", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "frei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich gar die Krone trug.", "tokens": ["End\u00b7lich", "gar", "die", "Kro\u00b7ne", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Von dem Dichter auch daheime,", "tokens": ["Von", "dem", "Dich\u00b7ter", "auch", "da\u00b7hei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ein M\u00e4dchen, gro\u00df und schlank,", "tokens": ["Der", "ein", "M\u00e4d\u00b7chen", ",", "gro\u00df", "und", "schlank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Zauberkraft der Reime", "tokens": ["Durch", "die", "Zau\u00b7ber\u00b7kraft", "der", "Rei\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fcckw\u00e4rts in die Kindheit sang.", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "in", "die", "Kind\u00b7heit", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}