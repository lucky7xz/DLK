{"textgrid.poem.40220": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ums Brot", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Einem, Einem m\u00f6cht' ich sagen,", "tokens": ["Nur", "Ei\u00b7nem", ",", "Ei\u00b7nem", "m\u00f6cht'", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was mir das Herz so gar beklemmt,", "tokens": ["was", "mir", "das", "Herz", "so", "gar", "be\u00b7klemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so schwer sein Hochverlangen hemmt!", "tokens": ["so", "schwer", "sein", "Hoch\u00b7ver\u00b7lan\u00b7gen", "hemmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O k\u00f6nnte ich noch Einmal klagen,", "tokens": ["O", "k\u00f6nn\u00b7te", "ich", "noch", "Ein\u00b7mal", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "wohin ich Einst mein Leid getragen:", "tokens": ["wo\u00b7hin", "ich", "Einst", "mein", "Leid", "ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "an meiner Mutter Brust!", "tokens": ["an", "mei\u00b7ner", "Mut\u00b7ter", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von all den Andern wei\u00df ich Keinen,", "tokens": ["Von", "all", "den", "An\u00b7dern", "wei\u00df", "ich", "Kei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "VVFIN", "PPER", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der ganz in mir sich selbst vergi\u00dft,", "tokens": ["der", "ganz", "in", "mir", "sich", "selbst", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nicht ", "tokens": ["nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "und froh und gl\u00fccklich mu\u00df ich scheinen.", "tokens": ["und", "froh", "und", "gl\u00fcck\u00b7lich", "mu\u00df", "ich", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O k\u00f6nnte ich noch Einmal weinen", "tokens": ["O", "k\u00f6nn\u00b7te", "ich", "noch", "Ein\u00b7mal", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "an meiner Mutter Brust!", "tokens": ["an", "mei\u00b7ner", "Mut\u00b7ter", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "