{"textgrid.poem.66486": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergessenheit, dir bracht' ich heut ein Brandopfer,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergessenheit, dir bracht' ich heut ein Brandopfer,", "tokens": ["Ver\u00b7ges\u00b7sen\u00b7heit", ",", "dir", "bracht'", "ich", "heut", "ein", "Bran\u00b7dop\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wie seit manchem Jahr' ich's bring', im Hauswesen", "tokens": ["So", "wie", "seit", "man\u00b7chem", "Jahr'", "ich's", "bring'", ",", "im", "Haus\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "PIAT", "NN", "PIS", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Papiernen Krams von Pros' und Versen aufr\u00e4umend;", "tokens": ["Pa\u00b7pier\u00b7nen", "Krams", "von", "Pros'", "und", "Ver\u00b7sen", "auf\u00b7r\u00e4u\u00b7mend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Heut nimm als Opferlied noch diese Hinkjamben!", "tokens": ["Heut", "nimm", "als", "Op\u00b7fer\u00b7lied", "noch", "die\u00b7se", "Hink\u00b7jam\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "KOUS", "NN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Viel speichert sich von Dingen auf, was unfertig,", "tokens": ["Viel", "spei\u00b7chert", "sich", "von", "Din\u00b7gen", "auf", ",", "was", "un\u00b7fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mi\u00dflungen, falsch gedacht, trotz mancher M\u00fchwaltung", "tokens": ["Mi\u00df\u00b7lun\u00b7gen", ",", "falsch", "ge\u00b7dacht", ",", "trotz", "man\u00b7cher", "M\u00fch\u00b7wal\u00b7tung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Zuletzt doch aufgegeben, in den Schubf\u00e4chern", "tokens": ["Zu\u00b7letzt", "doch", "auf\u00b7ge\u00b7ge\u00b7ben", ",", "in", "den", "Schub\u00b7f\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Den Raum nur engt: Novellen, Dramenbruchst\u00fccke,", "tokens": ["Den", "Raum", "nur", "engt", ":", "No\u00b7vel\u00b7len", ",", "Dra\u00b7men\u00b7bruchs\u00b7t\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Romanfragmente, hingeworfne Reimk\u00fcnste;", "tokens": ["Ro\u00b7man\u00b7frag\u00b7men\u00b7te", ",", "hin\u00b7ge\u00b7worf\u00b7ne", "Reim\u00b7k\u00fcns\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "So manches auch, was leidlich bei der Arbeit schien,", "tokens": ["So", "man\u00b7ches", "auch", ",", "was", "leid\u00b7lich", "bei", "der", "Ar\u00b7beit", "schien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und als es fertig, wie Empfang von Ohrfeigen,", "tokens": ["Und", "als", "es", "fer\u00b7tig", ",", "wie", "Emp\u00b7fang", "von", "Ohr\u00b7fei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$,", "PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Ach, von des eigenen Gesch\u00f6pfes Handreichung,", "tokens": ["Ach", ",", "von", "des", "ei\u00b7ge\u00b7nen", "Ge\u00b7sch\u00f6p\u00b7fes", "Hand\u00b7rei\u00b7chung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Besch\u00e4mend mich ber\u00fchrte! Wart, du Bosheitsbrut,", "tokens": ["Be\u00b7sch\u00e4\u00b7mend", "mich", "be\u00b7r\u00fchr\u00b7te", "!", "Wart", ",", "du", "Bos\u00b7heits\u00b7brut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "PPER", "VVFIN", "$.", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die so mich foppte! Wild Gez\u00fccht der Selbstt\u00e4uschung,", "tokens": ["Die", "so", "mich", "fopp\u00b7te", "!", "Wild", "Ge\u00b7z\u00fccht", "der", "Selbst\u00b7t\u00e4u\u00b7schung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$.", "NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "In's Ofenloch! Verkohle, brenne, flamm' hochauf!", "tokens": ["In's", "O\u00b7fen\u00b7loch", "!", "Ver\u00b7koh\u00b7le", ",", "bren\u00b7ne", ",", "flamm'", "hoch\u00b7auf", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ritz, ratz, ihr Fetzen! Immer mehr? Du f\u00fcnfakt'ge", "tokens": ["Ritz", ",", "ratz", ",", "ihr", "Fet\u00b7zen", "!", "Im\u00b7mer", "mehr", "?", "Du", "f\u00fcn\u00b7fakt'\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "ADV", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kom\u00f6dienrange warst die schlimmste! Hei\u00df werden", "tokens": ["Ko\u00b7m\u00f6\u00b7di\u00b7en\u00b7ran\u00b7ge", "warst", "die", "schlimms\u00b7te", "!", "Hei\u00df", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "$.", "NN", "VAINF"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.18": {"text": "Soll dir, wie mir! Und du, Novellenschlafm\u00fctze", "tokens": ["Soll", "dir", ",", "wie", "mir", "!", "Und", "du", ",", "No\u00b7vel\u00b7len\u00b7schlaf\u00b7m\u00fct\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "$.", "KON", "PPER", "$,", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "(die einz'ge, die ich jemals trug) du sperrst g\u00e4hnend", "tokens": ["(", "die", "einz'\u00b7ge", ",", "die", "ich", "je\u00b7mals", "trug", ")", "du", "sperrst", "g\u00e4h\u00b7nend"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$(", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Den Mund auf, h\u00f6hnend noch, da\u00df ich im Halbschlaf dich", "tokens": ["Den", "Mund", "auf", ",", "h\u00f6h\u00b7nend", "noch", ",", "da\u00df", "ich", "im", "Halb\u00b7schlaf", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJD", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Aus Lappen flickte? Ho! Du sollst in Glutasche", "tokens": ["Aus", "Lap\u00b7pen", "flick\u00b7te", "?", "Ho", "!", "Du", "sollst", "in", "Glu\u00b7ta\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "NE", "$.", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jetzt schwellen, lustiger als mir dein Dasein war!", "tokens": ["Jetzt", "schwel\u00b7len", ",", "lus\u00b7ti\u00b7ger", "als", "mir", "dein", "Da\u00b7sein", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJA", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was siehst du Liederheft mich an so scheinheilig?", "tokens": ["Was", "siehst", "du", "Lie\u00b7der\u00b7heft", "mich", "an", "so", "schein\u00b7hei\u00b7lig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Verw\u00fcnschter M\u00fcckenschwarm, der mir aus Hundstagen", "tokens": ["Ver\u00b7w\u00fcnschter", "M\u00fc\u00b7cken\u00b7schwarm", ",", "der", "mir", "aus", "Hunds\u00b7ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-++-+-+-+--", "measure": "unknown.measure.penta"}, "line.25": {"text": "Vergang'ner Sommer, die ich nicht zur\u00fcckw\u00fcnsche,", "tokens": ["Ver\u00b7gang'\u00b7ner", "Som\u00b7mer", ",", "die", "ich", "nicht", "zu\u00b7r\u00fcck\u00b7w\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Nas' umspielt! Das w\u00e4r' Originaltonart,", "tokens": ["Die", "Nas'", "um\u00b7spielt", "!", "Das", "w\u00e4r'", "O\u00b7rig\u00b7in\u00b7al\u00b7ton\u00b7art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Aus der du pfeifst? Wir kennen die Gebrauchsweise", "tokens": ["Aus", "der", "du", "pfeifst", "?", "Wir", "ken\u00b7nen", "die", "Ge\u00b7brauchs\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Der alten Leier! Knistre nun im Rauchloche!", "tokens": ["Der", "al\u00b7ten", "Lei\u00b7er", "!", "Knist\u00b7re", "nun", "im", "Rauch\u00b7lo\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.29": {"text": "Da ist Musik, die ich f\u00fcr dich gesetzt habe.", "tokens": ["Da", "ist", "Mu\u00b7sik", ",", "die", "ich", "f\u00fcr", "dich", "ge\u00b7setzt", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.30": {"text": "So kann ich freier wieder einmal aufathmen.", "tokens": ["So", "kann", "ich", "frei\u00b7er", "wie\u00b7der", "ein\u00b7mal", "auf\u00b7ath\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Und sterb' ich dieses Jahr, so ist der Nachla\u00df doch", "tokens": ["Und", "sterb'", "ich", "die\u00b7ses", "Jahr", ",", "so", "ist", "der", "Nachl\u00b7a\u00df", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Schon lichter, und erspart wird ernstes Kopfsch\u00fctteln", "tokens": ["Schon", "lich\u00b7ter", ",", "und", "er\u00b7spart", "wird", "erns\u00b7tes", "Kopf\u00b7sch\u00fct\u00b7teln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "VVPP", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.33": {"text": "Dem Guten, der da nachforscht um Herausgabe.", "tokens": ["Dem", "Gu\u00b7ten", ",", "der", "da", "nach\u00b7forscht", "um", "Her\u00b7aus\u00b7ga\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "So etwas kann ja doch geschehn. Wer schriftstellert", "tokens": ["So", "et\u00b7was", "kann", "ja", "doch", "ge\u00b7schehn", ".", "Wer", "schrift\u00b7stel\u00b7lert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIS", "VMFIN", "ADV", "ADV", "VVPP", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und stirbt, der wird mit einemmal auf sechs Wochen", "tokens": ["Und", "stirbt", ",", "der", "wird", "mit", "ei\u00b7nem\u00b7mal", "auf", "sechs", "Wo\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "VAFIN", "APPR", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "In Deutschland Mode, wenn er auch bei Lebzeiten", "tokens": ["In", "Deutschland", "Mo\u00b7de", ",", "wenn", "er", "auch", "bei", "Leb\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Niemals in Mode war. Drum ist die Druckschw\u00e4rze", "tokens": ["Nie\u00b7mals", "in", "Mo\u00b7de", "war", ".", "Drum", "ist", "die", "Druck\u00b7schw\u00e4r\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN", "$.", "PAV", "VAFIN", "ART", "NN"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.38": {"text": "Dem Nachla\u00df sicher, nebst ein bischen Theilnahme,", "tokens": ["Dem", "Nachl\u00b7a\u00df", "si\u00b7cher", ",", "nebst", "ein", "bi\u00b7schen", "Theil\u00b7nah\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.39": {"text": "Auf die der Lebende vielleicht umsonst hoffte. \u2013", "tokens": ["Auf", "die", "der", "Le\u00b7ben\u00b7de", "viel\u00b7leicht", "um\u00b7sonst", "hoff\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+--++-++-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Hochheil'ge Flamme! H\u00e4tt' ich doch von fr\u00fchauf dir", "tokens": ["Hoch\u00b7heil'\u00b7ge", "Flam\u00b7me", "!", "H\u00e4tt'", "ich", "doch", "von", "fr\u00fch\u00b7auf", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "APPR", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Mehr anvertraut zu weihevoller Selbstl\u00e4utrung!", "tokens": ["Mehr", "an\u00b7ver\u00b7traut", "zu", "wei\u00b7he\u00b7vol\u00b7ler", "Selbst\u00b7l\u00e4u\u00b7trung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mehr anvertraut von dem, was jetzt auf Staubbrettern", "tokens": ["Mehr", "an\u00b7ver\u00b7traut", "von", "dem", ",", "was", "jetzt", "auf", "Staub\u00b7bret\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "ART", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+---+-+--", "measure": "unknown.measure.tetra"}, "line.43": {"text": "Gebunden steht, vergessen halb vom Zornrichten,", "tokens": ["Ge\u00b7bun\u00b7den", "steht", ",", "ver\u00b7ges\u00b7sen", "halb", "vom", "Zorn\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.44": {"text": "Doch einst vielleicht herabgezerrt, zu trostloser", "tokens": ["Doch", "einst", "viel\u00b7leicht", "her\u00b7ab\u00b7ge\u00b7zerrt", ",", "zu", "trost\u00b7lo\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVPP", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Verwunderung der Guten, die drin nachbl\u00e4ttern!", "tokens": ["Ver\u00b7wun\u00b7de\u00b7rung", "der", "Gu\u00b7ten", ",", "die", "drin", "nach\u00b7bl\u00e4t\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Dein Segen, heil'ges Feuer, f\u00fcr das Hausopfer", "tokens": ["Dein", "Se\u00b7gen", ",", "heil'\u00b7ges", "Feu\u00b7er", ",", "f\u00fcr", "das", "Haus\u00b7op\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Im Ofenloch kam sp\u00e4t, doch kam mir's einleuchtend!", "tokens": ["Im", "O\u00b7fen\u00b7loch", "kam", "sp\u00e4t", ",", "doch", "kam", "mir's", "ein\u00b7leuch\u00b7tend", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "$,", "ADV", "VVFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Brennt es wohl noch? Nein. Schwarzverkohlter Aschflocken", "tokens": ["Brennt", "es", "wohl", "noch", "?", "Nein", ".", "Schwarz\u00b7ver\u00b7kohl\u00b7ter", "Aschflo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "PTKANT", "$.", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.49": {"text": "Ein H\u00e4uflein weht entgegen durch den Zugwind mir.", "tokens": ["Ein", "H\u00e4uf\u00b7lein", "weht", "ent\u00b7ge\u00b7gen", "durch", "den", "Zug\u00b7wind", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wie gro\u00df das Opfer wohl im n\u00e4chsten Jahr sein wird?", "tokens": ["Wie", "gro\u00df", "das", "Op\u00b7fer", "wohl", "im", "n\u00e4chs\u00b7ten", "Jahr", "sein", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich w\u00fcnscht' es kleiner doch. So klein, da\u00df nichts weiter", "tokens": ["Ich", "w\u00fcnscht'", "es", "klei\u00b7ner", "doch", ".", "So", "klein", ",", "da\u00df", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "$.", "ADV", "ADJD", "$,", "KOUS", "PIS", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Zu opfern w\u00e4r, als dieses H\u00e4uflein Hinkjamben!", "tokens": ["Zu", "op\u00b7fern", "w\u00e4r", ",", "als", "die\u00b7ses", "H\u00e4uf\u00b7lein", "Hink\u00b7jam\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "$,", "KOUS", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Vergessenheit, dir bracht' ich heut ein Brandopfer,", "tokens": ["Ver\u00b7ges\u00b7sen\u00b7heit", ",", "dir", "bracht'", "ich", "heut", "ein", "Bran\u00b7dop\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wie seit manchem Jahr' ich's bring', im Hauswesen", "tokens": ["So", "wie", "seit", "man\u00b7chem", "Jahr'", "ich's", "bring'", ",", "im", "Haus\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "PIAT", "NN", "PIS", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Papiernen Krams von Pros' und Versen aufr\u00e4umend;", "tokens": ["Pa\u00b7pier\u00b7nen", "Krams", "von", "Pros'", "und", "Ver\u00b7sen", "auf\u00b7r\u00e4u\u00b7mend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Heut nimm als Opferlied noch diese Hinkjamben!", "tokens": ["Heut", "nimm", "als", "Op\u00b7fer\u00b7lied", "noch", "die\u00b7se", "Hink\u00b7jam\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "KOUS", "NN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Viel speichert sich von Dingen auf, was unfertig,", "tokens": ["Viel", "spei\u00b7chert", "sich", "von", "Din\u00b7gen", "auf", ",", "was", "un\u00b7fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mi\u00dflungen, falsch gedacht, trotz mancher M\u00fchwaltung", "tokens": ["Mi\u00df\u00b7lun\u00b7gen", ",", "falsch", "ge\u00b7dacht", ",", "trotz", "man\u00b7cher", "M\u00fch\u00b7wal\u00b7tung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Zuletzt doch aufgegeben, in den Schubf\u00e4chern", "tokens": ["Zu\u00b7letzt", "doch", "auf\u00b7ge\u00b7ge\u00b7ben", ",", "in", "den", "Schub\u00b7f\u00e4\u00b7chern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Den Raum nur engt: Novellen, Dramenbruchst\u00fccke,", "tokens": ["Den", "Raum", "nur", "engt", ":", "No\u00b7vel\u00b7len", ",", "Dra\u00b7men\u00b7bruchs\u00b7t\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Romanfragmente, hingeworfne Reimk\u00fcnste;", "tokens": ["Ro\u00b7man\u00b7frag\u00b7men\u00b7te", ",", "hin\u00b7ge\u00b7worf\u00b7ne", "Reim\u00b7k\u00fcns\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "So manches auch, was leidlich bei der Arbeit schien,", "tokens": ["So", "man\u00b7ches", "auch", ",", "was", "leid\u00b7lich", "bei", "der", "Ar\u00b7beit", "schien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und als es fertig, wie Empfang von Ohrfeigen,", "tokens": ["Und", "als", "es", "fer\u00b7tig", ",", "wie", "Emp\u00b7fang", "von", "Ohr\u00b7fei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$,", "PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Ach, von des eigenen Gesch\u00f6pfes Handreichung,", "tokens": ["Ach", ",", "von", "des", "ei\u00b7ge\u00b7nen", "Ge\u00b7sch\u00f6p\u00b7fes", "Hand\u00b7rei\u00b7chung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Besch\u00e4mend mich ber\u00fchrte! Wart, du Bosheitsbrut,", "tokens": ["Be\u00b7sch\u00e4\u00b7mend", "mich", "be\u00b7r\u00fchr\u00b7te", "!", "Wart", ",", "du", "Bos\u00b7heits\u00b7brut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "PPER", "VVFIN", "$.", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die so mich foppte! Wild Gez\u00fccht der Selbstt\u00e4uschung,", "tokens": ["Die", "so", "mich", "fopp\u00b7te", "!", "Wild", "Ge\u00b7z\u00fccht", "der", "Selbst\u00b7t\u00e4u\u00b7schung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$.", "NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "In's Ofenloch! Verkohle, brenne, flamm' hochauf!", "tokens": ["In's", "O\u00b7fen\u00b7loch", "!", "Ver\u00b7koh\u00b7le", ",", "bren\u00b7ne", ",", "flamm'", "hoch\u00b7auf", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ritz, ratz, ihr Fetzen! Immer mehr? Du f\u00fcnfakt'ge", "tokens": ["Ritz", ",", "ratz", ",", "ihr", "Fet\u00b7zen", "!", "Im\u00b7mer", "mehr", "?", "Du", "f\u00fcn\u00b7fakt'\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "ADV", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kom\u00f6dienrange warst die schlimmste! Hei\u00df werden", "tokens": ["Ko\u00b7m\u00f6\u00b7di\u00b7en\u00b7ran\u00b7ge", "warst", "die", "schlimms\u00b7te", "!", "Hei\u00df", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "$.", "NN", "VAINF"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.18": {"text": "Soll dir, wie mir! Und du, Novellenschlafm\u00fctze", "tokens": ["Soll", "dir", ",", "wie", "mir", "!", "Und", "du", ",", "No\u00b7vel\u00b7len\u00b7schlaf\u00b7m\u00fct\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "$.", "KON", "PPER", "$,", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "(die einz'ge, die ich jemals trug) du sperrst g\u00e4hnend", "tokens": ["(", "die", "einz'\u00b7ge", ",", "die", "ich", "je\u00b7mals", "trug", ")", "du", "sperrst", "g\u00e4h\u00b7nend"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$(", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Den Mund auf, h\u00f6hnend noch, da\u00df ich im Halbschlaf dich", "tokens": ["Den", "Mund", "auf", ",", "h\u00f6h\u00b7nend", "noch", ",", "da\u00df", "ich", "im", "Halb\u00b7schlaf", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJD", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Aus Lappen flickte? Ho! Du sollst in Glutasche", "tokens": ["Aus", "Lap\u00b7pen", "flick\u00b7te", "?", "Ho", "!", "Du", "sollst", "in", "Glu\u00b7ta\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "NE", "$.", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jetzt schwellen, lustiger als mir dein Dasein war!", "tokens": ["Jetzt", "schwel\u00b7len", ",", "lus\u00b7ti\u00b7ger", "als", "mir", "dein", "Da\u00b7sein", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJA", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was siehst du Liederheft mich an so scheinheilig?", "tokens": ["Was", "siehst", "du", "Lie\u00b7der\u00b7heft", "mich", "an", "so", "schein\u00b7hei\u00b7lig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Verw\u00fcnschter M\u00fcckenschwarm, der mir aus Hundstagen", "tokens": ["Ver\u00b7w\u00fcnschter", "M\u00fc\u00b7cken\u00b7schwarm", ",", "der", "mir", "aus", "Hunds\u00b7ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-++-+-+-+--", "measure": "unknown.measure.penta"}, "line.25": {"text": "Vergang'ner Sommer, die ich nicht zur\u00fcckw\u00fcnsche,", "tokens": ["Ver\u00b7gang'\u00b7ner", "Som\u00b7mer", ",", "die", "ich", "nicht", "zu\u00b7r\u00fcck\u00b7w\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Nas' umspielt! Das w\u00e4r' Originaltonart,", "tokens": ["Die", "Nas'", "um\u00b7spielt", "!", "Das", "w\u00e4r'", "O\u00b7rig\u00b7in\u00b7al\u00b7ton\u00b7art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Aus der du pfeifst? Wir kennen die Gebrauchsweise", "tokens": ["Aus", "der", "du", "pfeifst", "?", "Wir", "ken\u00b7nen", "die", "Ge\u00b7brauchs\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Der alten Leier! Knistre nun im Rauchloche!", "tokens": ["Der", "al\u00b7ten", "Lei\u00b7er", "!", "Knist\u00b7re", "nun", "im", "Rauch\u00b7lo\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.29": {"text": "Da ist Musik, die ich f\u00fcr dich gesetzt habe.", "tokens": ["Da", "ist", "Mu\u00b7sik", ",", "die", "ich", "f\u00fcr", "dich", "ge\u00b7setzt", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.30": {"text": "So kann ich freier wieder einmal aufathmen.", "tokens": ["So", "kann", "ich", "frei\u00b7er", "wie\u00b7der", "ein\u00b7mal", "auf\u00b7ath\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Und sterb' ich dieses Jahr, so ist der Nachla\u00df doch", "tokens": ["Und", "sterb'", "ich", "die\u00b7ses", "Jahr", ",", "so", "ist", "der", "Nachl\u00b7a\u00df", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Schon lichter, und erspart wird ernstes Kopfsch\u00fctteln", "tokens": ["Schon", "lich\u00b7ter", ",", "und", "er\u00b7spart", "wird", "erns\u00b7tes", "Kopf\u00b7sch\u00fct\u00b7teln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "VVPP", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.33": {"text": "Dem Guten, der da nachforscht um Herausgabe.", "tokens": ["Dem", "Gu\u00b7ten", ",", "der", "da", "nach\u00b7forscht", "um", "Her\u00b7aus\u00b7ga\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "So etwas kann ja doch geschehn. Wer schriftstellert", "tokens": ["So", "et\u00b7was", "kann", "ja", "doch", "ge\u00b7schehn", ".", "Wer", "schrift\u00b7stel\u00b7lert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIS", "VMFIN", "ADV", "ADV", "VVPP", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und stirbt, der wird mit einemmal auf sechs Wochen", "tokens": ["Und", "stirbt", ",", "der", "wird", "mit", "ei\u00b7nem\u00b7mal", "auf", "sechs", "Wo\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "VAFIN", "APPR", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "In Deutschland Mode, wenn er auch bei Lebzeiten", "tokens": ["In", "Deutschland", "Mo\u00b7de", ",", "wenn", "er", "auch", "bei", "Leb\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Niemals in Mode war. Drum ist die Druckschw\u00e4rze", "tokens": ["Nie\u00b7mals", "in", "Mo\u00b7de", "war", ".", "Drum", "ist", "die", "Druck\u00b7schw\u00e4r\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN", "$.", "PAV", "VAFIN", "ART", "NN"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.38": {"text": "Dem Nachla\u00df sicher, nebst ein bischen Theilnahme,", "tokens": ["Dem", "Nachl\u00b7a\u00df", "si\u00b7cher", ",", "nebst", "ein", "bi\u00b7schen", "Theil\u00b7nah\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.39": {"text": "Auf die der Lebende vielleicht umsonst hoffte. \u2013", "tokens": ["Auf", "die", "der", "Le\u00b7ben\u00b7de", "viel\u00b7leicht", "um\u00b7sonst", "hoff\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+--++-++-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Hochheil'ge Flamme! H\u00e4tt' ich doch von fr\u00fchauf dir", "tokens": ["Hoch\u00b7heil'\u00b7ge", "Flam\u00b7me", "!", "H\u00e4tt'", "ich", "doch", "von", "fr\u00fch\u00b7auf", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "APPR", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Mehr anvertraut zu weihevoller Selbstl\u00e4utrung!", "tokens": ["Mehr", "an\u00b7ver\u00b7traut", "zu", "wei\u00b7he\u00b7vol\u00b7ler", "Selbst\u00b7l\u00e4u\u00b7trung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mehr anvertraut von dem, was jetzt auf Staubbrettern", "tokens": ["Mehr", "an\u00b7ver\u00b7traut", "von", "dem", ",", "was", "jetzt", "auf", "Staub\u00b7bret\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "ART", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+---+-+--", "measure": "unknown.measure.tetra"}, "line.43": {"text": "Gebunden steht, vergessen halb vom Zornrichten,", "tokens": ["Ge\u00b7bun\u00b7den", "steht", ",", "ver\u00b7ges\u00b7sen", "halb", "vom", "Zorn\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.44": {"text": "Doch einst vielleicht herabgezerrt, zu trostloser", "tokens": ["Doch", "einst", "viel\u00b7leicht", "her\u00b7ab\u00b7ge\u00b7zerrt", ",", "zu", "trost\u00b7lo\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVPP", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Verwunderung der Guten, die drin nachbl\u00e4ttern!", "tokens": ["Ver\u00b7wun\u00b7de\u00b7rung", "der", "Gu\u00b7ten", ",", "die", "drin", "nach\u00b7bl\u00e4t\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Dein Segen, heil'ges Feuer, f\u00fcr das Hausopfer", "tokens": ["Dein", "Se\u00b7gen", ",", "heil'\u00b7ges", "Feu\u00b7er", ",", "f\u00fcr", "das", "Haus\u00b7op\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Im Ofenloch kam sp\u00e4t, doch kam mir's einleuchtend!", "tokens": ["Im", "O\u00b7fen\u00b7loch", "kam", "sp\u00e4t", ",", "doch", "kam", "mir's", "ein\u00b7leuch\u00b7tend", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "$,", "ADV", "VVFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Brennt es wohl noch? Nein. Schwarzverkohlter Aschflocken", "tokens": ["Brennt", "es", "wohl", "noch", "?", "Nein", ".", "Schwarz\u00b7ver\u00b7kohl\u00b7ter", "Aschflo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "PTKANT", "$.", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.49": {"text": "Ein H\u00e4uflein weht entgegen durch den Zugwind mir.", "tokens": ["Ein", "H\u00e4uf\u00b7lein", "weht", "ent\u00b7ge\u00b7gen", "durch", "den", "Zug\u00b7wind", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wie gro\u00df das Opfer wohl im n\u00e4chsten Jahr sein wird?", "tokens": ["Wie", "gro\u00df", "das", "Op\u00b7fer", "wohl", "im", "n\u00e4chs\u00b7ten", "Jahr", "sein", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich w\u00fcnscht' es kleiner doch. So klein, da\u00df nichts weiter", "tokens": ["Ich", "w\u00fcnscht'", "es", "klei\u00b7ner", "doch", ".", "So", "klein", ",", "da\u00df", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "$.", "ADV", "ADJD", "$,", "KOUS", "PIS", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Zu opfern w\u00e4r, als dieses H\u00e4uflein Hinkjamben!", "tokens": ["Zu", "op\u00b7fern", "w\u00e4r", ",", "als", "die\u00b7ses", "H\u00e4uf\u00b7lein", "Hink\u00b7jam\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "$,", "KOUS", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}