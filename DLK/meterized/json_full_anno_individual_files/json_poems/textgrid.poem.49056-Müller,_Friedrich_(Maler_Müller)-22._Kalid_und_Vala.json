 "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lange stumm in frohem Schmerz.", "tokens": ["Lan\u00b7ge", "stumm", "in", "fro\u00b7hem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schlag auf Schlag, o s\u00fc\u00dfes Beben!", "tokens": ["Schlag", "auf", "Schlag", ",", "o", "s\u00fc\u00b7\u00dfes", "Be\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterne berget euer Licht!", "tokens": ["Ster\u00b7ne", "ber\u00b7get", "eu\u00b7er", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles ist mir unverlohren,", "tokens": ["Al\u00b7les", "ist", "mir", "un\u00b7ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich werde neu gebohren,", "tokens": ["Und", "ich", "wer\u00b7de", "neu", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da mein Herz in Wonne bricht.", "tokens": ["Da", "mein", "Herz", "in", "Won\u00b7ne", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df, o Vala, la\u00df mein Leben