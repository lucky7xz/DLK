trochaic.tetra"}, "line.3": {"text": "Sieh', das Meer, die Sterne trinken,", "tokens": ["Sieh'", ",", "das", "Meer", ",", "die", "Ster\u00b7ne", "trin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Nacht verlischt im Sinken", "tokens": ["Und", "die", "Nacht", "ver\u00b7lischt", "im", "Sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Silberfackel schon!", "tokens": ["Ih\u00b7re", "Sil\u00b7ber\u00b7fa\u00b7ckel", "schon", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bangem Harm bin ich zum Raube,", "tokens": ["Ban\u00b7gem", "Harm", "bin", "ich", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine sch\u00f6ne, holde Taube", "tokens": ["Mei\u00b7ne", "sch\u00f6\u00b7ne", ",", "hol\u00b7de", "Tau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist sie mir vielleicht entflohn?", "tokens": ["Ist", "sie", "mir", "viel\u00b7leicht", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Vor Medina auf der Haide", "tokens": ["Vor", "Me\u00b7di\u00b7na", "auf", "der", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Sank vor meinem Speer Oron;", "tokens": ["Sank", "vor", "mei\u00b7nem", "Speer", "O\u00b7ron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwey verlarvte Ritter kamen,", "tokens": ["Zwey", "ver\u00b7larv\u00b7te", "Rit\u00b7ter", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fodernd in des Helden Namen", "tokens": ["Fo\u00b7dernd", "in", "des", "Hel\u00b7den", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rach und Blut mit wildem Ton.", "tokens": ["Rach", "und", "Blut", "mit", "wil\u00b7dem", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich gew\u00e4hrt's, es fielen Beide;", "tokens": ["Ich", "ge\u00b7w\u00e4hrt's", ",", "es", "fie\u00b7len", "Bei\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PPER", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber der, im Purpurkleide,", "tokens": ["A\u00b7ber", "der", ",", "im", "Pur\u00b7pur\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War Zenith, des Sultans Sohn.", "tokens": ["War", "Ze\u00b7nith", ",", "des", "Sul\u00b7tans", "Sohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Abbadul, der Vater, sandte", "tokens": ["Ab\u00b7ba\u00b7dul", ",", "der", "Va\u00b7ter", ",", "sand\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcthend drei\u00dfig M\u00f6rder aus,", "tokens": ["W\u00fct\u00b7hend", "drei\u00b7\u00dfig", "M\u00f6r\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen Kopf hoch aufzupfhalen,", "tokens": ["Mei\u00b7nen", "Kopf", "hoch", "auf\u00b7zu\u00b7pf\u00b7ha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Doch nur eitel war sein Prahlen,", "tokens": ["Doch", "nur", "ei\u00b7tel", "war", "sein", "Prah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie verstoben in dem Strau\u00df.", "tokens": ["Sie", "ver\u00b7sto\u00b7ben", "in", "dem", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwanzig, dem Tyrann zur Schande,", "tokens": ["Zwan\u00b7zig", ",", "dem", "Ty\u00b7rann", "zur", "Schan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Warf ich von der schn\u00f6den Bande", "tokens": ["Warf", "ich", "von", "der", "schn\u00f6\u00b7den", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geyern hin zum Leckerschmau\u00df.", "tokens": ["Ge\u00b7yern", "hin", "zum", "Le\u00b7cker\u00b7schmau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Aber aus Cairo's Thoren,", "tokens": ["A\u00b7ber", "aus", "Cai\u00b7ro's", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "St\u00fcrmten drei mal drei\u00dfig her,", "tokens": ["St\u00fcrm\u00b7ten", "drei", "mal", "drei\u00b7\u00dfig", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADV", "CARD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sicher-stolz auf das gewandte", "tokens": ["Si\u00b7cher\u00b7stolz", "auf", "das", "ge\u00b7wand\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ro\u00df, h\u00e4lt Kali, der Verbannte,", "tokens": ["Ro\u00df", ",", "h\u00e4lt", "Ka\u00b7li", ",", "der", "Ver\u00b7bann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch getrost im Sclavenheer.", "tokens": ["Noch", "ge\u00b7trost", "im", "Scla\u00b7ven\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da erschallt vor meinen Ohren", "tokens": ["Da", "er\u00b7schallt", "vor", "mei\u00b7nen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Flucht, ich war verlohren,", "tokens": ["Dei\u00b7ne", "Flucht", ",", "ich", "war", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meiner Faust entglitt der Speer.", "tokens": ["Mei\u00b7ner", "Faust", "ent\u00b7glitt", "der", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Dennoch schwurst du mir noch immer,", "tokens": ["Den\u00b7noch", "schwurst", "du", "mir", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Vala! Treue auf das Schwert;", "tokens": ["Va\u00b7la", "!", "Treu\u00b7e", "auf", "das", "Schwert", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag der Stahl die Brust zerspalten,", "tokens": ["Mag", "der", "Stahl", "die", "Brust", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh die Flamme soll erkalten,", "tokens": ["Eh", "die", "Flam\u00b7me", "soll", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die allein mein Leben n\u00e4hrt.", "tokens": ["Die", "al\u00b7lein", "mein", "Le\u00b7ben", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Traue keinem falschen Schimmer,", "tokens": ["Trau\u00b7e", "kei\u00b7nem", "fal\u00b7schen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Edler Freund! sonst w\u00e4re nimmer", "tokens": ["Ed\u00b7ler", "Freund", "!", "sonst", "w\u00e4\u00b7re", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vala deiner Liebe werth.", "tokens": ["Va\u00b7la", "dei\u00b7ner", "Lie\u00b7be", "werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Ha! es tagt, schon kehrt mit Beute", "tokens": ["Ha", "!", "es", "tagt", ",", "schon", "kehrt", "mit", "Beu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00f6w' und Tiger heim; kein Blick!", "tokens": ["L\u00f6\u00b7w'", "und", "Ti\u00b7ger", "heim", ";", "kein", "Blick", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PTKVZ", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df ich ohne Trostes Zeichen", "tokens": ["Mu\u00df", "ich", "oh\u00b7ne", "Tros\u00b7tes", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heut zum F\u00fcnftenmale weichen,", "tokens": ["Heut", "zum", "F\u00fcnf\u00b7ten\u00b7ma\u00b7le", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber mir h\u00e4ngt Beil und Strick!", "tokens": ["Ue\u00b7ber", "mir", "h\u00e4ngt", "Beil", "und", "Strick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zweifel, die mich wild erf\u00fcllen,", "tokens": ["Zwei\u00b7fel", ",", "die", "mich", "wild", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bange mir die Seel' umh\u00fcllen!", "tokens": ["Ban\u00b7ge", "mir", "die", "Seel'", "um\u00b7h\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wissen will ich mein Geschick.", "tokens": ["Wis\u00b7sen", "will", "ich", "mein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Au\u00dfer sich, gepre\u00dft von bittern", "tokens": ["Au\u00b7\u00dfer", "sich", ",", "ge\u00b7pre\u00dft", "von", "bit\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRF", "$,", "VVPP", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sorgen, b\u00e4umt sich hoch der Mohr,", "tokens": ["Sor\u00b7gen", ",", "b\u00e4umt", "sich", "hoch", "der", "Mohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Drach' sich aufw\u00e4rts drehet,", "tokens": ["Wie", "ein", "Drach'", "sich", "auf\u00b7w\u00e4rts", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwingt er sich am Speer, und stehet", "tokens": ["Schwingt", "er", "sich", "am", "Speer", ",", "und", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Erker \u00fcber'm Thor;", "tokens": ["Auf", "dem", "Er\u00b7ker", "\u00fc\u00b7ber'm", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieh da tritt mit frohem Zittern,", "tokens": ["Sieh", "da", "tritt", "mit", "fro\u00b7hem", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die Sonne nach Gewittern,", "tokens": ["Wie", "die", "Son\u00b7ne", "nach", "Ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vala voller Huld hervor.", "tokens": ["Va\u00b7la", "vol\u00b7ler", "Huld", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Und gleich Tamarindensprossen,", "tokens": ["Und", "gleich", "Ta\u00b7ma\u00b7rin\u00b7den\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgehaucht vom lauen Merz,", "tokens": ["Auf\u00b7ge\u00b7haucht", "vom", "lau\u00b7en", "Merz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Thauen K\u00fcsse, unverlohren", "tokens": ["Thau\u00b7en", "K\u00fcs\u00b7se", ",", "un\u00b7ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt der letzte auf des Mohren", "tokens": ["Bleibt", "der", "letz\u00b7te", "auf", "des", "Moh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hei\u00dfen Mund und gl\u00fchend Herz.", "tokens": ["Hei\u00b7\u00dfen", "Mund", "und", "gl\u00fc\u00b7hend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Geliebte h\u00e4lt umschlossen", "tokens": ["Die", "Ge\u00b7lieb\u00b7te", "h\u00e4lt", "um\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kali, wie aus Erz gegossen,", "tokens": ["Ka\u00b7li", ",", "wie", "aus", "Erz", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lange stumm in frohem Schmerz.", "tokens": ["Lan\u00b7ge", "stumm", "in", "fro\u00b7hem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Schlag auf Schlag, o s\u00fc\u00dfes Beben!", "tokens": ["Schlag", "auf", "Schlag", ",", "o", "s\u00fc\u00b7\u00dfes", "Be\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterne berget euer Licht!", "tokens": ["Ster\u00b7ne", "ber\u00b7get", "eu\u00b7er", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles ist mir unverlohren,", "tokens": ["Al\u00b7les", "ist", "mir", "un\u00b7ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich werde neu gebohren,", "tokens": ["Und", "ich", "wer\u00b7de", "neu", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da mein Herz in Wonne bricht.", "tokens": ["Da", "mein", "Herz", "in", "Won\u00b7ne", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df, o Vala, la\u00df mein Leben", "tokens": ["La\u00df", ",", "o", "Va\u00b7la", ",", "la\u00df", "mein", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "FM", "FM", "$,", "VVIMP", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf der Lippe dir verschweben,", "tokens": ["Auf", "der", "Lip\u00b7pe", "dir", "ver\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sterben kann die Liebe nicht.", "tokens": ["Ster\u00b7ben", "kann", "die", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}