{"textgrid.poem.39521": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Hexameter einst in unendlichen R\u00e4umen des Epos", "tokens": ["Als", "der", "He\u00b7xa\u00b7me\u00b7ter", "einst", "in", "un\u00b7end\u00b7li\u00b7chen", "R\u00e4u\u00b7men", "des", "E\u00b7pos"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Ernst hinwandelnd, umsonst innigen Liebesverein", "tokens": ["Ernst", "hin\u00b7wan\u00b7delnd", ",", "um\u00b7sonst", "in\u00b7ni\u00b7gen", "Lie\u00b7bes\u00b7ver\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "ADV", "ADJA", "NN"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Suchte, da schuf aus eignem Gebl\u00fct ihm ein weibliches Abbild,", "tokens": ["Such\u00b7te", ",", "da", "schuf", "aus", "eig\u00b7nem", "Ge\u00b7bl\u00fct", "ihm", "ein", "weib\u00b7li\u00b7ches", "Ab\u00b7bild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Pentametrea, und ward selber Apoll Paranymph", "tokens": ["Pen\u00b7ta\u00b7me\u00b7trea", ",", "und", "ward", "sel\u00b7ber", "A\u00b7poll", "Par\u00b7a\u00b7nymph"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "VAFIN", "ADV", "NE", "NN"], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ihres unsterblichen Bundes. Ihr sanft anschmiegend Umarmen", "tokens": ["Ih\u00b7res", "uns\u00b7terb\u00b7li\u00b7chen", "Bun\u00b7des", ".", "Ihr", "sanft", "an\u00b7schmie\u00b7gend", "Um\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "ADJD", "ADJD", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Brachte dem Heldengemahl, spielender Genienschaar", "tokens": ["Brach\u00b7te", "dem", "Hel\u00b7den\u00b7ge\u00b7mahl", ",", "spie\u00b7len\u00b7der", "Ge\u00b7nien\u00b7schaar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Aehnlich, so manch anmuthiges Kind, elege\u00efsche Lieder.", "tokens": ["A\u00b7ehn\u00b7lich", ",", "so", "manch", "an\u00b7mut\u00b7hi\u00b7ges", "Kind", ",", "e\u00b7le\u00b7ge\u00ef\u00b7sche", "Lie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "PIAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Er sah l\u00e4chelnd darin ", "tokens": ["Er", "sah", "l\u00e4\u00b7chelnd", "da\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PAV"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "So, freiwillig beschr\u00e4nkt, nachl\u00e4\u00dfigen Gangs, in der Rhythmen", "tokens": ["So", ",", "frei\u00b7wil\u00b7lig", "be\u00b7schr\u00e4nkt", ",", "nach\u00b7l\u00e4\u00b7\u00dfi\u00b7gen", "Gangs", ",", "in", "der", "Rhyth\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "VVPP", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-++--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wellenverschlingungen, voll lieblicher Disharmonie,", "tokens": ["Wel\u00b7len\u00b7ver\u00b7schlin\u00b7gun\u00b7gen", ",", "voll", "lieb\u00b7li\u00b7cher", "Dis\u00b7har\u00b7mo\u00b7nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Welche, sich halb aufl\u00f6send, von neuem das Ohr dann fe\u00dfelnd", "tokens": ["Wel\u00b7che", ",", "sich", "halb", "auf\u00b7l\u00f6\u00b7send", ",", "von", "neu\u00b7em", "das", "Ohr", "dann", "fe\u00b7\u00df\u00b7elnd"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "$,", "PRF", "ADJD", "VVPP", "$,", "APPR", "ADJA", "ART", "NN", "ADV", "ADJD"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.12": {"text": "Sinnigen Zwist ausgleicht, bildeten dich, Elegie,", "tokens": ["Sin\u00b7ni\u00b7gen", "Zwist", "aus\u00b7gleicht", ",", "bil\u00b7de\u00b7ten", "dich", ",", "E\u00b7le\u00b7gie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+--+-++---+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Viel der hellenischen M\u00e4nner, und mancher in Latium, jedes", "tokens": ["Viel", "der", "hel\u00b7le\u00b7ni\u00b7schen", "M\u00e4n\u00b7ner", ",", "und", "man\u00b7cher", "in", "La\u00b7ti\u00b7um", ",", "je\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "KON", "PIS", "APPR", "NE", "$,", "PIAT"], "meter": "--+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Liebebewegten Gem\u00fcths linde Bew\u00e4ltigerin.", "tokens": ["