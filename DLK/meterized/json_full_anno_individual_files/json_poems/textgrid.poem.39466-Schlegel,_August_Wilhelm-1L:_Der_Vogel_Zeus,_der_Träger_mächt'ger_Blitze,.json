{"textgrid.poem.39466": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Vogel Zeus, der Tr\u00e4ger m\u00e4cht'ger Blitze,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vogel Zeus, der Tr\u00e4ger m\u00e4cht'ger Blitze,", "tokens": ["Der", "Vo\u00b7gel", "Zeus", ",", "der", "Tr\u00e4\u00b7ger", "m\u00e4cht'\u00b7ger", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ihn sein F\u00fcrst zum Raub auf Ida sandte,", "tokens": ["Als", "ihn", "sein", "F\u00fcrst", "zum", "Raub", "auf", "I\u00b7da", "sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hielt er den Knaben, der sich zagend wandte,", "tokens": ["Hielt", "er", "den", "Kna\u00b7ben", ",", "der", "sich", "za\u00b7gend", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Behutsam, da\u00df ihn nicht die Klaue ritze.", "tokens": ["Be\u00b7hut\u00b7sam", ",", "da\u00df", "ihn", "nicht", "die", "Klau\u00b7e", "rit\u00b7ze", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch \u00fcber Reiz und Anmuth rollt' in Hitze", "tokens": ["Doch", "\u00fc\u00b7ber", "Reiz", "und", "An\u00b7muth", "rollt'", "in", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Auge hin; auch keinen Ku\u00df entwandte", "tokens": ["Sein", "Au\u00b7ge", "hin", ";", "auch", "kei\u00b7nen", "Ku\u00df", "ent\u00b7wand\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Schnabel, der nur blut'ge Thaten kannte:", "tokens": ["Der", "Schna\u00b7bel", ",", "der", "nur", "blut'\u00b7ge", "Tha\u00b7ten", "kann\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So trug er rasch ihn zum olymp'schen Sitze.", "tokens": ["So", "trug", "er", "rasch", "ihn", "zum", "o\u00b7lym\u00b7p'\u00b7schen", "Sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du aber, holder Schwan, du wei\u00dft die Gaben", "tokens": ["Du", "a\u00b7ber", ",", "hol\u00b7der", "Schwan", ",", "du", "wei\u00dft", "die", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lieb' in hoher Sch\u00f