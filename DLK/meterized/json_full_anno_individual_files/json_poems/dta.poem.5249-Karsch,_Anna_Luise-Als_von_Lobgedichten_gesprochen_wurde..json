{"dta.poem.5249": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als von Lobgedichten gesprochen  \n wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Oft loben uns Dichter, die t\u00e4uschen.", "tokens": ["Oft", "lo\u00b7ben", "uns", "Dich\u00b7ter", ",", "die", "t\u00e4u\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es lebe mein Ramler! Er spricht:", "tokens": ["Es", "le\u00b7be", "mein", "Ram\u00b7ler", "!", "Er", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wenn es Verdienste nicht heischen,", "tokens": ["Wenn", "es", "Ver\u00b7diens\u00b7te", "nicht", "hei\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Lob\u2019 ich selbst K\u00f6nige nicht!", "tokens": ["Lob'", "ich", "selbst", "K\u00f6\u00b7ni\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}