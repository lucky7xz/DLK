{"textgrid.poem.35285": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Welt ist gro\u00df \u2013 ich wei\u00df es zur Gen\u00fcge,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist gro\u00df \u2013 ich wei\u00df es zur Gen\u00fcge,", "tokens": ["Die", "Welt", "ist", "gro\u00df", "\u2013", "ich", "wei\u00df", "es", "zur", "Ge\u00b7n\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe sie durchschritten und durchschwommen;", "tokens": ["Ich", "ha\u00b7be", "sie", "durch\u00b7schrit\u00b7ten", "und", "durch\u00b7schwom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt ist klein \u2013 ich bin zur\u00fcckgekommen", "tokens": ["Die", "Welt", "ist", "klein", "\u2013", "ich", "bin", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lache meiner Argonautenz\u00fcge.", "tokens": ["Und", "la\u00b7che", "mei\u00b7ner", "Ar\u00b7go\u00b7nau\u00b7ten\u00b7z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vergebens griff ich nach dem goldnen Vlie\u00df,", "tokens": ["Ver\u00b7ge\u00b7bens", "griff", "ich", "nach", "dem", "gold\u00b7nen", "Vlie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mir hat bis jetzt kein Lorbeer gr\u00fcnen wollen,", "tokens": ["Mir", "hat", "bis", "jetzt", "kein", "Lor\u00b7beer", "gr\u00fc\u00b7nen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und kindisch schien es mir zu grollen,", "tokens": ["Und", "kin\u00b7disch", "schien", "es", "mir", "zu", "grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als auch die Liebe mich verlie\u00df.", "tokens": ["Als", "auch", "die", "Lie\u00b7be", "mich", "ver\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Langweilig aber fand ich's \u00fcberall", "tokens": ["Lang\u00b7wei\u00b7lig", "a\u00b7ber", "fand", "ich's", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Trotz heitrer Frauen, sch\u00e4umender Pokale,", "tokens": ["Trotz", "hei\u00b7trer", "Frau\u00b7en", ",", "sch\u00e4u\u00b7men\u00b7der", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und fragen darf ich ohne W\u00f6rterschwall:", "tokens": ["Und", "fra\u00b7gen", "darf", "ich", "oh\u00b7ne", "W\u00f6r\u00b7ter\u00b7schwall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wo waren meines Herzens Ideale?", "tokens": ["Wo", "wa\u00b7ren", "mei\u00b7nes", "Her\u00b7zens", "I\u00b7dea\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Mir ging es, wie es stets zu gehen pflegt,", "tokens": ["Mir", "ging", "es", ",", "wie", "es", "stets", "zu", "ge\u00b7hen", "pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wenn Edles, Wahres sich in mir geregt,", "tokens": ["Wenn", "Ed\u00b7les", ",", "Wah\u00b7res", "sich", "in", "mir", "ge\u00b7regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADJA", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Dann haben meine werten Zeitgenossen", "tokens": ["Dann", "ha\u00b7ben", "mei\u00b7ne", "wer\u00b7ten", "Zeit\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mir gleich aufs Herz geschossen;", "tokens": ["Mir", "gleich", "aufs", "Herz", "ge\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Stets ward, was Ehrenhaftes mir passiert,", "tokens": ["Stets", "ward", ",", "was", "Eh\u00b7ren\u00b7haf\u00b7tes", "mir", "pas\u00b7siert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Von meinen G\u00f6nnern vornehm ignoriert,", "tokens": ["Von", "mei\u00b7nen", "G\u00f6n\u00b7nern", "vor\u00b7nehm", "ig\u00b7no\u00b7riert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.19": {"text": "Und wenn mich Ehrgeiz, Thatendurst gepeinigt,", "tokens": ["Und", "wenn", "mich", "Ehr\u00b7geiz", ",", "Tha\u00b7ten\u00b7durst", "ge\u00b7pei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Dann haben gute Freunde mich gesteinigt.", "tokens": ["Dann", "ha\u00b7ben", "gu\u00b7te", "Freun\u00b7de", "mich", "ge\u00b7stei\u00b7nigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "O gro\u00dfe Welt voll kleiner Leidenschaften,", "tokens": ["O", "gro\u00b7\u00dfe", "Welt", "voll", "klei\u00b7ner", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "O kleine Welt voll gro\u00dfer Eitelkeit,", "tokens": ["O", "klei\u00b7ne", "Welt", "voll", "gro\u00b7\u00dfer", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Mit welchem Aerger sah ich weit und breit", "tokens": ["Mit", "wel\u00b7chem", "A\u00b7er\u00b7ger", "sah", "ich", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Den gleichen Staub an unsern Sohlen haften!", "tokens": ["Den", "glei\u00b7chen", "Staub", "an", "un\u00b7sern", "Soh\u00b7len", "haf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Den Neid, den Wankelmut, die Heuchelei,", "tokens": ["Den", "Neid", ",", "den", "Wan\u00b7kel\u00b7mut", ",", "die", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Den Eigend\u00fcnkel, der, auf nichts begr\u00fcndet,", "tokens": ["Den", "Ei\u00b7gen\u00b7d\u00fcn\u00b7kel", ",", "der", ",", "auf", "nichts", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Bei jedem Druck phosphorisch sich entz\u00fcndet,", "tokens": ["Bei", "je\u00b7dem", "Druck", "phos\u00b7pho\u00b7risch", "sich", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Den G\u00f6tzendienst, die Kriecherei.", "tokens": ["Den", "G\u00f6t\u00b7zen\u00b7dienst", ",", "die", "Krie\u00b7che\u00b7rei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Das Schicksal gab mir stete Fingerzeige:", "tokens": ["Das", "Schick\u00b7sal", "gab", "mir", "ste\u00b7te", "Fin\u00b7ger\u00b7zei\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die Menschheit ist nicht schlecht \u2013 nur schwach und feige;", "tokens": ["Die", "Menschheit", "ist", "nicht", "schlecht", "\u2013", "nur", "schwach", "und", "fei\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$(", "ADV", "ADJD", "KON", "ADJA", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Es br\u00fcstet sich und st\u00f6\u00dft vergn\u00fcgt ins Horn,", "tokens": ["Es", "br\u00fcs\u00b7tet", "sich", "und", "st\u00f6\u00dft", "ver\u00b7gn\u00fcgt", "ins", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Wer sich gesichert glaubt auf gr\u00fcnem Zweige,", "tokens": ["Wer", "sich", "ge\u00b7si\u00b7chert", "glaubt", "auf", "gr\u00fc\u00b7nem", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und doch \u2013 zur Trauer ward zuletzt mein Zorn,", "tokens": ["Und", "doch", "\u2013", "zur", "Trau\u00b7er", "ward", "zu\u00b7letzt", "mein", "Zorn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "APPRART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Denn leider bin auch ich vom gleichen Teige,", "tokens": ["Denn", "lei\u00b7der", "bin", "auch", "ich", "vom", "glei\u00b7chen", "Tei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Wei\u00df selbst nicht, was ich m\u00f6chte oder kann;", "tokens": ["Wei\u00df", "selbst", "nicht", ",", "was", "ich", "m\u00f6ch\u00b7te", "o\u00b7der", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Verfehlter Zweck, verk\u00fcmmerter Genu\u00df,", "tokens": ["Ver\u00b7fehl\u00b7ter", "Zweck", ",", "ver\u00b7k\u00fcm\u00b7mer\u00b7ter", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Das war der Anfang, Schw\u00e4che, Ueberdru\u00df", "tokens": ["Das", "war", "der", "An\u00b7fang", ",", "Schw\u00e4\u00b7che", ",", "Ue\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Wird wohl das Ende sein, \u2013 doch wann?", "tokens": ["Wird", "wohl", "das", "En\u00b7de", "sein", ",", "\u2013", "doch", "wann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VAINF", "$,", "$(", "KON", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Noch war ich niemals recht in meinem Gleise;", "tokens": ["Noch", "war", "ich", "nie\u00b7mals", "recht", "in", "mei\u00b7nem", "Glei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Ein jeder denkt und f\u00fchlt nach seiner Weise,", "tokens": ["Ein", "je\u00b7der", "denkt", "und", "f\u00fchlt", "nach", "sei\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Und manchem, dem gleich mir die Arme sanken,", "tokens": ["Und", "man\u00b7chem", ",", "dem", "gleich", "mir", "die", "Ar\u00b7me", "san\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PRELS", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Hat Selbstbedauern alle Schuld verziehn.", "tokens": ["Hat", "Selbst\u00b7be\u00b7dau\u00b7ern", "al\u00b7le", "Schuld", "ver\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Ich m\u00f6chte meinen Gedanken,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "mei\u00b7nen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "N\u00fcchternen, b\u00f6sen Gedanken,", "tokens": ["N\u00fcch\u00b7ter\u00b7nen", ",", "b\u00f6\u00b7sen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.45": {"text": "Ewig, ewig entfliehn.", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "ent\u00b7fliehn", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die Welt ist gro\u00df \u2013 ich wei\u00df es zur Gen\u00fcge,", "tokens": ["Die", "Welt", "ist", "gro\u00df", "\u2013", "ich", "wei\u00df", "es", "zur", "Ge\u00b7n\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe sie durchschritten und durchschwommen;", "tokens": ["Ich", "ha\u00b7be", "sie", "durch\u00b7schrit\u00b7ten", "und", "durch\u00b7schwom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt ist klein \u2013 ich bin zur\u00fcckgekommen", "tokens": ["Die", "Welt", "ist", "klein", "\u2013", "ich", "bin", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "