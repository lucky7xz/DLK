{"dta.poem.552": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er wieder frey geworden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin vergn\u00fcgt;", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Ketten sind entzwey/", "tokens": ["Die", "Ket\u00b7ten", "sind", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Knechtschafft ist vorbey", "tokens": ["Die", "Knecht\u00b7schafft", "ist", "vor\u00b7bey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wodurch ", "tokens": ["Wo\u00b7durch"], "token_info": ["word"], "pos": ["PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Mein Geist der ist genesen/", "tokens": ["Mein", "Geist", "der", "ist", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun ihn nicht mehr besiegt", "tokens": ["Nun", "ihn", "nicht", "mehr", "be\u00b7siegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bellandrens Wiederschein.", "tokens": ["Bel\u00b7land\u00b7rens", "Wie\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Melintes mag ihr Selave seyn/", "tokens": ["Me\u00b7lin\u00b7tes", "mag", "ihr", "Se\u00b7la\u00b7ve", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ich liebe mehr ein gutes Buch", "tokens": ["Ich", "lie\u00b7be", "mehr", "ein", "gu\u00b7tes", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als ein geputztes Weib/", "tokens": ["Als", "ein", "ge\u00b7putz\u00b7tes", "Weib", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wer dieses w\u00e4hlt zum Zeit-Vertreib", "tokens": ["Wer", "die\u00b7ses", "w\u00e4hlt", "zum", "Zeit\u00b7Ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ist wahrlich nicht gantz klug.", "tokens": ["Ist", "wahr\u00b7lich", "nicht", "gantz", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Victoria, nun bin ich frey/", "tokens": ["Vic\u00b7to\u00b7ria", ",", "nun", "bin", "ich", "frey", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Die Ketten sind entzwey", "tokens": ["Die", "Ket\u00b7ten", "sind", "ent\u00b7zwey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Womit ich n\u00e4chst gef\u00e4sselt war", "tokens": ["Wo\u00b7mit", "ich", "n\u00e4chst", "ge\u00b7f\u00e4s\u00b7selt", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Freyheit ist gefunden/", "tokens": ["Die", "Frey\u00b7heit", "ist", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Victoria, das Hertz ist wieder dar.", "tokens": ["Vic\u00b7to\u00b7ria", ",", "das", "Hertz", "ist", "wie\u00b7der", "dar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Mein Geist ist frey", "tokens": ["Mein", "Geist", "ist", "frey"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Die Knechtschafft ist vorbey", "tokens": ["Die", "Knecht\u00b7schafft", "ist", "vor\u00b7bey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Wodurch ich war verbunden", "tokens": ["Wo\u00b7durch", "ich", "war", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Bellandren zu bedienen.", "tokens": ["Bel\u00b7land\u00b7ren", "zu", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Victoria! Victoria!", "tokens": ["Vic\u00b7to\u00b7ria", "!", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "He! lustig! He ", "tokens": ["He", "!", "lus\u00b7tig", "!", "He"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ITJ", "$.", "ADJD", "$.", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Die Freyheit ist erschienen.", "tokens": ["Die", "Frey\u00b7heit", "ist", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Bellandra geh nur hin/", "tokens": ["Bel\u00b7land\u00b7ra", "geh", "nur", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Weil ich kein Knecht mehr bin/", "tokens": ["Weil", "ich", "kein", "Knecht", "mehr", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "So achte ich auch nicht", "tokens": ["So", "ach\u00b7te", "ich", "auch", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Dein schl\u00e4ffriges Gesicht.", "tokens": ["Dein", "schl\u00e4f\u00b7fri\u00b7ges", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Adjeu Bellandra fahre wohl", "tokens": ["Ad\u00b7jeu", "Bel\u00b7land\u00b7ra", "fah\u00b7re", "wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Ich frage nichts nach dir.", "tokens": ["Ich", "fra\u00b7ge", "nichts", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Melintes nenn dich seine Zier/", "tokens": ["Me\u00b7lin\u00b7tes", "nenn", "dich", "sei\u00b7ne", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Er mache dich zu seiner Muhmen/", "tokens": ["Er", "ma\u00b7che", "dich", "zu", "sei\u00b7ner", "Muh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Er schencke dir ", "tokens": ["Er", "schen\u00b7cke", "dir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "Ich wei\u00df wol was ich glauben soll/", "tokens": ["Ich", "wei\u00df", "wol", "was", "ich", "glau\u00b7ben", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Jhn sticht der junge ", "tokens": ["Jhn", "sticht", "der", "jun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Und giebt er gleich so manchen Schmau\u00df", "tokens": ["Und", "giebt", "er", "gleich", "so", "man\u00b7chen", "Schmau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "So ist er doch nur halb geliebt", "tokens": ["So", "ist", "er", "doch", "nur", "halb", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ich wei\u00df wie es die Falsche \u00fcbt.", "tokens": ["Ich", "wei\u00df", "wie", "es", "die", "Fal\u00b7sche", "\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Doch hat er noch ein bessers Gl\u00fcck", "tokens": ["Doch", "hat", "er", "noch", "ein", "bes\u00b7sers", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Als wie ", "tokens": ["Als", "wie"], "token_info": ["word", "word"], "pos": ["KOUS", "KOKOM"], "meter": "-+", "measure": "iambic.single"}, "line.41": {"text": "Denn wenn der ", "tokens": ["Denn", "wenn", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "So steht er oben an.", "tokens": ["So", "steht", "er", "o\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Wenn nun ", "tokens": ["Wenn", "nun"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.44": {"text": "So soll ", "tokens": ["So", "soll"], "token_info": ["word", "word"], "pos": ["ADV", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Der best\u2019 im Korbe wie man schreibt", "tokens": ["Der", "best'", "im", "Kor\u00b7be", "wie", "man", "schreibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "KOKOM", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Wenn keine mehr darinnen/", "tokens": ["Wenn", "kei\u00b7ne", "mehr", "da\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADV", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Allein ich dancke vor die Ehr/", "tokens": ["Al\u00b7lein", "ich", "dan\u00b7cke", "vor", "die", "Ehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und verlange gar nicht mehr", "tokens": ["Und", "ver\u00b7lan\u00b7ge", "gar", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "So theur sie zugewinnen", "tokens": ["So", "theur", "sie", "zu\u00b7ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}