{"textgrid.poem.40902": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch von der langen schwarzen M\u00f6hr herab,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch von der langen schwarzen M\u00f6hr herab,", "tokens": ["Hoch", "von", "der", "lan\u00b7gen", "schwar\u00b7zen", "M\u00f6hr", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vom Platzberg her, auf wohlbekanntem Pfad", "tokens": ["vom", "Platz\u00b7berg", "her", ",", "auf", "wohl\u00b7be\u00b7kann\u00b7tem", "Pfad"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erschein ich dir, o Freund, den Blumenkranz", "tokens": ["er\u00b7schein", "ich", "dir", ",", "o", "Freund", ",", "den", "Blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "$,", "FM", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dir bringend, den ich j\u00fcngst in Wald und Flur", "tokens": ["dir", "brin\u00b7gend", ",", "den", "ich", "j\u00fcngst", "in", "Wald", "und", "Flur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und an der Wiese duftigem Gestad", "tokens": ["und", "an", "der", "Wie\u00b7se", "duf\u00b7ti\u00b7gem", "Ge\u00b7stad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und um die stillen D\u00f6rfer her gepfl\u00fcckt.", "tokens": ["und", "um", "die", "stil\u00b7len", "D\u00f6r\u00b7fer", "her", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zwar nur Gam\u00e4nderlein und Ehrenpreis,", "tokens": ["Zwar", "nur", "Ga\u00b7m\u00e4n\u00b7der\u00b7lein", "und", "Eh\u00b7ren\u00b7preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "nur Erdbeerbl\u00fcten, Dolden, Wohlgemut", "tokens": ["nur", "Erd\u00b7beer\u00b7bl\u00fc\u00b7ten", ",", "Dol\u00b7den", ",", "Wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und zwischendurch ein dunkles Rosmarin,", "tokens": ["und", "zwi\u00b7schen\u00b7durch", "ein", "dunk\u00b7les", "Ros\u00b7ma\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "geringe Gabe! doch so gut sie kann,", "tokens": ["ge\u00b7rin\u00b7ge", "Ga\u00b7be", "!", "doch", "so", "gut", "sie", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "hat l\u00e4chelnd und mit ungezwungener Hand", "tokens": ["hat", "l\u00e4\u00b7chelnd", "und", "mit", "un\u00b7ge\u00b7zwun\u00b7ge\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "des Feldes Muse sie in diesen Kranz", "tokens": ["des", "Fel\u00b7des", "Mu\u00b7se", "sie", "in", "die\u00b7sen", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "gewunden, und der reine Freundessinn,", "tokens": ["ge\u00b7wun\u00b7den", ",", "und", "der", "rei\u00b7ne", "Freun\u00b7des\u00b7sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAPP", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "der dir ihn bietet, sei allein sein Wert.", "tokens": ["der", "dir", "ihn", "bie\u00b7tet", ",", "sei", "al\u00b7lein", "sein", "Wert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und hing er nun hier unterm Spiegel sch\u00f6n,", "tokens": ["Und", "hing", "er", "nun", "hier", "un\u00b7term", "Spie\u00b7gel", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "so schwankt er sch\u00f6ner doch am Lindenast", "tokens": ["so", "schwankt", "er", "sch\u00f6\u00b7ner", "doch", "am", "Lin\u00b7de\u00b7nast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "in freier Weitung, leichter Weste Spiel.", "tokens": ["in", "frei\u00b7er", "Wei\u00b7tung", ",", "leich\u00b7ter", "Wes\u00b7te", "Spiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Dort schwank' er denn!, und sammelt um sich her", "tokens": ["Dort", "schwank'", "er", "denn", "!", ",", "und", "sam\u00b7melt", "um", "sich", "her"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "$,", "KON", "VVFIN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "die Linde unterm Sonntagshimmelblau", "tokens": ["die", "Lin\u00b7de", "un\u00b7term", "Sonn\u00b7tags\u00b7him\u00b7mel\u00b7blau"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "das frohe V\u00f6lklein aus dem nahen Dorf,", "tokens": ["das", "fro\u00b7he", "V\u00f6l\u00b7klein", "aus", "dem", "na\u00b7hen", "Dorf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.21": {"text": "das gute V\u00f6lklein, das dich liebt und ehrt,", "tokens": ["das", "gu\u00b7te", "V\u00f6l\u00b7klein", ",", "das", "dich", "liebt", "und", "ehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "und unter ihnen manchen mir von Blut", "tokens": ["und", "un\u00b7ter", "ih\u00b7nen", "man\u00b7chen", "mir", "von", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "verwandt, und manchen aus der goldnen Zeit", "tokens": ["ver\u00b7wandt", ",", "und", "man\u00b7chen", "aus", "der", "gold\u00b7nen", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "der frohen Kindheit mir noch wert und lieb,", "tokens": ["der", "fro\u00b7hen", "Kind\u00b7heit", "mir", "noch", "wert", "und", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "so teilst du gern des kleinen Spa\u00dfes Freud", "tokens": ["so", "teilst", "du", "gern", "des", "klei\u00b7nen", "Spa\u00b7\u00dfes", "Freud"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "mit ihnen. Seht, zu diesem leichten Strau\u00df,", "tokens": ["mit", "ih\u00b7nen", ".", "Seht", ",", "zu", "die\u00b7sem", "leich\u00b7ten", "Strau\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "VVFIN", "$,", "APPR", "PDAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "so sagst du, sind die besten Bl\u00fcmlein doch", "tokens": ["so", "sagst", "du", ",", "sind", "die", "bes\u00b7ten", "Bl\u00fcm\u00b7lein", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "von unsrer Flur, und unser Eigentum", "tokens": ["von", "uns\u00b7rer", "Flur", ",", "und", "un\u00b7ser", "Ei\u00b7gen\u00b7tum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "mit Recht. \u2013 Jo weger uffem Alzeb\u00fcehl,", "tokens": ["mit", "Recht", ".", "\u2013", "Jo", "we\u00b7ger", "uf\u00b7fem", "Al\u00b7ze\u00b7b\u00fcehl", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "$(", "NE", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "jo weger uffem Maiberg hen sie bl\u00fceiht,", "tokens": ["jo", "we\u00b7ger", "uf\u00b7fem", "Mai\u00b7berg", "hen", "sie", "bl\u00fce\u00b7iht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "und bin i nit im frische Morgetau", "tokens": ["und", "bin", "i", "nit", "im", "fri\u00b7sche", "Mor\u00b7ge\u00b7tau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NE", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "dur d'Matte gstreift, und \u00fcber d'Gr\u00e4be gumpt,", "tokens": ["dur", "d'\u00b7Mat\u00b7te", "gs\u00b7treift", ",", "und", "\u00fc\u00b7ber", "d'\u00b7Gr\u00e4\u00b7be", "gumpt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "KON", "APPR", "PDAT", "NN", "$,"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.33": {"text": "und hani nit ab mengem hoche Berg", "tokens": ["und", "ha\u00b7ni", "nit", "ab", "men\u00b7gem", "ho\u00b7che", "Berg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PTKNEG", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "mit nassen Augen abe gluegt ins Dorf", "tokens": ["mit", "nas\u00b7sen", "Au\u00b7gen", "a\u00b7be", "gluegt", "ins", "Dorf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "und hanich Fried und guti Stunde gw\u00fcnscht.", "tokens": ["und", "ha\u00b7nich", "Fried", "und", "gu\u00b7ti", "Stun\u00b7de", "gw\u00fcnscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "'s isch weger wohr, und glaubsch mer's nit, se frog", "tokens": ["'s", "isch", "we\u00b7ger", "wohr", ",", "und", "glaubsch", "mer's", "nit", ",", "se", "frog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "KON", "ADJD", "PIS", "PTKNEG", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.37": {"text": "de Bammert, mengmal het er mi verscheucht", "tokens": ["de", "Bam\u00b7mert", ",", "meng\u00b7mal", "het", "er", "mi", "ver\u00b7scheucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADV", "VAFIN", "PPER", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "im Habermark und im verh\u00e4ngte Wald.", "tokens": ["im", "Ha\u00b7ber\u00b7mark", "und", "im", "ver\u00b7h\u00e4ng\u00b7te", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Se bschauet denn mi Blumechr\u00e4nzli au", "tokens": ["Se", "bschau\u00b7et", "denn", "mi", "Blu\u00b7mech\u00b7r\u00e4nz\u00b7li", "au"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NE", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "am Lindenast, und 's freut mi, wenn's ich gfallt,", "tokens": ["am", "Lin\u00b7de\u00b7nast", ",", "und", "'s", "freut", "mi", ",", "wenn's", "ich", "gfallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "NE", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "und nehmet so verlieb! Es isch nit viel.", "tokens": ["und", "neh\u00b7met", "so", "ver\u00b7lieb", "!", "Es", "isch", "nit", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$.", "PPER", "ADJD", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hoch von der langen schwarzen M\u00f6hr herab,", "tokens": ["Hoch", "von", "der", "lan\u00b7gen", "schwar\u00b7zen", "M\u00f6hr", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vom Platzberg her, auf wohlbekanntem Pfad", "tokens": ["vom", "Platz\u00b7berg", "her", ",", "auf", "wohl\u00b7be\u00b7kann\u00b7tem", "Pfad"], "token_info": ["word", "word", "word",