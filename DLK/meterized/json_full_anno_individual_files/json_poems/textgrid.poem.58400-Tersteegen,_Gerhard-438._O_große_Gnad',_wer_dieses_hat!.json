{"textgrid.poem.58400": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "438. O gro\u00dfe Gnad', wer dieses hat!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott wohnt in meinem Grund, drum sich mein Alles beugt", "tokens": ["Gott", "wohnt", "in", "mei\u00b7nem", "Grund", ",", "drum", "sich", "mein", "Al\u00b7les", "beugt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PAV", "PRF", "PPOSAT", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stets mit s\u00fc\u00dfer Furcht in seiner Nahheit schweigt;", "tokens": ["Und", "stets", "mit", "s\u00fc\u00b7\u00dfer", "Furcht", "in", "sei\u00b7ner", "Nah\u00b7heit", "schweigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er salbet Seel' und Geist mit innig tiefem Frieden,", "tokens": ["Er", "sal\u00b7bet", "Seel'", "und", "Geist", "mit", "in\u00b7nig", "tie\u00b7fem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So leb' ich willenlos, von allem abgeschieden.", "tokens": ["So", "leb'", "ich", "wil\u00b7len\u00b7los", ",", "von", "al\u00b7lem", "ab\u00b7ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "APP