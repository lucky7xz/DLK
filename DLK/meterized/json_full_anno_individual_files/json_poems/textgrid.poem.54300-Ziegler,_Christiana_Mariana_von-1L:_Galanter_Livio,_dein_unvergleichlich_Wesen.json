{"textgrid.poem.54300": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Galanter Livio, dein unvergleichlich Wesen", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Galanter Livio, dein unvergleichlich Wesen", "tokens": ["Ga\u00b7lan\u00b7ter", "Li\u00b7vio", ",", "dein", "un\u00b7ver\u00b7gleich\u00b7lich", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJD", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "L\u00e4st nach dem Augen-Schein viel angenehmes lesen,", "tokens": ["L\u00e4st", "nach", "dem", "Au\u00b7gen\u00b7Schein", "viel", "an\u00b7ge\u00b7neh\u00b7mes", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PIAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Welt sieht / es ist wahr, das recht mit Wunder an,", "tokens": ["Die", "Welt", "sieht", "/", "es", "ist", "wahr", ",", "das", "recht", "mit", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VAFIN", "ADJD", "$,", "PRELS", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was die so g\u00fctige Natur an dir gethan.", "tokens": ["Was", "die", "so", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", "an", "dir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wen solte nicht der Glantz von deiner Sch\u00f6nheit blenden?", "tokens": ["Wen", "sol\u00b7te", "nicht", "der", "Glantz", "von", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "blen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Nymphen m\u00fcssen dir sogleich ihr Jawort senden,", "tokens": ["Die", "Nym\u00b7phen", "m\u00fcs\u00b7sen", "dir", "sog\u00b7leich", "ihr", "Ja\u00b7wort", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Strahl macht alle Welt bezaubernd und verliebt,", "tokens": ["Dein", "Strahl", "macht", "al\u00b7le", "Welt", "be\u00b7zau\u00b7bernd", "und", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dieweil es weit und breit nicht deines gleichens gibt.", "tokens": ["Die\u00b7weil", "es", "weit", "und", "breit", "nicht", "dei\u00b7nes", "glei\u00b7chens", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "PTKNEG", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man solte dich mit recht in Delphis Tempel setzen,", "tokens": ["Man", "sol\u00b7te", "dich", "mit", "recht", "in", "Del\u00b7phis", "Tem\u00b7pel", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "ADJD", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn da verehrt man auch die stumm- und sch\u00f6nen G\u00f6tzen.", "tokens": ["Denn", "da", "ver\u00b7ehrt", "man", "auch", "die", "stum\u00b7m", "und", "sch\u00f6\u00b7nen", "G\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Jedoch ein geiler Leib schickt sich nicht wohl darein,", "tokens": ["Je\u00b7doch", "ein", "gei\u00b7ler", "Leib", "schickt", "sich", "nicht", "wohl", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PRF", "PTKNEG", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dieweil ein G\u00f6tzen-Bild mu\u00df fromm und z\u00fcchtig seyn.", "tokens": ["Die\u00b7weil", "ein", "G\u00f6t\u00b7zen\u00b7Bild", "mu\u00df", "fromm", "und", "z\u00fcch\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein gantzer Mensch ist in der Lust der Welt ersoffen,", "tokens": ["Dein", "gant\u00b7zer", "Mensch", "ist", "in", "der", "Lust", "der", "Welt", "er\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Sch\u00f6nheit hat den Leib, die Seele nicht, betroffen,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "hat", "den", "Leib", ",", "die", "See\u00b7le", "nicht", ",", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Daher man t\u00e4glich dich mit K\u00f6rben tragen sieht,", "tokens": ["Da\u00b7her", "man", "t\u00e4g\u00b7lich", "dich", "mit", "K\u00f6r\u00b7ben", "tra\u00b7gen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "ADJD", "PRF", "APPR", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil das, was Tugend liebt, vor deinen Schimmer flieht.", "tokens": ["Weil", "das", ",", "was", "Tu\u00b7gend", "liebt", ",", "vor", "dei\u00b7nen", "Schim\u00b7mer", "flieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PWS", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die blosse Sch\u00f6nheit mu\u00df nicht gleich die Frauen r\u00fchren,", "tokens": ["Die", "blos\u00b7se", "Sch\u00f6n\u00b7heit", "mu\u00df", "nicht", "gleich", "die", "Frau\u00b7en", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil wir gar offt Gew\u00fcrm in sch\u00f6nsten Aepfeln sp\u00fchren.", "tokens": ["Weil", "wir", "gar", "offt", "Ge\u00b7w\u00fcrm", "in", "sch\u00f6ns\u00b7ten", "A\u00b7e\u00b7pfeln", "sp\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "--++-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Es nimmt dich jede gleich, denckst du, mit Jauchzen an;", "tokens": ["Es", "nimmt", "dich", "je\u00b7de", "gleich", ",", "denckst", "du", ",", "mit", "Jauch\u00b7zen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADV", "$,", "VVFIN", "PPER", "$,", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nein! Chloris, welche dich nicht um sich leiden kan,", "tokens": ["Nein", "!", "Chlo\u00b7ris", ",", "wel\u00b7che", "dich", "nicht", "um", "sich", "lei\u00b7den", "kan", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wird nun und nimmermehr von deinen Blicken brennen,", "tokens": ["Wird", "nun", "und", "nim\u00b7mer\u00b7mehr", "von", "dei\u00b7nen", "Bli\u00b7cken", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du magst gleich tausendmahl den Tag vor\u00fcber rennen.", "tokens": ["Du", "magst", "gleich", "tau\u00b7send\u00b7mahl", "den", "Tag", "vor\u00b7\u00fc\u00b7ber", "ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie heist dich, da\u00df du nicht darffst da vergebens stehn,", "tokens": ["Sie", "heist", "dich", ",", "da\u00df", "du", "nicht", "darffst", "da", "ver\u00b7ge\u00b7bens", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "PAV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.24": {"text": "Nach deiner Herberge und alten Schlupfloch gehn", "tokens": ["Nach", "dei\u00b7ner", "Her\u00b7ber\u00b7ge", "und", "al\u00b7ten", "Schlupf\u00b7loch", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wo du als Hahn in Korb gedenckst allein zu steigen,", "tokens": ["Wo", "du", "als", "Hahn", "in", "Korb", "ge\u00b7denckst", "al\u00b7lein", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "NN", "APPR", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ob sich gleich heimlich mit viel Neben-Buhler zeigen,", "tokens": ["Ob", "sich", "gleich", "heim\u00b7lich", "mit", "viel", "Ne\u00b7ben\u00b7Buh\u00b7ler", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Di\u00df schadt der Liebe nicht, es l\u00f6scht doch deine Brunst,", "tokens": ["Di\u00df", "schadt", "der", "Lie\u00b7be", "nicht", ",", "es", "l\u00f6scht", "doch", "dei\u00b7ne", "Brunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dein Absehn geht ohndi\u00df auf nichts als geile Gunst.", "tokens": ["Dein", "Ab\u00b7sehn", "geht", "ohn\u00b7di\u00df", "auf", "nichts", "als", "gei\u00b7le", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drum kanst du, rath ich dir, dein B\u00fcndel weiter tragen,", "tokens": ["Drum", "kanst", "du", ",", "rath", "ich", "dir", ",", "dein", "B\u00fcn\u00b7del", "wei\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mein zartes Ohr h\u00f6rt nicht auf unversch\u00e4mtes Klagen.", "tokens": ["Mein", "zar\u00b7tes", "Ohr", "h\u00f6rt", "nicht", "auf", "un\u00b7ver\u00b7sch\u00e4m\u00b7tes", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Man wirfft die Perle nicht in Schweine-Trog hinein;", "tokens": ["Man", "wirfft", "die", "Per\u00b7le", "nicht", "in", "Schwei\u00b7ne\u00b7Trog", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn was ich lieben soll mu\u00df tugendhafftig seyn.", "tokens": ["Denn", "was", "ich", "lie\u00b7ben", "soll", "mu\u00df", "tu\u00b7gend\u00b7haff\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wo sich die Sch\u00f6nheit pflegt nicht mit Verstand zu gatten,", "tokens": ["Wo", "sich", "die", "Sch\u00f6n\u00b7heit", "pflegt", "nicht", "mit", "Ver\u00b7stand", "zu", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da greifft man \u00fcberhaupt nach Wind und leeren Schatten.", "tokens": ["Da", "greifft", "man", "\u00fc\u00b7ber\u00b7haupt", "nach", "Wind", "und", "lee\u00b7ren", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du bist dem K\u00e4fer gleich, der, wie man t\u00e4glich sieht", "tokens": ["Du", "bist", "dem", "K\u00e4\u00b7fer", "gleich", ",", "der", ",", "wie", "man", "t\u00e4g\u00b7lich", "sieht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "$,", "PWAV", "PIS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Von einem Blumen-Beet bald zu dem andern zieht.", "tokens": ["Von", "ei\u00b7nem", "Blu\u00b7men\u00b7Beet", "bald", "zu", "dem", "an\u00b7dern", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dergleichen Buhlern weist man insgemein die Th\u00fcren,", "tokens": ["Derg\u00b7lei\u00b7chen", "Buh\u00b7lern", "weist", "man", "ins\u00b7ge\u00b7mein", "die", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dein sch\u00f6n seyn wird mich nicht so, wie du denckst, verf\u00fchren;", "tokens": ["Dein", "sch\u00f6n", "seyn", "wird", "mich", "nicht", "so", ",", "wie", "du", "denckst", ",", "ver\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAINF", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ja s\u00e4hst du, Livio, so sch\u00f6n, als Paris, aus,", "tokens": ["Ja", "s\u00e4hst", "du", ",", "Li\u00b7vio", ",", "so", "sch\u00f6n", ",", "als", "Pa\u00b7ris", ",", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "$,", "NE", "$,", "ADV", "ADJD", "$,", "KOUS", "NE", "$,", "PTKVZ", 