{"dta.poem.20291": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cupido fa\u00dfte dich vergangen zu gesichte/", "tokens": ["Cu\u00b7pi\u00b7do", "fa\u00df\u00b7te", "dich", "ver\u00b7gan\u00b7gen", "zu", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er nahm den besten pfeil/ und griff den bogen an/", "tokens": ["Er", "nahm", "den", "bes\u00b7ten", "pfeil", "/", "und", "griff", "den", "bo\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "ART", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schaute/ wie er ihn nach deinem hertzen richte/", "tokens": ["Ich", "schau\u00b7te", "/", "wie", "er", "ihn", "nach", "dei\u00b7nem", "hert\u00b7zen", "rich\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sprach: Es ist nunmehr um Flavien gethan.", "tokens": ["Ich", "sprach", ":", "Es", "ist", "nun\u00b7mehr", "um", "Fla\u00b7vi\u00b7en", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als aber dieser schalck genugsam angeschauet", "tokens": ["Als", "a\u00b7ber", "die\u00b7ser", "schalck", "ge\u00b7nug\u00b7sam", "an\u00b7ge\u00b7schau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDAT", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des angesichtes glantz/ so heller ist als tag/", "tokens": ["Des", "an\u00b7ge\u00b7sich\u00b7tes", "glantz", "/", "so", "hel\u00b7ler", "ist", "als", "tag", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "ADJD", "VAFIN", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das haar/ wo ihm das gold ein bergwerck auffgebauet/", "tokens": ["Das", "haar", "/", "wo", "ihm", "das", "gold", "ein", "berg\u00b7werck", "auff\u00b7ge\u00b7bau\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "ART", "NN", "ART", "ADJD", "VVFIN", "$("], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und sonnen-strahlen selbst mit ehren trotzen mag;", "tokens": ["Und", "son\u00b7nen\u00b7strah\u00b7len", "selbst", "mit", "eh\u00b7ren", "trot\u00b7zen", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die sch\u00f6nen zauberin/ die fleischichten rubinen/", "tokens": ["Die", "sch\u00f6\u00b7nen", "zau\u00b7be\u00b7rin", "/", "die", "flei\u00b7schich\u00b7ten", "ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die augen/ wo das vech sich in den schnee gesetzt/", "tokens": ["Die", "au\u00b7gen", "/", "wo", "das", "vech", "sich", "in", "den", "schnee", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die wangen/ welchen selbst Aurora w\u00fcnscht zu dienen/", "tokens": ["Die", "wan\u00b7gen", "/", "wel\u00b7chen", "selbst", "Au\u00b7ro\u00b7ra", "w\u00fcnscht", "zu", "die\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAT", "ADV", "NE", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der hals/ der auch den schwan in seiner pracht verletzt/", "tokens": ["Der", "hals", "/", "der", "auch", "den", "schwan", "in", "sei\u00b7ner", "pracht", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die br\u00fcste/ so den witz in kurtzem k\u00f6nnen blenden/", "tokens": ["Die", "br\u00fcs\u00b7te", "/", "so", "den", "witz", "in", "kurt\u00b7zem", "k\u00f6n\u00b7nen", "blen\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADV", "ART", "NN", "APPR", "ADJA", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die schultern/ so den stuhl der sch\u00f6nheit angericht;", "tokens": ["Die", "schul\u00b7tern", "/", "so", "den", "stuhl", "der", "sch\u00f6n\u00b7heit", "an\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So fiel der bogen ihm aus den geschwinden h\u00e4nden/", "tokens": ["So", "fiel", "der", "bo\u00b7gen", "ihm", "aus", "den", "ge\u00b7schwin\u00b7den", "h\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und sprach: dergleichen pracht f\u00fchrt auch die Venus nicht.", "tokens": ["Und", "sprach", ":", "derg\u00b7lei\u00b7chen", "pracht", "f\u00fchrt", "auch", "die", "Ve\u00b7nus", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PIS", "NN", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er sanck ihr auff den hals mit mehr als tausend k\u00fcssen/", "tokens": ["Er", "san\u00b7ck", "ihr", "auff", "den", "hals", "mit", "mehr", "als", "tau\u00b7send", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PIAT", "KOKOM", "CARD", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Es konte nicht sein mund von ihren lippen gehn/", "tokens": ["Es", "kon\u00b7te", "nicht", "sein", "mund", "von", "ih\u00b7ren", "lip\u00b7pen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er lie\u00df das s\u00fc\u00dfte gifft auff ihre zunge fliessen/", "tokens": ["Er", "lie\u00df", "das", "s\u00fc\u00df\u00b7te", "gifft", "auff", "ih\u00b7re", "zun\u00b7ge", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und in der reinen flut die heisse glut entstehn.", "tokens": ["Und", "in", "der", "rei\u00b7nen", "flut", "die", "heis\u00b7se", "glut", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er blie\u00df ihr in den mund was buhlschafft kan erregen/", "tokens": ["Er", "blie\u00df", "ihr", "in", "den", "mund", "was", "buhl\u00b7schafft", "kan", "er\u00b7re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PWS", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was Amber in sich h\u00e4lt/ und Bisem mit sich f\u00fchrt/", "tokens": ["Was", "Am\u00b7ber", "in", "sich", "h\u00e4lt", "/", "und", "Bi\u00b7sem", "mit", "sich", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PRF", "VVFIN", "$(", "KON", "NE", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was Paphos geben will/ und Cypern denckt zu hegen/", "tokens": ["Was", "Pa\u00b7phos", "ge\u00b7ben", "will", "/", "und", "Cy\u00b7pern", "denckt", "zu", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$(", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was kalte geister regt/ und schlaffe senen r\u00fchrt.", "tokens": ["Was", "kal\u00b7te", "geis\u00b7ter", "regt", "/", "und", "schlaf\u00b7fe", "se\u00b7nen", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$(", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er schwur bey seinem pfeil und seiner mutter br\u00fcsten/", "tokens": ["Er", "schwur", "bey", "sei\u00b7nem", "pfeil", "und", "sei\u00b7ner", "mut\u00b7ter", "br\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der sch\u00f6nen Flavia zu g\u00f6nnen ihre ruh;", "tokens": ["Der", "sch\u00f6\u00b7nen", "Fla\u00b7via", "zu", "g\u00f6n\u00b7nen", "ih\u00b7re", "ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Er sagte: Werd\u2019 ich mich mehr wider diese r\u00fcsten/", "tokens": ["Er", "sag\u00b7te", ":", "Werd'", "ich", "mich", "mehr", "wi\u00b7der", "die\u00b7se", "r\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VAFIN", "PPER", "PRF", "ADV", "APPR", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So schlage Jupiter mit blitz und donner zu!", "tokens": ["So", "schla\u00b7ge", "Ju\u00b7pi\u00b7ter", "mit", "blitz", "und", "don\u00b7ner", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So tadle mich nun nicht/ weil ich dir stets gesaget/", "tokens": ["So", "tad\u00b7le", "mich", "nun", "nicht", "/", "weil", "ich", "dir", "stets", "ge\u00b7sa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df deine k\u00fcsse sind mit anmuth angethan;", "tokens": ["Da\u00df", "dei\u00b7ne", "k\u00fcs\u00b7se", "sind", "mit", "an\u00b7muth", "an\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das/ was mir itzt an dir am meisten mi\u00dfbehaget/", "tokens": ["Das", "/", "was", "mir", "itzt", "an", "dir", "am", "meis\u00b7ten", "mi\u00df\u00b7be\u00b7ha\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "ADV", "APPR", "PPER", "PTKA", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ist dieses/ da\u00df dein geist mich nicht recht lieben kan.", "tokens": ["Ist", "die\u00b7ses", "/", "da\u00df", "dein", "geist", "mich", "nicht", "recht", "lie\u00b7ben", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "$(", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}