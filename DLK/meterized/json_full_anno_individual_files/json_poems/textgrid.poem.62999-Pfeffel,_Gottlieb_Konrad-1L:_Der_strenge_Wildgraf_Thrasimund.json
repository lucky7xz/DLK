"text": "Itzt \u00fcber einen Stock, itzt in das Wasser jagte;", "tokens": ["Itzt", "\u00fc\u00b7ber", "ei\u00b7nen", "Stock", ",", "itzt", "in", "das", "Was\u00b7ser", "jag\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wenn er endlich matt und krank", "tokens": ["Und", "wenn", "er", "end\u00b7lich", "matt", "und", "krank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu seinen F\u00fc\u00dfen niedersank,", "tokens": ["Zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "nie\u00b7der\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu murren oder gar sich zu verkriechen wagte,", "tokens": ["Zu", "mur\u00b7ren", "o\u00b7der", "gar", "sich", "zu", "ver\u00b7krie\u00b7chen", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "ADV", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So ward er aus dem Todesschlaf", "tokens": ["So", "ward", "er", "aus", "dem", "To\u00b7des\u00b7schlaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit hundert Pr\u00fcgeln auferwecket.", "tokens": ["Mit", "hun\u00b7dert", "Pr\u00fc\u00b7geln", "au\u00b7fer\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Einst hatte der erlauchte Graf", "tokens": ["Einst", "hat\u00b7te", "der", "er\u00b7lauch\u00b7te", "Graf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das fromme Thier bis auf das Blut genecket;", "tokens": ["Das", "from\u00b7me", "Thier", "bis", "auf", "das", "Blut", "ge\u00b7ne\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da schluchzte Fr\u00e4ulein Adelgund,", "tokens": ["Da", "schluchz\u00b7te", "Fr\u00e4u\u00b7lein", "A\u00b7del\u00b7gund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zu edel und zu sanft des Henkers Kind zu hei\u00dfen:", "tokens": ["Zu", "e\u00b7del", "und", "zu", "sanft", "des", "Hen\u00b7kers", "Kind", "zu", "hei\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "PTKA", "ADJD", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ach Vater, schlagt den armen Hund", "tokens": ["Ach", "Va\u00b7ter", ",", "schlagt", "den", "ar\u00b7men", "Hund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch nicht so hart! Er wird euch wohl noch bei\u00dfen.", "tokens": ["Doch", "nicht", "so", "hart", "!", "Er", "wird", "euch", "wohl", "noch", "bei\u00b7\