-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht ferner mich ber\u00fccken lie\u00df,", "tokens": ["Nicht", "fer\u00b7ner", "mich", "be\u00b7r\u00fc\u00b7cken", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich andern Sieg sie pfl\u00fccken hie\u00df,", "tokens": ["Ich", "an\u00b7dern", "Sieg", "sie", "pfl\u00fc\u00b7cken", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur mir vom Herzen ferne.", "tokens": ["Nur", "mir", "vom", "Her\u00b7zen", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch blonder Locken Helligkeit,", "tokens": ["Doch", "blon\u00b7der", "Lo\u00b7cken", "Hel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch blauen Aug's Gef\u00e4lligkeit,", "tokens": ["Doch", "blau\u00b7en", "Aug's", "Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schlanker Glieder Schnelligkeit,", "tokens": ["Doch", "schlan\u00b7ker", "Glie\u00b7der", "Schnel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die d\u00e4uchten mir nicht f\u00e4hrlich;", "tokens": ["Die", "d\u00e4uch\u00b7ten", "mir", "nicht", "f\u00e4hr\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Blicke Glut nicht spr\u00fchende,", "tokens": ["Die", "Bli\u00b7cke", "Glut", "nicht", "spr\u00fc\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Die Wangen zart erbl\u00fchende,", "tokens": ["Die", "Wan\u00b7gen", "zart", "er\u00b7bl\u00fc\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Die Lippen feucht ergl\u00fchende,", "tokens": ["Die", "Lip\u00b7pen", "feucht", "er\u00b7gl\u00fc\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Mir schien, sie meinten's ehrlich.", "tokens": ["Mir", "schien", ",", "sie", "mein\u00b7ten's", "ehr\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nicht schmachtende Geduldigung,", "tokens": ["Nicht", "schmach\u00b7ten\u00b7de", "Ge\u00b7dul\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Nicht eifernde Beschuldigung,", "tokens": ["Nicht", "ei\u00b7fern\u00b7de", "Be\u00b7schul\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Nur Scherz und Gru\u00df und Huldigung,", "tokens": ["Nur", "Scherz", "und", "Gru\u00df", "und", "Hul\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Nur Spiel und Lock' und Lippen!", "tokens": ["Nur", "Spiel", "und", "Lock'", "und", "Lip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht dacht ich ach! der Freudige,", "tokens": ["Nicht", "dacht", "ich", "ach", "!", "der", "Freu\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Mutwillig froh Geschmeidige,", "tokens": ["Mut\u00b7wil\u00b7lig", "froh", "Ge\u00b7schmei\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das heimlich bitter leidige,", "tokens": ["Das", "heim\u00b7lich", "bit\u00b7ter", "lei\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verliebte Gift zu nippen.", "tokens": ["Ver\u00b7lieb\u00b7te", "Gift", "zu", "nip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie h\u00f6hnen nun die Br\u00fcder mich,", "tokens": ["Wie", "h\u00f6h\u00b7nen", "nun", "die", "Br\u00fc\u00b7der", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie brennt es durch die Glieder mich,", "tokens": ["Wie", "brennt", "es", "durch", "die", "Glie\u00b7der", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie stralt verderbend wider mich", "tokens": ["Wie", "stralt", "ver\u00b7der\u00b7bend", "wi\u00b7der", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die leichte, schlanke Blonde!", "tokens": ["Die", "leich\u00b7te", ",", "schlan\u00b7ke", "Blon\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So blauer Augen schnelle Glut,", "tokens": ["So", "blau\u00b7er", "Au\u00b7gen", "schnel\u00b7le", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So gelber Locken helle Flut!", "tokens": ["So", "gel\u00b7ber", "Lo\u00b7cken", "hel\u00b7le", "Flut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Braus' auf mein Herz und schwelle Blut,", "tokens": ["Braus'", "auf", "mein", "Herz", "und", "schwel\u00b7le", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "