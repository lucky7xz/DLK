{"textgrid.poem.35644": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7. An einen jungen Britten", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Edler J\u00fcngling des Volks, welchem Gerechtigkeit", "tokens": ["Ed\u00b7ler", "J\u00fcng\u00b7ling", "des", "Volks", ",", "wel\u00b7chem", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PWAT", "NN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den gef\u00fcrchteten Thron nah an den Wolken gab,", "tokens": ["Den", "ge\u00b7f\u00fcrch\u00b7te\u00b7ten", "Thron", "nah", "an", "den", "Wol\u00b7ken", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dessen Ruhm in dem Flug strotzender Segel fern,", "tokens": ["Des\u00b7sen", "Ruhm", "in", "dem", "Flug", "strot\u00b7zen\u00b7der", "Se\u00b7gel", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Laut im Donner der Schlacht ert\u00f6nt.", "tokens": ["Laut", "im", "Don\u00b7ner", "der", "Schlacht", "er\u00b7t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Fortgetrieben vom Sturm hoher Gedanken, kamst,", "tokens": ["Fort\u00b7ge\u00b7trie\u00b7ben", "vom", "Sturm", "ho\u00b7her", "Ge\u