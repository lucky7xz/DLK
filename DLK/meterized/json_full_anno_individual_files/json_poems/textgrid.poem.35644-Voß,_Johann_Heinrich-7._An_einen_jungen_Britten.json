], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "--+--++---+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zu dem wartenden Kreis rosichter M\u00e4dchen, der", "tokens": ["Zu", "dem", "war\u00b7ten\u00b7den", "Kreis", "ro\u00b7sich\u00b7ter", "M\u00e4d\u00b7chen", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "PRELS"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dir wetteifernd die Arme beut!", "tokens": ["Dir", "wett\u00b7ei\u00b7fernd", "die", "Ar\u00b7me", "beut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Und gieb Botschaft von dem, was du gesehen hast;", "tokens": ["Und", "gieb", "Bot\u00b7schaft", "von", "dem", ",", "was", "du", "ge\u00b7se\u00b7hen", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "NN", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Da\u00df noch immer bei uns heimisch die Tugend ist,", "tokens": ["Da\u00df", "noch", "im\u00b7mer", "bei", "uns", "hei\u00b7misch", "die", "Tu\u00b7gend", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPER", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die der Angel euch gab, da\u00df noch die J\u00fcnglinge", "tokens": ["Die", "der", "An\u00b7gel", "euch", "gab", ",", "da\u00df", "noch", "die", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Treue, Keuschheit die M\u00e4dchen ziert;", "tokens": ["Treu\u00b7e", ",", "Keuschheit", "die", "M\u00e4d\u00b7chen", "ziert", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df der heilige Rat unserer Greise noch", "tokens": ["Da\u00df", "der", "hei\u00b7li\u00b7ge", "Rat", "un\u00b7se\u00b7rer", "Grei\u00b7se", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Alte Redlichkeit \u00fcbt, unserer M\u00e4nner Arm", "tokens": ["Al\u00b7te", "Red\u00b7lich\u00b7keit", "\u00fcbt", ",", "un\u00b7se\u00b7rer", "M\u00e4n\u00b7ner", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Noch, mit blitzendem Schwert, Freiheit und Vaterland,", "tokens": ["Noch", ",", "mit", "blit\u00b7zen\u00b7dem", "Schwert", ",", "Frei\u00b7heit", "und", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und den himmlischen Glauben sch\u00fctzt;", "tokens": ["Und", "den", "himm\u00b7li\u00b7schen", "Glau\u00b7ben", "sch\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Da\u00df in jeglicher Kunst unsere Meister gro\u00df,", "tokens": ["Da\u00df", "in", "jeg\u00b7li\u00b7cher", "Kunst", "un\u00b7se\u00b7re", "Meis\u00b7ter", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ohne Prahlen es sind: Oeser und Rode, mit", "tokens": ["Oh\u00b7ne", "Prah\u00b7len", "es", "sind", ":", "O\u00b7e\u00b7ser", "und", "Ro\u00b7de", ",", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "PPER", "VAFIN", "$.", "NN", "KON", "NN", "$,", "APPR"], "meter": "+-+-+-+---+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "K\u00fchnem Pinsel, erschafft; m\u00e4chtig in T\u00f6nen noch", "tokens": ["K\u00fch\u00b7nem", "Pin\u00b7sel", ",", "er\u00b7schafft", ";", "m\u00e4ch\u00b7tig", "in", "T\u00f6\u00b7nen", "noch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$.", "ADJD", "APPR", "NN", "ADV"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mancher H\u00e4ndel das Herz berauscht;", "tokens": ["Man\u00b7cher", "H\u00e4n\u00b7del", "das", "Herz", "be\u00b7rauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Da\u00df nur Tugend, die Glut z\u00e4rtlicher Unschuld nur,", "tokens": ["Da\u00df", "nur", "Tu\u00b7gend", ",", "die", "Glut", "z\u00e4rt\u00b7li\u00b7cher", "Un\u00b7schuld", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "$,", "ART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Unsrer Barden Gesang f\u00fcllet, und Ramlers und", "tokens": ["Uns\u00b7rer", "Bar\u00b7den", "Ge\u00b7sang", "f\u00fcl\u00b7let", ",", "und", "Ram\u00b7lers", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,", "KON", "NN", "KON"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Klopstocks g\u00f6ttlicher Schwung zu den Olympiern", "tokens": ["Klops\u00b7tocks", "g\u00f6tt\u00b7li\u00b7cher", "Schwung", "zu", "den", "O\u00b7lym\u00b7piern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die bezauberten Seelen rei\u00dft!", "tokens": ["Die", "be\u00b7zau\u00b7ber\u00b7ten", "See\u00b7len", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Dann erz\u00e4hl' auch, wie stark Freundschaft bei Deutschen ist,", "tokens": ["Dann", "er\u00b7z\u00e4hl'", "auch", ",", "wie", "stark", "Freund\u00b7schaft", "bei", "Deut\u00b7schen", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "NN", "APPR", "NE", "VAFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie ein thr\u00e4nender Schwarm stumm dich geleitete,", "tokens": ["Wie", "ein", "thr\u00e4\u00b7nen\u00b7der", "Schwarm", "stumm", "dich", "ge\u00b7lei\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "--+--+-