{"dta.poem.18569": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n ihre  G rausamkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "So mu\u00df ich stets in Jammer Schatten stehn!", "tokens": ["So", "mu\u00df", "ich", "stets", "in", "Jam\u00b7mer", "Schat\u00b7ten", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Furcht Angst und Weh best\u00fcrmen meine Seele/", "tokens": ["Furcht", "Angst", "und", "Weh", "be\u00b7st\u00fcr\u00b7men", "mei\u00b7ne", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein stetes Ach! heist mich zur Folter gehn/", "tokens": ["Ein", "ste\u00b7tes", "Ach", "!", "heist", "mich", "zur", "Fol\u00b7ter", "gehn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo ich den Geist mit tausend Martern qu\u00e4le:", "tokens": ["Wo", "ich", "den", "Geist", "mit", "tau\u00b7send", "Mar\u00b7tern", "qu\u00e4\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dennoch will der Ursprung meiner Pein", "tokens": ["Und", "den\u00b7noch", "will", "der", "Ur\u00b7sprung", "mei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Engel seyn.", "tokens": ["Ein", "En\u00b7gel", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ein Engel seyn/ reimt sich zu grausam nicht/", "tokens": ["Ein", "En\u00b7gel", "seyn", "/", "reimt", "sich", "zu", "grau\u00b7sam", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$(", "VVFIN", "PRF", "PTKA", "ADJD", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil sie zum Trost der Menschen sind erschaffen/", "tokens": ["Weil", "sie", "zum", "Trost", "der", "Men\u00b7schen", "sind", "er\u00b7schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie da\u00df dein Mund von keiner Wehmuht spricht?", "tokens": ["Wie", "da\u00df", "dein", "Mund", "von", "kei\u00b7ner", "Weh\u00b7muht", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein sch\u00f6ner Grimm f\u00fchrt allzu strenge Waffen/", "tokens": ["Dein", "sch\u00f6\u00b7ner", "Grimm", "f\u00fchrt", "all\u00b7zu", "stren\u00b7ge", "Waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "VVFIN", "PTKA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und meine Schuld/ da\u00df ich zu straffen sey/", "tokens": ["Und", "mei\u00b7ne", "Schuld", "/", "da\u00df", "ich", "zu", "straf\u00b7fen", "sey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist Lieb' und Treu.", "tokens": ["Ist", "Lieb'", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ist Lieb' und Treu nicht auch der Liebe wehrt?", "tokens": ["Ist", "Lieb'", "und", "Treu", "nicht", "auch", "der", "Lie\u00b7be", "wehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Will deine Brust nur Diamanten gleichen?", "tokens": ["Will", "dei\u00b7ne", "Brust", "nur", "Di\u00b7a\u00b7man\u00b7ten", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Fel\u00df zerspringt/ worauf das Blitzen f\u00e4hrt:", "tokens": ["Ein", "Fel\u00df", "zer\u00b7springt", "/", "wo\u00b7rauf", "das", "Blit\u00b7zen", "f\u00e4hrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jedoch dein Sinn ist gar nicht zu erweichen/", "tokens": ["Je\u00b7doch", "dein", "Sinn", "ist", "gar", "nicht", "zu", "er\u00b7wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum ist allein das Ende meiner Noht", "tokens": ["Drum", "ist", "al\u00b7lein", "das", "En\u00b7de", "mei\u00b7ner", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein harter Tod.", "tokens": ["Ein", "har\u00b7ter", "Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ein harter Tod wird endlich deinen Sinn", "tokens": ["Ein", "har\u00b7ter", "Tod", "wird", "end\u00b7lich", "dei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu sp\u00e4ter Reu und Wehmuht bringen m\u00fcssen.", "tokens": ["Zu", "sp\u00e4\u00b7ter", "Reu", "und", "Weh\u00b7muht", "brin\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "NE", "KON", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nimm denn das Blut zu einem Opffer hin/", "tokens": ["Nimm", "denn", "das", "Blut", "zu", "ei\u00b7nem", "Opf\u00b7fer", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich will das Grab auf dein Befehlen k\u00fcssen:", "tokens": ["Ich", "will", "das", "Grab", "auf", "dein", "Be\u00b7feh\u00b7len", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der gr\u00f6ste Trost ist mir ", "tokens": ["Der", "gr\u00f6s\u00b7te", "Trost", "ist", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich sterbe treu.", "tokens": ["Ich", "ster\u00b7be", "treu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich sterbe treu. Ach Sch\u00f6nste dieser Welt", "tokens": ["Ich", "ster\u00b7be", "treu", ".", "Ach", "Sch\u00f6ns\u00b7te", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "NN", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df meinen Tod doch deine Gnad erwerben!", "tokens": ["La\u00df", "mei\u00b7nen", "Tod", "doch", "dei\u00b7ne", "Gnad", "er\u00b7wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schau wie dein Knecht zu deinen F\u00fcssen f\u00e4llt/", "tokens": ["Schau", "wie", "dein", "Knecht", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch G\u00fctigkeit kan Sch\u00f6nheit nicht verderben.", "tokens": ["Durch", "G\u00fc\u00b7tig\u00b7keit", "kan", "Sch\u00f6n\u00b7heit", "nicht", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach dencke nur ein Hencker mu\u00df allein", "tokens": ["Ach", "den\u00b7cke", "nur", "ein", "Hen\u00b7cker", "mu\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So grausam seyn.", "tokens": ["So", "grau\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}