{"dta.poem.9424": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Die erste Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! sihe mein Liebster/ wie sch\u00f6ne bistu!", "tokens": ["Ach", "!", "si\u00b7he", "mein", "Liebs\u00b7ter", "/", "wie", "sch\u00f6\u00b7ne", "bis\u00b7tu", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPOSAT", "NN", "$(", "KOKOM", "ADJA", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Komm eylend herzu/", "tokens": ["Komm", "ey\u00b7lend", "her\u00b7zu", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es gr\u00fcnet das Bette von stadtlichen sachen/", "tokens": ["Es", "gr\u00fc\u00b7net", "das", "Bet\u00b7te", "von", "stadt\u00b7li\u00b7chen", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Balcken seyn Cedern/ so f\u00fcrstlich dich machen/", "tokens": ["Die", "Bal\u00b7cken", "seyn", "Ce\u00b7dern", "/", "so", "f\u00fcrst\u00b7lich", "dich", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ADV", "ADJD", "PRF", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Cypressen die zieren das Dach/", "tokens": ["Cyp\u00b7res\u00b7sen", "die", "zie\u00b7ren", "das", "Dach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VVFIN", "ART", "NN", "$("], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Dem herrlich gemach!", "tokens": ["Dem", "herr\u00b7lich", "ge\u00b7mach", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}