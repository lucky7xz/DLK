{"textgrid.poem.55114": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "10", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich nun gleich das wei\u00dfe Blatt dir schickte,", "tokens": ["Wenn", "ich", "nun", "gleich", "das", "wei\u00b7\u00dfe", "Blatt", "dir", "schick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anstatt da\u00df ich's mit Lettern erst beschreibe,", "tokens": ["An\u00b7statt", "da\u00df", "ich's", "mit", "Let\u00b7tern", "erst", "be\u00b7schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ausf\u00fclltest du's vielleicht zum Zeitvertreibe", "tokens": ["Aus\u00b7f\u00fcll\u00b7test", "du's", "viel\u00b7leicht", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sendetest's an mich, die Hochbegl\u00fcckte.", "tokens": ["Und", "sen\u00b7de\u00b7test's", "an", "mich", ",", "die", "Hoch\u00b7be\u00b7gl\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich den blauen Umschlag dann erblickte,", "tokens": ["Wenn", "ich", "den", "blau\u00b7en", "Um\u00b7schlag", "dann", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Neugierig schnell, wie es geziemt dem Weibe,", "tokens": ["Neu\u00b7gie\u00b7rig", "schnell", ",", "wie", "es", "ge\u00b7ziemt", "dem", "Wei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Riss' ich ihn auf, da\u00df nichts verborgen bleibe;", "tokens": ["Riss'", "ich", "ihn", "auf", ",", "da\u00df", "nichts", "ver\u00b7bor\u00b7gen", "blei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "PTKVZ", "$,", "KOUS", "PIS", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da l\u00e4s ich, was mich m\u00fcndlich sonst entz\u00fcckte:", "tokens": ["Da", "l\u00e4s", "ich", ",", "was", "mich", "m\u00fcnd\u00b7lich", "sonst", "ent\u00b7z\u00fcck\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie du so freundlich meine Sehnsucht stilltest", "tokens": ["Wie", "du", "so", "freund\u00b7lich", "mei\u00b7ne", "Sehn\u00b7sucht", "still\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit s\u00fc\u00dfem Wort und mich so ganz verw\u00f6hntest.", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Wort", "und", "mich", "so", "ganz", "ver\u00b7w\u00f6hn\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sogar dein Lispeln glaubt ich auch zu lesen,", "tokens": ["So\u00b7gar", "dein", "Lis\u00b7peln", "glaubt", "ich", "auch", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Womit du liebend meine Seele f\u00fclltest", "tokens": ["Wo\u00b7mit", "du", "lie\u00b7bend", "mei\u00b7ne", "See\u00b7le", "f\u00fcll\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mich auf ewig vor mir selbst versch\u00f6ntest.", "tokens": ["Und", "mich", "auf", "e\u00b7wig", "vor", "mir", "selbst", "ver\u00b7sch\u00f6n\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJD", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn ich nun gleich das wei\u00dfe Blatt dir schickte,", "tokens": ["Wenn", "ich", "nun", "gleich", "das", "wei\u00b7\u00dfe", "Blatt", "dir", "schick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anstatt da\u00df ich's mit Lettern erst beschreibe,", "tokens": ["An\u00b7statt", "da\u00df", "ich's", "mit", "Let\u00b7tern", "