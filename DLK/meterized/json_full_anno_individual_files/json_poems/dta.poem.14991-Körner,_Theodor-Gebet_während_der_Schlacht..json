{"dta.poem.14991": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gebet w\u00e4hrend der Schlacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcllend umw\u00f6lkt mich der Dampf der                                 Gesch\u00fctze,", "tokens": ["Br\u00fcl\u00b7lend", "um\u00b7w\u00f6lkt", "mich", "der", "Dampf", "der", "Ge\u00b7sch\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Spr\u00fchend umzucken mich rasselnde Blitze.", "tokens": ["Spr\u00fc\u00b7hend", "um\u00b7zu\u00b7cken", "mich", "ras\u00b7seln\u00b7de", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Lenker der Schlachten, ich rufe dich!", "tokens": ["Len\u00b7ker", "der", "Schlach\u00b7ten", ",", "ich", "ru\u00b7fe", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vater du, f\u00fchre mich!", "tokens": ["Va\u00b7ter", "du", ",", "f\u00fch\u00b7re", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Vater du, f\u00fchre mich!", "tokens": ["Va\u00b7ter", "du", ",", "f\u00fch\u00b7re", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "F\u00fchr' mich zum Sieg, f\u00fchr mich zum Tode,", "tokens": ["F\u00fchr'", "mich", "zum", "Sieg", ",", "f\u00fchr", "mich", "zum", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Herr, ich erkenne deine Gebote,", "tokens": ["Herr", ",", "ich", "er\u00b7ken\u00b7ne", "dei\u00b7ne", "Ge\u00b7bo\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Herr, wie du willst, so f\u00fchre mich,", "tokens": ["Herr", ",", "wie", "du", "willst", ",", "so", "f\u00fch\u00b7re", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott, ich erkenne dich!", "tokens": ["Gott", ",", "ich", "er\u00b7ken\u00b7ne", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gott, ich erkenne dich!", "tokens": ["Gott", ",", "ich", "er\u00b7ken\u00b7ne", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So im herbstlichen Rauschen der Bl\u00e4tter", "tokens": ["So", "im", "herbst\u00b7li\u00b7chen", "Rau\u00b7schen", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Als im Schlachtendonnerwetter", "tokens": ["Als", "im", "Schlach\u00b7ten\u00b7don\u00b7ner\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Urquell der Gnade, erkenn' ich dich.", "tokens": ["Ur\u00b7quell", "der", "Gna\u00b7de", ",", "er\u00b7kenn'", "ich", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Vater du, segne mich!", "tokens": ["Va\u00b7ter", "du", ",", "seg\u00b7ne", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Vater du, segne mich!", "tokens": ["Va\u00b7ter", "du", ",", "seg\u00b7ne", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "In deine Hand befehl' ich mein Leben,", "tokens": ["In", "dei\u00b7ne", "Hand", "be\u00b7fehl'", "ich", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du kannst es nehmen, du hast es gegeben,", "tokens": ["Du", "kannst", "es", "neh\u00b7men", ",", "du", "hast", "es", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Leben, zum Sterben segne mich.", "tokens": ["Zum", "Le\u00b7ben", ",", "zum", "Ster\u00b7ben", "seg\u00b7ne", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vater, ich preise dich!", "tokens": ["Va\u00b7ter", ",", "ich", "prei\u00b7se", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Vater, ich preise dich!", "tokens": ["Va\u00b7ter", ",", "ich", "prei\u00b7se", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "S'ist ja kein Kampf f\u00fcr die G\u00fcter der Erde;", "tokens": ["S'\u00b7ist", "ja", "kein", "Kampf", "f\u00fcr", "die", "G\u00fc\u00b7ter", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das Heiligste sch\u00fctzen wir mit dem Schwerdte,", "tokens": ["Das", "Hei\u00b7ligs\u00b7te", "sch\u00fct\u00b7zen", "wir", "mit", "dem", "Schwerd\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Drum fallend und siegend preis' ich dich,", "tokens": ["Drum", "fal\u00b7lend", "und", "sie\u00b7gend", "preis'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "KON", "ADJD", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gott, dir ergeb' ich mich!", "tokens": ["Gott", ",", "dir", "er\u00b7geb'", "ich", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gott, dir ergeb' ich mich!", "tokens": ["Gott", ",", "dir", "er\u00b7geb'", "ich", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn mich die Donner des Todes begr\u00fc\u00dfen,", "tokens": ["Wenn", "mich", "die", "Don\u00b7ner", "des", "To\u00b7des", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn meine Adern ge\u00f6ffnet flie\u00dfen,", "tokens": ["Wenn", "mei\u00b7ne", "A\u00b7dern", "ge\u00b7\u00f6ff\u00b7net", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dir, mein Gott, dir ergeb' ich mich!", "tokens": ["Dir", ",", "mein", "Gott", ",", "dir", "er\u00b7geb'", "ich", "mich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vater, ich rufe dich!", "tokens": ["Va\u00b7ter", ",", "ich", "ru\u00b7fe", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}