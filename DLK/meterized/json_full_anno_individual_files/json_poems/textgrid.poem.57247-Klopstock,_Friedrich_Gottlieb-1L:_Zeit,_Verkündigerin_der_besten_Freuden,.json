{"textgrid.poem.57247": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zeit, Verk\u00fcndigerin der besten Freuden,", "genre": "verse", "period": "N.A.", "pub_year": 1753, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeit, Verk\u00fcndigerin der besten Freuden,", "tokens": ["Zeit", ",", "Ver\u00b7k\u00fcn\u00b7di\u00b7ge\u00b7rin", "der", "bes\u00b7ten", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nahe selige Zeit, dich in der Ferne", "tokens": ["Na\u00b7he", "se\u00b7li\u00b7ge", "Zeit", ",", "dich", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "PRF", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Auszuforschen, vergoss ich", "tokens": ["Aus\u00b7zu\u00b7for\u00b7schen", ",", "ver\u00b7goss", "ich"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Tr\u00fcbender Thr\u00e4nen zu viel", "tokens": ["Tr\u00fc\u00b7ben\u00b7der", "Thr\u00e4\u00b7nen", "zu", "viel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PIAT"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Und doch komst du! O dich, ja Engel senden,", "tokens": ["Und", "doch", "komst", "du", "!", "O", "dich", ",", "ja", "En\u00b7gel", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "NE", "PPER", "$,", "ADV", "NN", "VVINF", "$,"], "