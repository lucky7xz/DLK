{"textgrid.poem.61309": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Einsame", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dreimal unselig Weib! Du warst einst sch\u00f6n und jung,", "tokens": ["Drei\u00b7mal", "un\u00b7se\u00b7lig", "Weib", "!", "Du", "warst", "einst", "sch\u00f6n", "und", "jung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gefl\u00fcgelt war dein Geist zu wundervollem Schwung;", "tokens": ["Ge\u00b7fl\u00fc\u00b7gelt", "war", "dein", "Geist", "zu", "wun\u00b7der\u00b7vol\u00b7lem", "Schwung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wie bei lautem Lied von selbst die Saiten t\u00f6nen,", "tokens": ["Und", "wie", "bei", "lau\u00b7tem", "Lied", "von", "selbst", "die", "Sai\u00b7ten", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "APPR", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Klang dir im Herzen nach ein Echo alles Sch\u00f6nen.", "tokens": ["Klang", "dir", "im", "Her\u00b7zen", "nach", "ein", "E\u00b7cho", "al\u00b7les", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Doch ach, du kostetest, niemals bedacht zu ruhn,", "tokens": ["Doch", "ach", ",", "du", "kos\u00b7te\u00b7test", ",", "nie\u00b7mals", "be\u00b7dacht", "zu", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "$,", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Von jeglichem Gef\u00fchl nur, wie die Bienen tun;", "tokens": ["Von", "jeg\u00b7li\u00b7chem", "Ge\u00b7f\u00fchl", "nur", ",", "wie", "die", "Bie\u00b7nen", "tun", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gleichwie durch Schlangenblick ans Neue stets gebunden,", "tokens": ["Gleich\u00b7wie", "durch", "Schlan\u00b7gen\u00b7blick", "ans", "Neu\u00b7e", "stets", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPRART", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des Trunks, der dich gereizt, schon satt nach wenig Stunden,", "tokens": ["Des", "Trunks", ",", "der", "dich", "ge\u00b7reizt", ",", "schon", "satt", "nach", "we\u00b7nig", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "$,", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zogst du, dem Augenblick als Sklavin untertan,", "tokens": ["Zogst", "du", ",", "dem", "Au\u00b7gen\u00b7blick", "als", "Skla\u00b7vin", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit jedem frischen Kleid ein frisch Verlangen an", "tokens": ["Mit", "je\u00b7dem", "fri\u00b7schen", "Kleid", "ein", "frisch", "Ver\u00b7lan\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "ADJD", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und schw\u00e4rmtest, sanft gewiegt in deiner Sch\u00f6nheit Ruhme,", "tokens": ["Und", "schw\u00e4rm\u00b7test", ",", "sanft", "ge\u00b7wiegt", "in", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von Sieg zu Sieg dahin, von Blume hin zu Blume,", "tokens": ["Von", "Sieg", "zu", "Sieg", "da\u00b7hin", ",", "von", "Blu\u00b7me", "hin", "zu", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "PAV", "$,", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als sei f\u00fcr immerdar dir zum Genu\u00df bereit", "tokens": ["Als", "sei", "f\u00fcr", "im\u00b7mer\u00b7dar", "dir", "zum", "Ge\u00b7nu\u00df", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "ADV", "PPER", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Erd' ein Rosenwald, die Jugend Ewigkeit.", "tokens": ["Die", "Erd'", "ein", "Ro\u00b7sen\u00b7wald", ",", "die", "Ju\u00b7gend", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch jeder Lustpokal hat seine Hef' am Grunde,", "tokens": ["Doch", "je\u00b7der", "Lust\u00b7po\u00b7kal", "hat", "sei\u00b7ne", "He\u00b7f'", "am", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es folgt dem Nachtbankett die tr\u00fcbe Zwielichtstunde;", "tokens": ["Es", "folgt", "dem", "Nacht\u00b7ban\u00b7kett", "die", "tr\u00fc\u00b7be", "Zwie\u00b7lichts\u00b7tun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kam auch dir der Tag, wo pl\u00f6tzlich unterm Spiel", "tokens": ["So", "kam", "auch", "dir", "der", "Tag", ",", "wo", "pl\u00f6tz\u00b7lich", "un\u00b7term", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "$,", "PWAV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus deinem Lockenhaar der Anmut Perle fiel,", "tokens": ["Aus", "dei\u00b7nem", "Lo\u00b7cken\u00b7haar", "der", "An\u00b7mut", "Per\u00b7le", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo all dein spr\u00fchnder Witz nicht mehr verhehlen konnte,", "tokens": ["Wo", "all", "dein", "spr\u00fchn\u00b7der", "Witz", "nicht", "mehr", "ver\u00b7heh\u00b7len", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Sonne neige sich an deinem Horizonte,", "tokens": ["Die", "Son\u00b7ne", "nei\u00b7ge", "sich", "an", "dei\u00b7nem", "Ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und durch des bunten Fests Musik sich abendlich", "tokens": ["Und", "durch", "des", "bun\u00b7ten", "Fests", "Mu\u00b7sik", "sich", "a\u00b7bend\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein fr\u00f6stelnd Ruhbegehr in deine Seele schlich.", "tokens": ["Ein", "fr\u00f6s\u00b7telnd", "Ruh\u00b7be\u00b7gehr", "in", "dei\u00b7ne", "See\u00b7le", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da sahst du um; doch ach, du trafst auf allen Z\u00fcgen", "tokens": ["Da", "sahst", "du", "um", ";", "doch", "ach", ",", "du", "trafst", "auf", "al\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "ADV", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Des Mitleids L\u00e4cheln nur, des Hohns verhaltne R\u00fcgen;", "tokens": ["Des", "Mit\u00b7leids", "L\u00e4\u00b7cheln", "nur", ",", "des", "Hohns", "ver\u00b7halt\u00b7ne", "R\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn keiner stand im Kreis, den lieblos nicht bis jetzt", "tokens": ["Denn", "kei\u00b7ner", "stand", "im", "Kreis", ",", "den", "lieb\u00b7los", "nicht", "bis", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "$,", "PRELS", "ADJD", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein scharfer Spott gekr\u00e4nkt, dein Wankelmut verletzt.", "tokens": ["Dein", "schar\u00b7fer", "Spott", "ge\u00b7kr\u00e4nkt", ",", "dein", "Wan\u00b7kel\u00b7mut", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du aber, allzu stolz und allzu schwach zur Bitte,", "tokens": ["Du", "a\u00b7ber", ",", "all\u00b7zu", "stolz", "und", "all\u00b7zu", "schwach", "zur", "Bit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schrittst \u2013 Frohsinn auf der Stirn \u2013 verst\u00f6rt aus ihrer Mitte;", "tokens": ["Schrittst", "\u2013", "Froh\u00b7sinn", "auf", "der", "Stirn", "\u2013", "ver\u00b7st\u00f6rt", "aus", "ih\u00b7rer", "Mit\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "APPR", "ART", "NN", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du wolltest selbst genug dir sein in deinem Sinn", "tokens": ["Du", "woll\u00b7test", "selbst", "ge\u00b7nug", "dir", "sein", "in", "dei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "PPOSAT", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und schlossest zu dein Herz. Doch \u00f6de war es drin.", "tokens": ["Und", "schlos\u00b7sest", "zu", "dein", "Herz", ".", "Doch", "\u00f6\u00b7de", "war", "es", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "KON", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O h\u00e4ttest damals du erkannt: Es waltet stille", "tokens": ["O", "h\u00e4t\u00b7test", "da\u00b7mals", "du", "er\u00b7kannt", ":", "Es", "wal\u00b7tet", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "PPER", "VVPP", "$.", "PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach ewigem Gesetz durchs All ein heil'ger Wille,", "tokens": ["Nach", "e\u00b7wi\u00b7gem", "Ge\u00b7setz", "durchs", "All", "ein", "heil'\u00b7ger", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Schlag auf Schlag den Trotz zerbricht, bis da\u00df er schweigt,", "tokens": ["Der", "Schlag", "auf", "Schlag", "den", "Trotz", "zer\u00b7bricht", ",", "bis", "da\u00df", "er", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch jede Stirn erquickt, die sich in Demut neigt:", "tokens": ["Doch", "je\u00b7de", "Stirn", "er\u00b7quickt", ",", "die", "sich", "in", "De\u00b7mut", "neigt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht, es w\u00e4re dir, der Weinenden, zum Frommen", "tokens": ["Viel\u00b7leicht", ",", "es", "w\u00e4\u00b7re", "dir", ",", "der", "Wei\u00b7nen\u00b7den", ",", "zum", "From\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach k\u00fchler Sommerszeit ein milder Herbst gekommen \u2013", "tokens": ["Nach", "k\u00fch\u00b7ler", "Som\u00b7mers\u00b7zeit", "ein", "mil\u00b7der", "Herbst", "ge\u00b7kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du aber dachtest nicht an S\u00fchnung, tiefverg\u00e4llt,", "tokens": ["Du", "a\u00b7ber", "dach\u00b7test", "nicht", "an", "S\u00fch\u00b7nung", ",", "tief\u00b7ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und grolltest, statt mit dir, mit Gott und mit der Welt.", "tokens": ["Und", "groll\u00b7test", ",", "statt", "mit", "dir", ",", "mit", "Gott", "und", "mit", "der", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUI", "APPR", "PPER", "$,", "APPR", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und dennoch hofftest du. Du wolltest, aus der Frauen", "tokens": ["Und", "den\u00b7noch", "hoff\u00b7test", "du", ".", "Du", "woll\u00b7test", ",", "aus", "der", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gebiet dich fl\u00fcchtend, k\u00fchn ein neu Geschick dir bauen;", "tokens": ["Ge\u00b7biet", "dich", "fl\u00fcch\u00b7tend", ",", "k\u00fchn", "ein", "neu", "Ge\u00b7schick", "dir", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", 