", ",", "die", "Tr\u00e4u\u00b7me", "neu\u00b7er", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die du wie Kinder stolz gen\u00e4hrt an deiner Brust,", "tokens": ["Die", "du", "wie", "Kin\u00b7der", "stolz", "ge\u00b7n\u00e4hrt", "an", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "NN", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du sahst sie Haupt f\u00fcr Haupt mit bittern Tr\u00e4nenfluten", "tokens": ["Du", "sahst", "sie", "Haupt", "f\u00fcr", "Haupt", "mit", "bit\u00b7tern", "Tr\u00e4\u00b7nen\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Vom scharfen Pfeil durchbohrt zu F\u00fc\u00dfen dir verbluten,", "tokens": ["Vom", "schar\u00b7fen", "Pfeil", "durch\u00b7bohrt", "zu", "F\u00fc\u00b7\u00dfen", "dir", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bis du, unselig Weib, zuletzt in deinem Weh", "tokens": ["Bis", "du", ",", "un\u00b7se\u00b7lig", "Weib", ",", "zu\u00b7letzt", "in", "dei\u00b7nem", "Weh"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "--+--