"], "pos": ["PPER", "ADV", "$,", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schrittst \u2013 Frohsinn auf der Stirn \u2013 verst\u00f6rt aus ihrer Mitte;", "tokens": ["Schrittst", "\u2013", "Froh\u00b7sinn", "auf", "der", "Stirn", "\u2013", "ver\u00b7st\u00f6rt", "aus", "ih\u00b7rer", "Mit\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "APPR", "ART", "NN", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du wolltest selbst genug dir sein in deinem Sinn", "tokens": ["Du", "woll\u00b7test", "selbst", "ge\u00b7nug", "dir", "sein", "in", "dei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "PPOSAT", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und schlossest zu dein Herz. Doch \u00f6de war es drin.", "tokens": ["Und", "schlos\u00b7sest", "zu", "dein", "Herz", ".", "Doch", "\u00f6\u00b7de", "war", "es", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "KON", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O h\u00e4ttest damals du erkannt: Es waltet stille", "tokens": ["O", "h\u00e4t\u00b7test", "da\u00b7mals", "du", "er\u00b7kannt", ":", "Es", "wal\u00b7tet", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "PPER", "VVPP", "$.", "PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach ewigem Gesetz durchs All ein heil'ger Wille,", "tokens": ["Nach", "e\u00b7wi\u00b7gem", "Ge\u00b7setz", "durchs", "All", "ein", "heil'\u00b7ger", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Schlag auf Schlag den Trotz zerbricht, bis da\u00df er schweigt,", "tokens": ["Der", "Schlag", "auf", "Schlag", "den", "Trotz", "zer\u00b7bricht", ",", "bis", "da\u00df", "er", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch jede Stirn erquickt, die sich in Demut neigt:", "tokens": ["Doch", "je\u00b7de", "Stirn", "er\u00b7quickt", ",", "die", "sich", "in", "De\u00b7mut", "neigt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht, es w\u00e4re dir, der Weinenden, zum Frommen", "tokens": ["Viel\u00b7leicht", ",", "es", "w\u00e4\u00b7re", "dir", ",", "der", "Wei\u00b7nen\u00b7den", ",", "zum", "From\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach k\u00fchler Sommerszeit ein milder Herbst gekommen \u2013", "tokens": ["Nach", "k\u00fch\u00b7ler", "Som\u00b7mers\u00b7zeit", "ein", "mil\u00b7der", "Herbst", "ge\u00b7kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du aber dachtest nicht an S\u00fchnung, tiefverg\u00e4llt,", "tokens": ["Du", "a\u00b7ber", "dach\u00b7test", "nicht", "an", "S\u00fch\u00b7nung", ",", "tief\u00b7ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und grolltest, statt mit dir, mit Gott und mit der Welt.", "tokens": ["Und", "groll\u00b7test", ",", "statt", "mit", "dir", ",", "mit", "Gott", "und", "mit", "der", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUI", "APPR", "PPER", "$,", "APPR", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und dennoch hofftest du. Du wolltest, aus der Frauen", "tokens": ["Und", "den\u00b7noch", "hoff\u00b7test", "du", ".", "Du", "woll\u00b7test", ",", "aus", "der", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gebiet dich fl\u00fcchtend, k\u00fchn ein neu Geschick dir bauen;", "tokens": ["Ge\u00b7biet", "dich", "fl\u00fcch\u00b7tend", ",", "k\u00fchn", "ein", "neu", "Ge\u00b7schick", "dir", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "w