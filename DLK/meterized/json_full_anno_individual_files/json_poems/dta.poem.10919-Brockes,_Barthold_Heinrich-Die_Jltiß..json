{"dta.poem.10919": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Jlti\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ordonanz in diesem \u00f6den und recht verwildertem\nGefilde,", "tokens": ["Die", "Or\u00b7do\u00b7nanz", "in", "die\u00b7sem", "\u00f6\u00b7den", "und", "recht", "ver\u00b7wil\u00b7der\u00b7tem", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ist w\u00fcrdig, da\u00df man sie betrachtet; es zeigt von dem erstorb-", "tokens": ["Ist", "w\u00fcr\u00b7dig", ",", "da\u00df", "man", "sie", "be\u00b7trach\u00b7tet", ";", "es", "zeigt", "von", "dem", "er\u00b7stor\u00b7b"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PIS", "PPER", "VVPP", "$.", "PPER", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ein d\u00fcrrer Ast sich auf dem Stamm des abgehaunen Stamms,", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Ast", "sich", "auf", "dem", "Stamm", "des", "ab\u00b7ge\u00b7hau\u00b7nen", "Stamms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "So deutlich, da\u00df man, durch den Schatten, den Sonnenschein", "tokens": ["So", "deut\u00b7lich", ",", "da\u00df", "man", ",", "durch", "den", "Schat\u00b7ten", ",", "den", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "$,", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wodurch zugleich sich, im Verschie\u00df, die Fluth und das Ge-", "tokens": ["Wo\u00b7durch", "zu\u00b7gleich", "sich", ",", "im", "Ver\u00b7schie\u00df", ",", "die", "Fluth", "und", "das", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PRF", "$,", "APPRART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch halt! was regt sich auf dem Vorgrund? ich seh hier", "tokens": ["Doch", "halt", "!", "was", "regt", "sich", "auf", "dem", "Vor\u00b7grund", "?", "ich", "seh", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Blutgierig Eyerdiebe w\u00fchlen; man kann in ihren Augen schier", "tokens": ["Blut\u00b7gie\u00b7rig", "Ey\u00b7er\u00b7die\u00b7be", "w\u00fch\u00b7len", ";", "man", "kann", "in", "ih\u00b7ren", "Au\u00b7gen", "schier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "$.", "PIS", "VMFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Den schwarzen kleinen Mord-Geist sehn. Sie w\u00fcrgen mehr, als", "tokens": ["Den", "schwar\u00b7zen", "klei\u00b7nen", "Mord\u00b7Geist", "sehn", ".", "Sie", "w\u00fcr\u00b7gen", "mehr", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$.", "PPER", "VVFIN", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Und saugen, aus zerbi\u00dfnen K\u00f6pfen der Tauben und der H\u00fcner,", "tokens": ["Und", "sau\u00b7gen", ",", "aus", "zer\u00b7bi\u00df\u00b7nen", "K\u00f6p\u00b7fen", "der", "Tau\u00b7ben", "und", "der", "H\u00fc\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Doch, ausser da\u00df sie unsre H\u00e4user von M\u00e4usen und von Ratzen", "tokens": ["Doch", ",", "aus\u00b7ser", "da\u00df", "sie", "uns\u00b7re", "H\u00e4u\u00b7ser", "von", "M\u00e4u\u00b7sen", "und", "von", "Rat\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "So kommen diese Thier uns Menschen auf andre Weise noch", "tokens": ["So", "kom\u00b7men", "die\u00b7se", "Thier", "uns", "Men\u00b7schen", "auf", "and\u00b7re", "Wei\u00b7se", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "PPER", "NN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Indem sie gar nicht kostbar fallen: Hat Gott auch Armen", "tokens": ["In\u00b7dem", "sie", "gar", "nicht", "kost\u00b7bar", "fal\u00b7len", ":", "Hat", "Gott", "auch", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$.", "VAFIN", "NN", "ADV", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Da\u00df sie, mit dieser Thiere B\u00e4lge, in scharfem Frost, sich w\u00e4r-", "tokens": ["Da\u00df", "sie", ",", "mit", "die\u00b7ser", "Thie\u00b7re", "B\u00e4l\u00b7ge", ",", "in", "schar\u00b7fem", "Frost", ",", "sich", "w\u00e4r"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PDAT", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$,", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}