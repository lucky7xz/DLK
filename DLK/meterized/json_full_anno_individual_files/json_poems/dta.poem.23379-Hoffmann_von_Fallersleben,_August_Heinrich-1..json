{"dta.poem.23379": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Teufel schuf die Pre\u00dffreiheit,", "tokens": ["Der", "Teu\u00b7fel", "schuf", "die", "Pre\u00df\u00b7frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Engel die Censur:", "tokens": ["Ein", "En\u00b7gel", "die", "Cen\u00b7sur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gottlob, es ist doch noch zur Zeit", "tokens": ["Gott\u00b7lob", ",", "es", "ist", "doch", "noch", "zur", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von jener wenig Spur.", "tokens": ["Von", "je\u00b7ner", "we\u00b7nig", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn wer ein b\u00f6s Gewissen hat,", "tokens": ["Denn", "wer", "ein", "b\u00f6s", "Ge\u00b7wis\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem stehn die Engel bei;", "tokens": ["Dem", "stehn", "die", "En\u00b7gel", "bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch hindert es noch Kirch und Staat,", "tokens": ["Auch", "hin\u00b7dert", "es", "noch", "Kirch", "und", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da man des Teufels sei.", "tokens": ["Da", "man", "des", "Teu\u00b7fels", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}