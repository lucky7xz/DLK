{"textgrid.poem.49715": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4nner und Schranzen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den G\u00e4ngen des Schlosses", "tokens": ["In", "den", "G\u00e4n\u00b7gen", "des", "Schlos\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dr\u00e4ngt sich die bunte,", "tokens": ["Dr\u00e4ngt", "sich", "die", "bun\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Farbig gekleidete Menge.", "tokens": ["Far\u00b7big", "ge\u00b7klei\u00b7de\u00b7te", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schnurrbartstarrende,", "tokens": ["Schnurr\u00b7bart\u00b7star\u00b7ren\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Vorne bis hinten", "tokens": ["Vor\u00b7ne", "bis", "hin\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durchgescheitelte Offiziere", "tokens": ["Durch\u00b7ge\u00b7schei\u00b7tel\u00b7te", "Of\u00b7fi\u00b7zie\u00b7re"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Werfen bedeutende", "tokens": ["Wer\u00b7fen", "be\u00b7deu\u00b7ten\u00b7de"], "token_info": ["word", "word"], "pos": ["NN", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.8": {"text": "Blicke um sich.", "tokens": ["Bli\u00b7cke", "um", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und die blitzenden Orden", "tokens": ["Und", "die", "blit\u00b7zen\u00b7den", "Or\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Klirren vernehmlich", "tokens": ["Klir\u00b7ren", "ver\u00b7nehm\u00b7lich"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Auf den gew\u00f6lbten", "tokens": ["Auf", "den", "ge\u00b7w\u00f6lb\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Soldatenbr\u00fcsten.", "tokens": ["Sol\u00b7da\u00b7ten\u00b7br\u00fcs\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Neben ihnen die glattrasierten", "tokens": ["Ne\u00b7ben", "ih\u00b7nen", "die", "glatt\u00b7ra\u00b7sier\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Schl\u00fcsselgeschm\u00fcckten Kammerherren", "tokens": ["Schl\u00fcs\u00b7sel\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Kam\u00b7mer\u00b7her\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit dem erstarrten", "tokens": ["Mit", "dem", "er\u00b7starr\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "K\u00fcnstlichen L\u00e4cheln", "tokens": ["K\u00fcnst\u00b7li\u00b7chen", "L\u00e4\u00b7cheln"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Um die Winkel des l\u00fcgenden Mundes.", "tokens": ["Um", "die", "Win\u00b7kel", "des", "l\u00fc\u00b7gen\u00b7den", "Mun\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Das fl\u00fcstert und wispert,", "tokens": ["Das", "fl\u00fcs\u00b7tert", "und", "wis\u00b7pert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Und gr\u00fc\u00dft sich mit kalten,", "tokens": ["Und", "gr\u00fc\u00dft", "sich", "mit", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Katzengreulichen,", "tokens": ["Kat\u00b7zen\u00b7greu\u00b7li\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.21": {"text": "Falschen Augen.", "tokens": ["Fal\u00b7schen", "Au\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Das windet und schiebt sich", "tokens": ["Das", "win\u00b7det", "und", "schiebt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Mit den h\u00f6flichen Ellenbogen,", "tokens": ["Mit", "den", "h\u00f6f\u00b7li\u00b7chen", "El\u00b7len\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Das gleitet lautlos", "tokens": ["Das", "glei\u00b7tet", "laut\u00b7los"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD"], "meter": "-+---", "measure": "dactylic.init"}, "line.25": {"text": "Den glattgewichsten", "tokens": ["Den", "glatt\u00b7ge\u00b7wichs\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Boden entlang.", "tokens": ["Bo\u00b7den", "ent\u00b7lang", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Da!", "tokens": ["Da", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Tiefe Stille.", "tokens": ["Tie\u00b7fe", "Stil\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Der hohe Adel", "tokens": ["Der", "ho\u00b7he", "A\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Reckelt die H\u00e4lse,", "tokens": ["Re\u00b7ckelt", "die", "H\u00e4l\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Richtet die scharfen,", "tokens": ["Rich\u00b7tet", "die", "schar\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Kritisch sp\u00e4henden H\u00f6flingsblicke", "tokens": ["Kri\u00b7tisch", "sp\u00e4\u00b7hen\u00b7den", "H\u00f6f\u00b7lings\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Zur ge\u00f6ffneten Fl\u00fcgelt\u00fcr.", "tokens": ["Zur", "ge\u00b7\u00f6ff\u00b7ne\u00b7ten", "Fl\u00fc\u00b7gel\u00b7t\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.34": {"text": "Drei Gestalten", "tokens": ["Drei", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "In schwarzem Rock", "tokens": ["In", "schwar\u00b7zem", "Rock"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Treten herein. Die blanken Augen", "tokens": ["Tre\u00b7ten", "her\u00b7ein", ".", "Die", "blan\u00b7ken", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.37": {"text": "Gleiten ruhig", "tokens": ["Glei\u00b7ten", "ru\u00b7hig"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.38": {"text": "Die reich betre\u00dfte,", "tokens": ["Die", "reich", "be\u00b7tre\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Ordenflimmernde Schar entlang.", "tokens": ["Or\u00b7den\u00b7flim\u00b7mern\u00b7de", "Schar", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.40": {"text": "Ganz unkundig der leicht geb\u00fcckten", "tokens": ["Ganz", "un\u00b7kun\u00b7dig", "der", "leicht", "ge\u00b7b\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJD", "ADJA"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.41": {"text": "Vorgeschriebenen R\u00fcckenhaltung,", "tokens": ["Vor\u00b7ge\u00b7schrie\u00b7be\u00b7nen", "R\u00fc\u00b7cken\u00b7hal\u00b7tung", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.42": {"text": "Aufrecht gehen sie, ehrenfesten", "tokens": ["Auf\u00b7recht", "ge\u00b7hen", "sie", ",", "eh\u00b7ren\u00b7fes\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.43": {"text": "Schrittes einher.", "tokens": ["Schrit\u00b7tes", "ein\u00b7her", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.44": {"text": "Botha, Dewet und", "tokens": ["Bo\u00b7tha", ",", "De\u00b7wet", "und"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "KON"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.45": {"text": "Delarey.", "tokens": ["De\u00b7la\u00b7rey", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.46": {"text": "T\u00e4nzelnd und h\u00fcpfend,", "tokens": ["T\u00e4n\u00b7zelnd", "und", "h\u00fcp\u00b7fend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Hierhin und dorthin", "tokens": ["Hier\u00b7hin", "und", "dor\u00b7thin"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.48": {"text": "Gr\u00fc\u00dfend, f\u00fchrt sie der Hofmarschall.", "tokens": ["Gr\u00fc\u00b7\u00dfend", ",", "f\u00fchrt", "sie", "der", "Hof\u00b7mar\u00b7schall", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.49": {"text": "Und er \u00f6ffnet", "tokens": ["Und", "er", "\u00f6ff\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.50": {"text": "Ehrfurchtschauernd", "tokens": ["Ehr\u00b7furcht\u00b7schau\u00b7ernd"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.51": {"text": "Die letzte T\u00fcre,", "tokens": ["Die", "letz\u00b7te", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.52": {"text": "L\u00e4\u00dft sie hinein,", "tokens": ["L\u00e4\u00dft", "sie", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.53": {"text": "Die Gl\u00fcckbegabten,", "tokens": ["Die", "Gl\u00fcck\u00b7be\u00b7gab\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.54": {"text": "Vor das Antlitz der Majest\u00e4t.", "tokens": ["Vor", "das", "Ant\u00b7litz", "der", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.55": {"text": "Hinter ihnen", "tokens": ["Hin\u00b7ter", "ih\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.56": {"text": "Lautlos schlie\u00dft sich die vielbegehrte,", "tokens": ["Laut\u00b7los", "schlie\u00dft", "sich", "die", "viel\u00b7be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.57": {"text": "Hei\u00df umworbene", "tokens": ["Hei\u00df", "um\u00b7wor\u00b7be\u00b7ne"], "token_info": ["word", "word"], "pos": ["NN", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.58": {"text": "Hohe Pforte.", "tokens": ["Ho\u00b7he", "Pfor\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.59": {"text": "Wie im herbstlichen Buchenwalde", "tokens": ["Wie", "im", "herbst\u00b7li\u00b7chen", "Bu\u00b7chen\u00b7wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.60": {"text": "In den d\u00fcrren,", "tokens": ["In", "den", "d\u00fcr\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.61": {"text": "Vergilbten Bl\u00e4ttern", "tokens": ["Ver\u00b7gilb\u00b7ten", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.62": {"text": "Raschelnd fl\u00fcstert", "tokens": ["Ra\u00b7schelnd", "fl\u00fcs\u00b7tert"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.63": {"text": "Der leise Wind,", "tokens": ["Der", "lei\u00b7se", "Wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.64": {"text": "Also geht ein ged\u00e4mpftes Raunen", "tokens": ["Al\u00b7so", "geht", "ein", "ge\u00b7d\u00e4mpf\u00b7tes", "Rau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.65": {"text": "Durch die Reihen des Hochgebornen.", "tokens": ["Durch", "die", "Rei\u00b7hen", "des", "Hoch\u00b7ge\u00b7bor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.66": {"text": "Das fl\u00fcstert und zischelt", "tokens": ["Das", "fl\u00fcs\u00b7tert", "und", "zi\u00b7schelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.67": {"text": "H\u00f6hnisch l\u00e4chelnd von groben H\u00e4nden,", "tokens": ["H\u00f6h\u00b7nisch", "l\u00e4\u00b7chelnd", "von", "gro\u00b7ben", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.68": {"text": "Und von derben", "tokens": ["Und", "von", "der\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "PDAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.69": {"text": "Festgenagelten Bauernstiefeln.", "tokens": ["Fest\u00b7ge\u00b7na\u00b7gel\u00b7ten", "Bau\u00b7ern\u00b7stie\u00b7feln", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.70": {"text": "Aber mancher der seichten Sp\u00f6tter", "tokens": ["A\u00b7ber", "man\u00b7cher", "der", "seich\u00b7ten", "Sp\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.71": {"text": "Sagt sich selber,", "tokens": ["Sagt", "sich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.72": {"text": "Wenn er wirklich einmal nicht l\u00fcgt,", "tokens": ["Wenn", "er", "wirk\u00b7lich", "ein\u00b7mal", "nicht", "l\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "PTKNEG",