,", "tokens": ["Die", "Gl\u00fcck\u00b7be\u00b7gab\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.54": {"text": "Vor das Antlitz der Majest\u00e4t.", "tokens": ["Vor", "das", "Ant\u00b7litz", "der", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.55": {"text": "Hinter ihnen", "tokens": ["Hin\u00b7ter", "ih\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.56": {"text": "Lautlos schlie\u00dft sich die vielbegehrte,", "tokens": ["Laut\u00b7los", "schlie\u00dft", "sich", "die", "viel\u00b7be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.57": {"text": "Hei\u00df umworbene", "tokens": ["Hei\u00df", "um\u00b7wor\u00b7be\u00b7ne"], "token_info": ["word", "word"], "pos": ["NN", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.58": {"text": "Hohe Pforte.", "tokens": ["Ho\u00b7he", "Pfor\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.59": {"text": "Wie im herbstlichen Buchenwalde", "tokens": ["Wie", "im", "herbst\u00b7li\u00b7chen", "Bu\u00b7chen\u00b7wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.60": {"text": "In den d\u00fcrren,", "tokens": ["In", "den", "d\u00fcr\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.61": {"text": "Vergilbten Bl\u00e4ttern", "tokens": ["Ver\u00b7gil