": {"text": "Festgenagelten Bauernstiefeln.", "tokens": ["Fest\u00b7ge\u00b7na\u00b7gel\u00b7ten", "Bau\u00b7ern\u00b7stie\u00b7feln", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.70": {"text": "Aber mancher der seichten Sp\u00f6tter", "tokens": ["A\u00b7ber", "man\u00b7cher", "der", "seich\u00b7ten", "Sp\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.71": {"text": "Sagt sich selber,", "tokens": ["Sagt", "sich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.72": {"text": "Wenn er wirklich einmal nicht l\u00fcgt,", "tokens": ["Wenn", "er", "wirk\u00b7lich", "ein\u00b7mal", "nicht", "l\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.73": {"text": "Da\u00df er heute das sonderbare,", "tokens": ["Da\u00df", "er", "heu\u00b7te", "das", "son\u00b7der\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.74": {"text": "Nie gesehene Bild geschaut:", "tokens": ["Nie", "ge\u00b7se\u00b7he\u00b7ne", "Bild", "ge\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.75": {"text": "Ehrlich blickende", "tokens": ["Ehr\u00b7lich", "bli\u00b7cken\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.76": {"text": "Feste M\u00e4nner", "tokens": ["F