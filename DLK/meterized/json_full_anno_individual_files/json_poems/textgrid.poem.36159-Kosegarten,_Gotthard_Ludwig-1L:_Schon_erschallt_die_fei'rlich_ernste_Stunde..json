{"textgrid.poem.36159": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon erschallt die fei'rlich ernste Stunde.", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon erschallt die fei'rlich ernste Stunde.", "tokens": ["Schon", "er\u00b7schallt", "die", "fei'\u00b7rlich", "erns\u00b7te", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schon entlispelt banges Lebewohl", "tokens": ["Schon", "ent\u00b7lis\u00b7pelt", "ban\u00b7ges", "Le\u00b7be\u00b7wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eurer Freunde trauerstummen Munde,", "tokens": ["Eu\u00b7rer", "Freun\u00b7de", "trau\u00b7er\u00b7stum\u00b7men", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Seele schwillt euch schw\u00fcl und voll!", "tokens": ["Und", "die", "See\u00b7le", "schwillt", "euch", "schw\u00fcl", "und", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Seyd getrost und ziehet hin in Frieden!", "tokens": ["Seyd", "ge\u00b7trost", "und", "zie\u00b7het", "hin", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unsre Augen sehn euch liebend nach;", "tokens": ["Uns\u00b7re", "Au\u00b7gen", "sehn", "euch", "lie\u00b7bend", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nie ermatten wird, und nie erm\u00fcden", "tokens": ["Nie", "er\u00b7mat\u00b7ten", "wird", ",", "und", "nie", "er\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "VAFIN", "$,", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unser Flehen f\u00fcr euch, Nacht und Tag.", "tokens": ["Un\u00b7ser", "Fle\u00b7hen", "f\u00fcr", "euch", ",", "Nacht", "und", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Seyd getrost, und trocknet eure Z\u00e4hren,", "tokens": ["Seyd", "ge\u00b7trost", ",", "und", "trock\u00b7net", "eu\u00b7re", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Folgt, wohin euch Gott und Tugend ruft.", "tokens": ["Folgt", ",", "wo\u00b7hin", "euch", "Gott", "und", "Tu\u00b7gend", "ruft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Klimmt hinan die steile Bahn der Ehren!", "tokens": ["Klimmt", "hi\u00b7nan", "die", "stei\u00b7le", "Bahn", "der", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bebt vor keinem Abhang, keiner Kluft!", "tokens": ["Bebt", "vor", "kei\u00b7nem", "Ab\u00b7hang", ",", "kei\u00b7ner", "Kluft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Zittert nicht, zu ziehn in eine gro\u00dfe,", "tokens": ["Zit\u00b7tert", "nicht", ",", "zu", "ziehn", "in", "ei\u00b7ne", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eine weite, eine fremde Welt!", "tokens": ["Ei\u00b7ne", "wei\u00b7te", ",", "ei\u00b7ne", "frem\u00b7de", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo ihr seyd, da ruht ihr dem im Schoo\u00dfe,", "tokens": ["Wo", "ihr", "seyd", ",", "da", "ruht", "ihr", "dem", "im", "Schoo\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "APPRART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Der in hohler Hand den Erdball h\u00e4lt.", "tokens": ["Der", "in", "hoh\u00b7ler", "Hand", "den", "Erd\u00b7ball", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "F\u00fcrchtet ihn, der alles wei\u00df und siehet!", "tokens": ["F\u00fcrch\u00b7tet", "ihn", ",", "der", "al\u00b7les", "wei\u00df", "und", "sie\u00b7het", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Traut auf ihn, der ewig lebt und liebt!", "tokens": ["Traut", "auf", "ihn", ",", "der", "e\u00b7wig", "lebt", "und", "liebt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denkt in guten Tagen sein, und fliehet", "tokens": ["Denkt", "in", "gu\u00b7ten", "Ta\u00b7gen", "sein", ",", "und", "flie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VAINF", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihm in Arm, wenn sich der Himmel tr\u00fcbt!", "tokens": ["Ihm", "in", "Arm", ",", "wenn", "sich", "der", "Him\u00b7mel", "tr\u00fcbt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Weichet nicht zur Linken noch zur Rechten", "tokens": ["Wei\u00b7chet", "nicht", "zur", "Lin\u00b7ken", "noch", "zur", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von den Pfaden, d'rauf euch Jesus wies.", "tokens": ["Von", "den", "Pfa\u00b7den", ",", "d'\u00b7rauf", "euch", "Je\u00b7sus", "wies", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PAV", "PPER", "NE", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Fackel ist sein Wort in Lebensn\u00e4chten.", "tokens": ["Fa\u00b7ckel", "ist", "sein", "Wort", "in", "Le\u00b7bens\u00b7n\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Morgenstern in Todesd\u00fcsterni\u00df.", "tokens": ["Mor\u00b7gens\u00b7tern", "in", "To\u00b7des\u00b7d\u00fcs\u00b7ter\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Werdet gro\u00df in eurer Br\u00fcder Mitte!", "tokens": ["Wer\u00b7det", "gro\u00df", "in", "eu\u00b7rer", "Br\u00fc\u00b7der", "Mit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Werdet Ruhm f\u00fcr euer Vaterland!", "tokens": ["Wer\u00b7det", "Ruhm", "f\u00fcr", "eu\u00b7er", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tauschet nimmer edle deutsche Sitte", "tokens": ["Tau\u00b7schet", "nim\u00b7mer", "ed\u00b7le", "deut\u00b7sche", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um des Auslands bunten Flittertand!", "tokens": ["Um", "des", "Aus\u00b7lands", "bun\u00b7ten", "Flit\u00b7ter\u00b7tand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Haltet fest an Redlichkeit und Treue,", "tokens": ["Hal\u00b7tet", "fest", "an", "Red\u00b7lich\u00b7keit", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fest an M\u00e4nnlichkeit und heil'ger Schaam,", "tokens": ["Fest", "an", "M\u00e4nn\u00b7lich\u00b7keit", "und", "heil'\u00b7ger", "Schaam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Unverzagt im Donnersturm wie Leue,", "tokens": ["Un\u00b7ver\u00b7zagt", "im", "Don\u00b7ner\u00b7sturm", "wie", "Leu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "KOKOM", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch im Sonnenschimmer l\u00e4mmchenzahm.", "tokens": ["Doch", "im", "Son\u00b7nen\u00b7schim\u00b7mer", "l\u00e4mm\u00b7chen\u00b7zahm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Nie vergeudet in den Sklavenl\u00fcsten", "tokens": ["Nie", "ver\u00b7geu\u00b7det", "in", "den", "Skla\u00b7ven\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Feiger Frevler eure edle Kraft!", "tokens": ["Fei\u00b7ger", "Frev\u00b7ler", "eu\u00b7re", "ed\u00b7le", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit dem Muth des Helden und des Christen", "tokens": ["Mit", "dem", "Muth", "des", "Hel\u00b7den", "und", "des", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "B\u00e4ndiget die Riesinn Leidenschaft!", "tokens": ["B\u00e4n\u00b7di\u00b7get", "die", "Rie\u00b7sinn", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Herrlich ist ein Held im Schlachtget\u00fcmmel,", "tokens": ["Herr\u00b7lich", "ist", "ein", "Held", "im", "Schlacht\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lorbeern werth ein stahlerrungner Sieg.", "tokens": ["Lor\u00b7beern", "werth", "ein", "stah\u00b7ler\u00b7rung\u00b7ner", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber herrlicher vor Gott im Himmel", "tokens": ["A\u00b7ber", "herr\u00b7li\u00b7cher", "vor", "Gott", "im", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist ein Held im Leidenschaftenkrieg.", "tokens": ["Ist", "ein", "Held", "im", "Lei\u00b7den\u00b7schaf\u00b7ten\u00b7krieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Solchem Helden rauschen Edens Palmen.", "tokens": ["Sol\u00b7chem", "Hel\u00b7den", "rau\u00b7schen", "E\u00b7dens", "Pal\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Solches Name flammt im Lebensbuch.", "tokens": ["Sol\u00b7ches", "Na\u00b7me", "flammt", "im", "Le\u00b7bens\u00b7buch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Himmeldichter singen Feuerpsalmen,", "tokens": ["Him\u00b7mel\u00b7dich\u00b7ter", "sin\u00b7gen", "Feu\u00b7er\u00b7psal\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie der Held die Schlachten Gottes schlug.", "tokens": ["Wie", "der", "Held", "die", "Schlach\u00b7ten", "Got\u00b7tes", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Solcher Kronen m\u00fc\u00dften viel euch schimmern!", "tokens": ["Sol\u00b7cher", "Kro\u00b7nen", "m\u00fc\u00df\u00b7ten", "viel", "euch", "schim\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Solcher Thaten m\u00fc\u00dft ihr viele thun.", "tokens": ["Sol\u00b7cher", "Tha\u00b7ten", "m\u00fc\u00dft", "ihr", "vie\u00b7le", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Also werdet ihr auf Weltentr\u00fcmmern,", "tokens": ["Al\u00b7so", "wer\u00b7det", "ihr", "auf", "Wel\u00b7ten\u00b7tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie auf Blumenrasen l\u00e4chelnd ruhn!", "tokens": ["Wie", "auf", "Blu\u00b7men\u00b7ra\u00b7sen", "l\u00e4\u00b7chelnd", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Also zieht durch Dorn- und Blumgefilde!", "tokens": ["Al\u00b7so", "zieht", "durch", "Dorn", "und", "Blum\u00b7ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zieht in Segen und in Frieden hin!", "tokens": ["Zieht", "in", "Se\u00b7gen", "und", "in", "Frie\u00b7den", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eure Obhut sey die ew'ge Milde,", "tokens": ["Eu\u00b7re", "Ob\u00b7hut", "sey", "die", "ew'\u00b7ge", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT"