{"dta.poem.4366": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ungl\u00fcckselige Folge der Unachtsamkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, mit angestrengtem Sinn, den Zustand die-", "tokens": ["Wenn", "ich", ",", "mit", "an\u00b7ge\u00b7streng\u00b7tem", "Sinn", ",", "den", "Zu\u00b7stand", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "ser Welt betrachte,", "tokens": ["ser", "Welt", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und da\u00df so gar viel Gut- und B\u00f6ses in selbiger zu finden", "tokens": ["Und", "da\u00df", "so", "gar", "viel", "Gut", "und", "B\u00f6\u00b7ses", "in", "sel\u00b7bi\u00b7ger", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADV", "ADV", "TRUNC", "KON", "NN", "APPR", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "seh,", "tokens": ["seh", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Auf die so sonderbare Mischung des Guten und des B\u00f6-", "tokens": ["Auf", "die", "so", "son\u00b7der\u00b7ba\u00b7re", "Misc\u00b7hung", "des", "Gu\u00b7ten", "und", "des", "B\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "ART", "NN", "KON", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "sen achte,", "tokens": ["sen", "ach\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und merke, wie sich beyderley darinn befinde: Wohl und", "tokens": ["Und", "mer\u00b7ke", ",", "wie", "sich", "bey\u00b7der\u00b7ley", "da\u00b7rinn", "be\u00b7fin\u00b7de", ":", "Wohl", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PRF", "PIS", "PAV", "VVFIN", "$.", "ADV", "KON"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Weh;", "tokens": ["Weh", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "So da\u00df man mit dem grossen ", "tokens": ["So", "da\u00df", "man", "mit", "dem", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Freuden, singen kann:", "tokens": ["Freu\u00b7den", ",", "sin\u00b7gen", "kann", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Himmel, Erd' und ihre Heere", "tokens": ["Him\u00b7mel", ",", "Erd'", "und", "ih\u00b7re", "Hee\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat er mir zum Dienst bestellt.", "tokens": ["Hat", "er", "mir", "zum", "Dienst", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich nur mein Aug' hinkehre,", "tokens": ["Wo", "ich", "nur", "mein", "Aug'", "hin\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Find' ich, was mich n\u00e4hrt und h\u00e4lt,", "tokens": ["Find'", "ich", ",", "was", "mich", "n\u00e4hrt", "und", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Thier' und Kr\u00e4uter, und Getrayde.", "tokens": ["Thier'", "und", "Kr\u00e4u\u00b7ter", ",", "und", "Ge\u00b7tray\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Gr\u00fcnden, in der H\u00f6h',", "tokens": ["In", "den", "Gr\u00fcn\u00b7den", ",", "in", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In den B\u00fcschen, in der See,", "tokens": ["In", "den", "B\u00fc\u00b7schen", ",", "in", "der", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ueberall ist meine Weide.", "tokens": ["Ue\u00b7be\u00b7rall", "ist", "mei\u00b7ne", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald aber auch mit einem andern, mit eben ja so vielem", "tokens": ["Bald", "a\u00b7ber", "auch", "mit", "ei\u00b7nem", "an\u00b7dern", ",", "mit", "e\u00b7ben", "ja", "so", "vie\u00b7lem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "$,", "APPR", "ADV", "ADV", "ADV", "PIS"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Rechte,", "tokens": ["Rech\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mit Thr\u00e4nen-reichen Klage-Liedern, voll Gram, wohl", "tokens": ["Mit", "Thr\u00e4\u00b7nen\u00b7rei\u00b7chen", "Kla\u00b7ge\u00b7Lie\u00b7dern", ",", "voll", "Gram", ",", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "NN", "$,", "ADJD", "NN", "$,", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "wieder singen m\u00f6gte:", "tokens": ["wie\u00b7der", "sin\u00b7gen", "m\u00f6g\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Es st\u00fcrmen auf ihn zu", "tokens": ["Es", "st\u00fcr\u00b7men", "auf", "ihn", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKZU"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Viel' Widerw\u00e4rtigkeiten.", "tokens": ["Viel'", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er mu\u00df mit Noht und Tod,", "tokens": ["Er", "mu\u00df", "mit", "Noht", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ja mit sich selber streiten.", "tokens": ["Ja", "mit", "sich", "sel\u00b7ber", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bald lacht ihn freundlich an", "tokens": ["Bald", "lacht", "ihn", "freund\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das wankelhafte Gl\u00fcck;", "tokens": ["Das", "wan\u00b7kel\u00b7haf\u00b7te", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald gibt es unverhoft", "tokens": ["Bald", "gibt", "es", "un\u00b7ver\u00b7hoft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jhm lauter saure Blick.", "tokens": ["Jhm", "lau\u00b7ter", "sau\u00b7re", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So bet\u2019 ich billig GOttes Ordnung, in dieser fremden", "tokens": ["So", "bet'", "ich", "bil\u00b7lig", "Got\u00b7tes", "Ord\u00b7nung", ",", "in", "die\u00b7ser", "frem\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "NN", "$,", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ordnung, an,", "tokens": ["Ord\u00b7nung", ",", "an", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und denke, mit ged\u00e4mpftem Witz: Was GOtt thut, das", "tokens": ["Und", "den\u00b7ke", ",", "mit", "ge\u00b7d\u00e4mpf\u00b7tem", "Witz", ":", "Was", "Gott", "thut", ",", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$.", "PWS", "NN", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ist wohl gethan.", "tokens": ["ist", "wohl", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch f\u00e4llt mir ein betr\u00fcbt Betragen der Menschen, bey", "tokens": ["Doch", "f\u00e4llt", "mir", "ein", "be\u00b7tr\u00fcbt", "Be\u00b7tra\u00b7gen", "der", "Men\u00b7schen", ",", "bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "dem Zustand, bey,", "tokens": ["dem", "Zu\u00b7stand", ",", "bey", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wor\u00fcber, wie so unvern\u00fcnftig der gr\u00f6\u00dfte Theil der Mensch-", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", ",", "wie", "so", "un\u00b7ver\u00b7n\u00fcnf\u00b7tig", "der", "gr\u00f6\u00df\u00b7te", "Theil", "der", "Men\u00b7sch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "ADV", "ADJD", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "heit sey,", "tokens": ["heit", "sey", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.9": {"text": "Ein Mensch, der es mit Ernst erwegt, sich nimmer gnug", "tokens": ["Ein", "Mensch", ",", "der", "es", "mit", "Ernst", "er\u00b7wegt", ",", "sich", "nim\u00b7mer", "gnug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NE", "VVFIN", "$,", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "verwundern kann.", "tokens": ["ver\u00b7wun\u00b7dern", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Da er in seiner Lebens-Zeit sich hier auf einer Welt befindet,", "tokens": ["Da", "er", "in", "sei\u00b7ner", "Le\u00b7bens\u00b7Zeit", "sich", "hier", "auf", "ei\u00b7ner", "Welt", "be\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.12": {"text": "Worauf, wie es unwidersprechlich, oft Gl\u00fcck mit Ungl\u00fcck", "tokens": ["Wo\u00b7rauf", ",", "wie", "es", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", ",", "oft", "Gl\u00fcck", "mit", "Un\u00b7gl\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "PPER", "ADJD", "$,", "ADV", "NN", "APPR", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.13": {"text": "sich verbindet,", "tokens": ["sich", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Und er, durch Denken blo\u00df allein, die Dinge sich zu eigen", "tokens": ["Und", "er", ",", "durch", "Den\u00b7ken", "blo\u00df", "al\u00b7lein", ",", "die", "Din\u00b7ge", "sich", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "ADV", "ADV", "$,", "ART", "NN", "PRF", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "macht;", "tokens": ["macht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.16": {"text": "So ist er doch fast nimmermehr aufs Gute, das er hat,", "tokens": ["So", "ist", "er", "doch", "fast", "nim\u00b7mer\u00b7mehr", "aufs", "Gu\u00b7te", ",", "das", "er", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "bedacht.", "tokens": ["be\u00b7dacht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Er denket tausend mahl so viel auf das, so ihn betr\u00fcbt", "tokens": ["Er", "den\u00b7ket", "tau\u00b7send", "mahl", "so", "viel", "auf", "das", ",", "so", "ihn", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "ADV", "ADV", "ADV", "APPR", "ART", "$,", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "und kr\u00e4nkt,", "tokens": ["und", "kr\u00e4nkt", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Als er an das so vieles Gute, was er besitzet, nie gedenkt.", "tokens": ["Als", "er", "an", "das", "so", "vie\u00b7les", "Gu\u00b7te", ",", "was", "er", "be\u00b7sit\u00b7zet", ",", "nie", "ge\u00b7denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADV", "PIS", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "--+--+-+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "So lang er etwas Gutes sucht, verlangt und w\u00fcnscht,", "tokens": ["So", "lang", "er", "et\u00b7was", "Gu\u00b7tes", "sucht", ",", "ver\u00b7langt", "und", "w\u00fcnscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "als Geld und Ehr',", "tokens": ["als", "Geld", "und", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "So hat ers nicht. Erlangt er es; so hat er es so gleich nicht", "tokens": ["So", "hat", "ers", "nicht", ".", "Er\u00b7langt", "er", "es", ";", "so", "hat", "er", "es", "so", "gleich", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "$.", "VVFIN", "PPER", "PPER", "$.", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "mehr,", "tokens": ["mehr", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Weil er sich nicht bequehmen will, sein ihm allein geh\u00f6rigs", "tokens": ["Weil", "er", "sich", "nicht", "be\u00b7queh\u00b7men", "will", ",", "sein", "ihm", "al\u00b7lein", "ge\u00b7h\u00f6\u00b7rigs"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,", "PPOSAT", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "Denken,", "tokens": ["Den\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Auf das von ihm bese\u00dfne Gut, sein zu geniessen, hinzulenken.", "tokens": ["Auf", "das", "von", "ihm", "be\u00b7se\u00df\u00b7ne", "Gut", ",", "sein", "zu", "ge\u00b7nies\u00b7sen", ",", "hin\u00b7zu\u00b7len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "ADJA", "NN", "$,", "VAINF", "PTKZU", "VVINF", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Er wirft es alsbald hinter sich, sieht aber nie darauf zur\u00fcck,", "tokens": ["Er", "wirft", "es", "als\u00b7bald", "hin\u00b7ter", "sich", ",", "sieht", "a\u00b7ber", "nie", "da\u00b7rauf", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PRF", "$,", "VVFIN", "ADV", "ADV", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und kehrt auf gegenw\u00e4rtigs B\u00f6ses best\u00e4ndig seinen steifen", "tokens": ["Und", "kehrt", "auf", "ge\u00b7gen\u00b7w\u00e4r\u00b7tigs", "B\u00f6\u00b7ses", "be\u00b7st\u00e4n\u00b7dig", "sei\u00b7nen", "stei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Blick.", "tokens": ["Blick", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Hieraus nun folgt unwidersprechlich, da\u00df wir durch eigne", "tokens": ["Hier\u00b7aus", "nun", "folgt", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", ",", "da\u00df", "wir", "durch", "eig\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Schuld allein,", "tokens": ["Schuld", "al\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Auf dieser Welt, bey vielem Gl\u00fcck, doch immer ungl\u00fcck-", "tokens": ["Auf", "die\u00b7ser", "Welt", ",", "bey", "vie\u00b7lem", "Gl\u00fcck", ",", "doch", "im\u00b7mer", "un\u00b7gl\u00fc\u00b7ck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PIS", "NN", "$,", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "selig seyn,", "tokens": ["se\u00b7lig", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Da\u00df jeder Mensch, mit Vorsatz fast, was GOtt ihm Gutes", "tokens": ["Da\u00df", "je\u00b7der", "Mensch", ",", "mit", "Vor\u00b7satz", "fast", ",", "was", "Gott", "ihm", "Gu\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punc