": ["Zum", "Blei\u00b7ben", "ich", ",", "zum", "Schei\u00b7den", "du", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Gingst du voran \u2013 und hast nicht viel verloren.", "tokens": ["Gingst", "du", "vo\u00b7ran", "\u2013", "und", "hast", "nicht", "viel", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$(", "KON", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Des Menschen Leben scheint ein herrlich Los:", "tokens": ["Des", "Men\u00b7schen", "Le\u00b7ben", "scheint", "ein", "herr\u00b7lich", "Los", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Tag wie lieblich, so die Nacht wie gro\u00df!", "tokens": ["Der", "Tag", "wie", "lieb\u00b7lich", ",", "so", "die", "Nacht", "wie", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJD", "$,", "ADV", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wir, gepflanzt in Paradieses Wonne,", "tokens": ["Und", "wir", ",", "ge\u00b7pflanzt", "in", "Pa\u00b7ra\u00b7die\u00b7ses", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Genie\u00dfen kaum der hocherlauchten Sonne,", "tokens": ["Ge\u