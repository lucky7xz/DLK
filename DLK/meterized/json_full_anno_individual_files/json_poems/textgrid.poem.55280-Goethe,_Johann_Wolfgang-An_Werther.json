{"textgrid.poem.55280": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An Werther", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einmal wagst du, vielbeweinter Schatten,", "tokens": ["Noch", "ein\u00b7mal", "wagst", "du", ",", "viel\u00b7be\u00b7wein\u00b7ter", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hervor dich an das Tageslicht,", "tokens": ["Her\u00b7vor", "dich", "an", "das", "Ta\u00b7ges\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begegnest mir auf neubebl\u00fcmten Matten,", "tokens": ["Be\u00b7geg\u00b7nest", "mir", "auf", "neu\u00b7be\u00b7bl\u00fcm\u00b7ten", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meinen Anblick scheust du nicht.", "tokens": ["Und", "mei\u00b7nen", "An\u00b7blick", "scheust", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist, als ob du lebtest in der Fr\u00fche,", "tokens": ["Es", "ist", ",", "als", "ob", "du", "leb\u00b7test", "in", "der", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo uns der Tau auf ", "tokens": ["Wo", "uns", "der", "Tau", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und nach des Tages unwillkommner M\u00fche", "tokens": ["Und", "nach", "des", "Ta\u00b7ges", "un\u00b7will\u00b7komm\u00b7ner", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Scheidesonne letzter Strahl entz\u00fcckt;", "tokens": ["Der", "Schei\u00b7de\u00b7son\u00b7ne", "letz\u00b7ter", "Strahl", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zum Bleiben ich, zum Scheiden du erkoren,", "tokens": ["Zum", "Blei\u00b7ben", "ich", ",", "zum", "Schei\u00b7den", "du", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Gingst du voran \u2013 und hast nicht viel verloren.", "tokens": ["Gingst", "du", "vo\u00b7ran", "\u2013", "und", "hast", "nicht", "viel", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$(", "KON", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Menschen Leben scheint ein herrlich Los:", "tokens": ["Des", "Men\u00b7schen", "Le\u00b7ben", "scheint", "ein", "herr\u00b7lich", "Los", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Tag wie lieblich, so die Nacht wie gro\u00df!", "tokens": ["Der", "Tag", "wie", "lieb\u00b7lich", ",", "so", "die", "Nacht", "wie", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJD", "$,", "ADV", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wir, gepflanzt in Paradieses Wonne,", "tokens": ["Und", "wir", ",", "ge\u00b7pflanzt", "in", "Pa\u00b7ra\u00b7die\u00b7ses", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Genie\u00dfen kaum der hocherlauchten Sonne,", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "kaum", "der", "ho\u00b7cher\u00b7lauch\u00b7ten", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da k\u00e4mpft sogleich verworrene Bestrebung", "tokens": ["Da", "k\u00e4mpft", "sog\u00b7leich", "ver\u00b7wor\u00b7re\u00b7ne", "Be\u00b7stre\u00b7bung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bald mit uns selbst und bald mit der Umgebung;", "tokens": ["Bald", "mit", "uns", "selbst", "und", "bald", "mit", "der", "Um\u00b7ge\u00b7bung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Keins wird vom andern w\u00fcnschenswert erg\u00e4nzt,", "tokens": ["Keins", "wird", "vom", "an\u00b7dern", "w\u00fcn\u00b7schens\u00b7wert", "er\u00b7g\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "ADJA", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von au\u00dfen d\u00fcstert's, wenn es innen gl\u00e4nzt,", "tokens": ["Von", "au\u00b7\u00dfen", "d\u00fcs\u00b7tert's", ",", "wenn", "es", "in\u00b7nen", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein gl\u00e4nzend \u00c4u\u00dfres deckt mein tr\u00fcber Blick,", "tokens": ["Ein", "gl\u00e4n\u00b7zend", "\u00c4u\u00df\u00b7res", "deckt", "mein", "tr\u00fc\u00b7ber", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da steht es nah \u2013 und man verkennt das Gl\u00fcck.", "tokens": ["Da", "steht", "es", "nah", "\u2013", "und", "man", "ver\u00b7kennt", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun glauben wir's zu kennen! Mit Gewalt", "tokens": ["Nun", "glau\u00b7ben", "wir's", "zu", "ken\u00b7nen", "!", "Mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ergreift uns Liebreiz weiblicher Gestalt:", "tokens": ["Er\u00b7greift", "uns", "Lieb\u00b7reiz", "weib\u00b7li\u00b7cher", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der J\u00fcngling, froh wie in der Kindheit Flor,", "tokens": ["Der", "J\u00fcng\u00b7ling", ",", "froh", "wie", "in", "der", "Kind\u00b7heit", "Flor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Fr\u00fchling tritt als Fr\u00fchling selbst hervor,", "tokens": ["Im", "Fr\u00fch\u00b7ling", "tritt", "als", "Fr\u00fch\u00b7ling", "selbst", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KOUS", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Entz\u00fcckt, erstaunt, wer dies ihm angetan?", "tokens": ["Ent\u00b7z\u00fcckt", ",", "er\u00b7staunt", ",", "wer", "dies", "ihm", "an\u00b7ge\u00b7tan", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "PWS", "PDS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er schaut umher, die Welt geh\u00f6rt ihm an.", "tokens": ["Er", "schaut", "um\u00b7her", ",", "die", "Welt", "ge\u00b7h\u00f6rt", "ihm", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ins Weite zieht ihn unbefangne Hast,", "tokens": ["Ins", "Wei\u00b7te", "zieht", "ihn", "un\u00b7be\u00b7fang\u00b7ne", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nichts engt ihn ein, nicht Mauer, nicht Palast;", "tokens": ["Nichts", "engt", "ihn", "ein", ",", "nicht", "Mau\u00b7er", ",", "nicht", "Pa\u00b7last", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie V\u00f6gelschar an W\u00e4ldergipfeln streift,", "tokens": ["Wie", "V\u00f6\u00b7gel\u00b7schar", "an", "W\u00e4l\u00b7der\u00b7gip\u00b7feln", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So schwebt auch er, der um die Liebste schweift,", "tokens": ["So", "schwebt", "auch", "er", ",", "der", "um", "die", "Liebs\u00b7te", "schweift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er sucht vom \u00c4ther, den er gern verl\u00e4\u00dft,", "tokens": ["Er", "sucht", "vom", "\u00c4\u00b7ther", ",", "den", "er", "gern", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den treuen Blick, und dieser h\u00e4lt ihn fest.", "tokens": ["Den", "treu\u00b7en", "Blick", ",", "und", "die\u00b7ser", "h\u00e4lt", "ihn", "fest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch erst zu fr\u00fch und dann zu sp\u00e4t gewarnt,", "tokens": ["Doch", "erst", "zu", "fr\u00fch", "und", "dann", "zu", "sp\u00e4t", "ge\u00b7warnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "KON", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlt er den Flug gehemmt, f\u00fchlt sich umgarnt,", "tokens": ["F\u00fchlt", "er", "den", "Flug", "ge\u00b7hemmt", ",", "f\u00fchlt", "sich", "um\u00b7garnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$,", "VVFIN", "PRF", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Wiedersehn ist froh, das Scheiden schwer,", "tokens": ["Das", "Wie\u00b7der\u00b7sehn", "ist", "froh", ",", "das", "Schei\u00b7den", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Wieder-Wiedersehn begl\u00fcckt noch mehr,", "tokens": ["Das", "Wie\u00b7der\u00b7Wie\u00b7der\u00b7sehn", "be\u00b7gl\u00fcckt", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Jahre sind im Augenblick ersetzt;", "tokens": ["Und", "Jah\u00b7re", "sind", "im", "Au\u00b7gen\u00b7blick", "er\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch t\u00fcckisch harrt das Lebewohl zuletzt.", "tokens": ["Doch", "t\u00fc\u00b7ckisch", "harrt", "das", "Le\u00b7be\u00b7wohl", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du l\u00e4chelst, Freund, gef\u00fchlvoll, wie sich ziemt:", "tokens": ["Du", "l\u00e4\u00b7chelst", ",", "Freund", ",", "ge\u00b7f\u00fchl\u00b7voll", ",", "wie", "sich", "ziemt", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "ADJD", "$,", "PWAV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein gr\u00e4\u00dflich Scheiden machte dich ber\u00fchmt;", "tokens": ["Ein", "gr\u00e4\u00df\u00b7lich", "Schei\u00b7den", "mach\u00b7te", "dich", "be\u00b7r\u00fchmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir feierten dein kl\u00e4glich Mi\u00dfgeschick,", "tokens": ["Wir", "fei\u00b7er\u00b7ten", "dein", "kl\u00e4g\u00b7lich", "Mi\u00df\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du lie\u00dfest uns zu Wohl und Weh zur\u00fcck;", "tokens": ["Du", "lie\u00b7\u00dfest", "uns", "zu", "Wohl", "und", "Weh", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann zog uns wieder ungewisse Bahn", "tokens": ["Dann", "zog", "uns", "wie\u00b7der", "un\u00b7ge\u00b7wis\u00b7se", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Leidenschaften labyrinthisch an;", "tokens": ["Der", "Lei\u00b7den\u00b7schaf\u00b7ten", "la\u00b7by\u00b7rin\u00b7thisch", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wir, verschlungen wiederholter Not,", "tokens": ["Und", "wir", ",", "ver\u00b7schlun\u00b7gen", "wie\u00b7der\u00b7hol\u00b7ter", "Not", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem Scheiden endlich \u2013 Scheiden ist der Tod!", "tokens": ["Dem", "Schei\u00b7den", "end\u00b7lich", "\u2013", "Schei\u00b7den", "ist", "der", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie klingt es r\u00fchrend, wenn der Dichter singt,", "tokens": ["Wie", "klingt", "es", "r\u00fch\u00b7rend", ",", "wenn", "der", "Dich\u00b7ter", "singt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den Tod zu meiden, den das Scheiden bringt!", "tokens": ["Den", "Tod", "zu", "mei\u00b7den", ",", "den", "das", "Schei\u00b7den", "bringt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verstrickt in solche Qualen, halbverschuldet,", "tokens": ["Ver\u00b7strickt", "in", "sol\u00b7che", "Qua\u00b7len", ",", "halb\u00b7ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Geb ihm ein Gott zu sagen, was er duldet.", "tokens": ["Geb", "ihm", "ein", "Gott", "zu", "sa\u00b7gen", ",", "was", "er", "dul\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Noch einmal wagst du, vielbeweinter Schatten,", "tokens": ["Noch", "ein\u00b7mal", "wagst", "du", ",", "viel\u00b7be\u00b7wein\u00b7ter", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hervor dich an das Tageslicht,", "tokens": ["Her\u00b7vor", "dich", "an", "das", "Ta\u00b7ges\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begegnest mir auf neubebl\u00fcmten Matten,", "tokens": ["Be\