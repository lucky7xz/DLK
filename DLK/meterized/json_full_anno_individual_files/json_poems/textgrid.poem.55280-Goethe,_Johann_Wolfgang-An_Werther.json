"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da steht es nah \u2013 und man verkennt das Gl\u00fcck.", "tokens": ["Da", "steht", "es", "nah", "\u2013", "und", "man", "ver\u00b7kennt", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nun glauben wir's zu kennen! Mit Gewalt", "tokens": ["Nun", "glau\u00b7ben", "wir's", "zu", "ken\u00b7nen", "!", "Mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ergreift uns Liebreiz weiblicher Gestalt:", "tokens": ["Er\u00b7greift", "uns", "Lieb\u00b7reiz", "weib\u00b7li\u00b7cher", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der J\u00fcngling, froh wie in der Kindheit Flor,", "tokens": ["Der", "J\u00fcng\u00b7ling", ",", "froh", "wie", "in", "der", "Kind\u00b7heit", "Flor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Fr\u00fchling tritt als Fr\u00fchling selbst hervor,", "tokens": ["Im", "Fr\u00fch\u00b7ling", "tritt", "als", "Fr\u00fch\u00b7ling", "selbst", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KOUS", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Entz\u00fcckt, erstaunt, wer dies ihm angetan?", "tokens": ["Ent\u00b7z\u00fcckt", ",", "er\u00b7staunt", ",", "wer", "dies", "ihm", "an\u00b7ge\u00b7tan", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "PWS", "PDS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er schaut umher, die Welt geh\u00f6rt ihm an.", "tokens": ["Er", "schaut", "um\u00b7her", ",", "die", "Welt", "ge\u00b7h\u00f6rt", "ihm", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ins Weite zieht ihn unbefangne Hast,", "tokens": ["Ins", "Wei\u00b7te", "zieht", "ihn", "un\u00b7be\u00b7fang\u00b7ne", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nichts engt ihn ein, nicht Mauer, nicht Palast;", "tokens": ["Nichts", "engt", "ihn", "ein", ",", "nicht", "Mau\u00b7er", ",", "nicht", "Pa\u00b7last", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie V\u00f6gelschar an W\u00e4ldergipfeln streift,", "tokens": ["Wie", "V\u00f6\u00b7gel\u00b7schar", "an", "W\u00e4l\u00b7der\u00b7gip\u00b7feln", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So schwebt auch er, der um die Liebste schweift,", "tokens": ["So", "schwebt", "auch", "er", ",", "der", "um", "die", "Liebs\u00b7te", "schweift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er sucht vom \u00c4ther, den er gern verl\u00e4\u00dft,", "tokens": ["Er", "sucht", "vom", "\u00c4\u00b7ther", ",", "den", "er", "gern", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den treuen Blick, und dieser h\u00e4lt ihn fest.", "tokens": ["Den", "treu\u00b7en", "Blick", ",", "und", "die\u00b7ser", "h\u00e4lt", "ihn", "fest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch erst zu fr\u00fch und dann zu sp\u00e4t gewarnt,", "tokens": ["Doch", "erst", "zu", "fr\u00fch", "und", "dann", "zu", "sp\u00e4t", "ge\u00b7warnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "KON", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlt er den Flug gehemmt, f\u00fchlt sich umgarnt,", "tokens": ["F\u00fchlt", "er", "den", "Flug", "ge\u00b7hemmt", ",", "f\u00fchlt", "sich", "um\u00b7garnt", ","], "token_info": ["word", "word", "word", "word", "word", "pu