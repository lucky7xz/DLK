{"textgrid.poem.33178": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Cap. 2.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit man die nacht vergehen sah, ist das wahrhaftige licht da:", "tokens": ["Seit", "man", "die", "nacht", "ver\u00b7ge\u00b7hen", "sah", ",", "ist", "das", "wahr\u00b7haf\u00b7ti\u00b7ge", "licht", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "VVINF", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+---+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "(so weit die liebe sonne reicht, und an aller welt ende leucht.)", "tokens": ["(", "so", "weit", "die", "lie\u00b7be", "son\u00b7ne", "reicht", ",", "und", "an", "al\u00b7ler", "welt", "en\u00b7de", "leucht", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "APPR", "PIAT", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+----+--+", "measure": "iambic.hexa.chol"}}, "stanza.3": {"line.1": {"text": "Ihr wi\u00dft doch, lieben s\u00fcnderlein! durch wen wir absolviret seyn?", "tokens": ["Ihr", "wi\u00dft", "doch", ",", "lie\u00b7ben", "s\u00fcn\u00b7der\u00b7lein", "!", "durch", "wen", "wir", "ab\u00b7sol\u00b7vi\u00b7ret", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "PTKVZ", "$.", "APPR", "PWS", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1":