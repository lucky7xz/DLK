{"textgrid.poem.38257": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6n bin ich nicht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n bin ich nicht, mein h\u00f6chster Hort,", "tokens": ["Sch\u00f6n", "bin", "ich", "nicht", ",", "mein", "h\u00f6chs\u00b7ter", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df mich das nicht entgelten,", "tokens": ["La\u00df", "mich", "das", "nicht", "ent\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lieb gilt f\u00fcr sch\u00f6n an manchem Ort,", "tokens": ["Lieb", "gilt", "f\u00fcr", "sch\u00f6n", "an", "man\u00b7chem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lieb soll vor aller Sch\u00f6nheit gelten.", "tokens": ["Lieb", "soll", "vor", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sch\u00f6n bin ich nicht, acht das gar klein,", "tokens": ["Sch\u00f6n", "bin", "ich", "nicht", ",", "acht", "das", "gar", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "$,", "CARD", "PDS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lieb thut all Ding bezwingen,", "tokens": ["Lieb", "thut", "all", "Ding", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Lieb zwingt die Sch\u00f6nheit ganz allein,", "tokens": ["Lieb", "zwingt", "die", "Sch\u00f6n\u00b7heit", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kann sie allein besingen:", "tokens": ["Kann", "sie", "al\u00b7lein", "be\u00b7sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbihr findet in Geschichten", "tokens": ["\u00bb", "ihr", "fin\u00b7det", "in", "Ge\u00b7schich\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Vom Fisch Delphin genannt,", "tokens": ["Vom", "Fisch", "Del\u00b7phin", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Kein Netz h\u00e4lt ihn mit nichten,", "tokens": ["Kein", "Netz", "h\u00e4lt", "ihn", "mit", "nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und zieht ihn an das Land,", "tokens": ["Und", "zieht", "ihn", "an", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Allein durch lieblich Singen", "tokens": ["Al\u00b7lein", "durch", "lieb\u00b7lich", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Thut man ihn also zwingen,", "tokens": ["Thut", "man", "ihn", "al\u00b7so", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Da\u00df er kommt selbst ans Land.", "tokens": ["Da\u00df", "er", "kommt", "selbst", "ans", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Zum wunderbaren Zeichen", "tokens": ["Zum", "wun\u00b7der\u00b7ba\u00b7ren", "Zei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Auch die Waldv\u00f6gelein,", "tokens": ["Auch", "die", "Wald\u00b7v\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Ihr Herzelein erweichen", "tokens": ["Ihr", "Her\u00b7ze\u00b7lein", "er\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Einander insgemein,", "tokens": ["Ein\u00b7an\u00b7der", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Mit lieblichem Gesange,", "toke