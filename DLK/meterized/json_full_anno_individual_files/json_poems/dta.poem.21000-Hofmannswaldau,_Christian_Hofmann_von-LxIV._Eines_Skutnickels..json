{"dta.poem.21000": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxIV.  \n Eines Skutnickels.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Weichsel war mein Meer/ und Dantzig der", "tokens": ["Die", "Weich\u00b7sel", "war", "mein", "Meer", "/", "und", "Dant\u00b7zig", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "KON", "NE", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Welt Ende/", "tokens": ["Welt", "En\u00b7de", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da furchte man mein Maul/ und haste meine H\u00e4nde.", "tokens": ["Da", "furch\u00b7te", "man", "mein", "Maul", "/", "und", "has\u00b7te", "mei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich starb und war nicht recht in ", "tokens": ["Ich", "starb", "und", "war", "nicht", "recht", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "PTKNEG", "ADJD", "APPR"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "kommen/", "tokens": ["kom\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "So hatt\u2019 ich ihm alsbald den besten Rock genom-", "tokens": ["So", "hatt'", "ich", "ihm", "als\u00b7bald", "den", "bes\u00b7ten", "Rock", "ge\u00b7nom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "men.", "tokens": ["men", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}}}}