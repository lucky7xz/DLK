{"dta.poem.7053": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Allmacht, die im sch\u00f6nen Bogen,", "tokens": ["Die", "All\u00b7macht", ",", "die", "im", "sch\u00f6\u00b7nen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit welchem sie das Firmament \u00fcmzogen,", "tokens": ["Mit", "wel\u00b7chem", "sie", "das", "Fir\u00b7ma\u00b7ment", "\u00fcm\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den kleinen Tr\u00f6pfgen solchen Schein", "tokens": ["Den", "klei\u00b7nen", "Tr\u00f6pf\u00b7gen", "sol\u00b7chen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und solche Farben eingepr\u00e4get,", "tokens": ["Und", "sol\u00b7che", "Far\u00b7ben", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df es die Seelen selbst erreget;", "tokens": ["Da\u00df", "es", "die", "See\u00b7len", "selbst", "er\u00b7re\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sollte die nicht f\u00e4hig seyn,", "tokens": ["Wie", "soll\u00b7te", "die", "nicht", "f\u00e4\u00b7hig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Thron von Seiner Majest\u00e4t", "tokens": ["Den", "Thron", "von", "Sei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit solchen Farben auszuschm\u00fccken,", "tokens": ["Mit", "sol\u00b7chen", "Far\u00b7ben", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jhm solche Herrlichkeit und Strahlen einzudr\u00fccken;", "tokens": ["Jhm", "sol\u00b7che", "Herr\u00b7lich\u00b7keit", "und", "Strah\u00b7len", "ein\u00b7zu\u00b7dr\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die auch dem allerhellsten Scheine", "tokens": ["Die", "auch", "dem", "al\u00b7ler\u00b7hells\u00b7ten", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der allerreinsten Edelsteine,", "tokens": ["Der", "al\u00b7ler\u00b7reins\u00b7ten", "E\u00b7del\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Worin sie durch die Sonne gl\u00fchn,", "tokens": ["Wo\u00b7rin", "sie", "durch", "die", "Son\u00b7ne", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Viel tausendmahl noch vorzuziehn.", "tokens": ["Viel", "tau\u00b7send\u00b7mahl", "noch", "vor\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}