{"textgrid.poem.48177": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Maien-Fest", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder taucht ein Maientag", "tokens": ["Wie\u00b7der", "taucht", "ein", "Mai\u00b7en\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "aus des Winters Tiefen,", "tokens": ["aus", "des", "Win\u00b7ters", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "lockt ein heller Amselschlag", "tokens": ["lockt", "ein", "hel\u00b7ler", "Am\u00b7sel\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "alle, die noch schliefen.", "tokens": ["al\u00b7le", ",", "die", "noch", "schlie\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wieder steht das goldene Licht", "tokens": ["Wie\u00b7der", "steht", "das", "gol\u00b7de\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "hoch im Heiligtume \u2013", "tokens": ["hoch", "im", "Hei\u00b7lig\u00b7tu\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "aus der Dornenhecke bricht", "tokens": ["aus", "der", "Dor\u00b7nen\u00b7he\u00b7cke", "bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "eine Rosenblume.", "tokens": ["ei\u00b7ne", "Ro\u00b7sen\u00b7blu\u00b7me", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Maientag und Maienfest!", "tokens": ["Mai\u00b7en\u00b7tag", "und", "Mai\u00b7en\u00b7fest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Klagen schweigen.", "tokens": ["Uns\u00b7re", "Kla\u00b7gen", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unsre stolze Sehnsucht l\u00e4\u00dft", "tokens": ["Uns\u00b7re", "stol\u00b7ze", "Sehn\u00b7sucht", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ihre Banner steigen;", "tokens": ["ih\u00b7re", "Ban\u00b7ner", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "ihre Tauben fliegen aus,", "tokens": ["ih\u00b7re", "Tau\u00b7ben", "flie\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ihre Knospen springen \u2013", "tokens": ["ih\u00b7re", "Knos\u00b7pen", "sprin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kirschen bl\u00fchn um jedes Haus,", "tokens": ["Kir\u00b7schen", "bl\u00fchn", "um", "je\u00b7des", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Glocken klingen.", "tokens": ["und", "die", "Glo\u00b7cken", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht im morschen Kirchenturm, \u2013", "tokens": ["Nicht", "im", "mor\u00b7schen", "Kir\u00b7chen\u00b7turm", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "tief in Volkes Herzen", "tokens": ["tief", "in", "Vol\u00b7kes", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "wogt ein ganzer Glockensturm,", "tokens": ["wogt", "ein", "gan\u00b7zer", "Glo\u00b7cken\u00b7sturm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "l\u00e4utet Lust und Schmerzen.", "tokens": ["l\u00e4u\u00b7tet", "Lust", "und", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nicht vergangne gro\u00dfe Zeit", "tokens": ["Nicht", "ver\u00b7gang\u00b7ne", "gro\u00b7\u00dfe", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "k\u00fcndet unsre Feier,", "tokens": ["k\u00fcn\u00b7det", "uns\u00b7re", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "von der Zukunft Herrlichkeit", "tokens": ["von", "der", "Zu\u00b7kunft", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "heben wir den Schleier.", "tokens": ["he\u00b7ben", "wir", "den", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Unsre Ernten schauen wir,", "tokens": ["Uns\u00b7re", "Ern\u00b7ten", "schau\u00b7en", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "reife goldne Felder,", "tokens": ["rei\u00b7fe", "gold\u00b7ne", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "stolzer St\u00e4mme Fr\u00fcchtezier,", "tokens": ["stol\u00b7zer", "St\u00e4m\u00b7me", "Fr\u00fcch\u00b7te\u00b7zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "schattenk\u00fchle W\u00e4lder.", "tokens": ["schat\u00b7ten\u00b7k\u00fch\u00b7le", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hunde nicht, die duckend sich,", "tokens": ["Hun\u00b7de", "nicht", ",", "die", "du\u00b7ckend", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PRELS", "ADJD", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "scheu am Boden schleichen:", "tokens": ["scheu", "am", "Bo\u00b7den", "schlei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Menschen, welche br\u00fcderlich", "tokens": ["Men\u00b7schen", ",", "wel\u00b7che", "br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "sich die H\u00e4nde reichen.", "tokens": ["sich", "die", "H\u00e4n\u00b7de", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Von den H\u00f6hen ringsherum", "tokens": ["Von", "den", "H\u00f6\u00b7hen", "rings\u00b7he\u00b7rum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "will ein Echo klingen;", "tokens": ["will", "ein", "E\u00b7cho", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "t\u00f6nend wird, was rauh und stumm,", "tokens": ["t\u00f6\u00b7nend", "wird", ",", "was", "rauh", "und", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lahmen wachsen Schwingen.", "tokens": ["Lah\u00b7men", "wach\u00b7sen", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Blinde Augen werden wach,", "tokens": ["Blin\u00b7de", "Au\u00b7gen", "wer\u00b7den", "wach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "schaun in blaue Weiten,", "tokens": ["schaun", "in", "blau\u00b7e", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "sehn den gro\u00dfen Fr\u00fchlingstag", "tokens": ["sehn", "den", "gro\u00b7\u00dfen", "Fr\u00fch\u00b7lings\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "durch die Lande schreiten.", "tokens": ["durch", "die", "Lan\u00b7de", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Maienfest und Maienlust! \u2013", "tokens": ["Mai\u00b7en\u00b7fest", "und", "Mai\u00b7en\u00b7lust", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Axt und Hammer ruhen \u2013", "tokens": ["Axt", "und", "Ham\u00b7mer", "ru\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "und der Alltag, schwarz beru\u00dft,", "tokens": ["und", "der", "All\u00b7tag", ",", "schwarz", "be\u00b7ru\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "geht in seidnen Schuhen.", "tokens": ["geht", "in", "seid\u00b7ne