, "Lied", "der", "Welt", "Ent\u00b7sa\u00b7gung", "lei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Ekel flutend durch die Seele schwillt.", "tokens": ["Und", "E\u00b7kel", "flu\u00b7tend", "durch", "die", "See\u00b7le", "schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "So zucken Lippen, wenn die Seele schreit,", "tokens": ["So", "zu\u00b7cken", "Lip\u00b7pen", ",", "wenn", "die", "See\u00b7le", "schreit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach einem Rausch, einem Gl\u00fcck, einem Glanz!", "tokens": ["Nach", "ei\u00b7nem", "Rausch", ",", "ei\u00b7nem", "Gl\u00fcck", ",", "ei\u00b7nem", "Glanz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und was in mir schl\u00e4ft, verklungen, weit, so weit,", "tokens": ["Und", "was", "in", "mir", "schl\u00e4ft", ",", "ver\u00b7klun\u00b7gen", ",", "weit", ",", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "VVFIN", "$,", "VVPP", "$,", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Das regt sich erwachend in schmerzlichem Tanz.", "tokens": ["Das", "regt", "sich