{"textgrid.poem.24667": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mann sitzt dort am Weg schon lang, so lang;", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann sitzt dort am Weg schon lang, so lang;", "tokens": ["Der", "Mann", "sitzt", "dort", "am", "Weg", "schon", "lang", ",", "so", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ich bin so m\u00fcd, und ich schliche so gern mich fort,", "tokens": ["Und", "ich", "bin", "so", "m\u00fcd", ",", "und", "ich", "schli\u00b7che", "so", "gern", "mich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "PPER", "VVFIN", "ADV", "ADV", "PPER", "PTKVZ", "$,"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und es h\u00e4lt mich sein Blick mit leisem, festem Zwang", "tokens": ["Und", "es", "h\u00e4lt", "mich", "sein", "Blick", "mit", "lei\u00b7sem", ",", "fes\u00b7tem", "Zwang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und mir ist, als m\u00fc\u00dft ich ihm sagen ein Wort ... und mir fehlt das Wort!", "tokens": ["Und", "mir", "ist", ",", "als", "m\u00fc\u00dft", "ich", "ihm", "sa\u00b7gen", "ein", "Wort", "...", "und", "mir", "fehlt", "das", "Wort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}}, "stanza.2": {"line.1": {"text": "Es d\u00e4mmert. Drau\u00dfen klirrt und rauscht die Stadt.", "tokens": ["Es", "d\u00e4m\u00b7mert", ".", "Drau\u00b7\u00dfen", "klirrt", "und", "rauscht", "die", "Stadt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Steine qualmen. Es ist dumpf und schw\u00fcl.", "tokens": ["Die", "Stei\u00b7ne", "qual\u00b7men", ".", "Es", "ist", "dumpf", "und", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Werktag geht zur Neige, schlurfenden Schritts und matt.", "tokens": ["Der", "Werk\u00b7tag", "geht", "zur", "Nei\u00b7ge", ",", "schlur\u00b7fen\u00b7den", "Schritts", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hier aber, im Garten, ists leer und feucht und k\u00fchl.", "tokens": ["Hier", "a\u00b7ber", ",", "im", "Gar\u00b7ten", ",", "ists", "leer", "und", "feucht", "und", "k\u00fchl", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPRART", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Jetzt steht er auf, der hagre alte Mann.", "tokens": ["Jetzt", "steht", "er", "auf", ",", "der", "hag\u00b7re", "al\u00b7te", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nein, nein, noch nicht ... Was schl\u00e4ft nur in den Augen,", "tokens": ["Nein", ",", "nein", ",", "noch", "nicht", "...", "Was", "schl\u00e4ft", "nur", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "ADV", "PTKNEG", "$(", "PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den m\u00fcdverschleierten ... mich h\u00e4lt ihr Bann ...", "tokens": ["Den", "m\u00fcd\u00b7ver\u00b7schlei\u00b7er\u00b7ten", "...", "mich", "h\u00e4lt", "ihr", "Bann", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sie die Kraft mir aus der Seele saugen?", "tokens": ["Da\u00df", "sie", "die", "Kraft", "mir", "aus", "der", "See\u00b7le", "sau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So d\u00e4mmern Augen, die der Tod umschleiert,", "tokens": ["So", "d\u00e4m\u00b7mern", "Au\u00b7gen", ",", "die", "der", "Tod", "um\u00b7schlei\u00b7ert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der langsame, der aus dem Leben quillt,", "tokens": ["Der", "lang\u00b7sa\u00b7me", ",", "der", "aus", "dem", "Le\u00b7ben", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indes das Lied der Welt Entsagung leiert", "tokens": ["In\u00b7des", "das", "Lied", "der", "Welt", "Ent\u00b7sa\u00b7gung", "lei\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Ekel flutend durch die Seele schwillt.", "tokens": ["Und", "E\u00b7kel", "flu\u00b7tend", "durch", "die", "See\u00b7le", "schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So zucken Lippen, wenn die Seele schreit,", "tokens": ["So", "zu\u00b7cken", "Lip\u00b7pen", ",", "wenn", "die", "See\u00b7le", "schreit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach einem Rausch, einem Gl\u00fcck, einem Glanz!", "tokens": ["Nach", "ei\u00b7nem", "Rausch", ",", "ei\u00b7nem", "Gl\u00fcck", ",", "ei\u00b7nem", "Glanz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und was in mir schl\u00e4ft, verklungen, weit, so weit,", "tokens": ["Und", "was", "in", "mir", "schl\u00e4ft", ",", "ver\u00b7klun\u00b7gen", ",", "weit", ",", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "VVFIN", "$,", "VVPP", "$,", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Das regt sich erwachend in schmerzlichem Tanz.", "tokens": ["Das", "regt", "sich", "er\u00b7wa\u00b7chend", "in", "schmerz\u00b7li\u00b7chem", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "So zucken Lippen, wenn zu oft betrogen", "tokens": ["So", "zu\u00b7cken", "Lip\u00b7pen", ",", "wenn", "zu", "oft", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "KOUS", "PTKA", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mi\u00dftrauisch jedes Wort im Innern lauert,", "tokens": ["Mi\u00df\u00b7trau\u00b7isch", "je\u00b7des", "Wort", "im", "In\u00b7nern", "lau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn, die einst fl\u00fcgelschlagend ausgeflogen,", "tokens": ["Wenn", ",", "die", "einst", "fl\u00fc\u00b7gel\u00b7schla\u00b7gend", "aus\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Seele frierend jetzt zusammenkauert.", "tokens": ["Die", "See\u00b7le", "frie\u00b7rend", "jetzt", "zu\u00b7sam\u00b7men\u00b7kau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Setz dich zu ihm und h\u00f6r dem Atmen zu,", "tokens": ["Setz", "dich", "zu", "ihm", "und", "h\u00f6r", "dem", "At\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie das gepre\u00dft, versch\u00fcchtert durch die Brust ihm schleicht,", "tokens": ["Wie", "das", "ge\u00b7pre\u00dft", ",", "ver\u00b7sch\u00fcch\u00b7tert", "durch", "die", "Brust", "ihm", "schleicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch st\u00f6r ihn nicht, er sehnt sich so nach Ruh ...", "tokens": ["Doch", "st\u00f6r", "ihn", "nicht", ",", "er", "sehnt", "sich", "so", "nach", "Ruh", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PRF", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nah ihm leise, er mi\u00dftraut dir leicht ...", "tokens": ["Und", "nah", "ihm", "lei\u00b7se", ",", "er", "mi\u00df\u00b7traut", "dir", "leicht", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Kennt ihr den Mann? Nicht wahr, ihr kennt ihn nicht?", "tokens": ["Kennt", "ihr", "den", "Mann", "?", "Nicht", "wahr", ",", "ihr", "kennt", "ihn", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den alten Mann mit seiner scheuen Pein,", "tokens": ["Den", "al\u00b7ten", "Mann", "mit", "sei\u00b7ner", "scheu\u00b7en", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und doch tr\u00e4gt dies selbe vergr\u00e4mte Gesicht", "tokens": ["Und", "doch", "tr\u00e4gt", "dies", "sel\u00b7be", "ver\u00b7gr\u00e4m\u00b7te", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PDS", "ADJA", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der eure auch, gehauen aus wei\u00dfem Stein.", "tokens": ["Der", "eu\u00b7re", "auch", ",", "ge\u00b7hau\u00b7en", "aus", "wei\u00b7\u00dfem", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADV", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Doch um ihn schimmert, den er t\u00f6nend schuf,", "tokens": ["Doch", "um", "ihn", "schim\u00b7mert", ",", "den", "er", "t\u00f6\u00b7nend", "schuf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der marmorwei\u00dfen Geisteskinder Chor,", "tokens": ["Der", "mar\u00b7mor\u00b7wei\u00b7\u00dfen", "Geis\u00b7tes\u00b7kin\u00b7der", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seines Genius reichumkr\u00e4nzter Ruf", "tokens": ["Und", "sei\u00b7nes", "Ge\u00b7nius", "reich\u00b7um\u00b7kr\u00e4nz\u00b7ter", "Ruf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schl\u00e4gt tausendz\u00fcngig heut an jedes Ohr.", "tokens": ["Schl\u00e4gt", "tau\u00b7send\u00b7z\u00fcn\u00b7gig", "heut", "an", "je\u00b7des", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Das ist, was wahllos diese Welt verleiht,", "tokens": ["Das", "ist", ",", "was", "wahl\u00b7los", "die\u00b7se", "Welt", "ver\u00b7leiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "ADJD", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was tosend durch das Reich der Zeiten wallt;", "tokens": ["Was", "to\u00b7send", "durch", "das", "Reich", "der", "Zei\u00b7ten", "wallt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Namens hallende Unsterblichkeit,", "tokens": ["Des", "Na\u00b7mens", "hal\u00b7len\u00b7de", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie Erz so unverg\u00e4nglich und so kalt.", "tokens": ["Wie", "Erz", "so", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "und", "so", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Der Name, den der Enkel sinnlos nennt,", "tokens": ["Der", "Na\u00b7me", ",", "den", "der", "En\u00b7kel", "sinn\u00b7los", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie wir Vergangnes sinnlos mit uns tragen,", "tokens": ["Wie", "wir", "Ver\u00b7gang\u00b7nes", "sinn\u00b7los", "mit", "uns", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Formelwahn, der ehrt, was er nicht kennt:", "tokens": ["Der", "For\u00b7mel\u00b7wahn", ",", "der", "ehrt", ",", "was", "er", "nicht", "kennt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "