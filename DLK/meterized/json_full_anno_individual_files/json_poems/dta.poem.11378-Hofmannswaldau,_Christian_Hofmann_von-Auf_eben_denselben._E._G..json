{"dta.poem.11378": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eben denselben.  \n E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje fische, die der strom am ufer ausgelegt,", "tokens": ["Wie", "fi\u00b7sche", ",", "die", "der", "strom", "am", "u\u00b7fer", "aus\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach d\u00fcnner k\u00fchlungs-lufft mit vollem halse schnappen;", "tokens": ["Nach", "d\u00fcn\u00b7ner", "k\u00fch\u00b7lungs\u00b7lufft", "mit", "vol\u00b7lem", "hal\u00b7se", "schnap\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sind wir, wenn der wind der hoffnung uns bewegt:", "tokens": ["So", "sind", "wir", ",", "wenn", "der", "wind", "der", "hoff\u00b7nung", "uns", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir m\u00fchn uns, rauch und dampf statt wollust zu ertappen.", "tokens": ["Wir", "m\u00fchn", "uns", ",", "rauch", "und", "dampf", "statt", "wol\u00b7lust", "zu", "er\u00b7tap\u00b7pen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "ADJD", "KON", "PAV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den schliest der liebes-wurm in thorheits-fessel ein:", "tokens": ["Den", "schliest", "der", "lie\u00b7bes\u00b7wurm", "in", "thor\u00b7heits\u00b7fes\u00b7sel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Midas will sich nur am koth von golde freun:", "tokens": ["Ein", "Mi\u00b7das", "will", "sich", "nur", "am", "koth", "von", "gol\u00b7de", "freun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "PTKA", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die aber, so hievon nicht brillen-glas bekommen,", "tokens": ["Die", "a\u00b7ber", ",", "so", "hie\u00b7von", "nicht", "bril\u00b7len\u00b7glas", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADV", "PAV", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die werden von dem schein der ehren eingenommen.", "tokens": ["Die", "wer\u00b7den", "von", "dem", "schein", "der", "eh\u00b7ren", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bald schicket man die lust mit v\u00f6geln durch die lufft:", "tokens": ["Bald", "schi\u00b7cket", "man", "die", "lust", "mit", "v\u00f6\u00b7geln", "durch", "die", "lufft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem soll ein hund und pferd vergn\u00fcgungs-zucker schaffen:", "tokens": ["Dem", "soll", "ein", "hund", "und", "pferd", "ver\u00b7gn\u00fc\u00b7gungs\u00b7zu\u00b7cker", "schaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn das j\u00e4ger-horn uns keine hasen rufft,", "tokens": ["Und", "wenn", "das", "j\u00e4\u00b7ger\u00b7horn", "uns", "kei\u00b7ne", "ha\u00b7sen", "rufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So h\u00e4ngen wir uns gar an katzen und an affen.", "tokens": ["So", "h\u00e4n\u00b7gen", "wir", "uns", "gar", "an", "kat\u00b7zen", "und", "an", "af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In summa, jeder sucht ein fremdes element,", "tokens": ["In", "sum\u00b7ma", ",", "je\u00b7der", "sucht", "ein", "frem\u00b7des", "e\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bi\u00df man, wie motten, sich am lichte selbst verbrennt;", "tokens": ["Bi\u00df", "man", ",", "wie", "mot\u00b7ten", ",", "sich", "am", "lich\u00b7te", "selbst", "ver\u00b7brennt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWAV", "VMFIN", "$,", "PRF", "APPRART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo aber Thales uns soll weise lehren geben:", "tokens": ["Wo", "a\u00b7ber", "Tha\u00b7les", "uns", "soll", "wei\u00b7se", "leh\u00b7ren", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PPER", "VMFIN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird die sch\u00f6nste lust bey k\u00fchlen brunnen leben.", "tokens": ["So", "wird", "die", "sch\u00f6ns\u00b7te", "lust", "bey", "k\u00fch\u00b7len", "brun\u00b7nen", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus brunnen quillt der leim, durch den die welt gebaut:", "tokens": ["Aus", "brun\u00b7nen", "quillt", "der", "leim", ",", "durch", "den", "die", "welt", "ge\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm brunnen liegt das bild der sterblichen begraben;", "tokens": ["Jm", "brun\u00b7nen", "liegt", "das", "bild", "der", "sterb\u00b7li\u00b7chen", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer da sein conterfait nicht wie Act\u00e4on schaut,", "tokens": ["Wer", "da", "sein", "con\u00b7ter\u00b7fait", "nicht", "wie", "Ac\u00b7t\u00e4on", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "PTKNEG", "KOKOM", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der kan nicht an sich selbst verdru\u00df und eckel haben:", "tokens": ["Der", "kan", "nicht", "an", "sich", "selbst", "ver\u00b7dru\u00df", "und", "ec\u00b7kel", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "APPR", "PRF", "ADV", "NN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn k\u00f6nigs-t\u00f6chter auch nach tieffen quellen sehn,", "tokens": ["Wenn", "k\u00f6\u00b7nigs\u00b7t\u00f6ch\u00b7ter", "auch", "nach", "tief\u00b7fen", "quel\u00b7len", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie zu Fontainebleau von Claudia geschehn:", "tokens": ["Wie", "zu", "Fon\u00b7tai\u00b7ne\u00b7ble\u00b7au", "von", "Clau\u00b7dia", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "APPR", "NE", "VVPP", "$."], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So mag man den mit recht zum ", "tokens": ["So", "mag", "man", "den", "mit", "recht", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ART", "APPR", "ADJD", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der nichts beliebtes sieht aus Thetis adern lauffen.", "tokens": ["Der", "nichts", "be\u00b7lieb\u00b7tes", "sieht", "aus", "The\u00b7tis", "a\u00b7dern", "lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJA", "VVFIN", "APPR", "NE", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Di\u00df hat der kluge geist desjenigen bedacht,", "tokens": ["Di\u00df", "hat", "der", "klu\u00b7ge", "geist", "des\u00b7je\u00b7ni\u00b7gen", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der einst Minervens bild lie\u00df in das wasser stellen,", "tokens": ["Der", "einst", "Mi\u00b7ner\u00b7vens", "bild", "lie\u00df", "in", "das", "was\u00b7ser", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "NE", "VVFIN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wobey der pinsel noch viel artig zeug gemacht,", "tokens": ["Wo\u00b7bey", "der", "pin\u00b7sel", "noch", "viel", "ar\u00b7tig", "zeug", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit dieser \u00fcberschrifft: Hier mu\u00df vergn\u00fcgung quellen.", "tokens": ["Mit", "die\u00b7ser", "\u00fc\u00b7bersc\u00b7hrifft", ":", "Hier", "mu\u00df", "ver\u00b7gn\u00fc\u00b7gung", "quel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVPP", "$.", "ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gewi\u00df! hat wo ein bild ein ding recht ausgedr\u00fcckt,", "tokens": ["Ge\u00b7wi\u00df", "!", "hat", "wo", "ein", "bild", "ein", "ding", "recht", "aus\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VAFIN", "PWAV", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ist es dieser hand vor andern wohl gegl\u00fcckt.", "tokens": ["So", "ist", "es", "die\u00b7ser", "hand", "vor", "an\u00b7dern", "wohl", "ge\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn seht: Die wei\u00dfheit ist aus liebendem verlangen", "tokens": ["Denn", "seht", ":", "Die", "wei\u00df\u00b7heit", "ist", "aus", "lie\u00b7ben\u00b7dem", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der menschen schimmer auch ins wasser nachgegangen.", "tokens": ["Der", "men\u00b7schen", "schim\u00b7mer", "auch", "ins", "was\u00b7ser", "nach\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jhr brunnen! die ihr klar aus erd und felsen steigt,", "tokens": ["Ihr", "brun\u00b7nen", "!", "die", "ihr", "klar", "aus", "erd", "und", "fel\u00b7sen", "steigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PRELS", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr m\u00f6gt, wie Castalis, mit eurem glantze pralen;", "tokens": ["Ihr", "m\u00f6gt", ",", "wie", "Cas\u00b7ta\u00b7lis", ",", "mit", "eu\u00b7rem", "glant\u00b7ze", "pra\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "NE", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der quell, wo sich ihr schein polirten augen zeigt,", "tokens": ["Der", "quell", ",", "wo", "sich", "ihr", "schein", "po\u00b7lir\u00b7ten", "au\u00b7gen", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PRF", "PPOSAT", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan auch erystall beschimpfft mit rothen flecken mahlen.", "tokens": ["Kan", "auch", "e\u00b7rys\u00b7tall", "be\u00b7schimpfft", "mit", "ro\u00b7then", "fle\u00b7cken", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Drum kommt Misanthropus f\u00fcrwahr erb\u00e4rmlich blind,", "tokens": ["Drum", "kommt", "Mi\u00b7san\u00b7thro\u00b7pus", "f\u00fcr\u00b7wahr", "er\u00b7b\u00e4rm\u00b7lich", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn er auf tr\u00fcben koth verfaulter grillen sinnt;", "tokens": ["Wenn", "er", "auf", "tr\u00fc\u00b7ben", "koth", "ver\u00b7faul\u00b7ter", "gril\u00b7len", "sinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und der wird nimmermehr von ihrem nectar lecken,", "tokens": ["Und", "der", "wird", "nim\u00b7mer\u00b7mehr", "von", "ih\u00b7rem", "nec\u00b7tar", "le\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der sich an pf\u00fctzen h\u00e4lt, wo fr\u00f6sch und kr\u00f6ten hecken.", "tokens": ["Der", "sich", "an", "pf\u00fct\u00b7zen", "h\u00e4lt", ",", "wo", "fr\u00f6sch", "und", "kr\u00f6\u00b7ten", "he\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "$,", "PWAV", "ADJD", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Zwar st\u00fcmper, die den weg zu hohen bergen fliehn,", "tokens": ["Zwar", "st\u00fcm\u00b7per", ",", "die", "den", "weg", "zu", "ho\u00b7hen", "ber\u00b7gen", "fliehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "ART", "ADV", "APPR", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die m\u00fcssen freylich nur im kothe sitzen bleiben;", "tokens": ["Die", "m\u00fcs\u00b7sen", "frey\u00b7lich", "nur", "im", "ko\u00b7the", "sit\u00b7zen", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein, wo geist und muth sich mit geduld bem\u00fchn,", "tokens": ["Al\u00b7lein", ",", "wo", "geist", "und", "muth", "sich", "mit", "ge\u00b7duld", "be\u00b7m\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVPP", "KON", "NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da l\u00e4st man sich nicht leicht von seinem zwecke treiben;", "tokens": ["Da", "l\u00e4st", "man", "sich", "nicht", "leicht", "von", "sei\u00b7nem", "zwe\u00b7cke", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn der Hippocrates und seiner s\u00f6hne schaar", "tokens": ["Denn", "der", "Hip\u00b7po\u00b7cra\u00b7tes", "und", "sei\u00b7ner", "s\u00f6h\u00b7ne", "schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Macht t\u00e4glich diesen schlu\u00df durch die erfahrung war:", "tokens": ["Macht", "t\u00e4g\u00b7lich", "die\u00b7sen", "schlu\u00df", "durch", "die", "er\u00b7fah\u00b7rung", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PDAT", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df brunnen, die sich hoch durch berg und klippen heben,", "tokens": ["Da\u00df", "brun\u00b7nen", ",", "die", "sich", "hoch", "durch", "berg", "und", "klip\u00b7pen", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PRF", "ADJD", "APPR", "NE", "KON", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem, der sie oben sucht, das beste wasser geben.", "tokens": ["Dem", ",", "der", "sie", "o\u00b7ben", "sucht", ",", "das", "bes\u00b7te", "was\u00b7ser", "ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der brunn des Plinius nahm t\u00e4glich ab und zu;", "tokens": ["Der", "brunn", "des", "Pli\u00b7nius", "nahm", "t\u00e4g\u00b7lich", "ab", "und", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hier sieht der anmuths-quell zu keinen zeiten stille,", "tokens": ["Hier", "sieht", "der", "an\u00b7muths\u00b7quell", "zu", "kei\u00b7nen", "zei\u0