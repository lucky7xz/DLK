: "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja, wie die lahmen sonst in warme b\u00e4der hincken,", "tokens": ["Ja", ",", "wie", "die", "lah\u00b7men", "sonst", "in", "war\u00b7me", "b\u00e4\u00b7der", "hin\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "ADJA", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So kan der geist sich hier gesund und weise trincken.", "tokens": ["So", "kan", "der", "geist", "sich", "hier", "ge\u00b7sund", "und", "wei\u00b7se", "trin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und wen ein paradies mit gr\u00fcner lust ergetzt,", "tokens": ["Und", "wen", "ein", "pa\u00b7ra\u00b7dies", "mit", "gr\u00fc\u00b7ner", "lust", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der kan den himmel selbst um diese gegend finden:", "tokens": ["Der", "kan", "den", "him\u00b7mel", "selbst", "um", "die\u00b7se", "ge\u00b7gend", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo sich ein gantzer wald von k\u00fcnsten hingesetzt:", "tokens": ["Wo", "sich", "ein", "gant\u00b7zer", "wald", "von", "k\u00fcns\u00b7ten", "hin\u00b7ge\u00b7setzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wo vier str\u00f6me sich an einen ursprung binden:", "tokens": ["Und", "wo", "vier", "str\u00f6\u00b7me", "sich", "an", "ei\u00b7nen", "ur\u00b7sprung", "bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "CARD", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo liebe, die der kern und grund der tugend ist,", "tokens": ["Wo", "lie\u00b7be", ",", "die", "der", "kern", "und", "grund", "der", "tu\u00b7gend", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PRELS", "ART", "NN", "KON", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit klugheit untermengt durch die can\u00e4le fliest:", "tokens": ["Mit", "klug\u00b7heit", "un\u00b7ter\u00b7mengt", "durch", "die", "ca\u00b7n\u00e4\u00b7le", "fliest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da wo die tapferkeit l\u00e4st frische str\u00f6me schiessen,", "tokens": ["Da", "wo", "die", "tap\u00b7fer\u00b7keit", "l\u00e4st", "fri\u00b7sche", "str\u00f6\u00b7me", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die mit bescheidenheit zuletzt in einen fliessen.", "tokens": ["Die", "mit", "be\u00b7schei\u00b7den\u00b7heit", "zu\u00b7letzt", "in", "ei\u00b7nen", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Geehrter Weissenborn! den heut der weisen rath", "tokens": ["Ge\u00b7ehr\u00b7ter", "Weis\u00b7sen\u00b7born", "!", "den", "heut", "der", "wei\u00b7sen", "rath"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An ihre seite setzt, dich mu\u00df man hier befragen:", "tokens": ["An", "ih\u00b7re", "sei\u00b7te", "setzt", ",", "dich", "mu\u00df", "man", "hier", "be\u00b7fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich, den der wei\u00dfheits-brunn bi\u00dfher vergn\u00fcget hat.", "tokens": ["Dich", ",", "den", "der", "wei\u00df\u00b7heits\u00b7brunn", "bi\u00df\u00b7her", "ver\u00b7gn\u00fc\u00b7get", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "ADV", "VVPP", "VAFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Du wirst ein frohes ja zu unsern worten sagen;", "tokens": ["Du", "wirst", "ein", "fro\u00b7hes", "ja", "zu", "un\u00b7sern", "wor\u00b7ten", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wer der weisen schrifft so hurtig durchgesucht,", "tokens": ["Denn", "wer", "der", "wei\u00b7sen", "schrifft", "so", "hur\u00b7tig", "durch\u00b7ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "VVFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, was nach thorheit schmeckt, mit recht und ernst verflucht:", "tokens": ["Und", ",", "was", "nach", "thor\u00b7heit", "schmeckt", ",", "mit", "recht", "und", "ernst", "ver\u00b7flucht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,", "APPR", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja wen die Gratien sich selber zugesellen,", "tokens": ["Ja", "wen", "die", "Gra\u00b7ti\u00b7en", "sich", "sel\u00b7ber", "zu\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der wird den besten schlu\u00df von diesem brunnen f\u00e4llen.", "tokens": ["Der", "wird", "den", "bes\u00b7ten", "schlu\u00df", "von", "die\u00b7sem", "brun\u00b7nen", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wohlan! so quelle denn dein wei\u00dfheits-brunn stets klar,", "tokens": ["Wo\u00b7hlan", "!", "so", "quel\u00b7le", "denn", "dein", "wei\u00df\u00b7heits\u00b7brunn", "stets", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zeige dir den weg zu h\u00f6hern ehren-b\u00fchnen!", "tokens": ["Und", "zei\u00b7ge", "dir", "den", "weg", "zu", "h\u00f6\u00b7hern", "eh\u00b7ren\u00b7b\u00fch\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der abflu\u00df stelle sich zu keinen zeiten dar,", "tokens": ["Der", "ab\u00b7flu\u00df", "stel\u00b7le", "sich", "zu", "kei\u00b7nen", "zei\u00b7ten", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit du kanst der welt mit raren fr\u00fcchten dienen!", "tokens": ["Da\u00b7mit", "du", "kanst", "der", "welt", "mit", "ra\u00b7ren", "fr\u00fcch\u00b7ten", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NN", "APPR", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Uns aber, die dein gl\u00fcck, gleichwie dich selbst, ergetzt,", "tokens": ["Uns", "a\u00b7ber", ",", "die", "dein", "gl\u00fcck", ",", "gleich\u00b7wie", "dich", "selbst", ",", "er\u00b7getzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPOSAT", "NN", "$,", "KON", "PPER", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00df, wenn der himmel es noch k\u00fcnfftig fester setzt,", "tokens": ["La\u00df", ",", "wenn", "der", "him\u00b7mel", "es", "noch", "k\u00fcnff\u00b7tig", "fes\u00b7ter", "setzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wenn dir gl\u00fcck und zeit was mehrers wird bestellen,", "tokens": ["Und", "wenn", "dir", "gl\u00fcck", "und", "zeit", "was", "meh\u00b7rers", "wird", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "KON", "NN", "PWS", "PIS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus deinem freundschaffts-brunn vergn\u00fcgungs-str\u00f6me quellen!", "tokens": ["Aus", "dei\u00b7nem", "freund\u00b7schaffts\u00b7brunn", "ver\u00b7gn\u00fc\u00b7gungs\u00b7str\u00f6\u00b7me", "quel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}