{"dta.poem.7469": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir haben, was auch eine Sage schreibe,", "tokens": ["Wir", "ha\u00b7ben", ",", "was", "auch", "ei\u00b7ne", "Sa\u00b7ge", "schrei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Funken des Prometheus nicht gepachtet;", "tokens": ["Den", "Fun\u00b7ken", "des", "Pro\u00b7me\u00b7theus", "nicht", "ge\u00b7pach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So tief wir unter uns das Weib geachtet,", "tokens": ["So", "tief", "wir", "un\u00b7ter", "uns", "das", "Weib", "ge\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die reinste Flamme wohnt in ", "tokens": ["Die", "reins\u00b7te", "Flam\u00b7me", "wohnt", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wer dem selbstisch frostigen Getreibe,", "tokens": ["Und", "wer", "dem", "selbs\u00b7tisch", "fros\u00b7ti\u00b7gen", "Ge\u00b7trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ihm des Herzens liebste Kinder schlachtet,", "tokens": ["Das", "ihm", "des", "Her\u00b7zens", "liebs\u00b7te", "Kin\u00b7der", "schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer dieser K\u00e4lte zu entrinnen trachtet,", "tokens": ["Wer", "die\u00b7ser", "K\u00e4l\u00b7te", "zu", "ent\u00b7rin\u00b7nen", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo fl\u00f6h' er hin, als zu dem treuen Weibe?", "tokens": ["Wo", "fl\u00f6h'", "er", "hin", ",", "als", "zu", "dem", "treu\u00b7en", "Wei\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Felsen ist der Mann, der nur ergl\u00fct,", "tokens": ["Ein", "Fel\u00b7sen", "ist", "der", "Mann", ",", "der", "nur", "er\u00b7gl\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn trotzig er gen Himmel sich erhoben,", "tokens": ["Wenn", "trot\u00b7zig", "er", "gen", "Him\u00b7mel", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur\u00fcck ihm schleudernd seiner Sonne Strahlen;", "tokens": ["Zu\u00b7r\u00fcck", "ihm", "schleu\u00b7dernd", "sei\u00b7ner", "Son\u00b7ne", "Strah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein stiller See des Weibes weich Gem\u00fct,", "tokens": ["Ein", "stil\u00b7ler", "See", "des", "Wei\u00b7bes", "weich", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das fromm in sich empf\u00e4ngt das Licht von Oben,", "tokens": ["Das", "fromm", "in", "sich", "emp\u00b7f\u00e4ngt", "das", "Licht", "von", "O\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PRF", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drin sich die Himmel himmlischer noch malen.", "tokens": ["Drin", "sich", "die", "Him\u00b7mel", "himm\u00b7li\u00b7scher", "noch", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}