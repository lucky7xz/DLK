{"dta.poem.20045": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An L. U.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als wir schieden, da war's am Himmel st\u00fcrmisch und tr\u00fcbe,", "tokens": ["Als", "wir", "schie\u00b7den", ",", "da", "wa\u00b7r's", "am", "Him\u00b7mel", "st\u00fcr\u00b7misch", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "APPRART", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Lag die Erde so kalt, schwiegen die V\u00f6gel im Thal.", "tokens": ["Lag", "die", "Er\u00b7de", "so", "kalt", ",", "schwie\u00b7gen", "die", "V\u00f6\u00b7gel", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jahre schwanden inde\u00df, noch st\u00fcrmt mir immer der Himmel,", "tokens": ["Jah\u00b7re", "schwan\u00b7den", "in\u00b7de\u00df", ",", "noch", "st\u00fcrmt", "mir", "im\u00b7mer", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Liegt die Erde mir kalt, singt mir kein Vogel im Thal.", "tokens": ["Liegt", "die", "Er\u00b7de", "mir", "kalt", ",", "singt", "mir", "kein", "Vo\u00b7gel", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}