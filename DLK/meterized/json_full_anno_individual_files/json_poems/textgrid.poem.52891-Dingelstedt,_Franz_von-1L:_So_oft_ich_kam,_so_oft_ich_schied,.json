{"textgrid.poem.52891": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So oft ich kam, so oft ich schied,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft ich kam, so oft ich schied,", "tokens": ["So", "oft", "ich", "kam", ",", "so", "oft", "ich", "schied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dieselben alten Gesichter,", "tokens": ["Die\u00b7sel\u00b7ben", "al\u00b7ten", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Immer das n\u00e4mliche dumme Lied:", "tokens": ["Im\u00b7mer", "das", "n\u00e4m\u00b7li\u00b7che", "dum\u00b7me", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Bewahrt das Feu'r und die Lichter!", "tokens": ["Be\u00b7wahrt", "das", "Feu'r", "und", "die", "Lich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "F\u00fcrwahr, das halt' ein anderer aus,", "tokens": ["F\u00fcr\u00b7wahr", ",", "das", "halt'", "ein", "an\u00b7de\u00b7rer", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "I