{"dta.poem.11598": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00f6nig bei der Kr\u00f6nung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "O Vaterland! wie bin ich dein!", "tokens": ["O", "Va\u00b7ter\u00b7land", "!", "wie", "bin", "ich", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df f\u00fcr das Rechte mich und Wahre", "tokens": ["La\u00df", "f\u00fcr", "das", "Rech\u00b7te", "mich", "und", "Wah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun Priester oder Opfer seyn!", "tokens": ["Nun", "Pries\u00b7ter", "o\u00b7der", "Op\u00b7fer", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geu\u00df auf mein Haupt, Herr! deine Schale,", "tokens": ["Geu\u00df", "auf", "mein", "Haupt", ",", "Herr", "!", "dei\u00b7ne", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein k\u00f6stlich Oel des Friedens, aus,", "tokens": ["Ein", "k\u00f6st\u00b7lich", "O\u00b7el", "des", "Frie\u00b7dens", ",", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df ich wie eine Sonne strahle", "tokens": ["Da\u00df", "ich", "wie", "ei\u00b7ne", "Son\u00b7ne", "strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Vaterland und meinem Haus!", "tokens": ["Dem", "Va\u00b7ter\u00b7land", "und", "mei\u00b7nem", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}