{"dta.poem.19075": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LvI.   Auf den grossen Evangelisten, August  \n Hermann Francken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer legt mein Sinn sich zu den F\u00fcssen nieder,", "tokens": ["Hjer", "legt", "mein", "Sinn", "sich", "zu", "den", "F\u00fcs\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ich vordem mit Thr\u00e4nen starck benetzt,", "tokens": ["Die", "ich", "vor\u00b7dem", "mit", "Thr\u00e4\u00b7nen", "starck", "be\u00b7netzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sich der Geist mit dem Gesetz der Glieder,", "tokens": ["Als", "sich", "der", "Geist", "mit", "dem", "Ge\u00b7setz", "der", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mit der Welt auf allezeit geletzt.", "tokens": ["Und", "mit", "der", "Welt", "auf", "al\u00b7le\u00b7zeit", "ge\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was war ich St\u00e4ublein; ich?", "tokens": ["Was", "war", "ich", "St\u00e4ub\u00b7lein", ";", "ich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "$.", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht nur allein ", "tokens": ["Nicht", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Du wecktest auch viel Schlafende durch mich.", "tokens": ["Du", "weck\u00b7test", "auch", "viel", "Schla\u00b7fen\u00b7de", "durch", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Hier liegt das Amt, ich meyne ", "tokens": ["Hier", "liegt", "das", "Amt", ",", "ich", "mey\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort ", "tokens": ["Dort"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wie komm ich doch zu einem solchen Lohne,", "tokens": ["Wie", "komm", "ich", "doch", "zu", "ei\u00b7nem", "sol\u00b7chen", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ich schon dort ein Fa\u00df zu Ehren war?", "tokens": ["Der", "ich", "schon", "dort", "ein", "Fa\u00df", "zu", "Eh\u00b7ren", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich habe wohl gestritten, ich habe was gelitten,", "tokens": ["Ich", "ha\u00b7be", "wohl", "ge\u00b7strit\u00b7ten", ",", "ich", "ha\u00b7be", "was", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "PPER", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Was ists? du ", "tokens": ["Was", "ists", "?", "du"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "$.", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ein HErr, wie Du, darf nicht um Diener bitten:", "tokens": ["Ein", "Herr", ",", "wie", "Du", ",", "darf", "nicht", "um", "Die\u00b7ner", "bit\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "$,", "VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Majest\u00e4t! darf ich mich unterwinden,", "tokens": ["O", "Ma\u00b7jes\u00b7t\u00e4t", "!", "darf", "ich", "mich", "un\u00b7ter\u00b7win\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So zeig ich mich zuerst, als deine ", "tokens": ["So", "zeig", "ich", "mich", "zu\u00b7erst", ",", "als", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Knecht wird auch was zu erzehlen finden,", "tokens": ["Der", "Knecht", "wird", "auch", "was", "zu", "er\u00b7zeh\u00b7len", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was du durch ihn ", "tokens": ["Was", "du", "durch", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch deine Br\u00e4utgams-Triebe, du ", "tokens": ["Doch", "dei\u00b7ne", "Br\u00e4ut\u00b7gams\u00b7Trie\u00b7be", ",", "du"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gehn allen andern vor.", "tokens": ["Gehn", "al\u00b7len", "an\u00b7dern", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was w\u00e4rs, wenn mich mein Werck vor ", "tokens": ["Was", "w\u00e4rs", ",", "wenn", "mich", "mein", "Werck", "vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Und h\u00e4tt\u2019 an dir gehandelt, als ein Thor?", "tokens": ["Und", "h\u00e4tt'", "an", "dir", "ge\u00b7han\u00b7delt", ",", "als", "ein", "Thor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "VVPP", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du hast mich zwar nicht ", "tokens": ["Du", "hast", "mich", "zwar", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und von der Zeit blieb dir mein Hertze treu;", "tokens": ["Und", "von", "der", "Zeit", "blieb", "dir", "mein", "Hert\u00b7ze", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da deine Kraft ", "tokens": ["Da", "dei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Umarmte dich mein Glauben ohne Scheu.", "tokens": ["Um\u00b7arm\u00b7te", "dich", "mein", "Glau\u00b7ben", "oh\u00b7ne", "Scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und der getreue H\u00fcter der himmlischen Gem\u00fcther,", "tokens": ["Und", "der", "ge\u00b7treu\u00b7e", "H\u00fc\u00b7ter", "der", "himm\u00b7li\u00b7schen", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Verschlo\u00df vor mir die Welt und ihre G\u00fcter,", "tokens": ["Ver\u00b7schlo\u00df", "vor", "mir", "die", "Welt", "und", "ih\u00b7re", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und that mir auf die Th\u00fcr der Gnaden-Zeit.", "tokens": ["Und", "that", "mir", "auf", "die", "Th\u00fcr", "der", "Gna\u00b7den\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich hie\u00df ", "tokens": ["Ich", "hie\u00df"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Doch das zerstob vor deines Geistes Wehen.", "tokens": ["Doch", "das", "zer\u00b7stob", "vor", "dei\u00b7nes", "Geis\u00b7tes", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Di\u00df lehrte mich des Himmel-Reichs Gebrauch:", "tokens": ["Di\u00df", "lehr\u00b7te", "mich", "des", "Him\u00b7mel\u00b7Reichs", "Ge\u00b7brauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo eigne Kraft begraben, und wo wir nichts mehr haben,", "tokens": ["Wo", "eig\u00b7ne", "Kraft", "be\u00b7gra\u00b7ben", ",", "und", "wo", "wir", "nichts", "mehr", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVPP", "$,", "KON", "PWAV", "PPER", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Als aus des K\u00f6nigs Hand;", "tokens": ["Als", "aus", "des", "K\u00f6\u00b7nigs", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erzeigen sich zum Nutz besondre Gaben,", "tokens": ["Er\u00b7zei\u00b7gen", "sich", "zum", "Nutz", "be\u00b7sond\u00b7re", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die werden mit dem Geist herab gesandt.", "tokens": ["Die", "wer\u00b7den", "mit", "dem", "Geist", "her\u00b7ab", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Kaum brachte mich mein H\u00fcter in die Kammer,", "tokens": ["Kaum", "brach\u00b7te", "mich", "mein", "H\u00fc\u00b7ter", "in", "die", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sah ich dich am Creutz, ", "tokens": ["So", "sah", "ich", "dich", "am", "Creutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Hertze ward auf diesen Blick voll Jammer;", "tokens": ["Mein", "Hert\u00b7ze", "ward", "auf", "die\u00b7sen", "Blick", "voll", "Jam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "APPR", "PDAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn ich verdiente ja den rauhen Stamm.", "tokens": ["Denn", "ich", "ver\u00b7dien\u00b7te", "ja", "den", "rau\u00b7hen", "Stamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ey, dacht ich, ", "tokens": ["Ey", ",", "dacht", "ich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Empfindst du neue Pein?", "tokens": ["Emp\u00b7findst", "du", "neu\u00b7e", "Pein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hinweg mit euch, ihr m\u00f6rderischen Triebe,", "tokens": ["Hin\u00b7weg", "mit", "euch", ",", "ihr", "m\u00f6r\u00b7de\u00b7ri\u00b7schen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich will ein Knecht der heilgen Liebe seyn!", "tokens": ["Ich", "will", "ein", "Knecht", "der", "heil\u00b7gen", "Lie\u00b7be", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So wolle nichts, erinnerte mein Meister,\nSo werd ein Kind so thu die Augen zu,", "tokens": ["So", "wol\u00b7le", "nichts", ",", "e\u00b7rin\u00b7ner\u00b7te", "mein", "Meis\u00b7ter", ",", "So", "werd", "ein", "Kind", "so", "thu", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "$,", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "So wirf dich hin dem Vater aller Geister,", "tokens": ["So", "wirf", "dich", "hin", "dem", "Va\u00b7ter", "al\u00b7ler", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eh er w\u00fcrckt, so bleib in stiller Ruh.", "tokens": ["Und", "eh", "er", "w\u00fcrckt", ",", "so", "bleib", "in", "stil\u00b7ler", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verlerne die Gelahrheit, und suche meine Klarheit,\nSo\n\n1726.\nSo will ich dich erh\u00f6hn.", "tokens": ["Ver\u00b7ler\u00b7ne", "die", "Ge\u00b7lahr\u00b7heit", ",", "und", "su\u00b7che", "mei\u00b7ne", "Klar\u00b7heit", ",", "So", "1726", ".", "So", "will", "ich", "dich", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "number", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "CARD", "$.", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Ich offenbahre mich dir, ich die Wahrheit,", "tokens": ["Ich", "of\u00b7fen\u00b7bah\u00b7re", "mich", "dir", ",", "ich", "die", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6"