{"textgrid.poem.37690": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Lieg' ich in der Freundin Armen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieg' ich in der Freundin Armen,", "tokens": ["Lieg'", "ich", "in", "der", "Freun\u00b7din", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weine und nicht wei\u00df warum,", "tokens": ["Wei\u00b7ne", "und", "nicht", "wei\u00df", "wa\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKNEG", "VVFIN", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie ist traurig, ich bin stumm,", "tokens": ["Sie", "ist", "trau\u00b7rig", ",", "ich", "bin", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis die Lippen mir erwarmen,", "tokens": ["Bis", "die", "Lip\u00b7pen", "mir", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach dann schwebt es auf der Zunge,", "tokens": ["Ach", "dann", "schwebt", "es", "auf", "der", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4re ich doch nur ein Junge!", "tokens": ["W\u00e4\u00b7re", "ich", "doch", "nur", "ein", "Jun\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4re ich doch nur ein Junge,", "tokens": ["W\u00e4\u00b7re", "ich", "doch", "nur", "ein", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gingen wir in die weite Welt,", "tokens": ["Gin\u00b7gen", "wir", "in", "die", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Treulich w\u00e4ren wir gesellt,", "tokens": ["Treu\u00b7lich", "w\u00e4\u00b7ren", "wir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielten uns noch fest umschlungen,", "tokens": ["Hiel\u00b7ten", "uns", "noch", "fest", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sich an der Welten Ende,", "tokens": ["Wenn", "sich", "an", "der", "Wel\u00b7ten", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein Italien einst f\u00e4nde.", "tokens": ["Mein", "I\u00b7ta\u00b7li\u00b7en", "einst", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich mein Italien f\u00e4nde,", "tokens": ["Wenn", "ich", "mein", "I\u00b7ta\u00b7li\u00b7en", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NE", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "H\u00f6hlten wir ein kleines Haus", "tokens": ["H\u00f6hl\u00b7ten", "wir", "ein", "klei\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns in Herkulanum aus,", "tokens": ["Uns", "in", "Her\u00b7ku\u00b7la\u00b7num", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die sch\u00f6n bemahlten W\u00e4nde;", "tokens": ["Wo", "die", "sch\u00f6n", "be\u00b7mahl\u00b7ten", "W\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Schwalben in dem Sande", "tokens": ["Wie", "die", "Schwal\u00b7ben", "in", "dem", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bauten wir uns an im Lande.", "tokens": ["Bau\u00b7ten", "wir", "uns", "an", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bauten wir uns an im Lande,", "tokens": ["Bau\u00b7ten", "wir", "uns", "an", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steckten manches Fl\u00fcgelkind", "tokens": ["Steck\u00b7ten", "man\u00b7ches", "Fl\u00fc\u00b7gel\u00b7kind"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In das K\u00f6rbchen schnell geschwind,", "tokens": ["In", "das", "K\u00f6rb\u00b7chen", "schnell", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verkauften's ohne Schande;", "tokens": ["Und", "ver\u00b7kauf\u00b7ten's", "oh\u00b7ne", "Schan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leutchen, wer kauft Liebesg\u00f6tter", "tokens": ["Leut\u00b7chen", ",", "wer", "kauft", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, es ist so liebreich Wetter.", "tokens": ["Ach", ",", "es", "ist", "so", "lieb\u00b7reich", "Wet\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach es ist so liebreich Wetter,", "tokens": ["Ach", "es", "ist", "so", "lieb\u00b7reich", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kauft ihr M\u00e4dchen jung und sch\u00f6n!", "tokens": ["Kauft", "ihr", "M\u00e4d\u00b7chen", "jung", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine kommt sie anzusehen,", "tokens": ["Ei\u00b7ne", "kommt", "sie", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht: Das sind die Liebesg\u00f6tter?", "tokens": ["Spricht", ":", "Das", "sind", "die", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei bewahre, das sind Tauben,", "tokens": ["Ei", "be\u00b7wah\u00b7re", ",", "das", "sind", "Tau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine nur geh\u00f6rt zum Glauben.", "tokens": ["Ei\u00b7ne", "nur", "ge\u00b7h\u00f6rt", "zum", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eine, die geh\u00f6rt zum Glauben,", "tokens": ["Ei\u00b7ne", ",", "die", "ge\u00b7h\u00f6rt", "zum", "Glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Liebe alle braucht,", "tokens": ["Doch", "die", "Lie\u00b7be", "al\u00b7le", "braucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Boten jede taugt,", "tokens": ["Und", "zum", "Bo\u00b7ten", "je\u00b7de", "taugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PIAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft sich nicht ihr Brieflein rauben,", "tokens": ["L\u00e4\u00dft", "sich", "nicht", "ihr", "Brie\u00b7flein", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als wo sie den Liebsten wittert,", "tokens": ["Als", "wo", "sie", "den", "Liebs\u00b7ten", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der sie oft mit Zucker f\u00fcttert.", "tokens": ["Der", "sie", "oft", "mit", "Zu\u00b7cker", "f\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lieg' ich in der Freundin Armen,", "tokens": ["Lieg'", "ich", "in", "der", "Freun\u00b7din", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weine und nicht wei\u00df warum,", "tokens": ["Wei\u00b7ne", "und", "nicht", "wei\u00df", "wa\u00b7rum", ","], "tok