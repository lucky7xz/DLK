{"dta.poem.10623": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Chansonnette.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Liebes Brunst behafftet sein/", "tokens": ["Mit", "Lie\u00b7bes", "Brunst", "be\u00b7haff\u00b7tet", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist warlich eine schwere Pein/", "tokens": ["Ist", "war\u00b7lich", "ei\u00b7ne", "schwe\u00b7re", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist kein Schmertz auff dieser Erdt/", "tokens": ["Es", "ist", "kein", "Schmertz", "auff", "die\u00b7ser", "Erdt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der recht mit jhm verglichen werdt:", "tokens": ["Der", "recht", "mit", "jhm", "ver\u00b7gli\u00b7chen", "werdt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drumb will ich mich gantz embsiglich", "tokens": ["Drumb", "will", "ich", "mich", "gantz", "emb\u00b7sig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von dem Leyden allzeitscheiden/", "tokens": ["Von", "dem", "Ley\u00b7den", "all\u00b7zeit\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vnd die s\u00fcsse Gifft vermeiden.", "tokens": ["Vnd", "die", "s\u00fcs\u00b7se", "Gifft", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auff da\u00df nun nicht die schn\u00f6de Brunst", "tokens": ["Auff", "da\u00df", "nun", "nicht", "die", "schn\u00f6\u00b7de", "Brunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ADV", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Mich lasse zu jhr tragen Gunst/", "tokens": ["Mich", "las\u00b7se", "zu", "jhr", "tra\u00b7gen", "Gunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Soll Venus mich nicht treffen an", "tokens": ["Soll", "Ve\u00b7nus", "mich", "nicht", "tref\u00b7fen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PPER", "PTKNEG", "VVINF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auff jergendt einer Liebes Bahn/", "tokens": ["Auff", "jer\u00b7gendt", "ei\u00b7ner", "Lie\u00b7bes", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Tugendt Weg ist ein sch\u00f6n Steg/", "tokens": ["Der", "Tu\u00b7gendt", "Weg", "ist", "ein", "sch\u00f6n", "Steg", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Darauff eben ich will schweben/", "tokens": ["Dar\u00b7auff", "e\u00b7ben", "ich", "will", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Vnd jhr gantz verpflichtet leben/", "tokens": ["Vnd", "jhr", "gantz", "ver\u00b7pflich\u00b7tet", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Recht vnd gar wol auch Pallas blieb", "tokens": ["Recht", "vnd", "gar", "wol", "auch", "Pal\u00b7las", "blieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "ADV", "ADV", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Allzeit befreyet von der Lieb/", "tokens": ["All\u00b7zeit", "be\u00b7fre\u00b7yet", "von", "der", "Lieb", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sie gab dem Fewer niemals raum/", "tokens": ["Sie", "gab", "dem", "Fe\u00b7wer", "nie\u00b7mals", "raum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Vnd hielte sich in st\u00e4tem Zaum/", "tokens": ["Vnd", "hiel\u00b7te", "sich", "in", "st\u00e4\u00b7tem", "Zaum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Auff gr\u00fcner Heyd sie allezeit", "tokens": ["Auff", "gr\u00fc\u00b7ner", "Heyd", "sie", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mit dem Hetzen sich thet letzen/", "tokens": ["Mit", "dem", "Het\u00b7zen", "sich", "thet", "let\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Vnd frey aller Sorg ergetzen.", "tokens": ["Vnd", "frey", "al\u00b7ler", "Sorg", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.22": {"text": "Ich will ins k\u00fcnfftig fleissig auch", "tokens": ["Ich", "will", "ins", "k\u00fcnff\u00b7tig", "fleis\u00b7sig", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJD", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nachfolgen dieser G\u00f6ttin Brauch/", "tokens": ["Nach\u00b7fol\u00b7gen", "die\u00b7ser", "G\u00f6t\u00b7tin", "Brauch", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Denn Venus ist die gr\u00f6ste Last/", "tokens": ["Denn", "Ve\u00b7nus", "ist", "die", "gr\u00f6s\u00b7te", "Last", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Cupido ist ein sch\u00e4dlich Gast.", "tokens": ["Cu\u00b7pi\u00b7do", "ist", "ein", "sch\u00e4d\u00b7lich", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Wen er einmal nur bringt zu fall/", "tokens": ["Wen", "er", "ein\u00b7mal", "nur", "bringt", "zu", "fall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "NN", "$("], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.27": {"text": "Mu\u00df verderben/ offt auch sterben/", "tokens": ["Mu\u00df", "ver\u00b7der\u00b7ben", "/", "offt", "auch", "ster\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "$(", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Vnd f\u00fcr Frewden schmertz ererben/", "tokens": ["Vnd", "f\u00fcr", "Frew\u00b7den", "schmertz", "er\u00b7er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Also belohnt er alle doch/", "tokens": ["Al\u00b7so", "be\u00b7lohnt", "er", "al\u00b7le", "doch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Die sich ergeben seinem Joch/", "tokens": ["Die", "sich", "er\u00b7ge\u00b7ben", "sei\u00b7nem", "Joch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Vnd di\u00df bedenck ich offt vnd viel/", "tokens": ["Vnd", "di\u00df", "be\u00b7denck", "ich", "offt", "vnd", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Es mag lieb haben wer da will/", "tokens": ["Es", "mag", "lieb", "ha\u00b7ben", "wer", "da", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "PWS", "ADV", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ich bleibe mein allzeit allein/", "tokens": ["Ich", "blei\u00b7be", "mein", "all\u00b7zeit", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Offt nach schertzen kommen schmertzen/", "tokens": ["Offt", "nach", "schert\u00b7zen", "kom\u00b7men", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVINF", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Wohl dem der das thut behertzen.", "tokens": ["Wohl", "dem", "der", "das", "thut", "be\u00b7hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "PDS", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}