{"dta.poem.17433": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "54.  Der Tugenden und Laster befchaf-  \n fenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend liegt in ruh/ die laster stehn im streit:", "tokens": ["Die", "Tu\u00b7gend", "liegt", "in", "ruh", "/", "die", "las\u00b7ter", "stehn", "im", "streit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$(", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie haben Pein in sich/ jen\u2019 aber Seeligkeit.", "tokens": ["Sie", "ha\u00b7ben", "Pein", "in", "sich", "/", "jen'", "a\u00b7ber", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PRF", "$(", "PDS", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}