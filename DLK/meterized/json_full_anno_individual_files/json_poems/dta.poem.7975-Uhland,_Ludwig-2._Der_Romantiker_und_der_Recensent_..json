{"dta.poem.7975": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Der Romantiker und der Recensent .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Finster ist die Nacht und bange,", "tokens": ["Fins\u00b7ter", "ist", "die", "Nacht", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nirgends eines Sternleins Funkel!", "tokens": ["Nir\u00b7gends", "ei\u00b7nes", "Stern\u00b7leins", "Fun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch in verliebtem Drange", "tokens": ["Den\u00b7noch", "in", "ver\u00b7lieb\u00b7tem", "Dran\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandl\u2019 ich durch das grause Dunkel", "tokens": ["Wandl'", "ich", "durch", "das", "grau\u00b7se", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Gesang und Lautenklange.", "tokens": ["Mit", "Ge\u00b7sang", "und", "Lau\u00b7ten\u00b7klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn Kamilla nun erwacht", "tokens": ["Wenn", "Ka\u00b7mil\u00b7la", "nun", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "VVPP"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Und das L\u00e4mpchen freundlich facht,", "tokens": ["Und", "das", "L\u00e4mp\u00b7chen", "freund\u00b7lich", "facht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dann erblick\u2019 ich, der Entz\u00fcckte,", "tokens": ["Dann", "er\u00b7blick'", "ich", ",", "der", "Ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Pl\u00f6tzlich eine sterngeschm\u00fcckte,", "tokens": ["Pl\u00f6tz\u00b7lich", "ei\u00b7ne", "stern\u00b7ge\u00b7schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mondbegl&#228;nzte Zaubernacht.", "tokens": ["Mond\u00b7begl", "&#228;", "nz\u00b7te", "Zau\u00b7ber\u00b7nacht", "."], "token_info": ["word", "XML_entity", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}