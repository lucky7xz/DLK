": {"text": "Und meine Leichenrede selber halten.", "tokens": ["Und", "mei\u00b7ne", "Lei\u00b7chen\u00b7re\u00b7de", "sel\u00b7ber", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was schauet ihr mich an so grausenvoll?", "tokens": ["Was", "schau\u00b7et", "ihr", "mich", "an", "so", "grau\u00b7sen\u00b7voll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich f\u00fchrte Krieg, wie jeder tut und soll,", "tokens": ["Ich", "f\u00fchr\u00b7te", "Krieg", ",", "wie", "je\u00b7der", "tut", "und", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PWAV", "PIS", "VVFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gen feindliche Gewalten.", "tokens": ["Gen", "feind\u00b7li\u00b7che", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich tat nur eben, was ihr alle tut,", "tokens": ["Ich", "tat", "nur", "e\u00b7ben", ",", "was", "ihr", "al\u00b7le", "tut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWS", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur besser; drum, begehret ihr mein Blut,", "tokens": ["Nur", "bes\u00b7ser", ";", "drum", ",", "be\u00b7ge\u00b7hret", "ihr", "mein", "Blut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PAV", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "So tut ihr gut.", "tokens": ["So", "tut", "ihr", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Es sinnt Gewalt und List nur dies Geschlecht;", "tokens": ["Es", "sinnt", "Ge\u00b7walt", "und", "List", "nur", "dies", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was will, was soll, was hei\u00dfet denn das Recht?", "tokens": ["Was", "will", ",", "was", "soll", ",", "was", "hei\u00b7\u00dfet", "denn", "das", "Recht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PWS", "VMFIN", "$,", "PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du die Macht, du hast das Recht auf Erden.", "tokens": ["Hast", "du", "die", "Macht", ",", "du", "hast", "das", "Recht", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selbsts\u00fcchtig schuf der St\u00e4rkre das Gesetz,", "tokens": ["Selbst\u00b7s\u00fcch\u00b7tig", "schuf", "der", "St\u00e4r\u00b7kre", "das", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "l