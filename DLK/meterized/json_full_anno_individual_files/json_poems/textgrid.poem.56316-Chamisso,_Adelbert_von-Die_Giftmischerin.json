{"textgrid.poem.56316": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Giftmischerin", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies hier der Block und dorten klafft die Gruft.", "tokens": ["Dies", "hier", "der", "Block", "und", "dor\u00b7ten", "klafft", "die", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "La\u00dft einmal noch mich atmen diese Luft,", "tokens": ["La\u00dft", "ein\u00b7mal", "noch", "mich", "at\u00b7men", "die\u00b7se", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und meine Leichenrede selber halten.", "tokens": ["Und", "mei\u00b7ne", "Lei\u00b7chen\u00b7re\u00b7de", "sel\u00b7ber", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was schauet ihr mich an so grausenvoll?", "tokens": ["Was", "schau\u00b7et", "ihr", "mich", "an", "so", "grau\u00b7sen\u00b7voll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich f\u00fchrte Krieg, wie jeder tut und soll,", "tokens": ["Ich", "f\u00fchr\u00b7te", "Krieg", ",", "wie", "je\u00b7der", "tut", "und", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PWAV", "PIS", "VVFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gen feindliche Gewalten.", "tokens": ["Gen", "feind\u00b7li\u00b7che", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich tat nur eben, was ihr alle tut,", "tokens": ["Ich", "tat", "nur", "e\u00b7ben", ",", "was", "ihr", "al\u00b7le", "tut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWS", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur besser; drum, begehret ihr mein Blut,", "tokens": ["Nur", "bes\u00b7ser", ";", "drum", ",", "be\u00b7ge\u00b7hret", "ihr", "mein", "Blut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PAV", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "So tut ihr gut.", "tokens": ["So", "tut", "ihr", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es sinnt Gewalt und List nur dies Geschlecht;", "tokens": ["Es", "sinnt", "Ge\u00b7walt", "und", "List", "nur", "dies", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was will, was soll, was hei\u00dfet denn das Recht?", "tokens": ["Was", "will", ",", "was", "soll", ",", "was", "hei\u00b7\u00dfet", "denn", "das", "Recht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PWS", "VMFIN", "$,", "PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du die Macht, du hast das Recht auf Erden.", "tokens": ["Hast", "du", "die", "Macht", ",", "du", "hast", "das", "Recht", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selbsts\u00fcchtig schuf der St\u00e4rkre das Gesetz,", "tokens": ["Selbst\u00b7s\u00fcch\u00b7tig", "schuf", "der", "St\u00e4r\u00b7kre", "das", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Schl\u00e4chterbeil zugleich und Fangenetz", "tokens": ["Ein", "Schl\u00e4ch\u00b7ter\u00b7beil", "zu\u00b7gleich", "und", "Fan\u00b7ge\u00b7netz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr Schw\u00e4chere zu werden.", "tokens": ["F\u00fcr", "Schw\u00e4\u00b7che\u00b7re", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Herrschaft Zauber aber ist das Geld:", "tokens": ["Der", "Herr\u00b7schaft", "Zau\u00b7ber", "a\u00b7ber", "ist", "das", "Geld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich wei\u00df mir Be\u00dfres nichts auf dieser Welt,", "tokens": ["Ich", "wei\u00df", "mir", "Be\u00df\u00b7res", "nichts", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als Gift und Geld.", "tokens": ["Als", "Gift", "und", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich habe mich aus tiefer Schmach entrafft,", "tokens": ["Ich", "ha\u00b7be", "mich", "aus", "tie\u00b7fer", "Schmach", "en\u00b7trafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor Kinderm\u00e4rchen Ruhe mir geschafft,", "tokens": ["Vor", "Kin\u00b7der\u00b7m\u00e4r\u00b7chen", "Ru\u00b7he", "mir", "ge\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Schrecken vor Gespenstern \u00fcberwunden.", "tokens": ["Die", "Schre\u00b7cken", "vor", "Ge\u00b7spens\u00b7tern", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Gift erschleicht im Dunkeln Geld und Macht,", "tokens": ["Das", "Gift", "er\u00b7schleicht", "im", "Dun\u00b7keln", "Geld", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich hab es zum Genossen mir erdacht,", "tokens": ["Ich", "hab", "es", "zum", "Ge\u00b7nos\u00b7sen", "mir", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und hab es gut befunden.", "tokens": ["Und", "hab", "es", "gut", "be\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hinunter stie\u00df ich in das Schattenreich", "tokens": ["Hin\u00b7un\u00b7ter", "stie\u00df", "ich", "in", "das", "Schat\u00b7ten\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mann, Br\u00fcder, Vater, und ich ward zugleich", "tokens": ["Mann", ",", "Br\u00fc\u00b7der", ",", "Va\u00b7ter", ",", "und", "ich", "ward", "zu\u00b7gleich"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Geehrt und reich.", "tokens": ["Ge\u00b7ehrt", "und", "reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Drei Kinder waren annoch mir zur Last,", "tokens": ["Drei", "Kin\u00b7der", "wa\u00b7ren", "an\u00b7noch", "mir", "zur", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drei Kinder meines Leibes; mir verha\u00dft,", "tokens": ["Drei", "Kin\u00b7der", "mei\u00b7nes", "Lei\u00b7bes", ";", "mir", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "$.", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erschwerten sie mein Ziel mir zu erreichen.", "tokens": ["Er\u00b7schwer\u00b7ten", "sie", "mein", "Ziel", "mir", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich habe sie vergiftet, sie gesehn,", "tokens": ["Ich", "ha\u00b7be", "sie", "ver\u00b7gif\u00b7tet", ",", "sie", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu mir um H\u00fclfe rufend, untergehn,", "tokens": ["Zu", "mir", "um", "H\u00fcl\u00b7fe", "ru\u00b7fend", ",", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVPP", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bald stumme, kalte Leichen.", "tokens": ["Bald", "stum\u00b7me", ",", "kal\u00b7te", "Lei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich hielt die Leichen lang auf meinem Scho\u00df,", "tokens": ["Ich", "hielt", "die", "Lei\u00b7chen", "lang", "auf", "mei\u00b7nem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und schien mir, sie betrachtend tr\u00e4nenlos,", "tokens": ["Und", "schien", "mir", ",", "sie", "be\u00b7trach\u00b7tend", "tr\u00e4\u00b7nen\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Erst stark und gro\u00df.", "tokens": ["Erst", "stark", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Nun fr\u00f6nt ich sicher heimlichem Genu\u00df,", "tokens": ["Nun", "fr\u00f6nt", "ich", "si\u00b7cher", "heim\u00b7li\u00b7chem", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Gift verwahrte mich vor \u00dcberdru\u00df", "tokens": ["Mein", "Gift", "ver\u00b7wahr\u00b7te", "mich", "vor", "\u00dc\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lie\u00df die Zeugen nach der Tat verschwinden.", "tokens": ["Und", "lie\u00df", "die", "Zeu\u00b7gen", "nach", "der", "Tat", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Lust am Gift, am Morden ich gewann,", "tokens": ["Da\u00df", "Lust", "am", "Gift", ",", "am", "Mor\u00b7den", "ich", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer, was ich tat, erw\u00e4gt und fassen kann,", "tokens": ["Wer", ",", "was", "ich", "tat", ",", "er\u00b7w\u00e4gt", "und", "fas\u00b7sen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der wird's begreiflich finden.", "tokens": ["Der", "wird's", "be\u00b7greif\u00b7lich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich teilte Gift wie milde Spenden aus,", "tokens": ["Ich", "teil\u00b7te", "Gift", "wie", "mil\u00b7de", "Spen\u00b7den", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KOKOM", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und weilte l\u00fcstern Auges, wo im Haus", "tokens": ["Und", "weil\u00b7te", "l\u00fcs\u00b7tern", "Au\u00b7ges", ",", "wo", "im", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Tod hielt Schmaus.", "tokens": ["Der", "Tod", "hielt", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ich habe mich zu sicher nur geglaubt,", "tokens": ["Ich", "ha\u00b7be", "mich", "zu", "si\u00b7cher", "nur", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und b\u00fc\u00df es billig mit dem eignen Haupt,", "tokens": ["Und", "b\u00fc\u00df", "es", "bil\u00b7lig", "mit", "dem", "eig\u00b7nen", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich der Vorsicht einmal mich begeben.", "tokens": ["Da\u00df", "ich", "der", "Vor\u00b7sicht", "ein\u00b7mal", "mich", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Fehl, den einen Fehl bereu ich nur,", "tokens": ["Den", "Fehl", ",", "den", "ei\u00b7nen", "Fehl", "be\u00b7reu", "ich", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und g\u00e4be, zu vertilgen dessen Spur,", "tokens": ["Und", "g\u00e4\u00b7be", ",", "zu", "ver\u00b7til\u00b7gen", "des\u00b7sen", "Spur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PTKZU", "VVINF", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie viele eurer Leben!", "tokens": ["