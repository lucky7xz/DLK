{"dta.poem.4096": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Erster Schmerz, letzter Scherz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit deinem hellen Rohr,", "tokens": ["Mit", "dei\u00b7nem", "hel\u00b7len", "Rohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und blas' den lieben Kindern", "tokens": ["Und", "blas'", "den", "lie\u00b7ben", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6nen Lieder vor.", "tokens": ["Die", "sch\u00f6\u00b7nen", "Lie\u00b7der", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Lust ist ja verrauschet,", "tokens": ["Die", "Lust", "ist", "ja", "ver\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Leid hat immer Zeit:", "tokens": ["Das", "Leid", "hat", "im\u00b7mer", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun singe neue Lieder", "tokens": ["Nun", "sin\u00b7ge", "neu\u00b7e", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von alter Seligkeit.", "tokens": ["Von", "al\u00b7ter", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Noch bl\u00fchn die alten Blumen,", "tokens": ["Noch", "bl\u00fchn", "die", "al\u00b7ten", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch rauscht der alte Bach,", "tokens": ["Noch", "rauscht", "der", "al\u00b7te", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es scheint die liebe Sonne", "tokens": ["Es", "scheint", "die", "lie\u00b7be", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch wie am ersten Tag.", "tokens": ["Noch", "wie", "am", "ers\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Fensterscheiben gl\u00e4nzen", "tokens": ["Die", "Fens\u00b7ter\u00b7schei\u00b7ben", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im klaren Morgenschein,", "tokens": ["Im", "kla\u00b7ren", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hinter den Fensterscheiben", "tokens": ["Und", "hin\u00b7ter", "den", "Fens\u00b7ter\u00b7schei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da sitzt die Liebste mein.", "tokens": ["Da", "sitzt", "die", "Liebs\u00b7te", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein J\u00e4ger, ein gr\u00fcner J\u00e4ger,", "tokens": ["Ein", "J\u00e4\u00b7ger", ",", "ein", "gr\u00fc\u00b7ner", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der liegt in ihrem Arm \u2014", "tokens": ["Der", "liegt", "in", "ih\u00b7rem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ei, Bach, wie lustig du rauschest,", "tokens": ["Ei", ",", "Bach", ",", "wie", "lus\u00b7tig", "du", "rau\u00b7schest", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ei, Sonne, wie scheinst du so warm!", "tokens": ["Ei", ",", "Son\u00b7ne", ",", "wie", "scheinst", "du", "so", "warm", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ich will einen Strau\u00df dir pfl\u00fccken,", "tokens": ["Ich", "will", "ei\u00b7nen", "Strau\u00df", "dir", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NE", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herzliebste, von buntem Klee,", "tokens": ["Herz\u00b7liebs\u00b7te", ",", "von", "bun\u00b7tem", "Klee", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den sollst du mir stellen an's Fenster,", "tokens": ["Den", "sollst", "du", "mir", "stel\u00b7len", "an's", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Damit ich den J\u00e4ger nicht seh'.", "tokens": ["Da\u00b7mit", "ich", "den", "J\u00e4\u00b7ger", "nicht", "seh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Ich will mit Rosenbl\u00e4ttern", "tokens": ["Ich", "will", "mit", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den M\u00fchlensteg bestreu'n:", "tokens": ["Den", "M\u00fch\u00b7lens\u00b7teg", "be\u00b7streu'n", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Steg hat mich getragen", "tokens": ["Der", "Steg", "hat", "mich", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu dir, Herzliebste mein!", "tokens": ["Zu", "dir", ",", "Herz\u00b7liebs\u00b7te", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wenn der stolze J\u00e4ger", "tokens": ["Und", "wenn", "der", "stol\u00b7ze", "J\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Bl\u00e4ttchen mir zertrit,", "tokens": ["Ein", "Bl\u00e4tt\u00b7chen", "mir", "zer\u00b7trit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann st\u00fcrz', o Steg, zusammen", "tokens": ["Dann", "st\u00fcrz'", ",", "o", "Steg", ",", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimm den Gr\u00fcnen mit!", "tokens": ["Und", "nimm", "den", "Gr\u00fc\u00b7nen", "mit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und trag' ihn auf dem R\u00fccken", "tokens": ["Und", "trag'", "ihn", "auf", "dem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In's Meer, mit gutem Wind,", "tokens": ["In's", "Meer", ",", "mit", "gu\u00b7tem", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach einer fernen Insel,", "tokens": ["Nach", "ei\u00b7ner", "fer\u00b7nen", "In\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo keine M\u00e4dchen sind.", "tokens": ["Wo", "kei\u00b7ne", "M\u00e4d\u00b7chen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Herzliebste, das Vergessen,", "tokens": ["Herz\u00b7liebs\u00b7te", ",", "das", "Ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es kommt dir ja nicht schwer \u2014", "tokens": ["Es", "kommt", "dir", "ja", "nicht", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst du den M\u00fcller wieder?", "tokens": ["Willst", "du", "den", "M\u00fcl\u00b7ler", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NE", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergi\u00dft dich nimmermehr.", "tokens": ["Ver\u00b7gi\u00dft", "dich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}