{"textgrid.poem.60749": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "39. Tristan", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer die Sch\u00f6nheit angeschaut mit Augen,", "tokens": ["Wer", "die", "Sch\u00f6n\u00b7heit", "an\u00b7ge\u00b7schaut", "mit", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist dem Tode schon anheimgegeben,", "tokens": ["Ist", "dem", "To\u00b7de", "schon", "an\u00b7heim\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wird f\u00fcr keinen Dienst auf Erden taugen,", "tokens": ["Wird", "f\u00fcr", "kei\u00b7nen", "Dienst", "auf", "Er\u00b7den", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und doch wird er vor dem Tode beben,", "tokens": ["Und", "doch", "wird", "er", "vor", "dem", "To\u00b7de", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wer die Sch\u00f6nheit angeschaut mit Augen!", "tokens": ["Wer", "die", "Sch\u00f6n\u00b7heit", "an\u00b7ge\u00b7schaut", "mit", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "APPR", "NN", "$."