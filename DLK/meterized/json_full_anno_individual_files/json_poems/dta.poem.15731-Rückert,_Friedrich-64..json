{"dta.poem.15731": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie das H\u00f6chste nicht ist in der Kunst zu nennen", "tokens": ["Gleich\u00b7wie", "das", "H\u00f6chs\u00b7te", "nicht", "ist", "in", "der", "Kunst", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "PTKNEG", "VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachahmung dessen, was die Sinne Sch\u00f6ns erkennen;", "tokens": ["Nac\u00b7hah\u00b7mung", "des\u00b7sen", ",", "was", "die", "Sin\u00b7ne", "Sch\u00f6ns", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So kann Nachahmung auch des Guten in der Zeit", "tokens": ["So", "kann", "Nac\u00b7hah\u00b7mung", "auch", "des", "Gu\u00b7ten", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht seyn das oberste Gesetz der Sittlichkeit.", "tokens": ["Nicht", "seyn", "das", "o\u00b7bers\u00b7te", "Ge\u00b7setz", "der", "Sitt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAINF", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Es mu\u00df, gleichwie es ein Ursch\u00f6nes gibt, so geben", "tokens": ["Es", "mu\u00df", ",", "gleich\u00b7wie", "es", "ein", "Ur\u00b7sch\u00f6\u00b7nes", "gibt", ",", "so", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KON", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch ein Urgutes, Kind! das mu\u00dft du selber leben.", "tokens": ["Auch", "ein", "Ur\u00b7gu\u00b7tes", ",", "Kind", "!", "das", "mu\u00dft", "du", "sel\u00b7ber", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "NN", "$.", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}