{"textgrid.poem.51058": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ohne Leid, der ist auch ohne Liebe,", "tokens": ["Wer", "oh\u00b7ne", "Leid", ",", "der", "ist", "auch", "oh\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,", "PRELS", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer ohne Reu, der ist auch ohne Treu,", "tokens": ["Wer", "oh\u00b7ne", "Reu", ",", "der", "ist", "auch", "oh\u00b7ne", "Treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "$,", "PRELS", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dem nur wird die Sonne wolkenfrei,", "tokens": ["Und", "dem", "nur", "wird", "die", "Son\u00b7ne", "wol\u00b7ken\u00b7frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der aus dem Dunkel ringt mit hei\u00dfem Triebe.", "tokens": ["Der", "aus", "dem", "Dun\u00b7kel", "ringt", "mit", "hei\u00b7\u00dfem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bei euch ist nichts als l\u00e4rmendes Geschiebe,", "tokens": ["Bei", "euch", "ist", "nichts", "als", "l\u00e4r\u00b7men\u00b7des", "Ge\u00b7schie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In wildem Tummel trollt ihr euch herbei,", "tokens": ["In", "wil\u00b7dem", "Tum\u00b7mel", "trollt", "ihr", "euch", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Me\u00dft aus und schlie\u00dft den Zirkel sonder Scheu,", "tokens": ["Me\u00dft", "aus", "und", "schlie\u00dft", "den", "Zir\u00b7kel", "son\u00b7der", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob zu hoffen kein Kolumb mehr bliebe!", "tokens": ["Als", "ob", "zu", "hof\u00b7fen", "kein", "Ko\u00b7lumb", "mehr", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PTKZU", "VVFIN", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Euch ist der eigne Leichnam noch nicht klar,", "tokens": ["Euch", "ist", "der", "eig\u00b7ne", "Leich\u00b7nam", "noch", "nicht", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr kennet nicht den Wurm zu euren F\u00fc\u00dfen,", "tokens": ["Ihr", "ken\u00b7net", "nicht", "den", "Wurm", "zu", "eu\u00b7ren", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Halmes Leben nicht auf eurem Grab;", "tokens": ["Des", "Hal\u00b7mes", "Le\u00b7ben", "nicht", "auf", "eu\u00b7rem", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dennoch kr\u00e4nzt ihr schon mit Stroh das Haar,", "tokens": ["Und", "den\u00b7noch", "kr\u00e4nzt", "ihr", "schon", "mit", "Stroh", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Eintagsg\u00f6tter stolz euch zu begr\u00fc\u00dfen \u2013", "tokens": ["Als", "Ein\u00b7tags\u00b7g\u00f6t\u00b7ter", "stolz", "euch", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Zweifel fehlt, der alte Wanderstab.", "tokens": ["Der", "Zwei\u00b7fel", "fehlt", ",", "der", "al\u00b7te", "Wan\u00b7der\u00b7stab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer ohne Leid, der ist auch ohne Liebe,", "tokens": ["Wer", "oh\u00b7ne", "Leid", ",", "der", "ist", "auch", "oh\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,