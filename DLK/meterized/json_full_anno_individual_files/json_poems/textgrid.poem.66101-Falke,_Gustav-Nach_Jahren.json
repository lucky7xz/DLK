{"textgrid.poem.66101": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Nach Jahren", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ruhenden, stillen Felder,", "tokens": ["Die", "ru\u00b7hen\u00b7den", ",", "stil\u00b7len", "Fel\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dar\u00fcber der Vollmond steht,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "der", "Voll\u00b7mond", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die weiten, schweigenden W\u00e4lder,", "tokens": ["Die", "wei\u00b7ten", ",", "schwei\u00b7gen\u00b7den", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Daher ein Schauer weht.", "tokens": ["Da\u00b7her", "ein", "Schau\u00b7er", "weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie hab ich selig genossen", "tokens": ["Wie", "hab", "ich", "se\u00b7lig", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die sch\u00f6ne Nachteinsamkeit", "tokens": ["Die", "sch\u00f6\u00b7ne", "Nach\u00b7tein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und habe den Schatz verschlossen", "tokens": ["Und", "ha\u00b7be", "den", "Schatz", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr kommende, d\u00fcrstende Zeit.", "tokens": ["F\u00fcr", "kom\u00b7men\u00b7de", ",", "d\u00fcrs\u00b7ten\u00b7de", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Nun tr\u00e4um ich die alten Tr\u00e4ume", "tokens": ["Nun", "tr\u00e4um", "ich", "die", "al\u00b7ten", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und r\u00fchre leise den Schatz,", "tokens": ["Und", "r\u00fch\u00b7re", "lei\u00b7se", "den", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sacht rauschen die alten B\u00e4ume,", "tokens": ["Sacht", "rau\u00b7schen", "die", "al\u00b7ten", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und alles am alten Platz.", "tokens": ["Und", "al\u00b7les", "am", "al\u00b7ten", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mir ist, als k\u00f6nnt ich gehen", "tokens": ["Mir", "ist", ",", "als", "k\u00f6nnt", "ich", "ge\u