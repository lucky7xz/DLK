{"dta.poem.7180": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur des HERRN Vollkommenheiten,", "tokens": ["Nur", "des", "HeRRN", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wobey auch die finstre Zahl,", "tokens": ["Wo\u00b7bey", "auch", "die", "finst\u00b7re", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ewiger Unendlichkeiten,", "tokens": ["E\u00b7wi\u00b7ger", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Nicht die erste Zahl einmahl,", "tokens": ["Nicht", "die", "ers\u00b7te", "Zahl", "ein\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uebersteigen so die Schrancken", "tokens": ["Ue\u00b7bers\u00b7tei\u00b7gen", "so", "die", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mensch- und Englischer Gedancken,", "tokens": ["Men\u00b7sch", "und", "Eng\u00b7li\u00b7scher", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Da\u00df, was aller Sch\u00e4rffe denckt,", "tokens": ["Da\u00df", ",", "was", "al\u00b7ler", "Sch\u00e4rf\u00b7fe", "denckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich wie nichts in Jhn versenckt.", "tokens": ["Sich", "wie", "nichts", "in", "Jhn", "ver\u00b7senckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}