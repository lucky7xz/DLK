{"textgrid.poem.67498": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Reformation", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbw\u00e4ren der Teufel so viel auch als hier Stein' auf den D\u00e4chern,", "tokens": ["\u00bb", "w\u00e4\u00b7ren", "der", "Teu\u00b7fel", "so", "viel", "auch", "als", "hier", "Stein'", "auf", "den", "D\u00e4\u00b7chern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "ADV", "ADV", "ADV", "KOKOM", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dennoch wagen wir es!\u00ab Also sprach Luther und ging", "tokens": ["Den\u00b7noch", "wa\u00b7gen", "wir", "es", "!", "\u00ab", "Al\u00b7so", "sprach", "Lu\u00b7ther", "und", "ging"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "$(", "ADV", "VVFIN", "NE", "KON", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Vor den Kaiser. Gelang's? Ich zweifle. Der Teufel an H\u00f6fen", "tokens": ["Vor", "den", "Kai\u00b7ser", ".", "Ge\u00b7lang's", "?", "Ich", "zweif\u00b7le", ".", "Der", "Teu\u00b7fel", "an", "H\u00f6\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN", "$.", "PPER", "VVFIN", "$.", "ART", "NN", "APPR", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Waren mehrere, fein wie der apulische Sand.", "tokens": ["Wa\u00b7ren", "meh\u00b7re\u00b7re", ",", "fein", "wie", "der", "a\u00b7pu\u00b7li\u00b7sche", "Sand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "$,", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Lehren bessertest Du, nicht Sitten. Sitten zu bessern,", "tokens": ["Leh\u00b7ren", "bes\u00b7ser\u00b7test", "Du", ",", "nicht", "Sit\u00b7ten", ".", "Sit\u00b7ten", "zu", "bes\u00b7sern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PTKNEG", "NN", "$.", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "War Der selber zu schwach, der auch die Teufel besiegt.", "tokens": ["War", "Der", "sel\u00b7ber", "zu", "schwach", ",", "der", "auch", "die", "Teu\u00b7fel", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "PTKA", "ADJD", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbw\u00e4ren der Teufel so viel auch als hier Stein' auf den D\u00e4chern,", "tokens": ["\u00bb", "w\u00e4\u00b7ren", "der", "Teu\u00b7fel", "so", "viel", "auch", "als", "hier", "Stein'", "auf", "den", "D\u00e4\u00b7chern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "ADV", "ADV", "ADV", "KOKOM", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dennoch wagen wir es!\u00ab Also sprach Luther und ging", "tokens": ["Den\u00b7noch", "wa\u00b7gen", "wir", "es", "!", "\u00ab", "Al\u00b7so", "sprach", "Lu\u00b7ther", "und", "ging"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word