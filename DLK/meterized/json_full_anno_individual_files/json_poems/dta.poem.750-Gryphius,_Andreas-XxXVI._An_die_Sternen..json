{"dta.poem.750": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.  An die Sternen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr lichter die ich nicht auff erden satt kan schawen/ ", "tokens": ["Ihr", "lich\u00b7ter", "die", "ich", "nicht", "auff", "er\u00b7den", "satt", "kan", "scha\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PRELS", "PPER", "PTKNEG", "APPR", "NN", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jhr fackeln die jhr stets das weite firmament", "tokens": ["Ihr", "fa\u00b7ckeln", "die", "jhr", "stets", "das", "wei\u00b7te", "fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit ewren flammen ziert/ vnd ohn auffh\u00f6ren brent; ", "tokens": ["Mit", "ew\u00b7ren", "flam\u00b7men", "ziert", "/", "vnd", "ohn", "auff\u00b7h\u00f6\u00b7ren", "brent", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "VVFIN", "$(", "KON", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr Blumen die jhr schm\u00fcckt des grossen himmels Awen:", "tokens": ["Ihr", "Blu\u00b7men", "die", "jhr", "schm\u00fcckt", "des", "gros\u00b7sen", "him\u00b7mels", "A\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Sein wort die weisheit selbst mit rechten nahmen nennt", "tokens": ["Sein", "wort", "die", "weis\u00b7heit", "selbst", "mit", "rech\u00b7ten", "nah\u00b7men", "nennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "APPR", "ADJA", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Gott allein recht misst/ die Gott allein recht kennt", "tokens": ["Die", "Gott", "al\u00b7lein", "recht", "misst", "/", "die", "Gott", "al\u00b7lein", "recht", "kennt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$(", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(wir blinden sterblichen! was wollen wir vns trawen!)", "tokens": ["(", "wir", "blin\u00b7den", "sterb\u00b7li\u00b7chen", "!", "was", "wol\u00b7len", "wir", "vns", "tra\u00b7wen", "!", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "VVINF", "$.", "PWS", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Jhr B\u00fcrgen meiner lust/ wie manche sch\u00f6ne Nacht", "tokens": ["Ihr", "B\u00fcr\u00b7gen", "mei\u00b7ner", "lust", "/", "wie", "man\u00b7che", "sch\u00f6\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$(", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab ich/ in dem ich euch betrachtete gewacht?", "tokens": ["Hab", "ich", "/", "in", "dem", "ich", "euch", "be\u00b7trach\u00b7te\u00b7te", "ge\u00b7wacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "APPR", "PRELS", "PPER", "PPER", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Regirer vnser zeit/ wenn wird es doch geschehen?", "tokens": ["Re\u00b7gi\u00b7rer", "vn\u00b7ser", "zeit", "/", "wenn", "wird", "es", "doch", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich/ der ewer nicht alhier vergessen kan/ ", "tokens": ["Da\u00df", "ich", "/", "der", "e\u00b7wer", "nicht", "al\u00b7hier", "ver\u00b7ges\u00b7sen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch/ derer Libe mir steckt ", "tokens": ["Euch", "/", "de\u00b7rer", "Li\u00b7be", "mir", "steckt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PDS", "NN", "PPER", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Von andern Sorgen frey werd vnter mir besehen. ", "tokens": ["Von", "an\u00b7dern", "Sor\u00b7gen", "frey", "werd", "vn\u00b7ter", "mir", "be\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}