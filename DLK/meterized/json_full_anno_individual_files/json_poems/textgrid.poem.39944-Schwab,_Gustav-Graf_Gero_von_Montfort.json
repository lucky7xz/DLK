{"textgrid.poem.39944": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Graf Gero von Montfort", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Montfort war's der greife Graf,", "tokens": ["Von", "Mont\u00b7fort", "wa\u00b7r's", "der", "grei\u00b7fe", "Graf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Ges\u00e4ttiget vom Leben,", "tokens": ["Ge\u00b7s\u00e4t\u00b7ti\u00b7get", "vom", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der sah den blauen See im Schlaf,", "tokens": ["Der", "sah", "den", "blau\u00b7en", "See", "im", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stille K\u00e4hne schweben,", "tokens": ["Und", "stil\u00b7le", "K\u00e4h\u00b7ne", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf Wasser, Erd' und Himmel Ruh;", "tokens": ["Auf", "Was\u00b7ser", ",", "Erd'", "und", "Him\u00b7mel", "Ruh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da flog sein Herz dem Frieden zu.", "tokens": ["Da", "flog", "sein", "Herz", "dem", "Frie\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als vom Traum er aufgewacht,", "tokens": ["Und", "als", "vom", "Traum", "er", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ruft er seine Knechte,", "tokens": ["Da", "ruft", "er", "sei\u00b7ne", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat sie belobt und gut bedacht,", "tokens": ["Hat", "sie", "be\u00b7lobt", "und", "gut", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmt Abschied vom Geschlechte,", "tokens": ["Nimmt", "Ab\u00b7schied", "vom", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verl\u00e4\u00dft die Herrschaft und das Schlo\u00df", "tokens": ["Ver\u00b7l\u00e4\u00dft", "die", "Herr\u00b7schaft", "und", "das", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zieht zum fernen Strand zu Ro\u00df.", "tokens": ["Und", "zieht", "zum", "fer\u00b7nen", "Strand", "zu", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie nun er an das Ufer trabt,", "tokens": ["Wie", "nun", "er", "an", "das", "U\u00b7fer", "trabt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt guten Wind er sausen,", "tokens": ["H\u00f6rt", "gu\u00b7ten", "Wind", "er", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und trifft am Strand den frommen Abt", "tokens": ["Und", "trifft", "am", "Strand", "den", "from\u00b7men", "Abt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom heil'gen Petershausen,", "tokens": ["Vom", "heil'\u00b7gen", "Pe\u00b7ter\u00b7shau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dazu ein Schiff, die Segel voll;", "tokens": ["Da\u00b7zu", "ein", "Schiff", ",", "die", "Se\u00b7gel", "voll", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O wie sein Herz von Sehnsucht schwoll!", "tokens": ["O", "wie", "sein", "Herz", "von", "Sehn\u00b7sucht", "schwoll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sankt Peters Haus, die stille Statt,", "tokens": ["Sankt", "Pe\u00b7ters", "Haus", ",", "die", "stil\u00b7le", "Statt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Wellen leis besp\u00fclet,", "tokens": ["Von", "Wel\u00b7len", "leis", "be\u00b7sp\u00fc\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Geist sich ausersehen hat,", "tokens": ["Sein", "Geist", "sich", "au\u00b7ser\u00b7se\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Ird'schen abgek\u00fchlet;", "tokens": ["Vom", "Ird'\u00b7schen", "ab\u00b7ge\u00b7k\u00fch\u00b7let", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dort will er dienen Gott dem Herrn,", "tokens": ["Dort", "will", "er", "die\u00b7nen", "Gott", "dem", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Lust und Pracht der Erde fern.", "tokens": ["Von", "Lust", "und", "Pracht", "der", "Er\u00b7de", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Abt erquickt der heil'ge Sinn,", "tokens": ["Den", "Abt", "er\u00b7quickt", "der", "heil'\u00b7ge", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hebt in's Schiff den Grafen;", "tokens": ["Er", "hebt", "in's", "Schiff", "den", "Gra\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl bringt dem Kloster das Gewinn,", "tokens": ["Wohl", "bringt", "dem", "Klos\u00b7ter", "das", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sto\u00dfen ab vom Hafen,", "tokens": ["Sie", "sto\u00b7\u00dfen", "ab", "vom", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon schwimmt das Schiff auf blauer Flut,", "tokens": ["Schon", "schwimmt", "das", "Schiff", "auf", "blau\u00b7er", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wird dem Greise da zu Mut!", "tokens": ["Wie", "wird", "dem", "Grei\u00b7se", "da", "zu", "Mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er spricht ger\u00fchrt: \u00bbO f\u00fchltet Ihr,", "tokens": ["Er", "spricht", "ge\u00b7r\u00fchrt", ":", "\u00bb", "O", "f\u00fchl\u00b7tet", "Ihr", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$.", "$(", "NE", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Abt, was ich empfinde!", "tokens": ["Herr", "Abt", ",", "was", "ich", "emp\u00b7fin\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es blickt das Wasser auf zu mir,", "tokens": ["Es", "blickt", "das", "Was\u00b7ser", "auf", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Mutter nach dem Kinde!", "tokens": ["Wie", "Mut\u00b7ter", "nach", "dem", "Kin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn wi\u00dft, bei jenes Hornes", "tokens": ["Denn", "wi\u00dft", ",", "bei", "je\u00b7nes", "Hor\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geboren ward ich einst im Schiff.", "tokens": ["Ge\u00b7bo\u00b7ren", "ward", "ich", "einst", "im", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und wenn ich in dem Nachen bin,", "tokens": ["Und", "wenn", "ich", "in", "dem", "Na\u00b7chen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sanft geschaukelt liege,", "tokens": ["So", "sanft", "ge\u00b7schau\u00b7kelt", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird mir wie einem Kind zu Sinn,", "tokens": ["Wird", "mir", "wie", "ei\u00b7nem", "Kind", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich ruh' in meiner Wiege,", "tokens": ["Ich", "ruh'", "in", "mei\u00b7ner", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Mutter lispelt in mein Ohr", "tokens": ["Die", "Mut\u00b7ter", "lis\u00b7pelt", "in", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und singt ein Schlummerlied mir vor.\u00ab", "tokens": ["Und", "singt", "ein", "Schlum\u00b7mer\u00b7lied", "mir", "vor", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Derweil sie segeln frisch nach vorn;", "tokens": ["Der\u00b7weil", "sie", "se\u00b7geln", "frisch", "nach", "vorn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da \u00fcbermannt's den Grafen,", "tokens": ["Da", "\u00fc\u00b7ber\u00b7mannt's", "den", "Gra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sind nicht ferne mehr vom Horn,", "tokens": ["Sie", "sind", "nicht", "fer\u00b7ne", "mehr", "vom", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hebt er an zu schlafen,", "tokens": ["So", "hebt", "er", "an", "zu", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und bei der Ruder gleichem Schlag", "tokens": ["Und", "bei", "der", "Ru\u00b7der", "glei\u00b7chem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er schlummernd auf dem Schiffe lag.", "tokens": ["Er", "schlum\u00b7mernd", "auf", "dem", "Schif\u00b7fe", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wie das Schiff vor\u00fcber zieht,", "tokens": ["Und", "wie", "das", "Schiff", "vor\u00b7\u00fc\u00b7ber", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort, wo er ward geboren,", "tokens": ["Dort", ",", "wo", "er", "ward", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da t\u00f6nt das s\u00fc\u00dfe Wiegenlied", "tokens": ["Da", "t\u00f6nt", "das", "s\u00fc\u00b7\u00dfe", "Wie\u00b7gen\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hell in seinen Ohren;", "tokens": ["So", "hell", "in", "sei\u00b7nen", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er schlug die Augen auf und rief:", "tokens": ["Er", "schlug", "die", "Au\u00b7gen", "auf", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbo Mutter, wie so tief ich schlief!\u00ab", "tokens": ["\u00bb", "o", "Mut\u00b7ter", ",", "wie", "so", "tief", "ich", "schlief", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "PWAV", "ADV", "ADJD", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er schlo\u00df die Augen wieder zu,", "tokens": ["Er", "schlo\u00df", "die", "Au\u00b7gen", "wie\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch tiefer fort zu schlafen.", "tokens": ["Noch", "tie\u00b7fer", "fort", "zu", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Steh Nachen still, nicht eile du!", "tokens": ["Steh", "Na\u00b7chen", "still", ",", "nicht", "ei\u00b7le", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,", "PTKNEG", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Gast ist schon im Hafen;", "tokens": ["Dein", "Gast", "ist", "schon", "im", "Ha\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Abt zu seinen F\u00fc\u00dfen kniet,", "tokens": ["Der", "Abt", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "kniet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihn mit dem letzten Trost versieht.", "tokens": ["Ihn", "mit", "dem", "letz\u00b7ten", "Trost", "ver\u00b7sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Bringt ihn zum heil'gen Haus hinab,", "tokens": ["Bringt", "ihn", "zum", "heil'\u00b7gen", "Haus", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt in den Chor den Frommen:", "tokens": ["Legt", "in", "den", "Chor", "den", "From\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Do