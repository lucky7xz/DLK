{"textgrid.poem.41365": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Jupiter und die Schnecke", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jupiter verhie\u00df den Thieren, die er in der Welt erschuf,", "tokens": ["Ju\u00b7pi\u00b7ter", "ver\u00b7hie\u00df", "den", "Thie\u00b7ren", ",", "die", "er", "in", "der", "Welt", "er\u00b7schuf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Das zu geben, was sie w\u00fcnschten. Jedes kam auf seinen Ruf.", "tokens": ["Das", "zu", "ge\u00b7ben", ",", "was", "sie", "w\u00fcnschten", ".", "Je\u00b7des", "kam", "auf", "sei\u00b7nen", "Ruf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$.", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Alle w\u00fcnschten, alle baten; was sie baten, ward verliehn.", "tokens": ["Al\u00b7le", "w\u00fcnschten", ",", "al\u00b7le", "ba\u00b7ten", ";", "was", "sie", "ba\u00b7ten", ",", "ward", "ver\u00b7liehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$.", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Zu den andern kroch die Schnecke, bis sie vor dem Zeus erschien.", "tokens": ["Zu", "den", "an\u00b7dern", "kroch", "die", "Schne\u00b7cke", ",", "bis", "sie", "vor", "dem", "Zeus", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Diese sprach: O Haubt der G\u00f6tter, la\u00df mich doch ein Haus erflehn,", "tokens": ["Die\u00b7se", "sprach", ":", "O", "Haubt", "der", "G\u00f6t\u00b7ter", ",", "la\u00df", "mich", "doch", "ein", "Haus", "er\u00b7flehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "NE", "NN", "ART", "NN", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Das nur mir, nicht andern, dienet, still darin herumzugehn!", "tokens": ["Das", "nur", "mir", ",", "nicht", "an\u00b7dern", ",", "die\u00b7net", ",", "still", "da\u00b7rin", "her\u00b7um\u00b7zu\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "$,", "PTKNEG", "ADJA", "$,", "VVFIN", "$,", "ADJD", "PAV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wenigstens bleibt meine Wohnung von Verdrie\u00dflichen befreit,", "tokens": ["We\u00b7nigs\u00b7tens", "bleibt", "mei\u00b7ne", "Woh\u00b7nung", "von", "Ver\u00b7drie\u00df\u00b7li\u00b7chen", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Ich entschleiche vielen Forschern, vielen Neidern, vielem Streit.", "tokens": ["Ich", "ent\u00b7schlei\u00b7che", "vie\u00b7len", "For\u00b7schern", ",", "vie\u00b7len", "Nei\u00b7dern", ",", "vie\u00b7lem", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIS", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Tausend m\u00f6gen stolzer w\u00e4hlen; jeder Segen, der mir bl\u00fcht,", "tokens": ["Tau\u00b7send", "m\u00f6\u00b7gen", "stol\u00b7zer", "w\u00e4h\u00b7len", ";", "je\u00b7der", "Se\u00b7gen", ",", "der", "mir", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "VMFIN", "ADJD", "VVINF", "$.", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Bl\u00fcht mir sch\u00f6ner und gedoppelt, wann ein B\u00f6ser ihn nicht sieht.", "tokens": ["Bl\u00fcht", "mir", "sch\u00f6\u00b7ner", "und", "ge\u00b7dop\u00b7pelt", ",", "wann", "ein", "B\u00f6\u00b7ser", "ihn", "nicht", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "KON", "VVPP", "$,", "PWAV", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Wahl und Vortrag ward gebilligt: Jupiter ging dieses ein,", "tokens": ["Wahl", "und", "Vor\u00b7trag", "ward", "ge\u00b7bil\u00b7ligt", ":", "Ju\u00b7pi\u00b7ter", "ging", "die\u00b7ses", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$.", "NN", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "+-+-+-+-+---+-+", "measure": "unknown.measure.septa"},