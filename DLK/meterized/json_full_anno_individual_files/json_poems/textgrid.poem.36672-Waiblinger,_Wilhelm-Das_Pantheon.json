{"textgrid.poem.36672": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Pantheon", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft in der Mittern\u00e4chte Schweigen", "tokens": ["Oft", "in", "der", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Pfleg' ich mit leisem Geistertritt", "tokens": ["Pfleg'", "ich", "mit", "lei\u00b7sem", "Geis\u00b7ter\u00b7tritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kapitol herabzusteigen,", "tokens": ["Das", "Ka\u00b7pi\u00b7tol", "her\u00b7ab\u00b7zu\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schnell befl\u00fcgelt sich mein Schritt,", "tokens": ["Und", "schnell", "be\u00b7fl\u00fc\u00b7gelt", "sich", "mein", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die dunkeln Wege wandl' ich schnelle,", "tokens": ["Die", "dun\u00b7keln", "We\u00b7ge", "wandl'", "ich", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die nur die tiefste Sehnsucht kennt,", "tokens": ["Die", "nur", "die", "tiefs\u00b7te", "Sehn\u00b7sucht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo selten noch ein Lichtchen helle", "tokens": ["Wo", "sel\u00b7ten", "noch", "ein", "Licht\u00b7chen", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vorm Bild der Mutter Gottes brennt.", "tokens": ["Vorm", "Bild", "der", "Mut\u00b7ter", "Got\u00b7tes", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da h\u00f6r' ich durch die d\u00fcstre Stille,", "tokens": ["Da", "h\u00f6r'", "ich", "durch", "die", "d\u00fcst\u00b7re", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In der so gern die Trauer sinnt,", "tokens": ["In", "der", "so", "gern", "die", "Trau\u00b7er", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schon des Brunnens k\u00fchle F\u00fclle", "tokens": ["Wie", "schon", "des", "Brun\u00b7nens", "k\u00fch\u00b7le", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Marmorbecken niederrinnt,", "tokens": ["Ins", "Mar\u00b7mor\u00b7be\u00b7cken", "nie\u00b7der\u00b7rinnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und pl\u00f6tzlich \u2013 als erst\u00fcnd' es eben,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "\u2013", "als", "er\u00b7st\u00fcnd'", "es", "e\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "KOKOM", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein hoher Geist, vom Grab empor \u2013", "tokens": ["Ein", "ho\u00b7her", "Geist", ",", "vom", "Grab", "em\u00b7por", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O G\u00f6tter Roms, ihr habt mein Leben!", "tokens": ["O", "G\u00f6t\u00b7ter", "Roms", ",", "ihr", "habt", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Taucht's herrlich aus der Nacht hervor.", "tokens": ["Taucht's", "herr\u00b7lich", "aus", "der", "Nacht", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O wie mit namenlosem Schauer", "tokens": ["O", "wie", "mit", "na\u00b7men\u00b7lo\u00b7sem", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4ngt Herz und Auge da an dir,", "tokens": ["H\u00e4ngt", "Herz", "und", "Au\u00b7ge", "da", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie voll schwermuthsvoller Trauer,", "tokens": ["Und", "wie", "voll", "schwer\u00b7muths\u00b7vol\u00b7ler", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll heil'gem Ernst erscheinst du mir,", "tokens": ["Voll", "heil'\u00b7gem", "Ernst", "er\u00b7scheinst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Stolz der Vorwelt und der Ahnen,", "tokens": ["Du", "Stolz", "der", "Vor\u00b7welt", "und", "der", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du Riesenkind voll Majest\u00e4t,", "tokens": ["Du", "Rie\u00b7sen\u00b7kind", "voll", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von V\u00f6lkerst\u00fcrmen und Orkanen", "tokens": ["Von", "V\u00f6l\u00b7ker\u00b7st\u00fcr\u00b7men", "und", "Or\u00b7ka\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Fast zwei Jahrtausende umweht,", "tokens": ["Fast", "zwei", "Jahr\u00b7tau\u00b7sen\u00b7de", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das sich, der dunkeln Macht der Horen,", "tokens": ["Das", "sich", ",", "der", "dun\u00b7keln", "Macht", "der", "Ho\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schicksal seines Roms zum Spott,", "tokens": ["Dem", "Schick\u00b7sal", "sei\u00b7nes", "Roms", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum gro\u00dfen Liebling auserkoren", "tokens": ["Zum", "gro\u00b7\u00dfen", "Lieb\u00b7ling", "au\u00b7ser\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein alter heil'ger Donnergott,", "tokens": ["Dein", "al\u00b7ter", "heil'\u00b7ger", "Don\u00b7ner\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Tempel, und mein h\u00f6chstes Sehnen", "tokens": ["Mein", "Tem\u00b7pel", ",", "und", "mein", "h\u00f6chs\u00b7tes", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der zarten Kindersehnsucht schon,", "tokens": ["Der", "zar\u00b7ten", "Kin\u00b7der\u00b7sehn\u00b7sucht", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du Opferschaale meiner Thr\u00e4nen,", "tokens": ["Du", "Op\u00b7fer\u00b7schaa\u00b7le", "mei\u00b7ner", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nun meine Braut, o Pantheon!", "tokens": ["Nun", "mei\u00b7ne", "Braut", ",", "o", "Pan\u00b7the\u00b7on", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir ist, es sei dir zugeschworen,", "tokens": ["Mir", "ist", ",", "es", "sei", "dir", "zu\u00b7ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4rest du mein gr\u00f6\u00df'res Herz", "tokens": ["Als", "w\u00e4\u00b7rest", "du", "mein", "gr\u00f6\u00df'\u00b7res", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur k\u00fchnen Sch\u00f6pfung ausgeboren,", "tokens": ["Zur", "k\u00fch\u00b7nen", "Sch\u00f6p\u00b7fung", "aus\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "All mein Gesang mit seinem Schmerz,", "tokens": ["All", "mein", "Ge\u00b7sang", "mit", "sei\u00b7nem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum hohen Marmorbild ger\u00fcndet,", "tokens": ["Zum", "ho\u00b7hen", "Mar\u00b7mor\u00b7bild", "ge\u00b7r\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der G\u00f6tter Herrlichstem geweiht,", "tokens": ["Der", "G\u00f6t\u00b7ter", "Herr\u00b7lichs\u00b7tem", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf ew'gen S\u00e4ulen fest gegr\u00fcndet,", "tokens": ["Auf", "ew'\u00b7gen", "S\u00e4u\u00b7len", "fest", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sein Altar Unsterblichkeit.", "tokens": ["Und", "sein", "Al\u00b7tar", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Der Wand'rer sieht mit sel'gen Blicken", "tokens": ["Der", "Wan\u00b7d'\u00b7rer", "sieht", "mit", "sel'\u00b7gen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Roms Forum in der Abendgluth,", "tokens": ["Roms", "Fo\u00b7rum", "in", "der", "A\u00b7bend\u00b7gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo unter m\u00e4cht'gen Tempelst\u00fccken", "tokens": ["Wo", "un\u00b7ter", "m\u00e4cht'\u00b7gen", "Tem\u00b7pel\u00b7st\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der breitgeh\u00f6rnte Stier nun ruht,", "tokens": ["Der", "breit\u00b7ge\u00b7h\u00f6rn\u00b7te", "Stier", "nun", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sanft umbl\u00fcht von frischem Gr\u00fcne,", "tokens": ["Und", "sanft", "um\u00b7bl\u00fcht", "von", "fri\u00b7schem", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durchstrahlt von Gold und Himmelblau,", "tokens": ["Durch\u00b7strahlt", "von", "Gold", "und", "Him\u00b7mel\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Vorwelt furchtbarste Ruine,", "tokens": ["Der", "Vor\u00b7welt", "furcht\u00b7bars\u00b7te", "Ru\u00b7i\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Des Colosseums Riesenbau.", "tokens": ["Des", "Co\u00b7los\u00b7se\u00b7ums", "Rie\u00b7sen\u00b7bau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch fl\u00fccht ich stets aus diesem Grause", "tokens": ["Doch", "fl\u00fccht", "ich", "stets", "aus", "die\u00b7sem", "Grau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erinnrungsvoller Einsamkeit", "tokens": ["E\u00b7rinn\u00b7rungs\u00b7vol\u00b7ler", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich wieder zu dem G\u00f6tterhause,", "tokens": ["Mich", "wie\u00b7der", "zu", "dem", "G\u00f6t\u00b7ter\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo eingeh\u00fcllt in Dunkelheit,", "tokens": ["Wo", "ein\u00b7ge\u00b7h\u00fcllt", "in", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von tiefem Schatten nur gehoben,", "tokens": ["Von", "tie\u00b7fem", "Schat\u00b7ten", "nur", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die stolze S\u00e4ulenhalle blickt,", "tokens": ["Die", "stol\u00b7ze", "S\u00e4u\u00b7len\u00b7hal\u00b7le", "blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber seiner W\u00f6lbung oben", "tokens": ["Und", "\u00fc\u00b7ber", "sei\u00b7ner", "W\u00f6l\u00b7bung", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mich nur ein einz'ger Stern entz\u00fcckt.", "tokens": ["Mich", "nur", "ein", "einz'\u00b7ger", "Stern", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+