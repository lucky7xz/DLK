{"textgrid.poem.53126": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Martin Neuman und Maria Paschke", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das gew\u00fcnschte Fr\u00fcelings-Liecht", "tokens": ["Das", "ge\u00b7w\u00fcnschte", "Fr\u00fce\u00b7lings\u00b7Liecht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird vns ja einmahl erfrewen,", "tokens": ["Wird", "vns", "ja", "ein\u00b7mahl", "er\u00b7fre\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df der Erden Angesicht", "tokens": ["Da\u00df", "der", "Er\u00b7den", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich sampt vnserm mag vernewen", "tokens": ["Sich", "sampt", "vn\u00b7serm", "mag", "ver\u00b7ne\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "PPER", "VMFIN", "VVINF"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Vnd ein warmer Sonnen-Schein", "tokens": ["Vnd", "ein", "war\u00b7mer", "Son\u00b7nen\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vns verg\u00f6nn' entpeltzt zu seyn.", "tokens": ["Vns", "ver\u00b7g\u00f6nn'", "ent\u00b7peltzt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der noch kahlen B\u00e4ume Safft", "tokens": ["Der", "noch", "kah\u00b7len", "B\u00e4u\u00b7me", "Safft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aengstigt fast sich in den Zweigen,", "tokens": ["A\u00b7engs\u00b7tigt", "fast", "sich", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sehnt sich mit belebter Krafft", "tokens": ["Sehnt", "sich", "mit", "be\u00b7leb\u00b7ter", "Krafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer Gipffel-an zu steigen,", "tokens": ["Im\u00b7mer", "Gipf\u00b7fel\u00b7an", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist gantz zur Geburt geschickt,", "tokens": ["Ist", "gantz", "zur", "Ge\u00b7burt", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn jhn W\u00e4rme nur erquickt.", "tokens": ["Wenn", "jhn", "W\u00e4r\u00b7me", "nur", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Des sehr kalten Nordens Macht", "tokens": ["Des", "sehr", "kal\u00b7ten", "Nor\u00b7dens", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt den B\u00e4umen vnd der Erden", "tokens": ["Liegt", "den", "B\u00e4u\u00b7men", "vnd", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Grawsam vor der Th\u00fcr vnd wacht,", "tokens": ["Graw\u00b7sam", "vor", "der", "Th\u00fcr", "vnd", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df nichts kan ge\u00f6ffnet werden.", "tokens": ["Da\u00df", "nichts", "kan", "ge\u00b7\u00f6ff\u00b7net", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles br\u00e4che stracks hervor,", "tokens": ["Al\u00b7les", "br\u00e4\u00b7che", "stracks", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00e4umte dieser Feind das Thor.", "tokens": ["R\u00e4um\u00b7te", "die\u00b7ser", "Feind", "das", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sonne, Printz vnd Mah\u00df der Zeit,", "tokens": ["Son\u00b7ne", ",", "Printz", "vnd", "Mah\u00df", "der", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du Gebieter aller Stunden,", "tokens": ["Du", "Ge\u00b7bie\u00b7ter", "al\u00b7ler", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bild der g\u00fcldnen Ewigheit,", "tokens": ["Bild", "der", "g\u00fcld\u00b7nen", "E\u00b7wig\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df erfrewlich seyn entbunden", "tokens": ["La\u00df", "er\u00b7frew\u00b7lich", "seyn", "ent\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "VAINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was die tr\u00fcbe Vorjahrs-K\u00e4lt'", "tokens": ["Was", "die", "tr\u00fc\u00b7be", "Vor\u00b7jahr\u00b7sK\u00e4lt'"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hin vnd her bestricket h\u00e4lt.", "tokens": ["Hin", "vnd", "her", "be\u00b7stri\u00b7cket", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wirff die angenehme Strahlen", "tokens": ["Wirff", "die", "an\u00b7ge\u00b7neh\u00b7me", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Erden weites Hau\u00df,", "tokens": ["Durch", "der", "Er\u00b7den", "wei\u00b7tes", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sie Thal vnd Berge mahlen,", "tokens": ["La\u00df", "sie", "Thal", "vnd", "Ber\u00b7ge", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fcck Feld, Wald vnd Wiesen aus,", "tokens": ["Schm\u00fcck", "Feld", ",", "Wald", "vnd", "Wie\u00b7sen", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Gib durch einen warmen Ost", "tokens": ["Gib", "durch", "ei\u00b7nen", "war\u00b7men", "Ost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vieh vnd Menschen Gn\u00fcg vnd Trost.", "tokens": ["Vieh", "vnd", "Men\u00b7schen", "Gn\u00fcg", "vnd", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zwar man hat der LerchenThon", "tokens": ["Zwar", "man", "hat", "der", "Ler\u00b7chen", "Thon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine gutte Zeit vernommen,", "tokens": ["Ei\u00b7ne", "gut\u00b7te", "Zeit", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd der trewe Storch ist schon", "tokens": ["Vnd", "der", "tre\u00b7we", "Storch", "ist", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auff sein alttes Nest gekommen,", "tokens": ["Auff", "sein", "alt\u00b7tes", "Nest", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Haaff vnd Pregel rinnen klar,", "tokens": ["Haaff", "vnd", "Pre\u00b7gel", "rin\u00b7nen", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur der Frost wehrt jmmerdar.", "tokens": ["Nur", "der", "Frost", "wehrt", "jm\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Was geht die\u00df den Br\u00e4utgam an?", "tokens": ["Was", "geht", "die\u00df", "den", "Br\u00e4ut\u00b7gam", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "ART", "NE", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gl\u00fcck und Zeit giebt jhm gewonnen.", "tokens": ["Gl\u00fcck", "und", "Zeit", "giebt", "jhm", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was sein Hertz vernewen kan,", "tokens": ["Was", "sein", "Hertz", "ver\u00b7ne\u00b7wen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nicht Lufft, nicht Glantz der Sonnen;", "tokens": ["Ist", "nicht", "Lufft", ",", "nicht", "Glantz", "der", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seiner Liebsten A\u00fcgelein", "tokens": ["Sei\u00b7ner", "Liebs\u00b7ten", "A\u00b7\u00fc\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sind jhm Lufft vnd Sonnen-Schein.", "tokens": ["Sind", "jhm", "Lufft", "vnd", "Son\u00b7nen\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dieser keuschen Einfalt Muth,", "tokens": ["Die\u00b7ser", "keu\u00b7schen", "Ein\u00b7falt", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses lieben Fr\u00fclings Gaben,", "tokens": ["Die\u00b7ses", "lie\u00b7ben", "Fr\u00fc\u00b7lings", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Newheit, dieses Gut", "tokens": ["Die\u00b7se", "New\u00b7heit", ",", "die\u00b7ses", "Gut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wil Herr Newman endlich haben:", "tokens": ["Wil", "Herr", "New\u00b7man", "end\u00b7lich", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NE", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd wird gnugsam seyn erfrewt,", "tokens": ["Vnd", "wird", "gnug\u00b7sam", "seyn", "er\u00b7frewt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun er so sein Hertz vernewt.", "tokens": ["Nun", "er", "so", "sein", "Hertz", "ver\u00b7newt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Gottesfurcht, der Demut Kunst,", "tokens": ["Got\u00b7tes\u00b7furcht", ",", "der", "De\u00b7mut", "Kunst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trew vnd Flei\u00df in allen Sachen,", "tokens": ["Trew", "vnd", "Flei\u00df", "in", "al\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd so hoher Leute Gunst", "tokens": ["Vnd", "so", "ho\u00b7her", "Leu\u00b7te", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolten jhn vernewert machen,", "tokens": ["Wol\u00b7ten", "jhn", "ver\u00b7ne\u00b7wert", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ein grosses Gnaden-Pfandt", "tokens": ["Die", "ein", "gros\u00b7ses", "Gna\u00b7den\u00b7Pfandt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm vom F\u00fcrsten zugewandt.", "tokens": ["Ihm", "vom", "F\u00fcrs\u00b7ten", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Die\u00df ist aber eigentlich", "tokens": ["Die\u00df", "ist", "a\u00b7ber", "ei\u00b7gent\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seines Lebens Zweck gewesen,", "tokens": ["Sei\u00b7nes", "Le\u00b7bens", "Zweck", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hie vernewt er erstlich sich,", "tokens": ["Hie", "ver\u00b7newt", "er", "erst\u00b7lich", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hiedurch kan er recht genesen,", "tokens": ["Hie\u00b7durch", "kan", "er", "recht", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jetzt bekennt er allen frey,", "tokens": ["Jetzt", "be\u00b7kennt", "er", "al\u00b7len", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df er recht ein New-Mann sey.", "tokens": ["Da\u00df", "er", "recht", "ein", "Ne\u00b7wMann", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", 