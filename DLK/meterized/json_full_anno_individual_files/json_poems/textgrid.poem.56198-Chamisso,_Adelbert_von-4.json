{"textgrid.poem.56198": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stolz, mein Stolz, wohin gekommen!", "tokens": ["Stolz", ",", "mein", "Stolz", ",", "wo\u00b7hin", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PWAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ein armes, armes Kind,", "tokens": ["Bin", "ein", "ar\u00b7mes", ",", "ar\u00b7mes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Augen, ausgeglommen,", "tokens": ["De\u00b7ren", "Au\u00b7gen", ",", "aus\u00b7ge\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "NN", "$,", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur zu weinen tauglich sind.", "tokens": ["Nur", "zu", "wei\u00b7nen", "taug\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lesen kann ich in den seinen", "tokens": ["Le\u00b7sen", "kann", "ich", "in", "den", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht das heimlich tiefe Wort,", "tokens": ["Nicht", "das", "heim\u00b7lich", "tie\u00b7fe", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine schweigen, aber weinen,", "tokens": ["Mei\u00b7ne", "schwei\u00b7gen", ",", "a\u00b7ber", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinen, weinen fort und fort.", "tokens": ["Wei\u00b7nen", ",", "wei\u00b7nen", "fort", "und", "fort", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja wir sind getrennt! In Scherzen", "tokens": ["Ja", "wir", "sind", "ge\u00b7trennt", "!", "In", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "PPER", "VAFIN", "VVPP", "$.", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und in Freuden wandelst du,", "tokens": ["Und", "in", "Freu\u00b7den", "wan\u00b7delst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber mich und meine Schmerzen", "tokens": ["\u00dc\u00b7ber", "mich", "und", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4gt die Nacht die Fl\u00fcgel zu.", "tokens": ["Schl\u00e4gt", "die", "Nacht", "die", "Fl\u00fc\u00b7gel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}