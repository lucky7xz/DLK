{"dta.poem.11774": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer etwas Gutes schafft, der halt' es nur f\u00fcrs Beste,", "tokens": ["Wer", "et\u00b7was", "Gu\u00b7tes", "schafft", ",", "der", "halt'", "es", "nur", "f\u00fcrs", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er sich ganz darin best\u00e4rke und befeste.", "tokens": ["Da\u00df", "er", "sich", "ganz", "da\u00b7rin", "be\u00b7st\u00e4r\u00b7ke", "und", "be\u00b7fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PAV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er mag, was Gutes sonst, was Bessres sei, vergessen,", "tokens": ["Er", "mag", ",", "was", "Gu\u00b7tes", "sonst", ",", "was", "Bess\u00b7res", "sei", ",", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWS", "NN", "ADV", "$,", "PRELS", "PIS", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das aufs beste thun, was ihm ist angemessen.", "tokens": ["Und", "das", "aufs", "bes\u00b7te", "thun", ",", "was", "ihm", "ist", "an\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "ADJA", "VVINF", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch gut ists auch, da\u00df ers erkenn' als mangelhaft,", "tokens": ["Doch", "gut", "ists", "auch", ",", "da\u00df", "ers", "er\u00b7kenn'", "als", "man\u00b7gel\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einseitig, und beschr\u00e4nkt nach seiner Eigenschaft.", "tokens": ["Ein\u00b7sei\u00b7tig", ",", "und", "be\u00b7schr\u00e4nkt", "nach", "sei\u00b7ner", "Ei\u00b7gen\u00b7schaft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht schelten wird er dann den andern, der ihn schilt,", "tokens": ["Nicht", "schel\u00b7ten", "wird", "er", "dann", "den", "an\u00b7dern", ",", "der", "ihn", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil das nicht gelten kann der Welt, was dir nur gilt.", "tokens": ["Weil", "das", "nicht", "gel\u00b7ten", "kann", "der", "Welt", ",", "was", "dir", "nur", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PTKNEG", "VVINF", "VMFIN", "ART", "NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}