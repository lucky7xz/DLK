{"dta.poem.23591": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fr\u00f6sch' und die Unken", "tokens": ["Die", "Fr\u00f6sch'", "und", "die", "Un\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und andre Hallunken,", "tokens": ["Und", "and\u00b7re", "Hal\u00b7lun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die k\u00f6nnen nur zechen", "tokens": ["Die", "k\u00f6n\u00b7nen", "nur", "ze\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "VVINF"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Mit r\u00f6chelnden Rachen,", "tokens": ["Mit", "r\u00f6\u00b7cheln\u00b7den", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie schl\u00fcrfen aus B\u00e4chen,", "tokens": ["Sie", "schl\u00fcr\u00b7fen", "aus", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Aus Pf\u00fctzen und Lachen,", "tokens": ["Aus", "Pf\u00fct\u00b7zen", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Aus Gruben und Kl\u00fcften,", "tokens": ["Aus", "Gru\u00b7ben", "und", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Aus Weihern und Teichen,", "tokens": ["Aus", "Wei\u00b7hern", "und", "Tei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Aus Gr\u00e4ben und Gr\u00fcften", "tokens": ["Aus", "Gr\u00e4\u00b7ben", "und", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und manchem dergleichen,", "tokens": ["Und", "man\u00b7chem", "derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und pl\u00e4rren im Chor", "tokens": ["Und", "pl\u00e4r\u00b7ren", "im", "Chor"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Auf Modder und Moor", "tokens": ["Auf", "Mod\u00b7der", "und", "Moor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Nur Schnickschnack, Schnackschnack,", "tokens": ["Nur", "Schnick\u00b7schnack", ",", "Schnack\u00b7schnack", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Und Unkunk, Quackquack.", "tokens": ["Und", "Un\u00b7kunk", ",", "Quack\u00b7quack", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Wir sitzen so sinnig,", "tokens": ["Wir", "sit\u00b7zen", "so", "sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Treuherzig und minnig,", "tokens": ["Treu\u00b7her\u00b7zig", "und", "min\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Wir frohen Gesellen,", "tokens": ["Wir", "fro\u00b7hen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Wir machen es besser,", "tokens": ["Wir", "ma\u00b7chen", "es", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Denn unsere Quellen", "tokens": ["Denn", "un\u00b7se\u00b7re", "Quel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Sind Flaschen und F\u00e4sser;", "tokens": ["Sind", "Fla\u00b7schen", "und", "F\u00e4s\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Wir lassen sie flie\u00dfen", "tokens": ["Wir", "las\u00b7sen", "sie", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Bei Lachen und Scherzen,", "tokens": ["Bei", "La\u00b7chen", "und", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Bis sie sich ergie\u00dfen", "tokens": ["Bis", "sie", "sich", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "In unsere Herzen;", "tokens": ["In", "un\u00b7se\u00b7re", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Draus t\u00f6nt dann der Wein", "tokens": ["Draus", "t\u00f6nt", "dann", "der", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Gar lieblich und fein", "tokens": ["Gar", "lieb\u00b7lich", "und", "fein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Nur Liedes-Singsang", "tokens": ["Nur", "Lie\u00b7des\u00b7Sing\u00b7sang"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Und Liebes-Klingklang.", "tokens": ["Und", "Lie\u00b7bes\u00b7Kling\u00b7klang", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}