{"dta.poem.964": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "Spr\u00fcche f\u00fcr die geladenen in T . . .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Indess deine mutter dich stillt                 ", "tokens": ["In\u00b7dess", "dei\u00b7ne", "mut\u00b7ter", "dich", "stillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Soll eine leidige fee                 ", "tokens": ["Soll", "ei\u00b7ne", "lei\u00b7di\u00b7ge", "fee"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Von schatten singen und tod                 ", "tokens": ["Von", "schat\u00b7ten", "sin\u00b7gen", "und", "tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sie giebt dir als pathengeschenk                 ", "tokens": ["Sie", "giebt", "dir", "als", "pa\u00b7then\u00b7ge\u00b7schenk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Augen so tr&#252;b und sonder                 ", "tokens": ["Au\u00b7gen", "so", "tr", "&#252;", "b", "und", "son\u00b7der"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "$(", "XY", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "In die sich die musen versenken               ", "tokens": ["In", "die", "sich", "die", "mu\u00b7sen", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ver&#228;chtlich wirst du blicken                 ", "tokens": ["Ver", "&#228;", "cht\u00b7lich", "wirst", "du", "bli\u00b7cken"], "token_info": ["word", "XML_entity", "word", "word", "word", "word"], "pos": ["NN", "$(", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf roher spiele gebahren                 ", "tokens": ["Auf", "ro\u00b7her", "spie\u00b7le", "ge\u00b7bah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor arbeit die niedrig macht                 ", "tokens": ["Vor", "ar\u00b7beit", "die", "nied\u00b7rig", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJD", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die grossen strengen gedanken                 ", "tokens": ["Die", "gros\u00b7sen", "stren\u00b7gen", "ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dich mahnen und wahren               ", "tokens": ["Dich", "mah\u00b7nen", "und", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn deine br&#252;der klagen                 ", "tokens": ["Wenn", "dei\u00b7ne", "br", "&#252;", "der", "kla\u00b7gen"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$(", "ART", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sagen: o schmerz! den deinen                 ", "tokens": ["Und", "sa\u00b7gen", ":", "o", "schmerz", "!", "den", "dei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "$.", "FM", "ADJD", "$.", "ART", "PPOSAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sag ihn den winden bei nacht                 ", "tokens": ["Sag", "ihn", "den", "win\u00b7den", "bei", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "APPR", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und unter der n&#228;gel waffe                 ", "tokens": ["Und", "un\u00b7ter", "der", "n", "&#228;", "gel", "waf\u00b7fe"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["KON", "APPR", "ART", "XY", "$(", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Blute die kindliche brust               ", "tokens": ["Blu\u00b7te", "die", "kind\u00b7li\u00b7che", "brust"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Vergiss es nicht: du musst                 ", "tokens": ["Ver\u00b7giss", "es", "nicht", ":", "du", "musst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Deine frische jugend t\u00f6ten                 ", "tokens": ["Dei\u00b7ne", "fri\u00b7sche", "ju\u00b7gend", "t\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf ihrem grab allein                 ", "tokens": ["Auf", "ih\u00b7rem", "grab", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn viele thr&#228;nen es begiessen &#8212; spriessen                 ", "tokens": ["Wenn", "vie\u00b7le", "thr", "&#228;", "nen", "es", "be\u00b7gies\u00b7sen", "&#8212;", "spries\u00b7sen"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PIAT", "NN", "$(", "VVFIN", "PPER", "VVINF", "$(", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Unter dem einzig wunderbaren gr&#252;n                 ", "tokens": ["Un\u00b7ter", "dem", "ein\u00b7zig", "wun\u00b7der\u00b7ba\u00b7ren", "gr", "&#252;", "n"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$(", "XY"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die einzigen sch\u00f6nen rosen.               ", "tokens": ["Die", "ein\u00b7zi\u00b7gen", "sch\u00f6\u00b7nen", "ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}