{"textgrid.poem.46213": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von herzog Bernharden", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, spanisch bist du neid, und torecht du misgunst,", "tokens": ["Ja", ",", "spa\u00b7nisch", "bist", "du", "neid", ",", "und", "to\u00b7recht", "du", "mis\u00b7gunst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr k\u00f6nt so wenig schmach an herzog Bernhard finden,", "tokens": ["ihr", "k\u00f6nt", "so", "we\u00b7nig", "schmach", "an", "her\u00b7zog", "Bern\u00b7hard", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "VVFIN", "APPR", "VVFIN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als wenig als der ris mit aller macht und kunst", "tokens": ["als", "we\u00b7nig", "als", "der", "ris", "mit", "al\u00b7ler", "macht", "und", "kunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "KOKOM", "ART", "NE", "APPR", "PIAT", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nach seinem falschen wohn den weltkreis \u00fcberwinden.", "tokens": ["nach", "sei\u00b7nem", "fal\u00b7schen", "wohn", "den", "welt\u00b7kreis", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dan er gedenket, thut und redet nichts umsunst,", "tokens": ["Dan", "er", "ge\u00b7den\u00b7ket", ",", "thut", "und", "re\u00b7det", "nichts", "um\u00b7sunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "sein geist kan des feinds list und f\u00fcrsatz bald ergr\u00fcnden,", "tokens": ["sein", "geist", "kan", "des", "feinds", "list", "und", "f\u00fcr\u00b7satz", "bald", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "ADJA", "NN", "KON", "VVIMP", "ADV", "VVINF", "$,"], "meter": "-+---+-+---+-", "measure": "dactylic.init"}, "line.7": {"text": "so l\u00f6schen seine wort der schn\u00f6den aufruhr brunst,", "tokens": ["so", "l\u00f6\u00b7schen", "sei\u00b7ne", "wort", "der", "schn\u00f6\u00b7den", "auf\u00b7ruhr", "brunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und seiner wafen blitz den adler selbs verblinden.", "tokens": ["und", "sei\u00b7ner", "wa\u00b7fen", "blitz", "den", "ad\u00b7ler", "selbs", "ver\u00b7blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kont er weis, k\u00fchn und stark des feinds list, macht, betrug", "tokens": ["Kont", "er", "weis", ",", "k\u00fchn", "und", "stark", "des", "feinds", "list", ",", "macht", ",", "be\u00b7trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "entdeckend, ihn zu ruck nicht schlagen und vertreiben,", "tokens": ["ent\u00b7de\u00b7ckend", ",", "ihn", "zu", "ruck", "nicht", "schla\u00b7gen", "und", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so h\u00e4tten wir l\u00e4ngst nichts, und doch der feind kaum gnug.", "tokens": ["so", "h\u00e4t\u00b7ten", "wir", "l\u00e4ngst", "nichts", ",", "und", "doch", "der", "feind", "kaum", "gnug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "$,", "KON", "ADV", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jedoch ohn deren schand kan man sein lob nicht schreiben,", "tokens": ["Je\u00b7doch", "ohn", "de\u00b7ren", "schand", "kan", "man", "sein", "lob", "nicht", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDS", "VVFIN", "VMFIN", "PIS", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+---+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "die, stiefs\u00f6hn des Teutschlands, tr\u00e4g, forchtsam und unklug", "tokens": ["die", ",", "stief\u00b7s\u00f6hn", "des", "Teutschlands", ",", "tr\u00e4g", ",", "forcht\u00