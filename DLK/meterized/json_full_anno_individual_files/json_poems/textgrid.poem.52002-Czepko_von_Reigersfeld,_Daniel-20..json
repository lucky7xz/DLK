{"textgrid.poem.52002": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht z\u00fcrne, wo das Saltz dich in die Augen bei\u00dft,", "tokens": ["Nicht", "z\u00fcr\u00b7ne", ",", "wo", "das", "Saltz", "dich", "in", "die", "Au\u00b7gen", "bei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Leser, das du wirst umb diese Bl\u00e4tter lesen:", "tokens": ["O", "Le\u00b7ser", ",", "das", "du", "wirst", "umb", "die\u00b7se", "Bl\u00e4t\u00b7ter", "le\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VAFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann meine Musa dich als wie mit Fingern weist,", "tokens": ["Wann", "mei\u00b7ne", "Mu\u00b7sa", "dich", "als", "wie", "mit", "Fin\u00b7gern", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "KOUS", "KOKOM", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann treff ich auff mein Ziel, da ist sie recht genesen.", "tokens": ["Dann", "treff", "ich", "auff", "mein", "Ziel", ",", "da", "ist", "sie", "recht", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wo du sonsten siehst, da\u00df etwann das Papier", "tokens": ["Und", "wo", "du", "sons\u00b7ten", "siehst", ",", "da\u00df", "et\u00b7wann", "das", "Pa\u00b7pier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die F\u00e4cher aufgedeckt, umb die die Buhler bitten:", "tokens": ["Die", "F\u00e4\u00b7cher", "auf\u00b7ge\u00b7deckt", ",", "umb", "die", "die", "Buh\u00b7ler", "bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUI", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So theile, wie du pflegst, die Laster nicht mit mir,", "tokens": ["So", "thei\u00b7le", ",", "wie", "du", "pflegst", ",", "die", "Las\u00b7ter", "nicht", "mit", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Reime, die sind mein, und deine sind die Sitten.", "tokens": ["Die", "Rei\u00b7me", ",", "die", "sind", "mein", ",", "und", "dei\u00b7ne", "sind", "die", "Sit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "$,", "KON", "PPOSAT", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexan