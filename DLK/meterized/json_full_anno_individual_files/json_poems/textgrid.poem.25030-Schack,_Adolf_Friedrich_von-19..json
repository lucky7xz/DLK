{"textgrid.poem.25030": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilde Blumen dir zu pfl\u00fccken,", "tokens": ["Wil\u00b7de", "Blu\u00b7men", "dir", "zu", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Duftende von frischem Tau,", "tokens": ["Duf\u00b7ten\u00b7de", "von", "fri\u00b7schem", "Tau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber wilde Bergesr\u00fccken", "tokens": ["Ue\u00b7ber", "wil\u00b7de", "Ber\u00b7ge\u00b7sr\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Streif' ich seit dem Morgengrau.", "tokens": ["Streif'", "ich", "seit", "dem", "Mor\u00b7gen\u00b7grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tief im Waldesgrund auf feuchten", "tokens": ["Tief", "im", "Wal\u00b7des\u00b7grund", "auf", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mooren die Vergi\u00dfmeinnicht,", "tokens": ["Moo\u00b7ren", "die", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"