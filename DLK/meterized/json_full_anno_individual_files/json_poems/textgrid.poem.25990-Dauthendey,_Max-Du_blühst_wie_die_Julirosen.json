{"textgrid.poem.25990": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du bl\u00fchst wie die Julirosen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bl\u00fchst wie die Julirosen, mehr Rosen als Bl\u00e4tter am roten Strauch.", "tokens": ["Du", "bl\u00fchst", "wie", "die", "Ju\u00b7li\u00b7ro\u00b7sen", ",", "mehr", "Ro\u00b7sen", "als", "Bl\u00e4t\u00b7ter", "am", "ro\u00b7ten", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,", "PIAT", "NN", "KOUS", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ich knie bei dir, dein G\u00e4rtner im Beet, die Sonne verbrennt ihn,", "tokens": ["Ich", "knie", "bei", "dir", ",", "dein", "G\u00e4rt\u00b7ner", "im", "Beet", ",", "die", "Son\u00b7ne", "ver\u00b7brennt", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch pflegt er dich mit den verbrannten H\u00e4nden.", "tokens": ["Doch", "pflegt", "er", "dich", "mit", "den", "ver\u00b7brann\u00b7ten", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Bin nur auf die Erde gekommen, weil du auf Erden bist, sch\u00f6ne Frau,", "tokens": ["Bin", "nur", "auf", "die", "Er\u00b7de", "ge\u00b7kom\u00b7men", ",", "weil", "du", "auf", "Er\u00b7den", "bist", ",", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+-++-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Bin nur auf der Erde geblieben, weil du die Erde so sch\u00f6n machst.", "tokens": ["Bin", "nur", "auf", "der", "Er\u00b7de", "ge\u00b7blie\u00b7ben", ",", "weil", "du", "die", "Er\u00b7de", "so", "sch\u00f6n", "machst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+-+-+++", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Deine Augen sind mir Fl\u00fcgel, sie tragen mich tief und tiefer in dein Herz.", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "sind", "mir", "Fl\u00fc\u00b7gel", ",", "sie", "tra\u00b7gen", "mich", "tief", "und", "tie\u00b7fer", "in", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer go\u00df Wein in dein Haar, da\u00df es duftet und scheint?", "tokens": ["Wer", "go\u00df", "Wein", "in", "dein", "Haar", ",", "da\u00df", "es", "duf\u00b7tet", "und", "scheint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word",