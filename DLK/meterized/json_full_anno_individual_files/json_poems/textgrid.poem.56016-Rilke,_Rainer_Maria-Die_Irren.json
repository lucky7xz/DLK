{"textgrid.poem.56016": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Irren", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und sie schweigen, weil die Scheidew\u00e4nde", "tokens": ["Und", "sie", "schwei\u00b7gen", ",", "weil", "die", "Schei\u00b7de\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "weggenommen sind aus ihrem Sinn,", "tokens": ["weg\u00b7ge\u00b7nom\u00b7men", "sind", "aus", "ih\u00b7rem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und die Stunden, da man sie verst\u00e4nde,", "tokens": ["und", "die", "Stun\u00b7den", ",", "da", "man", "sie", "ver\u00b7st\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "heben an und gehen hin.", "tokens": ["he\u00b7ben", "an", "und", "ge\u00b7hen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "N\u00e4chtens oft, wenn sie ans Fenster treten:", "tokens": ["N\u00e4ch\u00b7tens", "oft", ",", "wenn", "sie", "ans", "Fens\u00b7ter", "tre\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "pl\u00f6tzlich ist es alles gut.", "tokens": ["pl\u00f6tz\u00b7lich", "ist", "es", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre H\u00e4nde liegen im Konkreten,", "tokens": ["Ih\u00b7re", "H\u00e4n\u00b7de", "lie\u00b7gen", "im", "Kon\u00b7kre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und das Herz ist hoch und k\u00f6nnte beten,", "tokens": ["und", "das", "Herz", "ist", "hoch", "und", "k\u00f6nn\u00b7te", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und die Augen schauen ausgeruht", "tokens": ["und", "die", "Au\u00b7gen", "schau\u00b7en", "aus\u00b7ge\u00b7ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "auf den unverhofften, oftentstellten", "tokens": ["auf", "den", "un\u00b7ver\u00b7hoff\u00b7ten", ",", "of\u00b7tent\u00b7stell\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Garten im beruhigten Geviert,", "tokens": ["Gar\u00b7ten", "im", "be\u00b7ru\u00b7hig\u00b7ten", "Ge\u00b7viert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "der im Widerschein der fremden Welten", "tokens": ["der", "im", "Wi\u00b7der\u00b7schein", "der", "frem\u00b7den", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "weiterw\u00e4chst und niemals sich verliert.", "tokens": ["wei\u00b7ter\u00b7w\u00e4chst", "und", "nie\u00b7mals", "sich", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und sie schweigen, weil die Scheidew\u00e4nde", "tokens": ["Und", "sie", "schwei\u00b7gen", ",", "weil", "die", "Schei\u00b7de\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "weggenommen sind aus ihrem Sinn,", "tokens": ["weg\u00b7ge\u00b7nom\u00b7men", "sind", "aus", "ih\u00b7rem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und die Stunden, da man sie verst\u00e4nde,", "tokens": ["und", "die", "Stun\u00b7den", ",", "da", "man", "sie", "ver\u00b7st\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "heben an und gehen hin.", "tokens": ["he\u00b7ben", "an", "und", "ge\u00b7hen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tet