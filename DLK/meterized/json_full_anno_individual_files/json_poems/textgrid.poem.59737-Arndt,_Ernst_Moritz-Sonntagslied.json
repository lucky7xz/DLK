{"textgrid.poem.59737": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagslied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist Sonntag und ist stille", "tokens": ["Es", "ist", "Sonn\u00b7tag", "und", "ist", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "VAFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von allem wilden Tun,", "tokens": ["Von", "al\u00b7lem", "wil\u00b7den", "Tun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist des H\u00f6chsten Wille,", "tokens": ["Es", "ist", "des", "H\u00f6chs\u00b7ten", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heut soll die Arbeit ruhn,", "tokens": ["Heut", "soll", "die", "Ar\u00b7beit", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus allem wirren Leben", "tokens": ["Aus", "al\u00b7lem", "wir\u00b7ren", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und aus M\u00fchseligkeit", "tokens": ["Und", "aus", "M\u00fch\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Soll heut der Mensch sich heben", "tokens": ["Soll", "heut", "der", "Mensch", "sich", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Gott, zur Ewigkeit.", "tokens": ["Zu", "Gott", ",", "zur", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O gr\u00f6\u00dfter Held der Gnaden,", "tokens": ["O", "gr\u00f6\u00df\u00b7ter", "Held", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O s\u00fc\u00dfer Jesus Christ,", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch den die Welt geladen", "tokens": ["Durch", "den", "die", "Welt", "ge\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Himmelsfreude ist,", "tokens": ["Zur", "Him\u00b7mels\u00b7freu\u00b7de", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hilf, hilf uns aus den Schmerzen", "tokens": ["Hilf", ",", "hilf", "uns", "aus", "den", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der armen Zeitlichkeit!", "tokens": ["Der", "ar\u00b7men", "Zeit\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hilf! Hebe du die Herzen", "tokens": ["Hilf", "!", "He\u00b7be", "du", "die", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Gott, zur Ewigkeit!", "tokens": ["Zu", "Gott", ",", "zur", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O hilf uns! Hilf verstehen,", "tokens": ["O", "hilf", "uns", "!", "Hilf", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du s\u00fc\u00dfer Jesus Christ,", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warum du aus den H\u00f6hen", "tokens": ["Wa\u00b7rum", "du", "aus", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Himmels kommen bist,", "tokens": ["Des", "Him\u00b7mels", "kom\u00b7men", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch deine Liebesminne,", "tokens": ["Durch", "dei\u00b7ne", "Lie\u00b7bes\u00b7min\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch dein Vers\u00f6hnungswort", "tokens": ["Durch", "dein", "Ver\u00b7s\u00f6h\u00b7nungs\u00b7wort"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schleu\u00df Geister auf und Sinne", "tokens": ["Schleu\u00df", "Geis\u00b7ter", "auf", "und", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Heut f\u00fcr den Heimatsort.", "tokens": ["Heut", "f\u00fcr", "den", "Hei\u00b7mat\u00b7sort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was fromme Seelen weisen,", "tokens": ["Was", "from\u00b7me", "See\u00b7len", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchweht von Geisteswehn,", "tokens": ["Durch\u00b7weht", "von", "Geis\u00b7tes\u00b7wehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wovor die gr\u00f6\u00dften Weisen", "tokens": ["Wo\u00b7vor", "die", "gr\u00f6\u00df\u00b7ten", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Anbetend stille stehn,", "tokens": ["An\u00b7be\u00b7tend", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das Heimatland der Sterne,", "tokens": ["Das", "Hei\u00b7mat\u00b7land", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Geister Lebenslauf,", "tokens": ["Der", "Geis\u00b7ter", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schleu\u00df diese sel'ge Ferne,", "tokens": ["Schleu\u00df", "die\u00b7se", "sel'\u00b7ge", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schleu\u00df, Heiland, sie uns auf.", "tokens": ["Schleu\u00df", ",", "Hei\u00b7land", ",", "sie", "uns", "auf", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was Menschenangesichter", "tokens": ["Was", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word"], "pos": ["PWS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht schauen noch verstehn,", "tokens": ["Nicht", "schau\u00b7en", "noch", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das k\u00f6nnen, Licht der Lichter,", "tokens": ["Das", "k\u00f6n\u00b7nen", ",", "Licht", "der", "Lich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allein durch dich wir sehn \u2013", "tokens": ["Al\u00b7lein", "durch", "dich", "wir", "sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O dahin lehr' uns schauen,", "tokens": ["O", "da\u00b7hin", "lehr'", "uns", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Vom Erdenstaub zum Licht,", "tokens": ["Vom", "Er\u00b7dens\u00b7taub", "zum", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gib Glauben, gib Vertrauen,", "tokens": ["Gib", "Glau\u00b7ben", ",", "gib", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVIMP", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gib Himmelszuversicht.", "tokens": ["Gib", "Him\u00b7mels\u00b7zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es steht uns ja gerichtet", "tokens": ["Es", "steht", "uns", "ja", "ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Aug' zum Himmelsglanz,", "tokens": ["Das", "Aug'", "zum", "Him\u00b7mels\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wird's von dir gelichtet,", "tokens": ["Und", "wird's", "von", "dir", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schaut's den Himmel ganz;", "tokens": ["So", "schaut's", "den", "Him\u00b7mel", "ganz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ist das Herz befreiet", "tokens": ["Und", "ist", "das", "Herz", "be\u00b7frei\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch dich von Erdengier,", "tokens": ["Durch", "dich", "von", "Er\u00b7den\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "So stehn wir recht geweihet", "tokens": ["So", "stehn", "wir", "recht", "ge\u00b7wei\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur Sonntagsfreud' vor dir.", "tokens": ["Zur", "Sonn\u00b7tags\u00b7freud'", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es ist Sonntag und ist stille", "tokens": ["Es", "ist", "Sonn\u00b7tag", "und", "ist", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "VAFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von allem wilden Tun,", "tokens": ["Von", "al\u00b7lem", "wil\u00b7den", "Tun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist des H\u00f6chsten Wille,", "tokens": ["Es", "ist", "des", "H\u00f6chs\u00b7ten", "Wil