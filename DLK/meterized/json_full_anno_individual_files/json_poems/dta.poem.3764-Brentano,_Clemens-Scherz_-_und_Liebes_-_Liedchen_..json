{"dta.poem.3764": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Scherz -  und Liebes - Liedchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.57", "en:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hilft mir ein rother Apfel,               ", "tokens": ["Was", "hilft", "mir", "ein", "ro\u00b7ther", "Ap\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn er innen faul ist;", "tokens": ["Wenn", "er", "in\u00b7nen", "faul", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Was hilft mich ein sch\u00f6ns Kindlein,", "tokens": ["Was", "hilft", "mich", "ein", "sch\u00f6ns", "Kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sein Herzlein falsch ist.", "tokens": ["Wenn", "sein", "Herz\u00b7lein", "falsch", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich ein sch\u00f6n M\u00e4gdlein seh,", "tokens": ["Wenn", "ich", "ein", "sch\u00f6n", "M\u00e4gd\u00b7lein", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein ich, es sey mein,", "tokens": ["Mein", "ich", ",", "es", "sey", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ich mirs dann holen will,", "tokens": ["Wenn", "ich", "mirs", "dann", "ho\u00b7len", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft michs nicht hinein.", "tokens": ["L\u00e4\u00dft", "michs", "nicht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn mein Kindchen auf dem Tannen-", "tokens": ["Und", "wenn", "mein", "Kind\u00b7chen", "auf", "dem", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "baum w\u00e4r,", "tokens": ["baum", "w\u00e4r", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich wolls hinauf klettern, wenns' noch so hoch w\u00e4r.", "tokens": ["Ich", "wolls", "hin\u00b7auf", "klet\u00b7tern", ",", "wenns'", "noch", "so", "hoch", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "KOUS", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}