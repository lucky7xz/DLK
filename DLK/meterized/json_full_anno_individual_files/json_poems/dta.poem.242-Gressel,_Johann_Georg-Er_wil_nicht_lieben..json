{"dta.poem.242": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er wil nicht lieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Freyheit allein/", "tokens": ["Die", "Frey\u00b7heit", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die soll es nur seyn/", "tokens": ["Die", "soll", "es", "nur", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VAINF", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit welcher ich stete Freundschafft geh ein;", "tokens": ["Mit", "wel\u00b7cher", "ich", "ste\u00b7te", "Freund\u00b7schafft", "geh", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn wer da im Lieben/", "tokens": ["Denn", "wer", "da", "im", "Lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPRART", "ADJA", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sich t\u00e4glich wil \u00fcben/", "tokens": ["Sich", "t\u00e4g\u00b7lich", "wil", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der leidet nur Pein.", "tokens": ["Der", "lei\u00b7det", "nur", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Man sucht sich zu retten", "tokens": ["Man", "sucht", "sich", "zu", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Aus Banden und Ketten/", "tokens": ["Aus", "Ban\u00b7den", "und", "Ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ein armer Gefangner sucht gerne die Th\u00fcr.", "tokens": ["Ein", "ar\u00b7mer", "Ge\u00b7fang\u00b7ner", "sucht", "ger\u00b7ne", "die", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Das Wild sucht die Felder", "tokens": ["Das", "Wild", "sucht", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und dickesten W\u00e4lder/", "tokens": ["Und", "di\u00b7ckes\u00b7ten", "W\u00e4l\u00b7der", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Ich suche mit ihnen", "tokens": ["Ich", "su\u00b7che", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Der Freyheit zu dienen", "tokens": ["Der", "Frey\u00b7heit", "zu", "die\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Mit h\u00f6chster Begier.", "tokens": ["Mit", "h\u00f6chs\u00b7ter", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Mein Mund soll die Sachen", "tokens": ["Mein", "Mund", "soll", "die", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Der Liebe verlachen/", "tokens": ["Der", "Lie\u00b7be", "ver\u00b7la\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Weg ", "tokens": ["Weg"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Dir folg\u2019 ich nicht mehr;", "tokens": ["Dir", "fol\u00b7g'", "ich", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So auch deiner Lehr", "tokens": ["So", "auch", "dei\u00b7ner", "Lehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gibt ferner mein Hertze niemahls Geh\u00f6r;", "tokens": ["Gibt", "fer\u00b7ner", "mein", "Hert\u00b7ze", "nie\u00b7mahls", "Ge\u00b7h\u00f6r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du kehrest das Schertzen", "tokens": ["Du", "keh\u00b7rest", "das", "Schert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In kl\u00e4glichem Schmertzen/", "tokens": ["In", "kl\u00e4g\u00b7li\u00b7chem", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und bringest Beschwehr.", "tokens": ["Und", "brin\u00b7gest", "Be\u00b7schwehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ein h\u00f6llisches Qu\u00e4hlen", "tokens": ["Ein", "h\u00f6l\u00b7li\u00b7sches", "Qu\u00e4h\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Empfinden die Seelen/", "tokens": ["Emp\u00b7fin\u00b7den", "die", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "So deinen verdammten Trieben nachgehn.", "tokens": ["So", "dei\u00b7nen", "ver\u00b7damm\u00b7ten", "Trie\u00b7ben", "nach\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dein pr\u00e4chtig Gerichte", "tokens": ["Dein", "pr\u00e4ch\u00b7tig", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Sind gifftige Fr\u00fcchte/", "tokens": ["Sind", "giff\u00b7ti\u00b7ge", "Fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Die alle betriegen/", "tokens": ["Die", "al\u00b7le", "be\u00b7trie\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Die meinen Vergn\u00fcgen", "tokens": ["Die", "mei\u00b7nen", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "An ihnen zu sehn.", "tokens": ["An", "ih\u00b7nen", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Ich mag sie nicht schmecken/", "tokens": ["Ich", "mag", "sie", "nicht", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "So kan ich vor Schrecken/", "tokens": ["So", "kan", "ich", "vor", "Schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "In Sicherheit stehn.", "tokens": ["In", "Si\u00b7cher\u00b7heit", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}