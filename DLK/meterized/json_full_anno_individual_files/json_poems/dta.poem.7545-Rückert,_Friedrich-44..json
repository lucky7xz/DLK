{"dta.poem.7545": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von sichrer Meisterschaft ist Scherz ein sichres Zeichen;", "tokens": ["Von", "sich\u00b7rer", "Meis\u00b7ter\u00b7schaft", "ist", "Scherz", "ein", "sich\u00b7res", "Zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sich die Katze l\u00e4\u00dft zum Scherz die Maus entweichen.", "tokens": ["Wie", "sich", "die", "Kat\u00b7ze", "l\u00e4\u00dft", "zum", "Scherz", "die", "Maus", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Scherz ist ein Versuch, Ungleichheit gleichzustellen;", "tokens": ["Der", "Scherz", "ist", "ein", "Ver\u00b7such", ",", "Un\u00b7gleich\u00b7heit", "gleich\u00b7zu\u00b7stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum scherzen ungestraft nur unter sich Gesellen.", "tokens": ["Drum", "scher\u00b7zen", "un\u00b7ge\u00b7straft", "nur", "un\u00b7ter", "sich", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "ADV", "APPR", "PRF", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit Kleinerm scherze nicht! er wird sich \u00fcberheben;", "tokens": ["Mit", "Klei\u00b7nerm", "scher\u00b7ze", "nicht", "!", "er", "wird", "sich", "\u00fc\u00b7ber\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht mit Gr\u00f6\u00dferem! er wird dir's nicht vergeben.", "tokens": ["Und", "nicht", "mit", "Gr\u00f6\u00b7\u00dfe\u00b7rem", "!", "er", "wird", "dir's", "nicht", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NE", "$.", "PPER", "VAFIN", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Scherz ist sicher, der den Ernst hat an der Hand,", "tokens": ["Der", "Scherz", "ist", "si\u00b7cher", ",", "der", "den", "Ernst", "hat", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Schutz zu nehmen ihn vor bl\u00f6dem Misverstand.", "tokens": ["In", "Schutz", "zu", "neh\u00b7men", "ihn", "vor", "bl\u00f6\u00b7dem", "Mis\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Scherz ist sicher, nie die Achtung zu verscherzen,", "tokens": ["Der", "Scherz", "ist", "si\u00b7cher", ",", "nie", "die", "Ach\u00b7tung", "zu", "ver\u00b7scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ein Bewu\u00dftseyn tr\u00e4gt von h\u00f6hrer W\u00fcrd' im Herzen.", "tokens": ["Der", "ein", "Be\u00b7wu\u00df\u00b7tseyn", "tr\u00e4gt", "von", "h\u00f6h\u00b7rer", "W\u00fcrd'", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sich wegzuwerfen mag ein Weilchen sich nicht sch\u00e4men,", "tokens": ["Sich", "weg\u00b7zu\u00b7wer\u00b7fen", "mag", "ein", "Weil\u00b7chen", "sich", "nicht", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VMFIN", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sicher ist, sich selbst gleich wieder anzunehmen.", "tokens": ["Wer", "si\u00b7cher", "ist", ",", "sich", "selbst", "gleich", "wie\u00b7der", "an\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PRF", "ADV", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wer mit den Schmerzen scherzt, der hat sich \u00fcberwunden", "tokens": ["Wer", "mit", "den", "Schmer\u00b7zen", "scherzt", ",", "der", "hat", "sich", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entweder, oder wird von ihnen nie gefunden.", "tokens": ["Ent\u00b7we\u00b7der", ",", "o\u00b7der", "wird", "von", "ih\u00b7nen", "nie", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Drum reimet Scherz auf Schmerz, und beides reimt auf Herz,", "tokens": ["Drum", "rei\u00b7met", "Scherz", "auf", "Schmerz", ",", "und", "bei\u00b7des", "reimt", "auf", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "APPR", "NN", "$,", "KON", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Dichterherzen stets verwandeln Schmerz in Scherz.", "tokens": ["Weil", "Dich\u00b7ter\u00b7her\u00b7zen", "stets", "ver\u00b7wan\u00b7deln", "Schmerz", "in", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}