{"dta.poem.5709": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat ER zugleich den Stoff gemacht,", "tokens": ["Hat", "Er", "zu\u00b7gleich", "den", "Stoff", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in die Ordnung wird gebracht.", "tokens": ["Der", "in", "die", "Ord\u00b7nung", "wird", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur ER ", "tokens": ["Nur", "Er"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Kan alles; In JHM seyn,", "tokens": ["Kan", "al\u00b7les", ";", "In", "JhM", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$.", "APPR", "NE", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jm Stande der Verg\u00e4nglichkeiten,", "tokens": ["Jm", "Stan\u00b7de", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vereint unendliche Vollkommenheiten:", "tokens": ["Ver\u00b7eint", "un\u00b7end\u00b7li\u00b7che", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die durch JHN selbst, ohn\u2019 Ende, sonder Zeit,", "tokens": ["Die", "durch", "JhN", "selbst", ",", "ohn'", "En\u00b7de", ",", "son\u00b7der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "$,", "KOUI", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gekr\u00f6net sind mit ew\u2019ger Seeligkeit.", "tokens": ["Ge\u00b7kr\u00f6\u00b7net", "sind", "mit", "ew'\u00b7ger", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}