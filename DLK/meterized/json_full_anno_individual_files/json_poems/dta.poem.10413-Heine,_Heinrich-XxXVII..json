{"dta.poem.10413": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann es nicht vergessen,", "tokens": ["Ich", "kann", "es", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geliebtes, holdes Weib,", "tokens": ["Ge\u00b7lieb\u00b7tes", ",", "hol\u00b7des", "Weib", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich dich einst besessen,", "tokens": ["Da\u00df", "ich", "dich", "einst", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Seele und den Leib.", "tokens": ["Die", "See\u00b7le", "und", "den", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Leib m\u00f6cht' ich noch haben,", "tokens": ["Den", "Leib", "m\u00f6cht'", "ich", "noch", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Leib so zart und jung;", "tokens": ["Den", "Leib", "so", "zart", "und", "jung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele k\u00f6nnt Ihr begraben,", "tokens": ["Die", "See\u00b7le", "k\u00f6nnt", "Ihr", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hab' selber Seele genung.", "tokens": ["Hab'", "sel\u00b7ber", "See\u00b7le", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich will meine Seele zerschneiden,", "tokens": ["Ich", "will", "mei\u00b7ne", "See\u00b7le", "zer\u00b7schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und hauchen die H\u00e4lfte dir ein,", "tokens": ["Und", "hau\u00b7chen", "die", "H\u00e4lf\u00b7te", "dir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und will dich umschlingen, wir m\u00fcssen", "tokens": ["Und", "will", "dich", "um\u00b7schlin\u00b7gen", ",", "wir", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "$,", "PPER", "VMFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ganz Leib und Seele seyn.", "tokens": ["Ganz", "Leib", "und", "See\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}