{"dta.poem.12766": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine andere hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Bruder! dessen freud auch mein vergn\u00fcgen ist;", "tokens": ["Mein", "Bru\u00b7der", "!", "des\u00b7sen", "freud", "auch", "mein", "ver\u00b7gn\u00fc\u00b7gen", "ist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PDS", "VVFIN", "ADV", "PPOSAT", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast dir so ein kind zur Liebsten auserkiest,", "tokens": ["Du", "hast", "dir", "so", "ein", "kind", "zur", "Liebs\u00b7ten", "aus\u00b7er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus deren k\u00fcssen ich dir nichts als viel gel\u00fccke", "tokens": ["Aus", "de\u00b7ren", "k\u00fcs\u00b7sen", "ich", "dir", "nichts", "als", "viel", "ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "PPER", "PIS", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach wunsch versprechen kan. Heyrathen ist die br\u00fccke,", "tokens": ["Nach", "wunsch", "ver\u00b7spre\u00b7chen", "kan", ".", "Hey\u00b7ra\u00b7then", "ist", "die", "br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "VMFIN", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Auf welcher man zum tod\u2019 und auch zum leben geht.", "tokens": ["Auf", "wel\u00b7cher", "man", "zum", "tod'", "und", "auch", "zum", "le\u00b7ben", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "APPRART", "NN", "KON", "ADV", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer eine dirne freyt, die weiter nichts versteht,", "tokens": ["Wer", "ei\u00b7ne", "dir\u00b7ne", "freyt", ",", "die", "wei\u00b7ter", "nichts", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "VVFIN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als was den putz betrifft, die keine suppe kochen,", "tokens": ["Als", "was", "den", "putz", "be\u00b7tr\u00b7ifft", ",", "die", "kei\u00b7ne", "sup\u00b7pe", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Kein ey recht sieden kan; da ist in wenig wochen", "tokens": ["Kein", "ey", "recht", "sie\u00b7den", "kan", ";", "da", "ist", "in", "we\u00b7nig", "wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "VMFIN", "$.", "ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die liebe matt und todt. Ist sie ein z\u00e4nckisch thier,", "tokens": ["Die", "lie\u00b7be", "matt", "und", "todt", ".", "Ist", "sie", "ein", "z\u00e4n\u00b7ckisch", "thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "KON", "ADJD", "$.", "VAFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das nichts vertragen kan, und das den krug voll bier,", "tokens": ["Das", "nichts", "ver\u00b7tra\u00b7gen", "kan", ",", "und", "das", "den", "krug", "voll", "bier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "VMFIN", "$,", "KON", "PDS", "ART", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn ihr der mann was sagt, ihm ins gesichte giesset:", "tokens": ["Wenn", "ihr", "der", "mann", "was", "sagt", ",", "ihm", "ins", "ge\u00b7sich\u00b7te", "gies\u00b7set", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PWS", "VVFIN", "$,", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist sie ein fauler balg, den ieder schritt verdriesset:", "tokens": ["Ist", "sie", "ein", "fau\u00b7ler", "balg", ",", "den", "ie\u00b7der", "schritt", "ver\u00b7dries\u00b7set", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hat ihre wohllust nicht an einer kost genug:", "tokens": ["Hat", "ih\u00b7re", "wohl\u00b7lust", "nicht", "an", "ei\u00b7ner", "kost", "ge\u00b7nug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist sie aufs gute dumm, und nur aufs b\u00f6se klug;", "tokens": ["Ist", "sie", "aufs", "gu\u00b7te", "dumm", ",", "und", "nur", "aufs", "b\u00f6\u00b7se", "klug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "ADJD", "$,", "KON", "ADV", "APPRART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So wird der ehestand zu einer rechten h\u00f6lle.", "tokens": ["So", "wird", "der", "e\u00b7hes\u00b7tand", "zu", "ei\u00b7ner", "rech\u00b7ten", "h\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dir aber wird er nun zu einer lebens-qvelle.", "tokens": ["Dir", "a\u00b7ber", "wird", "er", "nun", "zu", "ei\u00b7ner", "le\u00b7bens\u00b7qvel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die nett\u2019 Elisabeth, so dich von hertzen liebt,", "tokens": ["Die", "nett'", "E\u00b7lis\u00b7a\u00b7beth", ",", "so", "dich", "von", "hert\u00b7zen", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ADV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und der dein hertze sich mit lauter lust ergiebt,", "tokens": ["Und", "der", "dein", "hert\u00b7ze", "sich", "mit", "lau\u00b7ter", "lust", "er\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ist von der besten art. Man hat sie klug erzogen:", "tokens": ["Ist", "von", "der", "bes\u00b7ten", "art", ".", "Man", "hat", "sie", "klug", "er\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$.", "PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die tugend hat sie selbst mit ihrer milch gesogen:", "tokens": ["Die", "tu\u00b7gend", "hat", "sie", "selbst", "mit", "ih\u00b7rer", "milch", "ge\u00b7so\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Man sieht die h\u00e4u\u00dfligkeit in allen ihrem thun:", "tokens": ["Man", "sieht", "die", "h\u00e4u\u00df\u00b7lig\u00b7keit", "in", "al\u00b7len", "ih\u00b7rem", "thun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "PIAT", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und die bescheidenheit und demuth, welche nun", "tokens": ["Und", "die", "be\u00b7schei\u00b7den\u00b7heit", "und", "de\u00b7muth", ",", "wel\u00b7che", "nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "So seltsam werden will, l\u00e4st dich viel gutes hoffen:", "tokens": ["So", "selt\u00b7sam", "wer\u00b7den", "will", ",", "l\u00e4st", "dich", "viel", "gu\u00b7tes", "hof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "VMFIN", "$,", "VVFIN", "PPER", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dir steht bey ihr das schlo\u00df der reinen keuschheit offen;", "tokens": ["Dir", "steht", "bey", "ihr", "das", "schlo\u00df", "der", "rei\u00b7nen", "keuschheit", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PDS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Doch aber dir allein. Die muntre wachsamkeit:", "tokens": ["Doch", "a\u00b7ber", "dir", "al\u00b7lein", ".", "Die", "mun\u00b7tre", "wach\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die anmuth, welche dich so ungemein erfreut:", "tokens": ["Die", "an\u00b7muth", ",", "wel\u00b7che", "dich", "so", "un\u00b7ge\u00b7mein", "er\u00b7freut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und andre tugenden, so deine Liebste zieren,", "tokens": ["Und", "and\u00b7re", "tu\u00b7gen\u00b7den", ",", "so", "dei\u00b7ne", "Liebs\u00b7te", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sind blumen, welche dir im winter nicht erfrieren.", "tokens": ["Sind", "blu\u00b7men", ",", "wel\u00b7che", "dir", "im", "win\u00b7ter", "nicht", "er\u00b7frie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "PPER", "APPRART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sind blumen, deren krafft dir lauter leben giebt.", "tokens": ["Sind", "blu\u00b7men", ",", "de\u00b7ren", "krafft", "dir", "lau\u00b7ter", "le\u00b7ben", "giebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELAT", "NN", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der ist dann recht begl\u00fcckt, der so gel\u00fccklich liebt.", "tokens": ["Der", "ist", "dann", "recht", "be\u00b7gl\u00fcckt", ",", "der", "so", "ge\u00b7l\u00fcck\u00b7lich", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wiewol wer die vernunfft n\u00e4chst GOtt zu rathe ziehet,", "tokens": ["Wie\u00b7wol", "wer", "die", "ver\u00b7nunfft", "n\u00e4chst", "Gott", "zu", "ra\u00b7the", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ART", "NN", "VVFIN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und erst was l\u00f6bliches zu lernen sich bem\u00fchet,", "tokens": ["Und", "erst", "was", "l\u00f6b\u00b7li\u00b7ches", "zu", "ler\u00b7nen", "sich", "be\u00b7m\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "ADJA", "PTKZU", "VVINF", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Eh er ans weib gedenckt, dem ist der himmel gut,", "tokens": ["Eh", "er", "ans", "weib", "ge\u00b7denckt", ",", "dem", "ist", "der", "him\u00b7mel", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$,", "PDS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dem folgt der segen nach in allem, was er thut.", "tokens": ["Dem", "folgt", "der", "se\u00b7gen", "nach", "in", "al\u00b7lem", ",", "was", "er", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der mu\u00df gel\u00fccklich freyn, den mu\u00df ein kind bedienen,", "tokens": ["Der", "mu\u00df", "ge\u00b7l\u00fcck\u00b7lich", "freyn", ",", "den", "mu\u00df", "ein", "kind", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "VAINF", "$,", "ART", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dem solche tugenden und gaben, wie Reginen,", "tokens": ["Dem", "sol\u00b7che", "tu\u00b7gen\u00b7den", "und", "ga\u00b7ben", ",", "wie", "Re\u00b7gi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die dich itzund umfa\u00dft und k\u00fcsset, eigen sind.", "tokens": ["Die", "dich", "it\u00b7zund", "um\u00b7fa\u00dft", "und", "k\u00fcs\u00b7set", ",", "ei\u00b7gen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wer aber ohne GOtt sein gantzes thun beginnt,", "tokens": ["Wer", "a\u00b7ber", "oh\u00b7ne", "Gott", "sein", "gant\u00b7zes", "thun", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "PPOSAT", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und von der kindheit an der lust den z\u00fcgel schiessen,", "tokens": ["Und", "von", "der", "kind\u00b7heit", "an", "der", "lust", "den", "z\u00fc\u00b7gel", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Den schmalen tugend-steg zu wandeln sich verdriessen,", "tokens": ["Den", "schma\u00b7len", "tu\u00b7gen\u00b7dsteg", "zu", "wan\u00b7deln", "sich", "ver\u00b7dries\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und stets den eitlen wahn der laster f\u00fchren l\u00e4st,", "tokens": ["Und", "stets", "den", "eit\u00b7len", "wahn", "der", "las\u00b7ter", "f\u00fch\u00b7ren", "l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der wundre sich ja nicht, wenn ihm sein hochzeit-fest", "tokens": ["Der", "wund\u00b7re", "sich", "ja", "nicht", ",", "wenn", "ihm", "sein", "hoch\u00b7zeit\u00b7fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PRF", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die th\u00fcre zum verdru\u00df und nicht zur freude zeiget.", "tokens": ["Die", "th\u00fc\u00b7re", "zum", "ver\u00b7dru\u00df", "und", "nicht", "zur", "freu\u00b7de", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Was meine poesie zu deinem ruhm verschweiget,", "tokens": ["Was", "mei\u00b7ne", "po\u00b7e\u00b7sie", "zu", "dei\u00b7nem", "ruhm", "ver\u00b7schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Mein Bruder! dieses sagt die wohlgetroffne wahl,", "tokens": ["Mein", "Bru\u00b7der", "!", "die\u00b7ses", "sagt", "die", "wohl\u00b7ge\u00b7troff\u00b7ne", "wahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die dich itzt durch die hand des priesters in den saal", "tokens": ["Die", "dich", "itzt", "durch", "die", "hand", "des", "pries\u00b7ters", "in", "den", "saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Der wahren freude f\u00fchrt, die kein verdru\u00df verg\u00e4llen,", "tokens": ["Der", "wah\u00b7ren", "freu\u00b7de", "f\u00fchrt", ",", "die", "kein", "ver\u00b7dru\u00df", "ver\u00b7g\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Kein unfall st\u00f6ren wird. Denn ob auch schon die wellen", "tokens": ["Kein", "un\u00b7fall", "st\u00f6\u00b7ren", "wird", ".", "Denn", "ob", "auch", "schon", "die", "wel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVINF", "VAFIN", "$.", "KON", "KOUS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Das schiff der besten eh mit ihrer flut erf\u00fcll\u2019n,", "tokens": ["Das", "schiff", "der", "bes\u00b7ten", "eh", "mit", "ih\u00b7rer", "flut", "er\u00b7f\u00fcll'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "KOUS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "So wird die liebe doch bald alles schrecken still\u2019n,", "tokens": ["So", "wird", "die", "lie\u00b7be", "doch", "bald", "al\u00b7les", "schre\u00b7cken", "still'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "VVFIN", "ADV", "ADV", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Der dr\u00e4uenden gefahr aus aller krafft begegnen.", "tokens": ["Der", "dr\u00e4u\u00b7en\u00b7den", "ge\u00b7fahr", "aus", "al\u00b7ler", "krafft", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Gott aber eure m\u00fch und heisse w\u00fcnsche segnen,", "tokens": ["Gott", "a\u00b7ber", "eu\u00b7re", "m\u00fch", "und", "heis\u00b7se", "w\u00fcn\u00b7sche", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJD", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Dieweil sie allesammt aus einem hertzen gehn.", "tokens": ["Die\u00b7weil", "sie", "al\u00b7le\u00b7sammt", "aus", "ei\u00b7nem", "hert\u00b7zen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Kurtz, bey der eintracht mu\u00df der segen feste stehn.", "tokens": ["Kurtz", ",", "bey", "der", "ein\u00b7tracht", "mu\u00df", "der", "se\u00b7gen", "fes\u00b7te", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "VMFIN", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.55": {"text": "Begl\u00fcckter Br\u00e4utigam! wer seine m\u00fch, sein reisen,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Br\u00e4u\u00b7ti\u00b7gam", "!", "wer", "sei\u00b7ne", "m\u00fch", ",", "sein", "rei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PWS", "PPOSAT", "ADJD", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Wie du, belohnet sieht, sich mit vergn\u00fcgung speisen,", "tokens": ["Wie", "du", ",", "be\u00b7loh\u00b7net", "sieht", ",", "sich", "mit", "ver\u00b7gn\u00fc\u00b7gung", "spei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "VVFIN", "$,", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Und alles hoffen kan, was nur vern\u00fcnfftig ist,", "tokens": ["Und", "al\u00b7les", "hof\u00b7fen", "kan", ",", "was", "nur", "ver\u00b7n\u00fcnff\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VMFIN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Der hat auf dieser see den rechten port erkiest,", "tokens": ["Der", "hat", "auf", "die\u00b7ser", "see", "den", "rech\u00b7ten", "port", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Und alles, was ihm nur die freunde w\u00fcnschen k\u00f6nnen.", "tokens": ["Und", "al\u00b7les", ",", "was", "ihm", "nur", "die", "freun\u00b7de", "w\u00fcn\u00b7schen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "ADV", "ART", "ADJA", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Willst du mir aber doch noch einen wunsch verg\u00f6nnen;", "tokens": ["Willst", "du", "mir", "a\u00b7ber", "doch", "noch", "ei\u00b7nen", "wunsch", "ver\u00b7g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "So w\u00fcnsch ich: weil der stand, der euch nunmehr vergn\u00fcgt,", "tokens": ["So", "w\u00fcnsch", "ich", ":", "weil", "der", "stand", ",", "der", "euch", "nun\u00b7mehr", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KOUS", "ART", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "So lange daurt, als ihr beysammen lebt und liegt,", "tokens": ["So", "lan\u00b7ge", "daurt", ",", "als", "ihr", "bey\u00b7sam\u00b7men", "lebt", "und", "liegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVIZU", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Da\u00df Br\u00e4utigam und Braut so lange leben m\u00fcssen,", "tokens": ["Da\u00df", "Br\u00e4u\u00b7ti\u00b7gam", "und", "Braut", "so", "lan\u00b7ge", "le\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "ADV", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Bis sie ihr ebenbild in kindes-kindern k\u00fcssen!", "tokens": ["Bis", "sie", "ihr", "e\u00b7ben\u00b7bild", "in", "kin\u00b7des\u00b7kin\u00b7dern", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}