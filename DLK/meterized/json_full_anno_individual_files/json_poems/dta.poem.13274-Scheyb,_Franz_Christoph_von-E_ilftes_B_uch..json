{"dta.poem.13274": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ees hat das treue Volck sehr oft nur gar zu recht;", "tokens": ["\u201e", "es", "hat", "das", "treu\u00b7e", "Volck", "sehr", "oft", "nur", "gar", "zu", "recht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eich halte seinen Ruff nicht allzeit vor so schlecht.", "tokens": ["\u201e", "ich", "hal\u00b7te", "sei\u00b7nen", "Ruff", "nicht", "all\u00b7zeit", "vor", "so", "schlecht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eerachte nur den Spruch, der oftmahls wahr gewesen:", "tokens": ["\u201e", "e\u00b7rach\u00b7te", "nur", "den", "Spruch", ",", "der", "oft\u00b7mahls", "wahr", "ge\u00b7we\u00b7sen", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201e(Man darff nur dieses Kriegs Begebenheiten lesen)", "tokens": ["\u201e", "(", "Man", "darff", "nur", "die\u00b7ses", "Kriegs", "Be\u00b7ge\u00b7ben\u00b7hei\u00b7ten", "le\u00b7sen", ")"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PIS", "VMFIN", "ADV", "PDAT", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eda\u00df eines Volcks Geschrey die Stimme GOttes sey.", "tokens": ["\u201e", "da\u00df", "ei\u00b7nes", "Volcks", "Ge\u00b7schrey", "die", "Stim\u00b7me", "Got\u00b7tes", "sey", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "550\u201dDie Art, mit welcher GOtt uns mahnt, ist vielerley.", "tokens": ["\"", "Die", "Art", ",", "mit", "wel\u00b7cher", "Gott", "uns", "mahnt", ",", "ist", "vie\u00b7ler\u00b7ley", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "APPR", "PWAT", "NN", "PPER", "VVFIN", "$,", "VAFIN", "PIAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ebald l\u00e4\u00dft er Gl\u00fcck und Trost durch solche Stimmen hoffen;", "tokens": ["\u201e", "bald", "l\u00e4\u00dft", "er", "Gl\u00fcck", "und", "Trost", "durch", "sol\u00b7che", "Stim\u00b7men", "hof\u00b7fen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ebald wird dadurch das Leid, so gleich erfolgt, getroffen.", "tokens": ["\u201e", "bald", "wird", "da\u00b7durch", "das", "Leid", ",", "so", "gleich", "er\u00b7folgt", ",", "ge\u00b7trof\u00b7fen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PAV", "ART", "NN", "$,", "ADV", "ADV", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201egewi\u00df ists, da\u00df, dem es an Geist und Wiz gebricht,", "tokens": ["\u201e", "ge\u00b7wi\u00df", "ists", ",", "da\u00df", ",", "dem", "es", "an", "Geist", "und", "Wiz", "ge\u00b7bricht", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "KOUS", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eoft weiser von der Sach, als ein Gelehrter spricht.", "tokens": ["\u201e", "oft", "wei\u00b7ser", "von", "der", "Sach", ",", "als", "ein", "Ge\u00b7lehr\u00b7ter", "spricht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "555\u201dWarum soll also jezt die Stimme gar nichts gelten?", "tokens": ["\"", "Wa\u00b7rum", "soll", "al\u00b7so", "jezt", "die", "Stim\u00b7me", "gar", "nichts", "gel\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "ADV", "ADV", "ART", "NN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eich sehe nicht, wie du sie billig k\u00f6nnest schelten.", "tokens": ["\u201e", "ich", "se\u00b7he", "nicht", ",", "wie", "du", "sie", "bil\u00b7lig", "k\u00f6n\u00b7nest", "schel\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eich gebe zu, da\u00df sie oft in der Hoffnung irrt;", "tokens": ["\u201e", "ich", "ge\u00b7be", "zu", ",", "da\u00df", "sie", "oft", "in", "der", "Hoff\u00b7nung", "irrt", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.14": {"text": "\u201esich von dem rechten Weeg auf einen Abweeg f\u00fchrt.", "tokens": ["\u201e", "sich", "von", "dem", "rech\u00b7ten", "Weeg", "auf", "ei\u00b7nen", "Ab\u00b7weeg", "f\u00fchrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRF", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eallein was ist im Buch der Allmacht eingeschrieben?", "tokens": ["\u201e", "al\u00b7lein", "was", "ist", "im", "Buch", "der", "All\u00b7macht", "ein\u00b7ge\u00b7schrie\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWS", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "560\u201dWer wei\u00df derselben Schlu\u00df, wer kennet ihr Belieben?", "tokens": ["\"", "Wer", "wei\u00df", "der\u00b7sel\u00b7ben", "Schlu\u00df", ",", "wer", "ken\u00b7net", "ihr", "Be\u00b7lie\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PDAT", "NN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eund wie? ist es des Volcks Amt und Obligenheit,", "tokens": ["\u201e", "und", "wie", "?", "ist", "es", "des", "Volcks", "Amt", "und", "Ob\u00b7li\u00b7gen\u00b7heit", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "$.", "VAFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "--+--++-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "\u201eda\u00df es Vernunft und Wiz, und solche F\u00e4higkeit", "tokens": ["\u201e", "da\u00df", "es", "Ver\u00b7nunft", "und", "Wiz", ",", "und", "sol\u00b7che", "F\u00e4\u00b7hig\u00b7keit"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "NN", "KON", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201ewie du, die Weisheit selbst, zu seinem Antheil habe?", "tokens": ["\u201e", "wie", "du", ",", "die", "Weis\u00b7heit", "selbst", ",", "zu", "sei\u00b7nem", "An\u00b7theil", "ha\u00b7be", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "$,", "ART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201edie\u00df ist dein Eigenthum, und nicht des P\u00f6bels Gabe.", "tokens": ["\u201e", "die\u00df", "ist", "dein", "Ei\u00b7gen\u00b7thum", ",", "und", "nicht", "des", "P\u00f6\u00b7bels", "Ga\u00b7be", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}