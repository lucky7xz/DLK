{"textgrid.poem.63512": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Stimme der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur eine Wachtel schlug im Feld,", "tokens": ["Nur", "ei\u00b7ne", "Wach\u00b7tel", "schlug", "im", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich vor\u00fcberging,", "tokens": ["Da", "ich", "vor\u00b7\u00fc\u00b7ber\u00b7ging", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur eine leise Glocke rief,", "tokens": ["Nur", "ei\u00b7ne", "lei\u00b7se", "Glo\u00b7cke", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die hoch im Turme hing.", "tokens": ["Die", "hoch", "im", "Tur\u00b7me", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verhallt die wirre Menschenlust,", "tokens": ["Ver\u00b7hallt", "die", "wir\u00b7re", "Men\u00b7schen\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wunde Menschenschrei.", "tokens": ["Der", "wun\u00b7de", "Men\u00b7schen\u00b7schrei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So still