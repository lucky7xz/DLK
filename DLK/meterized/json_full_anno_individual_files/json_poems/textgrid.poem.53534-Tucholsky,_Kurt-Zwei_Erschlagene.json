{"textgrid.poem.53534": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Erschlagene", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Garde-Kavallerie-Sch\u00fctzen-Division zu Berlin in Liebe und Verehrung", "tokens": ["Der", "Gar\u00b7de\u00b7Ka\u00b7val\u00b7le\u00b7rie\u00b7Sch\u00fct\u00b7zen\u00b7Di\u00b7vi\u00b7si\u00b7on", "zu", "Ber\u00b7lin", "in", "Lie\u00b7be", "und", "Ver\u00b7eh\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+---+-+++-+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "M\u00e4rtyrer . . . ? Nein.", "tokens": ["M\u00e4r\u00b7ty\u00b7rer", ".", ".", ".", "?", "Nein", "."], "token_info": ["word", "punct", "punct", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "$.", "$.", "$.", "PTKANT", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Aber P\u00f6belsbeute.", "tokens": ["A\u00b7ber", "P\u00f6\u00b7bels\u00b7beu\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sie wagtens. Wie selten ist das heute.", "tokens": ["Sie", "wag\u00b7tens", ".", "Wie", "sel\u00b7ten", "ist", "das", "heu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "ADJD", "VAFIN", "PDS", "ADV", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie packten zu, und sie setzten sich ein:", "tokens": ["Sie", "pack\u00b7ten", "zu", ",", "und", "sie", "setz\u00b7ten", "sich", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "sie wollten nicht nur Theoretiker sein.", "tokens": ["sie", "woll\u00b7ten", "nicht", "nur", "The\u00b7o\u00b7re\u00b7ti\u00b7ker", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Er: ein Wirrkopf von mittleren Ma\u00dfen,", "tokens": ["Er", ":", "ein", "Wirr\u00b7kopf", "von", "mitt\u00b7le\u00b7ren", "Ma\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "er suchte das Menschenheil in den Stra\u00dfen.", "tokens": ["er", "such\u00b7te", "das", "Men\u00b7schen\u00b7heil", "in", "den", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Armer Kerl: es liegt nicht da.", "tokens": ["Ar\u00b7mer", "Kerl", ":", "es", "liegt", "nicht", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er tat das Seine, wie er es sah.", "tokens": ["Er", "tat", "das", "Sei\u00b7ne", ",", "wie", "er", "es", "sah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er wollte die Unterdr\u00fcckten heben,", "tokens": ["Er", "woll\u00b7te", "die", "Un\u00b7ter\u00b7dr\u00fcck\u00b7ten", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "er wollte f\u00fcr sie ein menschliches Leben.", "tokens": ["er", "woll\u00b7te", "f\u00fcr", "sie", "ein", "menschli\u00b7ches", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie haben den Idealisten betrogen,", "tokens": ["Sie", "ha\u00b7ben", "den", "I\u00b7dea\u00b7lis\u00b7ten", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "den Meergott verschlangen die eigenen Wogen.", "tokens": ["den", "Meer\u00b7gott", "ver\u00b7schlan\u00b7gen", "die", "ei\u00b7ge\u00b7nen", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Sie knackten die Kassen, der Aufruhr tollt \u2013", "tokens": ["Sie", "knack\u00b7ten", "die", "Kas\u00b7sen", ",", "der", "Auf\u00b7ruhr", "tollt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Armer Kerl, hast du das gewollt?", "tokens": ["Ar\u00b7mer", "Kerl", ",", "hast", "du", "das", "ge\u00b7wollt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PPER", "PDS", "VMPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Sie: der Mann von den zwei beiden.", "tokens": ["Sie", ":", "der", "Mann", "von", "den", "zwei", "bei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ART", "NN", "APPR", "ART", "CARD", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Leben voll Hatz und Gef\u00e4ngnisleiden.", "tokens": ["Ein", "Le\u00b7ben", "voll", "Hatz", "und", "Ge\u00b7f\u00e4ng\u00b7nis\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hohn und Spott und schwarz-wei\u00dfe Schikane", "tokens": ["Hohn", "und", "Spott", "und", "schwa\u00b7rz\u00b7wei\u00b7\u00dfe", "Schi\u00b7ka\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "und dennoch treu der Fahne, der Fahne!", "tokens": ["und", "den\u00b7noch", "treu", "der", "Fah\u00b7ne", ",", "der", "Fah\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und immer wieder: Haft und Gef\u00e4ngnis", "tokens": ["Und", "im\u00b7mer", "wie\u00b7der", ":", "Haft", "und", "Ge\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$.", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "und Spitzeljagd und Landratsbedr\u00e4ngnis.", "tokens": ["und", "Spit\u00b7zel\u00b7jagd", "und", "Land\u00b7rats\u00b7be\u00b7dr\u00e4ng\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und immer wieder: Gef\u00e4ngnis und Haft \u2013", "tokens": ["Und", "im\u00b7mer", "wie\u00b7der", ":", "Ge\u00b7f\u00e4ng\u00b7nis", "und", "Haft", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sie hatte die st\u00e4rkste Manneskraft.", "tokens": ["Sie", "hat\u00b7te", "die", "st\u00e4rks\u00b7te", "Man\u00b7nes\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Parze des Rinnsteins zerschnitt die F\u00e4den.", "tokens": ["Die", "Par\u00b7ze", "des", "Rinn\u00b7steins", "zer\u00b7schnitt", "die", "F\u00e4\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da liegen die beiden am Hotel Eden.", "tokens": ["Da", "lie\u00b7gen", "die", "bei\u00b7den", "am", "Ho\u00b7tel", "E\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "APPRART", "NN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Bestellte Arbeit? Die Bourgeoisie?", "tokens": ["Be\u00b7stell\u00b7te", "Ar\u00b7beit", "?", "Die", "Bour\u00b7ge\u00b7o\u00b7i\u00b7sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So tatkr\u00e4ftig war die gute doch nie . . .", "tokens": ["So", "tat\u00b7kr\u00e4f\u00b7tig", "war", "die", "gu\u00b7te", "doch", "nie", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "ADV", "ADV", "$.", "$.", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wehrlos wurden zwei Menschen erschlagen.", "tokens": ["Wehr\u00b7los", "wur\u00b7den", "zwei", "Men\u00b7schen", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "CARD", "NN", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und es kreischen Geier die Totenklagen:", "tokens": ["Und", "es", "krei\u00b7schen", "Gei\u00b7er", "die", "To\u00b7ten\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gott sei Dank! Vorbei ist die Not!", "tokens": ["Gott", "sei", "Dank", "!", "Vor\u00b7bei", "ist", "die", "Not", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbman schlug\u00ab, schreibt einer, \u00bbdie Galizierin tot.\u00ab", "tokens": ["\u00bb", "man", "schlug", "\u00ab", ",", "schreibt", "ei\u00b7ner", ",", "\u00bb", "die", "Ga\u00b7li\u00b7zie\u00b7rin", "tot", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "VVFIN", "$(", "$,", "VVFIN", "PIS", "$,", "$(", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wir atmen auf! Hurra Bourgeoisie!", "tokens": ["Wir", "at\u00b7men", "auf", "!", "Hur\u00b7ra", "Bour\u00b7ge\u00b7o\u00b7i\u00b7sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "NE", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Jetzt spiele dein Spielchen ohne die!", "tokens": ["Jetzt", "spie\u00b7le", "dein", "Spiel\u00b7chen", "oh\u00b7ne", "die", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Nicht ohne! Man kann die K\u00f6rper zerschneiden.", "tokens": ["Nicht", "oh\u00b7ne", "!", "Man", "kann", "die", "K\u00f6r\u00b7per", "zer\u00b7schnei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUI", "$.", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aber das eine bleibt von den beiden:", "tokens": ["A\u00b7ber", "das", "ei\u00b7ne", "bleibt", "von", "den", "bei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "VVFIN", "APPR", "ART", "PIAT", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Wie man sich selber die Treue h\u00e4lt,", "tokens": ["Wie", "man", "sich", "sel\u00b7ber", "die", "Treu\u00b7e", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "wie man gegen eine feindliche Welt", "tokens": ["wie", "man", "ge\u00b7gen", "ei\u00b7ne", "feind\u00b7li\u00b7che", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "mit reinem Schilde streiten kann,", "tokens": ["mit", "rei\u00b7nem", "Schil\u00b7de", "strei\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das vergi\u00dft den beiden kein ehrlicher Mann!", "tokens": ["das", "ver\u00b7gi\u00dft", "den", "bei\u00b7den", "kein", "ehr\u00b7li\u00b7cher", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "PIAT", "PIAT", "ADJD", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Wir sind, wei\u00df Gott, keine Spartakiden.", "tokens": ["Wir", "sind", ",", "wei\u00df", "Gott", ",", "kei\u00b7ne", "Spar\u00b7ta\u00b7ki\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ehre zwei K\u00e4mpfern!", "tokens": ["Eh\u00b7re", "zwei", "K\u00e4mp\u00b7fern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie ruhen in Frieden!", "tokens": ["Sie", "ru\u00b7hen", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Garde-Kavallerie-Sch\u00fctze