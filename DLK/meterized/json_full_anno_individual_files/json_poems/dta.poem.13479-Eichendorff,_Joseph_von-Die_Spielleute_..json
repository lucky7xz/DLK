{"dta.poem.13479": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Spielleute .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchmorgens durch die Kl\u00fcfte", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", "durch", "die", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wir blasen Victoria!", "tokens": ["Wir", "bla\u00b7sen", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eine Lerche f\u00e4hrt in die L\u00fcfte:", "tokens": ["Ei\u00b7ne", "Ler\u00b7che", "f\u00e4hrt", "in", "die", "L\u00fcf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "\u201edie Spielleut' sind schon da!\u201c", "tokens": ["\u201e", "die", "Spiel\u00b7leut'", "sind", "schon", "da", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da dehnt ein Thurm und reckt sich", "tokens": ["Da", "dehnt", "ein", "Thurm", "und", "reckt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verschlafen im Morgengrau,", "tokens": ["Ver\u00b7schla\u00b7fen", "im", "Mor\u00b7gen\u00b7grau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wie aus dem Traume streckt sich", "tokens": ["Wie", "aus", "dem", "Trau\u00b7me", "streckt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Strom durch die stille Au,", "tokens": ["Der", "Strom", "durch", "die", "stil\u00b7le", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und ihre Aeuglein balde", "tokens": ["Und", "ih\u00b7re", "A\u00b7e\u00b7u\u00b7glein", "bal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Thun auf die B\u00e4chlein all',", "tokens": ["Thun", "auf", "die", "B\u00e4ch\u00b7lein", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Im Wald, im gr\u00fcnen Walde", "tokens": ["Im", "Wald", ",", "im", "gr\u00fc\u00b7nen", "Wal\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das ist ein lust'ger Schall!", "tokens": ["Das", "ist", "ein", "lust'\u00b7ger", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist ein lust'ges Reisen,", "tokens": ["Das", "ist", "ein", "lust'\u00b7ges", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Eichbaum k\u00fchl und frisch", "tokens": ["Der", "Eich\u00b7baum", "k\u00fchl", "und", "frisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schatten, wo wir speisen,", "tokens": ["Mit", "Schat\u00b7ten", ",", "wo", "wir", "spei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Deckt uns den gr\u00fcnen Tisch.", "tokens": ["Deckt", "uns", "den", "gr\u00fc\u00b7nen", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum Fr\u00fchst\u00fcck musiziren", "tokens": ["Zum", "Fr\u00fch\u00b7st\u00fcck", "mu\u00b7si\u00b7zi\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die muntern V\u00f6gelein,", "tokens": ["Die", "mun\u00b7tern", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Wald, wenn sie pausiren,", "tokens": ["Der", "Wald", ",", "wenn", "sie", "pau\u00b7si\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Stimmt wunderbar mit ein,", "tokens": ["Stimmt", "wun\u00b7der\u00b7bar", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die Wipfel thut er neigen,", "tokens": ["Die", "Wip\u00b7fel", "thut", "er", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Als gesegnet' er uns das Mahl,", "tokens": ["Als", "ge\u00b7seg\u00b7net'", "er", "uns", "das", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Und zeigt uns zwischen den Zweigen", "tokens": ["Und", "zeigt", "uns", "zwi\u00b7schen", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Tief unten das weite Thal.", "tokens": ["Tief", "un\u00b7ten", "das", "wei\u00b7te", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Tief unten da ist ein Garten,", "tokens": ["Tief", "un\u00b7ten", "da", "ist", "ein", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da wohnt eine sch\u00f6ne Frau,", "tokens": ["Da", "wohnt", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir k\u00f6nnen nicht lange warten,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nicht", "lan\u00b7ge", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch's Gitterthor wir schau'n,", "tokens": ["Durch's", "Git\u00b7ter\u00b7thor", "wir", "schau'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo die wei\u00dfen Statuen stehen,", "tokens": ["Wo", "die", "wei\u00b7\u00dfen", "Sta\u00b7tu\u00b7en", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Da ist's so still und k\u00fchl,", "tokens": ["Da", "ist's", "so", "still", "und", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Wasserk\u00fcnste gehen,", "tokens": ["Die", "Was\u00b7ser\u00b7k\u00fcns\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Flieder duftet schw\u00fcl.", "tokens": ["Der", "Flie\u00b7der", "duf\u00b7tet", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wir ziehn vorbei und singen", "tokens": ["Wir", "ziehn", "vor\u00b7bei", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In der stillen Morgenzeit,", "tokens": ["In", "der", "stil\u00b7len", "Mor\u00b7gen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Sie h\u00f6rt's im Traume klingen,", "tokens": ["Sie", "h\u00f6rt's", "im", "Trau\u00b7me", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wir aber sind schon weit.", "tokens": ["Wir", "a\u00b7ber", "sind", "schon", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}