{"textgrid.poem.36310": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Kunst-Orakel", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Dichter die Moyens nicht motivieren,", "tokens": ["Wenn", "Dich\u00b7ter", "die", "Mo\u00b7yens", "nicht", "mo\u00b7ti\u00b7vie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Statt den f\u00fcnf Akten weise zu vertrauen,", "tokens": ["Statt", "den", "f\u00fcnf", "Ak\u00b7ten", "wei\u00b7se", "zu", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "CARD", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das St\u00fcck aus St\u00fccken wunderlich erbauen,", "tokens": ["Das", "St\u00fcck", "aus", "St\u00fc\u00b7cken", "wun\u00b7der\u00b7lich", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Racine aus den Augen ganz verlieren;", "tokens": ["Ra\u00b7ci\u00b7ne", "aus", "den", "Au\u00b7gen", "ganz", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denk' ich an Rousseau, der auf allen Vieren", "tokens": ["Denk'", "ich", "an", "Rous\u00b7se\u00b7au", ",", "der", "auf", "al\u00b7len", "Vie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", 