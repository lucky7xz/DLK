{"textgrid.poem.60672": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Doktor Schlimmer ging zu einem Kranken,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Doktor Schlimmer ging zu einem Kranken,", "tokens": ["Herr", "Dok\u00b7tor", "Schlim\u00b7mer", "ging", "zu", "ei\u00b7nem", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den auch Kollege Besser untersucht.", "tokens": ["Den", "auch", "Kol\u00b7le\u00b7ge", "Bes\u00b7ser", "un\u00b7ter\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "NN", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die beiden sind verschiedener Gedanken:", "tokens": ["Die", "bei\u00b7den", "sind", "ver\u00b7schie\u00b7de\u00b7ner", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herr Besser hofft, Herr Schlimmer aber bucht", "tokens": ["Herr", "Bes\u00b7ser", "hofft", ",", "Herr", "Schlim\u00b7mer", "a\u00b7ber", "bucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "$,", "NN", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Patient aufs Konto des Verlustes. \u2013", "tokens": ["Den", "Pa\u00b7ti\u00b7ent", "aufs", "Kon\u00b7to", "des", "Ver\u00b7lus\u00b7tes", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["