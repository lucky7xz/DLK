{"dta.poem.12933": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn fragte: Warum er so  \n bla\u00df w\u00e4re?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lisette! wilt du wissen,", "tokens": ["Li\u00b7set\u00b7te", "!", "wilt", "du", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warum mein wange denn so blasse farbe f\u00fchrt?", "tokens": ["Wa\u00b7rum", "mein", "wan\u00b7ge", "denn", "so", "blas\u00b7se", "far\u00b7be", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVFIN", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So darffst du nur die liebe fragen;", "tokens": ["So", "darffst", "du", "nur", "die", "lie\u00b7be", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn diese wird dir sagen:", "tokens": ["Denn", "die\u00b7se", "wird", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df das feuer, so den demant deiner hellen augen ziert,", "tokens": ["Da\u00df", "das", "feu\u00b7er", ",", "so", "den", "de\u00b7mant", "dei\u00b7ner", "hel\u00b7len", "au\u00b7gen", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Mein hertze schon durchaus verzehret,", "tokens": ["Mein", "hert\u00b7ze", "schon", "durc\u00b7haus", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Davon die asche mir itzt ins gesichte f\u00e4hret.", "tokens": ["Da\u00b7von", "die", "asc\u00b7he", "mir", "itzt", "ins", "ge\u00b7sich\u00b7te", "f\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "PPER", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}