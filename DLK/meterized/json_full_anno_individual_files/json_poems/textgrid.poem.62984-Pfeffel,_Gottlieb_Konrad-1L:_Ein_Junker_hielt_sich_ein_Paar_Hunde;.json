{"textgrid.poem.62984": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Junker hielt sich ein Paar Hunde;", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Junker hielt sich ein Paar Hunde;", "tokens": ["Ein", "Jun\u00b7ker", "hielt", "sich", "ein", "Paar", "Hun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es war ein Pudel und sein Sohn.", "tokens": ["Es", "war", "ein", "Pu\u00b7del", "und", "sein", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der junge, Namens Pantalon,", "tokens": ["Der", "jun\u00b7ge", ",", "Na\u00b7mens", "Pan\u00b7ta\u00b7lon", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertrieb dem Herrchen manche Stunde.", "tokens": ["Ver\u00b7trieb", "dem", "Herr\u00b7chen", "man\u00b7che", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er konnte tanzen, Wache stehn,", "tokens": ["Er", "konn\u00b7te", "tan\u00b7zen", ",", "Wa\u00b7che", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Schubkarrn ziehn, ins Wasser gehn,", "tokens": ["Den", "Schub\u00b7karrn", "ziehn", ",", "ins", "Was\u00b7ser", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und alles dieses aus dem Grunde.", "tokens": ["Und", "al\u00b7les", "die\u00b7ses", "aus", "dem", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDAT", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der schlaue Fritz, des J\u00e4gers Kind,", "tokens": ["Der", "schlau\u00b7e", "Fritz", ",", "des", "J\u00e4\u00b7gers", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "War Lehrer unsers Hunds gewesen,", "tokens": ["War", "Leh\u00b7rer", "un\u00b7sers", "Hunds", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und dieser lernte so geschwind,", "tokens": ["Und", "die\u00b7ser", "lern\u00b7te", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Als mancher Knabe kaum das Lesen.", "tokens": ["Als", "man\u00b7cher", "Kna\u00b7be", "kaum", "das", "Le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Einst fiel dem kleinen Junker ein,", "tokens": ["Einst", "fiel", "dem", "klei\u00b7nen", "Jun\u00b7ker", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es m\u00fc\u00dfte noch viel leichter seyn,", "tokens": ["Es", "m\u00fc\u00df\u00b7te", "noch", "viel", "leich\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den alten Hund gelehrt zu machen. \u2013", "tokens": ["Den", "al\u00b7ten", "Hund", "ge\u00b7lehrt", "zu", "ma\u00b7chen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Herr Schnurr war sonst ein gutes Vieh,", "tokens": ["Herr", "Schnurr", "war", "sonst", "ein", "gu\u00b7tes", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Doch seine Herrschaft zog ihn nie,", "tokens": ["Doch", "sei\u00b7ne", "Herr\u00b7schaft", "zog", "ihn", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Zu solchen hochstudierten Sachen;", "tokens": ["Zu", "sol\u00b7chen", "hoch\u00b7stu\u00b7dier\u00b7ten", "Sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Er konnte blos das Haus bewachen.", "tokens": ["Er", "konn\u00b7te", "blos", "das", "Haus", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der Knabe nimmt ihn vor die Hand", "tokens": ["Der", "Kna\u00b7be", "nimmt", "ihn", "vor", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und stellt ihn aufrecht an die Wand,", "tokens": ["Und", "stellt", "ihn", "auf\u00b7recht", "an", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Allein der Hund f\u00e4llt immer wieder", "tokens": ["Al\u00b7lein", "der", "Hund", "f\u00e4llt", "im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Auf seine Vorderf\u00fc\u00dfe nieder.", "tokens": ["Auf", "sei\u00b7ne", "Vor\u00b7der\u00b7f\u00fc\u00b7\u00dfe", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Man rufet den Professor Fritz,", "tokens": ["Man", "ru\u00b7fet", "den", "Pro\u00b7fes\u00b7sor", "Fritz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.24": {"text": "Auch der ersch\u00f6pfet seinen Witz;", "tokens": ["Auch", "der", "er\u00b7sch\u00f6p\u00b7fet", "sei\u00b7nen", "Witz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Umsonst, es will ihm nicht gelingen", "tokens": ["Um\u00b7sonst", ",", "es", "will", "ihm", "nicht", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Den alten Sch\u00fcler zu bezwingen.", "tokens": ["Den", "al\u00b7ten", "Sch\u00fc\u00b7ler", "zu", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Vielleicht, sprach Fritze, hilft der Stock:", "tokens": ["Viel\u00b7leicht", ",", "sprach", "Frit\u00b7ze", ",", "hilft", "der", "Stock", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Er holt den Stock, man pr\u00fcgelt Schnurren;", "tokens": ["Er", "holt", "den", "Stock", ",", "man", "pr\u00fc\u00b7gelt", "Schnur\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PIS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Noch bleibt er steifer als ein Bock,", "tokens": ["Noch", "bleibt", "er", "stei\u00b7fer", "als", "ein", "Bock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und endlich f\u00e4ngt er an zu murren.", "tokens": ["Und", "end\u00b7lich", "f\u00e4ngt", "er", "an", "zu", "mur\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Was wollt ihr, sprach der arme Tropf,", "tokens": ["Was", "wollt", "ihr", ",", "sprach", "der", "ar\u00b7me", "Tropf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ihr werdet meinen grauen Kopf", "tokens": ["Ihr", "wer\u00b7det", "mei\u00b7nen", "grau\u00b7en", "Kopf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Doch nimmermehr zum Doctor schlagen;", "tokens": ["Doch", "nim\u00b7mer\u00b7mehr", "zum", "Doc\u00b7tor", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Geht, werdet durch mein Beyspiel klug,", "tokens": ["Geht", ",", "wer\u00b7det", "durch", "mein", "Bey\u00b7spiel", "klug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ihr Kinder, lernet itzt genug,", "tokens": ["Ihr", "Kin\u00b7der", ",", "ler\u00b7net", "itzt", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Ihr lernt nichts mehr in alten Tagen.", "tokens": ["Ihr", "lernt", "nichts", "mehr", "in", "al\u00b7ten", "Ta\u00b7gen", "."], "token_info