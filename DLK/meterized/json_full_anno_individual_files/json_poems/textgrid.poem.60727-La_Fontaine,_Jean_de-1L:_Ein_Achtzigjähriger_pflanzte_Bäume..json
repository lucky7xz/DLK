{"textgrid.poem.60727": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Achtzigj\u00e4hriger pflanzte B\u00e4ume.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Achtzigj\u00e4hriger pflanzte B\u00e4ume.", "tokens": ["Ein", "Acht\u00b7zig\u00b7j\u00e4h\u00b7ri\u00b7ger", "pflanz\u00b7te", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drei J\u00fcnglinge der Nachbarschaft erkl\u00e4rten", "tokens": ["Drei", "J\u00fcng\u00b7lin\u00b7ge", "der", "Nach\u00b7bar\u00b7schaft", "er\u00b7kl\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ART", "NN", "VVINF"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Sein Tun f\u00fcr unverst\u00e4ndige Narrentr\u00e4ume", "tokens": ["Sein", "Tun", "f\u00fcr", "un\u00b7ver\u00b7st\u00e4n\u00b7di\u00b7ge", "Nar\u00b7ren\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und lachten mit bezeichnenden Geb\u00e4rden.", "tokens": ["Und", "lach\u00b7ten", "mit", "be\u00b7zeich\u00b7nen\u00b7den", "Ge\u00b7b\u00e4r\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie riefen hin zum greisen Arbeitsamen:", "tokens": ["Sie", "rie\u00b7fen", "hin", "zum", "grei\u00b7sen", "Ar\u00b7beit\u00b7sa\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbwas schafft Ihr da, in aller G\u00f6tter Namen?", "tokens": ["\u00bb", "was", "schafft", "Ihr", "da", ",", "in", "al\u00b7ler", "G\u00f6t\u00b7ter", "Na\u00b7men", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$,", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es ging noch an, zu bauen, doch \u2013 zu pflanzen?", "tokens": ["Es", "ging", "noch", "an", ",", "zu", "bau\u00b7en", ",", "doch", "\u2013", "zu", "pflan\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PTKZU", "VVINF", "$,", "ADV", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Euren Jahren? Wollt Ihr noch erleben,", "tokens": ["In", "Eu\u00b7ren", "Jah\u00b7ren", "?", "Wollt", "Ihr", "noch", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df Fr\u00fcchte Euch vom Baum zu F\u00fc\u00dfen tanzen?", "tokens": ["Da\u00df", "Fr\u00fcch\u00b7te", "Euch", "vom", "Baum", "zu", "F\u00fc\u00b7\u00dfen", "tan\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Gott m\u00fc\u00dfte Euch ein Patriarchenalter geben!", "tokens": ["Gott", "m\u00fc\u00df\u00b7te", "Euch", "ein", "Pat\u00b7ri\u00b7ar\u00b7chen\u00b7al\u00b7ter", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was pflegt Ihr Sorge um zuk\u00fcnftge Zeit,", "tokens": ["Was", "pflegt", "Ihr", "Sor\u00b7ge", "um", "zu\u00b7k\u00fcnft\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die nicht mehr Eure ist? Gedenkt in Ruh", "tokens": ["Die", "nicht", "mehr", "Eu\u00b7re", "ist", "?", "Ge\u00b7denkt", "in", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "PPOSAT", "VAFIN", "$.", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Irrt\u00fcmer Eurer Vergangenheit.", "tokens": ["Der", "Irr\u00b7t\u00fc\u00b7mer", "Eu\u00b7rer", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Der Zukunft Sorge kommt uns Jungen zu.\u00ab", "tokens": ["Der", "Zu\u00b7kunft", "Sor\u00b7ge", "kommt", "uns", "Jun\u00b7gen", "zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Greis erwiderte: \u00bbNicht mehr als mir.", "tokens": ["Der", "Greis", "er\u00b7wi\u00b7der\u00b7te", ":", "\u00bb", "Nicht", "mehr", "als", "mir", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PTKNEG", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Gleichm\u00e4\u00dfig spielt der dunklen Parze Hand", "tokens": ["Gleich\u00b7m\u00e4\u00b7\u00dfig", "spielt", "der", "dunk\u00b7len", "Par\u00b7ze", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mit eurem wie mit meinem Lebensband.", "tokens": ["Mit", "eu\u00b7rem", "wie", "mit", "mei\u00b7nem", "Le\u00b7bens\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wir alle weilen nur ein Weilchen hier,", "tokens": ["Wir", "al\u00b7le", "wei\u00b7len", "nur", "ein", "Weil\u00b7chen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und was wir tun, ob ich es tu, ob ihr,", "tokens": ["Und", "was", "wir", "tun", ",", "ob", "ich", "es", "tu", ",", "ob", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "$,", "KOUS", "PPER", "PPER", "NE", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ist alles nur von fl\u00fcchtigem Bestand.", "tokens": ["Ist", "al\u00b7les", "nur", "von", "fl\u00fcch\u00b7ti\u00b7gem", "Be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Gibt's einen Augenblick denn, der uns schw\u00f6rt,", "tokens": ["Gibt's", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "denn", ",", "der", "uns", "schw\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Da\u00df noch ein zweiter lebend uns geh\u00f6rt?", "tokens": ["Da\u00df", "noch", "ein", "zwei\u00b7ter", "le\u00b7bend", "uns", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Urenkel werden danken meinen H\u00e4nden,", "tokens": ["Ur\u00b7en\u00b7kel", "wer\u00b7den", "dan\u00b7ken", "mei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wenn diese B\u00e4umchen ihnen Labsal spenden.", "tokens": ["Wenn", "die\u00b7se", "B\u00e4um\u00b7chen", "ih\u00b7nen", "Lab\u00b7sal", "spen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Was klagt ihr, da\u00df ich andern zum Vergn\u00fcgen", "tokens": ["Was", "klagt", "ihr", ",", "da\u00df", "ich", "an\u00b7dern", "zum", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Mir M\u00fche mache! Seht, ich m\u00fc\u00dfte l\u00fcgen,", "tokens": ["Mir", "M\u00fc\u00b7he", "ma\u00b7che", "!", "Seht", ",", "ich", "m\u00fc\u00df\u00b7te", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$.", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Gest\u00e4nde ich nicht gern und freudig ein:", "tokens": ["Ge\u00b7st\u00e4n\u00b7de", "ich", "nicht", "gern", "und", "freu\u00b7dig", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Lohn ist mir jene Hoffnung schon allein,", "tokens": ["Lohn", "ist", "mir", "je\u00b7ne", "Hoff\u00b7nung", "schon", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PDAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Sie ist schon Frucht; ich koste sie. Wer wei\u00df,", "tokens": ["Sie", "ist", "schon", "Frucht", ";", "ich", "kos\u00b7te", "sie", ".", "Wer", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$.", "PPER", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ob ich das Morgenrot nicht manchen Tag", "tokens": ["Ob", "ich", "das", "Mor\u00b7gen\u00b7rot", "nicht", "man\u00b7chen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Noch \u00fcber euren Gr\u00e4bern z\u00e4hlen mag.\u00ab", "tokens": ["Noch", "\u00fc\u00b7ber", "eu\u00b7ren", "Gr\u00e4\u00b7bern", "z\u00e4h\u00b7len", "mag.", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wie wahr gesprochen hatte doch der Greis!", "tokens": ["Wie", "wahr", "ge\u00b7spro\u00b7chen", "hat\u00b7te", "doch", "der", "Greis", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Ertrunken ist der erste der drei Jungen,", "tokens": ["Er\u00b7trun\u00b7ken", "ist", "der", "ers\u00b7te", "der", "drei", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Der plante, nach Amerika zu wandern,", "tokens": ["Der", "plan\u00b7te", ",", "nach", "A\u00b7me\u00b7ri\u00b7ka", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "NE", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Bereits im Hafen. Und die beiden andern", "tokens": ["Be\u00b7reits", "im", "Ha\u00b7fen", ".", "Und", "die", "bei\u00b7den", "an\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$.", "KON", "ART", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Hat gleichfalls j\u00e4h der Tod umschlungen:", "tokens": ["Hat", "gleich\u00b7falls", "j\u00e4h", "der", "Tod", "um\u00b7schlun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Der zwei