{"dta.poem.2395": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wje? seh' ich nicht Eufrosillen/", "tokens": ["Wie", "?", "seh'", "ich", "nicht", "Euf\u00b7ro\u00b7sil\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PTKNEG", "NE", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Unsre sch\u00f6ne Sch\u00e4ferinn?\nDer an-\nder.Ja sie tantzet mit Myrtillen/", "tokens": ["Uns\u00b7re", "sch\u00f6\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rinn", "?", "Der", "an", "der", ".", "Ja", "sie", "tant\u00b7zet", "mit", "Myr\u00b7til\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ART", "TRUNC", "ART", "$.", "PTKANT", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Mit sehr treuverliebtem ", "tokens": ["Mit", "sehr", "treu\u00b7ver\u00b7lieb\u00b7tem"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kommt ihr H\u00fcrten alzumal/", "tokens": ["Kommt", "ihr", "H\u00fcr\u00b7ten", "al\u00b7zu\u00b7mal", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier auf diesen Saal.", "tokens": ["Hier", "auf", "die\u00b7sen", "Saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Eufrosilchen ist Myrtillen/", "tokens": ["Euf\u00b7ro\u00b7sil\u00b7chen", "ist", "Myr\u00b7til\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich merke/ beygetraut.", "tokens": ["Wie", "ich", "mer\u00b7ke", "/", "bey\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$(", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner hertzgeliebten Braut.", "tokens": ["Sei\u00b7ner", "hertz\u00b7ge\u00b7lieb\u00b7ten", "Braut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott geb\u2019 ihnen allezeit/", "tokens": ["Gott", "geb'", "ih\u00b7nen", "al\u00b7le\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ihr Hertz\u2019 erfreut.", "tokens": ["Was", "ihr", "Hertz'", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Kom\u0303t ihr Sch\u00e4frinn' und ihr H\u00fcrten/", "tokens": ["Kom\u0303t", "ihr", "Sch\u00e4f\u00b7rinn'", "und", "ihr", "H\u00fcr\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt und suchet Tymian\nDer 2.Kommt und sucht stetsgr\u00fcne Myrten/", "tokens": ["Eilt", "und", "su\u00b7chet", "Ty\u00b7mi\u00b7an", "Der", "2.", "Kommt", "und", "sucht", "stets\u00b7gr\u00fc\u00b7ne", "Myr\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "ordinal", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "NE", "ART", "ADJA", "NN", "KON", "VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie auch Wintermeyeran.", "tokens": ["Wie", "auch", "Win\u00b7ter\u00b7mey\u00b7e\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziehret doch dem \u00e4dlen Paar'", "tokens": ["Zieh\u00b7ret", "doch", "dem", "\u00e4d\u00b7len", "Paa\u00b7r'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr so sch\u00f6nes Haar.", "tokens": ["Ihr", "so", "sch\u00f6\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Reicher Himmel la\u00df dein Segen/", "tokens": ["Rei\u00b7cher", "Him\u00b7mel", "la\u00df", "dein", "Se\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey den Neuverliebten stehn.", "tokens": ["Bey", "den", "Neu\u00b7ver\u00b7lieb\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie sich hoch erfreuet sehn/", "tokens": ["Sie", "sich", "hoch", "er\u00b7freu\u00b7et", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Treibt von ihrem \u00e4dlen Haus'", "tokens": ["Treibt", "von", "ih\u00b7rem", "\u00e4d\u00b7len", "Haus'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles ", "tokens": ["Al\u00b7les"], "token_info": ["word"], "pos": ["PIS"], "meter": "+-", "measure": "trochaic.single"}}}}}