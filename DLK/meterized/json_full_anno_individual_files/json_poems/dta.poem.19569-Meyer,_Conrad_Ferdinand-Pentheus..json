{"dta.poem.19569": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Pentheus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Sie schreitet in bacchisch bev\u00f6lkertem Raum", "tokens": ["Sie", "schrei\u00b7tet", "in", "bac\u00b7chisch", "be\u00b7v\u00f6l\u00b7ker\u00b7tem", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit wehenden Haaren ein gl\u00fchender Traum,", "tokens": ["Mit", "we\u00b7hen\u00b7den", "Haa\u00b7ren", "ein", "gl\u00fc\u00b7hen\u00b7der", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Von Faunen umh\u00fcpft,", "tokens": ["Von", "Fau\u00b7nen", "um\u00b7h\u00fcpft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Um die H\u00fcfte den G\u00fcrtel der Natter gekn\u00fcpft.", "tokens": ["Um", "die", "H\u00fcf\u00b7te", "den", "G\u00fcr\u00b7tel", "der", "Nat\u00b7ter", "ge\u00b7kn\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Melodisch gewiegt und von Eppich umlaubt,", "tokens": ["Me\u00b7lo\u00b7disch", "ge\u00b7wiegt", "und", "von", "Ep\u00b7pich", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KON", "APPR", "NE", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein fl\u00fcsterndes r\u00fccklings geworfenes Haupt \u2014", "tokens": ["Ein", "fl\u00fcs\u00b7tern\u00b7des", "r\u00fcck\u00b7lings", "ge\u00b7wor\u00b7fe\u00b7nes", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u201eich opfre mich Dir.", "tokens": ["\u201e", "ich", "opf\u00b7re", "mich", "Dir", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Verzehre, Lyaeus, was menschlich in mir!\u201c", "tokens": ["Ver\u00b7zeh\u00b7re", ",", "Ly\u00b7ae\u00b7us", ",", "was", "menschlich", "in", "mir", "!", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NE", "$,", "PRELS", "ADJD", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u201eagave!\u201c ruft's und der bacchische Schwarm", "tokens": ["\u201e", "a\u00b7ga\u00b7ve", "!", "\u201c", "ruft's", "und", "der", "bac\u00b7chi\u00b7sche", "Schwarm"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "$.", "$(", "VVFIN", "KON", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Zerstiebt und der Vater ergreift sie am Arm.", "tokens": ["Zer\u00b7stiebt", "und", "der", "Va\u00b7ter", "er\u00b7greift", "sie", "am", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u201eweg, trunken Gesind!", "tokens": ["\u201e", "weg", ",", "trun\u00b7ken", "Ge\u00b7sind", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Erwach und err\u00f6the, verlorenes Kind!", "tokens": ["Er\u00b7wach", "und", "er\u00b7r\u00f6\u00b7the", ",", "ver\u00b7lo\u00b7re\u00b7nes", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Du dienst einem Gaukler!\u201c Im Schutz des Gewands", "tokens": ["Du", "dienst", "ei\u00b7nem", "Gauk\u00b7ler", "!", "\u201c", "Im", "Schutz", "des", "Ge\u00b7wands"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "$.", "$(", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verh\u00fcllt er den Busen, entrei\u00dft ihr den Kranz \u2014", "tokens": ["Ver\u00b7h\u00fcllt", "er", "den", "Bu\u00b7sen", ",", "ent\u00b7rei\u00dft", "ihr", "den", "Kranz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wild hebt sie den Stab.", "tokens": ["Wild", "hebt", "sie", "den", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sie schlug! Aufst\u00f6hnt der das Leben ihr gab.", "tokens": ["Sie", "schlug", "!", "Auf\u00b7st\u00f6hnt", "der", "das", "Le\u00b7ben", "ihr", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201eich glaube den Gott! Ich empfinde die Macht!", "tokens": ["\u201e", "ich", "glau\u00b7be", "den", "Gott", "!", "Ich", "emp\u00b7fin\u00b7de", "die", "Macht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich strafe den Frevler der G\u00f6tter verlacht!", "tokens": ["Ich", "stra\u00b7fe", "den", "Frev\u00b7ler", "der", "G\u00f6t\u00b7ter", "ver\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wer bist du, Gesicht?", "tokens": ["Wer", "bist", "du", ",", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich bin die Bacchantin! Ich kenne dich nicht!\u201c", "tokens": ["Ich", "bin", "die", "Bac\u00b7chan\u00b7tin", "!", "Ich", "ken\u00b7ne", "dich", "nicht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Er betrachtet sein Kind. Er erstaunt. Er erbla\u00dft.", "tokens": ["Er", "be\u00b7trach\u00b7tet", "sein", "Kind", ".", "Er", "er\u00b7staunt", ".", "Er", "er\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Er entspringt, von entsetzlichem Grauen erfa\u00dft.", "tokens": ["Er", "ent\u00b7springt", ",", "von", "ent\u00b7setz\u00b7li\u00b7chem", "Grau\u00b7en", "er\u00b7fa\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Er flieht im Gefild,", "tokens": ["Er", "flieht", "im", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein rennender L\u00e4ufer, ein hastendes Wild.", "tokens": ["Ein", "ren\u00b7nen\u00b7der", "L\u00e4u\u00b7fer", ",", "ein", "has\u00b7ten\u00b7des", "Wild", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "\u201eherbei, alle Schwestern! M\u00e4naden, herbei!\u201c", "tokens": ["\u201e", "her\u00b7bei", ",", "al\u00b7le", "Schwes\u00b7tern", "!", "M\u00e4\u00b7na\u00b7den", ",", "her\u00b7bei", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "PIAT", "NN", "$.", "NN", "$,", "PTKVZ", "$.", "$("], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Erhebt sie den Weidruf, das helle Geschrei:", "tokens": ["Er\u00b7hebt", "sie", "den", "Weid\u00b7ruf", ",", "das", "hel\u00b7le", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u201ezur Jagd! Zur Jagd!\u201c", "tokens": ["\u201e", "zur", "Jagd", "!", "Zur", "Jagd", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "$.", "APPRART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "\u2014 \u201eWir folgen dir, blonde, begeisterte Magd!\u201c", "tokens": ["\u201e", "Wir", "fol\u00b7gen", "dir", ",", "blon\u00b7de", ",", "be\u00b7geis\u00b7ter\u00b7te", "Magd", "!", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "PPER", "$,", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Sie jagen den K\u00f6nig, Agave vorauf,", "tokens": ["Sie", "ja\u00b7gen", "den", "K\u00f6\u00b7nig", ",", "A\u00b7ga\u00b7ve", "vor\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er springt in den Strom und erneuert den Lauf", "tokens": ["Er", "springt", "in", "den", "Strom", "und", "er\u00b7neu\u00b7ert", "den", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Am andern Gestad,", "tokens": ["Am", "an\u00b7dern", "Ge\u00b7stad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sie st\u00fcrzt sich mit jubelnden Spr\u00fcngen ins Bad.", "tokens": ["Sie", "st\u00fcrzt", "sich", "mit", "ju\u00b7beln\u00b7den", "Spr\u00fcn\u00b7gen", "ins", "Bad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "APPRART", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Aufspritzen die Wasser. Er wirbelt den Staub", "tokens": ["Auf\u00b7sprit\u00b7zen", "die", "Was\u00b7ser", ".", "Er", "wir\u00b7belt", "den", "Staub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit bebenden F\u00fc\u00dfen. Sie hetzen den Raub \u2014", "tokens": ["Mit", "be\u00b7ben\u00b7den", "F\u00fc\u00b7\u00dfen", ".", "Sie", "het\u00b7zen", "den", "Raub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Was d\u00e4mmert empor?", "tokens": ["Was", "d\u00e4m\u00b7mert", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein Felsengestein ohne Pfad, ohne Thor.", "tokens": ["Ein", "Fel\u00b7sen\u00b7ge\u00b7stein", "oh\u00b7ne", "Pfad", ",", "oh\u00b7ne", "Thor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Die Sonne versank und die Wolke verglimmt.", "tokens": ["Die", "Son\u00b7ne", "ver\u00b7sank", "und", "die", "Wol\u00b7ke", "ver\u00b7glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er eilt und er schwankt und er keucht und er klimmt \u2014", "tokens": ["Er", "eilt", "und", "er", "schwankt", "und", "er", "keucht", "und", "er", "klimmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Am Fu\u00dfe der Wand", "tokens": ["Am", "Fu\u00b7\u00dfe", "der", "Wand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Erreicht ihn die rasende mordende Hand.", "tokens": ["Er\u00b7reicht", "ihn", "die", "ra\u00b7sen\u00b7de", "mor\u00b7den\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Am Grate des Berges verf\u00e4rbt sich die Glut,", "tokens": ["Am", "Gra\u00b7te", "des", "Ber\u00b7ges", "ver\u00b7f\u00e4rbt", "sich", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Schatten des Berges verstr\u00f6mt sich das Blut,", "tokens": ["Im", "Schat\u00b7ten", "des", "Ber\u00b7ges", "ver\u00b7str\u00f6mt", "sich", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nacht schwebt heran", "tokens": ["Nacht", "schwebt", "he\u00b7ran"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "++-+", "measure": "iambic.di"}, "line.4": {"text": "Und erschrickt und verh\u00fcllt was Agave gethan.", "tokens": ["Und", "er\u00b7schrickt", "und", "ver\u00b7h\u00fcllt", "was", "A\u00b7ga\u00b7ve", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KON", "VVFIN", "PIS", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}