{"dta.poem.12702": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen wein-schencken auf den  \n tod seiner ehe-frauen.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm keller hat er wein, und wasser auf den wangen;", "tokens": ["Jm", "kel\u00b7ler", "hat", "er", "wein", ",", "und", "was\u00b7ser", "auf", "den", "wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PTKVZ", "$,", "KON", "PWS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und beydes r\u00fchrt zugleich von GOtt im himmel her.", "tokens": ["Und", "bey\u00b7des", "r\u00fchrt", "zu\u00b7gleich", "von", "Gott", "im", "him\u00b7mel", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja, ist des wassers itzt mehr als ein gantzes meer?", "tokens": ["Ja", ",", "ist", "des", "was\u00b7sers", "itzt", "mehr", "als", "ein", "gant\u00b7zes", "meer", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "ART", "NN", "ADV", "PIAT", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So will GOtt alles selbst in einem schlauche fangen.", "tokens": ["So", "will", "Gott", "al\u00b7les", "selbst", "in", "ei\u00b7nem", "schlau\u00b7che", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PIS", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie aber kan ihm denn das wasser n\u00fctzlich seyn?", "tokens": ["Wie", "a\u00b7ber", "kan", "ihm", "denn", "das", "was\u00b7ser", "n\u00fctz\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gott, ders alleine kan, macht aus dem wasser wein.", "tokens": ["Gott", ",", "ders", "al\u00b7lei\u00b7ne", "kan", ",", "macht", "aus", "dem", "was\u00b7ser", "wein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VMFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}