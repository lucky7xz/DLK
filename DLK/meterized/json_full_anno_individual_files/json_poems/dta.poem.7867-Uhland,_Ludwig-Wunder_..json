{"dta.poem.7867": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Wunder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Sie war ein Kind vor wenig Tagen,", "tokens": ["Sie", "war", "ein", "Kind", "vor", "we\u00b7nig", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist es nicht mehr, wahrlich nein!", "tokens": ["Sie", "ist", "es", "nicht", "mehr", ",", "wahr\u00b7lich", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald ist die Blume aufgeschlagen,", "tokens": ["Bald", "ist", "die", "Blu\u00b7me", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald h\u00fcllt sie halb sich wieder ein.", "tokens": ["Bald", "h\u00fcllt", "sie", "halb", "sich", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wen kann ich um das Wunder fragen?", "tokens": ["Wen", "kann", "ich", "um", "das", "Wun\u00b7der", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie? oder t\u00e4uscht mich holder Schein?", "tokens": ["Wie", "?", "o\u00b7der", "t\u00e4uscht", "mich", "hol\u00b7der", "Schein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie spricht so ganz mit Kindersinne,", "tokens": ["Sie", "spricht", "so", "ganz", "mit", "Kin\u00b7der\u00b7sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So fromm ist ihrer Augen Spiel;", "tokens": ["So", "fromm", "ist", "ih\u00b7rer", "Au\u00b7gen", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch gro\u00dfer Dinge werd\u2019 ich inne,", "tokens": ["Doch", "gro\u00b7\u00dfer", "Din\u00b7ge", "werd'", "ich", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schau\u2019 in Tiefen ohne Ziel.", "tokens": ["Ich", "schau'", "in", "Tie\u00b7fen", "oh\u00b7ne", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja! Wunder sind\u2019s der s\u00fc\u00dfen Minne,", "tokens": ["Ja", "!", "Wun\u00b7der", "sin\u00b7d's", "der", "s\u00fc\u00b7\u00dfen", "Min\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Minne hat der Wunder viel.", "tokens": ["Die", "Min\u00b7ne", "hat", "der", "Wun\u00b7der", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}