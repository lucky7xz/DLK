{"dta.poem.2991": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht eine Stimme nur in dir warnt dich vorm B\u00f6sen,", "tokens": ["Nicht", "ei\u00b7ne", "Stim\u00b7me", "nur", "in", "dir", "warnt", "dich", "vorm", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du, wie leise, h\u00f6rst trotz lautesten Get\u00f6sen;", "tokens": ["Die", "du", ",", "wie", "lei\u00b7se", ",", "h\u00f6rst", "trotz", "lau\u00b7tes\u00b7ten", "Ge\u00b7t\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "ADJD", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dieselbe Stimme mahnt dich auch zum Guten an,", "tokens": ["Die\u00b7sel\u00b7be", "Stim\u00b7me", "mahnt", "dich", "auch", "zum", "Gu\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Z\u00fcgel ist zugleich und Sporn auf deiner Bahn.", "tokens": ["Die", "Z\u00fc\u00b7gel", "ist", "zu\u00b7gleich", "und", "Sporn", "auf", "dei\u00b7ner", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht das Gesetz nur spricht in dir, das du gebrochen;", "tokens": ["Nicht", "das", "Ge\u00b7setz", "nur", "spricht", "in", "dir", ",", "das", "du", "ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dasselbe hat in dem, der nie es brach, gesprochen.", "tokens": ["Das\u00b7sel\u00b7be", "hat", "in", "dem", ",", "der", "nie", "es", "brach", ",", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du f\u00fchlst, das dis Gesetz Gott selber in dir sei;", "tokens": ["Du", "f\u00fchlst", ",", "das", "dis", "Ge\u00b7setz", "Gott", "sel\u00b7ber", "in", "dir", "sei", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PDS", "NN", "NN", "ADV", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df du ihm gehorchst, das macht von ihm dich frei.", "tokens": ["Und", "da\u00df", "du", "ihm", "ge\u00b7horchst", ",", "das", "macht", "von", "ihm", "dich", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie ein gelehrig Ro\u00df nicht Z\u00fcgel f\u00fchlt noch Sporn;", "tokens": ["Wie", "ein", "ge\u00b7leh\u00b7rig", "Ro\u00df", "nicht", "Z\u00fc\u00b7gel", "f\u00fchlt", "noch", "Sporn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "PTKNEG", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das widerspenst'ge nur f\u00fchlt seines Meisters Zorn.", "tokens": ["Das", "wi\u00b7der\u00b7spenst'\u00b7ge", "nur", "f\u00fchlt", "sei\u00b7nes", "Meis\u00b7ters", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}