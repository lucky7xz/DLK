{"textgrid.poem.56058": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Begegnung in der Kastanien-Allee", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihm ward des Eingangs gr\u00fcne Dunkelheit", "tokens": ["Ihm", "ward", "des", "Ein\u00b7gangs", "gr\u00fc\u00b7ne", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "k\u00fchl wie ein Seidenmantel umgegeben", "tokens": ["k\u00fchl", "wie", "ein", "Sei\u00b7den\u00b7man\u00b7tel", "um\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVPP"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "den er noch nahm und ordnete: als eben", "tokens": ["den", "er", "noch", "nahm", "und", "ord\u00b7ne\u00b7te", ":", "als", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "am andern transparenten Ende, weit,", "tokens": ["am", "an\u00b7dern", "trans\u00b7pa\u00b7ren\u00b7ten", "En\u00b7de", ",", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "aus gr\u00fcner Sonne, wie aus gr\u00fcnen Scheiben,", "tokens": ["aus", "gr\u00fc\u00b7ner", "Son\u00b7ne", ",", "wie", "aus", "gr\u00fc\u00b7nen", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wei\u00df eine einzelne Gestalt", "tokens": ["wei\u00df", "ei\u00b7ne", "ein\u00b7zel\u00b7ne", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "aufleuchtete, um lange fern zu bleiben", "tokens": ["auf\u00b7leuch\u00b7te\u00b7te", ",", "um", "lan\u00b7ge", "fern", "zu", "blei\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUI", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und schlie\u00dflich, von dem Lichterniedertreiben", "tokens": ["und", "schlie\u00df\u00b7lich", ",", "von", "dem", "Lich\u00b7ter\u00b7nie\u00b7der\u00b7trei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "bei jedem Schritte \u00fcberwallt,", "tokens": ["bei", "je\u00b7dem", "Schrit\u00b7te", "\u00fc\u00b7berw\u00b7allt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "ein helles Wechseln auf sich herzutragen,", "tokens": ["ein", "hel\u00b7les", "Wech\u00b7seln", "auf", "sich", "her\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das scheu im Blond nach hinten lief.", "tokens": ["das", "scheu", "im", "Blond", "nach", "hin\u00b7ten", "lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aber auf einmal war der Schatten tief,", "tokens": ["A\u00b7ber", "auf", "ein\u00b7mal", "war", "der", "Schat\u00b7ten", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "und nahe Augen lagen aufgeschlagen", "tokens": ["und", "na\u00b7he", "Au\u00b7gen", "la\u00b7gen", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "in einem neuen deutlichen Gesicht,", "tokens": ["in", "ei\u00b7nem", "neu\u00b7en", "deut\u00b7li\u00b7chen", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das wie in einem Bildnis verweilte", "tokens": ["das", "wie", "in", "ei\u00b7nem", "Bild\u00b7nis", "ver\u00b7weil\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "in dem Moment, da man sich wieder teilte:", "tokens": ["in", "dem", "Mo\u00b7ment", ",", "da", "man", "sich", "wie\u00b7der", "teil\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erst war es immer, und dann war es nicht.", "tokens": ["erst", "war", "es", "im\u00b7mer", ",", "und", "dann", "war", "es", "nicht", "."], "token_i