{"dta.poem.7675": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df etwas gr\u00fcndlich du verstehst, ist nicht genug;", "tokens": ["Da\u00df", "et\u00b7was", "gr\u00fcnd\u00b7lich", "du", "ver\u00b7stehst", ",", "ist", "nicht", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gel\u00e4ufig mu\u00df dirs seyn, dann \u00fcbest du's mit Fug.", "tokens": ["Ge\u00b7l\u00e4u\u00b7fig", "mu\u00df", "dirs", "seyn", ",", "dann", "\u00fc\u00b7best", "du's", "mit", "Fug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PIS", "VAINF", "$,", "ADV", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und ist es dir nur recht gel\u00e4ufig, brauchst du's gar", "tokens": ["Und", "ist", "es", "dir", "nur", "recht", "ge\u00b7l\u00e4u\u00b7fig", ",", "brauchst", "du's", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht zu verstehn; das nimmst du leicht beim Rechnen wahr.", "tokens": ["Nicht", "zu", "ver\u00b7stehn", ";", "das", "nimmst", "du", "leicht", "beim", "Rech\u00b7nen", "wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$.", "PDS", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der edlen Rechenkunst Vollkommenheit gedeiht", "tokens": ["Der", "ed\u00b7len", "Re\u00b7chen\u00b7kunst", "Voll\u00b7kom\u00b7men\u00b7heit", "ge\u00b7deiht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am allerbesten bei Gedankenlosigkeit.", "tokens": ["Am", "al\u00b7ler\u00b7bes\u00b7ten", "bei", "Ge\u00b7dan\u00b7ken\u00b7lo\u00b7sig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}