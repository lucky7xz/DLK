{"textgrid.poem.41031": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An die Seherin von Prevorst", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch liegst du, aber la\u00df mich dein Schicksal nicht beklagen,", "tokens": ["Noch", "liegst", "du", ",", "a\u00b7ber", "la\u00df", "mich", "dein", "Schick\u00b7sal", "nicht", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das Auge Gottes seh' ich durch deine N\u00e4chte tagen,", "tokens": ["Das", "Au\u00b7ge", "Got\u00b7tes", "seh'", "ich", "durch", "dei\u00b7ne", "N\u00e4ch\u00b7te", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn als die Welt da drau\u00dfen zerronnen dir in Nacht,", "tokens": ["Denn", "als", "die", "Welt", "da", "drau\u00b7\u00dfen", "zer\u00b7ron\u00b7nen", "dir", "in", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hat sich dir jene Helle im Innern angefacht;", "tokens": ["Hat", "sich", "dir", "je\u00b7ne", "Hel\u00b7le", "im", "In\u00b7nern", "an\u00b7ge\u00b7facht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPER", "PDAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Da ward dir offenbaret in lichtgewobnen Kreisen", "tokens": ["Da", "ward", "dir", "of\u00b7fen\u00b7ba\u00b7ret", "in", "licht\u00b7ge\u00b7wob\u00b7nen", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des Innern geist'ges Wesen, was Geist und Seele hei\u00dfen,", "tokens": ["Des", "In\u00b7nern", "geist'\u00b7ges", "We\u00b7sen", ",", "was", "Geist", "und", "See\u00b7le", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie sie sich trennen, suchen, vereinigen im Tod,", "tokens": ["Wie", "sie", "sich", "tren\u00b7nen", ",", "su\u00b7chen", ",", "ver\u00b7ei\u00b7ni\u00b7gen", "im", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Das Auge bricht, doch innen aufsteigt ein Morgenrot;", "tokens": ["Das", "Au\u00b7ge", "bricht", ",", "doch", "in\u00b7nen", "auf\u00b7steigt", "ein", "Mor\u00b7gen\u00b7rot", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie eine Gnadensonne dem innern Auge scheint,", "tokens": ["Wie", "ei\u00b7ne", "Gna\u00b7den\u00b7son\u00b7ne", "dem", "in\u00b7nern", "Au\u00b7ge", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hat sich das \u00e4u\u00dfre Auge in Sehnsucht tr\u00fcb geweint;", "tokens": ["Hat", "sich", "das", "\u00e4u\u00df\u00b7re", "Au\u00b7ge", "in", "Sehn\u00b7sucht", "tr\u00fcb", "ge\u00b7weint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die helle Friedenstaube ob Tr\u00e4nenfluten schwebt,", "tokens": ["Die", "hel\u00b7le", "Frie\u00b7dens\u00b7tau\u00b7be", "ob", "Tr\u00e4\u00b7nen\u00b7flu\u00b7ten", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das B\u00f6se sinkt als Schwere, das Gute licht sich hebt.", "tokens": ["Das", "B\u00f6\u00b7se", "sinkt", "als", "Schwe\u00b7re", ",", "das", "Gu\u00b7te", "licht", "sich", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "$,", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "So konntest du ertragen der langen Krankheit Pein,", "tokens": ["So", "konn\u00b7test", "du", "er\u00b7tra\u00b7gen", "der", "lan\u00b7gen", "Krank\u00b7heit", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "ART", "ADJA", "