{"textgrid.poem.44309": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Zwerg", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lebt nicht fern von hier auf einem hohen ", "tokens": ["Es", "lebt", "nicht", "fern", "von", "hier", "auf", "ei\u00b7nem", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "APPR", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Mann, an Geiste gro\u00df, an K\u00f6rper nur ein ", "tokens": ["Ein", "Mann", ",", "an", "Geis\u00b7te", "gro\u00df", ",", "an", "K\u00f6r\u00b7per", "nur", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADV", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der kennet manches Kraut, manch zauberische ", "tokens": ["Der", "ken\u00b7net", "man\u00b7ches", "Kraut", ",", "manch", "zau\u00b7be\u00b7ri\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor dem flieht jedermann wie V\u00f6gel vor der ", "tokens": ["Vor", "dem", "flieht", "je\u00b7der\u00b7mann", "wie", "V\u00f6\u00b7gel", "vor", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVFIN", "PIS", "KOKOM", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie manch altes Weib in dieser Gegend ", "tokens": ["Und", "wie", "manch", "al\u00b7tes", "Weib", "in", "die\u00b7ser", "Ge\u00b7ge