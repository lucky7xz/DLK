{"textgrid.poem.64182": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Lady von Campion-Hall", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas klinget und singet vor meinem Schlo\u00df?", "tokens": ["\u00bb", "was", "klin\u00b7get", "und", "sin\u00b7get", "vor", "mei\u00b7nem", "Schlo\u00df", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was woget so bunt? \u2013 Ein Zigeunertro\u00df!", "tokens": ["Was", "wo\u00b7get", "so", "bunt", "?", "\u2013", "Ein", "Zi\u00b7geu\u00b7ner\u00b7tro\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$.", "$(", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mein Gemahl ist in London, der strenge Lord,", "tokens": ["Mein", "Ge\u00b7mahl", "ist", "in", "Lon\u00b7don", ",", "der", "stren\u00b7ge", "Lord", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mit Hunden wohl hetzte der sie fort!", "tokens": ["Mit", "Hun\u00b7den", "wohl", "hetz\u00b7te", "der", "sie", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "ART", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich aber, ich will \u2013 her ist's gar lang \u2013", "tokens": ["Ich", "a\u00b7ber", ",", "ich", "will", "\u2013", "her", "ist's", "gar", "lang", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VMFIN", "$(", "ADV", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mich einmal letzen an Tanz und Sang.\u00ab", "tokens": ["Mich", "ein\u00b7mal", "let\u00b7zen", "an", "Tanz", "und", "Sang", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVINF", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und herab die Terrasse die Lady schritt, \u2013", "tokens": ["Und", "her\u00b7ab", "die", "Ter\u00b7ras\u00b7se", "die", "La\u00b7dy", "schritt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "$("], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Ihr Fu\u00df war klein und leicht ihr Tritt:", "tokens": ["Ihr", "Fu\u00df", "war", "klein", "und", "leicht", "ihr", "Tritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie winkt mit dem F\u00e4cher, \u2013 der Reigen beginnt:", "tokens": ["Sie", "winkt", "mit", "dem", "F\u00e4\u00b7cher", ",", "\u2013", "der", "Rei\u00b7gen", "be\u00b7ginnt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie flattern die schwarzen Locken im Wind,", "tokens": ["Wie", "flat\u00b7tern", "die", "schwar\u00b7zen", "Lo\u00b7cken", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie schmettert die Zimbel, das Tamburin,", "tokens": ["Wie", "schmet\u00b7tert", "die", "Zim\u00b7bel", ",", "das", "Tam\u00b7bu\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wie brausen und sausen die Paare dahin!", "tokens": ["Wie", "brau\u00b7sen", "und", "sau\u00b7sen", "die", "Paa\u00b7re", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Da tritt mit der Laute der Fiedler zu ihr:", "tokens": ["Da", "tritt", "mit", "der", "Lau\u00b7te", "der", "Fied\u00b7ler", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "-+--+--+-++", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbo Lady, fair Lady, nun lausche du mir!", "tokens": ["\u00bb", "o", "La\u00b7dy", ",", "fair", "La\u00b7dy", ",", "nun", "lau\u00b7sche", "du", "mir", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Des Tanzes nur achtet dein Schlo\u00dfgesind \u2013 \u2013", "tokens": ["Des", "Tan\u00b7zes", "nur", "ach\u00b7tet", "dein", "Schlo\u00df\u00b7ge\u00b7sind", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$(", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gedenkst du noch Schottlands, Berthalind?\u00ab", "tokens": ["Ge\u00b7denkst", "du", "noch", "Schott\u00b7lands", ",", "Bert\u00b7ha\u00b7lind", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da wurde vor Schreck sie bla\u00df und rot:", "tokens": ["Da", "wur\u00b7de", "vor", "Schreck", "sie", "bla\u00df", "und", "rot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbdu, Edgar? Mein Edgar? O l\u00e4g' ich tot.\u00ab", "tokens": ["\u00bb", "du", ",", "Ed\u00b7gar", "?", "Mein", "Ed\u00b7gar", "?", "O", "l\u00e4g'", "ich", "tot", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "NE", "$.", "PPOSAT", "NN", "$.", "NE", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbtot lagst du drei Jahre beim eisigen Greis:", "tokens": ["\u00bb", "tot", "lagst", "du", "drei", "Jah\u00b7re", "beim", "ei\u00b7si\u00b7gen", "Greis", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "CARD", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nun sollst du erwachen zum Leben hei\u00df.", "tokens": ["Nun", "sollst", "du", "er\u00b7wa\u00b7chen", "zum", "Le\u00b7ben", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie haben gebrochen mein Ritterschlo\u00df,", "tokens": ["Sie", "ha\u00b7ben", "ge\u00b7bro\u00b7chen", "mein", "Rit\u00b7ter\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Doch blieb mir die Laute, das Schwert, das Ro\u00df:", "tokens": ["Doch", "blieb", "mir", "die", "Lau\u00b7te", ",", "das", "Schwert", ",", "das", "Ro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und mein ward die Heide, das Waldmoos weich", "tokens": ["Und", "mein", "ward", "die", "Hei\u00b7de", ",", "das", "Wald\u00b7moos", "weich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und aller Zigeuner K\u00f6nigreich!", "tokens": ["Und", "al\u00b7ler", "Zi\u00b7geu\u00b7ner", "K\u00f6\u00b7nig\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Dein Bett ist von Silber, dein Kamm von Gold,", "tokens": ["Dein", "Bett", "ist", "von", "Sil\u00b7ber", ",", "dein", "Kamm", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Demanten dir decken den Busen hold,", "tokens": ["De\u00b7man\u00b7ten", "dir", "de\u00b7cken", "den", "Bu\u00b7sen", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dich tr\u00e4gt die S\u00e4nfte von Pardelfell,", "tokens": ["Dich", "tr\u00e4gt", "die", "S\u00e4nf\u00b7te", "von", "Par\u00b7del\u00b7fell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Spiegel Kristall von Venedig hell: \u2013 \u2013", "tokens": ["Dein", "Spie\u00b7gel", "Kris\u00b7tall", "von", "Ve\u00b7ne\u00b7dig", "hell", ":", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPR", "NE", "ADJD", "$.", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Komm mit! Komm mit! la\u00df alles zur\u00fcck,", "tokens": ["Komm", "mit", "!", "Komm", "mit", "!", "la\u00df", "al\u00b7les", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Nur Lieb' ist Leben und Glanz und Gl\u00fcck.", "tokens": ["Nur", "Lieb'", "ist", "Le\u00b7ben", "und", "Glanz", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Dein Bett wird das duftige Heidekraut,", "tokens": ["Dein", "Bett", "wird", "das", "duf\u00b7ti\u00b7ge", "Hei\u00b7de\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Statt Demanten dir Perlen die Mainacht taut,", "tokens": ["Statt", "De\u00b7man\u00b7ten", "dir", "Per\u00b7len", "die", "Mai\u00b7nacht", "taut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Deine S\u00e4nfte mein Arm, dein Spiegel der See,", "tokens": ["Dei\u00b7ne", "S\u00e4nf\u00b7te", "mein", "Arm", ",", "dein", "Spie\u00b7gel", "der", "See", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dein Kamm meine Finger, dein Gespiel das Reh,", "tokens": ["Dein", "Kamm", "mei\u00b7ne", "Fin\u00b7ger", ",", "dein", "Ge\u00b7spiel", "das", "Reh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dein Kissen diese vieltreue Brust, \u2013", "tokens": ["Dein", "Kis\u00b7sen", "die\u00b7se", "viel\u00b7treu\u00b7e", "Brust", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dein Los \u2013 nie ausgesch\u00f6pfte Lust.\u00ab", "tokens": ["Dein", "Los", "\u2013", "nie", "aus\u00b7ge\u00b7sch\u00f6pf\u00b7te", "Lust", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und der Mond ging auf \u00fcber Campion-Schlo\u00df:", "tokens": ["Und", "der", "Mond", "ging", "auf", "\u00fc\u00b7ber", "Cam\u00b7pi\u00b7o\u00b7nSchlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "APPR", "NE", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da jagt in den Tann ein schwarzbraun Ro\u00df:", "tokens": ["Da", "jagt", "in", "den", "Tann", "ein", "schwarz\u00b7braun", "Ro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Hunde Gebell, der geketteten, hallt.", "tokens": ["Der", "Hun\u00b7de", "Ge\u00b7bell", ",", "der", "ge\u00b7ket\u00b7te\u00b7ten", ",", "hallt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein Mantel fliegt und ein Schleier wallt:", "tokens": ["Ein", "Man\u00b7tel", "fliegt", "und", "ein", "Schlei\u00b7er", "wallt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Jetzt sind sie verschwunden \u2013 sie ritten zu zweit: \u2013", "tokens": ["Jetzt", "sind", "sie", "ver\u00b7schwun\u00b7den", "\u2013", "sie", "rit\u00b7ten", "zu", "zweit", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$(", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wer will sie ersp\u00e4hen? \u2013 Die Welt ist weit!", "tokens": ["Wer", "will", "sie", "er\u00b7sp\u00e4\u00b7hen", "?", "\u2013", "Die", "Welt", "ist", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "\u00bbwas klinget und singet vor meinem Schlo\u00df?", "tokens": ["\u00bb", "was", "klin\u00b7get", "und", "sin\u00b7get", "vor", "mei\u00b7nem", "Schlo\u00df", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was woget so bunt? \u2013 Ein Zigeunertro\u00df!", "tokens": ["Was", "wo\u00b7get", "so", "bunt", "?", "\u2013", "Ein", "Zi\u00b7geu\u00b7ner\u00b7tro\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$.", "$(", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mein Gemahl ist in London, der strenge Lord,", "tokens": ["Mein", "Ge\u00b7mahl", "ist", "in", "Lon\u00b7don", ",", "der", "stren\u00b7ge", "Lord", ","], "token_info": ["word",