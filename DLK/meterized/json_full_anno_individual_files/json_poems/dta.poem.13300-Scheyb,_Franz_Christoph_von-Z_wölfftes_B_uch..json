{"dta.poem.13300": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eo Welt-gepriesnes Haupt! glorreiche K\u00f6niginn!", "tokens": ["\u201e", "o", "Welt\u00b7ge\u00b7pries\u00b7nes", "Haupt", "!", "glor\u00b7rei\u00b7che", "K\u00f6\u00b7ni\u00b7ginn", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "210\u201dIch sehe diesen Pomp schon in dem frohen Sinn!", "tokens": ["\"", "Ich", "se\u00b7he", "die\u00b7sen", "Pomp", "schon", "in", "dem", "fro\u00b7hen", "Sinn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PDAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eein \u00fcberirrdisches verhimmeltes Erscheinen", "tokens": ["\u201e", "ein", "\u00fc\u00b7be\u00b7rirr\u00b7di\u00b7sches", "ver\u00b7him\u00b7mel\u00b7tes", "Er\u00b7schei\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewird so der Feind als Freund an dir zu sehn vermeinen.", "tokens": ["\u201e", "wird", "so", "der", "Feind", "als", "Freund", "an", "dir", "zu", "sehn", "ver\u00b7mei\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "KOUS", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "\u201emir sagt das Herz: da\u00df auch selbst meine Majest\u00e4t", "tokens": ["\u201e", "mir", "sagt", "das", "Herz", ":", "da\u00df", "auch", "selbst", "mei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$.", "KOUS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ebey deiner Gegenwart, begl\u00fcckte Frau! vergeht.", "tokens": ["\u201e", "bey", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", ",", "be\u00b7gl\u00fcck\u00b7te", "Frau", "!", "ver\u00b7geht", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "215\u201dWir werden dich vielleicht zum dritten Mahl gekr\u00f6net,", "tokens": ["\"", "Wir", "wer\u00b7den", "dich", "viel\u00b7leicht", "zum", "drit\u00b7ten", "Mahl", "ge\u00b7kr\u00f6\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eund etwan mit dem Feind, zum Theil doch, ausges\u00f6hnet", "tokens": ["\u201e", "und", "et\u00b7wan", "mit", "dem", "Feind", ",", "zum", "Theil", "doch", ",", "aus\u00b7ge\u00b7s\u00f6h\u00b7net"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "KON", "ADV", "APPR", "ART", "NN", "$,", "APPRART", "NN", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ein dieser Ankunft sehn. Und du getreuer Flu\u00df", "tokens": ["\u201e", "in", "die\u00b7ser", "An\u00b7kunft", "sehn", ".", "Und", "du", "ge\u00b7treu\u00b7er", "Flu\u00df"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PDAT", "NN", "VVINF", "$.", "KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eder du Gewalt erfuhrst; daher zu ihrem Fu\u00df", "tokens": ["\u201e", "der", "du", "Ge\u00b7walt", "er\u00b7fuhrst", ";", "da\u00b7her", "zu", "ih\u00b7rem", "Fu\u00df"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PRELS", "PPER", "NN", "VVFIN", "$.", "PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eum Zuflucht dich begabst, so lang du noch wirst fliessen,", "tokens": ["\u201e", "um", "Zu\u00b7flucht", "dich", "be\u00b7gabst", ",", "so", "lang", "du", "noch", "wirst", "flies\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "220\u201dSo lang wird auch die Welt die Frau bewundern m\u00fcssen.", "tokens": ["\"", "So", "lang", "wird", "auch", "die", "Welt", "die", "Frau", "be\u00b7wun\u00b7dern", "m\u00fcs\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}