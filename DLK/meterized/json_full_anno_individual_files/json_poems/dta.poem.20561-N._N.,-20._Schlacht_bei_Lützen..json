{"dta.poem.20561": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "20.  \n  Schlacht bei L\u00fctzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dem neuen Heere, dem von Frankenland", "tokens": ["Mit", "dem", "neu\u00b7en", "Hee\u00b7re", ",", "dem", "von", "Fran\u00b7ken\u00b7land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nun erst Flinten nachgesandt,", "tokens": ["Nun", "erst", "Flin\u00b7ten", "nach\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trifft vor Erfurt\u2019s Thoren", "tokens": ["Trifft", "vor", "Er\u00b7furt's", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Er zusammen, der geboren", "tokens": ["Er", "zu\u00b7sam\u00b7men", ",", "der", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "PRELS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht zum Kaiser, aber fr\u00fch", "tokens": ["Nicht", "zum", "Kai\u00b7ser", ",", "a\u00b7ber", "fr\u00fch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ward gehoben durch Genie.", "tokens": ["Ward", "ge\u00b7ho\u00b7ben", "durch", "Ge\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er, ", "tokens": ["Er", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Blitzschnell, war dem Kriegsgeschick", "tokens": ["Blitz\u00b7schnell", ",", "war", "dem", "Kriegs\u00b7ge\u00b7schick"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Stets durch hellen Geist ein Leiter. \u2014", "tokens": ["Stets", "durch", "hel\u00b7len", "Geist", "ein", "Lei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dringend nun nach Leipzig weiter", "tokens": ["Drin\u00b7gend", "nun", "nach", "Leip\u00b7zig", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NE", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und erw\u00e4gend, da\u00df ihm Reiter", "tokens": ["Und", "er\u00b7w\u00e4\u00b7gend", ",", "da\u00df", "ihm", "Rei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "KOUS", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hier fast fehlen;", "tokens": ["Hier", "fast", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Kann er sich es nicht verhehlen,", "tokens": ["Kann", "er", "sich", "es", "nicht", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie er drum um so verwegner", "tokens": ["Wie", "er", "drum", "um", "so", "ver\u00b7weg\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PAV", "APPR", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Auf die Gegner", "tokens": ["Auf", "die", "Geg\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "St\u00fcrzen mu\u00df mit hei\u00dfer Kraft,", "tokens": ["St\u00fcr\u00b7zen", "mu\u00df", "mit", "hei\u00b7\u00dfer", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Die so oft ihm Sieg verschafft. \u2014", "tokens": ["Die", "so", "oft", "ihm", "Sieg", "ver\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Leipzig, z\u00fcrnt er, Mein sein mu\u00df!", "tokens": ["Leip\u00b7zig", ",", "z\u00fcrnt", "er", ",", "Mein", "sein", "mu\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "So schneid\u2019 ich vom Elbeflu\u00df", "tokens": ["So", "schneid'", "ich", "vom", "El\u00b7be\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Meine Widersacher ab;", "tokens": ["Mei\u00b7ne", "Wi\u00b7der\u00b7sa\u00b7cher", "ab", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Grabe so ihr bald\u2019ges Grab. \u2014", "tokens": ["Gra\u00b7be", "so", "ihr", "bald'\u00b7ges", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Preu\u00dfens, so wie Ru\u00dflands Fahnen", "tokens": ["Preu\u00b7\u00dfens", ",", "so", "wie", "Ru\u00df\u00b7lands", "Fah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "KOKOM", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Weh\u2019n beisammen, und es ahnen", "tokens": ["Weh'n", "bei\u00b7sam\u00b7men", ",", "und", "es", "ah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Jenen Vorsatz die Genossen,", "tokens": ["Je\u00b7nen", "Vor\u00b7satz", "die", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Die denn unter sich beschlossen,", "tokens": ["Die", "denn", "un\u00b7ter", "sich", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Sich zu werfen froh und frei", "tokens": ["Sich", "zu", "wer\u00b7fen", "froh", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Auf den Feind, am zweiten Mai.", "tokens": ["Auf", "den", "Feind", ",", "am", "zwei\u00b7ten", "Mai", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "(", "tokens": ["("], "token_info": ["punct"], "pos": ["$("]}, "line.29": {"text": "Russe, Preu\u00df\u2019 am n\u00e4chsten Tage.)", "tokens": ["Rus\u00b7se", ",", "Preu\u00df'", "am", "n\u00e4chs\u00b7ten", "Ta\u00b7ge", ".", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NE", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Mittags war\u2019s, am zweiten Mai,", "tokens": ["Mit\u00b7tags", "wa\u00b7r's", ",", "am", "zwei\u00b7ten", "Mai", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Als nach Goerschen, das F\u00fcrst ", "tokens": ["Als", "nach", "Goer\u00b7schen", ",", "das", "F\u00fcrst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "NE", "$,", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.32": {"text": "Stark besetzt hielt, Preu\u00dfen drangen", "tokens": ["Stark", "be\u00b7setzt", "hielt", ",", "Preu\u00b7\u00dfen", "dran\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "VVFIN", "$,", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Und den Feind zum Weichen zwangen.", "tokens": ["Und", "den", "Feind", "zum", "Wei\u00b7chen", "zwan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Nah beim Dorf auf leichten H\u00f6h\u2019n", "tokens": ["Nah", "beim", "Dorf", "auf", "leich\u00b7ten", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Sah man zwei Monarchen stehn.", "tokens": ["Sah", "man", "zwei", "Mon\u00b7ar\u00b7chen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Friedrich Wilhelm, Alexander", "tokens": ["Fried\u00b7rich", "Wil\u00b7helm", ",", "A\u00b7lex\u00b7an\u00b7der"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Sprachen traulich mit einander.", "tokens": ["Spra\u00b7chen", "trau\u00b7lich", "mit", "ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PRF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Das vereinte Heer gesammt", "tokens": ["Das", "ver\u00b7ein\u00b7te", "Heer", "ge\u00b7sammt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Blickt zu ihnen muthentflammt! \u2014", "tokens": ["Blickt", "zu", "ih\u00b7nen", "mu\u00b7thent\u00b7flammt", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Bei dem edlen Saft der Reben,", "tokens": ["Bei", "dem", "ed\u00b7len", "Saft", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Bei dem Fr\u00fchst\u00fcck sa\u00df so eben", "tokens": ["Bei", "dem", "Fr\u00fch\u00b7st\u00fcck", "sa\u00df", "so", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Souham, als Musketenblitz", "tokens": ["Sou\u00b7ham", ",", "als", "Mus\u00b7ke\u00b7ten\u00b7blitz"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "KOUS", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.43": {"text": "Sich ihm zeigt. Schnell auf vom Sitz", "tokens": ["Sich", "ihm", "zeigt", ".", "Schnell", "auf", "vom", "Sitz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PPER", "VVFIN", "$.", "ADJD", "APPR", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Springt der Marschall, f\u00fchrt hervor", "tokens": ["Springt", "der", "Mar\u00b7schall", ",", "f\u00fchrt", "her\u00b7vor"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "K\u00fchn sein untergeb\u2019nes Corps! \u2014", "tokens": ["K\u00fchn", "sein", "un\u00b7ter\u00b7ge\u00b7b'\u00b7nes", "Corps", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.46": {"text": "Doch vergeblich. Vater ", "tokens": ["Doch", "ver\u00b7geb\u00b7lich", ".", "Va\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADJD", "$.", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.47": {"text": "St\u00fcrmt Gro\u00df-G\u00f6rschen fest und sicher.", "tokens": ["St\u00fcrmt", "Gro\u00df\u00b7G\u00f6r\u00b7schen", "fest", "und", "si\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Waten w\u00fcrden durch den Styx", "tokens": ["Wa\u00b7ten", "w\u00fcr\u00b7den", "durch", "den", "Styx"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Roeder, Ziethen, Horn und Kl&#252;x. &#8212;", "tokens": ["Roe\u00b7der", ",", "Ziet\u00b7hen", ",", "Horn", "und", "Kl", "&#252;", "x.", "&#8212;"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "XML_entity", "abbreviation", "XML_entity"], "pos": ["NE", "$,", "NN", "$,", "NE", "KON", "NE", "$(", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Frisch und heiter", "tokens": ["Frisch", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.51": {"text": "F\u00fchrt die Reiter,", "tokens": ["F\u00fchrt", "die", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.52": {"text": "Mit dem K\u00fcra\u00df auf der Brust,", "tokens": ["Mit", "dem", "K\u00fc\u00b7ra\u00df", "auf", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Brandenburger, wie bewu\u00dft,", "tokens": ["Bran\u00b7den\u00b7bur\u00b7ger", ",", "wie", "be\u00b7wu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "K\u00f6nigs Bruder. \u2014 Pallaschklingen", "tokens": ["K\u00f6\u00b7nigs", "Bru\u00b7der", ".", "Pal\u00b7laschklin\u00b7gen"], "token_info": ["word", "word", "punct", "punct", "word"], "pos": ["NN", "NN", "$.", "$(", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.55": {"text": "Des Prinz ", "tokens": ["Des", "Prinz"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.56": {"text": "Und in Haufen", "tokens": ["Und", "in", "Hau\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.57": {"text": "Bei Starrsiedel Franken laufen! \u2014", "tokens": ["Bei", "Starr\u00b7sie\u00b7del", "Fran\u00b7ken", "lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.58": {"text": "Bonaparte", "tokens": ["Bo\u00b7na\u00b7par\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.59": {"text": "Blickt in Karte. \u2014", "tokens": ["Blickt", "in", "Kar\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.60": {"text": "Von der Stra\u00dfe abgekommen,", "tokens": ["Von", "der", "Stra\u00b7\u00dfe", "ab\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Die nach Leipzig er genommen,", "tokens": ["Die", "nach", "Leip\u00b7zig", "er", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Eilt er her mit Divisionen", "tokens": ["Eilt", "er", "her", "mit", "Di\u