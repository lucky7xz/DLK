{"dta.poem.9160": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Er tr\u00f6stet sich wegen seiner Mit-Buhler.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein hertze bist du noch betr\u00fcbt/", "tokens": ["Mein", "hert\u00b7ze", "bist", "du", "noch", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dieweil das m\u00e4dgen das dich liebt/", "tokens": ["Die\u00b7weil", "das", "m\u00e4d\u00b7gen", "das", "dich", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch andern sch\u00f6ne blicke gibt?", "tokens": ["Auch", "an\u00b7dern", "sch\u00f6\u00b7ne", "bli\u00b7cke", "gibt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach bilde dir durchau\u00df nicht ein/", "tokens": ["Ach", "bil\u00b7de", "dir", "durch\u00b7au\u00df", "nicht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als woltest du bey ihr allein", "tokens": ["Als", "wol\u00b7test", "du", "bey", "ihr", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der hahn allzeit im korbe seyn.", "tokens": ["Der", "hahn", "all\u00b7zeit", "im", "kor\u00b7be", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Bedenck es besser was du thust/", "tokens": ["Be\u00b7denck", "es", "bes\u00b7ser", "was", "du", "thust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wann du ja die s\u00fcsse lust", "tokens": ["Und", "wann", "du", "ja", "die", "s\u00fcs\u00b7se", "lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit andern leuten theilen must/", "tokens": ["Mit", "an\u00b7dern", "leu\u00b7ten", "thei\u00b7len", "must", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So habe doch genug daran/", "tokens": ["So", "ha\u00b7be", "doch", "ge\u00b7nug", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df kein affection-galan", "tokens": ["Da\u00df", "kein", "af\u00b7fec\u00b7ti\u00b7o\u00b7nga\u00b7lan"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir deinen spa\u00df verwehren kan.", "tokens": ["Dir", "dei\u00b7nen", "spa\u00df", "ver\u00b7weh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Du bist der beste bruder nicht/", "tokens": ["Du", "bist", "der", "bes\u00b7te", "bru\u00b7der", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast dein geiles angesicht", "tokens": ["Du", "hast", "dein", "gei\u00b7les", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar offt zu andern hingericht/", "tokens": ["Gar", "offt", "zu", "an\u00b7dern", "hin\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darum mein hertz/ was z\u00fcrnstu viel/", "tokens": ["Da\u00b7rum", "mein", "hertz", "/", "was", "z\u00fcrns\u00b7tu", "viel", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$(", "PWS", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann dich das m\u00e4dgen durch ihr spiel", "tokens": ["Wann", "dich", "das", "m\u00e4d\u00b7gen", "durch", "ihr", "spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit gleicher m\u00fcntze zahlen wil?", "tokens": ["Mit", "glei\u00b7cher", "m\u00fcnt\u00b7ze", "zah\u00b7len", "wil", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Sie nimmt sich warlich noch in acht", "tokens": ["Sie", "nimmt", "sich", "war\u00b7lich", "noch", "in", "acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann sie mit andern freundlich lacht/", "tokens": ["Wann", "sie", "mit", "an\u00b7dern", "freund\u00b7lich", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie es nicht zu lose macht/", "tokens": ["Da\u00df", "sie", "es", "nicht", "zu", "lo\u00b7se", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hergegen du bist so verpicht.", "tokens": ["Her\u00b7ge\u00b7gen", "du", "bist", "so", "ver\u00b7picht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Auffs liebe brod/ und achtest nicht/", "tokens": ["Auffs", "lie\u00b7be", "brod", "/", "und", "ach\u00b7test", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "KON", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob dich einander gleich verspricht.", "tokens": ["Ob", "dich", "ein\u00b7an\u00b7der", "gleich", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Kein mensch hat die beliebte krafft", "tokens": ["Kein", "mensch", "hat", "die", "be\u00b7lieb\u00b7te", "krafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mehr als theuren jungferschafft", "tokens": ["Der", "mehr", "als", "theu\u00b7ren", "jung\u00b7fer\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Au\u00df ihrer jungen scho\u00df gerafft/", "tokens": ["Au\u00df", "ih\u00b7rer", "jun\u00b7gen", "scho\u00df", "ge\u00b7rafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du aber frage deinen sinn", "tokens": ["Du", "a\u00b7ber", "fra\u00b7ge", "dei\u00b7nen", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo hast du l\u00e4ngsten den gewinn", "tokens": ["Wo", "hast", "du", "l\u00e4ngs\u00b7ten", "den", "ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der angebrannten keuschheit hin.", "tokens": ["Der", "an\u00b7ge\u00b7brann\u00b7ten", "keuschheit", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "6. Und endlich kriegst du loser dieb", "tokens": ["Und", "end\u00b7lich", "kriegst", "du", "lo\u00b7ser", "dieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich manchen guten lungen-hieb/", "tokens": ["Gleich", "man\u00b7chen", "gu\u00b7ten", "lun\u00b7gen\u00b7hieb", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat sie dich doch ein bi\u00dfgen lieb:", "tokens": ["Hat", "sie", "dich", "doch", "ein", "bi\u00df\u00b7gen", "lieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sie vertreibt die lange zeit", "tokens": ["Denn", "sie", "ver\u00b7treibt", "die", "lan\u00b7ge", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit v\u00f6lliger zufriedenheit", "tokens": ["Mit", "v\u00f6l\u00b7li\u00b7ger", "zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey dir und deiner lustigkeit.", "tokens": ["Bey", "dir", "und", "dei\u00b7ner", "lus\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Sie ist dir nicht alleine sch\u00f6n/", "tokens": ["Sie", "ist", "dir", "nicht", "al\u00b7lei\u00b7ne", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum la\u00df die andern immer gehn/", "tokens": ["Drum", "la\u00df", "die", "an\u00b7dern", "im\u00b7mer", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "ADV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kanst den possen doch verstehn.", "tokens": ["Du", "kanst", "den", "pos\u00b7sen", "doch", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es kost viel zeit eh es gelingt/", "tokens": ["Es", "kost", "viel", "zeit", "eh", "es", "ge\u00b7lingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df dich einander \u00fcberspringt/", "tokens": ["Da\u00df", "dich", "ein\u00b7an\u00b7der", "\u00fc\u00b7bers\u00b7pringt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich in deine stelle dringt.", "tokens": ["Und", "sich", "in", "dei\u00b7ne", "stel\u00b7le", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Darumb mein hertze sey getrost/", "tokens": ["Da\u00b7rumb", "mein", "hert\u00b7ze", "sey", "ge\u00b7trost", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann mancher kerle sich erboost/", "tokens": ["Wann", "man\u00b7cher", "ker\u00b7le", "sich", "er\u00b7boost", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit um deine liebe lost/", "tokens": ["Und", "mit", "um", "dei\u00b7ne", "lie\u00b7be", "lost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sie dir nur gewogen ist/", "tokens": ["Wo", "sie", "dir", "nur", "ge\u00b7wo\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So dencke/ da\u00df du vor der list", "tokens": ["So", "den\u00b7cke", "/", "da\u00df", "du", "vor", "der", "list"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "De\u00df henckers selber sicher bist.", "tokens": ["De\u00df", "hen\u00b7ckers", "sel\u00b7ber", "si\u00b7cher", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}