{"dta.poem.14713": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Unbekannte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Dorfe schon die Abendglocken klangen,", "tokens": ["Vom", "Dor\u00b7fe", "schon", "die", "A\u00b7bend\u00b7glo\u00b7cken", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die m\u00fcden V\u00f6glein gingen auch zur Ruh,", "tokens": ["Die", "m\u00fc\u00b7den", "V\u00f6\u00b7glein", "gin\u00b7gen", "auch", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur auf den Wiesen noch die Heimchen sangen", "tokens": ["Nur", "auf", "den", "Wie\u00b7sen", "noch", "die", "Heim\u00b7chen", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und von den Bergen rauscht der Wald dazu;", "tokens": ["Und", "von", "den", "Ber\u00b7gen", "rauscht", "der", "Wald", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da kam ein Wandrer durch die Aehrenwogen,", "tokens": ["Da", "kam", "ein", "Wand\u00b7rer", "durch", "die", "A\u00b7eh\u00b7ren\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Aus fernen Landen schien er hergezogen.", "tokens": ["Aus", "fer\u00b7nen", "Lan\u00b7den", "schien", "er", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}