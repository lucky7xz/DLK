{"dta.poem.21888": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die allerbeste Venus-Gabe", "tokens": ["Die", "al\u00b7ler\u00b7bes\u00b7te", "Ve\u00b7nus\u00b7Ga\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die ich ie zu verschenken habe", "tokens": ["die", "ich", "ie", "zu", "ver\u00b7schen\u00b7ken", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "versch\u00fcttet Fillis mit der Hand.", "tokens": ["ver\u00b7sch\u00fct\u00b7tet", "Fil\u00b7lis", "mit", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "viel besser w\u00e4r\u2019 es angewandt/", "tokens": ["viel", "bes\u00b7ser", "w\u00e4r'", "es", "an\u00b7ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wenn/ was sie di\u00dffalls mir genommen", "tokens": ["wenn", "/", "was", "sie", "di\u00df\u00b7falls", "mir", "ge\u00b7nom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "PWS", "PPER", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ihr selbst zu gute m\u00f6chte kommen.", "tokens": ["ihr", "selbst", "zu", "gu\u00b7te", "m\u00f6ch\u00b7te", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}