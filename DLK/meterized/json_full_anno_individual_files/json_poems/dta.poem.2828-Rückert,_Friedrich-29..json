{"dta.poem.2828": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den alten Malerspruch erkoren hab' auch ich", "tokens": ["Den", "al\u00b7ten", "Ma\u00b7ler\u00b7spruch", "er\u00b7ko\u00b7ren", "hab'", "auch", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Wahlspruch f\u00fcr mein Buch: Kein Tag ohn' einen Strich.", "tokens": ["Zum", "Wahl\u00b7spruch", "f\u00fcr", "mein", "Buch", ":", "Kein", "Tag", "ohn'", "ei\u00b7nen", "Strich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$.", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So lass' ich ohne Strich nun keinen Tag verstreichen,", "tokens": ["So", "lass'", "ich", "oh\u00b7ne", "Strich", "nun", "kei\u00b7nen", "Tag", "ver\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei mangmal es auch nur ein Strich um auszustreichen.", "tokens": ["Sei", "mang\u00b7mal", "es", "auch", "nur", "ein", "Strich", "um", "aus\u00b7zu\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "ADV", "ART", "NN", "APPR", "VVIZU", "$."], "meter": "-+-+-