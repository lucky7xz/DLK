{"dta.poem.2652": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Jahres-Schlus/  \n Hn. G. H. den 29. Septembr. 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So hat dich/ Seeliger/ der H\u00f6chste nun quittiret/", "tokens": ["So", "hat", "dich", "/", "See\u00b7li\u00b7ger", "/", "der", "H\u00f6chs\u00b7te", "nun", "quit\u00b7ti\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "NN", "$(", "ART", "ADJA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dein R\u00e4itungs-Schlu\u00df ist recht/ du wirst mit Ruhm", "tokens": ["Dein", "R\u00e4i\u00b7tungs\u00b7Schlu\u00df", "ist", "recht", "/", "du", "wirst", "mit", "Ruhm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer seiner Jahre Lauff so/ als wie du/ gef\u00fchret/", "tokens": ["Wer", "sei\u00b7ner", "Jah\u00b7re", "Lauff", "so", "/", "als", "wie", "du", "/", "ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "ADV", "$(", "KOUS", "PWAV", "PPER", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der kan mit Fried und Ruh\u2019 zu seinen V\u00e4tern gehn.", "tokens": ["Der", "kan", "mit", "Fried", "und", "Ruh'", "zu", "sei\u00b7nen", "V\u00e4\u00b7tern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du \u00fcbergiebest nun die Anzahl deiner Tage/", "tokens": ["Du", "\u00fc\u00b7berg\u00b7ie\u00b7best", "nun", "die", "An\u00b7zahl", "dei\u00b7ner", "Ta\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem/ so von Anbegin derselben Ma\u00df bestimmt/", "tokens": ["Dem", "/", "so", "von", "An\u00b7be\u00b7gin", "der\u00b7sel\u00b7ben", "Ma\u00df", "be\u00b7stimmt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "APPR", "NN", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Legst ab dein Cammer-Ampt sampt aller Kranckheit Plage/", "tokens": ["Legst", "ab", "dein", "Cam\u00b7mer\u00b7Ampt", "sampt", "al\u00b7ler", "Kran\u00b7ck\u00b7heit", "Pla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Weil dich des H\u00f6chsten Ruff zu seinen Renten nimmt.", "tokens": ["Weil", "dich", "des", "H\u00f6chs\u00b7ten", "Ruff", "zu", "sei\u00b7nen", "Ren\u00b7ten", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du bist ein treuer Knecht hier jederzeit gewesen/", "tokens": ["Du", "bist", "ein", "treu\u00b7er", "Knecht", "hier", "je\u00b7der\u00b7zeit", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gott und der Obrigkeit geleistet Schuld und Pflicht:", "tokens": ["Gott", "und", "der", "Ob\u00b7rig\u00b7keit", "ge\u00b7leis\u00b7tet", "Schuld", "und", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Nun h\u00f6rst du au\u00df dem Buch de\u00df Lebens dich verlesen/", "tokens": ["Nun", "h\u00f6rst", "du", "au\u00df", "dem", "Buch", "de\u00df", "Le\u00b7bens", "dich", "ver\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und was vor hohen Lohn dein Heyland dir verspricht.", "tokens": ["Und", "was", "vor", "ho\u00b7hen", "Lohn", "dein", "Hey\u00b7land", "dir", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du dachtest zwar noch wol den Jahr-Schlu\u00df zu vollenden/", "tokens": ["Du", "dach\u00b7test", "zwar", "noch", "wol", "den", "Jahr\u00b7Schlu\u00df", "zu", "voll\u00b7en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Allein der blasse Tod lescht dir die Ziffern au\u00df/", "tokens": ["Al\u00b7lein", "der", "blas\u00b7se", "Tod", "lescht", "dir", "die", "Zif\u00b7fern", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nimmt das gehaltne Buch von den verwelckten H\u00e4nden/", "tokens": ["Nimmt", "das", "ge\u00b7halt\u00b7ne", "Buch", "von", "den", "ver\u00b7welck\u00b7ten", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und f\u00fchrt dich sanfft und still ins finstre Sterbe-Haus.", "tokens": ["Und", "f\u00fchrt", "dich", "sanfft", "und", "still", "ins", "finst\u00b7re", "Ster\u00b7be\u00b7Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nun ist dein Wunsch erf\u00fcllt/ was du so offt gebeten/", "tokens": ["Nun", "ist", "dein", "Wunsch", "er\u00b7f\u00fcllt", "/", "was", "du", "so", "offt", "ge\u00b7be\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$(", "PWS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was du so hoch verlangt/ das wird dir jetzt zutheil/", "tokens": ["Was", "du", "so", "hoch", "ver\u00b7langt", "/", "das", "wird", "dir", "jetzt", "zu\u00b7theil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVPP", "$(", "PDS", "VAFIN", "PPER", "ADV", "KOUS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df du au\u00df diesem Joch de\u00df Lebens m\u00f6chtest treten/", "tokens": ["Da\u00df", "du", "au\u00df", "die\u00b7sem", "Joch", "de\u00df", "Le\u00b7bens", "m\u00f6ch\u00b7test", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und in dem Himmel sehn der Auserwehlten Heil.", "tokens": ["Und", "in", "dem", "Him\u00b7mel", "sehn", "der", "Au\u00b7ser\u00b7wehl\u00b7ten", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Erwegst du hier die Weit/ was hast du eingenommen?", "tokens": ["Er\u00b7wegst", "du", "hier", "die", "Weit", "/", "was", "hast", "du", "ein\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$(", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nichts als von Kindheit an viel Creutzer bittrer Noth:", "tokens": ["Nichts", "als", "von", "Kind\u00b7heit", "an", "viel", "Creut\u00b7zer", "bit\u00b7trer", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPR", "NN", "APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und als die Jugend war zu ihrem Wachsthum kommen/", "tokens": ["Und", "als", "die", "Ju\u00b7gend", "war", "zu", "ih\u00b7rem", "Wach\u00b7sthum", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So sahst du dich umbringt mit Rauben/ Pest und Tod.", "tokens": ["So", "sahst", "du", "dich", "um\u00b7bringt", "mit", "Rau\u00b7ben", "/", "Pest", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der n\u00e4chsten Jahre Lauff ist nur mit Schreckenbergern/", "tokens": ["Der", "n\u00e4chs\u00b7ten", "Jah\u00b7re", "Lauff", "ist", "nur", "mit", "Schre\u00b7cken\u00b7ber\u00b7gern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.26": {"text": "Nicht feiner M\u00fcntze Gold gewesen \u00fcberlegt/", "tokens": ["Nicht", "fei\u00b7ner", "M\u00fcnt\u00b7ze", "Gold", "ge\u00b7we\u00b7sen", "\u00fc\u00b7ber\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "VAPP", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es h\u00e4tte sich dein Hertz offt dr\u00fcber m\u00fcssen \u00e4rgern/", "tokens": ["Es", "h\u00e4t\u00b7te", "sich", "dein", "Hertz", "offt", "dr\u00fc\u00b7ber", "m\u00fcs\u00b7sen", "\u00e4r\u00b7gern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPOSAT", "NN", "ADV", "PAV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn nicht auch wiederumb der H\u00f6chste dein gepflegt.", "tokens": ["Wenn", "nicht", "auch", "wie\u00b7de\u00b7rumb", "der", "H\u00f6chs\u00b7te", "dein", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ADV", "ART", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das Gl\u00fccke schien dir drauff mit seinen Gnaden-Blicken/", "tokens": ["Das", "Gl\u00fc\u00b7cke", "schien", "dir", "drauff", "mit", "sei\u00b7nen", "Gna\u00b7den\u00b7Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PAV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als dir vom F\u00fcrstenthum die Cassa ward vertraut/", "tokens": ["Als", "dir", "vom", "F\u00fcrs\u00b7ten\u00b7thum", "die", "Cas\u00b7sa", "ward", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NE", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da aber wolt\u2019 es dich noch freundlicher erquicken/", "tokens": ["Da", "a\u00b7ber", "wolt'", "es", "dich", "noch", "freund\u00b7li\u00b7cher", "er\u00b7qui\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als es den Lebens-Sitz in ", "tokens": ["Als", "es", "den", "Le\u00b7bens\u00b7Sitz", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Denn hast du viel summirt/ viel Posten au\u00dfgezahlet/", "tokens": ["Denn", "hast", "du", "viel", "sum\u00b7mirt", "/", "viel", "Pos\u00b7ten", "au\u00df\u00b7ge\u00b7zah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$(", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Belag und Quittungen in Richtigkeit gesetzt/", "tokens": ["Be\u00b7lag", "und", "Quit\u00b7tun\u00b7gen", "in", "Rich\u00b7tig\u00b7keit", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So da\u00df der ", "tokens": ["So", "da\u00df", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Und zu der Heimligkeit der Cammer werth gesch\u00e4tzt.", "tokens": ["Und", "zu", "der", "Heim\u00b7lig\u00b7keit", "der", "Cam\u00b7mer", "werth", "ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du warst in deinem Ampt wach/ embsig/ unverdrossen/", "tokens": ["Du", "warst", "in", "dei\u00b7nem", "Ampt", "wach", "/", "emb\u00b7sig", "/", "un\u00b7ver\u00b7dros\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$(", "ADJD", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Von keiner M\u00fch erschreckt/ von keiner Arbeit bleich/", "tokens": ["Von", "kei\u00b7ner", "M\u00fch", "er\u00b7schreckt", "/", "von", "kei\u00b7ner", "Ar\u00b7beit", "bleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$(", "APPR", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Du hast die Rechnungen mit solcher Art geschlossen/", "tokens": ["Du", "hast", "die", "Rech\u00b7nun\u00b7gen", "mit", "sol\u00b7cher", "Art", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Da\u00df unter Hunderten dir wol nicht einer gleich.", "tokens": ["Da\u00df", "un\u00b7ter", "Hun\u00b7der\u00b7ten", "dir", "wol", "nicht", "ei\u00b7ner", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "ADV", "PTKNEG", "ART", "ADV", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.41": {"text": "Du bliebest stets bedacht/ wie Rechenschafft zu geben/", "tokens": ["Du", "blie\u00b7best", "stets", "be\u00b7dacht", "/", "wie", "Re\u00b7chen\u00b7schafft", "zu", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$(", "KOKOM", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.he