{"textgrid.poem.57346": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mach, Apoll, dass mein Lied, bat Hemis opfernd dem Gotte,", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mach, Apoll, dass mein Lied, bat Hemis opfernd dem Gotte,", "tokens": ["Mach", ",", "A\u00b7poll", ",", "dass", "mein", "Lied", ",", "bat", "He\u00b7mis", "op\u00b7fernd", "dem", "Got\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "NE", "VVPP", "ART", "NN", "$,"], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gleich dem Bilde Pigmalions sey:", "tokens": ["Gleich", "dem", "Bil\u00b7de", "Pig\u00b7ma\u00b7li\u00b7ons", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dass es die Kunst verberge, doch nicht dem beschauenden Richter:", "tokens": ["Dass", "es", "die", "Kunst", "ver\u00b7ber\u00b7ge", ",", "doch", "nicht", "dem", "be\u00b7schau\u00b7en\u00b7den", "Rich\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Dieser suche sie, finde sie schnell.", "tokens": ["Die\u00b7ser", "su\u00b7che", "sie", ",", "fin\u00b7de", "sie", "schnell", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "O dann rolle der stolze Rapsod' es zusammen, und sage", "tokens": ["O", "dann", "rol\u00b7le", "der", "stol\u00b7ze", "Rap\u00b7sod'", "es", "zu\u00b7sam\u00b7men", ",", "und", "sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Achselzuckend, es sey nicht f\u00fcr ihn.", "tokens": ["Ach\u00b7sel\u00b7zu\u00b7ckend", ",", "es", "sey", "nicht", "f\u00fcr", "ihn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "(artemis, trif den Rapsoden, den gleich Verg\u00e4nglichheit ahndet;", "tokens": ["(", "ar\u00b7te\u00b7mis", ",", "trif", "den", "Ra\u00b7pso\u00b7den", ",", "den", "gleich", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7heit", "ahn\u00b7det", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Weigert sich seinem Ton ein Gedicht.)", "tokens": ["Wei\u00b7gert", "sich", "sei\u00b7nem", "Ton", "ein", "Ge\u00b7dicht", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "ART", "NN", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Mache, Ph\u00f6bus Apoll, dass mein Lied, bat Telon am Altar,", "tokens": ["Ma\u00b7che", ",", "Ph\u00f6\u00b7bus", "A\u00b7poll", ",", "dass", "mein", "Lied", ",", "bat", "Te\u00b7lon", "am", "Al\u00b7tar", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "NE", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Gleich dem M\u00e4dchen Pigmalions sey,", "tokens": ["Gleich", "dem", "M\u00e4d\u00b7chen", "Pig\u00b7ma\u00b7li\u00b7ons", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Da verwandelt der Marmor nun war, die Wang' ihm entgl\u00fchte,", "tokens": ["Da", "ver\u00b7wan\u00b7delt", "der", "Mar\u00b7mor", "nun", "war", ",", "die", "Wang'", "ihm", "ent\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "VAFIN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Da die Ader ihm schlug, und das Herz!", "tokens": ["Da", "die", "A\u00b7der", "ihm", "schlug", ",", "und", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Dass der H\u00f6rer, wie er beseelt, des Sp\u00fchens vergesse", "tokens": ["Dass", "der", "H\u00f6\u00b7rer", ",", "wie", "er", "be\u00b7seelt", ",", "des", "Sp\u00fc\u00b7hens", "ver\u00b7ges\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+--+-", "measure": "hexame