{"dta.poem.19335": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war ein K\u00f6nigsschlo\u00df, darauf war eine Uhr,", "tokens": ["Es", "war", "ein", "K\u00f6\u00b7nigs\u00b7schlo\u00df", ",", "da\u00b7rauf", "war", "ei\u00b7ne", "Uhr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wies dem K\u00f6nige die eine Stunde nur,", "tokens": ["Die", "wies", "dem", "K\u00f6\u00b7ni\u00b7ge", "die", "ei\u00b7ne", "Stun\u00b7de", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die eine Stund', in der sein Vorfahr einst erblich,", "tokens": ["Die", "ei\u00b7ne", "Stund'", ",", "in", "der", "sein", "Vor\u00b7fahr", "einst", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieselbe zeigte ihm der Zeiger, der nie wich.", "tokens": ["Die\u00b7sel\u00b7be", "zeig\u00b7te", "ihm", "der", "Zei\u00b7ger", ",", "der", "nie", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wei\u00dft du, wann er sich erst von der Stelle r\u00fchrte?", "tokens": ["Und", "wei\u00dft", "du", ",", "wann", "er", "sich", "erst", "von", "der", "Stel\u00b7le", "r\u00fchr\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann er den nahen Tod des neuen K\u00f6nigs sp\u00fcrte.", "tokens": ["Wann", "er", "den", "na\u00b7hen", "Tod", "des", "neu\u00b7en", "K\u00f6\u00b7nigs", "sp\u00fcr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann gieng er kurze Frist, und wieder in den Frieden", "tokens": ["Dann", "gieng", "er", "kur\u00b7ze", "Frist", ",", "und", "wie\u00b7der", "in", "den", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sank er zur\u00fcck, sobald der K\u00f6nig auch verschieden.", "tokens": ["Sank", "er", "zu\u00b7r\u00fcck", ",", "so\u00b7bald", "der", "K\u00f6\u00b7nig", "auch", "ver\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du fragest: K\u00f6nige, mit solcher Uhr begabt,", "tokens": ["Du", "fra\u00b7gest", ":", "K\u00f6\u00b7ni\u00b7ge", ",", "mit", "sol\u00b7cher", "Uhr", "be\u00b7gabt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die also ihren Tod vor Augen stets gehabt,", "tokens": ["Die", "al\u00b7so", "ih\u00b7ren", "Tod", "vor", "Au\u00b7gen", "stets", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "APPR", "NN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vor allen sind sie weis' und m\u00e4\u00dfig wol gewesen?", "tokens": ["Vor", "al\u00b7len", "sind", "sie", "weis'", "und", "m\u00e4\u00b7\u00dfig", "wol", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "VVFIN", "KON", "ADJD", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sollte meinen, ja! Doch hab' ichs nicht gelesen.", "tokens": ["Man", "soll\u00b7te", "mei\u00b7nen", ",", "ja", "!", "Doch", "hab'", "ichs", "nicht", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "$,", "ADV", "$.", "KON", "VAFIN", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}