{"textgrid.poem.25247": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "Die Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Fernesucht geboren,", "tokens": ["Zur", "Fer\u00b7ne\u00b7sucht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird nie der Pilgram froh.", "tokens": ["Wird", "nie", "der", "Pilg\u00b7ram", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seine Heimat ging verloren,", "tokens": ["Sei\u00b7ne", "Hei\u00b7mat", "ging", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er wei\u00df nicht wo.", "tokens": ["Er", "wei\u00df", "nicht", "wo", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihn r\u00fchrt ein stummes Mahnen", "tokens": ["Ihn", "r\u00fchrt", "ein", "stum\u00b7mes", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von blauer Berge Wand.", "tokens": ["Von", "blau\u00b7er", "Ber\u00b7ge", "Wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darf er dahinter ahnen", "tokens": ["Darf", "er", "da\u00b7hin\u00b7ter", "ah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Wunderland?", "tokens": ["Sein", "Wun\u00b7der\u00b7land", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Im Tale Bauden winken,", "tokens": ["Im", "Ta\u00b7le", "Bau\u00b7den", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Dorfe traut gereiht.", "tokens": ["Zum", "Dor\u00b7fe", "traut", "ge\u00b7reiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er aber mu\u00df versinken", "tokens": ["Er", "a\u00b7ber", "mu\u00df", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Einsamkeit.", "tokens": ["In", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Er haust auf Bergesklippen", "tokens": ["Er", "haust", "auf", "Ber\u00b7ges\u00b7klip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dumpfer Schwermut Bann,", "tokens": ["In", "dum\u00b7pfer", "Schwer\u00b7mut", "Bann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umstarrt von Knieholz-Rippen", "tokens": ["Um\u00b7starrt", "von", "Knie\u00b7holz\u00b7Rip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00fcstem Tann.", "tokens": ["Und", "w\u00fcs\u00b7tem", "Tann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Verworren tr\u00e4umt im Grunde", "tokens": ["Ver\u00b7wor\u00b7ren", "tr\u00e4umt", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des M\u00fchlenrads Gesumm.", "tokens": ["Des", "M\u00fch\u00b7len\u00b7rads", "Ge\u00b7summ", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er lauscht mit zuckendem Munde,", "tokens": ["Er", "lauscht", "mit", "zu\u00b7cken\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sein Lied bleibt stumm.", "tokens": ["Sein", "Lied", "bleibt", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Er schmachtet, wie im Staube", "tokens": ["Er", "schmach\u00b7tet", ",", "wie", "im", "Stau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein welkes Blumenhaupt.", "tokens": ["Ein", "wel\u00b7kes", "Blu\u00b7men\u00b7haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ward sein frommer Glaube", "tokens": ["Doch", "ward", "sein", "from\u00b7mer", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm nicht geraubt.", "tokens": ["Ihm", "nicht", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "O Pilgram, du mu\u00dft lernen", "tokens": ["O", "Pilg\u00b7ram", ",", "du", "mu\u00dft", "ler\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Demut abseits stahn,", "tokens": ["In", "De\u00b7mut", "ab\u00b7seits", "stahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du darfst den blauen Fernen", "tokens": ["Du", "darfst", "den", "blau\u00b7en", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nie t\u00e4ppisch nahn.", "tokens": ["Nie", "t\u00e4p\u00b7pisch", "nahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Wenn ungest\u00fcme Minne", "tokens": ["Wenn", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Min\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich ri\u00df zum G\u00f6tterweib,", "tokens": ["Dich", "ri\u00df", "zum", "G\u00f6t\u00b7ter\u00b7weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umarmten deine Sinne", "tokens": ["Um\u00b7arm\u00b7ten", "dei\u00b7ne", "Sin\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur Menschenleib.", "tokens": ["Nur", "Men\u00b7schen\u00b7leib", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "So bleib dem Wunderlande", "tokens": ["So", "bleib", "dem", "Wun\u00b7der\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In keuscher Andacht hold.", "tokens": ["In", "keu\u00b7scher", "An\u00b7dacht", "hold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann sp\u00fclst du aus dem Sande", "tokens": ["Dann", "sp\u00fclst", "du", "aus", "dem", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ewige Gold.", "tokens": ["Das", "e\u00b7wi\u00b7ge", "Gold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Es sammelt alle Z\u00e4hren", "tokens": ["Es", "sam\u00b7melt", "al\u00b7le", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die treue Ewigkeit.", "tokens": ["Die", "treu\u00b7e", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sollen sich verkl\u00e4ren", "tokens": ["Sie", "sol\u00b7len", "sich", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Krongeschmeid.", "tokens": ["Zum", "Kron\u00b7ge\u00b7schmeid", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "O sieh, ein Fenster gl\u00fchet", "tokens": ["O", "sieh", ",", "ein", "Fens\u00b7ter", "gl\u00fc\u00b7het"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im roten Abendglast!", "tokens": ["Im", "ro\u00b7ten", "A\u00b7bend\u00b7glast", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Baudenhaus erbl\u00fchet", "tokens": ["Das", "Bau\u00b7den\u00b7haus", "er\u00b7bl\u00fc\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Goldpalast.", "tokens": ["Zum", "Gold\u00b7pa\u00b7last", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Die Felsenschatten dehnen", "tokens": ["Die", "Fel\u00b7sen\u00b7schat\u00b7ten", "deh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich weit ins Talgefild.", "tokens": ["Sich", "weit", "ins", "Tal\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wird wohl manches Sehnen", "tokens": ["So", "wird", "wohl", "man\u00b7ches", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch sp\u00e4t gestillt.", "tokens": ["Noch", "sp\u00e4t", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Erst wenn im gro\u00dfen Dunkel", "tokens": ["Erst", "wenn", "im", "gro\u00b7\u00dfen", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Versank die wirre Welt,", "tokens": ["Ver\u00b7sank", "die", "wir\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erbl\u00fcht das Trostgefunkel", "tokens": ["Er\u00b7bl\u00fcht", "das", "Trost\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Sternenzelt.", "tokens": ["Am", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Und birgt sich in der Erden", "tokens": ["Und", "birgt", "sich", "in", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ratlos dein Angesicht,", "tokens": ["Rat\u00b7los", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambi