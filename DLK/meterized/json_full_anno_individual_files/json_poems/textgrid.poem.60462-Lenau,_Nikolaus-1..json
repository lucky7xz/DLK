{"textgrid.poem.60462": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruhig ist der Wald bei Trocznow", "tokens": ["Ru\u00b7hig", "ist", "der", "Wald", "bei", "Trocz\u00b7now"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der abendlichen Stunde,", "tokens": ["In", "der", "a\u00b7bend\u00b7li\u00b7chen", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Alle Wipfel sind so stille,", "tokens": ["Al\u00b7le", "Wip\u00b7fel", "sind", "so", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Wurzeln tief im Grunde.", "tokens": ["Wie", "die", "Wur\u00b7zeln", "tief", "im", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In Gedanken naht ein Reiter,", "tokens": ["In", "Ge\u00b7dan\u00b7ken", "naht", "ein", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Arm den Zaum geschlungen,", "tokens": ["Um", "den", "Arm", "den", "Zaum", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Schlendernd senkt den Kopf sein Rappe", "tokens": ["Schlen\u00b7dernd", "senkt", "den", "Kopf", "sein", "Rap\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In Gedankend\u00e4mmerungen.", "tokens": ["In", "Ge\u00b7dan\u00b7ken\u00b7d\u00e4m\u00b7me\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich h\u00e4lt der Reiter inne,", "tokens": ["Pl\u00f6tz\u00b7lich", "h\u00e4lt", "der", "Rei\u00b7ter", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie erwacht aus einem Traume,", "tokens": ["Wie", "er\u00b7wacht", "aus", "ei\u00b7nem", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schreitet ab und zieht den Degen,", "tokens": ["Schrei\u00b7tet", "ab", "und", "zieht", "den", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht an einem Eichenbaume:", "tokens": ["Spricht", "an", "ei\u00b7nem", "Ei\u00b7chen\u00b7bau\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbhier an dieser festen Eiche", "tokens": ["\u00bb", "hier", "an", "die\u00b7ser", "fes\u00b7ten", "Ei\u00b7che"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat in einer Wetternacht,", "tokens": ["Hat", "in", "ei\u00b7ner", "Wet\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcberrascht von scharfen Wehen,", "tokens": ["\u00dc\u00b7berr\u00b7ascht", "von", "schar\u00b7fen", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mutter mich zur Welt gebracht.", "tokens": ["Mut\u00b7ter", "mich", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur der Wald vernahm ihr Krei\u00dfen,", "tokens": ["Nur", "der", "Wald", "ver\u00b7nahm", "ihr", "Krei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Windsbraut war die Hebeamme,", "tokens": ["Winds\u00b7braut", "war", "die", "He\u00b7beam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sie go\u00df dem Kinde segnend", "tokens": ["Und", "sie", "go\u00df", "dem", "Kin\u00b7de", "seg\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcbers Haupt die Blitzesflamme.", "tokens": ["\u00dc\u00b7bers", "Haupt", "die", "Blit\u00b7zes\u00b7flam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcr Geschosse mich zu st\u00e4rken", "tokens": ["F\u00fcr", "Ge\u00b7schos\u00b7se", "mich", "zu", "st\u00e4r\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein hartes Heldenlos,", "tokens": ["Und", "ein", "har\u00b7tes", "Hel\u00b7den\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlug der Hagel meiner Mutter", "tokens": ["Schlug", "der", "Ha\u00b7gel", "mei\u00b7ner", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den schmerzgesprengten Scho\u00df.", "tokens": ["In", "den", "schmerz\u00b7ge\u00b7spreng\u00b7ten", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Donner war mein erstes H\u00f6ren,", "tokens": ["Don\u00b7ner", "war", "mein", "ers\u00b7tes", "H\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sturm mein erster Atemzug;", "tokens": ["Sturm", "mein", "ers\u00b7ter", "A\u00b7tem\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein rauher Wetters\u00e4ugling", "tokens": ["Als", "ein", "rau\u00b7her", "Wet\u00b7ter\u00b7s\u00e4ug\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehm ich meinen Heldenflug.", "tokens": ["Nehm", "ich", "mei\u00b7nen", "Hel\u00b7den\u00b7flug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hu\u00df! an dieser festen Eiche", "tokens": ["Hu\u00df", "!", "an", "die\u00b7ser", "fes\u00b7ten", "Ei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schw\u00f6r ich Rache deinem Tod;", "tokens": ["Schw\u00f6r", "ich", "Ra\u00b7che", "dei\u00b7nem", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hu\u00df! vom Blute deiner Schergen", "tokens": ["Hu\u00df", "!", "vom", "Blu\u00b7te", "dei\u00b7ner", "Scher\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird es bald auf Erden rot.", "tokens": ["Wird", "es", "bald", "auf", "Er\u00b7den", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Hu\u00df! so reich aus ihren Adern", "tokens": ["Hu\u00df", "!", "so", "reich", "aus", "ih\u00b7ren", "A\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll das Blut zu Boden laufen,", "tokens": ["Soll", "das", "Blut", "zu", "Bo\u00b7den", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es hundertmal dir k\u00f6nnte", "tokens": ["Da\u00df", "es", "hun\u00b7dert\u00b7mal", "dir", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00f6schen deinen Scheiterhaufen.", "tokens": ["L\u00f6\u00b7schen", "dei\u00b7nen", "Schei\u00b7ter\u00b7hau\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hu\u00df! vom Brandschutt ihrer Burgen", "tokens": ["Hu\u00df", "!", "vom", "Brand\u00b7schutt", "ih\u00b7rer", "Bur\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll die Erde schwarz sich f\u00e4rben;", "tokens": ["Soll", "die", "Er\u00b7de", "schwarz", "sich", "f\u00e4r\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich einen Priester treffe,", "tokens": ["Wo", "ich", "ei\u00b7nen", "Pries\u00b7ter", "tref\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll er fallen, soll er sterben.", "tokens": ["Soll", "er", "fal\u00b7len", ",", "soll", "er", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Rotgebeizt von Raucheswolken", "tokens": ["Rot\u00b7ge\u00b7beizt", "von", "Rau\u00b7ches\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll des Himmels Aug sich tr\u00fcben,", "tokens": ["Soll", "des", "Him\u00b7mels", "Aug", "sich", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sie durften solchen Frevel", "tokens": ["Weil", "sie", "durf\u00b7ten", "sol\u00b7chen", "Fre\u00b7vel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm ins Angesicht ver\u00fcben.", "tokens": ["Ihm", "ins", "An\u00b7ge\u00b7sicht", "ver\u00b7\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Mir im Herzen brennt ein Funken,", "tokens": ["Mir", "im", "Her\u00b7zen", "brennt", "ein", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hu\u00df! von deinem Todesfeuer,", "tokens": ["Hu\u00df", "!", "von", "dei\u00b7nem", "To\u00b7des\u00b7feu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unausl\u00f6schbar; wie der Frevel", "tokens": ["Un\u00b7aus\u00b7l\u00f6schbar", ";", "wie", "der", "Fre\u00b7vel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sei die Rache ungeheuer.", "tokens": ["Sei", "die", "Ra\u00b7che", "un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Mann des Lichtes, Mann der Freiheit,", "tokens": ["Mann", "des", "Lich\u00b7tes", ",", "Mann", "der", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bester, den die Welt getragen,", "tokens": ["Bes\u00b7ter", ",", "den", "die", "Welt", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schn\u00f6d verraten, hingerichtet! \u2013", "tokens": ["Schn\u00f6d", "ver\u00b7ra\u00b7ten", ",", "hin\u00b7ge\u00b7rich\u00b7tet", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mordend will ich um dich klagen.", "tokens": ["Mor\u00b7dend", "will", "ich", "um", "dich", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "O wie still die L\u00fcfte B\u00f6hmens", "tokens": ["O", "wie", "still", "die", "L\u00fcf\u00b7te", "B\u00f6h\u00b7mens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horchen meinem Racheschw\u00f6ren,", "tokens": ["Hor\u00b7chen", "mei\u00b7nem", "Ra\u00b7ch\u00b7e\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und die vaterl\u00e4ndschen Bl\u00e4tter", "tokens": ["Und", "die", "va\u00b7ter\u00b7l\u00e4nd\u00b7schen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen mein Gel\u00fcbde h\u00f6ren.", "tokens": ["Wol\u00b7len", "mein", "Ge\u00b7l\u00fcb\u00b7de", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Leib und Seele will ich brauchen,", "tokens": ["Leib", "und", "See\u00b7le", "will", "ich", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwert und Flammen und Gescho\u00df,", "tokens": ["Schwert", "und", "Flam\u00b7men", "und", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3":