{"textgrid.poem.48425": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf eine Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kan und soll ich euch zu diesem neuen Leben,", "tokens": ["Was", "kan", "und", "soll", "ich", "euch", "zu", "die\u00b7sem", "neu\u00b7en", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "KON", "VMFIN", "PPER", "PRF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das ihr mit aller Treu' itzt wollet wol anheben,", "tokens": ["das", "ihr", "mit", "al\u00b7ler", "Treu'", "itzt", "wol\u00b7let", "wol", "an\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "ADV", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "vor ein Geschenke tun, das Gott und euch und mir", "tokens": ["vor", "ein", "Ge\u00b7schen\u00b7ke", "tun", ",", "das", "Gott", "und", "euch", "und", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "ART", "NN", "KON", "PPER", "KON", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "recht angen\u00e4hme sei? Mir ist nichts \u00fcbrig hier", "tokens": ["recht", "an\u00b7ge\u00b7n\u00e4h\u00b7me", "sei", "?", "Mir", "ist", "nichts", "\u00fcb\u00b7rig", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "VAFIN", "$.", "PPER", "VAFIN", "PIS", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "als ein beherzter Wundsch, den Gott f\u00fcr allen Gaben,", "tokens": ["als", "ein", "be\u00b7herz\u00b7ter", "Wund\u00b7sch", ",", "den", "Gott", "f\u00fcr", "al\u00b7len", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "f\u00fcr allen Reicht\u00fcmern ihm will geschenket haben,", "tokens": ["f\u00fcr", "al\u00b7len", "Reich\u00b7t\u00fc\u00b7mern", "ihm", "will", "ge\u00b7schen\u00b7ket", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und ihr seid auch vergn\u00fcgt. Gott helfe, da\u00df der Bund,", "tokens": ["und", "ihr", "seid", "auch", "ver\u00b7gn\u00fcgt", ".", "Gott", "hel\u00b7fe", ",", "da\u00df", "der", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$.", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den ihr, ihr liebes Paar, durch Wundsch, durch Hand, durch Mund", "tokens": ["den", "ihr", ",", "ihr", "lie\u00b7bes", "Paar", ",", "durch", "Wund\u00b7sch", ",", "durch", "Hand", ",", "durch", "Mund"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "bekr\u00e4ftigt und vollbracht, euch ewig m\u00f6ge n\u00fctzen,", "tokens": ["be\u00b7kr\u00e4f\u00b7tigt", "und", "voll\u00b7bracht", ",", "euch", "e\u00b7wig", "m\u00f6\u00b7ge", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ein Schild f\u00fcr Unfall sein, f\u00fcr allem \u00dcbel sch\u00fctzen,", "tokens": ["ein", "Schild", "f\u00fcr", "Un\u00b7fall", "sein", ",", "f\u00fcr", "al\u00b7lem", "\u00dc\u00b7bel", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAINF", "$,", "APPR", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "das sonst den Einsamen zu Handen sto\u00dfen pflegt", "tokens": ["das", "sonst", "den", "Ein\u00b7sa\u00b7men", "zu", "Han\u00b7den", "sto\u00b7\u00dfen", "pflegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und sie zu vieler Angst und K\u00fcmmern\u00fc\u00df bewegt!", "tokens": ["und", "sie", "zu", "vie\u00b7ler", "Angst", "und", "K\u00fcm\u00b7mer\u00b7n\u00fc\u00df", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Lebt frisch, lebt fruchtbarlich, lebt selig, wie ihr lebet!", "tokens": ["Lebt", "frisch", ",", "lebt", "frucht\u00b7bar\u00b7lich", ",", "lebt", "se\u00b7lig", ",", "wie", "ihr", "le\u00b7bet", "!"], "t