{"textgrid.poem.46942": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir schlingen unsre H\u00e4nd' in einen Knoten,", "tokens": ["Wir", "schlin\u00b7gen", "uns\u00b7re", "H\u00e4nd'", "in", "ei\u00b7nen", "Kno\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmel heben wir die Blick' und schw\u00f6ren;", "tokens": ["Zum", "Him\u00b7mel", "he\u00b7ben", "wir", "die", "Blick'", "und", "schw\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr alle, die ihr lebet, sollt es h\u00f6ren,", "tokens": ["Ihr", "al\u00b7le", ",", "die", "ihr", "le\u00b7bet", ",", "sollt", "es", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn ihr wollt, so h\u00f6rt auch ihr's, ihr Toten!", "tokens": ["Und", "wenn", "ihr", "wollt", ",", "so", "h\u00f6rt", "auch", "ih\u00b7r's", ",", "ihr", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "ADV", "PIS", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wir schw\u00f6ren: Stehn zu wollen den Geboten", "tokens": ["Wir", "schw\u00f6\u00b7ren", ":", "Stehn", "zu", "wol\u00b7len", "den", "Ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "PTKZU", "VMINF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Lands, des Mark wir tragen in den R\u00f6hren;", "tokens": ["Des", "Lands", ",", "des", "Mark", "wir", "tra\u00b7gen", "in", "den", "R\u00f6h\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und diese Schwerter, die wir hier emp\u00f6ren,", "tokens": ["Und", "die\u00b7se", "Schwer\u00b7ter", ",", "die", "wir", "hier", "em\u00b7p\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht eh'r zu senken, als vom Feind zerschroten.", "tokens": ["Nicht", "eh'r", "zu", "sen\u00b7ken", ",", "als", "vom", "Feind", "zer\u00b7schro\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "$,", "KOUS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wir schw\u00f6ren, da\u00df kein Vater nach dem Sohne", "tokens": ["Wir", "schw\u00f6\u00b7ren", ",", "da\u00df", "kein", "Va\u00b7ter", "nach", "dem", "Soh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Soll fragen, und nach seinem Weib kein Gatte,", "tokens": ["Soll", "fra\u00b7gen", ",", "und", "nach", "sei\u00b7nem", "Weib", "kein", "Gat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KON", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kein Krieger fragen soll nach seinem Lohne,", "tokens": ["Kein", "Krie\u00b7ger", "fra\u00b7gen", "soll", "nach", "sei\u00b7nem", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Noch heimgehn, eh' der Krieg, der nimmersatte,", "tokens": ["Noch", "heim\u00b7gehn", ",", "eh'", "der", "Krieg", ",", "der", "nim\u00b7mer\u00b7sat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ihn selbst entl\u00e4\u00dft mit einer blut'gen Krone,", "tokens": ["Ihn", "selbst", "ent\u00b7l\u00e4\u00dft", "mit", "ei\u00b7ner", "blut'\u00b7gen", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df man ihn heile, oder ihn bestatte!", "tokens": ["Da\u00df", "man", "ihn", "hei\u00b7le", ",", "o\u00b7der", "ihn", "be\u00b7stat\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": 