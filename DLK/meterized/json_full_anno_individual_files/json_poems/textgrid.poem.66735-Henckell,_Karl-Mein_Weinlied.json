, "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn man es nur nicht heimlich treibt", "tokens": ["Wenn", "man", "es", "nur", "nicht", "heim\u00b7lich", "treibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach Art der Potatoren.", "tokens": ["Nach", "Art", "der", "Po\u00b7ta\u00b7to\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch schw\u00f6r' ich mich in ", "tokens": ["Auch", "schw\u00f6r'", "ich", "mich", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und andrer sch\u00f6ner Dinge", "tokens": ["Und", "an\u00b7drer", "sch\u00f6\u00b7ner", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durchaus nicht auf den Chauvin ein,", "tokens": ["Durc\u00b7haus", "nicht", "auf", "den", "Chau\u00b7vin", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Weshalb ich z\u00e4rtlich singe:", "tokens": ["We\u00b7shalb", "ich", "z\u00e4rt\u00b7lich", "sin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein alter Valle d'oro schmeckt", "tokens": ["Ein", "al\u00b7ter", "Val\u00b7le", "d'\u00b7o\u00b7ro", "schmeckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "\u00d6l-perlend h\u00f6chst gediegen,", "tokens": ["\u00d6l\u00b7per\u00b7lend", "h\u00f6chst", "ge\u00b7die\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wenn er im Korbfiasko steckt", "tokens": ["Wenn", "er", "im", "Korb\u00b7fi\u00b7as\u00b7ko", "steckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und scheint nicht zu versiegen.", "tokens": ["Und", "scheint", "nicht", "zu", "ver\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So nach Bordeaux und nach Burgund", "tokens": ["So", "nach", "Bor\u00b7dea\u00b7ux", "und", "nach", "Bur\u00b7gund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "KON", "APPR", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wallfahrt' ich immer gerne,", "tokens": ["Wall\u00b7fahrt'", "ich", "im\u00b7mer", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach euch \u00bbweint\u00ab grade mir der Mund,", "tokens": ["Nach", "euch", "\u00bb", "weint", "\u00ab", "gra\u00b7de", "mir", "der", "Mund", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "VVFIN", "$(", "ADV", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Chablis und Haut Sauternes.", "tokens":