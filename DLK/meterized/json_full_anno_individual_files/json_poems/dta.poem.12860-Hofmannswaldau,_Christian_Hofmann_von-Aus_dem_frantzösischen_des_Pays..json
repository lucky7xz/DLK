{"dta.poem.12860": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem frantz\u00f6sischen des   Pays.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald das sonnen-licht die m\u00fcden augenlieder", "tokens": ["So", "bald", "das", "son\u00b7nen\u00b7licht", "die", "m\u00fc\u00b7den", "au\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach der geno\u00dfnen ruh von ihrem schlaf erweckt,", "tokens": ["Nach", "der", "ge\u00b7no\u00df\u00b7nen", "ruh", "von", "ih\u00b7rem", "schlaf", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und das verliebte kind die alabaster-glieder", "tokens": ["Und", "das", "ver\u00b7lieb\u00b7te", "kind", "die", "a\u00b7la\u00b7bas\u00b7ter\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit einem linden ach im bette von sich streckt,", "tokens": ["Mit", "ei\u00b7nem", "lin\u00b7den", "ach", "im", "bet\u00b7te", "von", "sich", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "APPRART", "VVFIN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So gehn viel hundert l\u00fcst\u2019 in ihrer brust spatzieren,", "tokens": ["So", "gehn", "viel", "hun\u00b7dert", "l\u00fcst'", "in", "ih\u00b7rer", "brust", "spat\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sanffte k\u00fctzelung durchstreichet geist und sinn,", "tokens": ["Die", "sanff\u00b7te", "k\u00fct\u00b7ze\u00b7lung", "durch\u00b7strei\u00b7chet", "geist", "und", "sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und die entz\u00fcckungen, so marck und adern r\u00fchren,", "tokens": ["Und", "die", "ent\u00b7z\u00fc\u00b7ckun\u00b7gen", ",", "so", "marck", "und", "a\u00b7dern", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADV", "NN", "KON", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zieh\u2019n sie gantz au\u00dfer sich in den gedancken hin.", "tokens": ["Zieh'n", "sie", "gantz", "au\u00b7\u00dfer", "sich", "in", "den", "ge\u00b7dan\u00b7cken", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Die liebe schmeichelt ihr, sie l\u00f6scht der sehnsucht kohlen,", "tokens": ["Die", "lie\u00b7be", "schmei\u00b7chelt", "ihr", ",", "sie", "l\u00f6scht", "der", "sehn\u00b7sucht", "koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und baut aus ihrem bett ein schlo\u00df der einsamkeit,", "tokens": ["Und", "baut", "aus", "ih\u00b7rem", "bett", "ein", "schlo\u00df", "der", "ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da kan sie in der ruh die wohllust wiederholen,", "tokens": ["Da", "kan", "sie", "in", "der", "ruh", "die", "wohl\u00b7lust", "wie\u00b7der\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit der sie sich zuvor in unruh hat erfreut.", "tokens": ["Mit", "der", "sie", "sich", "zu\u00b7vor", "in", "un\u00b7ruh", "hat", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "APPR", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}