{"textgrid.poem.67418": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich pflanzte fr\u00fch ein kleines Lorbeerreis", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich pflanzte fr\u00fch ein kleines Lorbeerreis", "tokens": ["Ich", "pflanz\u00b7te", "fr\u00fch", "ein", "klei\u00b7nes", "Lor\u00b7beer\u00b7reis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sah gen Himmel auf mit stiller Bitte:", "tokens": ["Und", "sah", "gen", "Him\u00b7mel", "auf", "mit", "stil\u00b7ler", "Bit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbla\u00df, Himmel, dieses B\u00e4umchen gl\u00fccklich wachsen,", "tokens": ["\u00bb", "la\u00df", ",", "Him\u00b7mel", ",", "die\u00b7ses", "B\u00e4um\u00b7chen", "gl\u00fcck\u00b7lich", "wach\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "PDAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es mit Zier einmal den Pflanzer kr\u00f6ne!\u00ab", "tokens": ["Da\u00df", "es", "mit", "Zier", "ein\u00b7mal", "den", "Pflan\u00b7zer", "kr\u00f6\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und bat den 