{"dta.poem.16182": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "236.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie unvollkommene Vorstellungen von Sf\u00e4ren", "tokens": ["Wie", "un\u00b7voll\u00b7kom\u00b7me\u00b7ne", "Vor\u00b7stel\u00b7lun\u00b7gen", "von", "Sf\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Himmels und der Welt kannst du im Geiste n\u00e4hren,", "tokens": ["Des", "Him\u00b7mels", "und", "der", "Welt", "kannst", "du", "im", "Geis\u00b7te", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und doch vollkommen fest in deiner Sf\u00e4re seyn;", "tokens": ["Und", "doch", "voll\u00b7kom\u00b7men", "fest", "in", "dei\u00b7ner", "Sf\u00e4\u00b7re", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wenig flie\u00dfet auf das Thun das Wissen ein.", "tokens": ["So", "we\u00b7nig", "flie\u00b7\u00dfet", "auf", "das", "Thun", "das", "Wis\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer recht thut in der Welt, hat rechten Weltverstand,", "tokens": ["Wer", "recht", "thut", "in", "der", "Welt", ",", "hat", "rech\u00b7ten", "Welt\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob er auch nicht die kunstgerechten Formeln fand.", "tokens": ["Ob", "er", "auch", "nicht", "die", "kunst\u00b7ge\u00b7rech\u00b7ten", "For\u00b7meln", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Ausdruck fehlt ihm nur, doch nicht der Einsicht Kern;", "tokens": ["Der", "Aus\u00b7druck", "fehlt", "ihm", "nur", ",", "doch", "nicht", "der", "Ein\u00b7sicht", "Kern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer entbehrt nicht um den Kern die Schale gern?", "tokens": ["Und", "wer", "ent\u00b7behrt", "nicht", "um", "den", "Kern", "die", "Scha\u00b7le", "gern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}