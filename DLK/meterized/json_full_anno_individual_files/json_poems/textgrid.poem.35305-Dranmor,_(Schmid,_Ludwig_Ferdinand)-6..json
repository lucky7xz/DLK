{"textgrid.poem.35305": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Engel des Lichts! Hast du es so gewollt,", "tokens": ["En\u00b7gel", "des", "Lichts", "!", "Hast", "du", "es", "so", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "VAFIN", "PPER", "PPER", "ADV", "VMPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df der Orkan uns nicht die Masten splittre,", "tokens": ["Da\u00df", "der", "Or\u00b7kan", "uns", "nicht", "die", "Mas\u00b7ten", "splitt\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df jetzt des Mondes Glanz herniederzittre,", "tokens": ["Da\u00df", "jetzt", "des", "Mon\u00b7des", "Glanz", "her\u00b7nie\u00b7der\u00b7zitt\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Zeichen, da\u00df Jehovah nicht mehr grollt?", "tokens": ["Zum", "Zei\u00b7chen", ",", "da\u00df", "Je\u00b7ho\u00b7vah", "nicht", "mehr", "grollt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "NE", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schickst du mir solche Gr\u00fc\u00dfe und Symbole?", "tokens": ["Schickst", "du", "mir", "sol\u00b7che", "Gr\u00fc\u00b7\