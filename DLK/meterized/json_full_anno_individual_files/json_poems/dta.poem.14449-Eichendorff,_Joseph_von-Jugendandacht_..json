{"dta.poem.14449": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Dann ist's als ob es d\u00fcrstend Schmerzen sauge", "tokens": ["Dann", "ist's", "als", "ob", "es", "d\u00fcrs\u00b7tend", "Schmer\u00b7zen", "sau\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "KOKOM", "KOUS", "PPER", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus lang vergess'ner Zeit Erinnerungen,", "tokens": ["Aus", "lang", "ver\u00b7ge\u00b7ss'\u00b7ner", "Zeit", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und kann sich r\u00fchren nicht, von Frost bezwungen.", "tokens": ["Und", "kann", "sich", "r\u00fch\u00b7ren", "nicht", ",", "von", "Frost", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVFIN", "PTKNEG", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}