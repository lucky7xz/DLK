{"dta.poem.20595": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Bettler .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Her Jesis, der Friedli, mi Friedli isch do!", "tokens": ["Her", "Je\u00b7sis", ",", "der", "Fried\u00b7li", ",", "mi", "Fried\u00b7li", "isch", "do", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Gottwilche, Gottwilche, wohl chenni di no!", "tokens": ["Gott\u00b7wil\u00b7che", ",", "Gott\u00b7wil\u00b7che", ",", "wohl", "chen\u00b7ni", "di", "no", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "FM", "FM", "FM", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl het mi bigleitet di liebligi Gstalt,", "tokens": ["Wohl", "het", "mi", "big\u00b7lei\u00b7tet", "di", "lieb\u00b7li\u00b7gi", "Gstalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "uf duftige Matten im schattige Wald.", "tokens": ["uf", "duf\u00b7ti\u00b7ge", "Mat\u00b7ten", "im", "schat\u00b7ti\u00b7ge", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wohl het di bigleitet mi b\u2019ch\u00fcmmeret Herz", "tokens": ["Wohl", "het", "di", "big\u00b7lei\u00b7tet", "mi", "b'\u00b7ch\u00fcm\u00b7me\u00b7ret", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "VVFIN", "NE", "VVFIN", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "dur Schwerdter und Chugle mit Hofnig", "tokens": ["dur", "Schwerd\u00b7ter", "und", "Chug\u00b7le", "mit", "Hof\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "und Schmerz,", "tokens": ["und", "Schmerz", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "und briegget und bettet. Gott het mer will-", "tokens": ["und", "brieg\u00b7get", "und", "bet\u00b7tet", ".", "Gott", "het", "mer", "will"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$.", "NN", "VAFIN", "ADV", "TRUNC"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "fahrt,", "tokens": ["fahrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "O", "measure": "unknown.measure.zero"}, "line.10": {"text": "und het mer mi Friedli und het mer en gspart.", "tokens": ["und", "het", "mer", "mi", "Fried\u00b7li", "und", "het", "mer", "en", "gs\u00b7part", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NE", "NE", "KON", "VVFIN", "ADV", "FM", "FM", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Wie chlopfts mer im Buese, wie bini so froh!", "tokens": ["Wie", "chlopfts", "mer", "im", "Bue\u00b7se", ",", "wie", "bi\u00b7ni", "so", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "APPRART", "NN", "$,", "PWAV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "O Muetter, chumm weidli, mi Fridli isch do!", "tokens": ["O", "Mu\u00b7et\u00b7ter", ",", "chumm", "weid\u00b7li", ",", "mi", "Frid\u00b7li", "isch", "do", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}