{"textgrid.poem.28730": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mammosa hat den Berg Parnassus auff der Brust;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mammosa hat den Berg Parnassus auff der Brust;", "tokens": ["Mam\u00b7mo\u00b7sa", "hat", "den", "Berg", "Par\u00b7nas\u00b7sus", "auff", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann dem, der drauff entschlieff, ist mehr als mir bewust,", "tokens": ["Dann", "dem", ",", "der", "drauff", "ent\u00b7schlieff", ",", "ist", "mehr", "als", "mir", "be\u00b7wust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PAV", "VVFIN", "$,", "VAFIN", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann ihm vielleicht getr\u00e4umt, was ihm f\u00fcr Regung kam,", "tokens": ["Wann", "ihm", "viel\u00b7leicht", "ge\u00b7tr\u00e4umt", ",", "was", "ihm", "f\u00fcr", "Re\u00b7gung", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch ob er seinen Weg zum Ritter-Bronnen nam,", "tokens": ["Auch", "ob", "er", "sei\u00b7nen", "Weg", "zum", "Rit\u00b7ter\u00b7Bron\u00b7nen", "nam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was er da gew\u00fcrckt. Mich d\u00fcnckt, er hat geticht,", "tokens": ["Und", "was", "er", "da", "ge\u00b7w\u00fcrckt", ".", "Mich", "d\u00fcnckt", ",", "er", "hat", "ge\u00b7ticht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was auff drey Viertel-Jahr erst kummen wird ans Licht.", "tokens": ["Was", "auff", "drey", "Vier\u00b7tel\u00b7Jahr", "erst", "kum\u00b7men", "wird", "ans", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "CARD", "NN", "ADV", "VVINF", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mammosa hat den Berg Parnassus auff der Brust;", "tokens": ["Mam\u00b7mo\u00b7sa", "hat", "den", "Berg", "Par\u00b7nas\u00b7sus", "auff", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann dem, der drauff entschlieff, ist mehr als mir bewust,", "tokens": ["Dann", "dem", ",", "der", "drauff", "ent\u00b7schlieff", ",", "ist", "mehr", "als", "mir", "be\u00b7wust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PAV", "VVFIN", "$,", "VAFIN", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text":