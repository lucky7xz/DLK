{"dta.poem.6834": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man irret sich hierinnen nicht,", "tokens": ["Man", "ir\u00b7ret", "sich", "hie\u00b7rin\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PAV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da der Verstand selbst \u00fcberzeuglich spricht:", "tokens": ["Da", "der", "Ver\u00b7stand", "selbst", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das, was uns f\u00fchlen macht, durchaus nicht einerley", "tokens": ["Das", ",", "was", "uns", "f\u00fch\u00b7len", "macht", ",", "durc\u00b7haus", "nicht", "ei\u00b7ner\u00b7ley"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$,", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit den so zarten Geistern sey,", "tokens": ["Mit", "den", "so", "zar\u00b7ten", "Geis\u00b7tern", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df die heisse Feuchtigkeit,", "tokens": ["Und", "da\u00df", "die", "heis\u00b7se", "Feuch\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die in den Nerven rinnt,", "tokens": ["Die", "in", "den", "Ner\u00b7ven", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jm Hertzen kocht und wallt, sich im Gehirn verd\u00fcnnt,", "tokens": ["Jm", "Hert\u00b7zen", "kocht", "und", "wallt", ",", "sich", "im", "Ge\u00b7hirn", "ver\u00b7d\u00fcnnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gantz anderer Beschaffenheit.", "tokens": ["Gantz", "an\u00b7de\u00b7rer", "Be\u00b7schaf\u00b7fen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}