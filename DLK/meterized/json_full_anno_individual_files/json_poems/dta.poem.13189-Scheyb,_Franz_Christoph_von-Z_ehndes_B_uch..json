{"dta.poem.13189": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Wunder-Thron der Welt! Saphir, Schmaragd und Gold,", "tokens": ["O", "Wun\u00b7der\u00b7Thron", "der", "Welt", "!", "Sa\u00b7phir", ",", "Schma\u00b7ragd", "und", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$.", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ja was man aus dem Marck der Erde kostbars hohlt,", "tokens": ["Ja", "was", "man", "aus", "dem", "Marck", "der", "Er\u00b7de", "kost\u00b7bars", "hohlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "PIS", "APPR", "ART", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umschimmerte den Raum mit solchem Glanz und Blizen,", "tokens": ["Um\u00b7schim\u00b7mer\u00b7te", "den", "Raum", "mit", "sol\u00b7chem", "Glanz", "und", "Bli\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ob man sie schon s\u00e4h in jenem Tempel sizen,", "tokens": ["Als", "ob", "man", "sie", "schon", "s\u00e4h", "in", "je\u00b7nem", "Tem\u00b7pel", "si\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "PPER", "ADV", "ADJD", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "65Den meine F\u00fchrerinn so gro\u00df und hell beschrieb,", "tokens": ["mei\u00b7ne", "F\u00fch\u00b7re\u00b7rinn", "so", "gro\u00df", "und", "hell", "be\u00b7schrieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Da\u00df aller K\u00fcnste Macht darob erstaunet blieb.", "tokens": ["Da\u00df", "al\u00b7ler", "K\u00fcns\u00b7te", "Macht", "da\u00b7rob", "er\u00b7stau\u00b7net", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "PAV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man sah die Klarheit nur aus diesen ", "tokens": ["Man", "sah", "die", "Klar\u00b7heit", "nur", "aus", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und alles in dem Saal durch ihren Glanz erhellen.", "tokens": ["Und", "al\u00b7les", "in", "dem", "Saal", "durch", "ih\u00b7ren", "Glanz", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So zierte diesen Thron nur fremder Gegenschein,", "tokens": ["So", "zier\u00b7te", "die\u00b7sen", "Thron", "nur", "frem\u00b7der", "Ge\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "70Es traf der Schimmer erst, nachdem sie sassen, ein.", "tokens": ["traf", "der", "Schim\u00b7mer", "erst", ",", "nach\u00b7dem", "sie", "sas\u00b7sen", ",", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Das Ansehn, die Gestallt, das Antliz war so pr\u00e4chtig;", "tokens": ["Das", "An\u00b7sehn", ",", "die", "Ge\u00b7stallt", ",", "das", "Ant\u00b7liz", "war", "so", "pr\u00e4ch\u00b7tig", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Freundlichkeit und Lieb und Majest\u00e4t so m\u00e4chtig;", "tokens": ["Die", "Freund\u00b7lich\u00b7keit", "und", "Lieb", "und", "Ma\u00b7jes\u00b7t\u00e4t", "so", "m\u00e4ch\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als ruhte, was die Welt zur Pracht der F\u00fcrsten spahrt,", "tokens": ["Als", "ruh\u00b7te", ",", "was", "die", "Welt", "zur", "Pracht", "der", "F\u00fcrs\u00b7ten", "spahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Allein auf diesem Thron, auf dieser Gegenwart.", "tokens": ["Al\u00b7lein", "auf", "die\u00b7sem", "Thron", ",", "auf", "die\u00b7ser", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "75Gleichwie sie Stuffen-wei\u00df desselben Raum bestiegen,", "tokens": ["sie", "Stuf\u00b7fen\u00b7wei\u00df", "des\u00b7sel\u00b7ben", "Raum", "be\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "So sah man Licht und Pracht sich erst zusammen f\u00fcgen.", "tokens": ["So", "sah", "man", "Licht", "und", "Pracht", "sich", "erst", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da ward er erst begl\u00e4nzt, so wunderbar geschm\u00fcckt,", "tokens": ["Da", "ward", "er", "erst", "be\u00b7gl\u00e4nzt", ",", "so", "wun\u00b7der\u00b7bar", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df sich der Augen Macht verblendet und entz\u00fcckt.", "tokens": ["Da\u00df", "sich", "der", "Au\u00b7gen", "Macht", "ver\u00b7blen\u00b7det", "und", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein feindliches Gem\u00fcth m\u00fc\u00dft sich in Furcht versencken;", "tokens": ["Ein", "feind\u00b7li\u00b7ches", "Ge\u00b7m\u00fcth", "m\u00fc\u00dft", "sich", "in", "Furcht", "ver\u00b7sen\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "80Ein Freund hingegen nur auf Gl\u00fcck und Trost gedencken.", "tokens": ["Freund", "hin\u00b7ge\u00b7gen", "nur", "auf", "Gl\u00fcck", "und", "Trost", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Die Mi\u00dfgunst h\u00e4tte sich dadurch besch\u00e4mt gezeigt;", "tokens": ["Die", "Mi\u00df\u00b7gunst", "h\u00e4t\u00b7te", "sich", "da\u00b7durch", "be\u00b7sch\u00e4mt", "ge\u00b7zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "PAV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Bo\u00dfheit ihren Trieb der Tadelsucht geneigt.", "tokens": ["Die", "Bo\u00df\u00b7heit", "ih\u00b7ren", "Trieb", "der", "Ta\u00b7del\u00b7sucht", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Geist, Seele, Sinn und Herz war durch den Pomp getroffen;", "tokens": ["Geist", ",", "See\u00b7le", ",", "Sinn", "und", "Herz", "war", "durch", "den", "Pomp", "ge\u00b7trof\u00b7fen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da sah man Th\u00fcr und Thor zu aller Wohlfart offen.", "tokens": ["Da", "sah", "man", "Th\u00fcr", "und", "Thor", "zu", "al\u00b7ler", "Wohl\u00b7fart", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}