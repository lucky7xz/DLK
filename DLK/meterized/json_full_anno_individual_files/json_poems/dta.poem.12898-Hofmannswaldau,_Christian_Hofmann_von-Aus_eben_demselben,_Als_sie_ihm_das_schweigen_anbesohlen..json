{"dta.poem.12898": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus eben demselben,  \n Als sie ihm das schweigen anbesohlen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast mir, Sylvia! das schweigen auferleget;", "tokens": ["Du", "hast", "mir", ",", "Syl\u00b7via", "!", "das", "schwei\u00b7gen", "auf\u00b7er\u00b7le\u00b7get", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NE", "$.", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wohlan! ich will gehorsam seyn:", "tokens": ["Wo\u00b7hlan", "!", "ich", "will", "ge\u00b7hor\u00b7sam", "seyn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Obgleich die hefftigkeit der tief-gesetzten pein,", "tokens": ["Ob\u00b7gleich", "die", "heff\u00b7tig\u00b7keit", "der", "tie\u00b7fge\u00b7setz\u00b7ten", "pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So zung\u2019 als hertze reget.", "tokens": ["So", "zung'", "als", "hert\u00b7ze", "re\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch, Sylvia! steht dir mein tod nicht an,", "tokens": ["Doch", ",", "Syl\u00b7via", "!", "steht", "dir", "mein", "tod", "nicht", "an", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du ursprung meiner plagen!", "tokens": ["Du", "ur\u00b7sprung", "mei\u00b7ner", "pla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So la\u00df, damit ich doch mein leben retten kan,", "tokens": ["So", "la\u00df", ",", "da\u00b7mit", "ich", "doch", "mein", "le\u00b7ben", "ret\u00b7ten", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich nur ein wort zu meinem artzte sagen.", "tokens": ["Mich", "nur", "ein", "wort", "zu", "mei\u00b7nem", "artz\u00b7te", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}