{"textgrid.poem.38246": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schau gut Gesell, was f\u00fchr ich allhier,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau gut Gesell, was f\u00fchr ich allhier,", "tokens": ["Schau", "gut", "Ge\u00b7sell", ",", "was", "f\u00fchr", "ich", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schau, was f\u00fchr ich allhier,", "tokens": ["Schau", ",", "was", "f\u00fchr", "ich", "all\u00b7hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein fein braun M\u00e4gdlein,", "tokens": ["Ein", "fein", "braun", "M\u00e4gd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Guter Ding nach meinem Begier,", "tokens": ["Gu\u00b7ter", "Ding", "nach", "mei\u00b7nem", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wir wollen fein miteinander seyn.", "tokens": ["Wir", "wol\u00b7len", "fein", "mi\u00b7tein\u00b7an\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Drum sing mir bald ein kleines T\u00e4nzlein,