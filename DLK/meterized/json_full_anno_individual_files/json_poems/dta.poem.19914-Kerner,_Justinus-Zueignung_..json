{"dta.poem.19914": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herz! gedenkst du noch der Stelle,", "tokens": ["Herz", "!", "ge\u00b7denkst", "du", "noch", "der", "Stel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo einst unser Fr\u00fchling war,", "tokens": ["Wo", "einst", "un\u00b7ser", "Fr\u00fch\u00b7ling", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines ", "tokens": ["Dei\u00b7nes"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Der verlassenen Kapelle,", "tokens": ["Der", "ver\u00b7las\u00b7se\u00b7nen", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jenes Himmels wunderklar?", "tokens": ["Je\u00b7nes", "Him\u00b7mels", "wun\u00b7der\u00b7klar", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! es war ein kurzes Tr\u00e4umen,", "tokens": ["Ach", "!", "es", "war", "ein", "kur\u00b7zes", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schmerz der Trennung lange Jahr!", "tokens": ["Schmerz", "der", "Tren\u00b7nung", "lan\u00b7ge", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herz vom Herzen weggerissen,", "tokens": ["Herz", "vom", "Her\u00b7zen", "weg\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelnd in der Fremde bang,", "tokens": ["Wan\u00b7delnd", "in", "der", "Frem\u00b7de", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward dein ", "tokens": ["Ward", "dein"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Meiner in den Finsternissen,", "tokens": ["Mei\u00b7ner", "in", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Liebe, mein Gesang,", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", ",", "mein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So der Welt ward keins zum Raube,", "tokens": ["So", "der", "Welt", "ward", "keins", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIAT", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Bis ich g\u00e4nzlich dich errang.", "tokens": ["Bis", "ich", "g\u00e4nz\u00b7lich", "dich", "er\u00b7rang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jezt, was kaum ich sah in Tr\u00e4umen,", "tokens": ["Jezt", ",", "was", "kaum", "ich", "sah", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bildete sich wirklich aus:", "tokens": ["Bil\u00b7de\u00b7te", "sich", "wirk\u00b7lich", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Berg der Frauentreue", "tokens": ["An", "dem", "Berg", "der", "Frau\u00b7en\u00b7treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehet unter gr\u00fcnen B\u00e4umen", "tokens": ["Ste\u00b7het", "un\u00b7ter", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freundlich unser kleines Haus", "tokens": ["Freund\u00b7lich", "un\u00b7ser", "klei\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und geliebter Kinder dreye", "tokens": ["Und", "ge\u00b7lieb\u00b7ter", "Kin\u00b7der", "drey\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00fcpfen fr\u00f6hlich ein und aus.", "tokens": ["H\u00fcp\u00b7fen", "fr\u00f6h\u00b7lich", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dahin sind Schmerz und Sehnen,", "tokens": ["Und", "da\u00b7hin", "sind", "Schmerz", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die in mir das Lied erregt,", "tokens": ["Die", "in", "mir", "das", "Lied", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch das scherzende, \u2014 entsprungen", "tokens": ["Auch", "das", "scher\u00b7zen\u00b7de", ",", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "$(", "VVINF"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Ist auch di\u00df nur stillen Thr\u00e4nen,", "tokens": ["Ist", "auch", "di\u00df", "nur", "stil\u00b7len", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur dem Gram, der mich bewegt.", "tokens": ["Nur", "dem", "Gram", ",", "der", "mich", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herz! \u2014 und ich hab' ausgesungen,", "tokens": ["Herz", "!", "und", "ich", "hab'", "aus\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil du allen Schmerz gelegt.", "tokens": ["Weil", "du", "al\u00b7len", "Schmerz", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}