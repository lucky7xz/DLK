ord", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich mit ihm fall' und stehe.", "tokens": ["Da\u00df", "ich", "mit", "ihm", "fall'", "und", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wird je der Beruf des Sch\u00f6nen", "tokens": ["Wird", "je", "der", "Be\u00b7ruf", "des", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bu\u00dfe predigen statt schildern", "tokens": ["Bu\u00b7\u00dfe", "pre\u00b7di\u00b7gen", "statt", "schil\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zerrei\u00dfen statt vers\u00f6hnen", "tokens": ["Und", "zer\u00b7rei\u00b7\u00dfen", "statt", "ver\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verwildern statt zu mildern,", "tokens": ["Und", "ver\u00b7wil\u00b7dern", "statt", "zu", "mil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Statt zu singen dumpf zu winseln,", "tokens": ["Statt", "zu", "sin\u00b7gen", "dumpf", "zu", "win\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Statt zu malen grell zu pinseln;", "tokens": ["Statt", "zu", "ma\u00b7len", "grell", "zu", "pin\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Siegt das Abenteuerliche", "tokens": ["Siegt", "das", "A\u00b7bent\u00b7eu\u00b7er\u00b7li\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber das Geb\u00fchrliche", "tokens": ["\u00dc\u00b7ber", "das", "Ge\u00b7b\u00fchr\u00b7li\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und das Ungeheuerliche", "tokens": ["Und", "das", "Un\u00b7ge\u00b7heu\u00b7er\u00b7li\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber das Nat\u00fcrliche:", "tokens": ["\u00dc\u00b7ber", "das", "Na\u00b7t\u00fcr\u00b7li\u00b7che", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Dann wird Goethe nicht mehr sein,", "tokens": ["Dann", "wird", "Goe\u00b7the", "nicht", "mehr", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wir andern gehn mit drein.", "tokens": ["Und", "wir", "an\u00b7dern", "gehn", "mit", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "VVFIN", "APPR", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}