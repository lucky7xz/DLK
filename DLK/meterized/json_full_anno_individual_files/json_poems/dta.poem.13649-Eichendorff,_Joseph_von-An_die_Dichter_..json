{"dta.poem.13649": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "da:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da soll er singen frei auf Erden,", "tokens": ["Da", "soll", "er", "sin\u00b7gen", "frei", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Lust und Noth auf Gott vertrau'n,", "tokens": ["In", "Lust", "und", "Noth", "auf", "Gott", "ver\u00b7trau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Aller Herzen freier werden,", "tokens": ["Da\u00df", "Al\u00b7ler", "Her\u00b7zen", "frei\u00b7er", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erathmend in die Kl\u00e4nge schau'n.", "tokens": ["E\u00b7rath\u00b7mend", "in", "die", "Kl\u00e4n\u00b7ge", "schau'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}