{"textgrid.poem.51178": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit ihr die Berge versetzet mit archimedischen Kr\u00e4ften,", "genre": "verse", "period": "N.A.", "pub_year": 1880, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ihr die Berge versetzet mit archimedischen Kr\u00e4ften,", "tokens": ["Seit", "ihr", "die", "Ber\u00b7ge", "ver\u00b7set\u00b7zet", "mit", "ar\u00b7chi\u00b7me\u00b7di\u00b7schen", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "F\u00fcrcht ich, den Hebel entf\u00fchrt euch ein d\u00e4monisch Geschlecht!", "tokens": ["F\u00fcrcht", "ich", ",", "den", "He\u00b7bel", "ent\u00b7f\u00fchrt", "euch", "ein", "d\u00e4\u00b7mo\u00b7nisch", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Gleich dem b\u00f6sen Gewissen geht um die verw\u00fcnschte Patrone,", "tokens": ["Gleich", "dem", "b\u00f6\u00b7sen", "Ge\u00b7wis\u00b7sen", "geht", "um", "die", "ver\u00b7w\u00fcnschte", "Pat\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Jegliches B\u00fcbchen verbirgt schielend den Greuel im Sack.", "tokens": ["Jeg\u00b7li\u00b7ches", "B\u00fcb\u00b7chen", "ver\u00b7birgt", "schie\u00b7lend", "den", "Greu\u00b7el", "im",