{"textgrid.poem.38490": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ey der Tausend", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df auf einem Birnenbaum,", "tokens": ["Ich", "sa\u00df", "auf", "ei\u00b7nem", "Bir\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt gelbe R\u00fcben graben,", "tokens": ["Wollt", "gel\u00b7be", "R\u00fc\u00b7ben", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da kam derselbe Bauersmann,", "tokens": ["Da", "kam", "der\u00b7sel\u00b7be", "Bau\u00b7ers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem diese Zwiebeln waren!", "tokens": ["Dem", "die\u00b7se", "Zwie\u00b7beln", "wa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, ach du Schelm, ach, ach du Dieb!", "tokens": ["Ach", ",", "ach", "du", "Schelm", ",", "ach", ",", "ach", "du", "Dieb", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "XY", "PPER", "NN", "$,", "ITJ", "$,", "XY", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was machst du in den N\u00fcssen?", "tokens": ["Was", "machst", "du", "in", "den", "N\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So hatt' ich all mein Lebetag", "tokens": ["So", "hatt'", "ich", "all", "mein", "Le\u00b7be\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein be\u00dfre Pflaumen gessen.", "tokens": ["Kein", "be\u00df\u00b7re", "Pflau\u00b7men", "ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Esel hat Pantoffeln an,", "tokens": ["Der", "E\u00b7sel", "hat", "Pan\u00b7tof\u00b7feln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam \u00fcbers Dach geflogen,", "tokens": ["Kam", "\u00fc\u00b7bers", "Dach", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach, ach, ich armes M\u00e4delein,", "tokens": ["Ach", ",", "ach", ",", "ich", "ar\u00b7mes", "M\u00e4\u00b7de\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie bin ich doch betrogen.", "tokens": ["Wie", "bin", "ich", "doch", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sa\u00