{"textgrid.poem.36715": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nie ist die G\u00f6ttin geworden, von Anfang ist sie, vollkommen", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie ist die G\u00f6ttin geworden, von Anfang ist sie, vollkommen", "tokens": ["Nie", "ist", "die", "G\u00f6t\u00b7tin", "ge\u00b7wor\u00b7den", ",", "von", "An\u00b7fang", "ist", "sie", ",", "voll\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VAPP", "$,", "APPR", "NN", "VAFIN", "PPER", "$,", "ADJD"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Stieg sie der Welt aus des Meers rauschenden Wassern empor.", "tokens": ["Stieg", "sie", "der", "Welt", "aus", "des", "Meers", "rau\u00b7schen\u00b7den", "Was\u00b7sern", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "In der fl\u00fccht'gen Natur ist sie die dauernde Seele,", "tokens": ["In", "der", "fl\u00fccht'\u00b7gen", "Na\u00b7tur", "ist", "sie", "die", "dau\u00b7ern\u00b7de", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und im Wechsel der Form ist sie das ew'ge Gesetz,", "tokens": ["Und", "im", "Wech\u00b7sel", "der", "Form", "ist", "sie", "das", "ew'\u00b7ge", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Unter sichtbar Gemischtem die tief unsichtbare Einheit,", "tokens": ["Un\u00b7ter", "sicht\u00b7bar", "Ge\u00b7mischtem", "die", "tief", "un\u00b7sicht\u00b7ba\u00b7re", "Ein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Unter dem Einzelnen ruht bleibend als Ganzes sie fest.", "tokens": ["Un\u00b7ter", "dem", "Ein\u00b7zel\u00b7nen", "ruht", "blei\u00b7bend", "als", "Gan\u00b7zes", "sie", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "KOKOM", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Und als vollkommne Idee gereifter dauernder Sch\u00f6nheit", "tokens": ["Und", "als", "voll\u00b7komm\u00b7ne", "I\u00b7dee", "ge\u00b7reif\u00b7ter", "dau\u00b7ern\u00b7der", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Zeigt sie dem Sinn nicht, dem Geist nur die olympische Macht.", "tokens": ["Zeigt", "sie", "dem", "Sinn", "nicht", ",", "dem", "Geist", "nur", "die", "o\u00b7lym\u00b7pi\u00b7sche", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}},