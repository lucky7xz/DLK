{"dta.poem.2661": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber dem fr\u00fchzeitigen Absterben  \n  Hn. G. T. j\u00fcngsten T\u00f6chterleins 24. Ju-  \n nii 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er hofft/ Hochwerther Freund/ ein Lied von meinen\nH\u00e4nden/", "tokens": ["Er", "hofft", "/", "Hoch\u00b7wert\u00b7her", "Freund", "/", "ein", "Lied", "von", "mei\u00b7nen", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das seine Thr\u00e4nen stillt/ und seinen Schmertz besiegt:", "tokens": ["Das", "sei\u00b7ne", "Thr\u00e4\u00b7nen", "stillt", "/", "und", "sei\u00b7nen", "Schmertz", "be\u00b7siegt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar wie bereit ich bin was tr\u00f6stliches zu senden/", "tokens": ["Zwar", "wie", "be\u00b7reit", "ich", "bin", "was", "tr\u00f6st\u00b7li\u00b7ches", "zu", "sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "PPER", "VAFIN", "PIS", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "So glaub\u2019 er/ Muth und Geist und alle Krafft erliegt.", "tokens": ["So", "glaub'", "er", "/", "Muth", "und", "Geist", "und", "al\u00b7le", "Krafft", "er\u00b7liegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "KON", "NN", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer kan/ wenn auffder See/ Well/ Sturm und Wetter krachen/", "tokens": ["Wer", "kan", "/", "wenn", "auff\u00b7der", "See", "/", "Well", "/", "Sturm", "und", "Wet\u00b7ter", "kra\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "KOUS", "ADJA", "NN", "$(", "NE", "$(", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nun der Untergang f\u00fcr aller Augen schwebt/", "tokens": ["Und", "nun", "der", "Un\u00b7ter\u00b7gang", "f\u00fcr", "al\u00b7ler", "Au\u00b7gen", "schwebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dem Nachbar/ der best\u00fcrtzt/ ein fr\u00f6lich Hertze machen/", "tokens": ["Dem", "Nach\u00b7bar", "/", "der", "be\u00b7st\u00fcrtzt", "/", "ein", "fr\u00f6\u00b7lich", "Hert\u00b7ze", "ma\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVPP", "$(", "ART", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Indem er Schreckens voll kaum selbst wei\u00df ob er lebt.", "tokens": ["In\u00b7dem", "er", "Schre\u00b7ckens", "voll", "kaum", "selbst", "wei\u00df", "ob", "er", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "ADV", "ADV", "VVFIN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bey hellem Sonnenschein/ und gutem Zustand singen/", "tokens": ["Bey", "hel\u00b7lem", "Son\u00b7nen\u00b7schein", "/", "und", "gu\u00b7tem", "Zu\u00b7stand", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wenn der West-Wind weht/ die Segel spannen auff/", "tokens": ["Und", "wenn", "der", "West\u00b7Wind", "weht", "/", "die", "Se\u00b7gel", "span\u00b7nen", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das ist ein Werck voll Lust; bey Schiffbruch Lieder bringen/", "tokens": ["Das", "ist", "ein", "Werck", "voll", "Lust", ";", "bey", "Schiff\u00b7bruch", "Lie\u00b7der", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "NN", "$.", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist wider die Natur/ und der Gewohnheit Lauff.", "tokens": ["Ist", "wi\u00b7der", "die", "Na\u00b7tur", "/", "und", "der", "Ge\u00b7wohn\u00b7heit", "Lauff", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$(", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es raucht die Asche noch von meines Sohnes Leiche/", "tokens": ["Es", "raucht", "die", "A\u00b7sche", "noch", "von", "mei\u00b7nes", "Soh\u00b7nes", "Lei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Geschweige wie mich hat die Kranckheit zugericht:", "tokens": ["Ge\u00b7schwei\u00b7ge", "wie", "mich", "hat", "die", "Kran\u00b7ck\u00b7heit", "zu\u00b7ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Wo ich nun/ ", "tokens": ["Wo", "ich", "nun", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "So denck\u2019 er: wer stets kranck/ der schreibt kein gut Gedicht.", "tokens": ["So", "denck", "er", ":", "wer", "stets", "kranck", "/", "der", "schreibt", "kein", "gut", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$.", "PWS", "ADV", "ADJD", "$(", "ART", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch wie bey meinem Schmertz er lie\u00df sein Beyleid sp\u00fcren/", "tokens": ["Doch", "wie", "bey", "mei\u00b7nem", "Schmertz", "er", "lie\u00df", "sein", "Bey\u00b7leid", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So locket gleiche Pflicht itzt meine Thr\u00e4nen aus.", "tokens": ["So", "lo\u00b7cket", "glei\u00b7che", "Pflicht", "itzt", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wolan! er folge mir: wo ich ihn hin wil f\u00fchren/", "tokens": ["Wo\u00b7lan", "!", "er", "fol\u00b7ge", "mir", ":", "wo", "ich", "ihn", "hin", "wil", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "$.", "PWAV", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist sonst kein ander Ort als nur sein Garten-Haus.", "tokens": ["Ist", "sonst", "kein", "an\u00b7der", "Ort", "als", "nur", "sein", "Gar\u00b7ten\u00b7Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "ADJD", "NN", "KOKOM", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er hat mit Lust gesehn wie bey den Sommer-Tagen", "tokens": ["Er", "hat", "mit", "Lust", "ge\u00b7sehn", "wie", "bey", "den", "Som\u00b7mer\u00b7Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Chloris bundte Zucht sich hundertf\u00e4rbig weist/", "tokens": ["Der", "Chlo\u00b7ris", "bund\u00b7te", "Zucht", "sich", "hun\u00b7dert\u00b7f\u00e4r\u00b7big", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In welcher Herrligkeit den hohen Purpur tragen", "tokens": ["In", "wel\u00b7cher", "Herr\u00b7lig\u00b7keit", "den", "ho\u00b7hen", "Pur\u00b7pur", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Rosen/ derer Kleid gleich g\u00f6ldnen Sternen gleist.", "tokens": ["Die", "Ro\u00b7sen", "/", "de\u00b7rer", "Kleid", "gleich", "g\u00f6ld\u00b7nen", "Ster\u00b7nen", "gleist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es hat sein Hertz erfreut/ wenn aus dem Scharlach-Bette", "tokens": ["Es", "hat", "sein", "Hertz", "er\u00b7freut", "/", "wenn", "aus", "dem", "Scha\u00b7rlach\u00b7Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Aurora sich gemacht/ und ihrer Perlen Na\u00df", "tokens": ["Au\u00b7ro\u00b7ra", "sich", "ge\u00b7macht", "/", "und", "ih\u00b7rer", "Per\u00b7len", "Na\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PRF", "VVPP", "$(", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den Bl\u00e4ttern auffgepfropfft/ wenn gleichsam in die Wette", "tokens": ["Den", "Bl\u00e4t\u00b7tern", "auff\u00b7ge\u00b7pfropfft", "/", "wenn", "gleich\u00b7sam", "in", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$(", "KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ein ied\u2019 als K\u00f6nigin auff ihrem Throne sa\u00df.", "tokens": ["Ein", "ied'", "als", "K\u00f6\u00b7ni\u00b7gin", "auff", "ih\u00b7rem", "Thro\u00b7ne", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die stand in voller Knosp\u2019 als wie bewacht/ verschlossen/", "tokens": ["Die", "stand", "in", "vol\u00b7ler", "Knosp'", "als", "wie", "be\u00b7wacht", "/", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "KOUS", "KOKOM", "VVPP", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein andre breite schon den rothen Mantel aus/", "tokens": ["Ein", "and\u00b7re", "brei\u00b7te", "schon", "den", "ro\u00b7then", "Man\u00b7tel", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die hing/ als wie gebadt/ von Morgen-Thau begossen/", "tokens": ["Die", "hing", "/", "als", "wie", "ge\u00b7badt", "/", "von", "Mor\u00b7gen\u00b7\u00b7T\u00b7hau", "be\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "KOKOM", "VVPP", "$(", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Und jener pr\u00e4chtig Haupt stieg an der Sternen Haus.", "tokens": ["Und", "je\u00b7ner", "pr\u00e4ch\u00b7tig", "Haupt", "stieg", "an", "der", "Ster\u00b7nen", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So viel nun Liebligkeit und s\u00fcsse Zimmet-Winde", "tokens": ["So", "viel", "nun", "Lieb\u00b7lig\u00b7keit", "und", "s\u00fcs\u00b7se", "Zim\u00b7met\u00b7Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Rosen fruchtbar Stock ihm reichlich hat gewehrt/", "tokens": ["Der", "Ro\u00b7sen", "frucht\u00b7bar", "Stock", "ihm", "reich\u00b7lich", "hat", "ge\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "PPER", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So wust er doch dabey da\u00df Blume/ Stock und Rinde", "tokens": ["So", "wust", "er", "doch", "da\u00b7bey", "da\u00df", "Blu\u00b7me", "/", "Stock", "und", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "KOUS", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Des heissen Sommers Gluth im Mittag offt verheert.", "tokens": ["Des", "heis\u00b7sen", "Som\u00b7mers", "Gluth", "im", "Mit\u00b7tag", "offt", "ver\u00b7heert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die stirbt in der Geburt eh\u2019 sie die Sonn erblicket/", "tokens": ["Die", "stirbt", "in", "der", "Ge\u00b7burt", "eh'", "sie", "die", "Sonn", "er\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein andre wenn sie kaum die gr\u00fcnen Windeln bricht/", "tokens": ["Ein", "and\u00b7re", "wenn", "sie", "kaum", "die", "gr\u00fc\u00b7nen", "Win\u00b7deln", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die hat ein scharffer Wind/ wenn sie schon voll/ zerknicket/", "tokens": ["Die", "hat", "ein", "scharf\u00b7fer", "Wind", "/", "wenn", "sie", "schon", "voll", "/", "zer\u00b7kni\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "ADV", "ADJD", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die wirfft die Bl\u00e4tter weg/ wenn sie die Sonne sticht.", "tokens": ["Die", "wirfft", "die", "Bl\u00e4t\u00b7ter", "weg", "/", "wenn", "sie", "die", "Son\u00b7ne", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$(", "KOUS", "PPER", "ART",