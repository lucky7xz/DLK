{"dta.poem.18456": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen l\u00e4cherlichen fall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Marcolph ging sehr berauscht die f\u00fcsse zu bedecken/", "tokens": ["Mar\u00b7col\u00b7ph", "ging", "sehr", "be\u00b7rauscht", "die", "f\u00fcs\u00b7se", "zu", "be\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Fiel aber in den koth/ weil ihm der kopf zu schwer:", "tokens": ["Fiel", "a\u00b7ber", "in", "den", "koth", "/", "weil", "ihm", "der", "kopf", "zu", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sau roch den geruch/ lies ihr den braten schmecken/", "tokens": ["Die", "sau", "roch", "den", "ge\u00b7ruch", "/", "lies", "ihr", "den", "bra\u00b7ten", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "$(", "VVFIN", "PPER", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und suchte mehr confect/ kam endlich ohngefehr", "tokens": ["Und", "such\u00b7te", "mehr", "con\u00b7fect", "/", "kam", "end\u00b7lich", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$(", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf des Marcolphen maul mit ihrem rauchen r\u00fc\u00dfel/", "tokens": ["Auf", "des", "Mar\u00b7col\u00b7phen", "maul", "mit", "ih\u00b7rem", "rau\u00b7chen", "r\u00fc\u00b7\u00dfel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und zwar was ungelind: Marcolph vermeint im traum/", "tokens": ["Und", "zwar", "was", "un\u00b7ge\u00b7lind", ":", "Mar\u00b7col\u00b7ph", "ver\u00b7meint", "im", "traum", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "ADJD", "$.", "NE", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Er hielte dem barbier/ der ihn sonst putzt/ die sch\u00fcssel/", "tokens": ["Er", "hiel\u00b7te", "dem", "bar\u00b7bier", "/", "der", "ihn", "sonst", "putzt", "/", "die", "sch\u00fcs\u00b7sel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Drum sprach er/ unterm bart/ so da\u00df mans h\u00f6rte kaum/", "tokens": ["Drum", "sprach", "er", "/", "un\u00b7term", "bart", "/", "so", "da\u00df", "mans", "h\u00f6r\u00b7te", "kaum", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "APPRART", "NN", "$(", "ADV", "KOUS", "PIS", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Herr putzet nicht so hart/ streicht doch das messer besser/", "tokens": ["Herr", "put\u00b7zet", "nicht", "so", "hart", "/", "streicht", "doch", "das", "mes\u00b7ser", "bes\u00b7ser", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "$(", "VVFIN", "ADV", "ART", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Potz velten! Putzt mich nicht mit solchem stumpfen messer.", "tokens": ["Potz", "vel\u00b7ten", "!", "Putzt", "mich", "nicht", "mit", "sol\u00b7chem", "stump\u00b7fen", "mes\u00b7ser", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "VVFIN", "PPER", "PTKNEG", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}