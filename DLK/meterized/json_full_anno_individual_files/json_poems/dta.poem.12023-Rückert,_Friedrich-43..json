{"dta.poem.12023": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df uns im Augenblick ein Gottesbild aufrichten,", "tokens": ["La\u00df", "uns", "im", "Au\u00b7gen\u00b7blick", "ein", "Got\u00b7tes\u00b7bild", "auf\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um es im Augenblick im n\u00e4chsten zu vernichten.", "tokens": ["Um", "es", "im", "Au\u00b7gen\u00b7blick", "im", "n\u00e4chs\u00b7ten", "zu", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPRART", "NN", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn jedes Bild ist falsch, das bleiben will und dauern,", "tokens": ["Denn", "je\u00b7des", "Bild", "ist", "falsch", ",", "das", "blei\u00b7ben", "will", "und", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$,", "PDS", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jedes wahr, das hin vorm Urbild sinkt mit Schauern.", "tokens": ["Und", "je\u00b7des", "wahr", ",", "das", "hin", "vorm", "Ur\u00b7bild", "sinkt", "mit", "Schau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dort seh' ich aufgethan den ew'gen Vaterscho\u00df,", "tokens": ["Dort", "seh'", "ich", "auf\u00b7ge\u00b7than", "den", "ew'\u00b7gen", "Va\u00b7ter\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem alles gr\u00f6ste klein und kleinstes auch ist gro\u00df.", "tokens": ["Dem", "al\u00b7les", "gr\u00f6s\u00b7te", "klein", "und", "kleins\u00b7tes", "auch", "ist", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "ADJD", "KON", "ADJA", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sieh, wie im Menschengeist geordnete Gedanken,", "tokens": ["Sieh", ",", "wie", "im", "Men\u00b7schen\u00b7geist", "ge\u00b7ord\u00b7ne\u00b7te", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kreisen Welten dort in selbstgesetzten Schranken.", "tokens": ["So", "krei\u00b7sen", "Wel\u00b7ten", "dort", "in", "selbst\u00b7ge\u00b7setz\u00b7ten", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein All Unz\u00e4hliger, von denen jed's ein All,", "tokens": ["Ein", "All", "Un\u00b7z\u00e4h\u00b7li\u00b7ger", ",", "von", "de\u00b7nen", "je\u00b7d's", "ein", "All", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "APPR", "PRELS", "PIS", "ART", "NN", "$,"], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Punkt im Ganzen ist, in sich ein Lebensball.", "tokens": ["Ein", "Punkt", "im", "Gan\u00b7zen", "ist", ",", "in", "sich", "ein", "Le\u00b7bens\u00b7ball", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "$,", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Alle, wie sie rings in Rangordnungen schweben,", "tokens": ["Die", "Al\u00b7le", ",", "wie", "sie", "rings", "in", "Ran\u00b7gord\u00b7nun\u00b7gen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entwickeln auch in sich ein ranggeordnet Leben.", "tokens": ["Ent\u00b7wi\u00b7ckeln", "auch", "in", "sich", "ein", "rang\u00b7ge\u00b7ord\u00b7net", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PRF", "ART", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Da ringen \u00fcberall Rangordnungen des Lebens", "tokens": ["Da", "rin\u00b7gen", "\u00fc\u00b7be\u00b7rall", "Ran\u00b7gord\u00b7nun\u00b7gen", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "ART", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In ungehemmtem Trieb des Immeraufwertsstrebens.", "tokens": ["In", "un\u00b7ge\u00b7hemm\u00b7tem", "Trieb", "des", "Im\u00b7mer\u00b7auf\u00b7werts\u00b7stre\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und wo Natur den Geist nun auf als Krone setzt,", "tokens": ["Und", "wo", "Na\u00b7tur", "den", "Geist", "nun", "auf", "als", "Kro\u00b7ne", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "ART", "NN", "ADV", "APPR", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da kehrt das Einzelste zur\u00fcck zum Ganzen jetzt.", "tokens": ["Da", "kehrt", "das", "Ein\u00b7zels\u00b7te", "zu\u00b7r\u00fcck", "zum", "Gan\u00b7zen", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPRART", "NN", "ADV", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.9": {"line.1": {"text": "Du suchst, o Menschengeist, wo auch dein Standpunkt ist,", "tokens": ["Du", "suchst", ",", "o", "Men\u00b7schen\u00b7geist", ",", "wo", "auch", "dein", "Stand\u00b7punkt", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Mittelpunkt, von dem du nirgends ferne bist.", "tokens": ["Den", "Mit\u00b7tel\u00b7punkt", ",", "von", "dem", "du", "nir\u00b7gends", "fer\u00b7ne", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Du f\u00fchlest selbst dich klein, du f\u00fchlest selbst dich gro\u00df,", "tokens": ["Du", "f\u00fch\u00b7lest", "selbst", "dich", "klein", ",", "du", "f\u00fch\u00b7lest", "selbst", "dich", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADJD", "$,", "PPER", "VVFIN", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich mit der ganzen Welt im ew'gen Vaterscho\u00df.", "tokens": ["Dich", "mit", "der", "gan\u00b7zen", "Welt", "im", "ew'\u00b7gen", "Va\u00b7ter\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}