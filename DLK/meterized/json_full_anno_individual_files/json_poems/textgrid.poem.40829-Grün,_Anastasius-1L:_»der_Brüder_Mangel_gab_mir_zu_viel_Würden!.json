{"textgrid.poem.40829": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbder Br\u00fcder Mangel gab mir zu viel W\u00fcrden!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Br\u00fcder Mangel gab mir zu viel W\u00fcrden!", "tokens": ["\u00bb", "der", "Br\u00fc\u00b7der", "Man\u00b7gel", "gab", "mir", "zu", "viel", "W\u00fcr\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im B\u00fcchersaal h\u00fct' ich Foliantenh\u00fcrden,", "tokens": ["Im", "B\u00fc\u00b7cher\u00b7saal", "h\u00fct'", "ich", "Fo\u00b7li\u00b7an\u00b7ten\u00b7h\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als G\u00e4rtner mu\u00df ich Kohl und Blumen treiben", "tokens": ["Als", "G\u00e4rt\u00b7ner", "mu\u00df", "ich", "Kohl", "und", "Blu\u00b7men", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und als Chronist des Klosters Chronik schreiben!", "tokens": ["Und", "als", "Chro\u00b7nist", "des", "Klos\u00b7ters", "Chro\u00b7nik", "schrei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Quartanten dort gleich Leichensteinen prangen,", "tokens": ["Quar\u00b7tan\u00b7ten", "dort", "gleich", "Lei\u00b7chen\u00b7stei\u00b7nen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dran Spinnennetz' als Todtenhemden hangen;", "tokens": ["Dran", "Spin\u00b7nen\u00b7netz'", "als", "Tod\u00b7ten\u00b7hem\u00b7den", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wehr' es nicht, da dieser Gr\u00fcfte Bl\u00fcthen", "tokens": ["Ich", "wehr'", "es", "nicht", ",", "da", "die\u00b7ser", "Gr\u00fcf\u00b7te", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Welt ja l\u00e4ngst mit Duft und Glanz durchgl\u00fchten.", "tokens": ["Die", "Welt", "ja", "l\u00e4ngst", "mit", "Duft", "und", "Glanz", "durch\u00b7gl\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Chronik schlag' ich auf; da find' ich wieder", "tokens": ["Die", "Chro\u00b7nik", "schlag'", "ich", "auf", ";", "da", "find'", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Rose, die ich drein einst legte nieder", "tokens": ["Die", "Ro\u00b7se", ",", "die", "ich", "drein", "einst", "leg\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Zeichen, wo mein Vorfahr stehn geblieben.", "tokens": ["Als", "Zei\u00b7chen", ",", "wo", "mein", "Vor\u00b7fahr", "stehn", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach! meine Hand hat noch kein Wort geschrieben!", "tokens": ["Ach", "!", "mei\u00b7ne", "Hand", "hat", "noch", "kein", "Wort", "ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ist's meine Schuld, da\u00df l\u00e4ngst die Wunder schweigen,", "tokens": ["Ist's", "mei\u00b7ne", "Schuld", ",", "da\u00df", "l\u00e4ngst", "die", "Wun\u00b7der", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein F\u00fcrst sich zum Besuch am Thor will zeigen,", "tokens": ["Kein", "F\u00fcrst", "sich", "zum", "Be\u00b7such", "am", "Thor", "will", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "APPRART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Bannstrahl blitzt, und in dem Klosterleben", "tokens": ["Kein", "Bann\u00b7strahl", "blitzt", ",", "und", "in", "dem", "Klos\u00b7ter\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich's nur begibt, da\u00df gar nichts sich begeben?", "tokens": ["Sich's", "nur", "be\u00b7gibt", ",", "da\u00df", "gar", "nichts", "sich", "be\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "KOUS", "ADV", "PIS", "PRF", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Mich aber d\u00fcnkt's, als ob die Weltgeschichte", "tokens": ["Mich", "a\u00b7ber", "d\u00fcnkt's", ",", "als", "ob", "die", "Welt\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich m\u00e4hlich ganz in meinen Garten fl\u00fcchte;", "tokens": ["Sich", "m\u00e4h\u00b7lich", "ganz", "in", "mei\u00b7nen", "Gar\u00b7ten", "fl\u00fcch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn drau\u00dfen thatenleer die Tage wandern,", "tokens": ["Wenn", "drau\u00b7\u00dfen", "tha\u00b7ten\u00b7leer", "die", "Ta\u00b7ge", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bl\u00fcht drin ein hold Ereigni\u00df nach dem andern.", "tokens": ["Bl\u00fcht", "drin", "ein", "hold", "Er\u00b7eig\u00b7ni\u00df", "nach", "dem", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Als sich des Winters W\u00fcsten in den Sonnen", "tokens": ["Als", "sich", "des", "Win\u00b7ters", "W\u00fcs\u00b7ten", "in", "den", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lenzes zu bev\u00f6lkern kaum begonnen,", "tokens": ["Des", "Len\u00b7zes", "zu", "be\u00b7v\u00f6l\u00b7kern", "kaum", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ward die Tulpe auf des Thrones Stufen", "tokens": ["Da", "ward", "die", "Tul\u00b7pe", "auf", "des", "Thro\u00b7nes", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erh\u00f6ht und laut als K\u00f6n'gin ausgerufen.", "tokens": ["Er\u00b7h\u00f6ht", "und", "laut", "als", "K\u00f6n'\u00b7gin", "aus\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Rose zeigt dem Volk sich vom Altane,", "tokens": ["Die", "Ro\u00b7se", "zeigt", "dem", "Volk", "sich", "vom", "Al\u00b7ta\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da wird entthront die eitle Tulipane!", "tokens": ["Da", "wird", "ent\u00b7thront", "die", "eit\u00b7le", "Tu\u00b7li\u00b7pa\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Rose prangt mit Duft und Dorn und Bl\u00fcthe:", "tokens": ["Die", "Ro\u00b7se", "prangt", "mit", "Duft", "und", "Dorn", "und", "Bl\u00fc\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es herrsche Sch\u00f6nheit, Kraft und Herzensg\u00fcte!", "tokens": ["Es", "herr\u00b7sche", "Sch\u00f6n\u00b7heit", ",", "Kraft", "und", "Her\u00b7zens\u00b7g\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "De\u00df nicht zufrieden, sind zum Bund verschworen", "tokens": ["De\u00df", "nicht", "zu\u00b7frie\u00b7den", ",", "sind", "zum", "Bund", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADJD", "$,", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Violen, die rebellisch tricoloren;", "tokens": ["Vi\u00b7o\u00b7len", ",", "die", "re\u00b7bel\u00b7lisch", "tri\u00b7co\u00b7lo\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die K\u00f6pfchen stecken fl\u00fcsternd sie zusammen,", "tokens": ["Die", "K\u00f6pf\u00b7chen", "ste\u00b7cken", "fl\u00fcs\u00b7ternd", "sie", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gen die Tyrannin Wettkampf zu entflammen.", "tokens": ["Gen", "die", "Ty\u00b7ran\u00b7nin", "Wett\u00b7kampf", "zu", "ent\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sieh Goldorangen, Kronen in den H\u00e4nden,", "tokens": ["Sieh", "Gol\u00b7do\u00b7ran\u00b7gen", ",", "Kro\u00b7nen", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Granaten, die das Aug' mit Purpur blenden,", "tokens": ["Gra\u00b7na\u00b7ten", ",", "die", "das", "Aug'", "mit", "Pur\u00b7pur", "blen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gesandte W\u00e4lschlands, Kron' und Purpur bietend,", "tokens": ["Ge\u00b7sand\u00b7te", "W\u00e4l\u00b7schlands", ",", "Kron'", "und", "Pur\u00b7pur", "bie\u00b7tend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Glashaus, das Hotel der Fremden, h\u00fctend!", "tokens": ["Das", "Glas\u00b7haus", ",", "das", "Ho\u00b7tel", "der", "Frem\u00b7den", ",", "h\u00fc\u00b7tend", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Sieh hier des Fruchtbaums goth'schen Domthurm ragen,", "tokens": ["Sieh", "hier", "des", "Frucht\u00b7baums", "go\u00b7th'\u00b7schen", "Dom\u00b7thurm", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Darin als Glocken hell die V\u00f6gel schlagen,", "tokens": ["Da\u00b7rin", "als", "Glo\u00b7cken", "hell", "die", "V\u00f6\u00b7gel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "NN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um seinen Fu\u00df die farb'gen Blumen alle,", "tokens": ["Um", "sei\u00b7nen", "Fu\u00df", "die", "fa\u00b7rb'\u00b7gen", "Blu\u00b7men", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie Gl\u00e4ub'ge Sonntags um des M\u00fcnsters Halle.", "tokens": ["Wie", "Gl\u00e4ub'\u00b7ge", "Sonn\u00b7tags", "um", "des", "M\u00fcns\u00b7ters", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Dort h\u00fcllt in Traubenschmuck und Laubgewebe", "tokens": ["Dort", "h\u00fcllt", "in", "Trau\u00b7ben\u00b7schmuck", "und", "Laub\u00b7ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den kahlen Pfahl, der sie gest\u00fctzt, die Rebe,", "tokens": ["Den", "kah\u00b7len", "Pfahl", ",", "der", "sie", "ge\u00b7st\u00fctzt", ",", "die", "Re\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Armen Bl\u00f6\u00dfe deckend und im Bilde", "tokens": ["Des", "Ar\u00b7men", "Bl\u00f6\u00b7\u00dfe", "de\u00b7ckend", "und", "im", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir sch\u00f6n entschleiernd christlich echte Milde.", "tokens": ["Mir", "sch\u00f6n", "ent\u00b7schlei\u00b7ernd", "christ\u00b7lich", "ech\u00b7te", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ich wei\u00df mit Bl\u00fcthenranken, Baumspalieren", "tokens": ["Ich", "wei\u00df", "mit", "Bl\u00fc\u00b7then\u00b7ran\u00b7ken", ",", "Baums\u00b7pa\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wand, die von der Welt uns trennt, zu zieren;", "tokens": ["Die", "Wand", ",", "die", "von", "der", "Welt", "uns", "trennt", ",", "zu", "zie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was sollt' ich ob der Scheidemauern klagen,", "tokens": ["Was", "sollt'", "ich", "ob", "der", "Schei\u00b7de\u00b7mau\u00b7ern", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mir so sch\u00f6ne Bl\u00fcth' und Fr\u00fcchte tragen!", "tokens": ["Die", "mir", "so", "sch\u00f6\u00b7ne", "Bl\u00fcth'", "und", "Fr\u00fcch\u00b7te", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "So ist, o Herr, ein stilles, sch\u00f6nes Schweben", "tokens": ["So", "ist", ",", "o", "Herr", ",", "ein", "stil\u00b7les", ",", "sch\u00f6\u00b7nes", "Schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "FM", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Bl\u00fcthenglanz und Sonnenduft mein Leben!", "tokens": ["Durch", "Bl\u00fc\u00b7then\u00b7glanz", "und", "Son\u00b7nen\u00b7duft", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So mag mein Geist z