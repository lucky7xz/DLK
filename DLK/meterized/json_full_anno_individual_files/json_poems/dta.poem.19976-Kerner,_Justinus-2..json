{"dta.poem.19976": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aufflog Sie nun zur ew'gen Sternenhalle,", "tokens": ["Auf\u00b7flog", "Sie", "nun", "zur", "ew'\u00b7gen", "Ster\u00b7nen\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dahin, woher Sie segnend einst gekommen,", "tokens": ["Da\u00b7hin", ",", "wo\u00b7her", "Sie", "seg\u00b7nend", "einst", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir aber steh'n, erkrankt in Thr\u00e4nen alle,", "tokens": ["Wir", "a\u00b7ber", "steh'n", ",", "er\u00b7krankt", "in", "Thr\u00e4\u00b7nen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Trost, kein Heilkraut kann uns Armen frommen.", "tokens": ["Kein", "Trost", ",", "kein", "Heil\u00b7kraut", "kann", "uns", "Ar\u00b7men", "from\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wie wir steh'n, so jedes Trosts benommen,", "tokens": ["Doch", "wie", "wir", "steh'n", ",", "so", "je\u00b7des", "Trosts", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ert\u00f6nt's zu uns mit himmlisch s\u00fc\u00dfem Schalle!", "tokens": ["Er\u00b7t\u00f6nt's", "zu", "uns", "mit", "himm\u00b7lisch", "s\u00fc\u00b7\u00dfem", "Schal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201eschaut himmelan! ich bin euch ja geblieben!", "tokens": ["\u201e", "schaut", "him\u00b7me\u00b7lan", "!", "ich", "bin", "euch", "ja", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201eein Schutzgeist schwe'b ich waltend ob euch Lieben.\u201c", "tokens": ["\u201e", "ein", "Schutz\u00b7geist", "schwe'b", "ich", "wal\u00b7tend", "ob", "euch", "Lie\u00b7ben", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "VVPP", "KOUS", "PPER", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun ist Sie erst um uns und bey uns allen,", "tokens": ["Nun", "ist", "Sie", "erst", "um", "uns", "und", "bey", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "KON", "APPR", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von keinem mehr getrennt durch Thal und H\u00f6hen;", "tokens": ["Von", "kei\u00b7nem", "mehr", "ge\u00b7trennt", "durch", "Thal", "und", "H\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Seufzer st\u00f6hnen, hei\u00dfe Thr\u00e4nen fallen,", "tokens": ["Wo", "Seuf\u00b7zer", "st\u00f6h\u00b7nen", ",", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verla\u00dfne Arme still zum Himmel flehen,", "tokens": ["Ver\u00b7la\u00df\u00b7ne", "Ar\u00b7me", "still", "zum", "Him\u00b7mel", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da wird man h\u00f6ren oft ein leises Wallen,", "tokens": ["Da", "wird", "man", "h\u00f6\u00b7ren", "oft", "ein", "lei\u00b7ses", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird ungehoffte H\u00fclfe staunend sehen.", "tokens": ["Wird", "un\u00b7ge\u00b7hoff\u00b7te", "H\u00fcl\u00b7fe", "stau\u00b7nend", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann fraget nicht: woher ist das gekommen?", "tokens": ["Dann", "fra\u00b7get", "nicht", ":", "wo\u00b7her", "ist", "das", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$.", "PWAV", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es kam von ihr, dem Schutzgeist aller Frommen.", "tokens": ["Es", "kam", "von", "ihr", ",", "dem", "Schutz\u00b7geist", "al\u00b7ler", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}