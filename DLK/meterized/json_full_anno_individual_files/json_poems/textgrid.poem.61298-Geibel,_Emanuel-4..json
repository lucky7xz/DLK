{"textgrid.poem.61298": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Held Parzival, der Junge, kam zum Grale", "tokens": ["Held", "Par\u00b7zi\u00b7val", ",", "der", "Jun\u00b7ge", ",", "kam", "zum", "Gra\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wu\u00dft' es nicht, doch f\u00fchlt er ungesehen", "tokens": ["Und", "wu\u00dft'", "es", "nicht", ",", "doch", "f\u00fchlt", "er", "un\u00b7ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Friedens Hauch in seinen Locken wehen,", "tokens": ["Des", "Frie\u00b7dens", "Hauch", "in", "sei\u00b7nen", "Lo\u00b7cken", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da man zu Montsalvatsch ihn speist' im Saale.", "tokens": ["Da", "man", "zu", "Mont\u00b7sal\u00b7vatsch", "ihn", "speist'", "im", "Saa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So sa\u00df auch ich einst an der Liebe Mahle,", "tokens": ["So", "sa\u00df", "auch", "ich", "einst", "an", "der", "Lie\u00b7be", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unwissend, welch ein Wunder mir geschehen;", "tokens": ["Un\u00b7wis\u00b7send", ",", "welch", "ein", "Wun\u00b7der", "mir", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAT", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur sah die Erd' ich licht in Bl\u00fcten stehen,", "tokens": ["Nur", "sah", "die", "Erd'", "ich", "licht", "in", "Bl\u00fc\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Meer und Himmel gl\u00fchn in ros'gem Strahle.", "tokens": ["Und", "Meer", "und", "Him\u00b7mel", "gl\u00fchn", "in", "ros'\u00b7gem", "Strah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weh, da\u00df wie jener ich bet\u00f6rt mich wandte", "tokens": ["Weh", ",", "da\u00df", "wie", "je\u00b7ner", "ich", "be\u00b7t\u00f6rt", "mich", "wand\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "KOKOM", "PDS", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fortzog, um zu sp\u00e4t es zu empfinden,", "tokens": ["Und", "fort\u00b7zog", ",", "um", "zu", "sp\u00e4t", "es", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "PTKZU", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich mich selbst von meinem Gl\u00fcck verbannte!", "tokens": ["Da\u00df", "ich", "mich", "selbst", "von", "mei\u00b7nem", "Gl\u00fcck", "ver\u00b7bann\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun schweif' ich durch die Welt mit allen Winden,", "tokens": ["Nun", "schweif'", "ich", "durch", "die", "Welt", "mit", "al\u00b7len", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ach, wohin ich auch die Segel spannte:", "tokens": ["Doch", "ach", ",", "wo\u00b7hin", "ich", "auch", "die", "Se\u00b7gel", "spann\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Montsalvatsch konnt' ich nicht wiederfinden.", "tokens": ["Mein", "Mont\u00b7sal\u00b7vatsch", "konnt'", "ich", "nicht", "wie\u00b7der\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Held Parzival, der Junge, kam zum Grale", "tokens": ["Held", "Par\u00b7zi\u00b7val", ",", "der",