{"dta.poem.326": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine unerbittliche Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du Sonne meines Lebens", "tokens": ["Du", "Son\u00b7ne", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bestrahle mein Hertz;", "tokens": ["Be\u00b7strah\u00b7le", "mein", "Hertz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und la\u00df mich nicht vergebens", "tokens": ["Und", "la\u00df", "mich", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beseuffzen den Schmertz.", "tokens": ["Be\u00b7seuff\u00b7zen", "den", "Schmertz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Verkehre mein Klagen", "tokens": ["Ver\u00b7keh\u00b7re", "mein", "Kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mein Seuffzen und Zagen", "tokens": ["Mein", "Seuff\u00b7zen", "und", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In lieblichen Schertz.", "tokens": ["In", "lieb\u00b7li\u00b7chen", "Schertz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Was n\u00fctzet dir mein Qu\u00e4hlen?", "tokens": ["Was", "n\u00fct\u00b7zet", "dir", "mein", "Qu\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hilfft dir meine Pein?", "tokens": ["Was", "hilfft", "dir", "mei\u00b7ne", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6r auf mich zu entseelen", "tokens": ["H\u00f6r", "auf", "mich", "zu", "ent\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch t\u00f6dtenden Schein.", "tokens": ["Durch", "t\u00f6d\u00b7ten\u00b7den", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ach la\u00df mich die Sinnen", "tokens": ["Ach", "la\u00df", "mich", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ART", "NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Mein Engel gewinnen", "tokens": ["Mein", "En\u00b7gel", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die Felsen hart seyn.", "tokens": ["Die", "Fel\u00b7sen", "hart", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Das Eysen mu\u00df erliegen", "tokens": ["Das", "Ey\u00b7sen", "mu\u00df", "er\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Flammen und Gluth/", "tokens": ["In", "Flam\u00b7men", "und", "Gluth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Demant l\u00e4st sich biegen", "tokens": ["Der", "De\u00b7mant", "l\u00e4st", "sich", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch stinckendes Blut/", "tokens": ["Durch", "stin\u00b7cken\u00b7des", "Blut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich aber mein Leben", "tokens": ["Ich", "a\u00b7ber", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mu\u00df Blut und Feur geben/", "tokens": ["Mu\u00df", "Blut", "und", "Feur", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Doch wird es nicht gut.", "tokens": ["Doch", "wird", "es", "nicht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ach \u00e4ndere doch Sch\u00f6ne", "tokens": ["Ach", "\u00e4n\u00b7de\u00b7re", "doch", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den steinernen Sinn/", "tokens": ["Den", "stei\u00b7ner\u00b7nen", "Sinn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und spanne mir die Sehne", "tokens": ["Und", "span\u00b7ne", "mir", "die", "Seh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ferne nicht hin.", "tokens": ["So", "fer\u00b7ne", "nicht", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Erf\u00fcll mein Verlangen/", "tokens": ["Er\u00b7f\u00fcll", "mein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und schenck mir die Wangen", "tokens": ["Und", "schenck", "mir", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Zum Liebes-Gewinn.", "tokens": ["Zum", "Lie\u00b7bes\u00b7Ge\u00b7winn", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}