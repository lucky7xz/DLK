{"dta.poem.19849": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "3.  \n  Lobgesang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Heller gl\u00e4nzt Aurorens Schleier.", "tokens": ["Hel\u00b7ler", "gl\u00e4nzt", "Au\u00b7ro\u00b7rens", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf! Begint den Lobgesang!", "tokens": ["Auf", "!", "Be\u00b7gint", "den", "Lob\u00b7ge\u00b7sang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6ne drein, geweihte Leier!", "tokens": ["T\u00f6\u00b7ne", "drein", ",", "ge\u00b7weih\u00b7te", "Lei\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hall\u2019 am Felsen, Wiederklang!", "tokens": ["Hall'", "am", "Fel\u00b7sen", ",", "Wie\u00b7der\u00b7klang", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eryzinens Hauch durchdringet,", "tokens": ["E\u00b7ry\u00b7zi\u00b7nens", "Hauch", "durch\u00b7drin\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bis zur Gr\u00e4nze der Natur,", "tokens": ["Bis", "zur", "Gr\u00e4n\u00b7ze", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo die lezte Sf\u00e4re klinget,", "tokens": ["Wo", "die", "lez\u00b7te", "Sf\u00e4\u00b7re", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Pulse der Natur.", "tokens": ["Al\u00b7le", "Pul\u00b7se", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie befruchtet Land und Meere,", "tokens": ["Sie", "be\u00b7fruch\u00b7tet", "Land", "und", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie das weite Luftrevier.", "tokens": ["Sie", "das", "wei\u00b7te", "Luf\u00b7tre\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie sie zeuge, wie geb\u00e4re,", "tokens": ["Wie", "sie", "zeu\u00b7ge", ",", "wie", "ge\u00b7b\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Weis die Kreatur von ihr.", "tokens": ["Weis", "die", "Kre\u00b7a\u00b7tur", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wie mit blinkendem Gesteine,", "tokens": ["Wie", "mit", "blin\u00b7ken\u00b7dem", "Ge\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fckt sie br\u00e4utlich unsre Welt;", "tokens": ["Schm\u00fckt", "sie", "br\u00e4ut\u00b7lich", "uns\u00b7re", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streuet Bl\u00fcthen auf die Haine,", "tokens": ["Streu\u00b7et", "Bl\u00fc\u00b7then", "auf", "die", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen \u00fcber Wies\u2019 und Feld.", "tokens": ["Blu\u00b7men", "\u00fc\u00b7ber", "Wies'", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie enth\u00fclt die Anemonen,", "tokens": ["Sie", "ent\u00b7h\u00fclt", "die", "A\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schliest den goldnen Krokus auf;", "tokens": ["Schliest", "den", "gold\u00b7nen", "Kro\u00b7kus", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sezet die azurnen Kronen", "tokens": ["Se\u00b7zet", "die", "a\u00b7zur\u00b7nen", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Prangenden Cyanen auf.", "tokens": ["Pran\u00b7gen\u00b7den", "Cy\u00b7a\u00b7nen", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Den P\u00e4onien entfaltet", "tokens": ["Den", "P\u00e4o\u00b7ni\u00b7en", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sie das purpurne Gewand.", "tokens": ["Sie", "das", "pur\u00b7pur\u00b7ne", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Wie der M\u00e4dchen Busen, spaltet", "tokens": ["Wie", "der", "M\u00e4d\u00b7chen", "Bu\u00b7sen", ",", "spal\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "NE", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Junge Rosen ihre Hand.", "tokens": ["Jun\u00b7ge", "Ro\u00b7sen", "ih\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "In den Ichor ihrer Wunde", "tokens": ["In", "den", "I\u00b7chor", "ih\u00b7rer", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ward ihr Silberblat getaucht,", "tokens": ["Ward", "ihr", "Sil\u00b7ber\u00b7blat", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und aus ihrem f\u00fcssen Munde", "tokens": ["Und", "aus", "ih\u00b7rem", "f\u00fcs\u00b7sen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wolgeruch hinein gehaucht.", "tokens": ["Wol\u00b7ge\u00b7ruch", "hin\u00b7ein", "ge\u00b7haucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APZR", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Liebe segnet die Gefilde,", "tokens": ["Lie\u00b7be", "seg\u00b7net", "die", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und beseliget den Hain;", "tokens": ["Und", "be\u00b7se\u00b7li\u00b7get", "den", "Hain", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe fl\u00f6st dem rauhen Wilde", "tokens": ["Lie\u00b7be", "fl\u00f6st", "dem", "rau\u00b7hen", "Wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wonnigliche Regung ein.", "tokens": ["Won\u00b7nig\u00b7li\u00b7che", "Re\u00b7gung", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Gatten um die Gatten h\u00fcpfen", "tokens": ["Gat\u00b7ten", "um", "die", "Gat\u00b7ten", "h\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00fcstig durch den Wiesengrund.", "tokens": ["R\u00fcs\u00b7tig", "durch", "den", "Wie\u00b7sen\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Afroditens H\u00e4nde kn\u00fcpfen", "tokens": ["Af\u00b7ro\u00b7di\u00b7tens", "H\u00e4n\u00b7de", "kn\u00fcp\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ihren s\u00fcssen Liebesbund.", "tokens": ["Ih\u00b7ren", "s\u00fcs\u00b7sen", "Lie\u00b7bes\u00b7bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Alte Sage bringt zu Ohren:", "tokens": ["Al\u00b7te", "Sa\u00b7ge", "bringt", "zu", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df sie auf der Hirtenflur", "tokens": ["Da\u00df", "sie", "auf", "der", "Hir\u00b7ten\u00b7flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Selber einst den Sohn geboren,", "tokens": ["Sel\u00b7ber", "einst", "den", "Sohn", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Den Beherscher der Natur.", "tokens": ["Den", "Be\u00b7her\u00b7scher", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Sie entris Anchisens Laren", "tokens": ["Sie", "ent\u00b7ris", "An\u00b7chi\u00b7sens", "La\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NE", "NE", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Dem entflamten Ilion,", "tokens": ["Dem", "ent\u00b7flam\u00b7ten", "I\u00b7li\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus tausend Meergefaren", "tokens": ["Und", "aus", "tau\u00b7send", "Meer\u00b7ge\u00b7fa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den verfolgten frommen Sohn.", "tokens": ["Den", "ver\u00b7folg\u00b7ten", "from\u00b7men", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie war\u2019s, die die Hand Aeneens", "tokens": ["Sie", "wa\u00b7r's", ",", "die", "die", "Hand", "A\u00b7e\u00b7neens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Laviniens verband;", "tokens": ["Und", "La\u00b7vi\u00b7ni\u00b7ens", "ver\u00b7band", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und die keusche Zone Rheens", "tokens": ["Und", "die", "keu\u00b7sche", "Zo\u00b7ne", "Rheens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00f6ste sie durch Mavors Hand.", "tokens": ["L\u00f6s\u00b7te", "sie", "durch", "Ma\u00b7vors", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie verm\u00e4lte Romuls Diener,", "tokens": ["Sie", "ver\u00b7m\u00e4l\u00b7te", "Ro\u00b7muls", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Halb durch List und halb durch Macht,", "tokens": ["Halb", "durch", "List", "und", "halb", "durch", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit den T\u00f6chtern der Sabiner.", "tokens": ["Mit", "den", "T\u00f6ch\u00b7tern", "der", "Sa\u00b7bi\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aus den K\u00fcssen erster Nacht", "tokens": ["Aus", "den", "K\u00fcs\u00b7sen", "ers\u00b7ter", "Nacht"], 