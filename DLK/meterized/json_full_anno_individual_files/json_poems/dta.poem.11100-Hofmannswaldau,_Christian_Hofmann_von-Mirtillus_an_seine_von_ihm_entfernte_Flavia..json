{"dta.poem.11100": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Mirtillus an seine von ihm entfernte  \n Flavia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00e4tt' es gar gewi\u00df nicht lange mehr getrieben,", "tokens": ["Ich", "h\u00e4tt'", "es", "gar", "ge\u00b7wi\u00df", "nicht", "lan\u00b7ge", "mehr", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich l\u00e4g\u2019 in kurtzer zeit in einer stillen grufft;", "tokens": ["Ich", "l\u00e4g'", "in", "kurt\u00b7zer", "zeit", "in", "ei\u00b7ner", "stil\u00b7len", "grufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein der s\u00fcsse ku\u00df, den Flavia verschrieben,", "tokens": ["Al\u00b7lein", "der", "s\u00fcs\u00b7se", "ku\u00df", ",", "den", "Fla\u00b7via", "ver\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Verg\u00e4llet mir die lust zur grauen todes-klufft.", "tokens": ["Ver\u00b7g\u00e4l\u00b7let", "mir", "die", "lust", "zur", "grau\u00b7en", "to\u00b7des\u00b7klufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So lange deine gunst mich noch mit zucker speiset,", "tokens": ["So", "lan\u00b7ge", "dei\u00b7ne", "gunst", "mich", "noch", "mit", "zu\u00b7cker", "spei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So lange mich dein geist noch durch Lisetten k\u00fc\u00dft;", "tokens": ["So", "lan\u00b7ge", "mich", "dein", "geist", "noch", "durch", "Li\u00b7set\u00b7ten", "k\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So lang\u2019 ist Flavia mir noch nicht gantz verreiset,", "tokens": ["So", "lang'", "ist", "Fla\u00b7via", "mir", "noch", "nicht", "gantz", "ver\u00b7rei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NE", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Ob meine liebe gleich den gr\u00f6sten theil vermi\u00dft.", "tokens": ["Ob", "mei\u00b7ne", "lie\u00b7be", "gleich", "den", "gr\u00f6s\u00b7ten", "theil", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein etwas ist genung, Mirtillen zu erhalten,", "tokens": ["Ein", "et\u00b7was", "ist", "ge\u00b7nung", ",", "Mir\u00b7til\u00b7len", "zu", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein bloses wort von dir erquicket meinen geist.", "tokens": ["Ein", "blo\u00b7ses", "wort", "von", "dir", "er\u00b7quic\u00b7ket", "mei\u00b7nen", "geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie solte meine brust, mein treuer mund erkalten;", "tokens": ["Wie", "sol\u00b7te", "mei\u00b7ne", "brust", ",", "mein", "treu\u00b7er", "mund", "er\u00b7kal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den dein verliebter kiel mit holden k\u00fcssen speist?", "tokens": ["Den", "dein", "ver\u00b7lieb\u00b7ter", "kiel", "mit", "hol\u00b7den", "k\u00fcs\u00b7sen", "speist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein du glaubest nicht, da\u00df diese kleine fasten", "tokens": ["Al\u00b7lein", "du", "glau\u00b7best", "nicht", ",", "da\u00df", "die\u00b7se", "klei\u00b7ne", "fas\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die starcke krafft gehabt, mich in die grufft zu ziehn;", "tokens": ["Die", "star\u00b7cke", "krafft", "ge\u00b7habt", ",", "mich", "in", "die", "grufft", "zu", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAPP", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie solt\u2019 ich aber wohl in einer w\u00fcsten rasten,", "tokens": ["Wie", "solt'", "ich", "a\u00b7ber", "wohl", "in", "ei\u00b7ner", "w\u00fcs\u00b7ten", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo keine rosen nicht von deinen lippen bl\u00fchn?", "tokens": ["Wo", "kei\u00b7ne", "ro\u00b7sen", "nicht", "von", "dei\u00b7nen", "lip\u00b7pen", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jedoch ich irre mich. Dein unglaub\u2019 ist gegr\u00fcndet,", "tokens": ["Je\u00b7doch", "ich", "ir\u00b7re", "mich", ".", "Dein", "un\u00b7glaub'", "ist", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$.", "PPOSAT", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Blieb auch dein aug\u2019 und mund durchaus von mir entfernt;", "tokens": ["Blieb", "auch", "dein", "aug'", "und", "mund", "durc\u00b7haus", "von", "mir", "ent\u00b7fernt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein aug\u2019, in dem mein geist des lebens sonne findet,", "tokens": ["Dein", "aug'", ",", "in", "dem", "mein", "geist", "des", "le\u00b7bens", "son\u00b7ne", "fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dein mund, der mich den stand der unschuld kennen lernt.", "tokens": ["Dein", "mund", ",", "der", "mich", "den", "stand", "der", "un\u00b7schuld", "ken\u00b7nen", "lernt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PRF", "ART", "VVFIN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dein bloses ebenbild kan meine brust beseelen,", "tokens": ["Dein", "blo\u00b7ses", "e\u00b7ben\u00b7bild", "kan", "mei\u00b7ne", "brust", "be\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das treuer liebe faust n\u00e4chsthin hinein gesetzt;", "tokens": ["Das", "treu\u00b7er", "lie\u00b7be", "faust", "n\u00e4chst\u00b7hin", "hin\u00b7ein", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer so begeistert wird, denckt an weit andre h\u00f6len,", "tokens": ["Wer", "so", "be\u00b7geis\u00b7tert", "wird", ",", "denckt", "an", "weit", "and\u00b7re", "h\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "APPR", "ADJD", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als in das kalte grab, da keine lieb\u2019 ergetzt.", "tokens": ["Als", "in", "das", "kal\u00b7te", "grab", ",", "da", "kei\u00b7ne", "lieb'", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Indessen, liebster schatz! obgleich Mirtillus lebet,", "tokens": ["In\u00b7des\u00b7sen", ",", "liebs\u00b7ter", "schatz", "!", "ob\u00b7gleich", "Mir\u00b7til\u00b7lus", "le\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil deine feder ihm itzt einen ku\u00df verschreibt;", "tokens": ["Weil", "dei\u00b7ne", "fe\u00b7der", "ihm", "itzt", "ei\u00b7nen", "ku\u00df", "ver\u00b7schreibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Obgleich dein schatten stets vor seinen augen schwebet,", "tokens": ["Ob\u00b7gleich", "dein", "schat\u00b7ten", "stets", "vor", "sei\u00b7nen", "au\u00b7gen", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und selbst im hertzen ihn an statt der seele bleibt;", "tokens": ["Und", "selbst", "im", "hert\u00b7zen", "ihn", "an", "statt", "der", "see\u00b7le", "bleibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PPER", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So kan dein etwas doch noch nicht dem gantzen gleichen:", "tokens": ["So", "kan", "dein", "et\u00b7was", "doch", "noch", "nicht", "dem", "gant\u00b7zen", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADV", "ADV", "ADV", "PTKNEG", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das wesen gehet auch dem sch\u00f6nsten schatten vor.", "tokens": ["Das", "we\u00b7sen", "ge\u00b7het", "auch", "dem", "sch\u00f6ns\u00b7ten", "schat\u00b7ten", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die gegenwart allein l\u00e4\u00dft mich den port erreichen,", "tokens": ["Die", "ge\u00b7gen\u00b7wart", "al\u00b7lein", "l\u00e4\u00dft", "mich", "den", "port", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die werthe gegenwart, die ich n\u00e4chsthin verlohr.", "tokens": ["Die", "wert\u00b7he", "ge\u00b7gen\u00b7wart", ",", "die", "ich", "n\u00e4chst\u00b7hin", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Empfindlicher verlust! drum komm, ach komm geschwinde!", "tokens": ["Emp\u00b7find\u00b7li\u00b7cher", "ver\u00b7lust", "!", "drum", "komm", ",", "ach", "komm", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PAV", "VVFIN", "$,", "ITJ", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Getreue Flavia! sonst bleibt er unersetzt.", "tokens": ["Ge\u00b7treu\u00b7e", "Fla\u00b7via", "!", "sonst", "bleibt", "er", "un\u00b7er\u00b7setzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Ob ich das leben gleich in deinem bilde finde;", "tokens": ["Ob", "ich", "das", "le\u00b7ben", "gleich", "in", "dei\u00b7nem", "bil\u00b7de", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So werd\u2019 ich doch durch nichts, als deinen ku\u00df, ergetzt.", "tokens": ["So", "werd'", "ich", "doch", "durch", "nichts", ",", "als", "dei\u00b7nen", "ku\u00df", ",", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}