{"dta.poem.8403": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod einer  \n Nachtigall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Die S\u00e4ngerin,", "tokens": ["Die", "S\u00e4n\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die durch ihr Lied den ganzen Hain versch\u00f6nte,", "tokens": ["Die", "durch", "ihr", "Lied", "den", "gan\u00b7zen", "Hain", "ver\u00b7sch\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie ist dahin!", "tokens": ["Sie", "ist", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sie, deren Ton mir in die Seele hallte,", "tokens": ["Sie", ",", "de\u00b7ren", "Ton", "mir", "in", "die", "See\u00b7le", "hall\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ich am Bach,", "tokens": ["Wenn", "ich", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der durchs Geb\u00fcsch im Abendgolde wallte,", "tokens": ["Der", "durchs", "Ge\u00b7b\u00fcsch", "im", "A\u00b7bend\u00b7gol\u00b7de", "wall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf Blumen lag!", "tokens": ["Auf", "Blu\u00b7men", "lag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie gurgelte, tief aus der vollen Kehle,", "tokens": ["Sie", "gur\u00b7gel\u00b7te", ",", "tief", "aus", "der", "vol\u00b7len", "Keh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den Silberschlag:", "tokens": ["Den", "Sil\u00b7ber\u00b7schlag", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Wiederhall in seiner Felsenh\u00f6hle", "tokens": ["Der", "Wie\u00b7der\u00b7hall", "in", "sei\u00b7ner", "Fel\u00b7sen\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schlug leis' ihn nach.", "tokens": ["Schlug", "leis'", "ihn", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die l\u00e4ndlichen Ges\u00e4ng' und Feldschalmeien", "tokens": ["Die", "l\u00e4nd\u00b7li\u00b7chen", "Ge\u00b7s\u00e4ng'", "und", "Feld\u00b7schal\u00b7mei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erklangen drein;", "tokens": ["Er\u00b7klan\u00b7gen", "drein", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Es tanzeten die Jungfraun ihre Reihen", "tokens": ["Es", "tan\u00b7ze\u00b7ten", "die", "Jung\u00b7fraun", "ih\u00b7re", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im Abendschein.", "tokens": ["Im", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Auf Moose horcht' ein J\u00fcngling mit Entz\u00fccken", "tokens": ["Auf", "Moo\u00b7se", "horcht'", "ein", "J\u00fcng\u00b7ling", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem holden Laut,", "tokens": ["Dem", "hol\u00b7den", "Laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und schmachtend hing an ihres Lieblings Blicken", "tokens": ["Und", "schmach\u00b7tend", "hing", "an", "ih\u00b7res", "Lieb\u00b7lings", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die junge Braut:", "tokens": ["Die", "jun\u00b7ge", "Braut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie dr\u00fcckten sich bei jeder deiner Fugen", "tokens": ["Sie", "dr\u00fcck\u00b7ten", "sich", "bei", "je\u00b7der", "dei\u00b7ner", "Fu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Hand einmal,", "tokens": ["Die", "Hand", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und h\u00f6rten nicht, wenn deine Schwestern schlugen,", "tokens": ["Und", "h\u00f6r\u00b7ten", "nicht", ",", "wenn", "dei\u00b7ne", "Schwes\u00b7tern", "schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O Nachtigall.", "tokens": ["O", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sie horchten dir, bis dumpf die Abendklocke", "tokens": ["Sie", "horch\u00b7ten", "dir", ",", "bis", "dumpf", "die", "A\u00b7bend\u00b7klo\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Dorfes klang,", "tokens": ["Des", "Dor\u00b7fes", "klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Hesperus, gleich einer goldnen Flocke,", "tokens": ["Und", "Hes\u00b7pe\u00b7rus", ",", "gleich", "ei\u00b7ner", "gold\u00b7nen", "Flo\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Wolken drang;", "tokens": ["Aus", "Wol\u00b7ken", "drang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und gingen dann im Wehn der Maienk\u00fchle", "tokens": ["Und", "gin\u00b7gen", "dann", "im", "Wehn", "der", "Mai\u00b7en\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der H\u00fctte zu,", "tokens": ["Der", "H\u00fct\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mit einer Brust voll z\u00e4rtlicher Gef\u00fchle,", "tokens": ["Mit", "ei\u00b7ner", "Brust", "voll", "z\u00e4rt\u00b7li\u00b7cher", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Voll s\u00fcsser Ruh.", "tokens": ["Voll", "s\u00fcs\u00b7ser", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}