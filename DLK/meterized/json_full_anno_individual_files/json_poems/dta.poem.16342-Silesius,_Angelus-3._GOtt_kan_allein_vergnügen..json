{"dta.poem.16342": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "3.  GOtt kan allein vergn\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Weg weg jhr ", "tokens": ["Weg", "weg", "jhr"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Weg weg jhr Engel all; und was an euch thut bikken:", "tokens": ["Weg", "weg", "jhr", "En\u00b7gel", "all", ";", "und", "was", "an", "euch", "thut", "bik\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PIAT", "$.", "KON", "PWS", "APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wil nun eurer nicht; ich werffe mich allein/", "tokens": ["Ich", "wil", "nun", "eu\u00b7rer", "nicht", ";", "ich", "werf\u00b7fe", "mich", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ins ungeschaffne Meer der blossen GOttheit ein.", "tokens": ["Ins", "un\u00b7ge\u00b7schaff\u00b7ne", "Meer", "der", "blos\u00b7sen", "Got\u00b7theit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}