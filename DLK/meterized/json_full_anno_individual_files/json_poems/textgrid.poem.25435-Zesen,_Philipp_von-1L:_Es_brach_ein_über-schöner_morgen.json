ach", "er", "/", "ha", "!", "dis", "ist", "ge\u00b7sie\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$(", "ITJ", "$.", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kom meine liebste Marilis/", "tokens": ["Kom", "mei\u00b7ne", "liebs\u00b7te", "Ma\u00b7ri\u00b7lis", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die mehr als gold von Ofix t\u00fcget;", "tokens": ["die", "mehr", "als", "gold", "von", "O\u00b7fix", "t\u00fc\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dis ist es dis.", "tokens": ["dis", "ist", "es", "dis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PDS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "So macht' er sich zu ihr mit freuden/", "tokens": ["So", "macht'", "er", "sich", "zu", "ihr", "mit", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und nahm sie bei der liljen-hand:", "tokens": ["und", "nahm", "sie", "bei", "der", "lil\u00b7jen\u00b7hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nuhn/ sprach er/ hab' ich lust f\u00fcr leiden/", "tokens": ["nuhn", "/", "sprach", "er", "/", "hab'", "ich", "lus