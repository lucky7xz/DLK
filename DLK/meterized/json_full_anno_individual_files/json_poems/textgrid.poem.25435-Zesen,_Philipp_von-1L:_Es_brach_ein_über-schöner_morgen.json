 ["APPR", "PPER", "VVFIN", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit krankem geist.", "tokens": ["mit", "kran\u00b7kem", "geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Auch Tugendhold kahm/ anzulangen", "tokens": ["Auch", "Tu\u00b7gend\u00b7hold", "kahm", "/", "an\u00b7zu\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "VVFIN", "$(", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die gro\u00dfe Frau der Fr\u00e4uerei/", "tokens": ["die", "gro\u00b7\u00dfe", "Frau", "der", "Fr\u00e4u\u00b7e\u00b7rei", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die ihn mit fast gezwungnen wangen", "tokens": ["die", "ihn", "mit", "fast", "ge\u00b7zwung\u00b7nen", "wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "schier machte sch\u00e4u.", "tokens": ["schier", "mach\u00b7te", "sch\u00e4u", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Er griff aus ihrem Gl\u00fckkes-schreine", "tokens": ["Er", "griff", "aus", "ih\u00b7rem", "Gl\u00fc\u00b7kkes\u00b7schrei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zuerst das ungewisse roht/", "tokens": ["zu\u00b7erst", "das", "un\u00b7ge\u00b7wis\u00b7se", "roht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "p