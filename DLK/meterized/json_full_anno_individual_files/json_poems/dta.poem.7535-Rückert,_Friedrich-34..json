{"dta.poem.7535": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein heil'ger Brauch, im reinen Gartenraum", "tokens": ["Es", "ist", "ein", "heil'\u00b7ger", "Brauch", ",", "im", "rei\u00b7nen", "Gar\u00b7ten\u00b7raum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei deines Sohns Geburt zu pflanzen einen Baum.", "tokens": ["Bei", "dei\u00b7nes", "Sohns", "Ge\u00b7burt", "zu", "pflan\u00b7zen", "ei\u00b7nen", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So \u00e4hnlich ist der Trieb des Menschen und der Pflanze,", "tokens": ["So", "\u00e4hn\u00b7lich", "ist", "der", "Trieb", "des", "Men\u00b7schen", "und", "der", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so verschieden auch, wie Blatt und Blatt am Kranze.", "tokens": ["Und", "so", "ver\u00b7schie\u00b7den", "auch", ",", "wie", "Blatt", "und", "Blatt", "am", "Kran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "$,", "PWAV", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das zarte Reis kann nur durch Jahresgunst gedeihn,", "tokens": ["Das", "zar\u00b7te", "Reis", "kann", "nur", "durch", "Jah\u00b7res\u00b7gunst", "ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur durch Himmelsgunst gedeiht ein Kind allein.", "tokens": ["Und", "nur", "durch", "Him\u00b7mels\u00b7gunst", "ge\u00b7deiht", "ein", "Kind", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Baum, gepflanzt, erw\u00e4chst dir ohne weitre M\u00fchn;", "tokens": ["Der", "Baum", ",", "ge\u00b7pflanzt", ",", "er\u00b7w\u00e4chst", "dir", "oh\u00b7ne", "weit\u00b7re", "M\u00fchn", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht sonder Sorge wirst du sehn den Sohn erbl\u00fchn.", "tokens": ["Nicht", "son\u00b7der", "Sor\u00b7ge", "wirst", "du", "sehn", "den", "Sohn", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KON", "NN", "VAFIN", "PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn du ihn biegen willst, so biege fein den jungen;", "tokens": ["Wenn", "du", "ihn", "bie\u00b7gen", "willst", ",", "so", "bie\u00b7ge", "fein", "den", "jun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VVFIN", "ADJD", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist vom Baum sowohl wie von dem Sohn gesungen.", "tokens": ["Das", "ist", "vom", "Baum", "so\u00b7wohl", "wie", "von", "dem", "Sohn", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "KON", "PWAV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Baum zu seiner Zeit tr\u00e4gt seine Frucht f\u00fcr dich;", "tokens": ["Der", "Baum", "zu", "sei\u00b7ner", "Zeit", "tr\u00e4gt", "sei\u00b7ne", "Frucht", "f\u00fcr", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Sohn tr\u00e4gt seine Frucht, wenn er sie tr\u00e4gt, f\u00fcr sich.", "tokens": ["Dein", "Sohn", "tr\u00e4gt", "sei\u00b7ne", "Frucht", ",", "wenn", "er", "sie", "tr\u00e4gt", ",", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch seine Frucht zu sehn, macht Freuden dich ersatten,", "tokens": ["Doch", "sei\u00b7ne", "Frucht", "zu", "sehn", ",", "macht", "Freu\u00b7den", "dich", "er\u00b7sat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einst zufrieden schl\u00e4fst du ein in seinem Schatten.", "tokens": ["Und", "einst", "zu\u00b7frie\u00b7den", "schl\u00e4fst", "du", "ein", "in", "sei\u00b7nem", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}