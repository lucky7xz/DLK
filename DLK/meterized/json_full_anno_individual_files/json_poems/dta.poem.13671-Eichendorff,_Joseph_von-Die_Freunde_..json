{"dta.poem.13671": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich, so lang noch trunken unsre Augen,", "tokens": ["La\u00df", "mich", ",", "so", "lang", "noch", "trun\u00b7ken", "uns\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "ADJD", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein'n bl\u00fch'nden Kranz aus den vergang'nen Stunden", "tokens": ["Ein'n", "bl\u00fch'n\u00b7den", "Kranz", "aus", "den", "ver\u00b7gang'\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir heiter um die wei\u00dfe Stirne winden;", "tokens": ["Dir", "hei\u00b7ter", "um", "die", "wei\u00b7\u00dfe", "Stir\u00b7ne", "win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}