{"textgrid.poem.61215": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sitzt die Zeit am gro\u00dfen Webestuhle,", "tokens": ["Es", "sitzt", "die", "Zeit", "am", "gro\u00b7\u00dfen", "We\u00b7be\u00b7stuh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Teppich der Geschicht' ein Bild zu weben;", "tokens": ["Im", "Tep\u00b7pich", "der", "Ge\u00b7schicht'", "ein", "Bild", "zu", "we\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon seh' ich hin und her die F\u00e4den streben,", "tokens": ["Schon", "seh'", "ich", "hin", "und", "her", "die", "F\u00e4\u00b7den", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Rieseneinschlag rauscht, es dr\u00f6hnt die Spule.", "tokens": ["Der", "Rie\u00b7sen\u00b7ein\u00b7schlag", "rauscht", ",", "es", "dr\u00f6hnt", "die", "Spu\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch kannst du w\u00e4hlen, Deutschland, ob zur Buhle", "tokens": ["Noch", "kannst", "du", "w\u00e4h\u00b7len", ",", "Deutschland", ",", "ob", "zur", "Buh\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "NE", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie dich dem sternbekr\u00f6nten Ruhm soll geben,", "tokens": ["Sie", "dich", "dem", "stern\u00b7be\u00b7kr\u00f6n\u00b7ten", "Ruhm", "soll", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob im Geweb' ein Schmachbild du willst leben,", "tokens": ["Ob", "im", "Ge\u00b7web'", "ein", "Schmach\u00b7bild", "du", "willst", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Hohn den V\u00f6lkern bis ans fernste Thule.", "tokens": ["Ein", "Hohn", "den", "V\u00f6l\u00b7kern", "bis", "ans", "ferns\u00b7te", "Thu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sprich aus - doch gilt kein Zaudern jetzt noch Zagen -", "tokens": ["Sprich", "aus", "doch", "gilt", "kein", "Zau\u00b7dern", "jetzt", "noch", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$(", "ADV", "VVFIN", "PIAT", "NN", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Willst h\u00fclflos du von deinem Angesichte", "tokens": ["Willst", "h\u00fcl\u00b7flos", "du", "von", "dei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Kinder sto\u00dfen, die dein Scho\u00df getragen?", "tokens": ["Die", "Kin\u00b7der", "sto\u00b7\u00dfen", ",", "die", "dein", "Scho\u00df", "ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sprich, oder willst in grollendem Gerichte,", "tokens": ["Sprich", ",", "o\u00b7der", "willst", "in", "grol\u00b7len\u00b7dem", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KON", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sie bedr\u00e4ngen, du zu Boden schlagen? -", "tokens": ["Die", "sie", "be\u00b7dr\u00e4n\u00b7gen", ",", "du", "zu", "Bo\u00b7den", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVINF", "$,", "PPER", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tu deinen Spruch! Es harrt die Weltgeschichte.", "tokens": ["Tu", "dei\u00b7nen", "Spruch", "!", "Es", "harrt", "die", "Welt\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es sitzt die Zeit am gro\u00dfen Webestuhle,", "tokens": ["Es", "sitzt", "die", "Zeit", 