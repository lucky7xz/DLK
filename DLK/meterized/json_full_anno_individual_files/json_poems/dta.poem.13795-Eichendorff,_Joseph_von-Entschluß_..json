{"dta.poem.13795": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Entschlu\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Viel andre stehen k\u00fchn in Rossesb\u00fcgel,", "tokens": ["Viel", "and\u00b7re", "ste\u00b7hen", "k\u00fchn", "in", "Ros\u00b7ses\u00b7b\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens h\u00f6chste G\u00fcter zu erjagen,", "tokens": ["Des", "Le\u00b7bens", "h\u00f6chs\u00b7te", "G\u00fc\u00b7ter", "zu", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was sie w\u00fcnschen, m\u00fcssen sie erst wagen,", "tokens": ["Und", "was", "sie", "w\u00fcn\u00b7schen", ",", "m\u00fcs\u00b7sen", "sie", "erst", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein strenger Geist regiert des Rosses Z\u00fcgel. \u2014", "tokens": ["Ein", "stren\u00b7ger", "Geist", "re\u00b7giert", "des", "Ros\u00b7ses", "Z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}