{"dta.poem.10752": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Vff der Allerliebsten Nase.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ach da\u00df ich m\u00f6chte sein die sch\u00f6nste Blum der Erden", "tokens": ["Ach", "da\u00df", "ich", "m\u00f6ch\u00b7te", "sein", "die", "sch\u00f6ns\u00b7te", "Blum", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPER", "VMFIN", "PPOSAT", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll k\u00f6stliches geruchs/ Ach da\u00df ich k\u00f6nte werden", "tokens": ["Voll", "k\u00f6st\u00b7li\u00b7ches", "ge\u00b7ruchs", "/", "Ach", "da\u00df", "ich", "k\u00f6n\u00b7te", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$(", "ITJ", "KOUS", "PPER", "VMFIN", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Weide gentzlich gleich/ die ", "tokens": ["Der", "Wei\u00b7de", "gentz\u00b7lich", "gleich", "/", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "ADV", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da Jo war in noth/ nach ", "tokens": ["Da", "Jo", "war", "in", "noth", "/", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "NE", "VAFIN", "APPR", "NN", "$(", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach da\u00df ich jetzund hettder Bletter art vnd tugent/", "tokens": ["Ach", "da\u00df", "ich", "je\u00b7tzund", "hett\u00b7der", "Blet\u00b7ter", "art", "vnd", "tu\u00b7gent", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "ADJA", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit ", "tokens": ["Da\u00b7mit"], "token_info": ["word"], "pos": ["PAV"], "meter": "++", "measure": "spondeus"}, "line.7": {"text": "Den Polster hat gef\u00fcllt/ darauff die erste Nacht", "tokens": ["Den", "Pols\u00b7ter", "hat", "ge\u00b7f\u00fcllt", "/", "dar\u00b7auff", "die", "ers\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An jhre Seiten zart ", "tokens": ["An", "jhre", "Sei\u00b7ten", "zart"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Wie wolt ich mich in eil so lieblich riechend geben/", "tokens": ["Wie", "wolt", "ich", "mich", "in", "eil", "so", "lieb\u00b7lich", "rie\u00b7chend", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "APPR", "NN", "ADV", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd rundt vmb den Altar mit s\u00fcsser Krafft vmbschweben/", "tokens": ["Vnd", "rundt", "vmb", "den", "Al\u00b7tar", "mit", "s\u00fcs\u00b7ser", "Krafft", "vmb\u00b7schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vmb den Altar/ der ziertjhrs Antlitz tempel clar", "tokens": ["Vmb", "den", "Al\u00b7tar", "/", "der", "ziertjhrs", "Ant\u00b7litz", "tem\u00b7pel", "clar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$(", "ART", "ADJA", "NN", "FM", "FM"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Mehr al\u00df die andere/ doch kundbar sch\u00f6nheit/ schar.", "tokens": ["Mehr", "al\u00df", "die", "an\u00b7de\u00b7re", "/", "doch", "kund\u00b7bar", "sch\u00f6n\u00b7heit", "/", "schar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "$(", "ADV", "ADJD", "ADJD", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vieleicht w\u00fcrd mich dann auch ", "tokens": ["Vie\u00b7leicht", "w\u00fcrd", "mich", "dann", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Da\u00df ich k\u00f6nt vngefehr den Purpurbogen r\u00fchren/", "tokens": ["Da\u00df", "ich", "k\u00f6nt", "vn\u00b7ge\u00b7fehr", "den", "Pur\u00b7pur\u00b7bo\u00b7gen", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Welchen ", "tokens": ["Wel\u00b7chen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Wofern nicht ", "tokens": ["Wo\u00b7fern", "nicht"], "token_info": ["word", "word"], "pos": ["KOUS", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Ach wie bin ich vmbsonst! Jetzt alles thut erkalten/", "tokens": ["Ach", "wie", "bin", "ich", "vmbsonst", "!", "Jetzt", "al\u00b7les", "thut", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VAFIN", "PPER", "ADV", "$.", "ADV", "PIS", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "De\u00df Winters ", "tokens": ["De\u00df", "Win\u00b7ters"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Keiner Viol geruch vnd keiner Nasen schon", "tokens": ["Kei\u00b7ner", "Vi\u00b7ol", "ge\u00b7ruch", "vnd", "kei\u00b7ner", "Na\u00b7sen", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADJD", "KON", "PIAT", "NN", "ADV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Mein wunsch erf\u00fcllen mag/ dann ach ich mu\u00df darvon.", "tokens": ["Mein", "wunsch", "er\u00b7f\u00fcl\u00b7len", "mag", "/", "dann", "ach", "ich", "mu\u00df", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVINF", "VMFIN", "$(", "ADV", "XY", "PPER", "VMFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}