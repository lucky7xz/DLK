{"textgrid.poem.41915": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo die alten Str\u00f6me rauschen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die alten Str\u00f6me rauschen", "tokens": ["Wo", "die", "al\u00b7ten", "Str\u00f6\u00b7me", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem wald'gen Felsenthor,", "tokens": ["Aus", "dem", "wald'\u00b7gen", "Fel\u00b7sen\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Setzt' ich einsam mich, zu lauschen,", "tokens": ["Setzt'", "ich", "ein\u00b7sam", "mich", ",", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dies Singen scholl empor:", "tokens": ["Und", "dies", "Sin\u00b7gen", "scholl", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es fluthet die Welle", "tokens": ["Es", "flut\u00b7het", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Dunkel in's Helle,", "tokens": ["Vom", "Dun\u00b7kel", "in's", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In's gr\u00fcnende Thal aus den W\u00e4ldern hervor.", "tokens": ["In's", "gr\u00fc\u00b7nen\u00b7de", "Thal", "aus", "den", "W\u00e4l\u00b7dern", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Klippen m\u00e4chtig ragen,", "tokens": ["Wenn", "die", "Klip\u00b7pen", "m\u00e4ch\u00b7tig", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Eis die Bahn verh\u00fcllt,", "tokens": ["Wenn", "das", "Eis", "die", "Bahn", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer soll das Leben zagen,", "tokens": ["Nim\u00b7mer", "soll", "das", "Le\u00b7ben", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das aus reichen Tiefen quillt.", "tokens": ["Das", "aus", "rei\u00b7chen", "Tie\u00b7fen", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was g\u00f6ttlich entstanden,", "tokens": ["Was", "g\u00f6tt\u00b7lich", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Tr\u00e4gt irdische Banden;", "tokens": ["Tr\u00e4gt", "ir\u00b7di\u00b7sche", "Ban\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Doch strahlt ihm im Busen das himmlische Bild.", "tokens": ["Doch", "strahlt", "ihm", "im", "Bu\u00b7sen", "das", "himm\u00b7li\u00b7sche", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Z\u00fcrnst du, da\u00df die moos'gen B\u00e4ume", "tokens": ["Z\u00fcrnst", "du", ",", "da\u00df", "die", "moos'\u00b7gen", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort die wilde Fluth entrafft,", "tokens": ["Dort", "die", "wil\u00b7de", "Fluth", "en\u00b7trafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie hier aus dunkelm Keime", "tokens": ["Wenn", "sie", "hier", "aus", "dun\u00b7kelm", "Kei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundlich neue Bl\u00fcthen schafft?", "tokens": ["Freund\u00b7lich", "neu\u00b7e", "Bl\u00fc\u00b7then", "schafft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Lieben und Hassen,", "tokens": ["Im", "Lie\u00b7ben", "und", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im Nahen und Lassen", "tokens": ["Im", "Na\u00b7hen", "und", "Las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Rollt wechselnd durchs Leben die waltende Kraft.", "tokens": ["Rollt", "wech\u00b7selnd", "durchs", "Le\u00b7ben", "die", "wal\u00b7ten\u00b7de", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Tr\u00fcmmer m\u00fcssen bald sich spiegeln", "tokens": ["Tr\u00fcm\u00b7mer", "m\u00fcs\u00b7sen", "bald", "sich", "spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den raschen Wellenh\u00f6hn,", "tokens": ["Auf", "den", "ra\u00b7schen", "Wel\u00b7len\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald von sonnenhellen H\u00fcgeln", "tokens": ["Bald", "von", "son\u00b7nen\u00b7hel\u00b7len", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolze Schl\u00f6sser niedersehn.", "tokens": ["Stol\u00b7ze", "Schl\u00f6s\u00b7ser", "nie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Schl\u00f6sser, die Tr\u00fcmmer,", "tokens": ["Die", "Schl\u00f6s\u00b7ser", ",", "die", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie halten uns nimmer,", "tokens": ["Sie", "hal\u00b7ten", "uns", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Fort treibet den Strom das lebendige Wehn.", "tokens": ["Fort", "trei\u00b7bet", "den", "Strom", "das", "le\u00b7ben\u00b7di\u00b7ge", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Aber wenn die leichten Wogen", "tokens": ["A\u00b7ber", "wenn", "die", "leich\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch des Strandes Lust und Graus", "tokens": ["Durch", "des", "Stran\u00b7des", "Lust", "und", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelbar hinabgezogen", "tokens": ["Wan\u00b7del\u00b7bar", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In Neptunus weites Haus,", "tokens": ["In", "Nep\u00b7tu\u00b7nus", "wei\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann breitet um alle", "tokens": ["Dann", "brei\u00b7tet", "um", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die himmlische Halle", "tokens": ["Die", "himm\u00b7li\u00b7sche", "Hal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit ewigen Sternen unendlich sich aus.", "tokens": ["Mit", "e\u00b7wi\u00b7gen", "Ster\u00b7nen", "un\u00b7end\u00b7lich", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Also h\u00f6rt' ichs rauschend t\u00f6nen", "tokens": ["Al\u00b7so", "h\u00f6rt'", "ichs", "rau\u00b7schend", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Wogen raschem Streit,", "tokens": ["Aus", "der", "Wo\u00b7gen", "ra\u00b7schem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich ging mit stillerm Sehnen", "tokens": ["Und", "ich", "ging", "mit", "stil\u00b7lerm", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Waldeseinsamkeit.", "tokens": ["Durch", "die", "Wal\u00b7de\u00b7sein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was frommen die Klagen!", "tokens": ["Was", 