een.", "tokens": ["de", "is", "mal", "stramm", "to", "Be\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Flink, ihr een anner fast se lo\u00dft!", "tokens": ["Flink", ",", "ihr", "e\u00b7en", "an\u00b7ner", "fast", "se", "lo\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wat d\u00fccht di vun de Witt?", "tokens": ["Wat", "d\u00fccht", "di", "vun", "de", "Witt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "De smitt sick m\u00e4chtig in de Bost", "tokens": ["De", "smitt", "sick", "m\u00e4ch\u00b7tig", "in", "de", "Bost"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un hett sonn stolzen Tritt, juchhei!", "tokens": ["un", "hett", "sonn", "stol\u00b7zen", "Tritt", ",", "juch\u00b7hei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM", "VAFIN", "ADV", "ADJA", "NN", "$,", "ITJ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "un hett sonn stolzen Tritt!", "tokens": ["un", "hett", "sonn", "stol\u00b7zen", "Tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "\u00bbnimm du de Witt, nimm du de Swart,", "tokens": ["\u00bb", "nimm", "du", "de", "Wi