{"dta.poem.2662": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die letzte Reise  \n  Hn. W. G. den 6. Septembr. 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jst di\u00df die letzte Fahrt von allen deinen Reisen?", "tokens": ["Ist", "di\u00df", "die", "letz\u00b7te", "Fahrt", "von", "al\u00b7len", "dei\u00b7nen", "Rei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ART", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schleust du auch zugleich mit der die Augen zu?", "tokens": ["Und", "schleust", "du", "auch", "zu\u00b7gleich", "mit", "der", "die", "Au\u00b7gen", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So m\u00fcssen/ ", "tokens": ["So", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VMFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Den so ein sanffter Tod f\u00fchrt zu der langen Ruh.", "tokens": ["Den", "so", "ein", "sanff\u00b7ter", "Tod", "f\u00fchrt", "zu", "der", "lan\u00b7gen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat noch nicht viermal das Silber ihrer Wangen", "tokens": ["Es", "hat", "noch", "nicht", "vier\u00b7mal", "das", "Sil\u00b7ber", "ih\u00b7rer", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der N\u00e4chte K\u00f6nigin/ die Cynthia/ entdecket/", "tokens": ["Der", "N\u00e4ch\u00b7te", "K\u00f6\u00b7ni\u00b7gin", "/", "die", "Cyn\u00b7thia", "/", "ent\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NE", "$(", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Als dein geliebter Sohn den Weg vorangegangen/", "tokens": ["Als", "dein", "ge\u00b7lieb\u00b7ter", "Sohn", "den", "Weg", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihn der Parcen Hand hat in den Sarg gestreckt.", "tokens": ["Und", "ihn", "der", "Par\u00b7cen", "Hand", "hat", "in", "den", "Sarg", "ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Musen klagten selbst/ da\u00df ihres Lentzens Blume", "tokens": ["Die", "Mu\u00b7sen", "klag\u00b7ten", "selbst", "/", "da\u00df", "ih\u00b7res", "Lent\u00b7zens", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So zeitlich untergieng. Apollo sprach: Mein Sohn/", "tokens": ["So", "zeit\u00b7lich", "un\u00b7ter\u00b7gieng", ".", "A\u00b7pol\u00b7lo", "sprach", ":", "Mein", "Sohn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "NE", "VVFIN", "$.", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der mir im Plei\u00df-Athen gelebt zu sonderm Ruhme/", "tokens": ["Der", "mir", "im", "Plei\u00df\u00b7A\u00b7then", "ge\u00b7lebt", "zu", "son\u00b7derm", "Ruh\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "PTKZU", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Mu\u00df/ ach zu strenger Schlu\u00df des Himmels! nur davon.", "tokens": ["Mu\u00df", "/", "ach", "zu", "stren\u00b7ger", "Schlu\u00df", "des", "Him\u00b7mels", "!", "nur", "da\u00b7von", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$(", "XY", "APPR", "ADJA", "NN", "ART", "NN", "$.", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Abzug/ ", "tokens": ["Der", "Ab\u00b7zug", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Da\u00df nie kein fr\u00f6lich Tag dich ferner angeblickt.", "tokens": ["Da\u00df", "nie", "kein", "fr\u00f6\u00b7lich", "Tag", "dich", "fer\u00b7ner", "an\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "ADJD", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der kl\u00e4gliche Verlust/ die blassen Todes-Kertzen", "tokens": ["Der", "kl\u00e4g\u00b7li\u00b7che", "Ver\u00b7lust", "/", "die", "blas\u00b7sen", "To\u00b7des\u00b7Kert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die haben neues Leid dir st\u00fcndlich zugeschickt:", "tokens": ["Die", "ha\u00b7ben", "neu\u00b7es", "Leid", "dir", "st\u00fcnd\u00b7lich", "zu\u00b7ge\u00b7schickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So/ da\u00df die Wanderschafft des Lebens dir recht bitter/", "tokens": ["So", "/", "da\u00df", "die", "Wan\u00b7der\u00b7schafft", "des", "Le\u00b7bens", "dir", "recht", "bit\u00b7ter", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "NN", "ART", "NN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das grosse Rund der Welt wie eine W\u00fcste schien:", "tokens": ["Das", "gros\u00b7se", "Rund", "der", "Welt", "wie", "ei\u00b7ne", "W\u00fcs\u00b7te", "schien", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Von oben schw\u00e4rtzet sie ein schreckliches Gewitter/", "tokens": ["Von", "o\u00b7ben", "schw\u00e4rt\u00b7zet", "sie", "ein", "schreck\u00b7li\u00b7ches", "Ge\u00b7wit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und unten sihet man nur Dorn und Disteln bl\u00fchn.", "tokens": ["Und", "un\u00b7ten", "si\u00b7het", "man", "nur", "Dorn", "und", "Dis\u00b7teln", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Noch dennoch m\u00fcssen wir di\u00df Thr\u00e4nen-Thal durchreisen/", "tokens": ["Noch", "den\u00b7noch", "m\u00fcs\u00b7sen", "wir", "di\u00df", "Thr\u00e4\u00b7nen\u00b7Thal", "durc\u00b7hrei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PDS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wo bald ein tieffer Schlund uns in den Abgrund st\u00fcrtzt/", "tokens": ["Wo", "bald", "ein", "tief\u00b7fer", "Schlund", "uns", "in", "den", "Ab\u00b7grund", "st\u00fcrtzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bald auff der Seite laurt ein m\u00f6rderisches Eisen/", "tokens": ["Bald", "auff", "der", "Sei\u00b7te", "laurt", "ein", "m\u00f6r\u00b7de\u00b7ri\u00b7sches", "Ei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bald die vergiffte Lufft das Leben uns verk\u00fcrtzt.", "tokens": ["Bald", "die", "ver\u00b7giff\u00b7te", "Lufft", "das", "Le\u00b7ben", "uns", "ver\u00b7k\u00fcrtzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und di\u00df entspringt daher: Wir sind nur frembd\u2019 auff Erden/", "tokens": ["Und", "di\u00df", "ent\u00b7springt", "da\u00b7her", ":", "Wir", "sind", "nur", "frembd'", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PAV", "$.", "PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Leben ist geliehn/ hier ist kein ewig Hau\u00df;", "tokens": ["Das", "Le\u00b7ben", "ist", "ge\u00b7liehn", "/", "hier", "ist", "kein", "e\u00b7wig", "Hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ADV", "VAFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wir m\u00fcssen G\u00e4sten gleich in einer Herbrig werden/", "tokens": ["Wir", "m\u00fcs\u00b7sen", "G\u00e4s\u00b7ten", "gleich", "in", "ei\u00b7ner", "Her\u00b7brig", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "APPR", "ART", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die heute ziehen ein und morgen wieder aus.", "tokens": ["Die", "heu\u00b7te", "zie\u00b7hen", "ein", "und", "mor\u00b7gen", "wie\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PTKVZ", "KON", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach! unsre Jahre sind doch nur ein stetes Wallen/", "tokens": ["Ach", "!", "uns\u00b7re", "Jah\u00b7re", "sind", "doch", "nur", "ein", "ste\u00b7tes", "Wal\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wir lauffen in der Welt wie einem Labyrinth/", "tokens": ["Wir", "lauf\u00b7fen", "in", "der", "Welt", "wie", "ei\u00b7nem", "La\u00b7by\u00b7rinth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bi\u00df da\u00df wir Lebens-satt erm\u00fcdet niederfallen/", "tokens": ["Bi\u00df", "da\u00df", "wir", "Le\u00b7bens\u00b7satt", "er\u00b7m\u00fc\u00b7det", "nie\u00b7der\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und der gequ\u00e4lte Leib zuletzt noch Ruhe findt.", "tokens": ["Und", "der", "ge\u00b7qu\u00e4l\u00b7te", "Leib", "zu\u00b7letzt", "noch", "Ru\u00b7he", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie fr\u00fch\u2019 ein Reisemann sich auffden Weg begiebet/", "tokens": ["Wie", "fr\u00fch'", "ein", "Rei\u00b7se\u00b7mann", "sich", "auff\u00b7den", "Weg", "be\u00b7gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Des Tages Hitz ertr\u00e4gt/ und sehnt sich nach der Nacht:", "tokens": ["Des", "Ta\u00b7ges", "Hitz", "er\u00b7tr\u00e4gt", "/", "und", "sehnt", "sich", "nach", "der", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So seufftzen gleichfalls wir/ durchs Creutzes Gluth ge\u00fcbet/", "tokens": ["So", "seufft\u00b7zen", "gleich\u00b7falls", "wir", "/", "durchs", "Creut\u00b7zes", "Gluth", "ge\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$(", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df doch ein seelig Tod den Feyrabend macht.", "tokens": ["Da\u00df", "doch", "ein", "see\u00b7lig", "Tod", "den", "Fey\u00b7ra\u00b7bend", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Nun diese s\u00fcsse Rast/ das ungest\u00f6rte Schlaffen", "tokens": ["Nun", "die\u00b7se", "s\u00fcs\u00b7se", "Rast", "/", "das", "un\u00b7ge\u00b7st\u00f6r\u00b7te", "Schlaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hast du/ ", "tokens": ["Hast", "du", "/"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.39": {"text": "Es schreckt dich weiter nicht noch List/ noch Feindes Waffen/", "tokens": ["Es", "schreckt", "dich", "wei\u00b7ter", "nicht", "noch", "List", "/", "noch", "Fein\u00b7des", "Waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "NE", "$(", "ADV", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und was sonst f\u00fcr Gefahr die Reisenden bestreicht.", "tokens": ["Und", "was", "sonst", "f\u00fcr", "Ge\u00b7fahr", "die", "Rei\u00b7sen\u00b7den", "be\u00b7streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wir irren hin und her in fl\u00fcchtigen Gedancken/", "tokens": ["Wir", "ir\u00b7ren", "hin", "und", "her", "in", "fl\u00fcch\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da dich die Sicherheit mit ihren Fl\u00fcgeln deckt:", "tokens": ["Da", "dich", "die", "Si\u00b7cher\u00b7heit", "mit", "ih\u00b7ren", "Fl\u00fc\u00b7geln", "deckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wir m\u00fcssen uns offt se