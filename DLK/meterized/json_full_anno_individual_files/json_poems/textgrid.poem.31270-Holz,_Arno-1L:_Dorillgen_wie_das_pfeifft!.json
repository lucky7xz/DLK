{"textgrid.poem.31270": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dorillgen/ wie das pfeifft!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dorillgen/ wie das pfeifft!", "tokens": ["Do\u00b7rill\u00b7gen", "/", "wie", "das", "pfeifft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOKOM", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur Nacht hat es gereifft.", "tokens": ["Zur", "Nacht", "hat", "es", "ge\u00b7reifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Itzt heizz for deinen lihben Mann", "tokens": ["Itzt", "heizz", "for", "dei\u00b7nen", "lih\u00b7ben", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den schwartzen Kachel-Ofen an.", "tokens": ["den", "schwart\u00b7zen", "Ka\u00b7chel\u00b7O\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihn blagt schon arg die Gicht/", "tokens": ["Ihn", "blagt", "schon", "arg", "die", "Gicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "ich br\u00e4uch so wa\u00df noch nicht.", "tokens": ["ich", "br\u00e4uch", "so", "wa\u00df", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Itzt ist so r\u00e4cht nach meinem Sinn", "tokens": ["Itzt", "ist", "so", "r\u00e4cht", "nach", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "dein rohter Rokk mit Bl\u00fchmckens drin!", "tokens": ["dein", "roh\u00b7ter", "Rokk", "mit", "Bl\u00fch\u00b7mckens", "drin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie G\u00f6ldt \u00fcmbfliesst dein Hahr", "tokens": ["Wie", "G\u00f6ldt", "\u00fcmb\u00b7fliesst", "dein", "Hahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "das ahrtlich runde Paar;", "tokens": ["das", "ahrt\u00b7lich", "run\u00b7de", "Paar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "kein Perser-", "tokens": ["kein", "Per\u00b7ser"], "token_info": ["word", "word"], "pos": ["PIAT", "TRUNC"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Nein/ nie w\u00e4r ich ", "tokens": ["Nein", "/", "nie", "w\u00e4r", "ich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$(", "ADV", "VAFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "auff eine Knochen-Kuh.", "tokens": ["auff", "ei\u00b7ne", "Kno\u00b7chen\u00b7Kuh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dih\u00df macht mich