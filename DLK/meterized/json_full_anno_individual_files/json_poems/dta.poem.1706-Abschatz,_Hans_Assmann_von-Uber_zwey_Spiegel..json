{"dta.poem.1706": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber zwey Spiegel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kan dir nichts als Gla\u00df/ mein Kind/ f\u00fcr di\u00dfmahl schen-", "tokens": ["Ich", "kan", "dir", "nichts", "als", "Gla\u00df", "/", "mein", "Kind", "/", "f\u00fcr", "di\u00df\u00b7mahl", "schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "KOKOM", "NN", "$(", "PPOSAT", "NN", "$(", "APPR", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "cken/", "tokens": ["cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Doch hab ich auch dabey mein sonderlich Bedencken.", "tokens": ["Doch", "hab", "ich", "auch", "da\u00b7bey", "mein", "son\u00b7der\u00b7lich", "Be\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PAV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das wei\u00dfe Silber ist ein Bild der reinen Treue/", "tokens": ["Das", "wei\u00b7\u00dfe", "Sil\u00b7ber", "ist", "ein", "Bild", "der", "rei\u00b7nen", "Treu\u00b7e", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die sich bey mir und dir von Jahr zu Jahr verneue.", "tokens": ["Die", "sich", "bey", "mir", "und", "dir", "von", "Jahr", "zu", "Jahr", "ver\u00b7neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "KON", "PPER", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Betr\u00fcgt zu Zeiten gleich ein glattes Spiegel-Ei\u00df/", "tokens": ["Be\u00b7tr\u00fcgt", "zu", "Zei\u00b7ten", "gleich", "ein", "glat\u00b7tes", "Spie\u00b7gel\u00b7Ei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Genung/ da\u00df unser Sinn von keiner Falschheit wei\u00df.", "tokens": ["Ge\u00b7nung", "/", "da\u00df", "un\u00b7ser", "Sinn", "von", "kei\u00b7ner", "Falschheit", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Ein Spiegel ist das He", "tokens": ["Ein", "Spie\u00b7gel", "ist", "das", "He"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Was sein vertrautes Hertz in Lieb und Treue meynet.", "tokens": ["Was", "sein", "ver\u00b7trau\u00b7tes", "Hertz", "in", "Lieb", "und", "Treu\u00b7e", "mey\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Spiegel f\u00e4ngt durch Kunst von andern Licht und Flam-", "tokens": ["Ein", "Spie\u00b7gel", "f\u00e4ngt", "durch", "Kunst", "von", "an\u00b7dern", "Licht", "und", "Flam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "men/", "tokens": ["men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Dein Auge kn\u00fcpfft mein Hertz/ das deine meins zusammen.", "tokens": ["Dein", "Au\u00b7ge", "kn\u00fcpfft", "mein", "Hertz", "/", "das", "dei\u00b7ne", "meins", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$(", "ART", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dergleichen Sinnen Bild sind Spiegel gleicher Art.", "tokens": ["Derg\u00b7lei\u00b7chen", "Sin\u00b7nen", "Bild", "sind", "Spie\u00b7gel", "glei\u00b7cher", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "NN", "VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gott halte sie und uns noch lange Zeit gepaart.", "tokens": ["Gott", "hal\u00b7te", "sie", "und", "uns", "noch", "lan\u00b7ge", "Zeit", "ge\u00b7paart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "PPER", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}