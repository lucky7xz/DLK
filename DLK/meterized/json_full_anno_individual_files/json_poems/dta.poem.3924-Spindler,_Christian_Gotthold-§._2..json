{"dta.poem.3924": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 2.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Der Anfang ist nur schwer; denn wer nur durch-", "tokens": ["Der", "An\u00b7fang", "ist", "nur", "schwer", ";", "denn", "wer", "nur", "durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "KON", "PWS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gedrungen,", "tokens": ["ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und endlich mit Gewalt ein M\u00e4ulgen abgezwungen,", "tokens": ["Und", "end\u00b7lich", "mit", "Ge\u00b7walt", "ein", "M\u00e4ul\u00b7gen", "ab\u00b7ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So thut sie selbst den Mund wie junge V\u00f6gel auf,", "tokens": ["So", "thut", "sie", "selbst", "den", "Mund", "wie", "jun\u00b7ge", "V\u00f6\u00b7gel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "KOKOM", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und seufftzet noch darzu! Ach k\u00fcsse mich doch drauf.", "tokens": ["Und", "seufft\u00b7zet", "noch", "dar\u00b7zu", "!", "Ach", "k\u00fcs\u00b7se", "mich", "doch", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$.", "ITJ", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die M\u00e4ulgen sollen nun einst doppelt wieder krigen;", "tokens": ["Die", "M\u00e4ul\u00b7gen", "sol\u00b7len", "nun", "einst", "dop\u00b7pelt", "wie\u00b7der", "kri\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn so als ich und du im Ehe-Bette liegen;", "tokens": ["Wenn", "so", "als", "ich", "und", "du", "im", "E\u00b7he\u00b7Bet\u00b7te", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KOUS", "PPER", "KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch fallen uns hierbey die ", "tokens": ["Auch", "fal\u00b7len", "uns", "hier\u00b7bey", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die sollen k\u00fcnfftig hin von hundert tausend seyn.", "tokens": ["Die", "sol\u00b7len", "k\u00fcnff\u00b7tig", "hin", "von", "hun\u00b7dert", "tau\u00b7send", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "ADV", "APPR", "CARD", "CARD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}