{"textgrid.poem.48728": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15. In des edlen Lorenz Wolf Sparn, K\u00f6nigl. Schwedischen Gro\u00df-Gesandten an den Gro\u00dff\u00fcrstl. Hof in der Moskaw, Marschallen, sein Stambuch", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie recht hat er gesagt, der Freundschaft mit der Sonnen", "tokens": ["Wie", "recht", "hat", "er", "ge\u00b7sagt", ",", "der", "Freund\u00b7schaft", "mit", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so wol verglichen hat! Sie scheint der gro\u00dfen Welt,", "tokens": ["so", "wol", "ver\u00b7gli\u00b7chen", "hat", "!", "Sie", "scheint", "der", "gro\u00b7\u00dfen", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bringt alles in sein Sein, erw\u00e4rmt, vermehrt, erh\u00e4lt,", "tokens": ["bringt", "al\u00b7les", "in", "sein", "Sein", ",", "er\u00b7w\u00e4rmt", ",", "ver\u00b7mehrt", ",", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$,", "VVPP", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "teilt Gl\u00fcck und Zeiten aus. Sie ist der Freundschaft Brunnen,", "tokens": ["teilt", "Gl\u00fcck", "und", "Zei\u00b7ten",