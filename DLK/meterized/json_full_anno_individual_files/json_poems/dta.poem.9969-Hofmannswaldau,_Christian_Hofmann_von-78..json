{"dta.poem.9969": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ein bauer mich erdult/ m\u00fcssen mich auch f\u00fcrsten leiden/", "tokens": ["Wo", "ein", "bau\u00b7er", "mich", "er\u00b7dult", "/", "m\u00fcs\u00b7sen", "mich", "auch", "f\u00fcrs\u00b7ten", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$(", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$("], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ich komm frey dem gr\u00f6sten k\u00f6nig/ wie den bettlern/ ins gesicht/", "tokens": ["Ich", "komm", "frey", "dem", "gr\u00f6s\u00b7ten", "k\u00f6\u00b7nig", "/", "wie", "den", "bett\u00b7lern", "/", "ins", "ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$(", "KOKOM", "ART", "NN", "$(", "APPRART", "VVPP", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Nehme stets/ und gebe nie; doch will keiner mich nicht meiden.", "tokens": ["Neh\u00b7me", "stets", "/", "und", "ge\u00b7be", "nie", ";", "doch", "will", "kei\u00b7ner", "mich", "nicht", "mei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "KON", "VVFIN", "ADV", "$.", "ADV", "VMFIN", "PIS", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Alle suchen meine h\u00fclffe/ nur das frauenzimmer nicht.", "tokens": ["Al\u00b7le", "su\u00b7chen", "mei\u00b7ne", "h\u00fclf\u00b7fe", "/", "nur", "das", "frau\u00b7en\u00b7zim\u00b7mer", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}