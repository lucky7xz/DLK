{"dta.poem.9922": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eng und klein ist meine wohnung/ mein verstand der ist nicht", "tokens": ["Eng", "und", "klein", "ist", "mei\u00b7ne", "woh\u00b7nung", "/", "mein", "ver\u00b7stand", "der", "ist", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "VVFIN", "ART", "VAFIN", "PTKNEG"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "gro\u00df/", "tokens": ["gro\u00df", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "In den fried- und krieges-zeiten pfleg ich offt und viel zu n\u00fctze\u0303/", "tokens": ["In", "den", "frie\u00b7d", "und", "krie\u00b7ge\u00b7szei\u00b7ten", "pfleg", "ich", "offt", "und", "viel", "zu", "n\u00fctz\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "VVFIN", "PPER", "ADV", "KON", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Mancher der wird recht gef\u00e4rbet/ wenn ich ihm begegne blo\u00df;", "tokens": ["Man\u00b7cher", "der", "wird", "recht", "ge\u00b7f\u00e4r\u00b7bet", "/", "wenn", "ich", "ihm", "be\u00b7geg\u00b7ne", "blo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "VAFIN", "ADJD", "VVPP", "$(", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Doch wei\u00df ich f\u00fcr schimpf und schand meine herrschaft zu be-", "tokens": ["Doch", "wei\u00df", "ich", "f\u00fcr", "schimpf", "und", "schand", "mei\u00b7ne", "herr\u00b7schaft", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "TRUNC"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "sch\u00fctzen.", "tokens": ["sch\u00fct\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}