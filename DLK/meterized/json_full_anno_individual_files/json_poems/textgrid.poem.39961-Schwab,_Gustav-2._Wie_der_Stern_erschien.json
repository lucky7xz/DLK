{"textgrid.poem.39961": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2. Wie der Stern erschien", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gingen Viele zu den Sternen,", "tokens": ["So", "gin\u00b7gen", "Vie\u00b7le", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sahen den verhei\u00dfnen nicht,", "tokens": ["Die", "sa\u00b7hen", "den", "ver\u00b7hei\u00df\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und andre stiegen auf, zu lernen,", "tokens": ["Und", "and\u00b7re", "stie\u00b7gen", "auf", ",", "zu", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von wannen schiene doch sein Licht.", "tokens": ["Von", "wan\u00b7nen", "schie\u00b7ne", "doch", "sein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und diese schieden auch im Glauben", "tokens": ["Und", "die\u00b7se", "schie\u00b7den", "auch", "im", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und starben hin in Hoffnungslust,", "tokens": ["Und", "star\u00b7ben", "hin", "in", "Hoff\u00b7nungs\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Zweifel kam, den Stern zu rauben,", "tokens": ["Kein", "Zwei\u00b7fel", "kam", ",", "den", "Stern", "zu", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In die erhellte Heidenbrust.", "tokens": ["In", "die", "er\u00b7hell\u00b7te", "Hei\u00b7den\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und Zw\u00f6lfe blieben's ihrer immer,", "tokens": ["Und", "Zw\u00f6l\u00b7fe", "blie\u00b7ben's", "ih\u00b7rer", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie harrten aus im Glanz der Nacht,", "tokens": ["Sie", "harr\u00b7ten", "aus", "im", "Glanz", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schliefen bei des Tages Schimmer,", "tokens": ["Sie", "schlie\u00b7fen", "bei", "des", "Ta\u00b7ges", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von stern'gen Tr\u00e4umen angelacht.", "tokens": ["Von", "stern'\u00b7gen", "Tr\u00e4u\u00b7men", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch lagen sie, in die Gewande", "tokens": ["Noch", "la\u00b7gen", "sie", ",", "in", "die", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geh\u00fcllt, in Abends erstem Duft,", "tokens": ["Ge\u00b7h\u00fcllt", ",", "in", "A\u00b7bends", "ers\u00b7tem", "Duft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da weckte sie ein Glanz am Rande,", "tokens": ["Da", "weck\u00b7te", "sie", "ein", "Glanz", "am", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo sich ber\u00fchren Erd' und Luft.", "tokens": ["Wo", "sich", "be\u00b7r\u00fch\u00b7ren", "Erd'", "und", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Blicke gl\u00fchn, die Herzen schwellen,", "tokens": ["Die", "Bli\u00b7cke", "gl\u00fchn", ",", "die", "Her\u00b7zen", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn, einer Morgenr\u00f6te gleich,", "tokens": ["Denn", ",", "ei\u00b7ner", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sehn sie den Osten sich erhellen,", "tokens": ["Sehn", "sie", "den", "Os\u00b7ten", "sich", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Sterne werden bleich;", "tokens": ["Und", "al\u00b7le", "Ster\u00b7ne", "wer\u00b7den", "bleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es steigt, es steigt \u2013 es ist die Sonne,", "tokens": ["Es", "steigt", ",", "es", "steigt", "\u2013", "es", "ist", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$(", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu nennen ist ein Stern es nicht,", "tokens": ["Zu", "nen\u00b7nen", "ist", "ein", "Stern", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Getrunken hat er aus dem Bronne", "tokens": ["Ge\u00b7trun\u00b7ken", "hat", "er", "aus", "dem", "Bron\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des ew'gen Lichtes selbst sein Licht.", "tokens": ["Des", "ew'\u00b7gen", "Lich\u00b7tes", "selbst", "sein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er sendet lange, goldne Stralen,", "tokens": ["Er", "sen\u00b7det", "lan\u00b7ge", ",", "gold\u00b7ne", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht, wie die andern Sterne thun,", "tokens": ["Nicht", ",", "wie", "die", "an\u00b7dern", "Ster\u00b7ne", "thun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die heute matt in ihrem fahlen,", "tokens": ["Die", "heu\u00b7te", "matt", "in", "ih\u00b7rem", "fah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verschwommnen, armen Glanze ruhn.", "tokens": ["Ver\u00b7schwomm\u00b7nen", ",", "ar\u00b7men", "Glan\u00b7ze", "ruhn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In ganzen Str\u00f6men gie\u00dft er nieder,", "tokens": ["In", "gan\u00b7zen", "Str\u00f6\u00b7men", "gie\u00dft", "er", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Licht, das seinem Kern entstammt,", "tokens": ["Das", "Licht", ",", "das", "sei\u00b7nem", "Kern", "ent\u00b7stammt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als schl\u00fcg' ein Adler sein Gefieder,", "tokens": ["Als", "schl\u00fcg'", "ein", "Ad\u00b7ler", "sein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wallt sein Stral, und fleugt und flammt.", "tokens": ["So", "wallt", "sein", "Stral", ",", "und", "fleugt", "und", "flammt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Zw\u00f6lfe sandten Zeichent\u00f6ne", "tokens": ["Die", "Zw\u00f6l\u00b7fe", "sand\u00b7ten", "Zei\u00b7chen\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins nebeleingeh\u00fcllte Land,", "tokens": ["Ins", "ne\u00b7bel\u00b7ein\u00b7ge\u00b7h\u00fcll\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dieweil der Stern in seiner Sch\u00f6ne,", "tokens": ["Die\u00b7weil", "der", "Stern", "in", "sei\u00b7ner", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Berg verkl\u00e4rend, stille stand.", "tokens": ["Den", "Berg", "ver\u00b7kl\u00e4\u00b7rend", ",", "stil\u00b7le", "stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er stand und wich nicht mit dem Dunkel,", "tokens": ["Er", "stand", "und", "wich", "nicht", "mit", "dem", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er spielte mit dem Morgenthau;", "tokens": ["Er", "spiel\u00b7te", "mit", "dem", "Mor\u00b7gent\u00b7hau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sonne kam, es drang sein Funkel", "tokens": ["Die", "Son\u00b7ne", "kam", ",", "es", "drang", "sein", "Fun\u00b7kel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Unausgel\u00f6scht hinab zur Au'.", "tokens": ["Un\u00b7aus\u00b7ge\u00b7l\u00f6scht", "hin\u00b7ab", "zur", "Au'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da ward ein Jubel und ein Schrecken,", "tokens": ["Da", "ward", "ein", "Ju\u00b7bel", "und", "ein", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als man gewahrte Berg und Thal", "tokens": ["Als", "man", "ge\u00b7wahr\u00b7te", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit zweier Sonnen Schein sich decken,", "tokens": ["Mit", "zwei\u00b7er", "Son\u00b7nen", "Schein", "sich", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles gl\u00fchn im Doppelstral.", "tokens": ["Und", "Al\u00b7les", "gl\u00fchn", "im", "Dop\u00b7pel\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es war, als ob mit Zungen s\u00e4ngen", "tokens": ["Es", "war", ",", "als", "ob", "mit", "Zun\u00b7gen", "s\u00e4n\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lichter hell einander an,", "tokens": ["Die", "Lich\u00b7ter", "hell", "ein\u00b7an\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es war, als spr\u00e4ch's in tausend Kl\u00e4ngen:", "tokens": ["Es", "war", ",", "als", "spr\u00e4ch's", "in", "tau\u00b7send", "Kl\u00e4n\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PIS", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geht, euren K\u00f6nig zu empfahn!", "tokens": ["Geht", ",", "eu\u00b7ren", "K\u00f6\u00b7nig", "zu", "em\u00b7pfahn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So gingen Viele zu den Sternen,", "tokens": ["So", "gin\u00b7gen", "Vie\u00b7le", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sahen den verhei\u00dfnen nicht,", "tokens": ["Die", "sa\u00b7hen", "den", "ver\u00b7hei\u00df\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und andre stiegen auf, zu lernen,", "tokens": ["Und", "and\u00b7re", "stie\u00b7gen", "auf", ",", "zu", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von wannen schiene doch sein Licht.", "tokens": ["Von", "wan\u00b7nen", "schie\u00b7ne", "doch", "sein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und diese schieden auch im Glauben", "tokens": ["Und", "die\u00b7se", "schie\u00b7den", "auch", "im", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und starben hin in Hoffnungslust,", "tokens": ["Und", "star\u00b7ben", "hin", "in", "Hoff\u00b7nungs\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punc