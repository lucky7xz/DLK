{"textgrid.poem.66216": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Pugatschef", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein gefangner Uhu trau'rt,", "tokens": ["Wie", "ein", "ge\u00b7fang\u00b7ner", "U\u00b7hu", "trau'rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sonst dem Wilde aufgelau'rt,", "tokens": ["Der", "sonst", "dem", "Wil\u00b7de", "auf\u00b7ge\u00b7lau'rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus der Eiche luft'ger H\u00f6h", "tokens": ["Und", "aus", "der", "Ei\u00b7che", "luft'\u00b7ger", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf manchen Hasen, manches Reh", "tokens": ["Auf", "man\u00b7chen", "Ha\u00b7sen", ",", "man\u00b7ches", "Reh"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit scharfen Krallen niederflog", "tokens": ["Mit", "schar\u00b7fen", "Kral\u00b7len", "nie\u00b7der\u00b7flog"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Blut in den gekr\u00fcmmten Schnabel sog;", "tokens": ["Und", "Blut", "in", "den", "ge\u00b7kr\u00fcmm\u00b7ten", "Schna\u00b7bel", "sog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So trau'rt nun Pugatschef, der W\u00fcthrich, der durch Morden", "tokens": ["So", "trau'rt", "nun", "Pu\u00b7gat\u00b7schef", ",", "der", "W\u00fcth\u00b7rich", ",", "der", "durch", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$,", "PRELS", "ADJD", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Raub und Brand so f\u00fcrchterlich geworden,", "tokens": ["Und", "Raub", "und", "Brand", "so", "f\u00fcrch\u00b7ter\u00b7lich", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und der im weiten kalten Norden", "tokens": ["Und", "der", "im", "wei\u00b7ten", "kal\u00b7ten", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Schnee mit Blut bedeckt", "tokens": ["Den", "Schnee", "mit", "Blut", "be\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und Fried' und Unschuld weggeschreckt.", "tokens": ["Und", "Fried'", "und", "Un\u00b7schuld", "weg\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ha Pugatschef, nun sei ein k\u00fchner Ritter,", "tokens": ["Ha", "Pu\u00b7gat\u00b7schef", ",", "nun