{"dta.poem.23574": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Adelszeitung  \n  nach Christi Geburt 1840 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was bringt die Adelszeitung Neues?", "tokens": ["Was", "bringt", "die", "A\u00b7dels\u00b7zei\u00b7tung", "Neu\u00b7es", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bringt die alte Herrlichkeit,", "tokens": ["Sie", "bringt", "die", "al\u00b7te", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte Gl\u00fcck der alten Zeit,", "tokens": ["Das", "al\u00b7te", "Gl\u00fcck", "der", "al\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Deutschen alten Preis und Ruhm:", "tokens": ["Der", "Deut\u00b7schen", "al\u00b7ten", "Preis", "und", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das heil'ge deutsche Adelsthum.", "tokens": ["Das", "heil'\u00b7ge", "deut\u00b7sche", "A\u00b7del\u00b7sthum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was bringt die Adelszeitung Neues?", "tokens": ["Was", "bringt", "die", "A\u00b7dels\u00b7zei\u00b7tung", "Neu\u00b7es", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bringt, was ihr von Alters wisst,", "tokens": ["Sie", "bringt", ",", "was", "ihr", "von", "Al\u00b7ters", "wisst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df uralt aller Adel ist,", "tokens": ["Da\u00df", "ur\u00b7alt", "al\u00b7ler", "A\u00b7del", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn eh die Welt den Heiland sah,", "tokens": ["Denn", "eh", "die", "Welt", "den", "Hei\u00b7land", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War schon der deutsche Adel da.", "tokens": ["War", "schon", "der", "deut\u00b7sche", "A\u00b7del", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was bringt die Adelszeitung Neues?", "tokens": ["Was", "bringt", "die", "A\u00b7dels\u00b7zei\u00b7tung", "Neu\u00b7es", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bringt und singt den alten Sang,", "tokens": ["Sie", "bringt", "und", "singt", "den", "al\u00b7ten", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df aus der G\u00f6tter Scho\u00df entsprang", "tokens": ["Da\u00df", "aus", "der", "G\u00f6t\u00b7ter", "Scho\u00df", "ent\u00b7sprang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des alten Adels echtes Reis,", "tokens": ["Des", "al\u00b7ten", "A\u00b7dels", "ech\u00b7tes", "Reis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der armen Menschheit Ehrenpreis.", "tokens": ["Der", "ar\u00b7men", "Menschheit", "Eh\u00b7ren\u00b7preis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Was bringt die Adelszeitung Neues?", "tokens": ["Was", "bringt", "die", "A\u00b7dels\u00b7zei\u00b7tung", "Neu\u00b7es", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bringt und singt das alte Lied,", "tokens": ["Sie", "bringt", "und", "singt", "das", "al\u00b7te", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte Lied vom Unterschied,", "tokens": ["Das", "al\u00b7te", "Lied", "vom", "Un\u00b7ter\u00b7schied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und da\u00df ein g\u00f6ttergleich Geschlecht", "tokens": ["Und", "da\u00df", "ein", "g\u00f6t\u00b7ter\u00b7gleich", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verdient ein eignes Menschenrecht.", "tokens": ["Ver\u00b7di\u00b7ent", "ein", "eig\u00b7nes", "Men\u00b7schen\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Was bringt die Adelszeitung Neues?", "tokens": ["Was", "bringt", "die", "A\u00b7dels\u00b7zei\u00b7tung", "Neu\u00b7es", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bringt den alten Satz zur\u00fcck,", "tokens": ["Sie", "bringt", "den", "al\u00b7ten", "Satz", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df F\u00fcrstenheil und V\u00f6lkergl\u00fcck", "tokens": ["Da\u00df", "F\u00fcrs\u00b7ten\u00b7heil", "und", "V\u00f6l\u00b7ker\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles Gut' in dieser Welt", "tokens": ["Und", "al\u00b7les", "Gut'", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur mit dem Adel steht und f\u00e4llt.", "tokens": ["Nur", "mit", "dem", "A\u00b7del", "steht", "und", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was bringt die Adelszeitung Neues?", "tokens": ["Was", "bringt", "die", "A\u00b7dels\u00b7zei\u00b7tung", "Neu\u00b7es", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bringet uns das Alte nur:", "tokens": ["Sie", "brin\u00b7get", "uns", "das", "Al\u00b7te", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df jede B\u00fcrgercreatur", "tokens": ["Da\u00df", "je\u00b7de", "B\u00fcr\u00b7ger\u00b7cre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie ein Verdienst hat um den Staat,", "tokens": ["Nie", "ein", "Ver\u00b7dienst", "hat", "um", "den", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lang sie nicht den Adel hat.", "tokens": ["So", "lang", "sie", "nicht", "den", "A\u00b7del", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKNEG", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was bringt die Adelszeitung Neues?", "tokens": ["Was", "bringt", "die", "A\u00b7dels\u00b7zei\u00b7tung", "Neu\u00b7es", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bringt das einz'ge Neue nur,", "tokens": ["Sie", "bringt", "das", "einz'\u00b7ge", "Neu\u00b7e", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df auf des Vaterlandes Flur", "tokens": ["Da\u00df", "auf", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stammb\u00e4ume wieder gut gedeihn \u2014", "tokens": ["Stamm\u00b7b\u00e4u\u00b7me", "wie\u00b7der", "gut", "ge\u00b7deihn"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott woll' uns allen gn\u00e4dig sein!", "tokens": ["Gott", "woll'", "uns", "al\u00b7len", "gn\u00e4\u00b7dig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}