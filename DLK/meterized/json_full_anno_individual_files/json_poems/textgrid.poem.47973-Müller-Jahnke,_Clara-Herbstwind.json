{"textgrid.poem.47973": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Herbstwind", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch fahlbelaubte B\u00e4ume", "tokens": ["Durch", "fahl\u00b7be\u00b7laub\u00b7te", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mit m\u00fcdem Ton der Herbstwind singt;", "tokens": ["mit", "m\u00fc\u00b7dem", "Ton", "der", "Herbst\u00b7wind", "singt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die sehnsuchtsbange Weise klingt", "tokens": ["die", "sehn\u00b7suchts\u00b7ban\u00b7ge", "Wei\u00b7se", "klingt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des Nachts in meine Tr\u00e4ume.", "tokens": ["des", "Nachts", "in", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, alle Blumend\u00fcfte,", "tokens": ["Ach", ",", "al\u00b7le", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das Farbenspiel der Rosenzeit,", "tokens": ["das", "Far\u00b7ben\u00b7spiel", "der", "Ro\u00b7sen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die ganze Sonnenseligkeit \u2013", "tokens": ["die", "gan\u00b7ze", "Son\u00b7nen\u00b7se\u00b7lig\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerstoben in die L\u00fcfte!", "tokens": ["Zer\u00b7sto\u00b7ben", "in", "die", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verstummt ist Scherz und Kosen. \u2013", "tokens": ["Ver\u00b7stummt", "ist", "Scherz", "und", "Ko\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mir gebl\u00fcht in tiefster Brust,", "tokens": ["Die", "mir", "ge\u00b7bl\u00fcht", "in", "tiefs\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das alte Leid, die alte Lust \u2013", "tokens": ["das", "al\u00b7te", "Leid", ",", "die", "al\u00b7te", "Lust", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie starben mit den Rosen!", "tokens": ["sie", "star\u00b7ben", "mit", "den", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun will kein Stern mehr scheinen.", "tokens": ["Nun", "will", "kein", "Stern", "mehr", "schei\u00b7nen", "."], "token_i