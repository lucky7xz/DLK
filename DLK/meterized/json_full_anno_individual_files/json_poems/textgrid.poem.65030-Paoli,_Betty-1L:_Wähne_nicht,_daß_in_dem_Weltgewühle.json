er Rose,", "tokens": ["Et\u00b7was", "sei", "der", "Dorn", "nur", "an", "der", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fl\u00fccht'ger Mi\u00dfklang, der bald ausgebebt?", "tokens": ["Fl\u00fccht'\u00b7ger", "Mi\u00df\u00b7klang", ",", "der", "bald", "aus\u00b7ge\u00b7bebt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nein! als unvertilgbares Erinnern", "tokens": ["Nein", "!", "als", "un\u00b7ver\u00b7tilg\u00b7ba\u00b7res", "E\u00b7rin\u00b7nern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wird's zum Gift, das heimlich aus dem Innern,", "tokens": ["Wird's", "zum", "Gift", ",", "das", "heim\u00b7lich", "aus", "dem", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Gl\u00fcck und Frieden t\u00f6dtlich untergr\u00e4bt!", "tokens": ["Gl\u00fcck", "und", "Frie\u00b7den", "t\u00f6dt\u00b7lich", "un\u00b7ter\u00b7gr\u00e4bt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Lerne d'rum aus ihrem Kreis verschwinden,", "tokens": ["Ler\u00b7ne", "d'\u00b7rum", "aus", "ih\u00b7rem", "Kreis", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dich in deiner Brust zurechtefinden,", "tokens": ["Dich", "in", "dei\u00b7ner", "Brust", "zu\u00b7rech\u00b7te\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lerne du dein eig'ner Freund zu sein!", "tokens": ["Ler\u00b7ne", "du", "dein", "eig'\u00b7ner", "Freund", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was sie dir an Treu' und Huld versprechen,", "tokens": ["Was", "sie", "dir", "an", "Treu'", "und", "Huld", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unwillk\u00fchrlich werden sie es brechen,", "tokens": ["Un\u00b7will\u00b7k\u00fchr\u00b7lich", "wer\u00b7den", "sie", "es", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denn des Lebens Losung hei\u00dft: ", "tokens": ["Denn", "des", "Le\u00b7bens", "Lo\u00b7sung", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}