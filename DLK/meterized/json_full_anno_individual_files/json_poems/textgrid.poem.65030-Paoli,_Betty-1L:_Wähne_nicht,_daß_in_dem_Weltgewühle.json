{"textgrid.poem.65030": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4hne nicht, da\u00df in dem Weltgew\u00fchle", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hne nicht, da\u00df in dem Weltgew\u00fchle", "tokens": ["W\u00e4h\u00b7ne", "nicht", ",", "da\u00df", "in", "dem", "Welt\u00b7ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Je ein Herz, so wie das deine f\u00fchle,", "tokens": ["Je", "ein", "Herz", ",", "so", "wie", "das", "dei\u00b7ne", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "KOKOM", "PDS", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ein zweites folge deiner Spur!", "tokens": ["Da\u00df", "ein", "zwei\u00b7tes", "fol\u00b7ge", "dei\u00b7ner", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4hne nicht im sehnenden Umschlingen", "tokens": ["W\u00e4h\u00b7ne", "nicht", "im", "seh\u00b7nen\u00b7den", "Um\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "And'rer Wesen also durchzudringen,", "tokens": ["An\u00b7d'\u00b7rer", "We\u00b7sen", "al\u00b7so", "durch\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df es mit dem deinen Eines nur! \u2013", "tokens": ["Da\u00df", "es", "mit", "dem", "dei\u00b7nen", "Ei\u00b7nes", "nur", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Einsam bist du, ob die bunte Menge", "tokens": ["Ein\u00b7sam", "bist", "du", ",", "ob", "die", "bun\u00b7te", "Men\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "R\u00fchmend oder tadelnd dich umdr\u00e4nge,", "tokens": ["R\u00fch\u00b7mend", "o\u00b7der", "ta\u00b7delnd", "dich", "um\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Einsam in dem Kampf' wie in der Ruh',", "tokens": ["Ein\u00b7sam", "in", "dem", "Kampf'", "wie", "in", "der", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Einsam bei der Freunde Scheinerbarmen,", "tokens": ["Ein\u00b7sam", "bei", "der", "Freun\u00b7de", "Schei\u00b7ner\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Einsam selbst in deiner Liebsten Armen,", "tokens": ["Ein\u00b7sam", "selbst", "in", "dei\u00b7ner", "Liebs\u00b7ten", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denn sie Alle sind nur sie, nicht du! \u2013", "tokens": ["Denn", "sie", "Al\u00b7le", "sind", "nur", "sie", ",", "nicht", "du", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "PIS", "VAFIN", "ADV", "PPER", "$,", "PTKNEG", "PPER", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Eine Stelle wird es ewig geben,", "tokens": ["Ei\u00b7ne", "Stel\u00b7le", "wird", "es", "e\u00b7wig", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Ri\u00df, der hin durch Aller Leben", "tokens": ["Wo", "der", "Ri\u00df", ",", "der", "hin", "durch", "Al\u00b7ler", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Trennend geht, aufklaffen wird im Sprung'!", "tokens": ["Tren\u00b7nend", "geht", ",", "auf\u00b7klaf\u00b7fen", "wird", "im", "Sprung'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eine Stelle, wo du bang', mit Schrecken,", "tokens": ["Ei\u00b7ne", "Stel\u00b7le", ",", "wo", "du", "bang'", ",", "mit", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nacht wirst sehen ihre Augen decken,", "tokens": ["Nacht", "wirst", "se\u00b7hen", "ih\u00b7re", "Au\u00b7gen", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVINF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und du hoffst noch auf Vereinigung? \u2013", "tokens": ["Und", "du", "hoffst", "noch", "auf", "Ver\u00b7ei\u00b7ni\u00b7gung", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Meinst du, jenes dunkle, formenlose", "tokens": ["Meinst", "du", ",", "je\u00b7nes", "dunk\u00b7le", ",", "for\u00b7men\u00b7lo\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "PDAT", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Etwas sei der Dorn nur an der Rose,", "tokens": ["Et\u00b7was", "sei", "der", "Dorn", "nur", "an", "der", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fl\u00fccht'ger Mi\u00dfklang, der bald ausgebebt?", "tokens": ["Fl\u00fccht'\u00b7ger", "Mi\u00df\u00b7klang", ",", "der", "bald", "aus\u00b7ge\u00b7bebt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nein! als unvertilgbares Erinnern", "tokens": ["Nein", "!", "als", "un\u00b7ver\u00b7tilg\u00b7ba\u00b7res", "E\u00b7rin\u00b7nern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wird's zum Gift, das heimlich aus dem Innern,", "tokens": ["Wird's", "zum", "Gift", ",", "das", "heim\u00b7lich", "aus", "dem", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Gl\u00fcck und Frieden t\u00f6dtlich untergr\u00e4bt!", "tokens": ["Gl\u00fcck", "und", "Frie\u00b7den", "t\u00f6dt\u00b7lich", "un\u00b7ter\u00b7gr\u00e4bt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Lerne d'rum aus ihrem Kreis verschwinden,", "tokens": ["Ler\u00b7ne", "d'\u00b7rum", "aus", "ih\u00b7rem", "Kreis", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dich in deiner Brust zurechtefinden,", "tokens": ["Dich", "in", "dei\u00b7ner", "Brust", "zu\u00b7rech\u00b7te\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lerne du dein eig'ner Freund zu sein!", "tokens": ["Ler\u00b7ne", "du", "dein", "eig'\u00b7ner", "Freund", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was sie dir an Treu' und Huld versprechen,", "tokens": ["Was", "sie", "dir", "an", "Treu'", "und", "Huld", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unwillk\u00fchrlich werden sie es brechen,", "tokens": ["Un\u00b7will\u00b7k\u00fchr\u00b7lich", "wer\u00b7den", "sie", "es", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denn des Lebens Losung hei\u00dft: ", "tokens": ["Denn", "des", "Le\u00b7bens", "Lo\u00b7sung", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "W\u00e4hne nicht, da\u00df in dem Weltgew\u00fchle", "tokens": ["W\u00e4h\u00b7ne", "nicht", ",", "da\u00df", "in", "dem", "Welt\u00b7ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Je ein Herz, so wie das deine f\u00fchle,", "tokens": ["Je", "ein", "Herz", ",", "so", "wie", "das", "dei\u00b7ne", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "KOKOM", "PDS", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ein zweites folge deiner Spur!", "tokens": ["Da\u00df", "ein", "zwei\u00b7tes", "fol\u00b7ge", "dei\u00b7ner", "