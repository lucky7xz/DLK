{"dta.poem.2656": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Zeilen  \n Uber das Absterben Fr. A. F. g. G. den 9.  \n Febr. 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So hat des HErren Hand Dich abermal geb\u00fccket/", "tokens": ["So", "hat", "des", "Her\u00b7ren", "Hand", "Dich", "a\u00b7ber\u00b7mal", "ge\u00b7b\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du must schwartz gekleidt in tieffstem Trauren", "tokens": ["Da\u00df", "du", "must", "schwartz", "ge\u00b7kleidt", "in", "tieffs\u00b7tem", "Trau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat di\u00df/ ", "tokens": ["Hat", "di\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PDS", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Da\u00df wiederumb dein Schatz mu\u00df auff der Bahre stehn?", "tokens": ["Da\u00df", "wie\u00b7de\u00b7rumb", "dein", "Schatz", "mu\u00df", "auff", "der", "Bah\u00b7re", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und zwar ein gleicher Tod/ und fast auch gleiche Stunde", "tokens": ["Und", "zwar", "ein", "glei\u00b7cher", "Tod", "/", "und", "fast", "auch", "glei\u00b7che", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$(", "KON", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Reist sie im ersten Lentz der besten Jahre hin.", "tokens": ["Reist", "sie", "im", "ers\u00b7ten", "Lentz", "der", "bes\u00b7ten", "Jah\u00b7re", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer tadelts/ wenn di\u00df Ach! erschallt aus deinem Munde:", "tokens": ["Wer", "ta\u00b7delts", "/", "wenn", "di\u00df", "Ach", "!", "er\u00b7schallt", "aus", "dei\u00b7nem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PDS", "ITJ", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich glaube da\u00df ich gar des Ungl\u00fccks Wurffball bin:", "tokens": ["Ich", "glau\u00b7be", "da\u00df", "ich", "gar", "des", "Un\u00b7gl\u00fccks", "Wurff\u00b7ball", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "ADV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df Kertzen meiner Lust sind Fackeln blasser Leichen/", "tokens": ["Da\u00df", "Kert\u00b7zen", "mei\u00b7ner", "Lust", "sind", "Fa\u00b7ckeln", "blas\u00b7ser", "Lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VAFIN", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und Grab und Hochzeit-Bett zusammen stehn gebaut.", "tokens": ["Und", "Grab", "und", "Hoch\u00b7zeit\u00b7Bett", "zu\u00b7sam\u00b7men", "stehn", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "L\u00e4st das Verh\u00e4ngn\u00fc\u00df denn durch Flehn sich nicht erweichen/", "tokens": ["L\u00e4st", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "denn", "durch", "Flehn", "sich", "nicht", "er\u00b7wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "APPR", "VVFIN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Da\u00df man mich anders nicht als zweymal Wittber schaut!", "tokens": ["Da\u00df", "man", "mich", "an\u00b7ders", "nicht", "als", "zwey\u00b7mal", "Witt\u00b7ber", "schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "PTKNEG", "KOKOM", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und zwar eh\u2019 noch ein Jahr hat seinen Krei\u00df vollzogen/", "tokens": ["Und", "zwar", "eh'", "noch", "ein", "Jahr", "hat", "sei\u00b7nen", "Krei\u00df", "voll\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vollziehet schon der Tod den Eisen-harten Schlu\u00df.", "tokens": ["Voll\u00b7zie\u00b7het", "schon", "der", "Tod", "den", "Ei\u00b7sen\u00b7har\u00b7ten", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach da\u00df f\u00fcr Rosen-Safft ich Wermuth nur gesogen!", "tokens": ["Ach", "da\u00df", "f\u00fcr", "Ro\u00b7sen\u00b7\u00b7S\u00b7afft", "ich", "Wer\u00b7muth", "nur", "ge\u00b7so\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "APPR", "NN", "PPER", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und meine Liebe stets ins Grab verschicken mu\u00df!", "tokens": ["Und", "mei\u00b7ne", "Lie\u00b7be", "stets", "ins", "Grab", "ver\u00b7schi\u00b7cken", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja was noch kl\u00e4glicher/ und mich darnieder schl\u00e4get/", "tokens": ["Ja", "was", "noch", "kl\u00e4g\u00b7li\u00b7cher", "/", "und", "mich", "dar\u00b7nie\u00b7der", "schl\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "ADV", "ADJD", "$(", "KON", "PRF", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Seegen meiner Eh\u2019 und Hoffnung geht mit ein/", "tokens": ["Der", "See\u00b7gen", "mei\u00b7ner", "Eh'", "und", "Hoff\u00b7nung", "geht", "mit", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Mutter wird ein Grab des Pfandes/ so sie tr\u00e4get/", "tokens": ["Die", "Mut\u00b7ter", "wird", "ein", "Grab", "des", "Pfan\u00b7des", "/", "so", "sie", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zwey Hertzen/ die mein Trost/ deckt itzt ein Leichen-Stein.", "tokens": ["Zwey", "Hert\u00b7zen", "/", "die", "mein", "Trost", "/", "deckt", "itzt", "ein", "Lei\u00b7chen\u00b7Stein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "ART", "PPOSAT", "NN", "$(", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So nun solch Angst-Geschrey erth\u00f6nt von deinen Lippen/", "tokens": ["So", "nun", "solch", "Angst\u00b7Ge\u00b7schrey", "er\u00b7th\u00f6nt", "von", "dei\u00b7nen", "Lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So/ ", "tokens": ["So", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-", "measure": "single.down"}, "line.23": {"text": "So steh ich freylich zu: Wir sind nicht Fels\u2019 und Klippen/", "tokens": ["So", "steh", "ich", "frey\u00b7lich", "zu", ":", "Wir", "sind", "nicht", "Fels'", "und", "Klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "PTKNEG", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und ein vern\u00fcnfftig Weib ist tausend Thr\u00e4nen werth.", "tokens": ["Und", "ein", "ver\u00b7n\u00fcnff\u00b7tig", "Weib", "ist", "tau\u00b7send", "Thr\u00e4\u00b7nen", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VAFIN", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jedoch dir sind bekant nicht nur der Themis Rechte/", "tokens": ["Je\u00b7doch", "dir", "sind", "be\u00b7kant", "nicht", "nur", "der", "The\u00b7mis", "Rech\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du weist auch GOttes Recht das sey unwandelbahr.", "tokens": ["Du", "weist", "auch", "Got\u00b7tes", "Recht", "das", "sey", "un\u00b7wan\u00b7del\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "NN", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Beym ersten Apffel-Bi\u00df starb unser gantz Geschlechte/", "tokens": ["Beym", "ers\u00b7ten", "Apf\u00b7fel\u00b7Bi\u00df", "starb", "un\u00b7ser", "gantz", "Ge\u00b7schlech\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Des Ungehorsams Frucht war Sterben und Gefahr.", "tokens": ["Des", "Un\u00b7ge\u00b7hor\u00b7sams", "Frucht", "war", "Ster\u00b7ben", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bi\u00df dieses schwartze Recht mit seinem Blut durchstrichen", "tokens": ["Bi\u00df", "die\u00b7ses", "schwart\u00b7ze", "Recht", "mit", "sei\u00b7nem", "Blut", "durch\u00b7stri\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Mittler unsers Heils und Retter aller Welt.", "tokens": ["Der", "Mitt\u00b7ler", "un\u00b7sers", "Heils", "und", "Ret\u00b7ter", "al\u00b7ler", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da ist der andre Tod von uns hinweg gewichen/", "tokens": ["Da", "ist", "der", "and\u00b7re", "Tod", "von", "uns", "hin\u00b7weg", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PRF", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der erste so bewand/ da\u00df er den Leib nur f\u00e4llt.", "tokens": ["Der", "ers\u00b7te", "so", "be\u00b7wand", "/", "da\u00df", "er", "den", "Leib", "nur", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Di\u00df irrdische Gef\u00e4\u00df erbaut von schlechter Erden", "tokens": ["Di\u00df", "irr\u00b7di\u00b7sche", "Ge\u00b7f\u00e4\u00df", "er\u00b7baut", "von", "schlech\u00b7ter", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie leichtlich schl\u00e4gtes nicht der Schopffer wieder ein?", "tokens": ["Wie", "leicht\u00b7lich", "schl\u00e4g\u00b7tes", "nicht", "der", "Schopf\u00b7fer", "wie\u00b7der", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "PTKNEG", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die H\u00fctte/ so von Leim/ mu\u00df Koth und Asche werden/", "tokens": ["Die", "H\u00fct\u00b7te", "/", "so", "von", "Leim", "/", "mu\u00df", "Koth", "und", "A\u00b7sche", "wer\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "NN", "$(", "VMFIN", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Glieder m\u00fcrbes Glas kan nicht best\u00e4ndig seyn.", "tokens": ["Der", "Glie\u00b7der", "m\u00fcr\u00b7bes", "Glas", "kan", "nicht", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Befreyt uns nun der Tod von angelegten Banden/", "tokens": ["Be\u00b7freyt", "uns", "nun", "der", "Tod", "von", "an\u00b7ge\u00b7leg\u00b7ten", "Ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "F\u00fchrt uns sein d\u00fcrrer Arm in stille Sicherheit/", "tokens": ["F\u00fchrt", "uns", "sein", "d\u00fcr\u00b7rer", "Arm", "in", "stil\u00b7le", "Si\u00b7cher\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So ist uns mehr als wohl/ da\u00df wir nun \u00fcberstanden", "tokens": ["So", "ist", "uns", "mehr", "als", "wohl", "/", "da\u00df", "wir", "nun", "\u00fc\u00b7bers\u00b7tan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "KOKOM", "ADV", "$(", "KOUS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Reise voller Angst/ den Weg der Sterbligkeit.", "tokens": ["Die", "Rei\u00b7se", "vol\u00b7ler", "Angst", "/", "den", "Weg", "der", "Ster\u00b7blig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Mein Freund/ es ist voran die Liebste nur geschicket", "tokens": ["Mein", "Freund", "/", "es", "ist", "vo\u00b7ran", "die", "Liebs\u00b7te", "nur", "ge\u00b7schi\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Dies\