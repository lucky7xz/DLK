 "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch diesen Stich ward Amor kl\u00fcger.", "tokens": ["Durch", "die\u00b7sen", "Stich", "ward", "A\u00b7mor", "kl\u00fc\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "NE", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unersch\u00f6pfliche Betr\u00fcger", "tokens": ["Der", "un\u00b7er\u00b7sch\u00f6pf\u00b7li\u00b7che", "Be\u00b7tr\u00fc\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sann einer neuen Kriegslist nach:", "tokens": ["Sann", "ei\u00b7ner", "neu\u00b7en", "Kriegs\u00b7list", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er lauscht' in Rosen und Violen;", "tokens": ["Er", "lauscht'", "in", "Ro\u00b7sen", "und", "Vi\u00b7o\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und kam ein M\u00e4dchen sie zu holen,", "tokens": ["Und", "kam", "ein", "M\u00e4d\u00b7chen", "sie", "zu", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Flog er als Bien' heraus, und stach.", "tokens": ["Flog", "er", "als", "Bi\u00b7en'", "he\u00b7raus", ",", "und", "stach", "."], "token_info": ["word", "word", "word", "wor