{"dta.poem.12474": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Aus den Bergen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jeder Mensch hat seinen Stern,", "tokens": ["Je\u00b7der", "Mensch", "hat", "sei\u00b7nen", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Hofrath seinen,", "tokens": ["Je\u00b7der", "Hof\u00b7rath", "sei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jeder Pudel seinen Kern:", "tokens": ["Je\u00b7der", "Pu\u00b7del", "sei\u00b7nen", "Kern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft auch mir den meinen!", "tokens": ["La\u00dft", "auch", "mir", "den", "mei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "ART", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ward mir leider nicht zu Theil", "tokens": ["Ward", "mir", "lei\u00b7der", "nicht", "zu", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich euch erg\u00f6tze,", "tokens": ["Da\u00df", "ich", "euch", "er\u00b7g\u00f6t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Aber denkt: ich bin ein Keil,", "tokens": ["A\u00b7ber", "denkt", ":", "ich", "bin", "ein", "Keil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil ihr grobe Kl\u00f6tze.", "tokens": ["Weil", "ihr", "gro\u00b7be", "Kl\u00f6t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ja \u2014 ich habe kein Gem\u00fcth", "tokens": ["Ja", "ich", "ha\u00b7be", "kein", "Ge\u00b7m\u00fcth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr der M\u00e4gdlein Wangen,", "tokens": ["F\u00fcr", "der", "M\u00e4gd\u00b7lein", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fcr die Bl\u00fcmchen, die verbl\u00fcht,", "tokens": ["F\u00fcr", "die", "Bl\u00fcm\u00b7chen", ",", "die", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh sie aufgegangen;", "tokens": ["Eh", "sie", "auf\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ja, ich bin ein schlechter Held", "tokens": ["Ja", ",", "ich", "bin", "ein", "schlech\u00b7ter", "Held"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wider T\u00fcrk' und Franken,", "tokens": ["Wi\u00b7der", "T\u00fcrk'", "und", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mache selbst um ", "tokens": ["Ma\u00b7che", "selbst", "um"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR"], "meter": "+---", "measure": "dactylic.init"}, "line.8": {"text": "Mir nicht viel Gedanken.", "tokens": ["Mir", "nicht", "viel", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ich geh\u00f6re zum Verband", "tokens": ["Ich", "ge\u00b7h\u00f6\u00b7re", "zum", "Ver\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller gro\u00dfen Thoren.", "tokens": ["Al\u00b7ler", "gro\u00b7\u00dfen", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heil! wenn unser Vaterland", "tokens": ["Heil", "!", "wenn", "un\u00b7ser", "Va\u00b7ter\u00b7land"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Verstand verloren!", "tokens": ["Den", "Ver\u00b7stand", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn's einmal, ein L\u00f6we noch,", "tokens": ["Wenn's", "ein\u00b7mal", ",", "ein", "L\u00f6\u00b7we", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "ART", "NE", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Seine M\u00e4hne sch\u00fcttelt,", "tokens": ["Sei\u00b7ne", "M\u00e4h\u00b7ne", "sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und am altgewohnten Joch", "tokens": ["Und", "am", "alt\u00b7ge\u00b7wohn\u00b7ten", "Joch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Den Philister r\u00fcttelt!", "tokens": ["Den", "Phi\u00b7lis\u00b7ter", "r\u00fct\u00b7telt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Alle Herzen, stolz und hei\u00df,", "tokens": ["Al\u00b7le", "Her\u00b7zen", ",", "stolz", "und", "hei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "M\u00fcssen dort verbluten;", "tokens": ["M\u00fcs\u00b7sen", "dort", "ver\u00b7blu\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Darum in dies Gletschereis", "tokens": ["Da\u00b7rum", "in", "dies", "Glet\u00b7sche\u00b7reis"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "PDS", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Fl\u00fccht' ich meine Gluten:", "tokens": ["Fl\u00fccht'", "ich", "mei\u00b7ne", "Glu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Droben an des Gie\u00dfbachs Strand,", "tokens": ["Dro\u00b7ben", "an", "des", "Gie\u00df\u00b7bachs", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An des silberhellen,", "tokens": ["An", "des", "sil\u00b7ber\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Jauchz' ich, da\u00df im flachen Land", "tokens": ["Jauch\u00b7z'", "ich", ",", "da\u00df", "im", "fla\u00b7chen", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Euch die Ohren gellen.", "tokens": ["Euch", "die", "Oh\u00b7ren", "gel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Was ihr nur mit Schmach und Tod", "tokens": ["Was", "ihr", "nur", "mit", "Schmach", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wisset zu befehden,", "tokens": ["Wis\u00b7set", "zu", "be\u00b7feh\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Trunken vor dem Morgenroth", "tokens": ["Trun\u00b7ken", "vor", "dem", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf ich's jetzo reden,", "tokens": ["Darf", "ich's", "jet\u00b7zo", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Rufen in den goldnen Tag", "tokens": ["Ru\u00b7fen", "in", "den", "gold\u00b7nen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tief aus Herz und Kehle:", "tokens": ["Tief", "aus", "Herz", "und", "Keh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wo mit unbez\u00e4hmter Lust", "tokens": ["Wo", "mit", "un\u00b7be\u00b7z\u00e4hm\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob den letzten H\u00fctten", "tokens": ["Ob", "den", "letz\u00b7ten", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "D\u00fcrre Felsen aus der Brust", "tokens": ["D\u00fcr\u00b7re", "Fel\u00b7sen", "aus", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewige Str\u00f6me sch\u00fctten;", "tokens": ["E\u00b7wi\u00b7ge", "Str\u00f6\u00b7me", "sch\u00fct\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo in ungez\u00fcgeltem Lauf", "tokens": ["Wo", "in", "un\u00b7ge\u00b7z\u00fc\u00b7gel\u00b7tem", "Lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Noch die Wasser tosen,", "tokens": ["Noch", "die", "Was\u00b7ser", "to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Lad' ich ", "tokens": ["Lad'", "ich"], "token_info": ["word", "word"], "pos": ["VVIMP", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Wilde, wilde Rosen!", "tokens": ["Wil\u00b7de", ",", "wil\u00b7de", "Ro\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Habt da drau\u00dfen manchen Tropf,", "tokens": ["Habt", "da", "drau\u00b7\u00dfen", "man\u00b7chen", "Tropf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mag vor euch zagen;", "tokens": ["Der", "mag", "vor", "euch", "za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ich will trotzig meinen Kopf", "tokens": ["Ich", "will", "trot\u00b7zig", "mei\u00b7nen", "Kopf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Berge tragen.", "tokens": ["Wie", "die", "Ber\u00b7ge", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "O, wie winzig d\u00fcnken mich", "tokens": ["O", ",", "wie", "win\u00b7zig", "d\u00fcn\u00b7ken", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure Sieben-Sachen!", "tokens": ["Eu\u00b7re", "Sie\u00b7ben\u00b7Sa\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wer die Blitze unter sich,", "tokens": ["Wer", "die", "Blit\u00b7ze", "un\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann auch eurer lachen.", "tokens": ["Kann", "auch", "eu\u00b7rer", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}