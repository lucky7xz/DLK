{"dta.poem.11770": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer viele B\u00fccher hat, und keines recht gelesen,", "tokens": ["Wer", "vie\u00b7le", "B\u00fc\u00b7cher", "hat", ",", "und", "kei\u00b7nes", "recht", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VAFIN", "$,", "KON", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist wie ein Geiziger mit seinem Schatz gewesen.", "tokens": ["Ist", "wie", "ein", "Gei\u00b7zi\u00b7ger", "mit", "sei\u00b7nem", "Schatz", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er nutzet nicht sein Gut und vorenth\u00e4lts der Welt;", "tokens": ["Er", "nut\u00b7zet", "nicht", "sein", "Gut", "und", "vor\u00b7ent\u00b7h\u00e4lts", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nur im Umlauf n\u00fctzt die Weisheit und das Geld.", "tokens": ["Denn", "nur", "im", "Um\u00b7lauf", "n\u00fctzt", "die", "Weis\u00b7heit", "und", "das", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie mancher k\u00f6nnte sich vom Abfall dessen m\u00e4sten,", "tokens": ["Wie", "man\u00b7cher", "k\u00f6nn\u00b7te", "sich", "vom", "Ab\u00b7fall", "des\u00b7sen", "m\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "PRF", "APPRART", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was solch ein Magrer hat in Geld- und B\u00fccherk\u00e4sten.", "tokens": ["Was", "solch", "ein", "Mag\u00b7rer", "hat", "in", "Geld", "und", "B\u00fc\u00b7cher\u00b7k\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ART", "NN", "VAFIN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch Weisheit statt vom Buch kann man vom Leben kaufen,", "tokens": ["Doch", "Weis\u00b7heit", "statt", "vom", "Buch", "kann", "man", "vom", "Le\u00b7ben", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "APPRART", "NN", "VMFIN", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Lebensweisheit gar vermi\u00dft nicht Goldes Haufen.", "tokens": ["Und", "Le\u00b7bens\u00b7weis\u00b7heit", "gar", "ver\u00b7mi\u00dft", "nicht", "Gol\u00b7des", "Hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PTKNEG", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}