{"dta.poem.6162": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Geist hingegen kennt nichts, als den Druck allein.", "tokens": ["Der", "Geist", "hin\u00b7ge\u00b7gen", "kennt", "nichts", ",", "als", "den", "Druck", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Regel nun, wodurch ein Ding sich reget,", "tokens": ["Die", "Re\u00b7gel", "nun", ",", "wo\u00b7durch", "ein", "Ding", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die unver\u00e4nderlich und allgemein,", "tokens": ["Die", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "und", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist, da\u00df ein w\u00fcrckend Seyn, und welches sich beweget,", "tokens": ["Ist", ",", "da\u00df", "ein", "w\u00fcr\u00b7ckend", "Seyn", ",", "und", "wel\u00b7ches", "sich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "ADJD", "PPOSAT", "$,", "KON", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Darinn befindlich sey. Ein\u2019Aenderung vom Ort", "tokens": ["Da\u00b7rinn", "be\u00b7find\u00b7lich", "sey", ".", "Ein'\u00b7Aen\u00b7de\u00b7rung", "vom", "Ort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ADJD", "VAFIN", "$.", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird man in ihm gewahr; man sieht darinn so fort", "tokens": ["Wird", "man", "in", "ihm", "ge\u00b7wahr", ";", "man", "sieht", "da\u00b7rinn", "so", "fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "APPR", "PPER", "ADJD", "$.", "PIS", "VVFIN", "PAV", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch eine Ursach des Bewegens,", "tokens": ["Auch", "ei\u00b7ne", "Ur\u00b7sach", "des", "Be\u00b7we\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wovon die Krafft, so \u00e4usserlich nur blos", "tokens": ["Wo\u00b7von", "die", "Krafft", ",", "so", "\u00e4us\u00b7ser\u00b7lich", "nur", "blos"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ADJD", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den C\u00f6rper rege macht, durch seinen Druck und Sto\u00df.", "tokens": ["Den", "C\u00f6r\u00b7per", "re\u00b7ge", "macht", ",", "durch", "sei\u00b7nen", "Druck", "und", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}