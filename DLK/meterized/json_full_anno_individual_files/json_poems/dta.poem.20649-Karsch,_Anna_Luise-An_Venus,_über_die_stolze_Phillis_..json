{"dta.poem.20649": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Venus,  \n \u00fcber die stolze Phillis .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die du den goldnen Apfel hingerissen                 ", "tokens": ["Die", "du", "den", "gold\u00b7nen", "Ap\u00b7fel", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu dir, mit sch\u00f6ner Augen Macht,", "tokens": ["Zu", "dir", ",", "mit", "sch\u00f6\u00b7ner", "Au\u00b7gen", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Venus! du wirst z\u00fcrnen m\u00fcssen;", "tokens": ["O", "Ve\u00b7nus", "!", "du", "wirst", "z\u00fcr\u00b7nen", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr als Ein Apfel ward der Phillis dargebracht.", "tokens": ["Mehr", "als", "Ein", "Ap\u00b7fel", "ward", "der", "Phil\u00b7lis", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VAFIN", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mehr als Ein Sch\u00e4fer wirst sich vor ihr nieder;", "tokens": ["Mehr", "als", "Ein", "Sch\u00e4\u00b7fer", "wirst", "sich", "vor", "ihr", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VAFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein reicher Graf umfa\u00dft ihr Knie", "tokens": ["Ein", "rei\u00b7cher", "Graf", "um\u00b7fa\u00dft", "ihr", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schmeichelt ihr, und bittet wieder", "tokens": ["Und", "schmei\u00b7chelt", "ihr", ",", "und", "bit\u00b7tet", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sonst um Z\u00e4rtlichkeit, um Gegenliebe sie.", "tokens": ["Wie", "sonst", "um", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "um", "Ge\u00b7gen\u00b7lie\u00b7be", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "$,", "KOUI", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "An ihrem andern ausgestreckten Fu\u00dfe", "tokens": ["An", "ih\u00b7rem", "an\u00b7dern", "aus\u00b7ge\u00b7streck\u00b7ten", "Fu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Liegt Deutschlands gr\u00f6\u00dfte S\u00e4ngerin,", "tokens": ["Liegt", "Deutschlands", "gr\u00f6\u00df\u00b7te", "S\u00e4n\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und singet von dem sanften Kusse,", "tokens": ["Und", "sin\u00b7get", "von", "dem", "sanf\u00b7ten", "Kus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ihre Phillis giebt, ein z\u00e4rtlich Lied dahin.", "tokens": ["Den", "ih\u00b7re", "Phil\u00b7lis", "giebt", ",", "ein", "z\u00e4rt\u00b7lich", "Lied", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJD", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Indessen l\u00e4\u00dft die Stolze durch den Diener", "tokens": ["In\u00b7des\u00b7sen", "l\u00e4\u00dft", "die", "Stol\u00b7ze", "durch", "den", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schwarzen Locken wickeln sich,", "tokens": ["Die", "schwar\u00b7zen", "Lo\u00b7cken", "wi\u00b7ckeln", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wird den Graf zu spotten k\u00fchner,", "tokens": ["Und", "wird", "den", "Graf", "zu", "spot\u00b7ten", "k\u00fch\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKZU", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hei\u00dft der S\u00e4ngerin ein h\u00f6hnisch Lied an dich.", "tokens": ["Und", "hei\u00dft", "der", "S\u00e4n\u00b7ge\u00b7rin", "ein", "h\u00f6h\u00b7nisch", "Lied", "an", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJD", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}