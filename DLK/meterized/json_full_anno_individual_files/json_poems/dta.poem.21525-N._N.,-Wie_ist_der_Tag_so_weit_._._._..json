{"dta.poem.21525": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wie ist der Tag so weit . . . .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Im Sclavendienst der L\u00fcge", "tokens": ["Im", "Scla\u00b7ven\u00b7dienst", "der", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab\u2019 ich den Tag verbracht \u2026", "tokens": ["Hab'", "ich", "den", "Tag", "ver\u00b7bracht", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun hat den Zauberschleier leis", "tokens": ["Nun", "hat", "den", "Zau\u00b7ber\u00b7schlei\u00b7er", "leis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herabgesenkt die Nacht.", "tokens": ["Her\u00b7ab\u00b7ge\u00b7senkt", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schweigt vertr\u00e4umt die Runde,", "tokens": ["Es", "schweigt", "ver\u00b7tr\u00e4umt", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur leise der Nachtwind rauscht \u2014", "tokens": ["Nur", "lei\u00b7se", "der", "Nacht\u00b7wind", "rauscht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ich aber mit brennendem Munde", "tokens": ["Ich", "a\u00b7ber", "mit", "bren\u00b7nen\u00b7dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Habe Stunde um Stunde", "tokens": ["Ha\u00b7be", "Stun\u00b7de", "um", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Mit Geistern ans n\u00e4cht\u2019gem Grunde", "tokens": ["Mit", "Geis\u00b7tern", "ans", "n\u00e4cht'\u00b7gem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wilde Zwiesprach getauscht.", "tokens": ["Wil\u00b7de", "Zwie\u00b7sprach", "ge\u00b7tauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}