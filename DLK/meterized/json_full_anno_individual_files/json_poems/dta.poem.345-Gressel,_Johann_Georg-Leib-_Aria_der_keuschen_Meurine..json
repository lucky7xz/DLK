{"dta.poem.345": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Leib- Aria  der keuschen  Meurine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Wollust Rosen weicht", "tokens": ["Ihr", "Wol\u00b7lust", "Ro\u00b7sen", "weicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor meinen Keuschheits-Liljen.", "tokens": ["Vor", "mei\u00b7nen", "Keuschheits\u00b7Lil\u00b7jen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jhr k\u00f6nnt sie nicht vertilgen/", "tokens": ["Ihr", "k\u00f6nnt", "sie", "nicht", "ver\u00b7til\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eur Wesen das erbleicht", "tokens": ["Eur", "We\u00b7sen", "das", "er\u00b7bleicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Davor/ wo diese stehn/", "tokens": ["Da\u00b7vor", "/", "wo", "die\u00b7se", "stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PWAV", "PDS", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da m\u00fcsset ihr vergehn.", "tokens": ["Da", "m\u00fcs\u00b7set", "ihr", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ob schon die Perl gesteckt", "tokens": ["Ob", "schon", "die", "Perl", "ge\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In schwartzen Muschel-Schalen/", "tokens": ["In", "schwart\u00b7zen", "Mu\u00b7schel\u00b7Scha\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So kan sie dennoch prahlen", "tokens": ["So", "kan", "sie", "den\u00b7noch", "prah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn selbige entdeckt.", "tokens": ["Wenn", "sel\u00b7bi\u00b7ge", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Demants helle Pracht", "tokens": ["Des", "De\u00b7mants", "hel\u00b7le", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Aus rauhen Schlacken lacht.", "tokens": ["Aus", "rau\u00b7hen", "Schla\u00b7cken", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und bin ich gleich nicht sch\u00f6n", "tokens": ["Und", "bin", "ich", "gleich", "nicht", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Prang ich mit keinen Br\u00fcsten/", "tokens": ["Prang", "ich", "mit", "kei\u00b7nen", "Br\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die jedermann gel\u00fcsten", "tokens": ["Die", "je\u00b7der\u00b7mann", "ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wird es doch geschehn/", "tokens": ["So", "wird", "es", "doch", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df man der Keuschheit hold", "tokens": ["Da\u00df", "man", "der", "Keuschheit", "hold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und nicht der Sch\u00f6nheit-Gold.", "tokens": ["Und", "nicht", "der", "Sch\u00f6n\u00b7heit\u00b7Gold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}