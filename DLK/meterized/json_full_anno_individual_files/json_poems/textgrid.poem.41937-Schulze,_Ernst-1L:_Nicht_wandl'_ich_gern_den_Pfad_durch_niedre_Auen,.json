{"textgrid.poem.41937": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht wandl' ich gern den Pfad durch niedre Auen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht wandl' ich gern den Pfad durch niedre Auen,", "tokens": ["Nicht", "wandl'", "ich", "gern", "den", "Pfad", "durch", "nied\u00b7re", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Menschensinn nach Maa\u00df und Schnur erdacht;", "tokens": ["Den", "Men\u00b7schen\u00b7sinn", "nach", "Maa\u00df", "und", "Schnur", "er\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Viel s\u00fc\u00dfer ist's, der irren Laune Macht", "tokens": ["Viel", "s\u00fc\u00b7\u00dfer", "ist's", ",", "der", "ir\u00b7ren", "Lau\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf eigner Bahn sich wagend zu vertrauen.", "tokens": ["Auf", "eig\u00b7ner", "Bahn", "sich", "wa\u00b7gend", "zu", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum schweif' ich hier, wo k\u00fchne Geyer bauen,", "tokens": ["Drum", "schweif'", "ich", "hier", ",", "wo