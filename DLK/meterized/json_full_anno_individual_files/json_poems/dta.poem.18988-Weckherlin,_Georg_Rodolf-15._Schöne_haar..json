{"dta.poem.18988": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "15.  \n  Sch\u00f6ne haar.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Oder Lieb liebste garn/ der Sch\u00f6nsten sch\u00f6nste", "tokens": ["O\u00b7der", "Lieb", "liebs\u00b7te", "garn", "/", "der", "Sch\u00f6ns\u00b7ten", "sch\u00f6ns\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "haar/", "tokens": ["haar", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wan schertzend in dem lufft jhr schon bandlo\u00df", "tokens": ["Wan", "schert\u00b7zend", "in", "dem", "lufft", "jhr", "schon", "ban\u00b7dlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "vmbflieget/", "tokens": ["vmb\u00b7flie\u00b7get", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Befind ich doch al\u00dfbald/ da\u00df jhr mein hertz be-", "tokens": ["Be\u00b7find", "ich", "doch", "al\u00df\u00b7bald", "/", "da\u00df", "jhr", "mein", "hertz", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "trieget/", "tokens": ["trie\u00b7get", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Vnd da\u00df je freyer jhr/ je gr\u00f6sser mein gefahr.", "tokens": ["Vnd", "da\u00df", "je", "frey\u00b7er", "jhr", "/", "je", "gr\u00f6s\u00b7ser", "mein", "ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "PPOSAT", "$(", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O goldflu\u00df blaich vnd reich/ Goldstriemen wahr", "tokens": ["O", "gold\u00b7flu\u00df", "blaich", "vnd", "reich", "/", "Gold\u00b7strie\u00b7men", "wahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$(", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vnd klar/", "tokens": ["vnd", "klar", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wan euch jhr weisse hand in taussent ringlein", "tokens": ["Wan", "euch", "jhr", "weis\u00b7se", "hand", "in", "taus\u00b7sent", "rin\u00b7glein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "bieget/", "tokens": ["bie\u00b7get", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Befind ich auch al\u00dfbald/ da\u00df jhr mein hertz be-", "tokens": ["Be\u00b7find", "ich", "auch", "al\u00df\u00b7bald", "/", "da\u00df", "jhr", "mein", "hertz", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "krieget/", "tokens": ["krie\u00b7get", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Vnd je mehr ewre kn\u00f6pff/ je mehr ich strick erfahr.", "tokens": ["Vnd", "je", "mehr", "ew\u00b7re", "kn\u00f6pff", "/", "je", "mehr", "ich", "strick", "er\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "NN", "$(", "ADV", "ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwar wie solt dises garn doch meine sehl verdriessen?", "tokens": ["Zwar", "wie", "solt", "di\u00b7ses", "garn", "doch", "mei\u00b7ne", "sehl", "ver\u00b7dries\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VMFIN", "PDS", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist ein hertz in der welt das disem schatz nicht hold?", "tokens": ["Ist", "ein", "hertz", "in", "der", "welt", "das", "di\u00b7sem", "schatz", "nicht", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wer wolt nicht einen strom von gold gern sehen", "tokens": ["Wer", "wolt", "nicht", "ei\u00b7nen", "strom", "von", "gold", "gern", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "fliessen?", "tokens": ["flies\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "O reiche haar/ zugleich der Freyhet strick vnd sold/", "tokens": ["O", "rei\u00b7che", "haar", "/", "zu\u00b7gleich", "der", "Frey\u00b7het", "strick", "vnd", "sold", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ADV", "ART", "NN", "ADJD", "KON", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie jhr/ als der Lieb strick/ mich pfleget zu be-", "tokens": ["Wie", "jhr", "/", "als", "der", "Lieb", "strick", "/", "mich", "pfle\u00b7get", "zu", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$(", "KOUS", "ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "TRUNC"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "schliessen/", "tokens": ["schlies\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Also belohnet jhr mich auch mit bestem gold.", "tokens": ["Al\u00b7so", "be\u00b7loh\u00b7net", "jhr", "mich", "auch", "mit", "bes\u00b7tem", "gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}