{"dta.poem.1648": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "RaIMUNDUS.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Izt mag die alte Welt von Sieben Wundern schweigen/", "tokens": ["Izt", "mag", "die", "al\u00b7te", "Welt", "von", "Sie\u00b7ben", "Wun\u00b7dern", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Rom nicht mehr wie vor die stoltzen Berge zeigen:", "tokens": ["Und", "Rom", "nicht", "mehr", "wie", "vor", "die", "stolt\u00b7zen", "Ber\u00b7ge", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PTKNEG", "ADV", "KOKOM", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gradivus r\u00fchme nicht die Wercke seiner Hand/", "tokens": ["Gra\u00b7di\u00b7vus", "r\u00fch\u00b7me", "nicht", "die", "Wer\u00b7cke", "sei\u00b7ner", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Die l\u00e4ngstens sind verst\u00f6rt durch Alter/ Raub und Brand.", "tokens": ["Die", "l\u00e4ngs\u00b7tens", "sind", "ver\u00b7st\u00f6rt", "durch", "Al\u00b7ter", "/", "Raub", "und", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ists nicht ein gr\u00f6sser Ruhm/ wenn man Venedig schaut/", "tokens": ["Ists", "nicht", "ein", "gr\u00f6s\u00b7ser", "Ruhm", "/", "wenn", "man", "Ve\u00b7ne\u00b7dig", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "ADJA", "NN", "$(", "KOUS", "PIS", "NE", "VVFIN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df Menschen/ G\u00f6tter nicht/ di\u00df edle Werck gebaut.", "tokens": ["Da\u00df", "Men\u00b7schen", "/", "G\u00f6t\u00b7ter", "nicht", "/", "di\u00df", "ed\u00b7le", "Werck", "ge\u00b7baut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "PTKNEG", "$(", "PDS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}