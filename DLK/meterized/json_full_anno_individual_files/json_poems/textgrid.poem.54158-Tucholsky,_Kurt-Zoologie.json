{"textgrid.poem.54158": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Zoologie", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Borvaselinchen lief, von Gott gesandt,", "tokens": ["Ein", "Bor\u00b7va\u00b7se\u00b7lin\u00b7chen", "lief", ",", "von", "Gott", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "durch deutsches Land.", "tokens": ["durch", "deut\u00b7sches", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es gl\u00e4nzte fettig-hell im Sonnenscheine", "tokens": ["Es", "gl\u00e4nz\u00b7te", "fet\u00b7tig\u00b7hell", "im", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und r\u00fchrte emsig seine kleinen Beine.", "tokens": ["und", "r\u00fchr\u00b7te", "em\u00b7sig", "sei\u00b7ne", "klei\u00b7nen", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch gestern morgen in der Abendstunde,", "tokens": ["Doch", "ge\u00b7stern", "mor\u00b7gen", "in", "der", "A\u00b7bends\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "verschwand es still