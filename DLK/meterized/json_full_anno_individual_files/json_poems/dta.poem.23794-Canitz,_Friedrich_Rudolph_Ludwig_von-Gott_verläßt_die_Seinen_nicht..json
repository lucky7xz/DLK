{"dta.poem.23794": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Gott verl\u00e4\u00dft die Seinen nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Soll mich die Hand des HErren ewig dr\u00fccken?", "tokens": ["Soll", "mich", "die", "Hand", "des", "Her\u00b7ren", "e\u00b7wig", "dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verfolgt Er mich als einen Feind?", "tokens": ["Ver\u00b7folgt", "Er", "mich", "als", "ei\u00b7nen", "Feind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll ich forthin sonst keinen Stern erblicken/", "tokens": ["Soll", "ich", "for\u00b7thin", "sonst", "kei\u00b7nen", "Stern", "er\u00b7bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als der mich schreckt/ und mir zum Falle scheint?", "tokens": ["Als", "der", "mich", "schreckt", "/", "und", "mir", "zum", "Fal\u00b7le", "scheint", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "VVFIN", "$(", "KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Soll denn mein Kelch nach nichts als Galle schmecken/", "tokens": ["Soll", "denn", "mein", "Kelch", "nach", "nichts", "als", "Gal\u00b7le", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "APPR", "PIS", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und eine stete Nacht des Traurens mich bedencken?", "tokens": ["Und", "ei\u00b7ne", "ste\u00b7te", "Nacht", "des", "Trau\u00b7rens", "mich", "be\u00b7den\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sonst donnert Er allein mit seinem Wetter/", "tokens": ["Sonst", "don\u00b7nert", "Er", "al\u00b7lein", "mit", "sei\u00b7nem", "Wet\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das voller Tod und Flammen ist/", "tokens": ["Das", "vol\u00b7ler", "Tod", "und", "Flam\u00b7men", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf das Geschlecht der unbekehrten Sp\u00f6tter/", "tokens": ["Auf", "das", "Ge\u00b7schlecht", "der", "un\u00b7be\u00b7kehr\u00b7ten", "Sp\u00f6t\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und schonet den der Ihm die Ruthe k\u00fc\u00dft;", "tokens": ["Und", "scho\u00b7net", "den", "der", "Ihm", "die", "Ru\u00b7the", "k\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sonst pflegt Er nur die Kinder zu bedr\u00e4uen/", "tokens": ["Sonst", "pflegt", "Er", "nur", "die", "Kin\u00b7der", "zu", "be\u00b7dr\u00e4u\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich aber soll \u00fcmsonst nach seiner H\u00fclffe schreyen.", "tokens": ["Ich", "a\u00b7ber", "soll", "\u00fcm\u00b7sonst", "nach", "sei\u00b7ner", "H\u00fclf\u00b7fe", "schre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.3": {"line.1": {"text": "Doch nein/ ich wei\u00df da\u00df Er in meinen N\u00f6then/", "tokens": ["Doch", "nein", "/", "ich", "wei\u00df", "da\u00df", "Er", "in", "mei\u00b7nen", "N\u00f6\u00b7then", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$(", "PPER", "VVFIN", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf jeden Seuftzer Achtung giebt.", "tokens": ["Auf", "je\u00b7den", "Seuft\u00b7zer", "Ach\u00b7tung", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm trau ich fest/ und solt Er mich gleich t\u00f6dten.", "tokens": ["Ihm", "trau", "ich", "fest", "/", "und", "solt", "Er", "mich", "gleich", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df da\u00df Er die Seinen hertzlich liebt/", "tokens": ["Ich", "wei\u00df", "da\u00df", "Er", "die", "Sei\u00b7nen", "hertz\u00b7lich", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "ART", "PPOSS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df Ihm so viel an meinem Thun gelegen/", "tokens": ["Da\u00df", "Ihm", "so", "viel", "an", "mei\u00b7nem", "Thun", "ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als dort den gantzen Bau des Himmels zu bewegen.", "tokens": ["Als", "dort", "den", "gant\u00b7zen", "Bau", "des", "Him\u00b7mels", "zu", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von Kindheit an hab\u2019 ich in grosser Menge", "tokens": ["Von", "Kind\u00b7heit", "an", "hab'", "ich", "in", "gros\u00b7ser", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APZR", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Proben seiner Huld gesp\u00fcrt/", "tokens": ["Die", "Pro\u00b7ben", "sei\u00b7ner", "Huld", "ge\u00b7sp\u00fcrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat mich offt durch unbekandte G\u00e4nge", "tokens": ["Er", "hat", "mich", "offt", "durch", "un\u00b7be\u00b7kand\u00b7te", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sehr wunderlich/ doch immer wol gef\u00fchrt;", "tokens": ["Sehr", "wun\u00b7der\u00b7lich", "/", "doch", "im\u00b7mer", "wol", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hab\u2019 ich nicht offt wenn aller Trost verschwunden/", "tokens": ["Hab'", "ich", "nicht", "offt", "wenn", "al\u00b7ler", "Trost", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "KOUS", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Artzney bey dem Gift/ un\u0303 Gl\u00fcck im Sturm gefunden?", "tokens": ["Die", "Artz\u00b7ney", "bey", "dem", "Gift", "/", "u\u00f1", "Gl\u00fcck", "im", "Sturm", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "KON", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So raset nun ihr Wellen und ihr Winde!", "tokens": ["So", "ra\u00b7set", "nun", "ihr", "Wel\u00b7len", "und", "ihr", "Win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey mir entsteht kein Zweifel-Muth/", "tokens": ["Bey", "mir", "ent\u00b7steht", "kein", "Zwei\u00b7fel\u00b7Muth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dieweil ich mich in Sicherheit befinde/", "tokens": ["Die\u00b7weil", "ich", "mich", "in", "Si\u00b7cher\u00b7heit", "be\u00b7fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn euer HErr in meinem Schifflein ruht/", "tokens": ["Wenn", "eu\u00b7er", "Herr", "in", "mei\u00b7nem", "Schif\u00b7flein", "ruht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fang\u2019t immer an aufs heftigste zu w\u00fcten!", "tokens": ["Fang't", "im\u00b7mer", "an", "aufs", "hef\u00b7tigs\u00b7te", "zu", "w\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er kan\u2019s mit einem Winck euch wiederum verbieten", "tokens": ["Er", "kan's", "mit", "ei\u00b7nem", "Win\u00b7ck", "euch", "wie\u00b7de\u00b7rum", "ver\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Des H\u00f6chsten Schlu\u00df und heimliche Gerichte", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Schlu\u00df", "und", "heim\u00b7li\u00b7che", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bet\u2019 ich in stiller Demuth an/", "tokens": ["Bet'", "ich", "in", "stil\u00b7ler", "De\u00b7muth", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bau\u2019t vielleicht mit zornigem Gesichte", "tokens": ["Er", "bau't", "viel\u00b7leicht", "mit", "zor\u00b7ni\u00b7gem", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein frohes Werck/ das mich erg\u00f6tzen kan.", "tokens": ["Ein", "fro\u00b7hes", "Werck", "/", "das", "mich", "er\u00b7g\u00f6t\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was sich kein Witz zu \u00e4ndern darf erk\u00fchnen/", "tokens": ["Was", "sich", "kein", "Witz", "zu", "\u00e4n\u00b7dern", "darf", "er\u00b7k\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PIAT", "NN", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dazu wird mir Gedult vielmehr als Murren dienen.", "tokens": ["Da\u00b7zu", "wird", "mir", "Ge\u00b7dult", "viel\u00b7mehr", "als", "Mur\u00b7ren", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "ADV", "KOUS", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Es ist mir schon genug da\u00df diese Plage", "tokens": ["Es", "ist", "mir", "schon", "ge\u00b7nug", "da\u00df", "die\u00b7se", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf meiner Seelen Wolfahrt zielt/", "tokens": ["Auf", "mei\u00b7ner", "See\u00b7len", "Wol\u00b7fahrt", "zielt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df im Lauf und Wechsel meiner Tage", "tokens": ["Und", "da\u00df", "im", "Lauf", "und", "Wech\u00b7sel", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht ungefehr ein blinder Zufall spielt/", "tokens": ["Nicht", "un\u00b7ge\u00b7fehr", "ein", "blin\u00b7der", "Zu\u00b7fall", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die rechte Zeit hat GOtt schon abgemessen/", "tokens": ["Die", "rech\u00b7te", "Zeit", "hat", "Gott", "schon", "ab\u00b7ge\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er wil bald seinen Grim\u0303/ ich bald mein Leyd vergessen.", "tokens": ["Er", "wil", "bald", "sei\u00b7nen", "Grim\u0303", "/", "ich", "bald", "mein", "Leyd", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "$(", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}