{"dta.poem.942": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Mit frohem grauen haben wir im sp&#228;ten               ", "tokens": ["Mit", "fro\u00b7hem", "grau\u00b7en", "ha\u00b7ben", "wir", "im", "sp", "&#228;", "ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ADJA", "ADJA", "VAFIN", "PPER", "APPRART", "FM", "FM", "FM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mondabend oft denselben weg begonnen               ", "tokens": ["Mond\u00b7a\u00b7bend", "oft", "den\u00b7sel\u00b7ben", "weg", "be\u00b7gon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "PDS", "ADV", "VVPP"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Als ob von feuchten bl&#252;ten ganz beronnen               ", "tokens": ["Als", "ob", "von", "feuch\u00b7ten", "bl", "&#252;", "ten", "ganz", "be\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "FM", "FM", "FM", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir in den alten wald der sage tr&#228;ten             ", "tokens": ["Wir", "in", "den", "al\u00b7ten", "wald", "der", "sa\u00b7ge", "tr", "&#228;", "ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du f&#252;hrtest mich zu den verwunschnen thalen               ", "tokens": ["Du", "f", "&#252;", "hr\u00b7test", "mich", "zu", "den", "ver\u00b7wun\u00b7schnen", "tha\u00b7len"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Von nackter helle und von blassen d&#252;ften               ", "tokens": ["Von", "nack\u00b7ter", "hel\u00b7le", "und", "von", "blas\u00b7sen", "d", "&#252;", "ften"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ADJA", "ADJA", "KON", "APPR", "ADJA", "XY", "XY", "XY"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und zeigtest mir von weitem wo aus gr&#252;ften               ", "tokens": ["Und", "zeig\u00b7test", "mir", "von", "wei\u00b7tem", "wo", "aus", "gr", "&#252;", "ften"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "PWAV", "APPR", "NE", "$(", "VVFIN"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die tr&#252;be liebe w&#228;chst im reif der qualen.             ", "tokens": ["Die", "tr", "&#252;", "be", "lie\u00b7be", "w", "&#228;", "chst", "im", "reif", "der", "qua\u00b7len", "."], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "APPRART", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}