{"textgrid.poem.37289": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[mein kleinster Fehler ist der Neid]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein kleinster Fehler ist der Neid. \u2013", "tokens": ["Mein", "kleins\u00b7ter", "Feh\u00b7ler", "ist", "der", "Neid", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufrichtigkeit, Bescheidenheit,", "tokens": ["Auf\u00b7rich\u00b7tig\u00b7keit", ",", "Be\u00b7schei\u00b7den\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dienstfertigkeit und Fr\u00f6mmigkeit,", "tokens": ["Dienst\u00b7fer\u00b7tig\u00b7keit", "und", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Obschon es herrlich sch\u00f6ne Gaben,", "tokens": ["Ob\u00b7schon", "es", "herr\u00b7lich", "sch\u00f6\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die g\u00f6nn' ich allen, die sie haben.", "tokens": ["Die", "g\u00f6nn'", "ich", "al\u00b7len", ",", "die", "sie", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur wenn ich sehe, da\u00df der Schlechte", "tokens": ["Nur", "wenn", "ich", "se\u00b7he", ",", "da\u00df", "der", "Schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das kriegt, was ich gern selber m\u00f6chte;", "tokens": ["Das", "kriegt", ",", "was", "ich", "gern", "sel\u00b7ber", "m\u00f6ch\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur wenn ich leider in der N\u00e4he", "tokens": ["Nur", "wenn", "ich", "lei\u00b7der", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So viele b\u00f6se Menschen sehe,", "tokens": ["So", "vie\u00b7le", "b\u00f6\u00b7se", "Men\u00b7schen", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und wenn ich dann so oft bemerke,", "tokens": ["Und", "wenn", "ich", "dann", "so", "oft", "be\u00b7mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie sie durch sittenlose Werke", "tokens": ["Wie", "sie", "durch", "sit\u00b7ten\u00b7lo\u00b7se", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den lasterhaften Leib erg\u00f6tzen,", "tokens": ["Den", "las\u00b7ter\u00b7haf\u00b7ten", "Leib", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das freilich tut mich tief verletzen.", "tokens": ["Das", "frei\u00b7lich", "tut", "mich", "tief", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sonst, wie gesagt, bin ich hienieden", "tokens": ["Sonst", ",", "wie", "ge\u00b7sagt", ",", "bin", "ich", "hien\u00b7ie\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "VVPP", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Gottlobunddank so recht zufrieden.", "tokens": ["Gott\u00b7lo\u00b7bund\u00b7dank", "so", "recht", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein kleinster Fehler ist der Neid. \u2013", "tokens": ["Mein", "kleins\u00b7ter", "Feh\u00b7ler", "ist", "der", "Neid", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufrichtigkeit, Bescheidenheit,", "tokens": ["Auf\u00b7rich\u00b7tig\u00b7keit", ",", "Be\u00b7schei\u00b7den\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dienstfertigkeit und Fr\u00f6mmigkeit,", "tokens": ["Dienst\u00b7fer\u00b7tig\u00b7keit", "und"