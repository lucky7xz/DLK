{"dta.poem.14437": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch h\u00f6r' ich wunderbare Stimmen sprechen:", "tokens": ["Doch", "h\u00f6r'", "ich", "wun\u00b7der\u00b7ba\u00b7re", "Stim\u00b7men", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201edie Perlen, die Du treu geweint im Schmerze,", "tokens": ["\u201e", "die", "Per\u00b7len", ",", "die", "Du", "treu", "ge\u00b7weint", "im", "Schmer\u00b7ze", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wird sie sorglich all' zusammenbinden,", "tokens": ["Sie", "wird", "sie", "sorg\u00b7lich", "all'", "zu\u00b7sam\u00b7men\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}