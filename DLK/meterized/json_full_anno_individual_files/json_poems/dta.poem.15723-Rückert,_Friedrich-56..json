{"dta.poem.15723": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erst vom Bed\u00fcrfnis gehn die K\u00fcnste aus zumeist,", "tokens": ["Erst", "vom", "Be\u00b7d\u00fcrf\u00b7nis", "gehn", "die", "K\u00fcns\u00b7te", "aus", "zu\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werden Ueppigkeit alsdann, und endlich Geist.", "tokens": ["Und", "wer\u00b7den", "Uep\u00b7pig\u00b7keit", "als\u00b7dann", ",", "und", "end\u00b7lich", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "$,", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bekleidung war zuerst Schutz gegen Witterung,", "tokens": ["Be\u00b7klei\u00b7dung", "war", "zu\u00b7erst", "Schutz", "ge\u00b7gen", "Wit\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann kam Kunstweberei, Sch\u00f6nf\u00e4rberei in Schwung.", "tokens": ["Dann", "kam", "Kunst\u00b7we\u00b7be\u00b7rei", ",", "Sch\u00f6n\u00b7f\u00e4r\u00b7be\u00b7rei", "in", "Schwung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun im Gewand der Mod' ist Sch\u00f6nheit selbst erschienen,", "tokens": ["Nun", "im", "Ge\u00b7wand", "der", "Mod'", "ist", "Sch\u00f6n\u00b7heit", "selbst", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VAFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihr, der ewigen, die Formen wechselnd dienen.", "tokens": ["Da\u00df", "ihr", ",", "der", "e\u00b7wi\u00b7gen", ",", "die", "For\u00b7men", "wech\u00b7selnd", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die H\u00fctte ward ein Haus, das Haus ward ein Palast,", "tokens": ["Die", "H\u00fct\u00b7te", "ward", "ein", "Haus", ",", "das", "Haus", "ward", "ein", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Tempel, wo die Kunst das G\u00f6ttliche umfa\u00dft.", "tokens": ["Ein", "Tem\u00b7pel", ",", "wo", "die", "Kunst", "das", "G\u00f6tt\u00b7li\u00b7che", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Feldmessung war zuerst Erfindung geiz'ger Br\u00fcder,", "tokens": ["Feld\u00b7mes\u00b7sung", "war", "zu\u00b7erst", "Er\u00b7fin\u00b7dung", "gei\u00b7z'\u00b7ger", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Zu theilen unter sich ganz gleich des Vaters G\u00fcter.", "tokens": ["Zu", "thei\u00b7len", "un\u00b7ter", "sich", "ganz", "gleich", "des", "Va\u00b7ters", "G\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PRF", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Me\u00dfschnur ward auf ein erobert Land gez\u00fcckt,", "tokens": ["Die", "Me\u00df\u00b7schnur", "ward", "auf", "ein", "er\u00b7o\u00b7bert", "Land", "ge\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "VVPP", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stellte Grenzen her, wenn sie der Strom verr\u00fcckt.", "tokens": ["Und", "stell\u00b7te", "Gren\u00b7zen", "her", ",", "wenn", "sie", "der", "Strom", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zuletzt ward sie auf Erd' und Himmel ausgedehnt,", "tokens": ["Zu\u00b7letzt", "ward", "sie", "auf", "Erd'", "und", "Him\u00b7mel", "aus\u00b7ge\u00b7dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Unerme\u00dfliches der Geist zu messen w\u00e4hnt.", "tokens": ["Wo", "Un\u00b7er\u00b7me\u00df\u00b7li\u00b7ches", "der", "Geist", "zu", "mes\u00b7sen", "w\u00e4hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}