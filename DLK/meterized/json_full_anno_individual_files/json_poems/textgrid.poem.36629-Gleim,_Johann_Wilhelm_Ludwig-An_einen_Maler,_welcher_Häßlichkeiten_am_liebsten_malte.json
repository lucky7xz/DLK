{"textgrid.poem.36629": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An einen Maler, welcher H\u00e4\u00dflichkeiten am liebsten malte", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine Grazie an eine Rose riecht,", "tokens": ["Wie", "ei\u00b7ne", "Gra\u00b7zie", "an", "ei\u00b7ne", "Ro\u00b7se", "riecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mal' uns! und, mal' uns nicht, wie eine Kr\u00f6te kriecht!", "tokens": ["Mal'", "uns", "!", "und", ",", "mal'", "uns", "nicht", ",", "wie", "ei\u00b7ne", "Kr\u00f6\u00b7te", "kriecht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "KON", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey Sch\u00f6pfer nur von sch\u00f6nen Bildern!", "tokens": ["Sey", "Sch\u00f6p\u00b7fer", "nur", "von", "sch\u00f6\u00b7nen", "Bil\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr, Schild'rer der Natur! m\u00fc\u00dft H\u00e4\u00dfliches nicht schildern,", "tokens": ["Ihr", ",", "Schild'\u00b7rer", "der", "Na\u00b7tur", "!", "m\u00fc\u00dft", "H\u00e4\u00df\u00b7li\u00b7ches", "nicht", "schil\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$.", "VMFIN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es w\u00e4re denn, da\u00df ihr der Ehre mehr, als Gold", "tokens": ["Es", "w\u00e4\u00b7re", "denn", ",", "da\u00df", "ihr", "der", "Eh\u00b7re", "mehr", ",", "als", "Gold"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Euch gern erwerben wollt!", "tokens": ["Euch", "gern", "er\u00b7wer\u00b7ben", "wollt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie eine Grazie an eine Rose riecht,", "tokens": ["Wie", "ei\u00b7ne", "Gra\u00b7zie", "an", "ei\u00b7ne", "Ro\u00b7se", "riecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mal' uns! und, mal' uns nicht, wie eine Kr\u00f6te kriecht!", "tokens": ["Mal'", "uns", "!", "und", ",", "mal'", "uns", "nicht", ",", "wie", "ei\u00b7ne", "Kr\u00f6\u00b7te", "kriecht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "KON", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey Sch\u00f6pfer nur von sch\u00f6nen Bildern!", "tokens": ["Sey", "Sch\u00f6p\u00b7fer", "nur", "von", "sch\u00f6\u00b7nen", "Bil\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN