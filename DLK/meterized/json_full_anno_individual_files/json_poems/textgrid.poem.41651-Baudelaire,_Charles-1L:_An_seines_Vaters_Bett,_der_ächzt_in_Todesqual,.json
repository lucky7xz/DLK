{"textgrid.poem.41651": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: An seines Vaters Bett, der \u00e4chzt in Todesqual,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An seines Vaters Bett, der \u00e4chzt in Todesqual,", "tokens": ["An", "sei\u00b7nes", "Va\u00b7ters", "Bett", ",", "der", "\u00e4chzt", "in", "To\u00b7des\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spricht sinnend Harpagon vor diesen spitzen Z\u00fcgen:", "tokens": ["Spricht", "sin\u00b7nend", "Har\u00b7pa\u00b7gon", "vor", "die\u00b7sen", "spit\u00b7zen", "Z\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NE", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Speicher haben in gen\u00fcgend gro\u00dfer Zahl", "tokens": ["Im", "Spei\u00b7cher", "ha\u00b7ben", "in", "ge\u00b7n\u00fc\u00b7gend", "gro\u00b7\u00dfer", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir alte Bretter doch liegen?", "tokens": ["Wir", "al\u00b7te", "Bret\u00b7ter", "doch", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und Celimene gurrt: Mein Herz ist gut und weich,", "tokens": ["Und", "Ce\u00b7li\u00b7me\u00b7ne", "gurrt", ":", "Mein", "Herz", "ist", "gut", "und", "weich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Sch\u00f6nheit gab mir Gott, die mir gar lieb und teuer.", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "gab", "mir", "Gott", ",", "die", "mir", "gar", "lieb", "und", "teu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Herz! Ein hartes Herz, verrauchtem Schinken gleich,", "tokens": ["Ihr", "Herz", "!", "Ein", "har\u00b7tes", "Herz", ",", "ver\u00b7rauch\u00b7tem", "Schin\u00b7ken", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verdorrt in dem ewigen Feuer.", "tokens": ["Ver\u00b7dorrt", "in", "dem", "e\u00b7wi\u00b7gen", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Ein stumpfer Schreiber, der f\u00fcr einen Geist sich h\u00e4lt,", "tokens": ["Ein", "stump\u00b7fer", "Schrei\u00b7ber", ",", "der", "f\u00fcr", "ei\u00b7nen", "Geist", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sagt zu dem Armen, den er stie\u00df in Wind und Wetter:", "tokens": ["Sagt", "zu", "dem", "Ar\u00b7men", ",", "den", "er", "stie\u00df", "in", "Wind", "und", "Wet\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sag an, wo siehst du ihn, den Sch\u00f6pfer deiner Welt,", "tokens": ["Sag", "an", ",", "wo", "siehst", "du", "ihn", ",", "den", "Sch\u00f6p\u00b7fer", "dei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Deinen g\u00fctigen Herrn und Erretter?", "tokens": ["Dei\u00b7nen", "g\u00fc\u00b7ti\u00b7gen", "Herrn", "und", "Er\u00b7ret\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wohl mehr als alle Welt kenn einen W\u00fcstling ich,", "tokens": ["Wohl", "mehr", "als", "al\u00b7le", "Welt", "kenn", "ei\u00b7nen", "W\u00fcst\u00b7ling", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "PIAT", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, g\u00e4hnend Tag und Nacht, mit kl\u00e4glichen Geb\u00e4rden", "tokens": ["Der", ",", "g\u00e4h\u00b7nend", "Tag", "und", "Nacht", ",", "mit", "kl\u00e4g\u00b7li\u00b7chen", "Ge\u00b7b\u00e4r\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelobt, der schwache Narr: Ach glaubt mir, sicherlich", "tokens": ["Ge\u00b7lobt", ",", "der", "schwa\u00b7che", "Narr", ":", "Ach", "glaubt", "mir", ",", "si\u00b7cher\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "ART", "ADJA", "NN", "$.", "ITJ", "VVFIN", "PPER", "$,", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will ich morgen tugendhaft werden.", "tokens": ["Will", "ich", "mor\u00b7gen", "tu\u00b7gend\u00b7haft", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbnun ist der Frevler reif\u00ab tickt unheilvoll die Uhr,", "tokens": ["\u00bb", "nun", "ist", "der", "Frev\u00b7ler", "reif", "\u00ab", "tickt", "un\u00b7heil\u00b7voll", "die", "Uhr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "ADJD", "$(", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umsonst, da\u00df warnend ichs dem kranken Fleische sagte,", "tokens": ["Um\u00b7sonst", ",", "da\u00df", "war\u00b7nend", "ichs", "dem", "kran\u00b7ken", "Flei\u00b7sche", "sag\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADJD", "PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mensch ist taub und blind, so schwach sind Mauern nur,", "tokens": ["Der", "Mensch", "ist", "taub", "und", "blind", ",", "so", "schwach", "sind", "Mau\u00b7ern", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ein Wurm bewohnt' und zernagte.", "tokens": ["Die", "ein", "Wurm", "be\u00b7wohnt'", "und", "zer\u00b7nag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und j\u00e4h ist Einer da, der stets geleugnet ward,", "tokens": ["Und", "j\u00e4h", "ist", "Ei\u00b7ner", "da", ",", "der", "stets", "ge\u00b7leug\u00b7net", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIS", "ADV", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und spricht mit stolzem Hohn: \u00bbGlaubt nicht, da\u00df ich vergesse,", "tokens": ["Und", "spricht", "mit", "stol\u00b7zem", "Hohn", ":", "\u00bb", "Glaubt", "nicht", ",", "da\u00df", "ich", "ver\u00b7ges\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$.", "$(", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie um die Hostie ihr euch freudig oft geschart,", "tokens": ["Wie", "um", "die", "Hos\u00b7tie", "ihr", "euch", "freu\u00b7dig", "oft", "ge\u00b7schart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur Feier der schwarzen Messe!", "tokens": ["Zur", "Fei\u00b7er", "der", "schwar\u00b7zen", "Mes\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Ein jeglicher von euch gab mir sein Herz zum Thron,", "tokens": ["Ein", "jeg\u00b7li\u00b7cher", "von", "euch", "gab", "mir", "sein", "Herz", "zum", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verruchte, ihr seid mein, durch K\u00fcsse tiefabscheulich.", "tokens": ["Ver\u00b7ruch\u00b7te", ",", "ihr", "seid", "mein", ",", "durch", "K\u00fcs\u00b7se", "tie\u00b7fab\u00b7scheu\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lernt Satan kennen nun an seinem Siegerhohn,", "tokens": ["Lernt", "Sa\u00b7tan", "ken\u00b7nen", "nun", "an", "sei\u00b7nem", "Sie\u00b7ger\u00b7hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie die Welt gigantisch und greulich!", "tokens": ["Wie", "die", "Welt", "gi\u00b7gan\u00b7tisch", "und", "greu\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Best\u00fcrztes Heuchlerpack! Wer ist, der glauben kann,", "tokens": ["Be\u00b7st\u00fcrz\u00b7tes", "Heuc\u00b7hler\u00b7pack", "!", "Wer", "ist", ",", "der", "glau\u00b7ben", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VAFIN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man den Herren h\u00f6hnt und f\u00e4ngt in einem Netze,", "tokens": ["Da\u00df", "man", "den", "Her\u00b7ren", "h\u00f6hnt", "und", "f\u00e4ngt", "in", "ei\u00b7nem", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df der Mensch zugleich zwei Preise je gewann,", "tokens": ["Und", "da\u00df", "der", "Mensch", "zu\u00b7gleich", "zwei", "Prei\u00b7se", "je", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "CARD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Himmel und irdische Sch\u00e4tze?", "tokens": ["Den", "Him\u00b7mel", "und", "ir\u00b7di\u00b7sche", "Sch\u00e4t\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Es ziemt sich, da\u00df das Wild bezahlt den J\u00e4ger macht,", "tokens": ["Es", "ziemt", "sich", ",", "da\u00df", "das", "Wild", "be\u00b7zahlt", "den", "J\u00e4\u00b7ger", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der auf dem Anstand lang gelauert auf die Beute,", "tokens": ["Der", "auf", "dem", "An\u00b7stand", "lang", "ge\u00b7lau\u00b7ert", "auf", "die", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun trage ich euch fort durch sonnenleere Nacht,", "tokens": ["Nun", "tra\u00b7ge", "ich", "euch", "fort", "durch", "son\u00b7nen\u00b7lee\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Meiner traurigen Lust Geleite,", "tokens": ["Mei\u00b7ner", "trau\u00b7ri\u00b7gen", "Lust", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Durch Erde und Gefels, durch mittern\u00e4chtges Graun,", "tokens": ["Durch", "Er\u00b7de", "und", "Ge\u00b7fels", ",", "durch", "mit\u00b7ter\u00b7n\u00e4cht\u00b7ges", "Graun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch einen Aschenhauf zerfallener Gebeine", "tokens": ["Durch", "ei\u00b7nen", "A\u00b7schen\u00b7hauf", "zer\u00b7fal\u00b7le\u00b7ner", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In mein gewaltig Schlo\u00df, aus einem Block gehaun,", "tokens": ["In", "mein", "ge\u00b7wal\u00b7tig", "Schlo\u00df", ",", "aus", "ei\u00b7nem", "Block", "ge\u00b7haun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nicht aus s\u00e4nftlichem Steine.", "tokens": ["Und", "nicht", "aus", "s\u00e4nft\u00b7li\u00b7chem", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Denn ewge S\u00fcnde schuf den Bau, und er enth\u00e4lt", "tokens": ["Denn", "ew\u00b7ge", "S\u00fcn\u00b7de", "schuf", "den", "Bau", ",", "und", "er", "ent\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Leiden und den Gram, der meinem Stolz verb\u00fcndet!\u00ab", "tokens": ["Mein", "Lei\u00b7den", "und", "den", "Gram", ",", "der", "mei\u00b7nem", "Stolz", "ver\u00b7b\u00fcn\u00b7det", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "w