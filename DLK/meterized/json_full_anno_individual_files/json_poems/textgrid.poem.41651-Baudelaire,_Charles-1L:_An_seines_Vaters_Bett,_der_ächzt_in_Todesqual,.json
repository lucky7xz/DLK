nct"], "pos": ["PDAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.14": {"line.1": {"text": "An seines Vaters Bett, der \u00e4chzt in Todesqual,", "tokens": ["An", "sei\u00b7nes", "Va\u00b7ters", "Bett", ",", "der", "\u00e4chzt", "in", "To\u00b7des\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spricht sinnend Harpagon vor diesen spitzen Z\u00fcgen:", "tokens": ["Spricht", "sin\u00b7nend", "Har\u00b7pa\u00b7gon", "vor", "die\u00b7sen", "spit\u00b7zen", "Z\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NE", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Speicher haben in gen\u00fcgend gro\u00dfer Zahl", "tokens": ["Im", "Spei\u00b7cher", "ha\u00b7ben", "in", "ge\u00b7n\u00fc\u00b7gend", "gro\u00b7\u00dfer", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir alte Bretter doch liegen?", "tokens": ["Wir", "al\u00b7te", "Bret\u00b7ter", "doch", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Und Celimene gurrt: Mein Herz ist gut und weich,", "tokens": ["Und", "Ce\u00b7li\u00b7me\u00b7ne", "gurrt", ":", "Mein", "Herz", "ist", "gut", "und", "weich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "N