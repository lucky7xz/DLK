{"textgrid.poem.36096": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "9. In Herren Mylii von Berenburg sein Stammbuch. September", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn nicht die H\u00fclfe selbst in Angst- vnd Bangeseyn", "tokens": ["Wenn", "nicht", "die", "H\u00fcl\u00b7fe", "selbst", "in", "Angst", "vnd", "Ban\u00b7ge\u00b7seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vns an die Mannheit denkt, vnd Herz vnd Muth redt ein,", "tokens": ["Vns", "an", "die", "Mann\u00b7heit", "denkt", ",", "vnd", "Herz", "vnd", "Muth", "redt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sind wir, wie ein Blatt, das auch der schw\u00e4chste Wind", "tokens": ["So", "sind", "wir", ",", "wie", "ein", "Blatt", ",", "das", "auch", "der", "schw\u00e4chs\u00b7te", "Wind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von seinem \u00c4thmen nur in stetem Zittern findt.", "tokens": ["Von", "sei\u00b7nem", "\u00c4th\u00b7men", "nur", "in", "ste\u00b7tem", "Zit\u00b7tern", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd strauchelt nur der Trost, so liegt der Mann schon gar,", "tokens": ["Vnd", "strau\u00b7chelt", "nur", "der", "Trost", ",", "so", "liegt", "der", "Mann", "schon", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der so beherzet stund vnd Held vnd Riese war.", "tokens": ["Der", "so", "be\u00b7her\u00b7zet", "stund", "vnd", "Held", "vnd", "Rie\u00b7se", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "KON", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So geht es mit vns zu. Des Gl\u00fcckes Stiefblick kan", "tokens": ["So", "geht", "es", "mit", "vns", "zu", ".", "Des", "Gl\u00fc\u00b7ckes", "Stief\u00b7blick", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$.", "ART", "NN", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ohn' Hand vnd ohne Streit er\u00f6bern einen Mann.", "tokens": ["Ohn'", "Hand", "vnd", "oh\u00b7ne", "Streit", "er\u00b7\u00f6\u00b7bern", "ei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn nicht die H\u00fclfe selbst in Angst- vnd Banges