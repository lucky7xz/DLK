{"textgrid.poem.60739": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Alexius", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor der Strenge seines Vaters, vor dem allgewaltigen Zar,", "tokens": ["Vor", "der", "Stren\u00b7ge", "sei\u00b7nes", "Va\u00b7ters", ",", "vor", "dem", "all\u00b7ge\u00b7wal\u00b7ti\u00b7gen", "Zar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Floh von Moskau weg Alexis, der aus zarterm Stoffe war:", "tokens": ["Floh", "von", "Mos\u00b7kau", "weg", "A\u00b7le\u00b7xis", ",", "der", "aus", "zar\u00b7term", "Stof\u00b7fe", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADV", "NE", "$,", "PRELS", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Gern verg\u00f6nnt der milde Kaiser, den er anzuflehn beschlo\u00df,", "tokens": ["Gern", "ver\u00b7g\u00f6nnt", "der", "mil\u00b7de", "Kai\u00b7ser", ",", "den", "er", "an\u00b7zu\u00b7flehn", "be\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ein Asyl dem armen Fl\u00fcchtling auf Neapels Felsenschlo\u00df.", "tokens": ["Ein", "A\u00b7syl", "dem", "ar\u00b7men", "Fl\u00fccht\u00b7ling", "auf", "Nea\u00b7pels", "Fel\u00b7sen\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "APPR", "NE", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf der Burg Sankt Elmo hielt sich nun des Zaren Sohn versteckt;", "tokens": ["Auf", "der", "Burg", "Sankt", "El\u00b7mo", "hielt", "sich", "nun", "des", "Za\u00b7ren", "Sohn", "ver\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "VVFIN", "PRF", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch die Sp\u00e4her seines Vaters hatten dort ihn bald entdeckt.", "tokens": ["Doch", "die", "Sp\u00e4\u00b7her", "sei\u00b7nes", "Va\u00b7ters", "hat\u00b7ten", "dort", "ihn", "bald", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Als zur\u00fcck ihn diese schleppten nach dem eisumstarrten Pol,", "tokens": ["Als", "zu\u00b7r\u00fcck", "ihn", "die\u00b7se", "schlepp\u00b7ten", "nach", "dem", "ei\u00b7sum\u00b7starr\u00b7ten", "Pol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKVZ", "PPER", "PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Richtet er an seine Freistatt ein beklommnes Lebewohl:", "tokens": ["Rich\u00b7tet", "er", "an", "sei\u00b7ne", "Freis\u00b7tatt", "ein", "be\u00b7klomm\u00b7nes", "Le\u00b7be\u00b7wohl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Lebe wohl, o Eden, dessen Reize doppelt ich gef\u00fchlt", "tokens": ["Le\u00b7be", "wohl", ",", "o", "E\u00b7den", ",", "des\u00b7sen", "Rei\u00b7ze", "dop\u00b7pelt", "ich", "ge\u00b7f\u00fchlt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "FM", "NN", "$,", "PRELAT", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wo die Woge purpurfarbig um die felsigen G\u00e4rten sp\u00fclt!", "tokens": ["Wo", "die", "Wo\u00b7ge", "pur\u00b7purf\u00b7ar\u00b7big", "um", "die", "fel\u00b7si\u00b7gen", "G\u00e4r\u00b7ten", "sp\u00fclt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Gern um deine Zauber h\u00e4tt ich eingetauscht das gr\u00f6\u00dfte Reich;", "tokens": ["Gern", "um", "dei\u00b7ne", "Zau\u00b7ber", "h\u00e4tt", "ich", "ein\u00b7ge\u00b7tauscht", "das", "gr\u00f6\u00df\u00b7te", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch es ist dem Feuerberg dort meines Vaters Busen Gleich!", "tokens": ["Doch", "es", "ist", "dem", "Feu\u00b7er\u00b7berg", "dort", "mei\u00b7nes", "Va\u00b7ters", "Bu\u00b7sen", "Gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Hab ich doch nach seiner Krone nie gestrebt, und was ich bin,", "tokens": ["Hab", "ich", "doch", "nach", "sei\u00b7ner", "Kro\u00b7ne", "nie", "ge\u00b7strebt", ",", "und", "was", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "War bereit ich abzutreten an den Sohn der Buhlerin!", "tokens": ["War", "be\u00b7reit", "ich", "ab\u00b7zu\u00b7tre\u00b7ten", "an", "den", "Sohn", "der", "Buh\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVIZU", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Blo\u00df des Klosters Zwang vermeiden wollt ich, als ich ihm entfloh:", "tokens": ["Blo\u00df", "des", "Klos\u00b7ters", "Zwang", "ver\u00b7mei\u00b7den", "wollt", "ich", ",", "als", "ich", "ihm", "ent\u00b7floh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Fern von ihm und fern von Ehrsucht war ich hier im Stillen froh!", "tokens": ["Fern", "von", "ihm", "und", "fern", "von", "Ehr\u00b7sucht", "war", "ich", "hier", "im", "Stil\u00b7len", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "ADJD", "APPR", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Stets vor seinem Geiste hat sich meine Seele tief geb\u00fcckt:", "tokens": ["Stets", "vor", "sei\u00b7nem", "Geis\u00b7te", "hat", "sich", "mei\u00b7ne", "See\u00b7le", "tief", "ge\u00b7b\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VAFIN", "PRF", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nicht den Zepter ihm beneidet hab ich, ach, ich war begl\u00fcckt!", "tokens": ["Nicht", "den", "Zep\u00b7ter", "ihm", "be\u00b7nei\u00b7det", "hab", "ich", ",", "ach", ",", "ich", "war", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PPER", "VVPP", "VAFIN", "PPER", "$,", "ITJ", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nicht beneidet ihm die Waffen, die von Sieg zu Sieg er schwang,", "tokens": ["Nicht", "be\u00b7nei\u00b7det", "ihm", "die", "Waf\u00b7fen", ",", "die", "von", "Sieg", "zu", "Sieg", "er", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Seine Tugend nicht beneidet, denn sie geht den Henkersgang!", "tokens": ["Sei\u00b7ne", "Tu\u00b7gend", "nicht", "be\u00b7nei\u00b7det", ",", "denn", "sie", "geht", "den", "Hen\u00b7kers\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Nicht die Krone blo\u00df, das Leben soll ich weihn ihm als Tribut,", "tokens": ["Nicht", "die", "Kro\u00b7ne", "blo\u00df", ",", "das", "Le\u00b7ben", "soll", "ich", "weihn", "ihm", "als", "Tri\u00b7but", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "$,", "ART", "NN", "VMFIN", "PPER", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ja, und wiederkehren soll ich, weil er lechzt nach meinem Blut!", "tokens": ["Ja", ",", "und", "wie\u00b7der\u00b7keh\u00b7ren", "soll", "ich", ",", "weil", "er", "lechzt", "nach", "mei\u00b7nem", "Blut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Vor der Allgewalt des Willens geht zugrunde jedes Recht:", "tokens": ["Vor", "der", "All\u00b7ge\u00b7walt", "des", "Wil\u00b7lens", "geht", "zu\u00b7grun\u00b7de", "je\u00b7des", "Recht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ADJA", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bin ich selbst doch ein Romanow, und ich kenne mein Geschlecht.", "tokens": ["Bin", "ich", "selbst", "doch", "ein", "Ro\u00b7ma\u00b7now", ",", "und", "ich", "ken\u00b7ne", "mein", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NE", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Wollte mich der Vater schonen, g\u00e4be doch mir keine Frist", "tokens": ["Woll\u00b7te", "mich", "der", "Va\u00b7ter", "scho\u00b7nen", ",", "g\u00e4\u00b7be", "doch", "mir", "kei\u00b7ne", "Frist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$,", "VVFIN", "ADV", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Menzikoff und dessen Kebsweib, welches nun die Zarin ist!", "tokens": ["Men\u00b7zi\u00b7koff", "und", "des\u00b7sen", "Kebs\u00b7weib", ",", "wel\u00b7ches", "nun", "die", "Za\u00b7rin", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PRELAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch die Rache folgt vielleicht mir in des Grabs ersehnten Scho\u00df,", "tokens": ["Doch", "die", "Ra\u00b7che", "folgt", "viel\u00b7leicht", "mir", "in", "des", "Grabs", "er\u00b7sehn\u00b7ten", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und dem Paar, das mich verfolgte, wird ein ungl\u00fcckselig Los!", "tokens": ["Und", "dem", "Paar", ",", "das", "mich", "ver\u00b7folg\u00b7te", ",", "wird", "ein", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Los", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "ADJD", "NE", "$."], "meter": "--+-+-+-+-+---+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Gerne f\u00fcr den Vater st\u00fcrb ich, w\u00e4r's der Welt und ihm zum Heil;", "tokens": ["Ger\u00b7ne", "f\u00fcr", "den", "Va\u00b7ter", "st\u00fcrb", "ich", ",", "w\u00e4r's", "der", "Welt", "und", "ihm", "zum", "Heil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "KON", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch ich f\u00fcrchte, seine Krone wird den Schlechtern einst zuteil!", "tokens": ["Doch", "ich", "f\u00fcrch\u00b7te", ",", "sei\u00b7ne", "Kro\u00b7ne", "wird", "den", "Schlech\u00b7tern", "einst", "zu\u00b7teil", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "M\u00f6g er kinderlos verwelken! Seine Herrschaft, ihm zum Hohn,", "tokens": ["M\u00f6g", "er", "kin\u00b7der\u00b7los", "ver\u00b7wel\u00b7ken", "!", "Sei\u00b7ne", "Herr\u00b7schaft", ",", "ihm", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$.", "PPOSAT", "NN", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "M\u00f6ge jene Bauerndirne teilen mit dem B\u00e4ckersohn!", "tokens": ["M\u00f6\u00b7ge", "je\u00b7ne", "Bau\u00b7ern\u00b7dir\u00b7ne", "tei\u00b7len", "mit", "dem", "B\u00e4\u00b7cker\u00b7sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Vor der Strenge seines Vaters, vor dem allgewaltigen Zar,", "tokens": ["Vor", "der", "Stren\u00b7ge", "sei\u00b7nes", "Va\u00b7ters", ",", "vor", "dem", "all\u00b7ge\u00b7wal\u00b7ti\u00b7gen", "Zar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Floh von Moskau weg Alexis, der aus zarterm Stoffe war:", "tokens": ["Floh", "von", "Mos\u00b7kau", "weg", "A\u00b7le\u00b7xis", ",", "der", "aus", "zar\u00b7term", "Stof\u00b7fe", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADV", "NE", "$,", "PRELS", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Gern verg\u00f6nnt der milde Kaiser, den er anzuflehn beschlo\u00df,", "tokens": ["Gern", "ver\u00b7g\u00f6nnt", "der", "mil\u00b7de", "Kai\u00b7ser", ",", "den", "er", "an\u00b7zu\u00b7flehn", "be\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ein Asyl dem armen Fl\u00fcchtling auf Neapels Felsenschlo\u00df.", "tokens": ["Ein", "A\u00b7syl", "dem", "ar\u00b7men", "Fl\u00fccht\u00b7ling", 