{"textgrid.poem.59250": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tiroler Nachtwache", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In stiller Bucht, bei finstrer Nacht,", "tokens": ["In", "stil\u00b7ler", "Bucht", ",", "bei", "finst\u00b7rer", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4ft tief die Welt im Grunde,", "tokens": ["Schl\u00e4ft", "tief", "die", "Welt", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Berge rings stehn auf der Wacht,", "tokens": ["Die", "Ber\u00b7ge", "rings", "stehn", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel macht die Runde,", "tokens": ["Der", "Him\u00b7mel", "macht", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Geht um und um,", "tokens": ["Geht", "um", "und", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ums Land herum", "tokens": ["Ums", "Land", "he\u00b7rum"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "NN", "APZR"], "meter": "-+-+", "measure": "iambic