{"dta.poem.3753": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Korbflechterlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will ein K\u00f6rblein flechten,               ", "tokens": ["Ich", "will", "ein", "K\u00f6rb\u00b7lein", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein K\u00f6rblein h\u00fcbsch und fein,", "tokens": ["Ein", "K\u00f6rb\u00b7lein", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm du dein falches Herze,", "tokens": ["Nimm", "du", "dein", "fal\u00b7ches", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und legs mit gr\u00f6\u00dftem Schmerze", "tokens": ["Und", "legs", "mit", "gr\u00f6\u00df\u00b7tem", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In dieses K\u00f6rblein fein.", "tokens": ["In", "die\u00b7ses", "K\u00f6rb\u00b7lein", "fein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}