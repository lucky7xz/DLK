{"textgrid.poem.49453": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[denn zur Gen\u00fcge litt ich jetzt]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denn zur Gen\u00fcge litt ich jetzt", "tokens": ["Denn", "zur", "Ge\u00b7n\u00fc\u00b7ge", "litt", "ich", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Wolf gleich, der gestellt, gehetzt,", "tokens": ["Dem", "Wolf", "gleich", ",", "der", "ge\u00b7stellt", ",", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "ADV", "$,", "PRELS", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis alle Kraft ihn verlasse,", "tokens": ["Bis", "al\u00b7le", "Kraft", "ihn", "ver\u00b7las\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dass auf der Jagd nach Ruh und Rast", "tokens": ["Dass", "auf", "der", "Jagd", "nach", "Ruh", "und", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er flieht und springt in wilder Hast", "tokens": ["Er", "flieht", "und", "springt", "in", "wil\u00b7der", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Unterm Schlag einer ganzen Rasse.", "tokens": ["Un\u00b7term", "Schlag", "ei\u00b7ner", "gan\u00b7zen", "Ras\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Geldmangel, Neid, versteckte Wut,", "tokens": ["Geld\u00b7man\u00b7gel", ",", "Neid", ",", "ver\u00b7steck\u00b7te", "Wut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sp\u00fcrhunde, deren Witt'rung gut,", "tokens": ["Sp\u00fcr\u00b7hun\u00b7de", ",", "de\u00b7ren", "Witt'\u00b7rung", "gut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stehn drohend rings auf der Lauer.", "tokens": ["Stehn", "dro\u00b7hend", "rings", "auf", "der", "Lau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Tag', Monat', Jahre dauert das;", "tokens": ["Tag'", ",", "Mo\u00b7nat'", ",", "Jah\u00b7re", "dau\u00b7ert", "das", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich esse Grau'n, ich schlucke Hass,", "tokens": ["Ich", "es\u00b7se", "Grau'n", ",", "ich", "schlu\u00b7cke", "Hass", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die harten Bissen der Trauer.", "tokens": ["Die", "har\u00b7ten", "Bis\u00b7sen", "der", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch in dem Wald voll grauser Not", "tokens": ["Doch", "in", "dem", "Wald", "voll", "grau\u00b7ser", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh pl\u00f6tzlich ich den Sp\u00fcrhund Tod,", "tokens": ["Seh", "pl\u00f6tz\u00b7lich", "ich", "den", "Sp\u00fcr\u00b7hund", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bestie, sich zu mir wenden.", "tokens": ["Die", "Be\u00b7stie", ",", "sich", "zu", "mir", "wen\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf mich, der schon halb tot vor Schmerz,", "tokens": ["Auf", "mich", ",", "der", "schon", "halb", "tot", "vor", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "ADV", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wirft sich der Tod, beisst mir ins Herz,", "tokens": ["Wirft", "sich", "der", "Tod", ",", "beisst", "mir", "ins", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "D