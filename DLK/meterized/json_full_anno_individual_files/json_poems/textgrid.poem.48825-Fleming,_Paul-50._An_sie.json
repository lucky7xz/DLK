{"textgrid.poem.48825": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "50. An sie", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den vielgef\u00e4rbten Klee zu diesem Kranze hier", "tokens": ["Den", "viel\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Klee", "zu", "die\u00b7sem", "Kran\u00b7ze", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hab' ich mit eigner Hand gelesen um die Wiesen,", "tokens": ["hab'", "ich", "mit", "eig\u00b7ner", "Hand", "ge\u00b7le\u00b7sen", "um", "die", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die f\u00fcr die sch\u00f6nsten hie von allen sind gepriesen,", "tokens": ["die", "f\u00fcr", "die", "sch\u00f6ns\u00b7ten", "hie", "von", "al\u00b7len", "sind", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "ADV", "APPR", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in welcher gr\u00fcnen Scho\u00df du sa\u00dfest neben mir", "tokens": ["in", "wel\u00b7cher", "gr\u00fc\u00b7nen", "Scho\u00df", "du", "sa\u00b7\u00dfest", "ne\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "und deine Schwestern auch. Den, Jungfrau, send' ich dir,", "tokens": ["und", "dei\u00b7ne", "Schwes\u00b7tern", "auch", ".", "Den", ",", "Jung\u00b7frau", ",", "send'", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$.", "ART", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ein allzu Schlechtes zwar, nicht aber, da\u00df du diesen", "tokens": ["ein", "all\u00b7zu", "Schlech\u00b7tes", "zwar", ",", "nicht", "a\u00b7ber", ",", "da\u00df", "du", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKA", "ADJA", "ADV", "$,", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf dein so w\u00fcrdigs H\u00e4upt zu setzen m\u00f6gst erkiesen.", "tokens": ["auf", "dein", "so", "w\u00fcr\u00b7digs", "H\u00e4upt", "zu", "set\u00b7zen", "m\u00f6gst", "er\u00b7kie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr diesem Glanze stirbt der Blumen sch\u00f6nste Zier.", "tokens": ["F\u00fcr", "die\u00b7sem", "Glan\u00b7ze", "stirbt", "der", "Blu\u00b7men", "sch\u00f6ns\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es stunde Quendel zwar, auch Augentrost die F\u00fclle", "tokens": ["Es", "stun\u00b7de", "Quen\u00b7del", 