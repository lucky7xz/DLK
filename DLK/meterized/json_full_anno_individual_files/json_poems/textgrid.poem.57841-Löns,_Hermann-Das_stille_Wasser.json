{"textgrid.poem.57841": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das stille Wasser", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solang' die liebe Sonne lacht", "tokens": ["So\u00b7lang'", "die", "lie\u00b7be", "Son\u00b7ne", "lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrem goldnen Schein,", "tokens": ["Mit", "ih\u00b7rem", "gold\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da mu\u00df ich meine Arbeit tun,", "tokens": ["Da", "mu\u00df", "ich", "mei\u00b7ne", "Ar\u00b7beit", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df fromm und flei\u00dfig sein.", "tokens": ["Mu\u00df", "fromm", "und", "flei\u00b7\u00dfig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Augen schlag' ich unter mich", "tokens": ["Die", "Au\u00b7gen", "schlag'", "ich", "un\u00b7ter", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sehe niemand an,", "tokens": ["Und", "se\u00b7he", "nie\u00b7mand", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ob ich nichts von Liebe wei\u00df", "tokens": ["Als", "ob", "ich", "nichts", "von", "Lie\u00b7be", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PIS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und davon reden kann.", "tokens": ["Und", "da\u00b7von", "re\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch davon reden tu ich nicht,", "tokens": ["Doch", "da\u00b7von", "re\u00b7den", "tu", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVINF", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schweige immer still,", "tokens": ["Ich", "schwei\u00b7ge", "im\u00b7mer", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sehe, ob das Sonnenlicht", "tokens": ["Und", "se\u00b7he", ",", "ob", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht bald verschwinden will.", "tokens": ["Nicht", "bald", "ver\u00b7schwin\u00b7den", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch wenn der Mond am Himmel steht,", "tokens": ["Doch", "wenn", "der", "Mond", "am", "Him\u00b7mel", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "p