": "trochaic.tetra"}, "line.3": {"text": "Garben, haben sie zwar K\u00f6rner,", "tokens": ["Gar\u00b7ben", ",", "ha\u00b7ben", "sie", "zwar", "K\u00f6r\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen doch gedroschen seyn.", "tokens": ["Wol\u00b7len", "doch", "ge\u00b7dro\u00b7schen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mit Threnen Saamen streuen,", "tokens": ["Die", "mit", "Thre\u00b7nen", "Saa\u00b7men", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werden fr\u00f6lich Korn abmeyen.", "tokens": ["Wer\u00b7den", "fr\u00f6\u00b7lich", "Korn", "ab\u00b7me\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "VVINF", "$."], "meter": "--+-++-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "St\u00f6cke mu\u00df man vor beschneiden,", "tokens": ["St\u00f6\u00b7cke", "mu\u00df", "man", "vor", "be\u00b7schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann w\u00e4chst erst die Traube wol,", "tokens": ["Dann", "w\u00e4chst", "erst", "die", "Trau\u00b7be", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Trauben m\u00fcssen Pressen leyden,", "tokens": ["Trau\u00b7ben", "m\u00fcs\u00b7sen", 