{"dta.poem.1943": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Unterschied der Sprachen und  \n Gem\u00fctter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Gemeines Spr\u00fcchwort sch\u00e4zt den Mann aus seinem", "tokens": ["Ge\u00b7mei\u00b7nes", "Spr\u00fcch\u00b7wort", "sch\u00e4zt", "den", "Mann", "aus", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gange/", "tokens": ["Gan\u00b7ge", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Den Groschen nach dem Klang/ und V\u00f6gel vom Gesange:", "tokens": ["Den", "Gro\u00b7schen", "nach", "dem", "Klang", "/", "und", "V\u00f6\u00b7gel", "vom", "Ge\u00b7san\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Frantzmann bricht die Red/ h\u00e4lt Sylb und Hertz zu-", "tokens": ["Der", "Frantz\u00b7mann", "bricht", "die", "Red", "/", "h\u00e4lt", "Sylb", "und", "Hertz", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "VVFIN", "NE", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "r\u00fccke/", "tokens": ["r\u00fc\u00b7cke", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Der Welsche f\u00e4lscht das Wort/ steckt insgemein voll T\u00fccke/", "tokens": ["Der", "Wel\u00b7sche", "f\u00e4lscht", "das", "Wort", "/", "steckt", "ins\u00b7ge\u00b7mein", "voll", "T\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "VVFIN", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein stoltzer Spanier wird Pracht in Worten f\u00fchren/ o", "tokens": ["Ein", "stolt\u00b7zer", "Spa\u00b7nier", "wird", "Pracht", "in", "Wor\u00b7ten", "f\u00fch\u00b7ren", "/", "o"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "APPR", "NN", "VVINF", "$(", "FM"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der Britten Wanckelmutt l\u00e4st ihre Zunge sp\u00fcren:", "tokens": ["Der", "Brit\u00b7ten", "Wan\u00b7ckel\u00b7mutt", "l\u00e4st", "ih\u00b7re", "Zun\u00b7ge", "sp\u00fc\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "um Zeichen/ da\u00df man deutsch und unver\u00e4ndert bleibt/", "tokens": ["um", "Zei\u00b7chen", "/", "da\u00df", "man", "deutsch", "und", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "bleibt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KOUS", "PIS", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So schreibt man/ wie man redt/ und redet wie man schreibt.", "tokens": ["So", "schreibt", "man", "/", "wie", "man", "redt", "/", "und", "re\u00b7det", "wie", "man", "schreibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "PWAV", "PIS", "VVFIN", "$(", "KON", "VVFIN", "KOKOM", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}