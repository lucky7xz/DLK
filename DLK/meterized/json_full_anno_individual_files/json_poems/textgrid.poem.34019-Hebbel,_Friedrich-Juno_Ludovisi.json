{"textgrid.poem.34019": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Juno Ludovisi", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du l\u00e4ssest uns die Bl\u00fcte alles Sch\u00f6nen", "tokens": ["Du", "l\u00e4s\u00b7sest", "uns", "die", "Bl\u00fc\u00b7te", "al\u00b7les", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seines Werdens holdes Wunder sehen;", "tokens": ["Und", "sei\u00b7nes", "Wer\u00b7dens", "hol\u00b7des", "Wun\u00b7der", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Stirn' ist streng, man sieht's in ihr entstehen,", "tokens": ["Die", "Stirn'", "ist", "streng", ",", "man", "sieht's", "in", "ihr", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo es noch ringen mu\u00df mit herben T\u00f6nen.", "tokens": ["Wo", "es", "noch", "rin\u00b7gen", "mu\u00df", "mit", "her\u00b7ben", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Wange will sich schon mit Anmuth kr\u00f6nen,", "tokens": ["Die", "Wan\u00b7ge", "will", "sich", "schon", "mit", "An\u00b7muth", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch darf sie noch im L\u00e4cheln nicht zergehen,", "tokens": ["Doch", "darf", "sie", "noch", "im", "L\u00e4\u00b7cheln", "nicht", "zer\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Mund jedoch zerschmilzt in s\u00fc\u00dfen Wehen,", "tokens": ["Der", "Mund", "je\u00b7doch", "zer\u00b7schmilzt", "in", "s\u00fc\u00b7\u00dfen", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Ernst und Milde sich im Reiz vers\u00f6hnen.", "tokens": ["Da\u00df", "Ernst", "und", "Mil\u00b7de", "sich", "im", "Reiz", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erst keusches Leben, wurzelhaft gebunden,", "tokens": ["Erst", "keu\u00b7sches", "Le\u00b7ben", ",", "wur\u00b7zel\u00b7haft", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann scheuer Vortraum von sich selbst, der leise", "tokens": ["Dann", "scheu\u00b7er", "Vor\u00b7traum", "von", "sich", "selbst", ",", "der", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "PRF", "ADV", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hin\u00fcber f\u00fchrt zur wirklichen Entfaltung;", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "f\u00fchrt", "zur", "wirk\u00b7li\u00b7chen", "Ent\u00b7fal\u00b7tung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und nun ist auch der Werdekampf verwunden,", "tokens": ["Und", "nun", "ist", "auch", "der", "Wer\u00b7de\u00b7kampf", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man sieht nicht Anfang mehr, noch Schlu\u00df im Kreise,", "tokens": ["Man", "sieht", "nicht", "An\u00b7fang", "mehr", ",", "noch", "Schlu\u00df", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "NN", "ADV", "$,", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dieses ist der Gipfel der Gestaltung.", "tokens": ["Und", "die\u00b7ses", "ist", "der", "Gip\u00b7fel", "der", "Ge\u00b7stal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du l\u00e4ssest uns die Bl\u00fcte alles Sch\u00f6nen", "tokens": ["Du", "l\u00e4s\u00b7sest", "uns", "die", "Bl\u00fc\u00b7te", "al\u00b7les", "Sch\u00f6\u00b7nen"], "token_info": ["w