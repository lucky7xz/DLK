{"textgrid.poem.35645": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "8. Tobacksode", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rolf, beim sch\u00e4umenden Kelch, oder beim Trank, den die Levante br\u00e4unt,", "tokens": ["Rolf", ",", "beim", "sch\u00e4u\u00b7men\u00b7den", "Kelch", ",", "o\u00b7der", "beim", "Trank", ",", "den", "die", "Le\u00b7van\u00b7te", "br\u00e4unt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "$,", "KON", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "La\u00df von Knastergew\u00f6lk unsere Stirn bl\u00e4ulich umwirbelt sein!", "tokens": ["La\u00df", "von", "Knas\u00b7ter\u00b7ge\u00b7w\u00f6lk", "un\u00b7se\u00b7re", "Stirn", "bl\u00e4u\u00b7lich", "um\u00b7wir\u00b7belt", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "PPOSAT", "NN", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+--+---+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Zeus, im Opfergeduft, l\u00e4chelte nie froher, als wir, umdampft", "tokens": ["Zeus", ",", "im", "Op\u00b7fer\u00b7ge\u00b7duft", ",", "l\u00e4\u00b7chel\u00b7te", "nie", "fro\u00b7her", ",", "als", "wir", ",", "um\u00b7dampft"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "APPRART", "NN", "$,", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "$,", "VVPP"], "meter": "+-+--+-+-+---+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Von Virginischem Kraut, welches dein Wink, holder Tobackus, schuf!", "tokens": ["Von", "Vir\u00b7gi\u00b7ni\u00b7schem", "Kraut", ",", "wel\u00b7ches", "dein", "Wink", ",", "hol\u00b7der", "To\u00b7bac\u00b7kus", ",", "schuf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+---+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Furcht noch Kargheit des Gro\u00dfoheims die Stirn, darbender J\u00fcngling, dir?", "tokens": ["Furcht", "noch", "Karg\u00b7heit", "des", "Gro\u00df\u00b7o\u00b7heims", "die", "Stirn", ",", "dar\u00b7ben\u00b7der", "J\u00fcng\u00b7ling", ",", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "NN", "ART", "NN", "ART", "NN", "$,", "PAV", "NN", "$,", "PPER", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Tr\u00e4umst du, st\u00fctzend das Haupt, noch die Gestalt spr\u00f6der Geliebten nur?", "tokens": ["Tr\u00e4umst", "du", ",", "st\u00fct\u00b7zend", "das", "Haupt", ",", "noch", "die", "Ge\u00b7stalt", "spr\u00f6\u00b7der", "Ge\u00b7lieb\u00b7ten", "nur", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "ART", "NN", "$,", "ADV", "ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Strahlt nicht pl\u00f6tzlich dein Blick? Jauchzest du nicht hohen Triumphgesang?", "tokens": ["Strahlt", "nicht", "pl\u00f6tz\u00b7lich", "dein", "Blick", "?", "Jauch\u00b7zest", "du", "nicht", "ho\u00b7hen", "Tri\u00b7umph\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Aber wehe! wer stets stygischen Schwall wie ein Volkan verspricht!", "tokens": ["A\u00b7ber", "we\u00b7he", "!", "wer", "stets", "sty\u00b7gi\u00b7schen", "Schwall", "wie", "ein", "Vol\u00b7kan", "ver\u00b7spricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PWS", "ADV", "ADJA", "NN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "+-+---+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "In die Tiefe des Meers st\u00fcrzte der Gott rohe Matrosen oft,", "tokens": ["In", "die", "Tie\u00b7fe", "des", "Meers", "st\u00fcrz\u00b7te", "der", "Gott", "ro\u00b7he", "Mat\u00b7ro\u00b7sen", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "--+--++---+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Wenn aus geudendem Rohr Feuer zum Sitz laurender Donner schlich.", "tokens": ["Wenn", "aus", "geu\u00b7den\u00b7dem", "Rohr", "Feu\u00b7er", "zum", "Sitz", "lau\u00b7ren\u00b7der", "Don\u00b7ner", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN", "APPRART", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "--+--++--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Auf zum Messer der Schlacht taumelt der Kreis schwelgender Indier", "tokens": ["Auf", "zum", "Mes\u00b7ser", "der", "Schlacht", "tau\u00b7melt", "der", "Kreis", "schwel\u00b7gen\u00b7der", "In\u00b7dier"], "token_info": ["word", "word", "word", "word", "word", "word", "wo