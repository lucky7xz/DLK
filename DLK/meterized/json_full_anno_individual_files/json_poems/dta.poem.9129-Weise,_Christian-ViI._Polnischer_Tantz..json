{"dta.poem.9129": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n Polnischer Tantz.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der hochzeit ist mir keine lust/", "tokens": ["Auf", "der", "hoch\u00b7zeit", "ist", "mir", "kei\u00b7ne", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Als die blosse courtesie bewust/", "tokens": ["Als", "die", "blos\u00b7se", "cour\u00b7te\u00b7sie", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wann die m\u00e4dgen und studenten", "tokens": ["Wann", "die", "m\u00e4d\u00b7gen", "und", "stu\u00b7den\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "KON", "ADJA"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "In der bunten reihe stehn/", "tokens": ["In", "der", "bun\u00b7ten", "rei\u00b7he", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit s\u00fcssen complimenten", "tokens": ["Und", "mit", "s\u00fcs\u00b7sen", "com\u00b7pli\u00b7men\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich auff den tantz-platz gehn.", "tokens": ["End\u00b7lich", "auff", "den", "tant\u00b7zplatz", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Da ist freude/ da ist lustigkeit/", "tokens": ["Da", "ist", "freu\u00b7de", "/", "da", "ist", "lus\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$(", "ADV", "VAFIN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da vergi\u00dft man alles hertzeleid:", "tokens": ["Da", "ver\u00b7gi\u00dft", "man", "al\u00b7les", "hert\u00b7ze\u00b7leid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Leib und seele kommt zusammen:", "tokens": ["Leib", "und", "see\u00b7le", "kommt", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mancher giebet nur die hand/", "tokens": ["Man\u00b7cher", "gie\u00b7bet", "nur", "die", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wird durch die liebes-flammen", "tokens": ["Und", "wird", "durch", "die", "lie\u00b7bes\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Unversehens angebrannt.", "tokens": ["Un\u00b7ver\u00b7se\u00b7hens", "an\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Wie lieb\u00e4ugelt mancher courtisan", "tokens": ["Wie", "lie\u00b7b\u00e4u\u00b7gelt", "man\u00b7cher", "cour\u00b7ti\u00b7san"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Eh er sich ins tantzen finden kan.", "tokens": ["Eh", "er", "sich", "ins", "tant\u00b7zen", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seht wie k\u00f6mmt er auffgezogen/", "tokens": ["Seht", "wie", "k\u00f6mmt", "er", "auff\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "VVFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4ulgen geben darff er nicht/", "tokens": ["M\u00e4ul\u00b7gen", "ge\u00b7ben", "darff", "er", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil sie mit dem ellenbogen.", "tokens": ["Weil", "sie", "mit", "dem", "el\u00b7len\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhn in das gesichte sticht.", "tokens": ["Jhn", "in", "das", "ge\u00b7sich\u00b7te", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Endlich k\u00f6mmt die fiedel auf den platz/", "tokens": ["End\u00b7lich", "k\u00f6mmt", "die", "fie\u00b7del", "auf", "den", "platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da erwischt ein jeder seinen schatz:", "tokens": ["Da", "er\u00b7wischt", "ein", "je\u00b7der", "sei\u00b7nen", "schatz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieser geht als wie auf kohlen/", "tokens": ["Die\u00b7ser", "geht", "als", "wie", "auf", "koh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOUS", "KOKOM", "APPR", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der macht einen blossen pa\u00df/", "tokens": ["Der", "macht", "ei\u00b7nen", "blos\u00b7sen", "pa\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Jener schneidet capriolen/", "tokens": ["Je\u00b7ner", "schnei\u00b7det", "cap\u00b7ri\u00b7o\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser knickt ohn unterla\u00df.", "tokens": ["Die\u00b7ser", "knickt", "ohn", "un\u00b7ter\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Mancher setzt die beine gar zu krumm/", "tokens": ["Man\u00b7cher", "setzt", "die", "bei\u00b7ne", "gar", "zu", "krumm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "PIS", "ADV", "PTKA", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Einer springt nur um das M\u00e4dgen rumm/", "tokens": ["Ei\u00b7ner", "springt", "nur", "um", "das", "M\u00e4d\u00b7gen", "rumm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieser tantzet im gewichte/", "tokens": ["Die\u00b7ser", "tant\u00b7zet", "im", "ge\u00b7wich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener schont die neuen schuh/", "tokens": ["Je\u00b7ner", "schont", "die", "neu\u00b7en", "schuh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser kehrt ihr das gesichte/", "tokens": ["Die\u00b7ser", "kehrt", "ihr", "das", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PDS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jener gar den fertzer zu.", "tokens": ["Je\u00b7ner", "gar", "den", "fert\u00b7zer", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Wann ein m\u00e4dgen in der einsamkeit", "tokens": ["Wann", "ein", "m\u00e4d\u00b7gen", "in", "der", "ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Stille sitzt und welcke r\u00fcben schneidt/", "tokens": ["Stil\u00b7le", "sitzt", "und", "wel\u00b7cke", "r\u00fc\u00b7ben", "schneidt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kommen bald die t\u00fctschem\u00fctter/", "tokens": ["Kom\u00b7men", "bald", "die", "t\u00fct\u00b7sche\u00b7m\u00fct\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erwischet mit verbru\u00df", "tokens": ["Und", "er\u00b7wi\u00b7schet", "mit", "ver\u00b7bru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jrgend einen armen ritter", "tokens": ["Jr\u00b7gend", "ei\u00b7nen", "ar\u00b7men", "rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der die l\u00fccke b\u00fcssen mu\u00df.", "tokens": ["Der", "die", "l\u00fc\u00b7cke", "b\u00fcs\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Also wird das liebe geld verthan/", "tokens": ["Al\u00b7so", "wird", "das", "lie\u00b7be", "geld", "ver\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bi\u00df man keinen tantz mehr zahlen kan/", "tokens": ["Bi\u00df", "man", "kei\u00b7nen", "tantz", "mehr", "zah\u00b7len", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da spricht man es sey zu sp\u00e4te/", "tokens": ["Da", "spricht", "man", "es", "sey", "zu", "sp\u00e4\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "VAFIN", "PTKZU", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Giebt zum abschied einen ku\u00df/", "tokens": ["Giebt", "zum", "ab\u00b7schied", "ei\u00b7nen", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil man doch auff die karrete/", "tokens": ["Weil", "man", "doch", "auff", "die", "kar\u00b7re\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch zwey groscheu wagen mu\u00df.", "tokens": ["Noch", "zwey", "gro\u00b7scheu", "wa\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}