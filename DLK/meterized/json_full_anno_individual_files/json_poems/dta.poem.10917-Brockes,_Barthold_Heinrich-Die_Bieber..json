{"dta.poem.10917": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Bieber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So oft ich hier vor diesem Kupfer, wenn es im Zimmer\nh\u00e4nget, stehe:", "tokens": ["So", "oft", "ich", "hier", "vor", "die\u00b7sem", "Kup\u00b7fer", ",", "wenn", "es", "im", "Zim\u00b7mer", "h\u00e4n\u00b7get", ",", "ste\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "So deucht mich, da\u00df ich in demselben ein recht nat\u00fcrlich-wirk-", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "ich", "in", "dem\u00b7sel\u00b7ben", "ein", "recht", "na\u00b7t\u00fcr\u00b7lich\u00b7wirk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PDAT", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und ein durchsichtig-klares Wasser, aus einem offnen Fenster sehe.", "tokens": ["Und", "ein", "durch\u00b7sich\u00b7tig\u00b7kla\u00b7res", "Was\u00b7ser", ",", "aus", "ei\u00b7nem", "off\u00b7nen", "Fens\u00b7ter", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Des Zimmers Wand scheint auf der Stell, als w\u00e4r sie in der", "tokens": ["Des", "Zim\u00b7mers", "Wand", "scheint", "auf", "der", "Stell", ",", "als", "w\u00e4r", "sie", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "KOKOM", "VAFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So gar nat\u00fcrlich ist die Landschaft gebildet, und in Erz gestochen.", "tokens": ["So", "gar", "na\u00b7t\u00fcr\u00b7lich", "ist", "die", "Land\u00b7schaft", "ge\u00b7bil\u00b7det", ",", "und", "in", "Erz", "ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Die beyden Bieber sind so lebhaft in ihrer Handlung vorgestellt;", "tokens": ["Die", "bey\u00b7den", "Bie\u00b7ber", "sind", "so", "leb\u00b7haft", "in", "ih\u00b7rer", "Hand\u00b7lung", "vor\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Man glaubt, man h\u00f6r und seh sie nagen. Doch la\u00dft uns", "tokens": ["Man", "glaubt", ",", "man", "h\u00f6r", "und", "seh", "sie", "na\u00b7gen", ".", "Doch", "la\u00dft", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "KON", "VVFIN", "PPER", "VVFIN", "$.", "KON", "VVIMP", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "La\u00dft uns das Urbild, die Natur, zum Ruhm des Sch\u00f6pfers,", "tokens": ["La\u00dft", "uns", "das", "Ur\u00b7bild", ",", "die", "Na\u00b7tur", ",", "zum", "Ruhm", "des", "Sch\u00f6p\u00b7fers", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wie sie diese Thiere, fast mit einem weisen Geist, verein\u2019!", "tokens": ["Und", "wie", "sie", "die\u00b7se", "Thie\u00b7re", ",", "fast", "mit", "ei\u00b7nem", "wei\u00b7sen", "Geist", ",", "ver\u00b7ein'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PDAT", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Der fast den Thiergeist \u00fcbertrifft, der unserm sich fast naht,", "tokens": ["Der", "fast", "den", "Thier\u00b7geist", "\u00fc\u00b7bert\u00b7rifft", ",", "der", "un\u00b7serm", "sich", "fast", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Der Bieber Geist in Canada, da\u00df nicht die Fluth ihr Nest", "tokens": ["Der", "Bie\u00b7ber", "Geist", "in", "Ca\u00b7na\u00b7da", ",", "da\u00df", "nicht", "die", "Fluth", "ihr", "Nest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NE", "$,", "KOUS", "PTKNEG", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Haut gro\u00dfe Balken, schleppt sie fort, verbindet sie, verfertigt", "tokens": ["Haut", "gro\u00b7\u00dfe", "Bal\u00b7ken", ",", "schleppt", "sie", "fort", ",", "ver\u00b7bin\u00b7det", "sie", ",", "ver\u00b7fer\u00b7tigt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Und leitet Str\u00f6me von sich ab. Ist dieses nicht Bewunderns-", "tokens": ["Und", "lei\u00b7tet", "Str\u00f6\u00b7me", "von", "sich", "ab", ".", "Ist", "die\u00b7ses", "nicht", "Be\u00b7wun\u00b7derns"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "PRF", "PTKVZ", "$.", "VAFIN", "PDS", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Da\u00df die Natur so plumpe Thiere so sonderbare K\u00fcnste lehrt,", "tokens": ["Da\u00df", "die", "Na\u00b7tur", "so", "plum\u00b7pe", "Thie\u00b7re", "so", "son\u00b7der\u00b7ba\u00b7re", "K\u00fcns\u00b7te", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.15": {"text": "Und wird derselben Quell und Herr, auch hierin, nicht mit", "tokens": ["Und", "wird", "der\u00b7sel\u00b7ben", "Quell", "und", "Herr", ",", "auch", "hie\u00b7rin", ",", "nicht", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PDAT", "NN", "KON", "NN", "$,", "ADV", "ADV", "$,", "PTKNEG", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}