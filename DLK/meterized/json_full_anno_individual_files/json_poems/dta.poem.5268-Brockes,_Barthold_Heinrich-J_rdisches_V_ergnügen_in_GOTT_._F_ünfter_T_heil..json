{"dta.poem.5268": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Formiret in der blauen Luft", "tokens": ["For\u00b7mi\u00b7ret", "in", "der", "blau\u00b7en", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Solch einen lieblichen durchsichtig-gr\u00fcnen Duft,", "tokens": ["Solch", "ei\u00b7nen", "lieb\u00b7li\u00b7chen", "durch\u00b7sich\u00b7tig\u00b7gr\u00fc\u00b7nen", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df keine Vorw\u00fcrf\u2019 unsern Augen", "tokens": ["Da\u00df", "kei\u00b7ne", "Vor\u00b7w\u00fcr\u00b7f'", "un\u00b7sern", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mehr Anmuth zu erwecken taugen.", "tokens": ["Mehr", "An\u00b7muth", "zu", "er\u00b7we\u00b7cken", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wann dieses gelbe Gr\u00fcn bald dort, bald hier,", "tokens": ["Wann", "die\u00b7ses", "gel\u00b7be", "Gr\u00fcn", "bald", "dort", ",", "bald", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An etwas mehr schon ausgebrochnen B\u00fcschen,", "tokens": ["An", "et\u00b7was", "mehr", "schon", "aus\u00b7ge\u00b7broch\u00b7nen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf dunckel-gr\u00fcnem Grunde stehet,", "tokens": ["Auf", "dun\u00b7ckel\u00b7gr\u00fc\u00b7nem", "Grun\u00b7de", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und also gelblich-gr\u00fcn und dunckel-gr\u00fcn sich mischen;", "tokens": ["Und", "al\u00b7so", "gelb\u00b7lich\u00b7gr\u00fcn", "und", "dun\u00b7ckel\u00b7gr\u00fcn", "sich", "mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wird ihrer beider Zier,", "tokens": ["Wird", "ih\u00b7rer", "bei\u00b7der", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Durch beider Gegensatz, erh\u00f6het.", "tokens": ["Durch", "bei\u00b7der", "Ge\u00b7gen\u00b7satz", ",", "er\u00b7h\u00f6\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Man sieht die Knospen sich fast sichtbarlich vergr\u00f6ssern,", "tokens": ["Man", "sieht", "die", "Knos\u00b7pen", "sich", "fast", "sicht\u00b7bar\u00b7lich", "ver\u00b7gr\u00f6s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Der Bl\u00e4tter Bildungen sich sichtbarlich verbessern,", "tokens": ["Der", "Bl\u00e4t\u00b7ter", "Bil\u00b7dun\u00b7gen", "sich", "sicht\u00b7bar\u00b7lich", "ver\u00b7bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "PRF", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Aus ihren r\u00f6thlichen Beh\u00e4ltern m\u00e4hlig steigen,", "tokens": ["Aus", "ih\u00b7ren", "r\u00f6th\u00b7li\u00b7chen", "Be\u00b7h\u00e4l\u00b7tern", "m\u00e4h\u00b7lig", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ein durchlauchtiges und zart Gewebe zeigen.", "tokens": ["Und", "ein", "durch\u00b7lauch\u00b7ti\u00b7ges", "und", "zart", "Ge\u00b7we\u00b7be", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "KON", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man sieht fast sichtbarlich", "tokens": ["Man", "sieht", "fast", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Die aufgeqvollne Knospen spalten", "tokens": ["Die", "auf\u00b7ge\u00b7qvoll\u00b7ne", "Knos\u00b7pen", "spal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und ihre gr\u00fcne Zucht, die zarten Bl\u00e4tter,", "tokens": ["Und", "ih\u00b7re", "gr\u00fc\u00b7ne", "Zucht", ",", "die", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Fast sichtbarlich entwickeln und entfalten.", "tokens": ["Fast", "sicht\u00b7bar\u00b7lich", "ent\u00b7wi\u00b7ckeln", "und", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Jetzt sieht ein jeder Baum, ein jegliches Gestr\u00e4uch,", "tokens": ["Jetzt", "sieht", "ein", "je\u00b7der", "Baum", ",", "ein", "jeg\u00b7li\u00b7ches", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "An jungen Bl\u00e4tterchern und ofnen Knospen reich,", "tokens": ["An", "jun\u00b7gen", "Bl\u00e4t\u00b7ter\u00b7chern", "und", "of\u00b7nen", "Knos\u00b7pen", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier braun- da grau- dort gr\u00fcnen Netzen gleich,", "tokens": ["Hier", "braun", "da", "grau", "dort", "gr\u00fc\u00b7nen", "Net\u00b7zen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "ADV", "TRUNC", "ADV", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Durch die der Blick frey hin und wieder irrt,", "tokens": ["Durch", "die", "der", "Blick", "frey", "hin", "und", "wie\u00b7der", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJD", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Bald aber, durch der gr\u00fcnen Knoten Menge,", "tokens": ["Bald", "a\u00b7ber", ",", "durch", "der", "gr\u00fc\u00b7nen", "Kno\u00b7ten", "Men\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "In eine holde gr\u00fcne Enge", "tokens": ["In", "ei\u00b7ne", "hol\u00b7de", "gr\u00fc\u00b7ne", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Getrieben und gefangen wird,", "tokens": ["Ge\u00b7trie\u00b7ben", "und", "ge\u00b7fan\u00b7gen", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Indessen, da\u00df mit Anmuth und Vergn\u00fcgen", "tokens": ["In\u00b7des\u00b7sen", ",", "da\u00df", "mit", "An\u00b7muth", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die neuen V\u00f6gel, par bey par,", "tokens": ["Die", "neu\u00b7en", "V\u00f6\u00b7gel", ",", "par", "bey", "par", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Durch dieses Netz bald hier, bald dar,", "tokens": ["Durch", "die\u00b7ses", "Netz", "bald", "hier", ",", "bald", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADV", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dem Schein nach fest, doch frey, unaufgehalten fliegen.", "tokens": ["Dem", "Schein", "nach", "fest", ",", "doch", "frey", ",", "un\u00b7auf\u00b7ge\u00b7hal\u00b7ten", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PTKVZ", "$,", "ADV", "ADJD", "$,", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}