{"textgrid.poem.65763": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "37", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein verlebter Vater freudevoll", "tokens": ["Wie", "ein", "ver\u00b7leb\u00b7ter", "Va\u00b7ter", "freu\u00b7de\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich labt an seines Sohnes Jugendklarheit,", "tokens": ["Sich", "labt", "an", "sei\u00b7nes", "Soh\u00b7nes", "Ju\u00b7gend\u00b7klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erbl\u00fcht auch mir, durch Gl\u00fcckes \u00e4rgsten Groll", "tokens": ["Er\u00b7bl\u00fcht", "auch", "mir", ",", "durch", "Gl\u00fc\u00b7ckes", "\u00e4rgs\u00b7ten", "Groll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "$,", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gel\u00e4hmten, all mein Trost aus deines Wertes Wahrheit.", "tokens": ["Ge\u00b7l\u00e4hm\u00b7ten", ",", "all", "mein", "Trost", "aus", "dei\u00b7nes", "Wer\u00b7tes", "Wahr\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn, ob Geburt, Witz, Sch\u00f6nheit, Reichtum nun,", "tokens": ["Denn", ",", "ob", "Ge\u00b7burt", ",", "Witz", ",", "Sch\u00f6n\u00b7heit", ",", "Reich\u00b7tum", "nun", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "NN", "$,", "NN", "$,", "NN", "$,", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gesondert oder alle, ja noch mehr,", "tokens": ["Ge\u00b7son\u00b7dert", "o\u00b7der", "al\u00b7le", ",", "ja", "noch", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PIS", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In deiner Gaben Flor gekr\u00f6net ruh'n,", "tokens": ["In", "dei\u00b7ner", "Ga\u00b7ben", "Flor", "ge\u00b7kr\u00f6\u00b7net", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Pfropf' ich mein Lieben auf dies Tugendheer.", "tokens": ["Pfropf'", "ich", "mein", "Lie\u00b7ben", "auf", "dies", "Tu\u00b7gend\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So bin ich denn nicht lahm, arm noch versto\u00dfen,", "tokens": ["So", "bin", "ich", "denn", "nicht", "lahm", ",", "arm", "noch", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn so der Schatten mir zum Wesen sich verkehrt;", "tokens": ["Wenn", "so", "der", "Schat\u00b7ten", "mir", "zum", "We\u00b7sen", "sich", "ver\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word