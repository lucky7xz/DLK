{"dta.poem.14051": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Studenten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling ist der Freudensaal,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "der", "Freu\u00b7den\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel tausend V\u00f6glein spielen auf,", "tokens": ["Viel", "tau\u00b7send", "V\u00f6\u00b7glein", "spie\u00b7len", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schallt's im Wald bergab, bergauf:", "tokens": ["Da", "schallt's", "im", "Wald", "berg\u00b7ab", ",", "ber\u00b7gauf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df' dich, mein Schatz, viel tausendmal!", "tokens": ["Gr\u00fc\u00df'", "dich", ",", "mein", "Schatz", ",", "viel", "tau\u00b7send\u00b7mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}