{"textgrid.poem.63176": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ibykos", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hasse das Weib.", "tokens": ["Ich", "has\u00b7se", "das", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Sie hat die Erdkugel auseinandergerissen in zwei Br\u00fcste,", "tokens": ["Sie", "hat", "die", "Erd\u00b7ku\u00b7gel", "aus\u00b7ein\u00b7an\u00b7der\u00b7ge\u00b7ris\u00b7sen", "in", "zwei", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "CARD", "NN", "$,"], "meter": "-+-+--+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Zwei H\u00e4lften, die kein T\u00f6pfer mehr zusammenkittet.", "tokens": ["Zwei", "H\u00e4lf\u00b7ten", ",", "die", "kein", "T\u00f6p\u00b7fer", "mehr", "zu\u00b7sam\u00b7men\u00b7kit\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihre Haare sind schlammiges Moos", "tokens": ["Ih\u00b7re", "Haa\u00b7re", "sind", "schlam\u00b7mi\u00b7ges", "Moos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Aus dem Teiche der Tr\u00fcbsal.", "tokens": ["Aus", "dem", "Tei\u00b7che", "der", "Tr\u00fcb\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr Ruf ist der Ruf der br\u00fcnstigen Unke.", "tokens": ["Ihr", "Ruf", "ist", "der", "Ruf", "der", "br\u00fcns\u00b7ti\u00b7gen", "Un\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ihre Beine stahl sie der Gazelle,", "tokens": ["Ih\u00b7re", "Bei\u00b7ne", "stahl", "sie", "der", "Ga\u00b7zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihren Schoss einer fleischfressenden Pflanze,", "tokens": ["Ih\u00b7ren", "Schoss", "ei\u00b7ner", "fleischfres\u00b7sen\u00b7den", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Ihre Ohren der Spitzmaus.", "tokens": ["Ih\u00b7re", "Oh\u00b7ren", "der", "Spitz\u00b7maus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Ihre Augen dem Maulwurf, als er schlief. \u2013", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "dem", "Maul\u00b7wurf", ",", "als", "er", "schlief", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Ibykos bin ich aus Rhegium,", "tokens": ["I\u00b7by\u00b7kos", "bin", "ich", "aus", "Rhe\u00b7gi\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Wohl erfahren in sanftem und wildem Melos.", "tokens": ["Wohl", "er\u00b7fah\u00b7ren", "in", "sanf\u00b7tem", "und", "wil\u00b7dem", "Me\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Polykrates dem Tyrannen", "tokens": ["Po\u00b7ly\u00b7kra\u00b7tes", "dem", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sang ich die Liebe der delphischen Knaben,", "tokens": ["Sang", "ich", "die", "Lie\u00b7be", "der", "del\u00b7phi\u00b7schen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und Samos l\u00e4chelte meinem Gesang.", "tokens": ["Und", "Sa\u00b7mos", "l\u00e4\u00b7chel\u00b7te", "mei\u00b7nem", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Der Helden gedacht ich", "tokens": ["Der", "Hel\u00b7den", "ge\u00b7dacht", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "In chorischen Liedern,", "tokens": ["In", "cho\u00b7ri\u00b7schen", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Enkomien sann ich", "tokens": ["En\u00b7ko\u00b7mi\u00b7en", "sann", "ich"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und Hyporchemen dem Apoll", "tokens": ["Und", "Hy\u00b7por\u00b7che\u00b7men", "dem", "A\u00b7poll"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Und zur Kythara und Fl\u00f6te", "tokens": ["Und", "zur", "Ky\u00b7tha\u00b7ra", "und", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Die heiligen Nomen.", "tokens": ["Die", "hei\u00b7li\u00b7gen", "No\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Eros", "tokens": ["E\u00b7ros"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Der Kypria hitziger Sohn", "tokens": ["Der", "Kyp\u00b7ria", "hit\u00b7zi\u00b7ger", "Sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Hat mein Herz verwundet.", "tokens": ["Hat", "mein", "Herz", "ver\u00b7wun\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Es rinnt das Blut", "tokens": ["Es", "rinnt", "das", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Und tr\u00e4nkt die Fr\u00fchlingserde", "tokens": ["Und", "tr\u00e4nkt", "die", "Fr\u00fch\u00b7lings\u00b7er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Und d\u00fcngt die Sommererde,", "tokens": ["Und", "d\u00fcngt", "die", "Som\u00b7me\u00b7rer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Dass reicher reife", "tokens": ["Dass", "rei\u00b7cher", "rei\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Der kydonische Apfelbaum,", "tokens": ["Der", "ky\u00b7do\u00b7ni\u00b7sche", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Um den die feldblumenduftenden Dryaden spielen", "tokens": ["Um", "den", "die", "feld\u00b7blu\u00b7men\u00b7duf\u00b7ten\u00b7den", "Dry\u00b7a\u00b7den", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Und die bocksger\u00fcchigen Satyrn.", "tokens": ["Und", "die", "bocks\u00b7ge\u00b7r\u00fc\u00b7chi\u00b7gen", "Sa\u00b7tyrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.32": {"text": "O komm,", "tokens": ["O", "komm", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.33": {"text": "Knabe,", "tokens": ["Kna\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.34": {"text": "Dem der Flaum die Oberlippe noch nicht verunziert,", "tokens": ["Dem", "der", "Flaum", "die", "O\u00b7berl\u00b7ip\u00b7pe", "noch", "nicht", "ver\u00b7un\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Springe,", "tokens": ["Sprin\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Du thrakisches F\u00fcllen!", "tokens": ["Du", "thr\u00b7a\u00b7ki\u00b7sches", "F\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Auf deiner nackten braunen Haut", "tokens": ["Auf", "dei\u00b7ner", "nack\u00b7ten", "brau\u00b7nen", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Spiegelt sich l\u00fcstern die Sonne.", "tokens": ["Spie\u00b7gelt", "sich", "l\u00fcs\u00b7tern", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.39": {"text": "Der Wind w\u00fchlt in deinem Gelock.", "tokens": ["Der", "Wind", "w\u00fchlt", "in", "dei\u00b7nem", "Ge\u00b7lock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.40": {"text": "Dem matt ins Gras Sinkenden", "tokens": ["Dem", "matt", "ins", "Gras", "Sin\u00b7ken\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPRART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "\u00d6ffnet die Erde den jungfr\u00e4ulichen Schoss.", "tokens": ["\