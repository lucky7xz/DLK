{"textgrid.poem.59973": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Veni, creator spiritus!", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sprich, was soll es werden", "tokens": ["O", "sprich", ",", "was", "soll", "es", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit dir, du deutscher Geist!", "tokens": ["Mit", "dir", ",", "du", "deut\u00b7scher", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist ja auf der Erden", "tokens": ["Du", "bist", "ja", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entfremdet und verwaist!", "tokens": ["Ent\u00b7frem\u00b7det", "und", "ver\u00b7waist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df sehn, ob du noch rei\u00dfen", "tokens": ["La\u00df", "sehn", ",", "ob", "du", "noch", "rei\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dich magst aus deinem Bann", "tokens": ["Dich", "magst", "aus", "dei\u00b7nem", "Bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ob der Stein der Weisen", "tokens": ["Und", "ob", "der", "Stein", "der", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch Funken geben kann!", "tokens": ["Noch", "Fun\u00b7ken", "ge\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wirf ab die Wolkenh\u00fclle,", "tokens": ["Wirf", "ab", "die", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wirf ab dein himmlisch Kleid,", "tokens": ["Wirf", "ab", "dein", "himm\u00b7lisch", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und st\u00fcrz dich in die F\u00fclle", "tokens": ["Und", "st\u00fcrz", "dich", "in", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ganzen Sterblichkeit,", "tokens": ["Der", "gan\u00b7zen", "Sterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Steig ins gemeine Leben", "tokens": ["Steig", "ins", "ge\u00b7mei\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von deinem kalten Thron,", "tokens": ["Von", "dei\u00b7nem", "kal\u00b7ten", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ins Leben und ins Streben", "tokens": ["Ins", "Le\u00b7ben", "und", "ins", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von einer Nation.", "tokens": ["Von", "ei\u00b7ner", "Na\u00b7tion", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Du hattest dich so scheue", "tokens": ["Du", "hat\u00b7test", "dich", "so", "scheu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Pergament verbaut;", "tokens": ["In", "Per\u00b7ga\u00b7ment", "ver\u00b7baut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schliefst du wie ein Leue", "tokens": ["Da", "schliefst", "du", "wie", "ein", "Leu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einer Eselshaut \u2013", "tokens": ["In", "ei\u00b7ner", "E\u00b7sels\u00b7haut", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir k\u00f6nnen solche Pfiffe", "tokens": ["Wir", "k\u00f6n\u00b7nen", "sol\u00b7che", "Pfif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bei L\u00f6wen nicht verstehn;", "tokens": ["Bei", "L\u00f6\u00b7wen", "nicht", "ver\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O L\u00f6we, la\u00df die Griffe", "tokens": ["O", "L\u00f6\u00b7we", ",", "la\u00df", "die", "Grif\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVIMP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Statt der Begriffe sehn.", "tokens": ["Statt", "der", "Be\u00b7grif\u00b7fe", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Zerrei\u00df, o Geist, die Netze,", "tokens": ["Zer\u00b7rei\u00df", ",", "o", "Geist", ",", "die", "Net\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drein dumpfer Wahn uns flicht;", "tokens": ["Drein", "dum\u00b7pfer", "Wahn", "uns", "flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du gabst genug Gesetze,", "tokens": ["Du", "gabst", "ge\u00b7nug", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Oh, halte dein Gericht!", "tokens": ["Oh", ",", "hal\u00b7te", "dein", "Ge\u00b7richt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fall in die schn\u00f6den Horden,", "tokens": ["Fall", "in", "die", "schn\u00f6\u00b7den", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ein z\u00fcndender Wetterstrahl,", "tokens": ["Ein", "z\u00fcn\u00b7den\u00b7der", "Wet\u00b7ter\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die mit dem Golde morden,", "tokens": ["Die", "mit", "dem", "Gol\u00b7de", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Und heile mit dem Stahl!", "tokens": ["Und", "hei\u00b7le", "mit", "dem", "Stahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Freiheit, Glutgedanke,", "tokens": ["O", "Frei\u00b7heit", ",", "Glut\u00b7ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschaffe deine Welt,", "tokens": ["Er\u00b7schaf\u00b7fe", "dei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und brich die letzte Schranke,", "tokens": ["Und", "brich", "die", "letz\u00b7te", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dich gefangenh\u00e4lt;", "tokens": ["Die", "dich", "ge\u00b7fan\u00b7gen\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht mehr mit mildem Glanze", "tokens": ["Nicht", "mehr", "mit", "mil\u00b7dem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Umleuchte unsre Stirn,", "tokens": ["Um\u00b7leuch\u00b7te", "uns\u00b7re", "Stirn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im Kriegsschmuck, mit der Lanze", "tokens": ["Im", "Kriegs\u00b7schmuck", ",", "mit", "der", "Lan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Spring aus des Denkers Hirn!", "tokens": ["Spring", "aus", "des", "Den\u00b7kers", "Hirn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hervor aus deiner Stille,", "tokens": ["Her\u00b7vor", "aus", "dei\u00b7ner", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darin du br\u00fctend liegst!", "tokens": ["Da\u00b7rin", "du", "br\u00fc\u00b7tend", "liegst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinaus, ein Riesenwille,", "tokens": ["Hin\u00b7aus", ",", "ein", "Rie\u00b7sen\u00b7wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Damit du endlich siegst!", "tokens": ["Da\u00b7mit", "du", "end\u00b7lich", "siegst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als freie Tat, o Wonne,", "tokens": ["Als", "frei\u00b7e", "Tat", ",", "o", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In die Welt mit k\u00fchnem Schwung,", "tokens": ["In", "die", "Welt", "mit", "k\u00fch\u00b7nem", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie eine rote Sonne", "tokens": ["Wie", "ei\u00b7ne", "ro\u00b7te", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus bleicher D\u00e4mmerung!", "tokens": ["Aus", "blei\u00b7cher", "D\u00e4m\u00b7me\u00b7rung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Wir m\u00fcssen uns verwandeln,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "ver\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Puppenzeit ist aus,", "tokens": ["Die", "Pup\u00b7pen\u00b7zeit", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": 