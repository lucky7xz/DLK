{"textgrid.poem.61144": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bist du als K\u00fcnstler, als Poet gesendet,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du als K\u00fcnstler, als Poet gesendet,", "tokens": ["Bist", "du", "als", "K\u00fcnst\u00b7ler", ",", "als", "Po\u00b7et", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "NN", "$,", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O la\u00df dich nicht vom Preis des Marktes leiten!", "tokens": ["O", "la\u00df", "dich", "nicht", "vom", "Preis", "des", "Mark\u00b7tes", "lei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn sinnlos hat die Welt zu allen Zeiten", "tokens": ["Denn", "sinn\u00b7los", "hat", "die", "Welt", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An Mittelm\u00e4\u00df'ges ihre Gunst verschwendet.", "tokens": ["An", "Mit\u00b7tel\u00b7m\u00e4\u00df'\u00b7ges", "ih\u00b7re", "Gunst", "ver\u00b7schwen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zeig' ihr ein Bild vom Genius vollendet,", "tokens": ["Zeig'", "ihr", "ein", "Bild", "vom", "Ge\u00b7nius", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drauf alle Himmel stille Glorien breiten,", "tokens": ["Drauf", "al\u00b7le", "Him\u00b7mel", "stil\u00b7le", "Glo\u00b7ri\u00b7en", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und eins, wo grell und roh die Farben streiten:", "tokens": ["Und", "eins", ",", "wo", "grell", "und", "roh", "die", "Far\u00b7ben", "strei\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du wirst es sehn, wohin ihr Herz sich wendet.", "tokens": ["Du", "wirst", "es", "sehn", ",", "wo\u00b7hin", "ihr", "Herz", "sich", "wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nein, ihrem Tadeln l\u00e4chle, ihrem Loben;", "tokens": ["Nein", ",", "ih\u00b7rem", "Ta\u00b7deln", "l\u00e4ch\u00b7le", ",", "ih\u00b7rem", "Lo\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast genug der Wonnen eingetauscht,", "tokens": ["Du", "hast", "ge\u00b7nug", "der", "Won\u00b7nen