{"textgrid.poem.39497": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Heut sich end'gen wird, was Morgen bringen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Heut sich end'gen wird, was Morgen bringen,", "tokens": ["Wie", "Heut", "sich", "en\u00b7d'\u00b7gen", "wird", ",", "was", "Mor\u00b7gen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PRF", "VVINF", "VAFIN", "$,", "PWS", "NN", "VVINF", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ich wei\u00df es nicht; doch streu' ich gerne Saaten.", "tokens": ["Ich", "wei\u00df", "es", "nicht", ";", "doch", "streu'", "ich", "ger\u00b7ne", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie la\u00dfe Luft und Boden dann gerathen,", "tokens": ["Sie", "la\u00b7\u00dfe", "Luft", "und", "Bo\u00b7den", "dann", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch meine Tr\u00e4gheit soll es nicht mi\u00dflingen.", "tokens": ["Durch", "mei\u00b7ne", "Tr\u00e4g\u00b7heit", "soll", "es", "nicht", "mi\u00df\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kenn' ich nur mich, was frag' ich nach den Dingen?", "tokens": ["Kenn'", "ich", "nur", "mich", ",", "was", "frag'", "ich", "nach", "den", "Din\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "$,", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meiner Brust versteh' ich Andrer Thaten.", "tokens": ["In", "mei\u00b7ner", "Brust", "ver\u00b7steh'", "ich", "A\u00b7ndrer", "Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Weisheit mu\u00df mir Ma\u00df und Stille rathen,", "tokens": ["Die", "Weis\u00b7heit", "mu\u00df", "mir", "Ma\u00df", "und", "Stil\u00b7le", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf da\u00df nicht blindlings meine Kr\u00e4fte ringen.", "tokens": ["Auf", "da\u00df", "nicht", "blind\u00b7lings", "mei\u00b7ne", "Kr\u00e4f\u00b7te", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PTKNEG", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den ew'gen Schlangenkrei\u00df, der uns umfahet,", "tokens": ["Den", "ew'\u00b7gen", "Schlan\u00b7gen\u00b7krei\u00df", ",", "der", "uns", "um\u00b7fa\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.