{"dta.poem.4284": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Steinbock.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Steh ich gleich hier auf ebnem Boden, so tritt mich doch", "tokens": ["Steh", "ich", "gleich", "hier", "auf", "eb\u00b7nem", "Bo\u00b7den", ",", "so", "tritt", "mich", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "ein Schwindel an,", "tokens": ["ein", "Schwin\u00b7del", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mein Blick, verleitet durch die Kunst, verf\u00fchrt fast meinen", "tokens": ["Mein", "Blick", ",", "ver\u00b7lei\u00b7tet", "durch", "die", "Kunst", ",", "ver\u00b7f\u00fchrt", "fast", "mei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geist. Ich kann", "tokens": ["Geist", ".", "Ich", "kann"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Kaum glauben, auf der steilen Felsen und schroffen ungeheu-", "tokens": ["Kaum", "glau\u00b7ben", ",", "auf", "der", "stei\u00b7len", "Fel\u00b7sen", "und", "schrof\u00b7fen", "un\u00b7ge\u00b7heu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "ren H\u00f6hen", "tokens": ["ren", "H\u00f6\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "(die selbst die Wolken \u00fcbersteigen) zerborstnen Gipfeln", "tokens": ["(", "die", "selbst", "die", "Wol\u00b7ken", "\u00fc\u00b7bers\u00b7tei\u00b7gen", ")", "zer\u00b7borst\u00b7nen", "Gip\u00b7feln"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "ADV", "ART", "NN", "VVPP", "$(", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "nicht zu stehen;", "tokens": ["nicht", "zu", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Allein der unverzagte Steinbock, der auf den Spitzen sicher", "tokens": ["Al\u00b7lein", "der", "un\u00b7ver\u00b7zag\u00b7te", "Stein\u00b7bock", ",", "der", "auf", "den", "Spit\u00b7zen", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "ist,", "tokens": ["ist", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Macht, da\u00df mein Geist, der ihn betrachtet, dar\u00fcber der Ge-", "tokens": ["Macht", ",", "da\u00df", "mein", "Geist", ",", "der", "ihn", "be\u00b7trach\u00b7tet", ",", "da\u00b7r\u00fc\u00b7ber", "der", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PAV", "ART", "TRUNC"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "fahr vergi\u00dft.", "tokens": ["fahr", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Ich \u00fcberleg\u2019, auf welche Weise, durch seiner H\u00f6rner St\u00e4rk\u2019", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7leg'", ",", "auf", "wel\u00b7che", "Wei\u00b7se", ",", "durch", "sei\u00b7ner", "H\u00f6r\u00b7ner", "St\u00e4rk'"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PWAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "und Pracht,", "tokens": ["und", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Der Sch\u00f6pfer diese Klippen-B\u00fcrger so sicher f\u00fcr Gefahr", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "die\u00b7se", "Klip\u00b7pen\u00b7B\u00fcr\u00b7ger", "so", "si\u00b7cher", "f\u00fcr", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "gemacht;", "tokens": ["ge\u00b7macht", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Da er nicht nur daran sich h\u00e4nget, nein, da er, wenn er auch", "tokens": ["Da", "er", "nicht", "nur", "da\u00b7ran", "sich", "h\u00e4n\u00b7get", ",", "nein", ",", "da", "er", ",", "wenn", "er", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PAV", "PRF", "VVFIN", "$,", "PTKANT", "$,", "KOUS", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "wo f\u00e4llt,", "tokens": ["wo", "f\u00e4llt", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Da er sie vorwirft, seinen Kopf besch\u00fctzet, da\u00df er nicht", "tokens": ["Da", "er", "sie", "vor\u00b7wirft", ",", "sei\u00b7nen", "Kopf", "be\u00b7sch\u00fct\u00b7zet", ",", "da\u00df", "er", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "zerschellt.", "tokens": ["zer\u00b7schellt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Es trotzet sein verwegnes Aug\u2019 und fester Schenkel alle", "tokens": ["Es", "trot\u00b7zet", "sein", "ver\u00b7weg\u00b7nes", "Aug'", "und", "fes\u00b7ter", "Schen\u00b7kel", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "Gr\u00fcfte,", "tokens": ["Gr\u00fcf\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Er fliegt fast mehr, als da\u00df er springt, schnell \u00fcber alle tiefe", "tokens": ["Er", "fliegt", "fast", "mehr", ",", "als", "da\u00df", "er", "springt", ",", "schnell", "\u00fc\u00b7ber", "al\u00b7le", "tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.24": {"text": "Kl\u00fcfte.", "tokens": ["Kl\u00fcf\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Wohin sich fast kein Vogel schwingt, dahin gelangt sein", "tokens": ["Wo\u00b7hin", "sich", "fast", "kein", "Vo\u00b7gel", "schwingt", ",", "da\u00b7hin", "ge\u00b7langt", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADV", "PIAT", "NN", "VVFIN", "$,", "PAV", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "reger Fu\u00df.", "tokens": ["re\u00b7ger", "Fu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.27": {"text": "Wer ist denn, der die weise Macht der Gottheit nicht bewun-", "tokens": ["Wer", "ist", "denn", ",", "der", "die", "wei\u00b7se", "Macht", "der", "Got\u00b7theit", "nicht", "be\u00b7wun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "dern mu\u00df,", "tokens": ["dern", "mu\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Die auch die hohe Felsen-Welt so weislich zu bev\u00f6lkern", "tokens": ["Die", "auch", "die", "ho\u00b7he", "Fel\u00b7sen\u00b7Welt", "so", "weis\u00b7lich", "zu", "be\u00b7v\u00f6l\u00b7kern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "wei\u00df,", "tokens": ["wei\u00df", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Auch Geister dergestalt bereitet, da\u00df sie, in wohlgezognen", "tokens": ["Auch", "Geis\u00b7ter", "der\u00b7ge\u00b7stalt", "be\u00b7rei\u00b7tet", ",", "da\u00df", "sie", ",", "in", "wohl\u00b7ge\u00b7zog\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "ADV", "VVFIN", "$,", "KOUS", "PPER", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "Z\u00fcgen,", "tokens": ["Z\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Des Sch\u00f6pfers Werk so deutlich zeigen, und, GOtt zu Ehren,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Werk", "so", "deut\u00b7lich", "zei\u00b7gen", ",", "und", ",", "Gott", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVINF", "$,", "KON", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "uns vergn\u00fcgen?", "tokens": ["uns", "ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Dir sey demnach, Quell\u2019 aller Dinge, auch darinn Ehre,", "tokens": ["Dir", "sey", "dem\u00b7nach", ",", "Quell'", "al\u00b7ler", "Din\u00b7ge", ",", "auch", "da\u00b7rinn", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "NN", "PIAT", "NN", "$,", "ADV", "PAV", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Lob und Preis!", "tokens": ["Lob", "und", "Preis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}