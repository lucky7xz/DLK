{"textgrid.poem.52695": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Hirt", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt die Nacht mit ihren k\u00fchlen Schatten", "tokens": ["Kommt", "die", "Nacht", "mit", "ih\u00b7ren", "k\u00fch\u00b7len", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ueber alles Land;", "tokens": ["Ue\u00b7ber", "al\u00b7les", "Land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schwer bedunkelt schlafen schon die Matten", "tokens": ["Schwer", "be\u00b7dun\u00b7kelt", "schla\u00b7fen", "schon", "die", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "An der Felsenwand.", "tokens": ["An", "der", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und her\u00fcber zieht der Wind,", "tokens": ["Und", "her\u00b7\u00fc\u00b7ber", "zieht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Leiser Schauer fa\u00dft die Glieder \u2013", "tokens": ["Lei\u00b7ser", "Schau\u00b7er", "fa\u00dft", "die", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "O mein liebes Kind,", "tokens": ["O", "mein", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Wann sehen wir uns wieder?", "tokens": ["Wann", "se\u00b7hen", "wir", "uns", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fand dich nicht zu Hause bei den Eltern,", "tokens": ["Fand", "dich", "nicht", "zu", "Hau\u00b7se", "bei", "den", "El\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fand dich nicht bei mir,", "tokens": ["Fand", "dich", "nicht", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Fr\u00fche sucht ich dich auf allen Feldern", "tokens": ["Fr\u00fc\u00b7he", "sucht", "ich", "dich", "auf", "al\u00b7len", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und am Abend hier.", "tokens": ["Und", "am", "A\u00b7bend", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Keinen Gru\u00df, kein Lebewohl?", "tokens": ["Kei\u00b7nen", "Gru\u00df", ",", "kein", "Le\u00b7be\u00b7wohl", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tiefe Nacht und tiefes Schweigen \u2013", "tokens": ["Tie\u00b7fe", "Nacht", "und", "tie\u00b7fes", "Schwei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "O mein Kind schlaf wohl,", "tokens": ["O", "mein", "Kind", "schlaf", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Bis die Lerchen steigen.", "tokens": ["Bis", "die", "Ler\u00b7chen", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auf den Fluren bin ich noch alleine", "tokens": ["Auf", "den", "Flu\u00b7ren", "bin", "ich", "noch", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und mein Herz mit mir.", "tokens": ["Und", "mein", "Herz", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sieh! der Mond mit liebetrautem Scheine", "tokens": ["Sieh", "!", "der", "Mond", "mit", "lie\u00b7be\u00b7trau\u00b7tem", "Schei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kommt die Wolken f\u00fcr.", "tokens": ["Kommt", "die", "Wol\u00b7ken", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "O du treues, goldnes Licht!", "tokens": ["O", "du", "treu\u00b7es", ",", "gold\u00b7nes", "Licht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leuchte mir zu n\u00e4chtgen Schritten,", "tokens": ["Leuch\u00b7te", "mir", "zu", "n\u00e4cht\u00b7gen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wei\u00dft was mir gebricht,", "tokens": ["Wei\u00dft", "was", "mir", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Was ich schon gelitten.", "tokens": ["Was", "ich", "schon", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Berge starren, dunkle W\u00e4lder rauschen,", "tokens": ["Ber\u00b7ge", "star\u00b7ren", ",", "dunk\u00b7le", "W\u00e4l\u00b7der", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heilig ist es hier.", "tokens": ["Hei\u00b7lig", "ist", "es", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wind und Wellen will ich scheu belauschen,", "tokens": ["Wind", "und", "Wel\u00b7len", "will", "ich", "scheu", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fl\u00fcstern sie von dir?", "tokens": ["Fl\u00fcs\u00b7tern", "sie", "von", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Von den Wiesen steigt der Duft,", "tokens": ["Von", "den", "Wie\u00b7sen", "steigt", "der", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sanfte Geister weben drinnen,", "tokens": ["Sanf\u00b7te", "Geis\u00b7ter", "we\u00b7ben", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis der Morgen ruft", "tokens": ["Bis", "der", "Mor\u00b7gen", "ruft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und sie scheucht von hinnen.", "tokens": ["Und", "sie", "scheucht", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Manche Nacht schon bin ich umgewandelt,", "tokens": ["Man\u00b7che", "Nacht", "schon", "bin", "ich", "um\u00b7ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mutterseelenallein,", "tokens": ["Mut\u00b7ter\u00b7see\u00b7len\u00b7al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Was die Sternlein unter sich verhandelt,", "tokens": ["Was", "die", "Stern\u00b7lein", "un\u00b7ter", "sich", "ver\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist Geheimni\u00df mein.", "tokens": ["Ist", "Ge\u00b7heim\u00b7ni\u00df", "mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und ihr Schweigen auch ist Gold,", "tokens": ["Und", "ihr", "Schwei\u00b7gen", "auch", "ist", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Viele wissens nicht zu deuten,", "tokens": ["Vie\u00b7le", "wis\u00b7sens", "nicht", "zu", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber wem sie hold,", "tokens": ["A\u00b7ber", "wem", "sie", "hold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Dem gelingts bei Zeiten.", "tokens": ["Dem", "ge\u00b7lingts", "bei", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Du, nur du, mein Engel, sollst erfahren", "tokens": ["Du", ",", "nur", "du", ",", "mein", "En\u00b7gel", ",", "sollst", "er\u00b7fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was ich hier geh\u00f6rt,", "tokens": ["Was", "ich", "hier", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich wills im stillen Busen wahren,", "tokens": ["Und", "ich", "wills", "im", "stil\u00b7len", "Bu\u00b7sen", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis ich dichs gelehrt.", "tokens": ["Bis", "ich", "dichs", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Gl\u00fccklich werden wir einmal!", "tokens": ["Gl\u00fcck\u00b7lich", "wer\u00b7den", "wir", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses m\u00f6gen Alle h\u00f6ren \u2013", "tokens": ["Die\u00b7ses", "m\u00f6\u00b7gen", "Al\u00b7le", "h\u00f6\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn wir uns einmal", "tokens": ["Wenn", "wir", "uns", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV"], "meter": "++-+-", "measure": "iambic.di"}, "line.8": {"text": "Einzig angeh\u00f6ren.", "tokens": ["Ein\u00b7zig", "an\u00b7ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Kommt die Nacht mit ihren k\u00fchlen Schatten", "tokens": ["Kommt", "die", "Nacht", "mit", "ih\u00b7ren", "k\u00fch\u00b7len", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ueber alles Land;", "tokens": ["Ue\u00b7ber", "al\u00b7les", "Land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schwer bedunkelt schlafen schon die Matten", "tokens": ["Schwer", "be\u00b7dun\u00b7kelt", "schla\u00b7fen", "schon", "die", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "An der Felsenwand.", "tokens": ["An", "der", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und her\u00fcber zieht der Wind,", "tokens": ["Und", "her\u00b7\u00fc\u00b7ber", "zieht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Leiser Schauer fa\u00dft die Glieder \u2013", "tokens": ["Lei\u00b7ser", "Schau\u00b7er", "fa\u00dft", "die", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN"