{"dta.poem.1114": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eingang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel pflanzt uns selbst die Neigung zu euch ein/ ", "tokens": ["Der", "Him\u00b7mel", "pflanzt", "uns", "selbst", "die", "Nei\u00b7gung", "zu", "euch", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum will ich euer Lob/ ihr Nimphen/ izt erheben.", "tokens": ["Drum", "will", "ich", "eu\u00b7er", "Lob", "/", "ihr", "Nim\u00b7phen", "/", "izt", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob gleich der Warheit scheint mein Wort zu widerstreben/", "tokens": ["Ob", "gleich", "der", "War\u00b7heit", "scheint", "mein", "Wort", "zu", "wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer Frauenzimmer lobt/ kan nimmer unrecht seyn.", "tokens": ["Wer", "Frau\u00b7en\u00b7zim\u00b7mer", "lobt", "/", "kan", "nim\u00b7mer", "un\u00b7recht", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$(", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was schadet in der H\u00f6h der Sonne lichtem Schein/", "tokens": ["Was", "scha\u00b7det", "in", "der", "H\u00f6h", "der", "Son\u00b7ne", "lich\u00b7tem", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob sie in Wolcken steckt/ ob D\u00fcnste sie umgeben?", "tokens": ["Ob", "sie", "in", "Wol\u00b7cken", "steckt", "/", "ob", "D\u00fcns\u00b7te", "sie", "um\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "KOUS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So mu\u00df auch eure Zier stets siegend oben schweben/", "tokens": ["So", "mu\u00df", "auch", "eu\u00b7re", "Zier", "stets", "sie\u00b7gend", "o\u00b7ben", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist sie gleich dann und wann verdeckt und minder rein.", "tokens": ["Ist", "sie", "gleich", "dann", "und", "wann", "ver\u00b7deckt", "und", "min\u00b7der", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "KON", "PWAV", "VVPP", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Glantz von oben her feurt eure Sinnen an/", "tokens": ["Ein", "Glantz", "von", "o\u00b7ben", "her", "feurt", "eu\u00b7re", "Sin\u00b7nen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "APZR", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und spielet unvermerckt auff uns mit so viel Strahlen/", "tokens": ["Und", "spie\u00b7let", "un\u00b7ver\u00b7merckt", "auff", "uns", "mit", "so", "viel", "Strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "APPR", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Da\u00df sie kein Mangel nicht von aussen blenden kan.", "tokens": ["Da\u00df", "sie", "kein", "Man\u00b7gel", "nicht", "von", "aus\u00b7sen", "blen\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "PTKNEG", "APPR", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hilff mir/ Terpsichore/ der Welt f\u00fcr Augen mahlen/", "tokens": ["Hilff", "mir", "/", "Ter\u00b7psi\u00b7cho\u00b7re", "/", "der", "Welt", "f\u00fcr", "Au\u00b7gen", "mah\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "NE", "$(", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Da\u00df/ wann man die Vernunfft der Warheit leget bey/", "tokens": ["Da\u00df", "/", "wann", "man", "die", "Ver\u00b7nunfft", "der", "War\u00b7heit", "le\u00b7get", "bey", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Frauen Alle sch\u00f6n/ und keine greulich sey.", "tokens": ["Die", "Frau\u00b7en", "Al\u00b7le", "sch\u00f6n", "/", "und", "kei\u00b7ne", "greu\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJD", "$(", "KON", "PIAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}