{"textgrid.poem.61223": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Unter der Lorelei", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00fchl der Felsen dunkelt", "tokens": ["Wie", "k\u00fchl", "der", "Fel\u00b7sen", "dun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hernieder in den Rhein!", "tokens": ["Her\u00b7nie\u00b7der", "in", "den", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Strahl der Sonne funkelt", "tokens": ["Kein", "Strahl", "der", "Son\u00b7ne", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im gr\u00fcnen Wasserschein.", "tokens": ["Im", "gr\u00fc\u00b7nen", "Was\u00b7ser\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es kommt im Windesweben", "tokens": ["Es", "kommt", "im", "Win\u00b7des\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Gru\u00df der M\u00e4rchenzeit -", "tokens": ["Ein", "Gru\u00df", "der", "M\u00e4r\u00b7chen\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie fern von hier das Leben!", "tokens": ["Wie", "fern", "von", "hier", "das", "Le\u00b7ben", "!"], "token_info