{"dta.poem.20954": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.  \n Cardinals Richelieu.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein Cardinal der Franckreich hat gezieret/", "tokens": ["Hier", "liegt", "ein", "Car\u00b7di\u00b7nal", "der", "Fran\u00b7ck\u00b7reich", "hat", "ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und seine Lilien durch manches Land gef\u00fchret.", "tokens": ["Und", "sei\u00b7ne", "Li\u00b7li\u00b7en", "durch", "man\u00b7ches", "Land", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er band ", "tokens": ["Er", "band"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Durch ihn kan Ludewig ein Alexander seyn.", "tokens": ["Durch", "ihn", "kan", "Lu\u00b7de\u00b7wig", "ein", "A\u00b7lex\u00b7an\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "NE", "ART", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}