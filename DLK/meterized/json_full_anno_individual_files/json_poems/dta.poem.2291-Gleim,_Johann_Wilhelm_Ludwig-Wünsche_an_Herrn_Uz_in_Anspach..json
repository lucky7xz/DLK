{"dta.poem.2291": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcnsche  \n an  \n Herrn Uz in Anspach.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt ich wider Willen lachen,", "tokens": ["K\u00f6nnt", "ich", "wi\u00b7der", "Wil\u00b7len", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt ich, was ich wolte, machen,", "tokens": ["K\u00f6nnt", "ich", ",", "was", "ich", "wol\u00b7te", ",", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt ich iedem, und vor allen,", "tokens": ["K\u00f6nnt", "ich", "ie\u00b7dem", ",", "und", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "$,", "KON", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen Sch\u00f6nen wolgefallen,", "tokens": ["Al\u00b7len", "Sch\u00f6\u00b7nen", "wol\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt ich niemals beim Erwehlen,", "tokens": ["K\u00f6nnt", "ich", "nie\u00b7mals", "beim", "Er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Wahl des Besten fehlen,", "tokens": ["In", "der", "Wahl", "des", "Bes\u00b7ten", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00f6nnt ich allen braven Sch\u00f6nen,", "tokens": ["K\u00f6nnt", "ich", "al\u00b7len", "bra\u00b7ven", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Sitten angew\u00f6hnen,", "tokens": ["Mei\u00b7ne", "Sit\u00b7ten", "an\u00b7ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "K\u00f6nnt ich stets, in iedem Leben,", "tokens": ["K\u00f6nnt", "ich", "stets", ",", "in", "ie\u00b7dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "K\u00fcsse nehmen, K\u00fcsse geben,", "tokens": ["K\u00fcs\u00b7se", "neh\u00b7men", ",", "K\u00fcs\u00b7se", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "K\u00f6nnt ich mich in Scherz und Lieben,", "tokens": ["K\u00f6nnt", "ich", "mich", "in", "Scherz", "und", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Stets, wie diesen Abend, \u00fcben,", "tokens": ["Stets", ",", "wie", "die\u00b7sen", "A\u00b7bend", ",", "\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PDAT", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "K\u00f6nnt ich mitten im Vergn\u00fcgen", "tokens": ["K\u00f6nnt", "ich", "mit\u00b7ten", "im", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dich, mein Uz, zu k\u00fcssen kriegen;", "tokens": ["Dich", ",", "mein", "Uz", ",", "zu", "k\u00fcs\u00b7sen", "krie\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "K\u00f6nnt ich denn bei solchen Freuden", "tokens": ["K\u00f6nnt", "ich", "denn", "bei", "sol\u00b7chen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Meines F\u00fcrsten Gl\u00fckk beneiden?", "tokens": ["Mei\u00b7nes", "F\u00fcrs\u00b7ten", "Gl\u00fckk", "be\u00b7nei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}