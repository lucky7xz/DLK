{"dta.poem.4247": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die,  \n  in g\u00f6ttlichen Werken,  \n in  GOTT  vergn\u00fcgte Seele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durchs Auge sieht mein Geist die Welt, und freut sich ihrer", "tokens": ["Durchs", "Au\u00b7ge", "sieht", "mein", "Geist", "die", "Welt", ",", "und", "freut", "sich", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pracht und Zier;", "tokens": ["Pracht", "und", "Zier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Allein der Geist sieht weiter noch: Er siehet Dich, mein GOtt,", "tokens": ["Al\u00b7lein", "der", "Geist", "sieht", "wei\u00b7ter", "noch", ":", "Er", "sie\u00b7het", "Dich", ",", "mein", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "in ihr.", "tokens": ["in", "ihr", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Es zeigt auf Erden und am Himmel der Sonnen Glanz sich", "tokens": ["Es", "zeigt", "auf", "Er\u00b7den", "und", "am", "Him\u00b7mel", "der", "Son\u00b7nen", "Glanz", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPRART", "NN", "ART", "NN", "NN", "PRF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "nicht so klar;", "tokens": ["nicht", "so", "klar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Die Ordnung macht dein g\u00f6ttlich Daseyn viel deutlicher", "tokens": ["Die", "Ord\u00b7nung", "macht", "dein", "g\u00f6tt\u00b7lich", "Da\u00b7seyn", "viel", "deut\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "noch offenbar.", "tokens": ["noch", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Er fa\u00dft, da\u00df nichts sich selber bilden, und sich in Ordnung", "tokens": ["Er", "fa\u00dft", ",", "da\u00df", "nichts", "sich", "sel\u00b7ber", "bil\u00b7den", ",", "und", "sich", "in", "Ord\u00b7nung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PRF", "ADV", "VVINF", "$,", "KON", "PRF", "APPR", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "setzen kann.", "tokens": ["set\u00b7zen", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Zeigt denn die Bildung aller Dinge nicht Einen, Der sie", "tokens": ["Zeigt", "denn", "die", "Bil\u00b7dung", "al\u00b7ler", "Din\u00b7ge", "nicht", "Ei\u00b7nen", ",", "Der", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PIAT", "NN", "PTKNEG", "ART", "$,", "ART", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "bildet, an?", "tokens": ["bil\u00b7det", ",", "an", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Was je der menschliche Verstand ersonnen und hervorge-", "tokens": ["Was", "je", "der", "menschli\u00b7che", "Ver\u00b7stand", "er\u00b7son\u00b7nen", "und", "her\u00b7vor\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVINF", "KON", "TRUNC"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "bracht,", "tokens": ["bracht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Ist nichts, bringt man es in Vergleichung mit dem, was die", "tokens": ["Ist", "nichts", ",", "bringt", "man", "es", "in", "Ver\u00b7glei\u00b7chung", "mit", "dem", ",", "was", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "VVFIN", "PIS", "PPER", "APPR", "NN", "APPR", "ART", "$,", "PRELS", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Natur gemacht.", "tokens": ["Na\u00b7tur", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Ein jeder Baum, ein\u2019 jede Bluhme kann allem Witz der", "tokens": ["Ein", "je\u00b7der", "Baum", ",", "ein'", "je\u00b7de", "Bluh\u00b7me", "kann", "al\u00b7lem", "Witz", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN", "VMFIN", "PIS", "NN", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Menschen zeigen,", "tokens": ["Men\u00b7schen", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Wie sehr die Werke der Natur die Kunst der Menschen", "tokens": ["Wie", "sehr", "die", "Wer\u00b7ke", "der", "Na\u00b7tur", "die", "Kunst", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u00fcbersteigen.", "tokens": ["\u00fc\u00b7bers\u00b7tei\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Was kl\u00fcger nun, als dein Verstand, von dem kann dein", "tokens": ["Was", "kl\u00fc\u00b7ger", "nun", ",", "als", "dein", "Ver\u00b7stand", ",", "von", "dem", "kann", "dein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "APPR", "PDS", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Verstand nicht denken,", "tokens": ["Ver\u00b7stand", "nicht", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Da\u00df es von ohngefehr, ohn\u2019 Absicht, sich in so richt\u2019ge", "tokens": ["Da\u00df", "es", "von", "ohn\u00b7ge\u00b7fehr", ",", "ohn'", "Ab\u00b7sicht", ",", "sich", "in", "so", "richt'\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "$,", "KOUI", "NN", "$,", "PRF", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Ordnung lenken,", "tokens": ["Ord\u00b7nung", "len\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und, ohne Weisheit, bilden soll; wo je ein Widerspruch kann", "tokens": ["Und", ",", "oh\u00b7ne", "Weis\u00b7heit", ",", "bil\u00b7den", "soll", ";", "wo", "je", "ein", "Wi\u00b7der\u00b7spruch", "kann"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUI", "NN", "$,", "VVINF", "VMFIN", "$.", "PWAV", "ADV", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "seyn,", "tokens": ["seyn", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "So schl\u00f6\u00df ein solcher falscher Satz ihn, folglich was unm\u00f6g-", "tokens": ["So", "schl\u00f6\u00df", "ein", "sol\u00b7cher", "fal\u00b7scher", "Satz", "ihn", ",", "folg\u00b7lich", "was", "un\u00b7m\u00f6g"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "ADJA", "NN", "PPER", "$,", "ADV", "PWS", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "lichs, ein.", "tokens": ["lichs", ",", "ein", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Hiewider ist nichts einzuwenden, als da\u00df dein Aug\u2019 Jhn nicht", "tokens": ["Hie\u00b7wi\u00b7der", "ist", "nichts", "ein\u00b7zu\u00b7wen\u00b7den", ",", "als", "da\u00df", "dein", "Aug'", "Jhn", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "VVIZU", "$,", "KOKOM", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "erblickt.", "tokens": ["er\u00b7blickt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Allein ist denn dein Auge Richter? Ist seine Kraft doch nicht", "tokens": ["Al\u00b7lein", "ist", "denn", "dein", "Au\u00b7ge", "Rich\u00b7ter", "?", "Ist", "sei\u00b7ne", "Kraft", "doch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$.", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "geschickt,", "tokens": ["ge\u00b7schickt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Die Luft, die ganz unleugbar da, und die ein C\u00f6rper ist, zu", "tokens": ["Die", "Luft", ",", "die", "ganz", "un\u00b7leug\u00b7bar", "da", ",", "und", "die", "ein", "C\u00f6r\u00b7per", "ist", ",", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "PTKVZ", "$,", "KON", "ART", "ART", "NN", "VAFIN", "$,", "APPR"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.10": {"text": "sehen.", "tokens": ["se\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Wie kann, im c\u00f6rperlichen Spiegel, man sich mit Recht", "tokens": ["Wie", "kann", ",", "im", "c\u00f6r\u00b7per\u00b7li\u00b7chen", "Spie\u00b7gel", ",", "man", "sich", "mit", "Recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "$,", "APPRART", "ADJA", "NN", "$,", "PIS", "PRF", "APPR", "NN"], "meter": "-+-+---+-++-+", "measure": "unknown.measure.hexa"}, "line.12": {"text": "wohl unterstehen,", "tokens": ["wohl", "un\u00b7ter\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Das Wesen eines Geist\u2019s zu schauen? Wann nun ein Geist", "tokens": ["Das", "We\u00b7sen", "ei\u00b7nes", "Geist's", "zu", "schau\u00b7en", "?", "Wann", "nun", "ein", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$.", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "nicht sichtbar ist,", "tokens": ["nicht", "sicht\u00b7bar", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Wie da\u00df dein c\u00f6rperliches Wesen, o Mensch! sich denn so sehr", "tokens": ["Wie", "da\u00df", "dein", "c\u00f6r\u00b7per\u00b7li\u00b7ches", "We\u00b7sen", ",", "o", "Mensch", "!", "sich", "denn", "so", "sehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$.", "PRF", "ADV", "ADV", "ADV"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.16": {"text": "vergi\u00dft!", "tokens": ["ver\u00b7gi\u00dft", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Du willt die Gottheit anders seh\u2019n, als in den dir gezeigten", "tokens": ["Du", "willt", "die", "Got\u00b7theit", "an\u00b7ders", "seh'n", ",", "als", "in", "den", "dir", "ge\u00b7zeig\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,", "KOUS", "APPR", "ART", "PPER", "VVPP"], "meter": "-+-+-+-+-++--+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Werken,", "tokens": ["Wer\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Worinn doch Weisheit, Liebe, Macht, die GOttes Wesen,", "tokens": ["Wo\u00b7rinn", "doch", "Weis\u00b7heit", ",", "Lie\u00b7be", ",", "Macht", ",", "die", "Got\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "$,", "NN", "$,", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "klar zu merken.", "tokens": ["klar", "zu", "mer\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Die Sonne zeiget ihre Strahlen, und ihr die Welt versch\u00f6-", "tokens": ["Die", "Son\u00b7ne", "zei\u00b7get", "ih\u00b7re", "Strah\u00b7len", ",", "und", "ihr", "die", "Welt", "ver\u00b7sch\u00f6"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "nernd Licht", "tokens": ["nernd", "Licht"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Des C\u00f6rpers Augen \u00fcberall so sichtbar und so deutlich nicht,", "tokens": ["Des", "C\u00f6r\u00b7pers", "Au\u00b7gen", "\u00fc\u00b7be\u00b7rall", "so", "sicht\u00b7bar", "und", "so", "deut\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "ADJD", "KON", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.24": {"text": "Als GOttes wunderbare Werk\u2019 Sein unleugbares Wesen", "tokens": ["Als", "Got\u00b7tes", "wun\u00b7der\u00b7ba\u00b7re", "Werk'", "Sein", "un\u00b7leug\u00b7ba\u00b7res", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.25": {"text": "weisen.", "tokens": ["wei\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Wer wollte denn, in ihrer Sch\u00f6nheit, sich Sein nicht freuen,", "tokens": ["Wer", "woll\u00b7te", "denn", ",", "in", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", ",", "sich", "Sein", "nicht", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "PRF", "PPOSAT", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Jhn nicht preisen?", "tokens": ["Jhn", "nicht", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Komm, la\u00df uns denn, in froher Andacht, Jhn nach Verm\u00f6-", "tokens": ["Komm", ",", "la\u00df", "uns", "denn", ",", "in", "fro\u00b7her", "An\u00b7dacht", ",", "Jhn", "nach", "Ver\u00b7m\u00f6"], "token_info": ["word", "punct", "word", "word", "word