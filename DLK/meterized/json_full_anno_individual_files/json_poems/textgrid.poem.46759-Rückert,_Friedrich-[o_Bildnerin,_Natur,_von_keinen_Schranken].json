{"textgrid.poem.46759": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o Bildnerin, Natur, von keinen Schranken]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Bildnerin, Natur, von keinen Schranken", "tokens": ["O", "Bild\u00b7ne\u00b7rin", ",", "Na\u00b7tur", ",", "von", "kei\u00b7nen", "Schran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist deiner Phantasie Gebiet umgrenzet;", "tokens": ["Ist", "dei\u00b7ner", "Phan\u00b7ta\u00b7sie", "Ge\u00b7biet", "um\u00b7gren\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie dein Odem wintert oder lenzet,", "tokens": ["Und", "wie", "dein", "O\u00b7dem", "win\u00b7tert", "o\u00b7der", "len\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entflittert er und kr\u00e4uselt neue Ranken.", "tokens": ["Ent\u00b7flit\u00b7tert", "er", "und", "kr\u00e4u\u00b7selt", "neu\u00b7e", "Ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie Blasen stiegen auf im Strom und sanken,", "tokens": ["Wie", "Bla\u00b7sen", "stie\u00b7gen", "auf", "im", "Strom", "und", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "APPRART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fort der Strom im Schein der Sonne gl\u00e4nzet;", "tokens": ["Und", "fort", "der", "Strom", "im", "Schein", "der", "Son\u00b7ne", "gl\u00e4n\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie unter Stirnen, die der Lorbeer kr\u00e4nzet,", "tokens": ["Wie", "un\u00b7ter", "Stir\u00b7nen", ",", "die", "der", "Lor\u00b7beer", "kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aufblitzen und zerflattern Lichtgedanken:", "tokens": ["Auf\u00b7blit\u00b7zen", "und", "zer\u00b7flat\u00b7tern", "Licht\u00b7ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht alle werden dauernde Gebilde,", "tokens": ["Nicht", "al\u00b7le", "wer\u00b7den", "dau\u00b7ern\u00b7de", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sch\u00f6nsten sterben, eh sie sind geboren,", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "ster\u00b7ben", ",", "eh", "sie", "sind", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "