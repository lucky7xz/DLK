{"textgrid.poem.52926": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Mekka zieht der Araber auf stolperndem Kamele,", "tokens": ["Nach", "Mek\u00b7ka", "zieht", "der", "A\u00b7ra\u00b7ber", "auf", "stol\u00b7pern\u00b7dem", "Ka\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-----+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und so der Dichter nach Berlin auf holperndem Ghasele.", "tokens": ["Und", "so", "der", "Dich\u00b7ter", "nach", "Ber\u00b7lin", "auf", "hol\u00b7pern\u00b7dem", "Gha\u00b7se\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+---+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Berlin ist Deutschlands Orient, und wenn ihm Palmen mangeln,", "tokens": ["Ber\u00b7lin", "ist", "Deutschlands", "O\u00b7rient", ",", "und", "wenn", "ihm", "Pal\u00b7men", "man\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "NN", "$,", "KON", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sagt doch niemand in der Welt, da\u00df Sand und Staub ihm fehle;", "tokens": ["So", "sagt", "doch", "nie\u00b7mand", "in", "der", "Welt", ",", "da\u00df", "Sand", "und", "Staub", "ihm", "feh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "APPR", "ART", "NN", "$,", "KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Berlin ist Deutschlands Minarett, und statt der Muezzin schreien", "tokens": ["Ber\u00b7lin", "ist", "Deutschlands", "Mi\u00b7na\u00b7rett", ",", "und", "statt", "der", "Muez\u00b7zin", "schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "NE", "NN", "$,", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Sich tausend Journalisten wund die wohl-geschmierte Kehle.", "tokens": ["Sich", "tau\u00b7send", "Jour\u00b7na\u00b7lis\u00b7ten", "wund", "die", "wohl\u00b7ge\u00b7schmier\u00b7te", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "CARD", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Dann sinken im Gebete hin die Gl\u00e4ubigen und Frommen,", "tokens": ["Dann", "sin\u00b7ken", "im", "Ge\u00b7be\u00b7te", "hin", "die", "Gl\u00e4u\u00b7bi\u00b7gen", "und", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Ein Pietisten-Derwisch tanzt, kasteiend Leib und Seele;", "tokens": ["Ein", "Pi\u00b7e\u00b7tis\u00b7ten\u00b7Der\u00b7wisch", "tanzt", ",", "kas\u00b7tei\u00b7end", "Leib", "und", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Im Fusel-O