, "tokens": ["Sich", "wei\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Und nun gelassen habe", "tokens": ["Und", "nun", "ge\u00b7las\u00b7sen", "ha\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Am Grabe", "tokens": ["Am", "Gra\u00b7be"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ich dich, doch nicht an meinem,", "tokens": ["Ich", "dich", ",", "doch", "nicht", "an", "mei\u00b7nem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "ADV", "PTKNEG", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "An deinem", "tokens": ["An", "dei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "La\u00df ich, soll ich dich lassen,", "tokens": ["La\u00df", "ich", ",", "soll", "ich", "dich", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Mich fassen,", "tokens": ["Mich", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Um weiter in die Weiten", "tokens": ["Um", "wei\u00b7ter", "in", "die", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Zu schreiten,", "tokens": ["Zu", "schrei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Zu schreiten weiter ohne", "tokens": ["Zu", "schrei\u00b7ten", "wei\u00b7ter", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die Krone", "tokens": ["Die", "Kro\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Des Lebens, die mich schm\u00fcckte,", "tokens": ["Des", "Le\u00b7bens", ",", "die", "mich", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Begl\u00fcckte,", "tokens": ["Be\u00b7gl\u00fcck\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Die Krone nicht des Goldes,", "tokens": ["Die", "Kro\u00b7ne", "nicht", "des", "Gol\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Mein holdes", "tokens": ["Mein", "hol\u00b7des"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Herzeinz'ges Angebindchen,", "tokens": ["Herzeinz'\u00b7ges", "An\u00b7ge\u00b7bind\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Goldkindchen!", "tokens": ["Gold\u00b7kind\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "So la\u00df ich dich im Staube?", "tokens": ["So", "la\u00df", "ich", "dich", "im", "Stau\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich glaube", "tokens": ["Ich", "glau\u00b7be"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dich \u00fcberm Staub hinwehen", "tokens": ["Dich", "\u00fc\u00b7berm", "Staub", "hin\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu sehen.", "tokens": ["Zu", "se\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So ist doch eingetroffen", "tokens": ["So", "ist", "doch", "ein\u00b7ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Hoffen,", "tokens": ["Mein", "Hof\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Wenn's auch sich anders findet:", "tokens": ["Wenn's", "auch", "sich", "an\u00b7ders", "fin\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es windet", "tokens": ["Es", "win\u00b7det"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Vom Grabe, wo die beiden", "tokens": ["Vom", "Gra\u00b7be", ",", "wo", "die", "bei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sich scheiden,", "tokens": ["Sich", "schei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Dein Weg dar\u00fcber heiter", "tokens": ["Dein", "Weg", "da\u00b7r\u00fc\u00b7ber", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sich weiter,", "tokens": ["Sich", "wei\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Und meiner geht im Dunkeln", "tokens": ["Und", "mei\u00b7ner", "geht", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Doch funkeln", "tokens": ["Doch", "fun\u00b7keln"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Von obenher dem Dichter", "tokens": ["Von", "o\u00b7ben\u00b7her", "dem", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die Lichter,", "tokens": ["Die", "Lich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Die dich im Engelreigen", "tokens": ["Die", "dich", "im", "En\u00b7gel\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Mir zeigen,", "tokens": ["Mir", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Auf mich herniederl\u00e4chelnd,", "tokens": ["Auf", "mich", "her\u00b7nie\u00b7der\u00b7l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Und f\u00e4chelnd,", "tokens": ["Und", "f\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Da\u00df ich darunter heiter", "tokens": ["Da\u00df", "ich", "da\u00b7run\u00b7ter", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Geh weiter.", "tokens": ["Geh", "wei\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Hin \u00fcbers Grab, das deine", "tokens": ["Hin", "\u00fc\u00b7bers", "Grab", ",", "das", "dei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Ans meine,", "tokens": ["Ans", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Hin \u00fcbers Grab des Lebens", "tokens": ["Hin", "\u00fc\u00b7bers", "Grab", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Und Strebens,", "tokens": ["Und", "Stre\u00b7bens", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Das Grab der Erdenwonne,", "tokens": ["Das", "Grab",