, "measure": "iambic.tri"}, "line.5": {"text": "Hier steh' ich voll Grauen,", "tokens": ["Hier", "steh'", "ich", "voll", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und schwindelnd, j\u00e4hen Sturzes,", "tokens": ["Und", "schwin\u00b7delnd", ",", "j\u00e4\u00b7hen", "Stur\u00b7zes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vom Kraterrande des Feuerberges", "tokens": ["Vom", "Kra\u00b7ter\u00b7ran\u00b7de", "des", "Feu\u00b7er\u00b7ber\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gleitet der Geist mir hinab", "tokens": ["Glei\u00b7tet", "der", "Geist", "mir", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "In die unterirdischen Hallen,", "tokens": ["In", "die", "un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Wo deine Kinder, die finsteren Erdgewalten,", "tokens": ["Wo", "dei\u00b7ne", "Kin\u00b7der", ",", "die", "fins\u00b7te\u00b7ren", "Erd\u00b7ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wie schlummernde Riesen", "tokens": ["Wie", "schlum\u00b7mern\u00b7de", "Rie\u00b7sen"], "t