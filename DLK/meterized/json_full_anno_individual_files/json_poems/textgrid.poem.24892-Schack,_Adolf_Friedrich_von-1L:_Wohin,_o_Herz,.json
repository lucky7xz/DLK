{"textgrid.poem.24892": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin, o Herz,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, o Herz,", "tokens": ["Wo\u00b7hin", ",", "o", "Herz", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das fort und fort im Busen mich stachelt,", "tokens": ["Das", "fort", "und", "fort", "im", "Bu\u00b7sen", "mich", "sta\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "KON", "PTKVZ", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In welches Wagnis mich hast du verlockt?", "tokens": ["In", "wel\u00b7ches", "Wag\u00b7nis", "mich", "hast", "du", "ver\u00b7lockt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf himmelnahem Gipfel,", "tokens": ["Auf", "him\u00b7mel\u00b7na\u00b7hem", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den kaum der Gedanke erklimmt,", "tokens": ["Den", "kaum", "der", "Ge\u00b7dan\u00b7ke", "er\u00b7klimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Der einzig Atmende ich,", "tokens": ["Der", "ein\u00b7zig", "At\u00b7men\u00b7de", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Im unendlichen Raume verloren;", "tokens": ["Im", "un\u00b7end\u00b7li\u00b7chen", "Rau\u00b7me", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "H\u00f6her als ich nur der strahlende Orion,", "tokens": ["H\u00f6\u00b7her", "als", "ich", "nur", "der", "strah\u00b7len\u00b7de", "O\u00b7rion", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Den Schild durchs Unerme\u00dfliche streckend!", "tokens": ["Den", "Schild", "durchs", "Un\u00b7er\u00b7me\u00df\u00b7li\u00b7che", "stre\u00b7ckend", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Unten die Tiefe, die bodenlose,", "tokens": ["Un\u00b7ten", "die", "Tie\u00b7fe", ",", "die", "bo\u00b7den\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Drin Meer und Inseln begraben.", "tokens": ["Drin", "Meer", "und", "In\u00b7seln", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Uralte Nacht,", "tokens": ["Ur\u00b7al\u00b7te", "Nacht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Riesige Sphinx, die in dunkler Brust", "tokens": ["Rie\u00b7si\u00b7ge", "Sphinx", ",", "die", "in", "dunk\u00b7ler", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Des Daseins R\u00e4tsel du h\u00fctest,", "tokens": ["Des", "Da\u00b7seins", "R\u00e4t\u00b7sel", "du", "h\u00fc\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An deines Reiches Pforten", "tokens": ["An", "dei\u00b7nes", "Rei\u00b7ches", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hier steh' ich voll Grauen,", "tokens": ["Hier", "steh'", "ich", "voll", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und schwindelnd, j\u00e4hen Sturzes,", "tokens": ["Und", "schwin\u00b7delnd", ",", "j\u00e4\u00b7hen", "Stur\u00b7zes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vom Kraterrande des Feuerberges", "tokens": ["Vom", "Kra\u00b7ter\u00b7ran\u00b7de", "des", "Feu\u00b7er\u00b7ber\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gleitet der Geist mir hinab", "tokens": ["Glei\u00b7tet", "der", "Geist", "mir", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "In die unterirdischen Hallen,", "tokens": ["In", "die", "un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Wo deine Kinder, die finsteren Erdgewalten,", "tokens": ["Wo", "dei\u00b7ne", "Kin\u00b7der", ",", "die", "fins\u00b7te\u00b7ren", "Erd\u00b7ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wie schlummernde Riesen", "tokens": ["Wie", "schlum\u00b7mern\u00b7de", "Rie\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Auf ihren Lagern ruhn.", "tokens": ["Auf", "ih\u00b7ren", "La\u00b7gern", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So durch des Menschen Seele", "tokens": ["So", "durch", "des", "Men\u00b7schen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fchren tiefe Schachte,", "tokens": ["F\u00fch\u00b7ren", "tie\u00b7fe", "Schach\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "D\u00fcstere, vielgewundne,", "tokens": ["D\u00fcs\u00b7te\u00b7re", ",", "viel\u00b7ge\u00b7wund\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Hinab in Finsternis,", "tokens": ["Hin\u00b7ab", "in", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und oft, hinunterstarrend,", "tokens": ["Und", "oft", ",", "hin\u00b7un\u00b7ter\u00b7star\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In sich selbst zu versinken zagt sie.", "tokens": ["In", "sich", "selbst", "zu", "ver\u00b7sin\u00b7ken", "zagt", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Furchtbare M\u00e4chte", "tokens": ["Furcht\u00b7ba\u00b7re", "M\u00e4ch\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Schlummern in ihrer Tiefe;", "tokens": ["Schlum\u00b7mern", "in", "ih\u00b7rer", "Tie\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Weh, wenn die entsetzlichen,", "tokens": ["Weh", ",", "wenn", "die", "ent\u00b7setz\u00b7li\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "Vom Unheil geweckt,", "tokens": ["Vom", "Un\u00b7heil", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Die schlaftrunknen H\u00e4upter sch\u00fctteln!", "tokens": ["Die", "schlaf\u00b7trunk\u00b7nen", "H\u00e4up\u00b7ter", "sch\u00fct\u00b7teln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie die Titanen dort unten,", "tokens": ["Wie", "die", "Ti\u00b7ta\u00b7nen", "dort", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Des schwarzen Kerkers Pforten sprengend,", "tokens": ["Des", "schwar\u00b7zen", "Ker\u00b7kers", "Pfor\u00b7ten", "spren\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ihr Fest der Zerst\u00f6rung feiern,", "tokens": ["Ihr", "Fest", "der", "Zer\u00b7st\u00f6\u00b7rung", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Gewitternd so aus der Seele Abgrund", "tokens": ["Ge\u00b7wit\u00b7ternd", "so", "aus", "der", "See\u00b7le", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Steigen die grausen D\u00e4monen", "tokens": ["Stei\u00b7gen", "die", "grau\u00b7sen", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.17": {"text": "Verzweiflung, Wahnsinn,", "tokens": ["Ver\u00b7zwei\u00b7flung", ",", "Wahn\u00b7sinn", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Mit Wirbelrauch", "tokens": ["Mit", "Wir\u00b7bel\u00b7rauch"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Ihr todgeweihtes Opfer umh\u00fcllend.", "tokens": ["Ihr", "tod\u00b7ge\u00b7weih\u00b7tes", "Op\u00b7fer", "um\u00b7h\u00fcl\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber was zuckt durch das Dunkel?", "tokens": ["A\u00b7ber", "was", "zuckt", "durch", "das", "Dun\u00b7kel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "D\u00e4mmernd am Himmelsrande", "tokens": ["D\u00e4m\u00b7mernd", "am", "Him\u00b7mels\u00b7ran\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Glimmt es empor,", "tokens": ["Glimmt", "es", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein Flammenglanz umspielt den Gipfel,", "tokens": ["Ein", "Flam\u00b7men\u00b7glanz", "um\u00b7spielt", "den", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo gleich Adlern in L\u00fcften ich schwebe;", "tokens": ["Wo", "gleich", "Ad\u00b7lern", "in", "L\u00fcf\u00b7ten", "ich", "schwe\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wie gl\u00fchende Tropfen", "tokens": ["Wie", "gl\u00fc\u00b7hen\u00b7de", "Trop\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sinken die Sterne", "tokens": ["Sin\u00b7ken", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In die Wirbel des steigenden Tages;", "tokens": ["In", "die", "Wir\u00b7bel", "des", "stei\u00b7gen\u00b7den", "Ta\u00b7ges", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Unten in schwindelnder Tiefe", "tokens": ["Un\u00b7ten", "in", "schwin\u00b7deln\u00b7der", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Leuchtet und blitzt mit den duftenden Inseln", "tokens": ["Leuch\u00b7tet", "und", "blitzt", "mit", "den", "duf\u00b7ten\u00b7den", "In\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Der unerme\u00dfliche Ocean,", "tokens": ["Der", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7che", "O\u00b7cean", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Und allein, allein,", "tokens": ["Und", "al\u00b7lein", ",", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Wie in der Seele ein gro\u00dfer Gedanke,", "tokens": ["Wie", "in", "der", "See\u00b7le", "ein", "gro\u00b7\u00dfer", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Schreitet der Lichtgeist", "tokens": ["Schrei\u00b7tet", "der", "Licht\u00b7geist"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Ueber den Weltrand.", "tokens": ["Ue\u00b7ber", "den", "Welt\u00b7rand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Heil, Glorreich-Herrlicher!", "tokens": ["Heil", ",", "Glor\u00b7reich\u00b7Herr\u00b7li\u00b7cher", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Durch alle R\u00e4ume", "tokens": ["Durch", "al\u00b7le", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bis in des Dunkels tiefste Falten,", "tokens": ["Bis", "in", "des", "Dun\u00b7kels", "tiefs\u00b7te", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Seele verborgensten Abgrund", "tokens": ["Der", "See\u00b7le", "ver\u00b7bor\u00b7gens\u00b7ten", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "La\u00df deine Feuerstr\