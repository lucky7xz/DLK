{"textgrid.poem.27818": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist voller Raub. Sie raubet Gott die Ehre", "tokens": ["Die", "Welt", "ist", "vol\u00b7ler", "Raub", ".", "Sie", "rau\u00b7bet", "Gott", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibt sie ihr nur selbst. Sie raubt sein Wort und Lehre,", "tokens": ["Und", "gibt", "sie", "ihr", "nur", "selbst", ".", "Sie", "raubt", "sein", "Wort", "und", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Ordnung und Befehl und setzt an dessen stat,", "tokens": ["Sein", "Ord\u00b7nung", "und", "Be\u00b7fehl", "und", "setzt", "an", "des\u00b7sen", "stat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "KON", "VVFIN", "APPR", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ihr gev\u00f6llter Wanst zur Zeit getraumet hat.", "tokens": ["Was", "ihr", "ge\u00b7v\u00f6ll\u00b7ter", "Wanst", "zur", "Zeit", "ge\u00b7trau\u00b7met", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drauff raubt der Teuffel nun das Gl\u00fcck und allen Segen", "tokens": ["Drauff", "raubt", "der", "Teuf\u00b7fel", "nun", "das", "Gl\u00fcck", "und", "al\u00b7len", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und pflegt hingegen nichts, denn Unmut, zu erregen.", "tokens": ["Und", "pflegt", "hin\u00b7ge\u00b7gen", "nichts", ",", "denn", "Un\u00b7mut", ",", "zu", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "$,", "KON", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er raubet Fried und Ruh, er raubt die gute Zeit,", "tokens": ["Er", "rau\u00b7bet", "Fried", "und", "Ruh", ",", "er", "raubt", "die", "gu\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er raubet Scham und Zucht, er raubt die Seligkeit.", "tokens": ["Er", "rau\u00b7bet", "Scham", "und", "Zucht", ",", "er", "raubt", "die", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Mensch beraubt den Mensch an dem, das ihm gegeben", "tokens": ["Der", "Mensch", "be\u00b7raubt", "den", "Mensch", "an", "dem", ",", "das", "ihm", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von Leumut, Ehre, Gut, Gesundheit, Wolfahrt, Leben.", "tokens": ["Von", "Leu\u00b7mut", ",", "Eh\u00b7re", ",", "Gut", ",", "Ge\u00b7sund\u00b7heit", ",", "Wol\u00b7fahrt", ",", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJD", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Oberstand raubt hin den letzten Bissen Brot", "tokens": ["Der", "O\u00b7bers\u00b7tand", "raubt", "hin", "den", "letz\u00b7ten", "Bis\u00b7sen", "Brot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und l\u00e4st gemeiner Schaar nichts, als die leere Noth.", "tokens": ["Und", "l\u00e4st", "ge\u00b7mei\u00b7ner", "Schaar", "nichts", ",", "als", "die", "lee\u00b7re", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PIS", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Unterthan raubt weg Gehorsam, Pflicht und Treue,", "tokens": ["Der", "Un\u00b7ter\u00b7than", "raubt", "weg", "Ge\u00b7hor\u00b7sam", ",", "Pflicht", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Furchte vor der Straff und vor den Lastern Scheue.", "tokens": ["Die", "Furch\u00b7te", "vor", "der", "Straff", "und", "vor", "den", "Las\u00b7tern", "Scheu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Liebe, die ein Christ zum Christen billich tr\u00e4gt,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "ein", "Christ", "zum", "Chris\u00b7ten", "bil\u00b7lich", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ist durchau\u00df entraubt, die ist seitab gelegt.", "tokens": ["Die", "ist", "durch\u00b7au\u00df", "en\u00b7traubt", ",", "die", "ist", "seit\u00b7ab", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$,", "PRELS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was macht denn der Soldat? (Das Volk von fremden Sinnen,", "tokens": ["Was", "macht", "denn", "der", "Sol\u00b7dat", "?", "(", "Das", "Volk", "von", "frem\u00b7den", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$.", "$(", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df Menschen man hinfort nicht mehr wird achten k\u00fcnnen.)", "tokens": ["Da\u00df", "Men\u00b7schen", "man", "hin\u00b7fort", "nicht", "mehr", "wird", "ach\u00b7ten", "k\u00fcn\u00b7nen", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "PIS", "ADV", "PTKNEG", "ADV", "VAFIN", "VVINF", "VMINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er h\u00e4tte gar f\u00fcrl\u00e4ngst, wenns ihm nur w\u00e4r erlaubt,", "tokens": ["Er", "h\u00e4t\u00b7te", "gar", "f\u00fcr\u00b7l\u00e4ngst", ",", "wenns", "ihm", "nur", "w\u00e4r", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den Himmel und Gott selbst gepl\u00fcndert und beraubt.", "tokens": ["Den", "Him\u00b7mel", "und", "Gott", "selbst", "ge\u00b7pl\u00fcn\u00b7dert", "und", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Was Rauber hat die Welt! Doch mag ein ieder glauben,", "tokens": ["Was", "Rau\u00b7ber", "hat", "die", "Welt", "!", "Doch", "mag", "ein", "ie\u00b7der", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "ART", "NN", "$.", "KON", "VMFIN", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df den, der so geraubt, man wieder wird berauben,", "tokens": ["Da\u00df", "den", ",", "der", "so", "ge\u00b7raubt", ",", "man", "wie\u00b7der", "wird", "be\u00b7rau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "ADV", "VVPP", "$,", "PIS", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich wett, ob er ihm schon geraubt h\u00e4tt alle Welt,", "tokens": ["Ich", "wett", ",", "ob", "er", "ihm", "schon", "ge\u00b7raubt", "h\u00e4tt", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df er davon doch nichts, als H\u00f6ll und Tod, beh\u00e4lt.", "tokens": ["Da\u00df", "er", "da\u00b7von", "doch", "nichts", ",", "als", "H\u00f6ll", "und", "Tod", ",", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADV", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Welt ist voller Raub. Sie raubet Gott die Ehre", "tokens": ["Die", "Welt", "ist", "vol\u00b7ler", "Raub", ".", "Sie", "rau\u00b7bet", "Gott", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibt sie ihr nur selbst. Sie raubt sein Wort und Lehre,", "tokens": ["Und", "gibt", "sie", "ihr", "nur", "selbst", ".", "Sie", "raubt", "sein", "Wort", "und", "Leh\u