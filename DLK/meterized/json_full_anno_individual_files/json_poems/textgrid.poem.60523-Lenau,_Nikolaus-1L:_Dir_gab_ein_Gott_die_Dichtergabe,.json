{"textgrid.poem.60523": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir gab ein Gott die Dichtergabe,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir gab ein Gott die Dichtergabe,", "tokens": ["Dir", "gab", "ein", "Gott", "die", "Dich\u00b7ter\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Nachen ist der Ruhm bereit,", "tokens": ["Als", "Na\u00b7chen", "ist", "der", "Ruhm", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dir zum Strand Unsterblichkeit", "tokens": ["Mit", "dir", "zum", "Strand", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu tanzen \u00fcberm Wellengrabe;", "tokens": ["Zu", "tan\u00b7zen", "\u00fc\u00b7berm", "Wel\u00b7len\u00b7gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch mu\u00dft du einsam ihn beschreiten,", "tokens": ["Doch", "mu\u00dft", "du", "ein\u00b7sam", "ihn", "be\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mut allein sei dein Gespan!", "tokens": ["Der", "Mut", "al\u00b7lein", "s