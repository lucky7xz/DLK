{"textgrid.poem.26208": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Dr\u00fcckende Botschaft", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abendhimmel br\u00e4unt sich still entz\u00fcndet,", "tokens": ["Der", "A\u00b7bend\u00b7him\u00b7mel", "br\u00e4unt", "sich", "still", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Meer wird blasser und von Dunkelheit umz\u00e4unt.", "tokens": ["Das", "Meer", "wird", "blas\u00b7ser", "und", "von", "Dun\u00b7kel\u00b7heit", "um\u00b7z\u00e4unt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und alte B\u00e4ume, in den Kronen breit ger\u00fcndet,", "tokens": ["Und", "al\u00b7te", "B\u00e4u\u00b7me", ",", "in", "den", "Kro\u00b7nen", "breit", "ge\u00b7r\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wurzeln an dem Weg, der leer zum Wasser m\u00fcndet,", "tokens": ["Die", "wur\u00b7zeln", "an", "dem", "Weg", ",", "der", "leer", "zum", "Was\u00b7ser", "m\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "me