{"textgrid.poem.53418": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Lied an der Mittwoche", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nacht Gefahr und Grauen", "tokens": ["Der", "Nacht", "Ge\u00b7fahr", "und", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist die\u00dfmahl auch vorbey,", "tokens": ["Ist", "die\u00df\u00b7mahl", "auch", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Tag-Licht l\u00e4\u00dft sich schauen,", "tokens": ["Das", "Tag\u00b7Licht", "l\u00e4\u00dft", "sich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das wache Hahn-Geschrey", "tokens": ["Das", "wa\u00b7che", "Hahn\u00b7Ge\u00b7schrey"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sagt, da\u00df es Morgen sey.", "tokens": ["Sagt", ",", "da\u00df", "es", "Mor\u00b7gen", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Welt springt aus dem Bette", "tokens": ["Die", "Welt", "springt", "aus", "dem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Arbeit die sie kan,", "tokens": ["Zur", "Ar\u00b7beit", "die", "sie", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es legt sich umb die Wette", "tokens": ["Es", "legt", "sich", "umb", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zugleich ein jederman", "tokens": ["Zu\u00b7gleich", "ein", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Kleid und Sorgen an.", "tokens": ["Mit", "Kleid", "und", "Sor\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich wil f\u00fcr allen Dingen,", "tokens": ["Ich", "wil", "f\u00fcr", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott, deiner Liebe Macht", "tokens": ["Gott", ",", "dei\u00b7ner", "Lie\u00b7be", "Macht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auff meinem Psalter singen,", "tokens": ["Auff", "mei\u00b7nem", "Psal\u00b7ter", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Du mich diese Nacht", "tokens": ["Da\u00df", "Du", "mich", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So v\u00e4terlich bewacht.", "tokens": ["So", "v\u00e4\u00b7ter\u00b7lich", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich hab' als tod geschlaffen", "tokens": ["Ich", "hab'", "als", "tod", "ge\u00b7schlaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn Sinn und ohn Verstand,", "tokens": ["Ohn", "Sinn", "und", "ohn", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beschirmt durch keine Waffen", "tokens": ["Be\u00b7schirmt", "durch", "kei\u00b7ne", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Sathans starcken Hand,", "tokens": ["F\u00fcr", "Sa\u00b7thans", "star\u00b7cken", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Dieberey und Brand,", "tokens": ["F\u00fcr", "Die\u00b7be\u00b7rey", "und", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ohn da\u00df Du mich verborgen", "tokens": ["Ohn", "da\u00df", "Du", "mich", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deiner Gunst Gezelt", "tokens": ["In", "dei\u00b7ner", "Gunst", "Ge\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hast aus treuen Sorgen", "tokens": ["Und", "hast", "aus", "treu\u00b7en", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Hut umb mich gestellt,", "tokens": ["Dein", "Hut", "umb", "mich", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die uns stets schadlo\u00df h\u00e4lt.", "tokens": ["Die", "uns", "stets", "schad\u00b7lo\u00df", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Kein Vater deckt die Kinder", "tokens": ["Kein", "Va\u00b7ter", "deckt", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So treu des Abends zu,", "tokens": ["So", "treu", "des", "A\u00b7bends", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df jedes ihr nicht minder", "tokens": ["Da\u00df", "je\u00b7des", "ihr", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn er gew\u00fcnschet ruh", "tokens": ["Denn", "er", "ge\u00b7w\u00fcn\u00b7schet", "ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als, treue Gott, mich Du.", "tokens": ["Als", ",", "treu\u00b7e", "Gott", ",", "mich", "Du", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "$,", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So wil ich auch erh\u00f6hen", "tokens": ["So", "wil", "ich", "auch", "er\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich, weil ich leb allhier,", "tokens": ["Dich", ",", "weil", "ich", "leb", "all\u00b7hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Jetzt la\u00df ich mit auffstehen", "tokens": ["Jetzt", "la\u00df", "ich", "mit", "auffs\u00b7te\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "VVINF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Die Seiten, meine Zier,", "tokens": ["Die", "Sei\u00b7ten", ",", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dir dancken einig Dir.", "tokens": ["Dir", "dan\u00b7cken", "ei\u00b7nig", "Dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du bist Israels H\u00fctter,", "tokens": ["Du", "bist", "Is\u00b7raels", "H\u00fct\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wen Du besch\u00fctzest, Gott,", "tokens": ["Wen", "Du", "be\u00b7sch\u00fct\u00b7zest", ",", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den schreckt kein Vngewitter,", "tokens": ["Den", "schreckt", "kein", "Vn\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sch\u00e4tzt der H\u00f6llen Rott", "tokens": ["Er", "sch\u00e4tzt", "der", "H\u00f6l\u00b7len", "Rott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd auch den Tod f\u00fcr Spott.", "tokens": ["Vnd", "auch", "den", "Tod", "f\u00fcr", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er mag zu Lande fahren,", "tokens": ["Er", "mag", "zu", "Lan\u00b7de", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er reise Seewerts ein,", "tokens": ["Er", "rei\u00b7se", "See\u00b7werts", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst ihn wol bewahren,", "tokens": ["Du", "wirst", "ihn", "wol", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm wider alle Pein", "tokens": ["Ihm", "wi\u00b7der", "al\u00b7le", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schild, Burg und Mauer seyn.", "tokens": ["Schild", ",", "Burg", "und", "Mau\u00b7er", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nur nimm mich heut auch wieder", "tokens": ["Nur", "nimm", "mich", "heut", "auch", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mang deiner Engel Schaar,", "tokens": ["Mang", "dei\u00b7ner", "En\u00b7gel", "Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beh\u00fct mir Seel und Glieder,", "tokens": ["Be\u00b7h\u00fct", "mir", "Seel", "und", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Damit ich immerdar", "tokens": ["Da\u00b7mit", "ich", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sey sicher f\u00fcr Gefahr.", "tokens": ["Sey", "si\u00b7cher", "f\u00fcr", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "La\u00df mich bescheiden wandeln", "tokens": ["La\u00df", "mich", "be\u00b7schei\u00b7den", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd redlich allermeist", "tokens": ["Vnd", "red\u00b7lich", "al\u00b7ler\u00b7meist"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit meinem Nechsten handeln", "tokens": ["Mit", "mei\u00b7nem", "Nechs\u00b7ten", "han\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd dempff den Eifer-Geist", "tokens": ["Vnd", "dempff", "den", "Ei\u00b7fer\u00b7Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der mich zu Boden reist.", "tokens": ["Der", "mich", "zu", "Bo\u00b7den", "reist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So m\u00f6cht' ich heut auch fallen", "tokens": ["So", "m\u00f6cht'", "ich", "heut", "auch", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielleicht in Todes Strick.", "tokens": ["Viel\u00b7leicht", "in", "To\u00b7des", "Strick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O gieb, da\u00df ich f\u00fcr allen", "tokens": ["O", "gieb", ",", "da\u00df", "ich", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auff jeden Augenblick", "tokens": ["Auff", "je\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich zu dem Ende schick.", "tokens": ["Mich", "zu", "dem", "En\u00b7de", "schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Herr, Du kanst alles geben,", "tokens": ["Herr", ",", "Du", "kanst", "al\u00b7les", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df mich durch Lieb und Leid", "tokens": ["La\u00df", "mich", "durch", "Lieb", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Dir sterben, Dir auch leben", "tokens": ["Dir", "ster\u00b7ben", ",", "Dir", "auch", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie hier in dieser Zeit", "tokens": ["Wie", "hier", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So dort in Ewigkeit.", "tokens": ["So", "dort", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der Nacht Gefahr und Grauen", "tokens": ["Der", "Nacht", "Ge\u00b7fahr", "und", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist die\u00dfmahl auch vorbey,", "tokens": ["Ist", "die\u00df\u00b7mahl", "auch", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Tag-Licht l\u00e4\u00dft sich schauen,", "tokens": ["Das", "Tag\u00b7Licht", "l\u00e4\u00dft", "sich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das wache Hahn-Geschrey", "tokens": ["Das", "wa\u00b7che", "Hahn\u00b7Ge\u00b7schrey"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sagt, da\u00df es Morgen sey.", "tokens": ["Sagt", ",", "da\u00df", "es", "Mor\u00b7gen", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die Welt springt aus dem Bette", "tokens": ["Die", "Welt", "springt", "aus", "dem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Arbeit die sie kan,", "tokens": ["Zur", "Ar\u00b7beit", "die", "sie", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es legt sich umb die Wette", "tokens": ["Es", "legt", "sich", "umb", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zugleich ein jederman", "tokens": ["Zu\u00b7gleich", "ein", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Kleid und Sorgen an.", "tokens": ["Mit", "Kleid", "und", "Sor\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich wil f\u00fcr allen Dingen,", "tokens": ["Ich", "wil", "f\u00fcr", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott, deiner Liebe Macht", "tokens": ["Gott", ",", "dei\u00b7ner", "Lie\u00b7be", "Macht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auff meinem Psalter singen,", "tokens": ["Auff", "mei\u00b7nem", "Psal\u00b7ter", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Du mich diese Nacht", "tokens": ["Da\u00df", "Du", "mich", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So v\u00e4terlich bewacht.", "tokens": ["So", "v\u00e4\u00b7ter\u00b7lich", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ich hab' als tod geschlaffen", "tokens": ["Ich", "hab'", "als", "tod", "ge\u00b7schlaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn Sinn und ohn Verstand,", "tokens": ["Ohn", "Sinn", "und", "ohn", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beschirmt durch keine Waffen", "tokens": ["Be\u00b7schirmt", "durch", "kei\u00b7ne", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Sathans starcken Hand,", "tokens": ["F\u00fcr", "Sa\u00b7thans", "star\u00b7cken", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Dieberey und Brand,", "tokens": ["F\u00fcr", "Die\u00b7be\u00b7rey", "und", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Ohn da\u00df Du mich verborgen", "tokens": ["Ohn", "da\u00df", "Du", "mich", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deiner Gunst Gezelt", "tokens": ["In", "dei\u00b7ner", "Gunst", "Ge\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hast aus treuen Sorgen", "tokens": ["Und", "hast", "aus", "treu\u00b7en", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Hut umb mich gestellt,", "tokens": ["Dein", "Hut", "umb", "mich", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die uns stets schadlo\u00df h\u00e4lt.", "tokens": ["Die", "uns", "stets", "schad\u00b7lo\u00df", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Kein Vater deckt die Kinder", "tokens": ["Kein", "Va\u00b7ter", "deckt", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So treu des Abends zu,", "tokens": ["So", "treu", "des", "A\u00b7bends", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df jedes ihr nicht minder", "tokens": ["Da\u00df", "je\u00b7des", "ihr", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn er gew\u00fcnschet ruh", "tokens": ["Denn", "er", "ge\u00b7w\u00fcn\u00b7schet", "ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als, treue Gott, mich Du.", "tokens": ["Als", ",", "treu\u00b7e", "Gott", ",", "mich", "Du", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "$,", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "So wil ich auch erh\u00f6hen", "tokens": ["So", "wil", "ich", "auch", "er\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich, weil ich leb allhier,", "tokens": ["Dich", ",", "weil", "ich", "leb", "all\u00b7hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Jetzt la\u00df ich mit auffstehen", "tokens": ["Jetzt", "la\u00df", "ich", "mit", "auffs\u00b7te\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "VVINF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Die Seiten, meine Zier,", "tokens": ["Die", "Sei\u00b7ten", ",", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dir dancken einig Dir.", "tokens": ["Dir", "dan\u00b7cken", "ei\u00b7nig", "Dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Du bist Israels H\u00fctter,", "tokens": ["Du", "bist", "Is\u00b7raels", "H\u00fct\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wen Du besch\u00fctzest, Gott,", "tokens": ["Wen", "Du", "be\u00b7sch\u00fct\u00b7zest", ",", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den schreckt kein Vngewitter,", "tokens": ["Den", "schreckt", "kein", "Vn\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sch\u00e4tzt der H\u00f6llen Rott", "tokens": ["Er", "sch\u00e4tzt", "der", "H\u00f6l\u00b7len", "Rott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd auch den Tod f\u00fcr Spott.", "tokens": ["Vnd", "auch", "den", "Tod", "f\u00fcr", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Er mag zu Lande fahren,", "tokens": ["Er", "mag", "zu", "Lan\u00b7de", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er reise Seewerts ein,", "tokens": ["Er", "rei\u00b7se", "See\u00b7werts", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst ihn wol bewahren,", "tokens": ["Du", "wirst", "ihn", "wol", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm wider alle Pein", "tokens": ["Ihm", "wi\u00b7der", "al\u00b7le", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schild, Burg und Mauer seyn.", "tokens": ["Schild", ",", "Burg", "und", "Mau\u00b7er", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Nur nimm mich heut auch wieder", "tokens": ["Nur", "nimm", "mich", "heut", "auch", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mang deiner Engel Schaar,", "tokens": ["Mang", "dei\u00b7ner", "En\u00b7gel", "Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beh\u00fct mir Seel und Glieder,", "tokens": ["Be\u00b7h\u00fct", "mir", "Seel", "und", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Damit ich immerdar", "tokens": ["Da\u00b7mit", "ich", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sey sicher f\u00fcr Gefahr.", "tokens": ["Sey", "si\u00b7cher", "f\u00fcr", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "La\u00df mich bescheiden wandeln", "tokens": ["La\u00df", "mich", "be\u00b7schei\u00b7den", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd redlich allermeist", "tokens": ["Vnd", "red\u00b7lich", "al\u00b7ler\u00b7meist"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit meinem Nechsten handeln", "tokens": ["Mit", "mei\u00b7nem", "Nechs\u00b7ten", "han\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd dempff den Eifer-Geist", "tokens": ["Vnd", "dempff", "den", "Ei\u00b7fer\u00b7Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der mich zu Boden reist.", "tokens": ["Der", "mich", "zu", "Bo\u00b7den", "reist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "So m\u00f6cht' ich heut auch fallen", "tokens": ["So", "m\u00f6cht'", "ich", "heut", "auch", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielleicht in Todes Strick.", "tokens": ["Viel\u00b7leicht", "in", "To\u00b7des", "Strick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O gieb, da\u00df ich f\u00fcr allen", "tokens": ["O", "gieb", ",", "da\u00df", "ich", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auff jeden Augenblick", "tokens": ["Auff", "je\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich zu dem Ende schick.", "tokens": ["Mich", "zu", "dem", "En\u00b7de", "schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.26": {"line.1": {"text": "Herr, Du kanst alles geben,", "tokens": ["Herr", ",", "Du", "kanst", "al\u00b7les", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df mich durch Lieb und Leid", "tokens": ["La\u00df", "mich", "durch", "Lieb", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Dir sterben, Dir auch leben", "tokens": ["Dir", "ster\u00b7ben", ",", "Dir", "auch", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie hier in dieser Zeit", "tokens": ["Wie", "hier", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So dort in Ewigkeit.", "tokens": ["So", "dort", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}