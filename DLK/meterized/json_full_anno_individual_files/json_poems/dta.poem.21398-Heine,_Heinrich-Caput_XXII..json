{"dta.poem.21398": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch mehr ver\u00e4ndert als die Stadt", "tokens": ["Noch", "mehr", "ver\u00b7\u00e4n\u00b7dert", "als", "die", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind mir die Menschen erschienen,", "tokens": ["Sind", "mir", "die", "Men\u00b7schen", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie geh\u2019n so betr\u00fcbt und gebrochen herum,", "tokens": ["Sie", "geh'n", "so", "be\u00b7tr\u00fcbt", "und", "ge\u00b7bro\u00b7chen", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "KON", "VVPP", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie wandelnde Ruinen.", "tokens": ["Wie", "wan\u00b7deln\u00b7de", "Ru\u00b7i\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}}}}