{"dta.poem.5413": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.57", "id:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man kann durch ", "tokens": ["Man", "kann", "durch"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "vergangnen Sachen,", "tokens": ["ver\u00b7gang\u00b7nen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch Dencken, gleichsam ", "tokens": ["Durch", "Den\u00b7cken", ",", "gleich\u00b7sam"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "genw\u00e4rtig machen.", "tokens": ["gen\u00b7w\u00e4r\u00b7tig", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Indem es nun unstreitig wahr, da\u00df unser w\u00fcrckliches", "tokens": ["In\u00b7dem", "es", "nun", "un\u00b7strei\u00b7tig", "wahr", ",", "da\u00df", "un\u00b7ser", "w\u00fcrck\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKVZ", "$,", "KOUS", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Vergn\u00fcgen,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ja gar, im Danck, das wahre Lob des Sch\u00f6pfers, blos", "tokens": ["Ja", "gar", ",", "im", "Danck", ",", "das", "wah\u00b7re", "Lob", "des", "Sch\u00f6p\u00b7fers", ",", "blos"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "ADV", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "im Dencken liegen;", "tokens": ["im", "Den\u00b7cken", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ach, so vers\u00e4umt, geliebte Menschen, doch euer Gl\u00fcck", "tokens": ["Ach", ",", "so", "ver\u00b7s\u00e4umt", ",", "ge\u00b7lieb\u00b7te", "Men\u00b7schen", ",", "doch", "eu\u00b7er", "Gl\u00fcck"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVPP", "$,", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "und eure Pflicht,", "tokens": ["und", "eu\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Zu dem Gesch\u00e4fte, das so n\u00f6thig, als leicht und n\u00fctzlich ist,", "tokens": ["Zu", "dem", "Ge\u00b7sch\u00e4f\u00b7te", ",", "das", "so", "n\u00f6\u00b7thig", ",", "als", "leicht", "und", "n\u00fctz\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "KOUS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "doch nicht!", "tokens": ["doch", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Wo etwas noch in unsrer Macht, so sind es die Gedancken", "tokens": ["Wo", "et\u00b7was", "noch", "in", "uns\u00b7rer", "Macht", ",", "so", "sind", "es", "die", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "ja,", "tokens": ["ja", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Als die wir gleichsam selbst erzielen, daran wir selber \u00e4n-", "tokens": ["Als", "die", "wir", "gleich\u00b7sam", "selbst", "er\u00b7zie\u00b7len", ",", "da\u00b7ran", "wir", "sel\u00b7ber", "\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "ADJD", "ADV", "VVINF", "$,", "PAV", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "dern k\u00f6nnen;", "tokens": ["dern", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VMFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Indem nun solche F\u00e4higkeit uns unser Sch\u00f6pfer wollen", "tokens": ["In\u00b7dem", "nun", "sol\u00b7che", "F\u00e4\u00b7hig\u00b7keit", "uns", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PPER", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "g\u00f6nnen", "tokens": ["g\u00f6n\u00b7nen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Und er uns so gemacht, als wenn, zu unsrer Lust und", "tokens": ["Und", "er", "uns", "so", "ge\u00b7macht", ",", "als", "wenn", ",", "zu", "uns\u00b7rer", "Lust", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "ADV", "VVPP", "$,", "KOKOM", "KOUS", "$,", "APPR", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "seinen Ehren,", "tokens": ["sei\u00b7nen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Von unserm w\u00fcrcklichem Vergn\u00fcgen wir gleichsam selber", "tokens": ["Von", "un\u00b7serm", "w\u00fcrck\u00b7li\u00b7chem", "Ver\u00b7gn\u00fc\u00b7gen", "wir", "gleich\u00b7sam", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Meister w\u00e4ren:", "tokens": ["Meis\u00b7ter", "w\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Ach, so bestrebt euch immer mehr, durch ein bedachtsam", "tokens": ["Ach", ",", "so", "be\u00b7strebt", "euch", "im\u00b7mer", "mehr", ",", "durch", "ein", "be\u00b7dacht\u00b7sam"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "APPR", "ART", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "sehn und f\u00fchlen,", "tokens": ["sehn", "und", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Zu eurer Lust und GOtt zum Ruhm, den holden End-", "tokens": ["Zu", "eu\u00b7rer", "Lust", "und", "Gott", "zum", "Ruhm", ",", "den", "hol\u00b7den", "En\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "zweck zu erzielen:", "tokens": ["zweck", "zu", "er\u00b7zie\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Durch eure fl\u00fcchtigen Gedancken der Zeiten Fl\u00fcchtigkeit zu", "tokens": ["Durch", "eu\u00b7re", "fl\u00fcch\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7cken", "der", "Zei\u00b7ten", "Fl\u00fcch\u00b7tig\u00b7keit", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "PTKZU"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "binden,", "tokens": ["bin\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Sein Lob und eure Lust zu mehren durch ein vern\u00fcnftiges", "tokens": ["Sein", "Lob", "und", "eu\u00b7re", "Lust", "zu", "meh\u00b7ren", "durch", "ein", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "Empfinden!", "tokens": ["Emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}