{"dta.poem.14732": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schnee .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wann die lauen L\u00fcfte wehen,", "tokens": ["Wann", "die", "lau\u00b7en", "L\u00fcf\u00b7te", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheint die Sonne lieblich warm:", "tokens": ["Scheint", "die", "Son\u00b7ne", "lieb\u00b7lich", "warm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirst Du wohl spazieren gehen,", "tokens": ["Wirst", "Du", "wohl", "spa\u00b7zie\u00b7ren", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er f\u00fchret Dich am Arm,", "tokens": ["Und", "er", "f\u00fch\u00b7ret", "Dich", "am", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Thr\u00e4nen Dir im Auge stehen,", "tokens": ["Thr\u00e4\u00b7nen", "Dir", "im", "Au\u00b7ge", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn so sch\u00f6n klingt, was er spricht,", "tokens": ["Denn", "so", "sch\u00f6n", "klingt", ",", "was", "er", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wann die lauen L\u00fcfte wehen,", "tokens": ["Wann", "die", "lau\u00b7en", "L\u00fcf\u00b7te", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaub' dem falschen Herzen nicht!", "tokens": ["Glaub'", "dem", "fal\u00b7schen", "Her\u00b7zen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}