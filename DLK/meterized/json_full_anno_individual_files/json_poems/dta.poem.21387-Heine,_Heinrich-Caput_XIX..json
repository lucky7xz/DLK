{"dta.poem.21387": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Cicerone sprach: \u201eHier wohnt", "tokens": ["Mein", "Ci\u00b7ce\u00b7ro\u00b7ne", "sprach", ":", "\u201e", "Hier", "wohnt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ernst Augustus, ein alter,", "tokens": ["Der", "Ernst", "Au\u00b7gus\u00b7tus", ",", "ein", "al\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hochtoryscher Lord, ein Edelmann,", "tokens": ["Hoch\u00b7to\u00b7ry\u00b7scher", "Lord", ",", "ein", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sehr r\u00fcstig f\u00fcr sein Alter.", "tokens": ["Sehr", "r\u00fcs\u00b7tig", "f\u00fcr", "sein", "Al\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eidyllisch sicher haust er hier,", "tokens": ["\u201e", "i\u00b7dyl\u00b7lisch", "si\u00b7cher", "haust", "er", "hier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn besser als alle Trabanten", "tokens": ["Denn", "bes\u00b7ser", "als", "al\u00b7le", "Tra\u00b7ban\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Besch\u00fctzet ihn der mangelnde Muth", "tokens": ["Be\u00b7sch\u00fct\u00b7zet", "ihn", "der", "man\u00b7geln\u00b7de", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Von unseren lieben Bekannten.", "tokens": ["Von", "un\u00b7se\u00b7ren", "lie\u00b7ben", "Be\u00b7kann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u201eich seh\u2019 ihn zuweilen, er klagt alsdann", "tokens": ["\u201e", "ich", "seh'", "ihn", "zu\u00b7wei\u00b7len", ",", "er", "klagt", "als\u00b7dann"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie gar langweilig das Amt sey,", "tokens": ["Wie", "gar", "lang\u00b7wei\u00b7lig", "das", "Amt", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das K\u00f6nigsamt, wozu er jetzt", "tokens": ["Das", "K\u00f6\u00b7nig\u00b7samt", ",", "wo\u00b7zu", "er", "jetzt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier in Hannover verdammt sey.", "tokens": ["Hier", "in", "Han\u00b7no\u00b7ver", "ver\u00b7dammt", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201ean gro\u00dfbritanisches Leben gew\u00f6hnt,", "tokens": ["\u201e", "an", "gro\u00df\u00b7bri\u00b7ta\u00b7ni\u00b7sches", "Le\u00b7ben", "ge\u00b7w\u00f6hnt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sey es ihm hier zu enge,", "tokens": ["Sey", "es", "ihm", "hier", "zu", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihn plage der Spleen, er f\u00fcrchte schier,", "tokens": ["Ihn", "pla\u00b7ge", "der", "Spleen", ",", "er", "f\u00fcrch\u00b7te", "schier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er halt\u2019 es nicht aus auf die L\u00e4nge.", "tokens": ["Er", "halt'", "es", "nicht", "aus", "auf", "die", "L\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}