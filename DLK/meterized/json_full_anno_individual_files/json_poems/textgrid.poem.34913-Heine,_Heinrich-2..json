{"textgrid.poem.34913": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbh\u00e4tt er menschlich ordin\u00e4r", "tokens": ["\u00bb", "h\u00e4tt", "er", "menschlich", "or\u00b7di\u00b7n\u00e4r"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "ADJD", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nicht gehalten, was versprochen,", "tokens": ["Nicht", "ge\u00b7hal\u00b7ten", ",", "was", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "PWS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tt er nur sein Wort gebrochen,", "tokens": ["H\u00e4tt", "er", "nur", "sein", "Wort", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00fcrnen wollt ich nimmermehr.", "tokens": ["Z\u00fcr\u00b7nen", "wollt", "ich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber unverzeihlich ist,", "tokens": ["A\u00b7ber", "un\u00b7ver\u00b7zeih\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er mich get\u00e4uscht so schn\u00f6de", "tokens": ["Da\u00df", "er", "mich", "ge\u00b7t\u00e4uscht", "so", "schn\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word"