{"dta.poem.15970": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Welt Grund\u00fcbel\u201c nennt den leeren Raum ein Weiser;", "tokens": ["\u201e", "der", "Welt", "Grund\u00b7\u00fc\u00b7bel", "\u201c", "nennt", "den", "lee\u00b7ren", "Raum", "ein", "Wei\u00b7ser", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und widersprechen wird kein Bettler und kein Kaiser.", "tokens": ["Und", "wi\u00b7der\u00b7spre\u00b7chen", "wird", "kein", "Bett\u00b7ler", "und", "kein", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Beutels leerer Raum, der leere Raum des Magens,", "tokens": ["Des", "Beu\u00b7tels", "lee\u00b7rer", "Raum", ",", "der", "lee\u00b7re", "Raum", "des", "Ma\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist jedes Uebels Grund und jedes Unbehagens.", "tokens": ["Ist", "je\u00b7des", "Ue\u00b7bels", "Grund", "und", "je\u00b7des", "Un\u00b7be\u00b7ha\u00b7gens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ob leer kein Weltraum sei vom Glanze der Gestirne,", "tokens": ["Ob", "leer", "kein", "Welt\u00b7raum", "sei", "vom", "Glan\u00b7ze", "der", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIAT", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist mehr als einer doch in mehr als einem Hirne.", "tokens": ["Ist", "mehr", "als", "ei\u00b7ner", "doch", "in", "mehr", "als", "ei\u00b7nem", "Hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "KOKOM", "ART", "ADV", "APPR", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Manch leeren Raum hat auch manch \u00fcbervolles Buch,", "tokens": ["Manch", "lee\u00b7ren", "Raum", "hat", "auch", "manch", "\u00fc\u00b7ber\u00b7vol\u00b7les", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie diesen hier, den f\u00fcllt vom leeren Raum der Spruch.", "tokens": ["Wie", "die\u00b7sen", "hier", ",", "den", "f\u00fcllt", "vom", "lee\u00b7ren", "Raum", "der", "Spruch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "$,", "ART", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}