{"textgrid.poem.64163": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die letzten Ritter von Marienburg", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sahen, sie waren verloren, verlassen in Jammer und Not:", "tokens": ["Sie", "sa\u00b7hen", ",", "sie", "wa\u00b7ren", "ver\u00b7lo\u00b7ren", ",", "ver\u00b7las\u00b7sen", "in", "Jam\u00b7mer", "und", "Not", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Da brachen sie aus den Toren und suchten freudigen Tod.", "tokens": ["Da", "bra\u00b7chen", "sie", "aus", "den", "To\u00b7ren", "und", "such\u00b7ten", "freu\u00b7di\u00b7gen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Greis, ein Mann und ein Knabe, das waren die letzten drei:", "tokens": ["Ein", "Greis", ",", "ein", "Mann", "und", "ein", "Kna\u00b7be", ",", "das", "wa\u00b7ren", "die", "letz\u00b7ten", "drei", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "CARD", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Viel Heiden sanken zu Grabe mit gellendem Todesschrei.", "tokens": ["Viel", "Hei\u00b7den", "san\u00b7ken", "zu", "Gra\u00b7be", "mit", "gel\u00b7len\u00b7dem", "To\u00b7des\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "\u00bbhie Christus!\u00ab in blonden Locken mit dem Banner der Knabe rief,", "tokens": ["\u00bb", "hie", "Chris\u00b7tus", "!", "\u00ab", "in", "blon\u00b7den", "Lo\u00b7cken", "mit", "dem", "Ban\u00b7ner", "der", "Kna\u00b7be", "rief", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "$.", "$(", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bis er sp\u00fcrte den Herzschlag stocken \u2013 der Litauerpfeil traf tief.", "tokens": ["Bis", "er", "sp\u00fcr\u00b7te", "den", "Herz\u00b7schlag", "sto\u00b7cken", "\u2013", "der", "Li\u00b7tau\u00b7er\u00b7pfeil", "traf", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VVINF", "$(", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "\u00bbhie Deutschland!\u00ab rief der Alte mit dem wallenden Silberhaar,", "tokens": ["\u00bb", "hie", "Deutschland", "!", "\u00ab", "rief", "der", "Al\u00b7te", "mit", "dem", "wal\u00b7len\u00b7den", "Sil\u00b7ber\u00b7haar", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "$.", "$(", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Bis ihm mit blut'ger Spalte der Helm zerschroten war.", "tokens": ["Bis", "ihm", "mit", "blut'\u00b7ger", "Spal\u00b7te", "der", "Helm", "zer\u00b7schro\u00b7ten", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch stumm, mit schrecklichem Schweigen, der dritte schreitet durchs Feld:", "tokens": ["Doch", "stumm", ",", "mit", "schreck\u00b7li\u00b7chem", "Schwei\u00b7gen", ",", "der", "drit\u00b7te", "schrei\u00b7tet", "durchs", "Feld", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Das war ein grimmer Reigen: wen er erreicht, der f\u00e4llt.", "tokens": ["Das", "war", "ein", "grim\u00b7mer", "Rei\u00b7gen", ":", "wen", "er", "er\u00b7reicht", ",", "der", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "PPER", "VVPP", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Es splittern Pfeil' und Speere an seiner schwarzen Brust:", "tokens": ["Es", "split\u00b7tern", "Pfeil'", "und", "Spee\u00b7re", "an", "sei\u00b7ner", "schwar\u00b7zen", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Er tr\u00e4gt nicht Wappenehre, er zeigt nicht Farbenlust:", "tokens": ["Er", "tr\u00e4gt", "nicht", "Wap\u00b7pe\u00b7neh\u00b7re", ",", "er", "zeigt", "nicht", "Far\u00b7ben\u00b7lust", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Ein schwarzes Schwert er wieget, ihn deckt nicht Helm, nicht Schild,", "tokens": ["Ein", "schwar\u00b7zes", "Schwert", "er", "wie\u00b7get", ",", "ihn", "deckt", "nicht", "Helm", ",", "nicht", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Um bleiche Wangen flieget sein schwarz Gelock so wild,", "tokens": ["Um", "blei\u00b7che", "Wan\u00b7gen", "flie\u00b7get", "sein", "schwarz", "Ge\u00b7lock", "so", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Sein dunkles Auge leuchtet, sein Mund bleibt schrecklich stumm,", "tokens": ["Sein", "dunk\u00b7les", "Au\u00b7ge", "leuch\u00b7tet", ",", "sein", "Mund", "bleibt", "schreck\u00b7lich", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Die schwarze Br\u00fcnne feuchtet von Blute sich ringsum. \u2013", "tokens": ["Die", "schwar\u00b7ze", "Br\u00fcn\u00b7ne", "feuch\u00b7tet", "von", "Blu\u00b7te", "sich", "ring\u00b7sum", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PRF", "ADV", "$.", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Ein Heer hat er erschlagen, das schwarze Schwert ward rot,", "tokens": ["Ein", "Heer", "hat", "er", "er\u00b7schla\u00b7gen", ",", "das", "schwar\u00b7ze", "Schwert", "ward", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Die Heiden fliehen und jagen und kreischen: \u00bbDas ist der Tod.\u00ab", "tokens": ["Die", "Hei\u00b7den", "flie\u00b7hen", "und", "ja\u00b7gen", "und", "krei\u00b7schen", ":", "\u00bb", "Das", "ist", "der", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "KON", "VVINF", "$.", "$(", "PDS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Und als er geblieben alleine, aufseuzt' er tief und laut:", "tokens": ["Und", "als", "er", "ge\u00b7blie\u00b7ben", "al\u00b7lei\u00b7ne", ",", "auf\u00b7seuzt'", "er", "tief", "und", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "ADV", "$,", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.20": {"text": "Dann glitt er am moosigen Steine ins duftende Heidekraut,", "tokens": ["Dann", "glitt", "er", "am", "moo\u00b7si\u00b7gen", "Stei\u00b7ne", "ins", "duf\u00b7ten\u00b7de", "Hei\u00b7de\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.21": {"text": "Und als verschollen die Hufen, da hat er in Todespein", "tokens": ["Und", "als", "ver\u00b7schol\u00b7len", "die", "Hu\u00b7fen", ",", "da", "hat", "er", "in", "To\u00b7de\u00b7spein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Noch einen Namen gerufen: \u2013 den h\u00f6rte nur Gott allein.", "tokens": ["Noch", "ei\u00b7nen", "Na\u00b7men", "ge\u00b7ru\u00b7fen", ":", "\u2013", "den", "h\u00f6r\u00b7te", "nur", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "$(", "ART", "VVFIN", "ADV", "NN", "ADV", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie sahen, sie waren verloren, verlassen in Jammer und Not:", "tokens": ["Sie", "sa\u00b7hen", ",", "sie", "wa\u00b7ren", "ver\u00b7lo\u00b7ren", ",", "ver\u00b7las\u00b7sen", "in", "Jam\u00b7mer", "und", "Not", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Da brachen sie aus den Toren und suchten freudigen Tod.", "tokens": ["Da", "bra\u00b7chen", "sie", "aus", "den", "To\u00b7ren", "und", "such\u00b7ten", "freu\u00b7