{"textgrid.poem.44401": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[es liegt ein d\u00fcstrer Wald von alten Buchen]", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt ein d\u00fcstrer Wald von alten Buchen", "tokens": ["Es", "liegt", "ein", "d\u00fcst\u00b7rer", "Wald", "von", "al\u00b7ten", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einem klippgen Felstal der Ardennen,", "tokens": ["In", "ei\u00b7nem", "klipp\u00b7gen", "Fel\u00b7stal", "der", "Ar\u00b7den\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das nur der Wildnis scheue Tiere kennen,", "tokens": ["Das", "nur", "der", "Wild\u00b7nis", "scheu\u00b7e", "Tie\u00b7re", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Fl\u00fcchtling kaum, die Ausgesto\u00dfnen suchen;", "tokens": ["Der", "Fl\u00fccht\u00b7ling", "kaum", ",", "die", "Aus\u00b7ge\u00b7sto\u00df\u00b7nen", "su\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum dichten Laubdach w\u00f6lben sich die Buchen,", "tokens": ["Zum", "dich\u00b7ten", "Laub\u00b7dach", "w\u00f6l\u00b7ben", "sich", "die", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und kaum vermag die Sonne durchzubrennen,", "tokens": ["Und", "kaum", "ver\u00b7mag", "die", "Son\u00b7ne", "durch\u00b7zu\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die L\u00fcfte kaum, die allverbreitet blauen,", "tokens": ["Die", "L\u00fcf\u00b7te", "kaum", ",", "die", "all\u00b7ver\u00b7brei\u00b7tet", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch ihre dichten Gitter durchzuschauen.", "tokens": ["Durch", "ih\u00b7re", "dich\u00b7ten", "Git\u00b7ter", "durch\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Wurzeln Teppich, Moos, so unber\u00fchrt", "tokens": ["Der", "Wur\u00b7zeln", "Tep\u00b7pich", ",", "Moos", ",", "so", "un\u00b7be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und also stolz auf seiner Freiheit Zeichen,", "tokens": ["Und", "al\u00b7so", "stolz", "auf", "sei\u00b7ner", "Frei\u00b7heit", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-"