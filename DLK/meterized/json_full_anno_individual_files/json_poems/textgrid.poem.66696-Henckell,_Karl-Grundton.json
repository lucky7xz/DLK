{"textgrid.poem.66696": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Grundton", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meiner Seele zittert eine Saite,", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "zit\u00b7tert", "ei\u00b7ne", "Sai\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tief mit der Wurzel meines Seins verkn\u00fcpft;", "tokens": ["Tief", "mit", "der", "Wur\u00b7zel", "mei\u00b7nes", "Seins", "ver\u00b7kn\u00fcpft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wenn mit dem Bogen ich dar\u00fcber gleite,", "tokens": ["Wenn", "mit", "dem", "Bo\u00b7gen", "ich", "da\u00b7r\u00fc\u00b7ber", "glei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geschieht's, da\u00df Psyche selbst dem Laut entschl\u00fcpft.", "tokens": ["Ge\u00b7schieht's", ",", "da\u00df", "Psy\u00b7che", "selbst", "dem", "Laut", "ent\u00b7schl\u00fcpft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NN", "ADV", "ART", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soll ich der Sprache grobem Ohr verraten,", "tokens": ["Soll", "ich", "der", "Spra\u00b7che", "gro\u00b7bem", "Ohr", "ver\u00b7ra\u00b7ten", ","], "token_info": ["wor