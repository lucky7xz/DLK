{"textgrid.poem.47755": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[weh mir armen Kuttentr\u00e4ger!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh mir armen Kuttentr\u00e4ger!", "tokens": ["Weh", "mir", "ar\u00b7men", "Kut\u00b7ten\u00b7tr\u00e4\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich habe b\u00f6se Schw\u00e4ger,", "tokens": ["Denn", "ich", "ha\u00b7be", "b\u00f6\u00b7se", "Schw\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich habe schlimme Vettern,", "tokens": ["Denn", "ich", "ha\u00b7be", "schlim\u00b7me", "Vet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6nche, Pfaffen, Kirchenpfleger.", "tokens": ["M\u00f6n\u00b7che", ",", "Pfaf\u00b7fen", ",", "Kir\u00b7ch\u00b7en\u00b7pfle\u00b7ger", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ach, wie taug' ich doch zu diesen,", "tokens": ["Ach", ",", "wie", "taug'", "ich", "doch", "zu", "die\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich verliebter Versej\u00e4ger? \u2013", "tokens": ["Ich", "ver\u00b7lieb\u00b7ter", "Ver\u00b7se\u00b7j\u00e4\u00b7ger", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wein, o Schenke! Denn zur Bu\u00dfe", "tokens": ["Wein", ",", "o", "Schen\u00b7ke", "!", "Denn", "zur", "Bu\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$.", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Werd' ich alle Tage tr\u00e4ger,", "tokens": ["Werd'", "ich", "al\u00b7le", "Ta\u00b7ge", "tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die Sonne hoher Tugend", "tokens": ["Und", "die", "Son\u00b7ne", "ho\u00b7her", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Steht mir alle Tage schr\u00e4ger;", "tokens": ["Steht", "mir", "al\u00b7le", "Ta\u00b7ge", "schr\u00e4\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nimmer auch mich schw\u00e4rzer f\u00e4rben", "tokens": ["Nim\u00b7mer", "auch", "mich", "schw\u00e4r\u00b7zer", "f\u00e4r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wird die Zunge der Verkl\u00e4ger,", "tokens": ["Wird", "die", "Zun\u00b7ge", "der", "Ver\u00b7kl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Denn ich bin ja schon so dunkel,", "tokens": ["Denn", "ich", "bin", "ja", "schon", "so", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie ein afrikan'scher Neger.", "tokens": ["Wie", "ein", "a\u00b7fri\u00b7kan'\u00b7scher", "Ne\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Weh mir armen Kuttentr\u00e4ger!", "tokens": ["Weh", "mir", "ar\u00b7men", "Kut\u00b7ten\u00b7tr\u00e4\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich habe b\u00f6se Schw\u00e4ger,", "tokens": ["Denn", "ich", "ha\u00b7be", "b\u00f6\u00b7se", "Schw\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich habe schlimme Vettern,", "tokens": ["Denn", "ich", "ha\u00b7be", "schlim\u00b7me", "Vet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6nche, Pfaffen, Kirchenpfleger.", "tokens": ["M\u00f6n\u00b7che", ",", "Pfaf\u00b7fen", ",", "Kir\u00b7ch\u00b7en\u00b7pfle\u00b7ger", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ach, wie taug