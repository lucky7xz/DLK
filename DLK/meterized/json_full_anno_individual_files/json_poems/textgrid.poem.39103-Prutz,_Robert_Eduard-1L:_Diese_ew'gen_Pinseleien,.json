{"textgrid.poem.39103": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Diese ew'gen Pinseleien,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese ew'gen Pinseleien,", "tokens": ["Die\u00b7se", "ew'\u00b7gen", "Pin\u00b7se\u00b7lei\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dieses Reden, dies Beschw\u00f6ren,", "tokens": ["die\u00b7ses", "Re\u00b7den", ",", "dies", "Be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mag der Himmel mir's verzeihen!", "tokens": ["mag", "der", "Him\u00b7mel", "mir's", "ver\u00b7zei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ich mag es nicht mehr h\u00f6ren.", "tokens": ["Doch", "ich", "mag", "es", "nicht", "mehr", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Volksvertreter, Volksvertreter,", "tokens": ["Volks\u00b7ver\u00b7tre\u00b7ter", ",", "Volks\u00b7ver\u00b7tre\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seid doch nicht wie junge K\u00e4lber,", "tokens": ["seid", "doch", "nicht", "wie", "jun\u00b7ge", "K\u00e4l\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "werdet doch nicht zum Verr\u00e4ter", "tokens": ["wer\u00b7det", "doch", "nicht", "zum", "Ver\u00b7r\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "an dem Volk und an euch selber!", "tokens": ["an", "dem", "Volk", "und", "an", "euch", "sel\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Statt zu reden, statt zu schwatzen,", "tokens": ["Statt", "zu", "re\u00b7den", ",", "statt", "zu", "schwat\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "jeder frisch auf seinen Posten,", "tokens": ["je\u00b7der", "frisch", "auf", "sei\u00b7nen", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "zugepackt mit kr\u00e4ft'gen Tatzen \u2013", "tokens": ["zu\u00b7ge\u00b7packt", "mit", "kr\u00e4ft'\u00b7gen", "Tat\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sollt' es auch das Fell euch kosten!", "tokens": ["sollt'", "es", "auch", "das", "Fell", "euch", "kos\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": [