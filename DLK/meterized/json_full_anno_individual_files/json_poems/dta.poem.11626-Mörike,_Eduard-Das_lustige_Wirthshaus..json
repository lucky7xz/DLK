{"dta.poem.11626": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das lustige Wirthshaus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater, hei\u00dft's, hab' ein klein M\u00e4nnlein im Sold,", "tokens": ["Mein", "Va\u00b7ter", ",", "hei\u00dft's", ",", "hab'", "ein", "klein", "M\u00e4nn\u00b7lein", "im", "Sold", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "VAFIN", "ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein M\u00e4nnlein, so fein und so klug,", "tokens": ["Ein", "M\u00e4nn\u00b7lein", ",", "so", "fein", "und", "so", "klug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wenn er nur m\u00f6chte und wenn er nur wollt',", "tokens": ["Und", "wenn", "er", "nur", "m\u00f6ch\u00b7te", "und", "wenn", "er", "nur", "wollt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "KON", "KOUS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wir h\u00e4tten Dukaten genug.", "tokens": ["Wir", "h\u00e4t\u00b7ten", "Du\u00b7ka\u00b7ten", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}