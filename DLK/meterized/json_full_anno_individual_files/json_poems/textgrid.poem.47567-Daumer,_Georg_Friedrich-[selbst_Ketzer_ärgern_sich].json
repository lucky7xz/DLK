{"textgrid.poem.47567": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[selbst Ketzer \u00e4rgern sich]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selbst Ketzer \u00e4rgern sich", "tokens": ["Selbst", "Ket\u00b7zer", "\u00e4r\u00b7gern", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An einer Ketzerei, die keine Pflicht", "tokens": ["An", "ei\u00b7ner", "Ket\u00b7ze\u00b7rei", ",", "die", "kei\u00b7ne", "Pflicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und keine Satzung ehrt,", "tokens": ["Und", "kei\u00b7ne", "Sat\u00b7zung", "ehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mein geliebter Christenknabe spricht:", "tokens": ["Und", "mein", "ge\u00b7lieb\u00b7ter", "Chris\u00b7ten\u00b7kna\u00b7be", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbbei'm Himmel, es ist Schad'", "tokens": ["\u00bb", "bei'm", "Him\u00b7mel", ",", "es", "ist", "Schad'"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "$,", "PPER", "VAFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Um einen solchen Mann, wie du, Hafis!", "tokens": ["Um", "ei\u00b7nen", "sol\u00b7chen", "Mann", ",", "wie", "du", ",", "Ha\u00b7fis", "!"], "token_info": [