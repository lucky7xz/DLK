{"textgrid.poem.26727": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Spiele, Kind, in der Mutter Scho\u00df! Auf der heiligen Insel", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spiele, Kind, in der Mutter Scho\u00df! Auf der heiligen Insel", "tokens": ["Spie\u00b7le", ",", "Kind", ",", "in", "der", "Mut\u00b7ter", "Scho\u00df", "!", "Auf", "der", "hei\u00b7li\u00b7gen", "In\u00b7sel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ART", "NN", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Findet der tr\u00fcbe Gram, findet die Sorge dich nicht,", "tokens": ["Fin\u00b7det", "der", "tr\u00fc\u00b7be", "Gram", ",", "fin\u00b7det", "die", "Sor\u00b7ge", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "$,"], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Liebend halten die Arme der Mutter dich \u00fcber dem Abgrund,", "tokens": ["Lie\u00b7bend", "hal\u00b7ten", "die", "Ar\u00b7me", "der", "Mut\u00b7ter", "dich", "\u00fc\u00b7ber", "dem", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und in das flutende Grab l\u00e4chelst du schuldlos hinab.", "tokens": ["Und", "in", "das", "flu\u00b7ten\u00b7de", "Grab", "l\u00e4\u00b7chelst", "du", "schuld\u00b7los", "hin\u0