{"dta.poem.17924": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "167.  Wer wahrhafftig Reich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Viel haben macht nicht Reich Der ist ein reicher Mann/", "tokens": ["Viel", "ha\u00b7ben", "macht", "nicht", "Reich", "Der", "ist", "ein", "rei\u00b7cher", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "PTKNEG", "NN", "PDS", "VAFIN", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alles was er hat ohn Leid verliehren kan.", "tokens": ["Der", "al\u00b7les", "was", "er", "hat", "ohn", "Leid", "ver\u00b7lieh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PWS", "PPER", "VAFIN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}