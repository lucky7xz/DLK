{"textgrid.poem.56890": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und da ich nun so frei wie nur ein Mensch,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und da ich nun so frei wie nur ein Mensch,", "tokens": ["Und", "da", "ich", "nun", "so", "frei", "wie", "nur", "ein", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von Sch\u00f6nheit \u00fcbervoll und hell an Geist,", "tokens": ["von", "Sch\u00f6n\u00b7heit", "\u00fc\u00b7ber\u00b7voll", "und", "hell", "an", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so wei\u00df ich nicht, was ich ", "tokens": ["so", "wei\u00df", "ich", "nicht", ",", "was", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "in meiner Kunst gef\u00fcgig Alphabet.", "tokens": ["in", "mei\u00b7ner", "Kunst", "ge\u00b7f\u00fc\u00b7gig", "Al\u00b7pha\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Frei, ", "tokens": ["Frei", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Pa\u00df aller Uners\u00e4ttlichen und Gl\u00fcck!", "tokens": ["Pa\u00df", "al\u00b7