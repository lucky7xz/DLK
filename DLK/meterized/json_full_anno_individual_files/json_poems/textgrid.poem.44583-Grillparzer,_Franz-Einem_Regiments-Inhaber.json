{"textgrid.poem.44583": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Einem Regiments-Inhaber", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein halb Jahrhundert ist vorbeiger\u00fcckt,", "tokens": ["Ein", "halb", "Jahr\u00b7hun\u00b7dert", "ist", "vor\u00b7bei\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seit du der F\u00fchrer wardst von unsern Fahnen,", "tokens": ["Seit", "du", "der", "F\u00fch\u00b7rer", "wardst", "von", "un\u00b7sern", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erlaube, da\u00df wir des, die du begl\u00fcckt,", "tokens": ["Er\u00b7lau\u00b7be", ",", "da\u00df", "wir", "des", ",", "die", "du", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein treues Regiment, dich heute mahnen.", "tokens": ["Dein", "treu\u00b7es", "Re\u00b7gi\u00b7ment", ",", "dich", "heu\u00b7te", "mah\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gerecht und mild, so fanden wir dich stets,", "tokens": ["Ge\u00b7recht", "und", "mild", ",", "so", "fan\u00b7den", "wir", "dich", "stets", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Freigebger Hand von allem, was das Deine;", "tokens": ["Frei\u00b7geb\u00b7ger", "Hand", "von", "al\u00b7lem", ",", "was", "das", "Dei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIS", "$,", "PRELS", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So war f\u00fcr uns, f\u00fcr andre dein Gesetz,", "tokens": ["So", "war", "f\u00fcr", "uns", ",", "f\u00fcr", "and\u00b7re", "dein", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "$,", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allein die Welt, sie urteilt nach dem Scheine.", "tokens": ["Al\u00b7lein", "die", "Welt", ",", "sie", "ur\u00b7teilt", "nach", "dem", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als fremden Guts Bewahrer aufgestellt,", "tokens": ["Als", "frem\u00b7den", "Guts", "Be\u00b7wah\u00b7rer", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schien es dir Pflicht, das Fremde nicht zu mindern,", "tokens": ["Schien", "es", "dir", "Pflicht", ",", "das", "Frem\u00b7de", "nicht", "zu", "min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "$,", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie tief du f\u00fchltest manche Not der Welt,", "tokens": ["Wie", "tief", "du", "f\u00fchl\u00b7test", "man\u00b7che", "Not", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du konntest nur versagen und verhindern.", "tokens": ["Du", "konn\u00b7test", "nur", "ver\u00b7sa\u00b7gen", "und", "ver\u00b7hin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "War es das Deine, o wie gabst du gern!", "tokens": ["War", "es", "das", "Dei\u00b7ne", ",", "o", "wie", "gabst", "du", "gern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "PPOSAT", "$,", "FM", "KOKOM", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nie konnte Selbstsucht je dein Wohltun schm\u00e4lern;", "tokens": ["Nie", "konn\u00b7te", "Selbst\u00b7sucht", "je", "dein", "Wohl\u00b7tun", "schm\u00e4\u00b7lern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr andre handeln war dein b\u00f6ser Stern,", "tokens": ["F\u00fcr", "and\u00b7re", "han\u00b7deln", "war", "dein", "b\u00f6\u00b7ser", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du trugst die Last von l\u00e4ngstgewesnen Fehlern.", "tokens": ["Du", "trugst", "die", "Last", "von", "l\u00e4ngst\u00b7ge\u00b7wes\u00b7nen", "Feh\u00b7lern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So wie der Tag zur tollen Fastnachtzeit", "tokens": ["So", "wie", "der", "Tag", "zur", "tol\u00b7len", "Fast\u00b7nacht\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem lustbegiergen T\u00e4nzer scheint zu zaudern,", "tokens": ["Dem", "lust\u00b7be\u00b7gier\u00b7gen", "T\u00e4n\u00b7zer", "scheint", "zu", "zau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er w\u00fcnscht den heitern Strahl schon fern und weit,", "tokens": ["Er", "w\u00fcnscht", "den", "hei\u00b7tern", "Strahl", "schon", "fern", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Uhr geme\u00dfner Gang erweckt ihm Schaudern.", "tokens": ["Der", "Uhr", "ge\u00b7me\u00df\u00b7ner", "Gang", "er\u00b7weckt", "ihm", "Schau\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Allein, wenn nun die laute Nacht durchtobt,", "tokens": ["Al\u00b7lein", ",", "wenn", "nun", "die", "lau\u00b7te", "Nacht", "durch\u00b7tobt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erscheint der Tag nichts weniger vom neuen,", "tokens": ["Er\u00b7scheint", "der", "Tag", "nichts", "we\u00b7ni\u00b7ger", "vom", "neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIS", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und selbst der m\u00fcd sein Gegenteil erprobt,", "tokens": ["Und", "selbst", "der", "m\u00fcd", "sein", "Ge\u00b7gen\u00b7teil", "er\u00b7probt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird seiner R\u00fcckkehr sich wie andre freuen.", "tokens": ["Wird", "sei\u00b7ner", "R\u00fcck\u00b7kehr", "sich", "wie", "and\u00b7re", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "KOKOM", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Welt ist m\u00fcd. Sie hat die Lust geb\u00fc\u00dft", "tokens": ["Die", "Welt", "ist", "m\u00fcd", ".", "Sie", "hat", "die", "Lust", "ge\u00b7b\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und b\u00fc\u00dft f\u00fcr ihre Lust. Die b\u00f6sen Worte,", "tokens": ["Und", "b\u00fc\u00dft", "f\u00fcr", "ih\u00b7re", "Lust", ".", "Die", "b\u00f6\u00b7sen", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Ewges als Veraltetes begr\u00fc\u00dft,", "tokens": ["Die", "Ew\u00b7ges", "als", "Ver\u00b7al\u00b7te\u00b7tes", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verstummen an des Unheils offner Pforte.", "tokens": ["Ver\u00b7stum\u00b7men", "an", "des", "Un\u00b7heils", "off\u00b7ner", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wohl dem, der nie sich von der Pflicht getrennt,", "tokens": ["Wohl", "dem", ",", "der", "nie", "sich", "von", "der", "Pflicht", "ge\u00b7trennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Wandellose sichert vor der Reue,", "tokens": ["Das", "Wan\u00b7del\u00b7lo\u00b7se", "si\u00b7chert", "vor", "der", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drum sieh auf uns, dein altes Regiment,", "t