{"dta.poem.11860": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Seelchen kam so fr\u00fch vom Himmel schon hinaus,", "tokens": ["Das", "Seel\u00b7chen", "kam", "so", "fr\u00fch", "vom", "Him\u00b7mel", "schon", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es vergessen hat sein elterliches Haus,", "tokens": ["Da\u00df", "es", "ver\u00b7ges\u00b7sen", "hat", "sein", "el\u00b7ter\u00b7li\u00b7ches", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sein elterliches Haus vergessen, davon kaum", "tokens": ["Sein", "el\u00b7ter\u00b7li\u00b7ches", "Haus", "ver\u00b7ges\u00b7sen", ",", "da\u00b7von", "kaum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "PAV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm die Erinnerung noch manchmal kommt im Traum.", "tokens": ["Ihm", "die", "E\u00b7rin\u00b7ne\u00b7rung", "noch", "manch\u00b7mal", "kommt", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Kind kam in der Fremd' an eine fremde Amme,", "tokens": ["Das", "Kind", "kam", "in", "der", "Fremd'", "an", "ei\u00b7ne", "frem\u00b7de", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Pflegevater auch ward ihm von fremdem Stamme.", "tokens": ["Ein", "Pfle\u00b7ge\u00b7va\u00b7ter", "auch", "ward", "ihm", "von", "frem\u00b7dem", "Stam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie nannt' es Mutter, weil es ihre Br\u00fcste sog,", "tokens": ["Sie", "nannt'", "es", "Mut\u00b7ter", ",", "weil", "es", "ih\u00b7re", "Br\u00fcs\u00b7te", "sog", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn nannt' es Vater, weil er mit der Ruth' es zog.", "tokens": ["Ihn", "nannt'", "es", "Va\u00b7ter", ",", "weil", "er", "mit", "der", "Ruth'", "es", "zog", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch ein Gef\u00fchl erwacht ihm in der Brust und spricht:", "tokens": ["Doch", "ein", "Ge\u00b7f\u00fchl", "er\u00b7wacht", "ihm", "in", "der", "Brust", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der rechte Vater ists, die rechte Mutter, nicht.", "tokens": ["Der", "rech\u00b7te", "Va\u00b7ter", "ists", ",", "die", "rech\u00b7te", "Mut\u00b7ter", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein bessrer Vater mu\u00df es seyn, den ich verloren,", "tokens": ["Ein", "bess\u00b7rer", "Va\u00b7ter", "mu\u00df", "es", "seyn", ",", "den", "ich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VAINF", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine sch\u00f6nere Mutter, die mich geboren.", "tokens": ["Und", "ei\u00b7ne", "sch\u00f6\u00b7ne\u00b7re", "Mut\u00b7ter", ",", "die", "mich", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und seine Sehnsucht w\u00e4chst, und Ruhe hat es nicht,", "tokens": ["Und", "sei\u00b7ne", "Sehn\u00b7sucht", "w\u00e4chst", ",", "und", "Ru\u00b7he", "hat", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "KON", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis es des Vaters sieht, der Mutter Angesicht.", "tokens": ["Bis", "es", "des", "Va\u00b7ters", "sieht", ",", "der", "Mut\u00b7ter", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}