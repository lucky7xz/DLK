", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht edlen Waffengang gewohnt,", "tokens": ["Nicht", "ed\u00b7len", "Waf\u00b7fen\u00b7gang", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat er die T\u00fccke sich erkoren,", "tokens": ["Hat", "er", "die", "T\u00fc\u00b7cke", "sich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wen im Streite er verschont,", "tokens": ["Und", "wen", "im", "Strei\u00b7te", "er", "ver\u00b7schont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ist gewi\u00df erst recht verloren.", "tokens": ["Der", "ist", "ge\u00b7wi\u00df", "erst", "recht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf, wappne dich; er kommt nicht erst;", "tokens": ["Auf", ",", "wapp\u00b7ne", "dich", ";", "er", "kommt", "nicht", "erst", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PWAV", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist schon da, ists stets gewesen.", "tokens": ["Er", "ist", "schon", "da", ",", "ists", "stets", "ge\u00b7we\u00b7sen", "."], "token_info": ["wo