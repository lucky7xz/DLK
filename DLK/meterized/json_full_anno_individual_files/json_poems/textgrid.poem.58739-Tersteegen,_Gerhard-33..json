-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Liebst du ihn, zeige deine Treu,", "tokens": ["Liebst", "du", "ihn", ",", "zei\u00b7ge", "dei\u00b7ne", "Treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er dein Gut allein und ganze St\u00fctze sei!", "tokens": ["Da\u00df", "er", "dein", "Gut", "al\u00b7lein", "und", "gan\u00b7ze", "St\u00fct\u00b7ze", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}