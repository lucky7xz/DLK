{"textgrid.poem.27829": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fried ist nun gemacht, die Einigkeit verpflichtet;", "tokens": ["Der", "Fried", "ist", "nun", "ge\u00b7macht", ",", "die", "Ei\u00b7nig\u00b7keit", "ver\u00b7pflich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Treu ist nun verkn\u00fcpfft, die Freundschafft angerichtet.", "tokens": ["Die", "Treu", "ist", "nun", "ver\u00b7kn\u00fcpfft", ",", "die", "Freund\u00b7schafft", "an\u00b7ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der diesen Bund gestifft, erhalte diesen Bund,", "tokens": ["Der", "die\u00b7sen", "Bund", "ge\u00b7stifft", ",", "er\u00b7hal\u00b7te", "die\u00b7sen", "Bund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er besteh zuletzt, wie er von Anfang stund!", "tokens": ["Da\u00df", "er", "be\u00b7steh", "zu\u00b7letzt", ",", "wie", "er", "von", "An\u00b7fang", "stund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Welt mag, wie sie wil, sich kochen oder braten,", "tokens": ["Die", "Welt", "mag", ",", "wie", "sie", "wil", ",", "sich", "ko\u00b7chen", "o\u00b7der", "bra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PRF", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "M\u00fc\u00df euch doch alles arg zu lautrem wol gerathen,", "tokens": ["M\u00fc\u00df", "euch", "doch", "al\u00b7les", "arg", "zu", "laut\u00b7rem", "wol", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PIS", "ADJD", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bi\u00df da\u00df ihr kummt ins wol, das wol bleibt f\u00fcr und f\u00fcr,", "tokens": ["Bi\u00df", "da\u00df", "ihr", "kummt", "ins", "wol", ",", "das", "wol", "bleibt", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "APPRART", "ADV", "$,", "PRELS", "ADV", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und last denn hinter euch, was heissen kan wie ihr.", "tokens": ["Und", "last", "denn", "hin\u00b7ter", "euch", ",", "was", "heis\u00b7sen", "kan", "wie", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$,", "PWS", "VVINF", "VMFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexan