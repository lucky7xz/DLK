{"dta.poem.3128": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "173.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In allen Zonen hat gebl\u00fcht und bl\u00fcht noch jetzt", "tokens": ["In", "al\u00b7len", "Zo\u00b7nen", "hat", "ge\u00b7bl\u00fcht", "und", "bl\u00fcht", "noch", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Allgemeines, nur mit Oertlichem versetzt.", "tokens": ["Ein", "All\u00b7ge\u00b7mei\u00b7nes", ",", "nur", "mit", "O\u00b7ert\u00b7li\u00b7chem", "ver\u00b7setzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Die Menschen sind getheilt in Volks- und Glaubenszunft,", "tokens": ["Die", "Men\u00b7schen", "sind", "ge\u00b7theilt", "in", "Volks", "und", "Glau\u00b7bens\u00b7zunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ihr Gemeinsames ist menschliche Vernunft.", "tokens": ["Doch", "ihr", "Ge\u00b7mein\u00b7sa\u00b7mes", "ist", "menschli\u00b7che", "Ver\u00b7nunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Je mehr vom Zwang der Zunft sich die Vernunft befreit,", "tokens": ["Je", "mehr", "vom", "Zwang", "der", "Zunft", "sich", "die", "Ver\u00b7nunft", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je weitres Feld gewinnt die reine Menschlichkeit.", "tokens": ["Je", "weit\u00b7res", "Feld", "ge\u00b7winnt", "die", "rei\u00b7ne", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zwei Wege aber sind zur Freiheit, gut und b\u00f6se,", "tokens": ["Zwei", "We\u00b7ge", "a\u00b7ber", "sind", "zur", "Frei\u00b7heit", ",", "gut", "und", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VAFIN", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier da\u00df man Formen brech', und dort da\u00df man sie l\u00f6se.", "tokens": ["Hier", "da\u00df", "man", "For\u00b7men", "brech'", ",", "und", "dort", "da\u00df", "man", "sie", "l\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "NN", "PTKVZ", "$,", "KON", "ADV", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht die Verschiedenheit soll ausgestrichen seyn,", "tokens": ["Nicht", "die", "Ver\u00b7schie\u00b7den\u00b7heit", "soll", "aus\u00b7ge\u00b7stri\u00b7chen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch des Verschiednen Streit soll ausgeglichen seyn.", "tokens": ["Doch", "des", "Ver\u00b7schied\u00b7nen", "Streit", "soll", "aus\u00b7ge\u00b7gli\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}