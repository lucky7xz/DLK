{"dta.poem.12388": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Mordknecht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es reit ein Herr und auch sein Knecht,               ", "tokens": ["Es", "reit", "ein", "Herr", "und", "auch", "sein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl \u00fcber ein Heide, die war schlecht,", "tokens": ["Wohl", "\u00fc\u00b7ber", "ein", "Hei\u00b7de", ",", "die", "war", "schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ja schlecht!", "tokens": ["Ja", "schlecht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und alles was sie redeten da,", "tokens": ["Und", "al\u00b7les", "was", "sie", "re\u00b7de\u00b7ten", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "War all's von einer wundersch\u00f6nen Frauen,", "tokens": ["War", "all's", "von", "ei\u00b7ner", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ja Frauen!", "tokens": ["Ja", "Frau\u00b7en", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "\u201each Schildknecht, lieber Schildknecht mein,", "tokens": ["\u201e", "ach", "Schild\u00b7knecht", ",", "lie\u00b7ber", "Schild\u00b7knecht", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADV", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewas redst von meiner Frauen?", "tokens": ["\u201e", "was", "redst", "von", "mei\u00b7ner", "Frau\u00b7en", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eja Frauen!", "tokens": ["\u201e", "ja", "Frau\u00b7en", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201eund f\u00fcrchtest nicht mein braunen Schild,", "tokens": ["\u201e", "und", "f\u00fcrch\u00b7test", "nicht", "mein", "brau\u00b7nen", "Schild", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ezu St\u00fccken will ich dich hauen,", "tokens": ["\u201e", "zu", "St\u00fc\u00b7cken", "will", "ich", "dich", "hau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKZU", "VVINF", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "\u201evor mein'n Augen.\u201c", "tokens": ["\u201e", "vor", "mein'n", "Au\u00b7gen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "\u201eeuern braunen Schild den f\u00fcrcht ich klein,", "tokens": ["\u201e", "eu\u00b7ern", "brau\u00b7nen", "Schild", "den", "f\u00fcrcht", "ich", "klein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "ART", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u201eder lieb Gott wird mich wohl beh\u00fcten,", "tokens": ["\u201e", "der", "lieb", "Gott", "wird", "mich", "wohl", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ebeh\u00fcten!\u201c", "tokens": ["\u201e", "be\u00b7h\u00fc\u00b7ten", "!", "\u201c"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Da schlug der Knecht sein'n Herrn zu todt,", "tokens": ["Da", "schlug", "der", "Knecht", "sein'n", "Herrn", "zu", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das geschahe um Fr\u00e4uleins-G\u00fcte,", "tokens": ["Das", "ge\u00b7scha\u00b7he", "um", "Fr\u00e4u\u00b7leins\u00b7G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ja G\u00fcte!", "tokens": ["Ja", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "\u201enun will ich heim gehen landw\u00e4rts ein,", "tokens": ["\u201e", "nun", "will", "ich", "heim", "ge\u00b7hen", "land\u00b7w\u00e4rts", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PTKVZ", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "\u201ezu einer wundersch\u00f6nen Frauen,", "tokens": ["\u201e", "zu", "ei\u00b7ner", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Frau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eja Frauen!", "tokens": ["\u201e", "ja", "Frau\u00b7en", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201each Fr\u00e4ulein, gebt mir Boten-Lohn,", "tokens": ["\u201e", "ach", "Fr\u00e4u\u00b7lein", ",", "gebt", "mir", "Bo\u00b7ten\u00b7Lohn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eeuer edler Herr und der ist todt,", "tokens": ["\u201e", "eu\u00b7er", "ed\u00b7ler", "Herr", "und", "der", "ist", "todt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "KON", "ART", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u201eso fern auf breiter Heide,", "tokens": ["\u201e", "so", "fern", "auf", "brei\u00b7ter", "Hei\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eja Heide!\u201c", "tokens": ["\u201e", "ja", "Hei\u00b7de", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NE", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "\u201eund ist mein edler Herre todt,", "tokens": ["\u201e", "und", "ist", "mein", "ed\u00b7ler", "Her\u00b7re", "todt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edarum will ich nicht weinen,", "tokens": ["\u201e", "da\u00b7rum", "will", "ich", "nicht", "wei\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eja weinen!", "tokens": ["\u201e", "ja", "wei\u00b7nen", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201eden sch\u00f6nsten Buhlen, den ich hab,", "tokens": ["\u201e", "den", "sch\u00f6ns\u00b7ten", "Buh\u00b7len", ",", "den", "ich", "hab", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eder sitzt bei mir daheime,", "tokens": ["\u201e", "der", "sitzt", "bei", "mir", "da\u00b7hei\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201emutter alleine.\u201c", "tokens": ["\u201e", "mut\u00b7ter", "al\u00b7lei\u00b7ne", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "ADV", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201enun sattel mir mein graues Ro\u00df,", "tokens": ["\u201e", "nun", "sat\u00b7tel", "mir", "mein", "grau\u00b7es", "Ro\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich will von hinnen reiten,", "tokens": ["\u201e", "ich", "will", "von", "hin\u00b7nen", "rei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eja reiten!\u201c", "tokens": ["\u201e", "ja", "rei\u00b7ten", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und da sie auf die Heide kam,", "tokens": ["Und", "da", "sie", "auf", "die", "Hei\u00b7de", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lilien th\u00e4ten sich neigen,", "tokens": ["Die", "Li\u00b7li\u00b7en", "th\u00e4\u00b7ten", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Auf breit'r Heiden.", "tokens": ["Auf", "breit'r", "Hei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Auf band sie ihm sein blanken Helm,", "tokens": ["Auf", "band", "sie", "ihm", "sein", "blan\u00b7ken", "Helm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sahe ihm unter sein' Augen,", "tokens": ["Und", "sa\u00b7he", "ihm", "un\u00b7ter", "sein'", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ja Augen.", "tokens": ["Ja", "Au\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201enun mu\u00df es Christ geklaget seyn,", "tokens": ["\u201e", "nun", "mu\u00df", "es", "Christ", "ge\u00b7kla\u00b7get", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ewie bist so sehr zerhauen,", "tokens": ["\u201e", "wie", "bist", "so", "sehr", "zer\u00b7hau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eunter dein Augen.", "tokens": ["\u201e", "un\u00b7ter", "dein", "Au\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "\u201enun will ich in ein Kloster ziehn,", "tokens": ["\u201e", "nun", "will", "ich", "in", "ein", "Klos\u00b7ter", "ziehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewill 'n lieben Gott f\u00fcr dich bitten,", "tokens": ["\u201e", "will", "'n", "lie\u00b7ben", "Gott", "f\u00fcr", "dich", "bit\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u201eja bitten!", "tokens": ["\u201e", "ja", "bit\u00b7ten", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201eda\u00df er dich ins Himmelreich woll lahn,", "tokens": ["\u201e", "da\u00df", "er", "dich", "ins", "Him\u00b7mel\u00b7reich", "woll", "lahn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u201edas gescheh durch meinetwillen,", "tokens": ["\u201e", "das", "ge\u00b7scheh", "durch", "mei\u00b7net\u00b7wil\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "APPR", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201eschweig stillen!", "tokens": ["\u201e", "schweig", "stil\u00b7len", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Wer ist's, der uns den Reihen sang,", "tokens": ["Wer", "ist's", ",", "der", "uns", "den", "Rei\u00b7hen", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Matthias J\u00e4ger ist er genannt,", "tokens": ["Mat\u00b7thi\u00b7as", "J\u00e4\u00b7ger", "ist", "er", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Beim Trunk hat er's gesungen,", "tokens": ["Beim", "Trunk", "hat", "er's", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesungen!", "tokens": ["Ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Er ist sein'm Widersacher von Herzen Feind,", "tokens": ["Er", "ist", "sein'm", "Wi\u00b7der\u00b7sa\u00b7cher", "von", "Her\u00b7zen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zu ihm kann er nicht kommen,", "tokens": ["Zu", "ihm", "kann", "er", "nicht", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Ja kommen.", "tokens": ["Ja", "kom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}