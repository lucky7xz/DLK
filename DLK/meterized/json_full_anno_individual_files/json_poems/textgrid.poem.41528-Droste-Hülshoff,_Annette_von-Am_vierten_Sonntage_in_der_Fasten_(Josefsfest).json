ct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da k\u00f6mmst du mich zu laben", "tokens": ["Da", "k\u00f6mmst", "du", "mich", "zu", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit seltner Freudigkeit.", "tokens": ["Mit", "selt\u00b7ner", "Freu\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Zu dir will ich mich fl\u00fcchten,", "tokens": ["Zu", "dir", "will", "ich", "mich", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein scheues Leben richten,", "tokens": ["Mein", "scheu\u00b7es", "Le\u00b7ben", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Josef, milder Hauch!", "tokens": ["O", "Jo\u00b7sef", ",", "mil\u00b7der", "Hauch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du hast gekannt die Fehle", "tokens": ["Du", "hast", "ge\u00b7kannt", "die", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deiner starken Seele,", "tokens": ["In", "dei\u00b7ner", "star\u00b7ken", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und die Vergebung auch!", "tokens": ["Und", "die", "Ver\u00b7ge\u00b7bung", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Was hast du nicht geduldet,", "tokens": ["Was", "hast", "du", "nicht", "ge\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da in geheim verschuldet", "tokens": ["Da", "in", "ge\u00b7heim", "ver\u00b7schul\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Maria dir erschien?", "tokens": ["Ma\u00b7ria", "dir", "er\u00b7schien", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und konntest ihr nicht trauen,", "tokens": ["Und", "konn\u00b7test", "ihr", "nicht", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Worauf die Himmel bauen,", "tokens": ["Wo\u00b7rauf", "die", "Him\u00b7mel", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und hast ihr doch verziehn!", "tokens": ["Und", "hast", "ihr", "doch", "ver\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Und da du mu\u00dftest scheiden", "