{"textgrid.poem.41528": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am vierten Sonntage in der Fasten (Josefsfest)", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gegr\u00fc\u00dft in deinem Scheine,", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "in", "dei\u00b7nem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Abendsonne reine,", "tokens": ["Du", "A\u00b7bend\u00b7son\u00b7ne", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du alter Lilienzweig!", "tokens": ["Du", "al\u00b7ter", "Li\u00b7li\u00b7en\u00b7zweig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der du noch hast getragen", "tokens": ["Der", "du", "noch", "hast", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deinen grauen Tagen", "tokens": ["In", "dei\u00b7nen", "grau\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So mildes Bl\u00fctenreich!", "tokens": ["So", "mil\u00b7des", "Bl\u00fc\u00b7ten\u00b7reich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Je mehr es sich entfaltet,", "tokens": ["Je", "mehr", "es", "sich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Ehrenkranz gestaltet,", "tokens": ["Zum", "Eh\u00b7ren\u00b7kranz", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der deine Stirn umlaubt:", "tokens": ["Der", "dei\u00b7ne", "Stirn", "um\u00b7laubt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Je mehr hast du geneiget,", "tokens": ["Je", "mehr", "hast", "du", "ge\u00b7nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In Ehrfurcht ganz gebeuget", "tokens": ["In", "Ehr\u00b7furcht", "ganz", "ge\u00b7beu\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein gnadenschweres Haupt.", "tokens": ["Dein", "gna\u00b7den\u00b7schwe\u00b7res", "Haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie ist zu meinem Frommen", "tokens": ["Wie", "ist", "zu", "mei\u00b7nem", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein freundlich Fest gekommen", "tokens": ["Dein", "freund\u00b7lich", "Fest", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In diese ernste Zeit?", "tokens": ["In", "die\u00b7se", "erns\u00b7te", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich war fast wie begraben:", "tokens": ["Ich", "war", "fast", "wie", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da k\u00f6mmst du mich zu laben", "tokens": ["Da", "k\u00f6mmst", "du", "mich", "zu", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit seltner Freudigkeit.", "tokens": ["Mit", "selt\u00b7ner", "Freu\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu dir will ich mich fl\u00fcchten,", "tokens": ["Zu", "dir", "will", "ich", "mich", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein scheues Leben richten,", "tokens": ["Mein", "scheu\u00b7es", "Le\u00b7ben", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Josef, milder Hauch!", "tokens": ["O", "Jo\u00b7sef", ",", "mil\u00b7der", "Hauch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du hast gekannt die Fehle", "tokens": ["Du", "hast", "ge\u00b7kannt", "die", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deiner starken Seele,", "tokens": ["In", "dei\u00b7ner", "star\u00b7ken", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und die Vergebung auch!", "tokens": ["Und", "die", "Ver\u00b7ge\u00b7bung", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was hast du nicht geduldet,", "tokens": ["Was", "hast", "du", "nicht", "ge\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da in geheim verschuldet", "tokens": ["Da", "in", "ge\u00b7heim", "ver\u00b7schul\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Maria dir erschien?", "tokens": ["Ma\u00b7ria", "dir", "er\u00b7schien", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und konntest ihr nicht trauen,", "tokens": ["Und", "konn\u00b7test", "ihr", "nicht", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Worauf die Himmel bauen,", "tokens": ["Wo\u00b7rauf", "die", "Him\u00b7mel", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und hast ihr doch verziehn!", "tokens": ["Und", "hast", "ihr", "doch", "ver\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und da du mu\u00dftest scheiden", "tokens": ["Und", "da", "du", "mu\u00df\u00b7test", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit deinen lieben beiden:", "tokens": ["Mit", "dei\u00b7nen", "lie\u00b7ben", "bei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie gro\u00df war deine Not!", "tokens": ["Wie", "gro\u00df", "war", "dei\u00b7ne", "Not", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die W\u00fcste schien dir lange;", "tokens": ["Die", "W\u00fcs\u00b7te", "schien", "dir", "lan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch war vom Untergange", "tokens": ["Doch", "war", "vom", "Un\u00b7ter\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein liebes Kind bedroht.", "tokens": ["Dein", "lie\u00b7bes", "Kind", "be\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und da er glanzumkr\u00f6net:", "tokens": ["Und", "da", "er", "glan\u00b7zu\u00b7mkr\u00f6\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie bist du nicht geh\u00f6hnet", "tokens": ["Wie", "bist", "du", "nicht", "ge\u00b7h\u00f6h\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um seine Gotteskraft!", "tokens": ["Um", "sei\u00b7ne", "Got\u00b7tes\u00b7kraft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie mag, den Groll zu laben,", "tokens": ["Wie", "mag", ",", "den", "Groll", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich nicht gel\u00e4stert haben", "tokens": ["Dich", "nicht", "ge\u00b7l\u00e4s\u00b7tert", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "VVPP", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die arge Priesterschaft!", "tokens": ["Die", "ar\u00b7ge", "Pries\u00b7ter\u00b7schaft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und gar, wenn gottdurchdrungen", "tokens": ["Und", "gar", ",", "wenn", "gott\u00b7durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich gr\u00fc\u00dften fromme Zungen", "tokens": ["Dich", "gr\u00fc\u00df\u00b7ten", "from\u00b7me", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und priesen laut und weit:", "tokens": ["Und", "prie\u00b7sen", "laut", "und", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie hast du nicht in Zagen", "tokens": ["Wie", "hast", "du", "nicht", "in", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An deine Brust geschlagen", "tokens": ["An", "dei\u00b7ne", "Brust", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In deiner S\u00fcndlichkeit!", "tokens": ["In", "dei\u00b7ner", "S\u00fcnd\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So hast du viel getragen,", "tokens": ["So", "hast", "du", "viel", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unendlich viele Plagen,", "tokens": ["Un\u00b7end\u00b7lich", "vie\u00b7le", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit freundlicher Geduld,", "tokens": ["Mit", "freund\u00b7li\u00b7cher", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ist in all den Jahren", "tokens": ["Und", "ist", "in", "all", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Manch Seufzer dir entfahren", "tokens": ["Manch", "Seuf\u00b7zer", "dir", "ent\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und manche kleine Schuld.", "tokens": ["Und", "man\u00b7che", "klei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du frommer Held! im Glauben,", "tokens": ["Du", "from\u00b7mer", "Held", "!", "im", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den schrecklich dir zu rauben", "tokens": ["Den", "schreck\u00b7lich", "dir", "zu", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich alle Welt verband:", "tokens": ["Sich", "al\u00b7le", "Welt", "ver\u00b7band", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hast k\u00f6nnen nicht erhalten", "tokens": ["Hast", "k\u00f6n\u00b7nen", "nicht", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein unbeflecktes Walten", "tokens": ["Ein", "un\u00b7be\u00b7fleck\u00b7tes", "Wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An deines Jesu Hand.", "tokens": ["An", "dei\u00b7nes", "Je\u00b7su", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Was soll ich denn nicht hoffen,", "tokens": ["Was", "soll", "ich", "denn", "nicht", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da noch der Himmel offen,", "tokens": ["Da", "noch", "der", "Him\u00b7mel", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Seele still?", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "still", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Will sich die Gnade nahen:", "tokens": ["Will", "sich", "die", "Gna\u00b7de", "na\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich kann sie wohl empfahen,", "tokens": ["Ich", "kann", "sie", "wohl", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So Gott mir helfen will.", "tokens": ["So", "Gott", "mir",