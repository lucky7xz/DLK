": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Was soll ich denn nicht hoffen,", "tokens": ["Was", "soll", "ich", "denn", "nicht", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da noch der Himmel offen,", "tokens": ["Da", "noch", "der", "Him\u00b7mel", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Seele still?", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "still", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Will sich die Gnade nahen:", "tokens": ["Will", "sich", "die", "Gna\u00b7de", "na\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich kann sie wohl empfahen,", "tokens": ["Ich", "kann", "sie", "wohl", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So Gott mir helfen will.", "tokens": ["So", "Gott", "mir", "hel\u00b7fen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Zerrissen in den Gr\u00fcnden", "tokens": ["Zer\u00b7ris\u00b7sen", "in", "den", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "ia