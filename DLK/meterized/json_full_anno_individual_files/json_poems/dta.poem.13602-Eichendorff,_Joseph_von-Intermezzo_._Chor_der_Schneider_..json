{"dta.poem.13602": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Intermezzo .  \n  Chor der Schneider .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nur vom Ganzen frisch gerissen,", "tokens": ["Nur", "vom", "Gan\u00b7zen", "frisch", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' die Waare ganz verschlissen,", "tokens": ["Eh'", "die", "Waa\u00b7re", "ganz", "ver\u00b7schlis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier ein uralt g\u00fclden St\u00fcck,", "tokens": ["Hier", "ein", "ur\u00b7alt", "g\u00fcl\u00b7den", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Giebt so'n gewissen frommen Blick,", "tokens": ["Giebt", "so'n", "ge\u00b7wis\u00b7sen", "from\u00b7men", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAPP", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier ein bunter welscher Flick,", "tokens": ["Hier", "ein", "bun\u00b7ter", "wel\u00b7scher", "Flick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "PWAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Drauf ein St\u00fcck Hausleinewand,", "tokens": ["Drauf", "ein", "St\u00fcck", "Haus\u00b7lei\u00b7ne\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Macht das Welsche erst pikant", "tokens": ["Macht", "das", "Wel\u00b7sche", "erst", "pi\u00b7kant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hie 'nen Fetzen B\u00e4renhaut,", "tokens": ["Hie", "'nen", "Fet\u00b7zen", "B\u00e4\u00b7ren\u00b7haut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df man auch das Teutsche schaut,", "tokens": ["Da\u00df", "man", "auch", "das", "Teut\u00b7sche", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dr\u00fcber einen span'schen Kragen,", "tokens": ["Dr\u00fc\u00b7ber", "ei\u00b7nen", "span'\u00b7schen", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Das Erhab'ne wird behagen,", "tokens": ["Das", "Er\u00b7hab'\u00b7ne", "wird", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Frisch gestichelt, fein zum Werke,", "tokens": ["Frisch", "ge\u00b7sti\u00b7chelt", ",", "fein", "zum", "Wer\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und wird auch nichts Ganzes draus,", "tokens": ["Und", "wird", "auch", "nichts", "Gan\u00b7zes", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "NN", "PAV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Sieht es doch gar niedlich aus!", "tokens": ["Sieht", "es", "doch", "gar", "nied\u00b7lich", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}