{"textgrid.poem.59984": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland, sie sagen, du h\u00e4ngst den Kopf \u2013", "tokens": ["Deutschland", ",", "sie", "sa\u00b7gen", ",", "du", "h\u00e4ngst", "den", "Kopf", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mir geht ins Herz das Gestichel \u2013", "tokens": ["Mir", "geht", "ins", "Herz", "das", "Ge\u00b7sti\u00b7chel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du seist ein tatenloser Tropf;", "tokens": ["Du", "seist", "ein", "ta\u00b7ten\u00b7lo\u00b7ser", "Tropf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sagen die Leute, o Michel!", "tokens": ["So", "sa\u00b7gen", "die", "Leu\u00b7te", ",", "o", "Mi\u00b7chel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "FM", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das alte Lied vom alten Malheur", "tokens": ["Das", "al\u00b7te", "Lied", "vom", "al\u00b7ten", "Mal\u00b7heur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "H\u00f6r ich von neuem erklingen:", "tokens": ["H\u00f6r", "ich", "von", "neu\u00b7em", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Du werdest's nimmer zum Akteur", "tokens": ["Du", "wer\u00b7dest's", "nim\u00b7mer", "zum", "Ak\u00b7teur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dieser B\u00fchne bringen \u2013", "tokens": ["Auf", "die\u00b7ser", "B\u00fch\u00b7ne", "brin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo alles l\u00e4uft, wo alles rennt,", "tokens": ["Wo", "al\u00b7les", "l\u00e4uft", ",", "wo", "al\u00b7les", "rennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zuaven und Turkos schw\u00e4rmen", "tokens": ["Die", "Zu\u00b7a\u00b7ven", "und", "Tur\u00b7kos", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr V\u00f6lkergl\u00fcck. \u2013 Du hast kein Talent", "tokens": ["F\u00fcr", "V\u00f6l\u00b7ker\u00b7gl\u00fcck", ".", "\u2013", "Du", "hast", "kein", "Ta\u00b7lent"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "$(", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu welthistorischem L\u00e4rmen!", "tokens": ["Zu", "welt\u00b7his\u00b7to\u00b7ri\u00b7schem", "L\u00e4r\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Du dehnst dich ruhig auf deinem Pf\u00fchl", "tokens": ["Du", "dehnst", "dich", "ru\u00b7hig", "auf", "dei\u00b7nem", "Pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und tr\u00e4umst von Hegel und Fichte,", "tokens": ["Und", "tr\u00e4umst", "von", "He\u00b7gel", "und", "Fich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hast doch erlebt so dumpf und schw\u00fcl", "tokens": ["Und", "hast", "doch", "er\u00b7lebt", "so", "dumpf", "und", "schw\u00fcl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hundstage der Weltgeschichte.", "tokens": ["Hunds\u00b7ta\u00b7ge", "der", "Welt\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Hundstage \u2013 die V\u00f6lker wurden toll,", "tokens": ["Hunds\u00b7ta\u00b7ge", "\u2013", "die", "V\u00f6l\u00b7ker", "wur\u00b7den", "toll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch Deutschland rief vern\u00fcnftig:", "tokens": ["Doch", "Deutschland", "rief", "ver\u00b7n\u00fcnf\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbman soll nicht nur zerst\u00f6ren, man soll", "tokens": ["\u00bb", "man", "soll", "nicht", "nur", "zer\u00b7st\u00f6\u00b7ren", ",", "man", "soll"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PIS", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,", "PIS", "VMFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Auch wieder aufbauen k\u00fcnftig.", "tokens": ["Auch", "wie\u00b7der", "auf\u00b7bau\u00b7en", "k\u00fcnf\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIZU", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Eh ich Zwing-Uri zerst\u00f6re, traun,", "tokens": ["Eh", "ich", "Zwing\u00b7U\u00b7ri", "zer\u00b7st\u00f6\u00b7re", ",", "traun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$,", "VVINF", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Was setz ich an seine Stelle?", "tokens": ["Was", "setz", "ich", "an", "sei\u00b7ne", "Stel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie werd ich die Gef\u00e4ngnisse baun", "tokens": ["Wie", "werd", "ich", "die", "Ge\u00b7f\u00e4ng\u00b7nis\u00b7se", "baun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In Zukunft und die \u2013 Kasernen?", "tokens": ["In", "Zu\u00b7kunft", "und", "die", "\u2013", "Ka\u00b7ser\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "$(", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Man mu\u00df der Stimme der Natur", "tokens": ["Man", "mu\u00df", "der", "Stim\u00b7me", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vor allem sich bequemen;", "tokens": ["Vor", "al\u00b7lem", "sich", "be\u00b7que\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PRF", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schrein die Schafe nach der Schur:", "tokens": ["Und", "schrein", "die", "Scha\u00b7fe", "nach", "der", "Schur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wird sie \u00fcbernehmen?", "tokens": ["Wer", "wird", "sie", "\u00fc\u00b7ber\u00b7neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sind alles Fragen von Wichtigkeit,", "tokens": ["Sind", "al\u00b7les", "Fra\u00b7gen", "von", "Wich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gediegen, tief und edel;", "tokens": ["Ge\u00b7die\u00b7gen", ",", "tief", "und", "e\u00b7del", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daran soll man die Dichtigkeit", "tokens": ["Da\u00b7ran", "soll", "man", "die", "Dich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erkennen der deutschen Sch\u00e4del!\u00ab \u2013", "tokens": ["Er\u00b7ken\u00b7nen", "der", "deut\u00b7schen", "Sch\u00e4\u00b7del", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Ja, Michel, du bist kein Franzos,", "tokens": ["Ja", ",", "Mi\u00b7chel", ",", "du", "bist", "kein", "Fran\u00b7zos", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stets nur negativ ist,", "tokens": ["Der", "stets", "nur", "ne\u00b7ga\u00b7tiv", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er kennt die Oberfl\u00e4che blo\u00df,", "tokens": ["Er", "kennt", "die", "O\u00b7berf\u00b7l\u00e4\u00b7che", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wei\u00dft allein, was tief ist.", "tokens": ["Du", "wei\u00dft", "al\u00b7lein", ",", "was", "tief", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ja, Deutschland, du bist tief im Wort", "tokens": ["Ja", ",", "Deutschland", ",", "du", "bist", "tief", "im", "Wort"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "$,", "PPER", "VAFIN", "ADJD", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und bist im Tun noch weiser;", "tokens": ["Und", "bist", "im", "Tun", "noch", "wei\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du l\u00e4\u00dft nicht ", "tokens": ["Du", "l\u00e4\u00dft", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Bis fertig du \u2013 mit dem Kaiser.", "tokens": ["Bis", "fer\u00b7tig", "du", "\u2013", "mit", "dem", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Ein Kaiser, das ist der h\u00f6chste Wunsch,", "tokens": ["Ein", "Kai\u00b7ser", ",", "das", "ist", "der", "h\u00f6chs\u00b7te", "Wunsch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den wir im Herzen tragen;", "tokens": ["Den", "wir", "im", "Her\u00b7zen", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir lassen ihn bei Wein u