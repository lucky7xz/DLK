{"dta.poem.7104": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will etwann dieses dir zu gro\u00df, zu herrlich scheinen,", "tokens": ["Will", "et\u00b7wann", "die\u00b7ses", "dir", "zu", "gro\u00df", ",", "zu", "herr\u00b7lich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDAT", "PPER", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zw\u00e4nge dich dein Unglaub\u2019 auch zu meinen,", "tokens": ["Und", "zw\u00e4n\u00b7ge", "dich", "dein", "Un\u00b7glaub'", "auch", "zu", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df diese Herrlichkeit f\u00fcr dich zu herrlich w\u00e4re,", "tokens": ["Da\u00df", "die\u00b7se", "Herr\u00b7lich\u00b7keit", "f\u00fcr", "dich", "zu", "herr\u00b7lich", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "PPER", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein gantzes Wesen sey nicht f\u00e4hig solcher Ehre;", "tokens": ["Dein", "gant\u00b7zes", "We\u00b7sen", "sey", "nicht", "f\u00e4\u00b7hig", "sol\u00b7cher", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So dencke nur zur\u00fcck auf das, was du gewesen,", "tokens": ["So", "den\u00b7cke", "nur", "zu\u00b7r\u00fcck", "auf", "das", ",", "was", "du", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "APPR", "ART", "$,", "PWS", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Eh, da\u00df du worden bist. Die Finsterni\u00df, das Nichts,", "tokens": ["Eh", ",", "da\u00df", "du", "wor\u00b7den", "bist", ".", "Die", "Fins\u00b7ter\u00b7ni\u00df", ",", "das", "Nichts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VAPP", "VAFIN", "$.", "ART", "NN", "$,", "PRELS", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus welchem GOTT dich hat gezogen und erlesen,", "tokens": ["Aus", "wel\u00b7chem", "GoTT", "dich", "hat", "ge\u00b7zo\u00b7gen", "und", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VAFIN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zum fr\u00f6lichen Genu\u00df des Sonnen-Lichts.", "tokens": ["Zum", "fr\u00f6\u00b7li\u00b7chen", "Ge\u00b7nu\u00df", "des", "Son\u00b7nen\u00b7Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie unbeschreiblich gro\u00df ist doch der Unterscheid,", "tokens": ["Wie", "un\u00b7be\u00b7schreib\u00b7lich", "gro\u00df", "ist", "doch", "der", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vom Schatten zu dem Licht, vom Nichts zur W\u00fcrcklichkeit!", "tokens": ["Vom", "Schat\u00b7ten", "zu", "dem", "Licht", ",", "vom", "Nichts", "zur", "W\u00fcrck\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "APPRART", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}