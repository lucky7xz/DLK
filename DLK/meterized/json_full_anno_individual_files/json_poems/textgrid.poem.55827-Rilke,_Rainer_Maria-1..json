{"textgrid.poem.55827": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Andere m\u00fcssen auf langen Wegen", "tokens": ["An\u00b7de\u00b7re", "m\u00fcs\u00b7sen", "auf", "lan\u00b7gen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zu den dunklen Dichtern gehn;", "tokens": ["zu", "den", "dunk\u00b7len", "Dich\u00b7tern", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "fragen immer irgendwen,", "tokens": ["fra\u00b7gen", "im\u00b7mer", "ir\u00b7gend\u00b7wen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ob er nicht einen hat singen sehn", "tokens": ["ob", "er", "nicht", "ei\u00b7nen", "hat", "sin\u00b7gen", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "PIS", "VAFIN", "VVINF", "VVINF"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "oder H\u00e4nde auf Saiten legen.", "tokens": ["o\u00b7der", "H\u00e4n\u00b7de", "auf", "Sai\u00b7ten", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Nur die M\u00e4dchen fragen nicht,", "tokens": ["Nur", "die", "M\u00e4d\u00b7chen", "fra\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "welche Br\u00fccke zu Bildern f\u00fchre;", "tokens": ["wel\u00b7che", "Br\u00fc\u00b7cke", "zu", "Bil\u00b7dern", "f\u00fch\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "l\u00e4cheln nur, lichter als Perlenschn\u00fcre,", "tokens": ["l\u00e4\u00b7cheln", "nur", ",", "lich\u00b7ter", "als", "Per\u00b7len\u00b7schn\u00fc\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "die man an Schalen von Silber h\u00e4lt.", "tokens": ["die", "man", "an", "Scha\u00b7len", "von", "Sil\u00b7ber", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Aus ihrem Leben geht jede T\u00fcre", "tokens": ["Aus", "ih\u00b7rem", "Le\u00b7ben", "geht", "je\u00b7de", "T\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "in einen Dichter", "tokens": ["in", "ei\u00b7nen", "Dich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "und in die Welt.", "tokens": ["und", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Andere m\u00fcssen auf lange