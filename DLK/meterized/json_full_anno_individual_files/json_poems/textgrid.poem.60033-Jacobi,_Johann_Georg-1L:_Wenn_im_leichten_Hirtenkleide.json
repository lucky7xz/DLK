text": "Z\u00e4rtlich in dem Wiederhall!", "tokens": ["Z\u00e4rt\u00b7lich", "in", "dem", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lispelt' ich an Rosenw\u00e4nden", "tokens": ["Lis\u00b7pelt'", "ich", "an", "Ro\u00b7sen\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Abendwind herab,", "tokens": ["Als", "ein", "Ab\u00b7end\u00b7wind", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder w\u00e4r' in ihren H\u00e4nden", "tokens": ["O\u00b7der", "w\u00e4r'", "in", "ih\u00b7ren", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der bebl\u00fcmte Hirtenstab!", "tokens": ["Der", "be\u00b7bl\u00fcm\u00b7te", "Hir\u00b7ten\u00b7stab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "K\u00f6nnt' ich ihr als Veilchen dienen,", "tokens": ["K\u00f6nnt'", "ich", "ihr", "als", "Veil\u00b7chen", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie neue Kr\u00e4nze flicht;", "tokens": ["Wenn", "sie", "neu\u00b7e", "Kr\u00e4n\u00b7ze", "flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "