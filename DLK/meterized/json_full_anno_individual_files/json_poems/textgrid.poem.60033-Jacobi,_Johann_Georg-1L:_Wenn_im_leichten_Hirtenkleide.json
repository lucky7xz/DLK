{"textgrid.poem.60033": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn im leichten Hirtenkleide", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn im leichten Hirtenkleide", "tokens": ["Wenn", "im", "leich\u00b7ten", "Hir\u00b7ten\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein geliebtes M\u00e4dchen geht,", "tokens": ["Mein", "ge\u00b7lieb\u00b7tes", "M\u00e4d\u00b7chen", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn um sie die junge Freude", "tokens": ["Wenn", "um", "sie", "die", "jun\u00b7ge", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich im s\u00fc\u00dfen Taumel dreht,", "tokens": ["Sich", "im", "s\u00fc\u00b7\u00dfen", "Tau\u00b7mel", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Rosen, zwischen Reben,", "tokens": ["Un\u00b7ter", "Ro\u00b7sen", ",", "zwi\u00b7schen", "Re\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Hain und an dem Bach,", "tokens": ["In", "dem", "Hain", "und", "an", "dem", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Folgt ihr dann mit stillem Beben", "tokens": ["Folgt", "ihr", "dann", "mit", "stil\u00b7lem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine ganze Seele nach.", "tokens": ["Mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4r' ich auf der Fr\u00fchlingsaue", "tokens": ["W\u00e4r'", "ich", "auf", "der", "Fr\u00fch\u00b7lin\u00b7gsau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Nur das L\u00fcftchen, das sie f\u00fchlt,", "tokens": ["Nur", "das", "L\u00fcft\u00b7chen", ",", "das", "sie", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein Tropfen von dem Thaue", "tokens": ["Nur", "ein", "Trop\u00b7fen", "von", "dem", "Thau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der um sie die Blume k\u00fchlt;", "tokens": ["Der", "um", "sie", "die", "Blu\u00b7me", "k\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nur das B\u00e4umchen an der Quelle,", "tokens": ["Nur", "das", "B\u00e4um\u00b7chen", "an", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das sie sch\u00fctzet und erg\u00f6tzt,", "tokens": ["Das", "sie", "sch\u00fct\u00b7zet", "und", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die kleine Silberwelle,", "tokens": ["Und", "die", "klei\u00b7ne", "Sil\u00b7ber\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die den sch\u00f6nsten Fu\u00df benetzt!", "tokens": ["Die", "den", "sch\u00f6ns\u00b7ten", "Fu\u00df", "be\u00b7netzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4ren meine Klaget\u00f6ne", "tokens": ["W\u00e4\u00b7ren", "mei\u00b7ne", "Kla\u00b7ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Gesang der Nachtigall,", "tokens": ["Der", "Ge\u00b7sang", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rte mich die sanfte Sch\u00f6ne", "tokens": ["H\u00f6r\u00b7te", "mich", "die", "sanf\u00b7te", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4rtlich in dem Wiederhall!", "tokens": ["Z\u00e4rt\u00b7lich", "in", "dem", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lispelt' ich an Rosenw\u00e4nden", "tokens": ["Lis\u00b7pelt'", "ich", "an", "Ro\u00b7sen\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Abendwind herab,", "tokens": ["Als", "ein", "Ab\u00b7end\u00b7wind", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder w\u00e4r' in ihren H\u00e4nden", "tokens": ["O\u00b7der", "w\u00e4r'", "in", "ih\u00b7ren", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der bebl\u00fcmte Hirtenstab!", "tokens": ["Der", "be\u00b7bl\u00fcm\u00b7te", "Hir\u00b7ten\u00b7stab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00f6nnt' ich ihr als Veilchen dienen,", "tokens": ["K\u00f6nnt'", "ich", "ihr", "als", "Veil\u00b7chen", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie neue Kr\u00e4nze flicht;", "tokens": ["Wenn", "sie", "neu\u00b7e", "Kr\u00e4n\u00b7ze", "flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich in der Laube gr\u00fcnen,", "tokens": ["K\u00f6nnt'", "ich", "in", "der", "Lau\u00b7be", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo mit ihr ein Engel spricht!", "tokens": ["Wo", "mit", "ihr", "ein", "En\u00b7gel", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "B\u00f6t' ich in vertrauten Schatten", "tokens": ["B\u00f6t'", "ich", "in", "ver\u00b7trau\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihrem Schlummer sanftes Moos,", "tokens": ["Ih\u00b7rem", "Schlum\u00b7mer", "sanf\u00b7tes", "Moos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder, wo sich T\u00e4ubchen gatten,", "tokens": ["O\u00b7der", ",", "wo", "sich", "T\u00e4ub\u00b7chen", "gat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meinen blumenreichen Schoo\u00df!", "tokens": ["Mei\u00b7nen", "blu\u00b7men\u00b7rei\u00b7chen", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mach', o Liebe! dort im Stillen,", "tokens": ["Mach'", ",", "o", "Lie\u00b7be", "!", "dort", "im", "Stil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$.", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter jenem Mirthenbaum,", "tokens": ["Un\u00b7ter", "je\u00b7nem", "Mir\u00b7then\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sie ruht, um ihretwillen", "tokens": ["Wo", "sie", "ruht", ",", "um", "ih\u00b7ret\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KOUI", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zum leichten Morgentraum!", "tokens": ["Mich", "zum", "leich\u00b7ten", "Mor\u00b7gen\u00b7traum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit versch\u00e4mtem holden Lachen", "tokens": ["Mit", "ver\u00b7sch\u00e4m\u00b7tem", "hol\u00b7den", "La\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sehe sie mein Schattenbild \u2013", "tokens": ["Se\u00b7he", "sie", "mein", "Schat\u00b7ten\u00b7bild", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, o Liebe, beym Erwachen", "tokens": ["Und", ",", "o", "Lie\u00b7be", ",", "beym", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "We