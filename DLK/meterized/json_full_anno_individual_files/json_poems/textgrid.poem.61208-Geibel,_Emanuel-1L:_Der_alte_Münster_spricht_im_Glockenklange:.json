{"textgrid.poem.61208": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der alte M\u00fcnster spricht im Glockenklange:", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte M\u00fcnster spricht im Glockenklange:", "tokens": ["Der", "al\u00b7te", "M\u00fcns\u00b7ter", "spricht", "im", "Glo\u00b7cken\u00b7klan\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich hie\u00df die deutsche Kunst in bessern Tagen", "tokens": ["Mich", "hie\u00df", "die", "deut\u00b7sche", "Kunst", "in", "bes\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit meinen Gipfeln in die Sterne ragen,", "tokens": ["Mit", "mei\u00b7nen", "Gip\u00b7feln", "in", "die", "Ster\u00b7ne", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch steh' ich l\u00e4ngst betr\u00fcbt in welschem Zwange.", "tokens": ["Doch", "steh'", "ich", "l\u00e4ngst", "be\u00b7tr\u00fcbt", "in", "wel\u00b7schem", "Zwan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt, wo ich schaue nach der Zeiten Gange,", "tokens": ["Jetzt", ",", "wo", "ich", "schau\u00b7e", "nach", "der", "Zei\u00b7ten", "Gan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewahr' ich, da\u00df aufs neu' mit frechem Wagen", "tokens": ["Ge\u00b7wahr'", "ich", ",", "da\u00df", "aufs", "neu'", "mit", "fre\u00b7chem", "Wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "APPRART", "ADJA", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Fremdling sich vermi\u00dft, ein Glied zu schlagen", "tokens": ["Ein", "Fremd\u00b7ling", "sich", "ver\u00b7mi\u00dft", ",", "ein", "Glied", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom deutschen Leib, und lauschen mu\u00df ich bange.", "tokens": ["Vom", "deut\u00b7schen", "Leib", ",", "und", "lau\u00b7schen", "mu\u00df", "ich", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gelingt's ihm: weh, so will im Staub ich trauern,", "tokens": ["Ge\u00b7lingt's", "ihm", ":", "weh", ",", "so", "will", "im", "Staub", "ich", "trau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$.", "PTKVZ", "$,", "ADV", "VMFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gluten 