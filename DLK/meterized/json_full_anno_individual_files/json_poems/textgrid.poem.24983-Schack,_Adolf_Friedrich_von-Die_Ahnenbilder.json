{"textgrid.poem.24983": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Ahnenbilder", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem altergrauen Rahmen", "tokens": ["Aus", "dem", "al\u00b7ter\u00b7grau\u00b7en", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt ihr fremd auf mich herab,", "tokens": ["Blickt", "ihr", "fremd", "auf", "mich", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ins Aug' euch mit Vertrauen", "tokens": ["Und", "ins", "Aug'", "euch", "mit", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Sohn nicht kann ich schauen;", "tokens": ["Wie", "ein", "Sohn", "nicht", "kann", "ich", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts mit euch ja als den Namen", "tokens": ["Nichts", "mit", "euch", "ja", "als", "den", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PPER", "ADV", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Teil' ich und dereinst das Grab.", "tokens": ["Teil'", "ich", "und", "de\u00b7reinst", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Still am v\u00e4terlichen Herde,", "tokens": ["Still", "am", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An die Scholle festgebannt,", "tokens": ["An", "die", "Schol\u00b7le", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebtet ihr im Kreis, dem engen,", "tokens": ["Leb\u00b7tet", "ihr", "im", "Kreis", ",", "dem", "en\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kanntet nicht das wilde Dr\u00e4ngen,", "tokens": ["Kann\u00b7tet", "nicht", "das", "wil\u00b7de", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das mich \u00fcber diese Erde", "tokens": ["Das", "mich", "\u00fc\u00b7ber", "die\u00b7se", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruhlos trieb von Land zu Land;", "tokens": ["Ruh\u00b7los", "trieb", "von", "Land", "zu", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht der N\u00e4chte bleiche Qualen,", "tokens": ["Nicht", "der", "N\u00e4ch\u00b7te", "blei\u00b7che", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Geist in Fieberhast", "tokens": ["Wenn", "der", "Geist", "in", "Fie\u00b7ber\u00b7hast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht ein Traumbild zu erreichen,", "tokens": ["Sucht", "ein", "Traum\u00b7bild", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es weichen sieht und weichen,", "tokens": ["Doch", "es", "wei\u00b7chen", "sieht", "und", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis es in des Morgens Strahlen", "tokens": ["Bis", "es", "in", "des", "Mor\u00b7gens", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Meteor erbla\u00dft.", "tokens": ["Wie", "ein", "Me\u00b7te\u00b7or", "er\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ob des Enkels Thun und Trachten", "tokens": ["Ob", "des", "En\u00b7kels", "Thun", "und", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fctteln seh' ich euch das Haupt;", "tokens": ["Sch\u00fct\u00b7teln", "seh'", "ich", "euch", "das", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch schon hat es ihn inmitten", "tokens": ["Fr\u00fch", "schon", "hat", "es", "ihn", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Verwandten nicht gelitten;", "tokens": ["Der", "Ver\u00b7wand\u00b7ten", "nicht", "ge\u00b7lit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht gedacht, so wie sie dachten,", "tokens": ["Nicht", "ge\u00b7dacht", ",", "so", "wie", "sie", "dach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat er, noch wie sie geglaubt.", "tokens": ["Hat", "er", ",", "noch", "wie", "sie", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wert der M\u00fchn schien ihm nur eines \u2013", "tokens": ["Wert", "der", "M\u00fchn", "schien", "ihm", "nur", "ei\u00b7nes", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch ein Werk, von ihm vollbracht,", "tokens": ["Durch", "ein", "Werk", ",", "von", "ihm", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Menschen Angedenken", "tokens": ["In", "der", "Men\u00b7schen", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Namen einzusenken,", "tokens": ["Sei\u00b7nen", "Na\u00b7men", "ein\u00b7zu\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er fernhin lichten Scheines", "tokens": ["Da\u00df", "er", "fern\u00b7hin", "lich\u00b7ten", "Schei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Strahle durch der Zeiten Nacht.", "tokens": ["Strah\u00b7le", "durch", "der", "Zei\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alpengipfel, nie erstiegen,", "tokens": ["Al\u00b7pen\u00b7gip\u00b7fel", ",", "nie", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockten ihn zu sich empor;", "tokens": ["Lock\u00b7ten", "ihn", "zu", "sich", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, kaum da\u00df er sie erklommen,", "tokens": ["Doch", ",", "kaum", "da\u00df", "er", "sie", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "H\u00f6her, morgenlichtumglommen,", "tokens": ["H\u00f6\u00b7her", ",", "mor\u00b7gen\u00b7lich\u00b7tum\u00b7glom\u00b7men", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sah er andre Firnen liegen,", "tokens": ["Sah", "er", "and\u00b7re", "Fir\u00b7nen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein Abgrund war davor.", "tokens": ["Und", "ein", "Ab\u00b7grund", "war", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aus des Abends fernsten Meeren,", "tokens": ["Aus", "des", "A\u00b7bends", "ferns\u00b7ten", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Ostens Purpursaum,", "tokens": ["Von", "des", "Os\u00b7tens", "Pur\u00b7pur\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dacht' er heim den Schatz zu bringen;", "tokens": ["Dacht'", "er", "heim", "den", "Schatz", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch vergebens war sein Ringen,", "tokens": ["Doch", "ver\u00b7ge\u00b7bens", "war", "sein", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, im Auge hei\u00dfe Z\u00e4hren,", "tokens": ["Und", ",", "im", "Au\u00b7ge", "hei\u00b7\u00dfe", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sagt er sich: Es war ein Traum.", "tokens": ["Sagt", "er", "sich", ":", "Es", "war", "ein", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bald den S\u00e4rgen seiner V\u00e4ter", "tokens": ["Bald", "den", "S\u00e4r\u00b7gen", "sei\u00b7ner", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird nun seiner eingereiht,", "tokens": ["Wird", "nun", "sei\u00b7ner", "ein\u00b7ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, wie in der Jahre Rollen", "tokens": ["Und", ",", "wie", "in", "der", "Jah\u00b7re", "Rol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eure Namen l\u00e4ngst verschollen,", "tokens": ["Eu\u00b7re", "Na\u00b7men", "l\u00e4ngst", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur um ein'ge Tage sp\u00e4ter", "tokens": ["Nur", "um", "ein'\u00b7ge", "Ta\u00b7ge", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deckt auch ihn Vergessenheit.", "tokens": ["Deckt", "auch", "ihn", "Ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aus dem altergrauen Rahmen", "tokens": ["Aus", "dem", "al\u00b7ter\u00b7grau\u00b7en", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt ihr fremd auf mich herab,", "tokens": ["Blickt", "ihr", "fremd", "auf", "mich", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ins Aug' euch mit Vertrauen", "tokens": ["Und", "ins", "Aug'", "euch", "mit", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Sohn nicht kann ich schauen;", "tokens": ["Wie", "ein", "Sohn", "nicht", "kann", "ich", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts mit euch ja als den Namen", "tokens": ["Nichts", "mit", "euch", "ja", "als", "den", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PPER", "ADV", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Teil' ich und dereinst das Grab.", "tokens": ["Teil'", "ich", "und", "de\u00b7reinst", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Still am v\u00e4terlichen Herde,", "tokens": ["Still", "am", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An die Scholle festgebannt,", "tokens": ["An", "die", "Schol\u00b7le", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebtet ihr im Kreis, dem engen,", "tokens": ["Leb\u00b7tet", "ihr", "im", "Kreis", ",", "dem", "en\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kanntet nicht das wilde Dr\u00e4ngen,", "tokens": ["Kann\u00b7tet", "nicht", "das", "wil\u00b7de", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das mich \u00fcber diese Erde", "tokens": ["Das", "mich", "\u00fc\u00b7ber", "die\u00b7se", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruhlos trieb von Land zu Land;", "tokens": ["Ruh\u00b7los", "trieb", "von", "Land", "zu", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nicht der N\u00e4chte bleiche Qualen,", "tokens": ["Nicht", "der", "N\u00e4ch\u00b7te", "blei\u00b7che", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Geist in Fieberhast", "tokens": ["Wenn", "der", "Geist", "in", "Fie\u00b7ber\u00b7hast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht ein Traumbild zu erreichen,", "tokens": ["Sucht", "ein", "Traum\u00b7bild", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es weichen sieht und weichen,", "tokens": ["Doch", "es", "wei\u00b7chen", "sieht", "und", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis es in des Morgens Strahlen", "tokens": ["Bis", "es", "in", "des", "Mor\u00b7gens", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Meteor erbla\u00dft.", "tokens": ["Wie", "ein", "Me\u00b7te\u00b7or", "er\u00b7bla\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ob des Enkels Thun und Trachten", "tokens": ["Ob", "des", "En\u00b7kels", "Thun", "und", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fctteln seh' ich euch das Haupt;", "tokens": ["Sch\u00fct\u00b7teln", "seh'", "ich", "euch", "das", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch schon hat es ihn inmitten", "tokens": ["Fr\u00fch", "schon", "hat", "es", "ihn", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Verwandten nicht gelitten;", "tokens": ["Der", "Ver\u00b7wand\u00b7ten", "nicht", "ge\u00b7lit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht gedacht, so wie sie dachten,", "tokens": ["Nicht", "ge\u00b7dacht", ",", "so", "wie", "sie", "dach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat er, noch wie sie geglaubt.", "tokens": ["Hat", "er", ",", "noch", "wie", "sie", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wert der M\u00fchn schien ihm nur eines \u2013", "tokens": ["Wert", "der", "M\u00fchn", "schien", "ihm", "nur", "ei\u00b7nes", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch ein Werk, von ihm vollbracht,", "tokens": ["Durch", "ein", "Werk", ",", "von", "ihm", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Menschen Angedenken", "tokens": ["In", "der", "Men\u00b7schen", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Namen einzusenken,", "tokens": ["Sei\u00b7nen", "Na\u00b7men", "ein\u00b7zu\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er fernhin lichten Scheines", "tokens": ["Da\u00df", "er", "fern\u00b7hin", "lich\u00b7ten", "Schei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Strahle durch der Zeiten Nacht.", "tokens": ["Strah\u00b7le", "durch", "der", "Zei\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Alpengipfel, nie erstiegen,", "tokens": ["Al\u00b7pen\u00b7gip\u00b7fel", ",", "nie", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockten ihn zu sich empor;", "tokens": ["Lock\u00b7ten", "ihn", "zu", "sich", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, kaum da\u00df er sie erklommen,", "tokens": ["Doch", ",", "kaum", "da\u00df", "er", "sie", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "H\u00f6her, morgenlichtumglommen,", "tokens": ["H\u00f6\u00b7her", ",", "mor\u00b7gen\u00b7lich\u00b7tum\u00b7glom\u00b7men", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sah er andre Firnen liegen,", "tokens": ["Sah", "er", "and\u00b7re", "Fir\u00b7nen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein Abgrund war davor.", "tokens": ["Und", "ein", "Ab\u00b7grund", "war", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Aus des Abends fernsten Meeren,", "tokens": ["Aus", "des", "A\u00b7bends", "ferns\u00b7ten", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Ostens Purpursaum,", "tokens": ["Von", "des", "Os\u00b7tens", "Pur\u00b7pur\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dacht' er heim den Schatz zu bringen;", "tokens": ["Dacht'", "er", "heim", "den", "Schatz", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch vergebens war sein Ringen,", "tokens": ["Doch", "ver\u00b7ge\u00b7bens", "war", "sein", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, im Auge hei\u00dfe Z\u00e4hren,", "tokens": ["Und", ",", "im", "Au\u00b7ge", "hei\u00b7\u00dfe", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sagt er sich: Es war ein Traum.", "tokens": ["Sagt", "er", "sich", ":", "Es", "war", "ein", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Bald den S\u00e4rgen seiner V\u00e4ter", "tokens": ["Bald", "den", "S\u00e4r\u00b7gen", "sei\u00b7ner", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird nun seiner eingereiht,", "tokens": ["Wird", "nun", "sei\u00b7ner", "ein\u00b7ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, wie in der Jahre Rollen", "tokens": ["Und", ",", "wie", "in", "der", "Jah\u00b7re", "Rol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eure Namen l\u00e4ngst verschollen,", "tokens": ["Eu\u00b7re", "Na\u00b7men", "l\u00e4ngst", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur um ein'ge Tage sp\u00e4ter", "tokens": ["Nur", "um", "ein'\u00b7ge", "Ta\u00b7ge", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deckt auch ihn Vergessenheit.", "tokens": ["Deckt", "auch", "ihn", "Ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}