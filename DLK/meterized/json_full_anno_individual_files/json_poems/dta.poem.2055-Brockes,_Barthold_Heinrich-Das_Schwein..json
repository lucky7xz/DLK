{"dta.poem.2055": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Schwein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun wird auch zu gleicher Absicht das so zahm' als", "tokens": ["Nun", "wird", "auch", "zu", "glei\u00b7cher", "Ab\u00b7sicht", "das", "so", "zahm'", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ADJA", "NN", "ART", "ADV", "ADJD", "KOKOM"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "wilde Schwein,", "tokens": ["wil\u00b7de", "Schwein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ein dem Menschen n\u00fctzlichs Thier, billig zu betrachten", "tokens": ["Ein", "dem", "Men\u00b7schen", "n\u00fctz\u00b7lichs", "Thier", ",", "bil\u00b7lig", "zu", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "$,", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Es hat eine spitze Schnauze, kurzen Hals, gespaltne Klauen,", "tokens": ["Es", "hat", "ei\u00b7ne", "spit\u00b7ze", "Schnau\u00b7ze", ",", "kur\u00b7zen", "Hals", ",", "ge\u00b7spalt\u00b7ne", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Einen R\u00fc\u00dfel, niedre Beine, starke Borsten, dicke Haut,", "tokens": ["Ei\u00b7nen", "R\u00fc\u00b7\u00dfel", ",", "nied\u00b7re", "Bei\u00b7ne", ",", "star\u00b7ke", "Bors\u00b7ten", ",", "di\u00b7cke", "Haut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Waffen, womit oft, die wilden sonderlich, gewaltig hauen.", "tokens": ["Waf\u00b7fen", ",", "wo\u00b7mit", "oft", ",", "die", "wil\u00b7den", "son\u00b7der\u00b7lich", ",", "ge\u00b7wal\u00b7tig", "hau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "$,", "ART", "ADJA", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Es ist leicht zu unterhalten. Alles fri\u00dft es, Fr\u00fcchte, Kraut,", "tokens": ["Es", "ist", "leicht", "zu", "un\u00b7ter\u00b7hal\u00b7ten", ".", "Al\u00b7les", "fri\u00dft", "es", ",", "Fr\u00fcch\u00b7te", ",", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKZU", "VVINF", "$.", "PIS", "VVFIN", "PPER", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Eicheln, B\u00fcchen, Sp\u00fclicht, Bohnen, Wurzeln, Treber,", "tokens": ["Ei\u00b7cheln", ",", "B\u00fc\u00b7chen", ",", "Sp\u00fc\u00b7licht", ",", "Boh\u00b7nen", ",", "Wur\u00b7zeln", ",", "Tre\u00b7ber", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "ja was man", "tokens": ["ja", "was", "man"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PWS", "PIS"], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "In der Wirthschaft von dem Abfall sonst fast gar nicht", "tokens": ["In", "der", "Wirth\u00b7schaft", "von", "dem", "Ab\u00b7fall", "sonst", "fast", "gar", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "ADV", "ADV", "ADV", "PTKNEG"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "brauchen kann.", "tokens": ["brau\u00b7chen", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Es ist dieses Thier so fruchtbar, da\u00df es oft in einem Jahr", "tokens": ["Es", "ist", "die\u00b7ses", "Thier", "so", "frucht\u00b7bar", ",", "da\u00df", "es", "oft", "in", "ei\u00b7nem", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Zweymal ferkelt, und zur Zeit wohl auf achtzehn Junge", "tokens": ["Zwey\u00b7mal", "fer\u00b7kelt", ",", "und", "zur", "Zeit", "wohl", "auf", "acht\u00b7zehn", "Jun\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "APPRART", "NN", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "bringet,", "tokens": ["brin\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Wodurch denn in unsrer Wirthschaft mannichfacher Nutz", "tokens": ["Wo\u00b7durch", "denn", "in", "uns\u00b7rer", "Wirth\u00b7schaft", "man\u00b7nich\u00b7fa\u00b7cher", "Nutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN", "PIAT", "NN"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "entspringet.", "tokens": ["ent\u00b7sprin\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Speck und Fleisch, der Kopf, die Ohren, W\u00fcrste, Schin-", "tokens": ["Speck", "und", "Fleisch", ",", "der", "Kopf", ",", "die", "Oh\u00b7ren", ",", "W\u00fcrs\u00b7te", ",", "Schin"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "ken, roh und gar,", "tokens": ["ken", ",", "roh", "und", "gar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Auch der R\u00fc\u00dfel, Zungen, F\u00fc\u00dfe liefern uns manch sch\u00f6n", "tokens": ["Auch", "der", "R\u00fc\u00b7\u00dfel", ",", "Zun\u00b7gen", ",", "F\u00fc\u00b7\u00dfe", "lie\u00b7fern", "uns", "manch", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "NN", "$,", "NN", "VVFIN", "PPER", "PIAT", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "Gericht;", "tokens": ["Ge\u00b7richt", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Und es fehlt in Arzeneyen auch an manchem Nutzen nicht.", "tokens": ["Und", "es", "fehlt", "in", "Ar\u00b7ze\u00b7ne\u00b7yen", "auch", "an", "man\u00b7chem", "Nut\u00b7zen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "ADV", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Haut und Borsten dienen uns. Ja was geben uns im", "tokens": ["Haut", "und", "Bors\u00b7ten", "die\u00b7nen", "uns", ".", "Ja", "was", "ge\u00b7ben", "uns", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "$.", "PTKANT", "PWS", "VVFIN", "PPER", "APPRART"], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Jagen", "tokens": ["Ja\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Auch die wilden Schweine nicht f\u00fcr Ergetzen und Be-", "tokens": ["Auch", "die", "wil\u00b7den", "Schwei\u00b7ne", "nicht", "f\u00fcr", "Er\u00b7get\u00b7zen", "und", "Be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKNEG", "APPR", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.26": {"text": "hagen!", "tokens": ["ha\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "So gestehe denn ein jeder, voll Erkenntlichkeit, mit mir,", "tokens": ["So", "ge\u00b7ste\u00b7he", "denn", "ein", "je\u00b7der", ",", "voll", "Er\u00b7kennt\u00b7lich\u00b7keit", ",", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "PIS", "$,", "ADJD", "NN", "$,", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "So von wild- als zahmen Schweinen, es sey ein sehr nutz-", "tokens": ["So", "von", "wild", "als", "zah\u00b7men", "Schwei\u00b7nen", ",", "es", "sey", "ein", "sehr", "nutz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "TRUNC", "KON", "ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "ADV", "TRUNC"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "bar Thier,", "tokens": ["bar", "Thier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Und erheb\u2019 und ehr\u2019 und preise den, der sie uns schenkt,", "tokens": ["Und", "er\u00b7heb'", "und", "ehr'", "und", "prei\u00b7se", "den", ",", "der", "sie", "uns", "schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "ART", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.31": {"text": "daf\u00fcr!", "tokens": ["da\u00b7f\u00fcr", "!"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}}}}}