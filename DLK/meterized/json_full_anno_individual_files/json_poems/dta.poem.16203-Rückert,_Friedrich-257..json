{"dta.poem.16203": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "257.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein gutes Werkzeug braucht zur Arbeit ein Arbeiter,", "tokens": ["Ein", "gu\u00b7tes", "Werk\u00b7zeug", "braucht", "zur", "Ar\u00b7beit", "ein", "Ar\u00b7bei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gute Waffen auch zum Waffenstreit ein Streiter.", "tokens": ["Und", "gu\u00b7te", "Waf\u00b7fen", "auch", "zum", "Waf\u00b7fen\u00b7streit", "ein", "Strei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du Streiter Gottes und Arbeiter, merk's, o Geist,", "tokens": ["Du", "Strei\u00b7ter", "Got\u00b7tes", "und", "Ar\u00b7bei\u00b7ter", ",", "merk's", ",", "o", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "KON", "NN", "$,", "NE", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df deines eignen Leibs du nicht unachtsam seist.", "tokens": ["Da\u00df", "dei\u00b7nes", "eig\u00b7nen", "Leibs", "du", "nicht", "u\u00b7nacht\u00b7sam", "seist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist dein Arbeitszeug, das ist dein Streitgewaffen;", "tokens": ["Das", "ist", "dein", "Ar\u00b7beits\u00b7zeug", ",", "das", "ist", "dein", "Streit\u00b7ge\u00b7waf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das halte wohl in Stand, zu streiten und zu schaffen!", "tokens": ["Das", "hal\u00b7te", "wohl", "in", "Stand", ",", "zu", "strei\u00b7ten", "und", "zu", "schaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O wie du dich beth\u00f6rst, wenn du den Leib zerst\u00f6rst,", "tokens": ["O", "wie", "du", "dich", "be\u00b7th\u00f6rst", ",", "wenn", "du", "den", "Leib", "zer\u00b7st\u00f6rst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "PRF", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dir so angeh\u00f6rt, wie du Gott angeh\u00f6rst.", "tokens": ["Der", "dir", "so", "an\u00b7ge\u00b7h\u00f6rt", ",", "wie", "du", "Gott", "an\u00b7ge\u00b7h\u00f6rst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,", "PWAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie du Gott angeh\u00f6rst, geh\u00f6rt dein Leib dir an,", "tokens": ["Wie", "du", "Gott", "an\u00b7ge\u00b7h\u00f6rst", ",", "ge\u00b7h\u00f6rt", "dein", "Leib", "dir", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne deinen Leib bist du kein Gottesmann.", "tokens": ["Und", "oh\u00b7ne", "dei\u00b7nen", "Leib", "bist", "du", "kein", "Got\u00b7tes\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}