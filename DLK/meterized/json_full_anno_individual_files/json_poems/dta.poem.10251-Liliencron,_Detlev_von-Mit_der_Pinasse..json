{"dta.poem.10251": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Mit der Pinasse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "M&#228;dchen, reich' mir deine H&#228;nde,             ", "tokens": ["M", "&#228;", "dchen", ",", "reich'", "mir", "dei\u00b7ne", "H", "&#228;", "nde", ","], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "$(", "VVINF", "$,", "VVFIN", "PPER", "PPOSAT", "XY", "$(", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Spring ins Boot, nicht zu behende,", "tokens": ["Spring", "ins", "Boot", ",", "nicht", "zu", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PTKNEG", "PTKZU", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6s das Tau vom Bohlenring.", "tokens": ["L\u00f6s", "das", "Tau", "vom", "Boh\u00b7len\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber kleine Wellenh\u00fcgel", "tokens": ["\u00dc\u00b7ber", "klei\u00b7ne", "Wel\u00b7len\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tanzen unsre Segelfl\u00fcgel", "tokens": ["Tan\u00b7zen", "uns\u00b7re", "Se\u00b7gel\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der wei\u00dfe Schmetterling.", "tokens": ["Wie", "der", "wei\u00b7\u00dfe", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lustig Liebesabenteuer,", "tokens": ["Lus\u00b7tig", "Lie\u00b7be\u00b7sa\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich und du allein am Steuer,", "tokens": ["Ich", "und", "du", "al\u00b7lein", "am", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weite Wassereinsamkeit.", "tokens": ["Wei\u00b7te", "Was\u00b7ser\u00b7ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Letztes Ufer im Verblassen,", "tokens": ["Letz\u00b7tes", "U\u00b7fer", "im", "Ver\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoch am Maste der Pinassen", "tokens": ["Hoch", "am", "Mas\u00b7te", "der", "Pi\u00b7nas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wimpelt die Verschwiegenheit.", "tokens": ["Wim\u00b7pelt", "die", "Ver\u00b7schwie\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Bretter pl\u00f6tzlich krachen,", "tokens": ["Wenn", "die", "Bret\u00b7ter", "pl\u00f6tz\u00b7lich", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Tiefe taucht der Nachen,", "tokens": ["In", "die", "Tie\u00b7fe", "taucht", "der", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah es nur der wilde Schwan.", "tokens": ["Sah", "es", "nur", "der", "wil\u00b7de", "Schwan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klopft dein Herzchen? La\u00df uns wenden", "tokens": ["Klopft", "dein", "Herz\u00b7chen", "?", "La\u00df", "uns", "wen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die stille Fahrt beenden,", "tokens": ["Und", "die", "stil\u00b7le", "Fahrt", "be\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald am Herde spr\u00fcht dein Span.", "tokens": ["Bald", "am", "Her\u00b7de", "spr\u00fcht", "dein", "Span", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}