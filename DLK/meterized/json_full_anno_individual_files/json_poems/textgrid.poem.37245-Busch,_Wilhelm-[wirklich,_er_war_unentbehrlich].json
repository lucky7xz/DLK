{"textgrid.poem.37245": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[wirklich, er war unentbehrlich]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wirklich, er war unentbehrlich!", "tokens": ["Wirk\u00b7lich", ",", "er", "war", "un\u00b7ent\u00b7behr\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberall, wo was geschah", "tokens": ["\u00dc\u00b7be\u00b7rall", ",", "wo", "was", "ge\u00b7schah"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PWS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Wohle der Gemeinde,", "tokens": ["Zu", "dem", "Woh\u00b7le", "der", "Ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er war t\u00e4tig, er war da.", "tokens": ["Er", "war", "t\u00e4\u00b7tig", ",", "er", "war", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Sch\u00fctzenfest, Kasinob\u00e4lle,", "tokens": ["Sch\u00fct\u00b7zen\u00b7fest", ",", "Ka\u00b7si\u00b7no\u00b7b\u00e4l\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pferderennen, Preisgericht,", "tokens": ["Pfer\u00b7de\u00b7ren\u00b7nen", ",", "Preis\u00b7ge\u00b7richt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liedertafel, Spritzenprobe,", "tokens": ["Lie\u00b7der\u00b7ta\u00b7fel", ",", "Sprit\u00b7zen\u00b7pro\u00b7be", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne ihn da ging es nicht.", "tokens": ["Oh\u00b7ne", "ihn", "da", "ging", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ohne ihn war nichts zu machen,", "tokens": ["Oh\u00b7ne", "ihn", "war", "nichts", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Stunde hatt er frei.", "tokens": ["Kei\u00b7ne", "Stun\u00b7de", "hatt", "er", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gestern, als sie ihn begruben,", "tokens": ["Ge\u00b7stern", ",", "als", "sie", "ihn", "be\u00b7gru\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "War er richtig auch dabei.", "tokens": ["War", "er", "rich\u00b7tig", "auch", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wirklich, er war unentbehrlich!", "tokens": ["Wirk\u00b7lich"