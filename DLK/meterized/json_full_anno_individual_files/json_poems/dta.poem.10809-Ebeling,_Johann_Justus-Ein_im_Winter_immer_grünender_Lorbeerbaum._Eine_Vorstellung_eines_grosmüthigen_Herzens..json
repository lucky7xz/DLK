{"dta.poem.10809": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ein  \n im Winter immer gr\u00fcnender  \n Lorbeerbaum.  \n Eine Vorstellung eines grosm\u00fcthigen  \n Herzens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein gr\u00fcner Lorbeerbaum ist auch im\nWinter sch\u00f6n,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Lor\u00b7beer\u00b7baum", "ist", "auch", "im", "Win\u00b7ter", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gedoppelt lieblicher im Schnee-Puz", "tokens": ["Ge\u00b7dop\u00b7pelt", "lieb\u00b7li\u00b7cher", "im", "Schnee\u00b7Puz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel tobe gleich im Sturm", "tokens": ["Der", "Him\u00b7mel", "to\u00b7be", "gleich", "im", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er prangt in seinem Schmuk, bewahret seine Bl\u00e4t-", "tokens": ["Er", "prangt", "in", "sei\u00b7nem", "Schmuk", ",", "be\u00b7wah\u00b7ret", "sei\u00b7ne", "Bl\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn andre B\u00e4ume kahl, und ihres Schmuks", "tokens": ["Wenn", "and\u00b7re", "B\u00e4u\u00b7me", "kahl", ",", "und", "ih\u00b7res", "Schmuks"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie der kalte Nord durch seinem Braus ent-", "tokens": ["Wenn", "sie", "der", "kal\u00b7te", "Nord", "durch", "sei\u00b7nem", "Braus", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So sezt er sich den Sturm und sch\u00fctternden Be-", "tokens": ["So", "sezt", "er", "sich", "den", "Sturm", "und", "sch\u00fct\u00b7tern\u00b7den", "Be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Zum Troz der rauhen Luft, mit aller Macht entgegen.", "tokens": ["Zum", "Troz", "der", "rau\u00b7hen", "Luft", ",", "mit", "al\u00b7ler", "Macht", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Grosmuth sieht darin ein sch\u00f6nes Bildnis an,", "tokens": ["Die", "Gros\u00b7muth", "sieht", "da\u00b7rin", "ein", "sch\u00f6\u00b7nes", "Bild\u00b7nis", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das ihren starken Geist im Gleichnis schildern", "tokens": ["Das", "ih\u00b7ren", "star\u00b7ken", "Geist", "im", "Gleich\u00b7nis", "schil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So wie ein Lorbeerbaum, wenn es gleich friert und", "tokens": ["So", "wie", "ein", "Lor\u00b7beer\u00b7baum", ",", "wenn", "es", "gleich", "friert", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "KON"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Von gr\u00fcnen Laub bekr\u00e4nzt, den rauhen Frost nicht", "tokens": ["Von", "gr\u00fc\u00b7nen", "Laub", "be\u00b7kr\u00e4nzt", ",", "den", "rau\u00b7hen", "Frost", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So scheut die Grosmuth nicht, wenn gleich ein tr\u00fc-", "tokens": ["So", "scheut", "die", "Gros\u00b7muth", "nicht", ",", "wenn", "gleich", "ein", "tr\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "$,", "KOUS", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Den Leib mit Wehen plagt, den Geist mit Unge-", "tokens": ["Den", "Leib", "mit", "We\u00b7hen", "plagt", ",", "den", "Geist", "mit", "Un\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Sie h\u00e4lt das gr\u00fcne Blat, der Hofnung Sinnbild", "tokens": ["Sie", "h\u00e4lt", "das", "gr\u00fc\u00b7ne", "Blat", ",", "der", "Hof\u00b7nung", "Sinn\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und denkt nach rauhen Nord, kommt wol des Gl\u00fck-", "tokens": ["Und", "denkt", "nach", "rau\u00b7hen", "Nord", ",", "kommt", "wol", "des", "Gl\u00fc\u00b7k"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da alle Noth zerschmelzt, die unser Herze qu\u00e4lt,", "tokens": ["Da", "al\u00b7le", "Noth", "zer\u00b7schmelzt", ",", "die", "un\u00b7ser", "Her\u00b7ze", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Himmel denkt und wei\u00df, was ihren Kindern", "tokens": ["Der", "Him\u00b7mel", "denkt", "und", "wei\u00df", ",", "was", "ih\u00b7ren", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Es kan der Lorbeerbaum, so lang er gr\u00fcnt bestehen,", "tokens": ["Es", "kan", "der", "Lor\u00b7beer\u00b7baum", ",", "so", "lang", "er", "gr\u00fcnt", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch falln die Bl\u00e4tter hin; so ists mit ihm gesche-", "tokens": ["Doch", "falln", "die", "Bl\u00e4t\u00b7ter", "hin", ";", "so", "ists", "mit", "ihm", "ge\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Die Grosmuth bleibet auch, so lang noch Hofnung", "tokens": ["Die", "Gros\u00b7muth", "blei\u00b7bet", "auch", ",", "so", "lang", "noch", "Hof\u00b7nung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "ADJD", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Doch wenn die Hofnung fehlt; so ist ihr Ende nah:", "tokens": ["Doch", "wenn", "die", "Hof\u00b7nung", "fehlt", ";", "so", "ist", "ihr", "En\u00b7de", "nah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer standhafft bleiben wil, der mu\u00df durch weise", "tokens": ["Wer", "stand\u00b7hafft", "blei\u00b7ben", "wil", ",", "der", "mu\u00df", "durch", "wei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "VVINF", "VMFIN", "$,", "ART", "VMFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die Hofnung immerfort in seiner Seele n\u00e4h-", "tokens": ["Die", "Hof\u00b7nung", "im\u00b7mer\u00b7fort", "in", "sei\u00b7ner", "See\u00b7le", "n\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}