{"textgrid.poem.55952": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Blaue Hortensie", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie das letzte Gr\u00fcn in Farbentiegeln", "tokens": ["So", "wie", "das", "letz\u00b7te", "Gr\u00fcn", "in", "Far\u00b7ben\u00b7tie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sind diese Bl\u00e4tter, trocken, stumpf und rauh,", "tokens": ["sind", "die\u00b7se", "Bl\u00e4t\u00b7ter", ",", "tro\u00b7cken", ",", "stumpf", "und", "rauh", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "hinter den Bl\u00fctendolden, die ein Blau", "tokens": ["hin\u00b7ter", "den", "Bl\u00fc\u00b7ten\u00b7dol\u00b7den", ",", "die", "ein", "Blau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "nicht auf sich tragen, nur von ferne spiegeln.", "tokens": ["nicht", "auf", "sich", "tra\u00b7gen", ",", "nur", "von", "fer\u00b7ne", "spie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PRF", "VVINF", "$,", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie spiegeln es verweint und ungenau,", "tokens": ["Sie", "spie\u00b7geln", "es", "ver\u00b7weint", "und", "un\u00b7ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als wollten sie es wiederum verlieren,", "tokens": ["als", "woll\u00b7ten", "sie", "es", "wie\u00b7de\u00b7rum", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und wie in alten blauen Briefpapieren", "tokens": ["und", "wie", "in", "al\u00b7ten", "blau\u00b7en", "Brief\u00b7pa\u00b7pie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ist Gelb in ihnen, Violett und Grau;", "tokens": ["ist", "Gelb", "in", "ih\u00b7nen", ",", "Vi\u00b7o\u00b7lett", "und", "Grau", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verwaschnes wie an einer Kindersch\u00fcrze,", "tokens": ["Ver\u00b7waschnes", "wie", "an", "ei\u00b7ner", "Kin\u00b7der\u00b7sch\u00fcr\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nichtmehrgetragnes, dem nichts mehr geschieht:", "tokens": ["Nicht\u00b7mehr\u00b7ge\u00b7trag\u00b7nes", ",", "dem", "nichts", "mehr", "ge\u00b7schieht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wie f\u00