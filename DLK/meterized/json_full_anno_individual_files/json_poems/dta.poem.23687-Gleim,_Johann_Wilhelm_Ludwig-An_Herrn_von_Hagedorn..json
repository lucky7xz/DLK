{"dta.poem.23687": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn von Hagedorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dichter, du bist Amors Liebling!", "tokens": ["Dich\u00b7ter", ",", "du", "bist", "A\u00b7mors", "Lieb\u00b7ling", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du Liebeslieder singest,", "tokens": ["Wenn", "du", "Lie\u00b7bes\u00b7lie\u00b7der", "sin\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmt er schnell den Pfeil vom Bogen", "tokens": ["Nimmt", "er", "schnell", "den", "Pfeil", "vom", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehnt sich m\u00fc\u00dfig an die Mutter,", "tokens": ["Lehnt", "sich", "m\u00fc\u00b7\u00dfig", "an", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn ihn die Mutter fr\u00e4get:", "tokens": ["Und", "wenn", "ihn", "die", "Mut\u00b7ter", "fr\u00e4\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Sohn, bewegst du nicht zur Liebe?", "tokens": ["Sohn", ",", "be\u00b7wegst", "du", "nicht", "zur", "Lie\u00b7be", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Spricht er: Liebste Mutter, horche!", "tokens": ["Spricht", "er", ":", "Liebs\u00b7te", "Mut\u00b7ter", ",", "hor\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hagedorn bewegt zur Liebe!", "tokens": ["Ha\u00b7ge\u00b7dorn", "be\u00b7wegt", "zur", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hilf mir nur die Spr\u00f6den z\u00e4len,", "tokens": ["Hilf", "mir", "nur", "die", "Spr\u00f6\u00b7den", "z\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Z\u00e4le nur die Neuverliebten,", "tokens": ["Z\u00e4\u00b7le", "nur", "die", "Neu\u00b7ver\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die er schon dazu bewogen!", "tokens": ["Die", "er", "schon", "da\u00b7zu", "be\u00b7wo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dichter, du bist Amors Liebling!", "tokens": ["Dich\u00b7ter", ",", "du", "bist", "A\u00b7mors", "Lieb\u00b7ling", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wenn du mit den Sch\u00f6nen scherzest,", "tokens": ["Wenn", "du", "mit", "den", "Sch\u00f6\u00b7nen", "scher\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Schimpft er auf die Possenreisser.", "tokens": ["Schimpft", "er", "auf", "die", "Pos\u00b7sen\u00b7reis\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "O wie hat er dich gepriesen,", "tokens": ["O", "wie", "hat", "er", "dich", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Als dich i\u00fcngst der Weingott lobte!", "tokens": ["Als", "dich", "i\u00b7\u00fcngst", "der", "Wein\u00b7gott", "lob\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Voll von Eifer dich zu loben,", "tokens": ["Voll", "von", "Ei\u00b7fer", "dich", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Zankten sich die frohen G\u00f6tter.", "tokens": ["Zank\u00b7ten", "sich", "die", "fro\u00b7hen", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Amor sang mit zarten Trillern,", "tokens": ["A\u00b7mor", "sang", "mit", "zar\u00b7ten", "Tril\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Eins von deinen Liebesliedern;", "tokens": ["Eins", "von", "dei\u00b7nen", "Lie\u00b7bes\u00b7lie\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Pl\u00f6tzlich liebten alle Sch\u00f6nen.", "tokens": ["Pl\u00f6tz\u00b7lich", "lieb\u00b7ten", "al\u00b7le", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Bacchus lie\u00df ein Trinklied h\u00f6ren;", "tokens": ["Bac\u00b7chus", "lie\u00df", "ein", "Trin\u00b7klied", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Pl\u00f6tzlich wolten alle trinken.", "tokens": ["Pl\u00f6tz\u00b7lich", "wol\u00b7ten", "al\u00b7le", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}