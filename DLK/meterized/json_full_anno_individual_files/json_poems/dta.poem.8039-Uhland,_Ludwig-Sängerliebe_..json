{"dta.poem.8039": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4ngerliebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Seit der hohe Gott der Lieder", "tokens": ["Seit", "der", "ho\u00b7he", "Gott", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft\u2019 in Liebesschmerz erbleichen,", "tokens": ["Mu\u00dft'", "in", "Lie\u00b7bes\u00b7schmerz", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit der Lorbeer seiner Schl\u00e4fe", "tokens": ["Seit", "der", "Lor\u00b7beer", "sei\u00b7ner", "Schl\u00e4\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ungl\u00fccksel\u2019ger Liebe Zeichen:", "tokens": ["Un\u00b7gl\u00fcck\u00b7sel'\u00b7ger", "Lie\u00b7be", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wundert\u2019s wen da\u00df ird\u2019schen S\u00e4ngern,", "tokens": ["Wun\u00b7dert's", "wen", "da\u00df", "ird'\u00b7schen", "S\u00e4n\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dasselbe Zeichen kranzet,", "tokens": ["Die", "das\u00b7sel\u00b7be", "Zei\u00b7chen", "kran\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Selten in der Liebe Leben", "tokens": ["Sel\u00b7ten", "in", "der", "Lie\u00b7be", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein begl\u00fcckter Stern ergl\u00e4nzet?", "tokens": ["Ein", "be\u00b7gl\u00fcck\u00b7ter", "Stern", "er\u00b7gl\u00e4n\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df sie ernst und d\u00fcster blicken,", "tokens": ["Da\u00df", "sie", "ernst", "und", "d\u00fcs\u00b7ter", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihre Saiten traurig t\u00f6nen,", "tokens": ["Ih\u00b7re", "Sai\u00b7ten", "trau\u00b7rig", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df von Lust sie wenig singen,", "tokens": ["Da\u00df", "von", "Lust", "sie", "we\u00b7nig", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aber viel von Schmerz und Sehnen?", "tokens": ["A\u00b7ber", "viel", "von", "Schmerz", "und", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "S\u00e4ngerliebe, tief und schmerzlich,", "tokens": ["S\u00e4n\u00b7ger\u00b7lie\u00b7be", ",", "tief", "und", "schmerz\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "La\u00dft euch denn in ernsten Bildern", "tokens": ["La\u00dft", "euch", "denn", "in", "erns\u00b7ten", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Aus den Tagen des Gesanges,", "tokens": ["Aus", "den", "Ta\u00b7gen", "des", "Ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Aus der Zeit der Minne, schildern!", "tokens": ["Aus", "der", "Zeit", "der", "Min\u00b7ne", ",", "schil\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}