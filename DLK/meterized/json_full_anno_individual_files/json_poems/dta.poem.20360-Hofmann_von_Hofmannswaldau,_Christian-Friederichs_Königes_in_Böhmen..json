{"dta.poem.20360": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Friederichs/ K\u00f6niges in  \n B\u00f6hmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich hat die Venus mehr als Jupiter gekennt/", "tokens": ["Mich", "hat", "die", "Ve\u00b7nus", "mehr", "als", "Ju\u00b7pi\u00b7ter", "ge\u00b7kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die flamme kommt von mir/ so noch in Deutschland brennt.", "tokens": ["Die", "flam\u00b7me", "kommt", "von", "mir", "/", "so", "noch", "in", "Deutschland", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "APPR", "PPER", "$(", "ADV", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach da\u00df mein junger sohn doch nicht erfahren h\u00e4tte/", "tokens": ["Ach", "da\u00df", "mein", "jun\u00b7ger", "sohn", "doch", "nicht", "er\u00b7fah\u00b7ren", "h\u00e4t\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PTKNEG", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df h\u00f6her ist ein thron/ als der gemahlin bette.", "tokens": ["Da\u00df", "h\u00f6\u00b7her", "ist", "ein", "thron", "/", "als", "der", "ge\u00b7mah\u00b7lin", "bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$(", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}