{"textgrid.poem.61441": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Elysium", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heitre N\u00e4chte, heitre Tage", "tokens": ["Heit\u00b7re", "N\u00e4ch\u00b7te", ",", "heit\u00b7re", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feiert der Erw\u00e4hlten Schar", "tokens": ["Fei\u00b7ert", "der", "Er\u00b7w\u00e4hl\u00b7ten", "Schar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "In Elysiums duft'gem Hage,", "tokens": ["In", "E\u00b7ly\u00b7si\u00b7ums", "duft'\u00b7gem", "Ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo Musik die L\u00fcfte hauchen,", "tokens": ["Wo", "Mu\u00b7sik", "die", "L\u00fcf\u00b7te", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und aus Wassern, spiegelklar,", "tokens": ["Und", "aus", "Was\u00b7sern", ",", "spie\u00b7gel\u00b7klar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Goldne Blumen tauchen.", "tokens": ["Gold\u00b7ne", "Blu\u00b7men", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "O wie l\u00f6st sich hier das Trauern!", "tokens": ["O", "wie", "l\u00f6st", "sich", "hier", "das", "Trau\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie stirbt in Lebensschauern", "tokens": ["O", "wie", "stirbt", "in", "Le\u00b7bens\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00df dahin des Siechtums Leid!", "tokens": ["S\u00fc\u00df", "da\u00b7hin", "des", "Siech\u00b7tums", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig jugendliche Glieder", "tokens": ["E\u00b7wig", "ju\u00b7gend\u00b7li\u00b7che", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind hinfort der Seele Kleid,", "tokens": ["Sind", "hin\u00b7fort", "der", "See\u00b7le", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leicht wie Schwangefieder.", "tokens": ["Leicht", "wie", "Schwan\u00b7ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wer vom Lethe getrunken,", "tokens": ["Wer", "vom", "Le\u00b7the", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "lin