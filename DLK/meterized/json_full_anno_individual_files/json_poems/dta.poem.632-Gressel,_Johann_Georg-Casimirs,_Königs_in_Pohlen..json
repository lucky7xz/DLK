{"dta.poem.632": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Casimirs,   K\u00f6nigs in Pohlen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Haupt hat eine Kron ver\u00e4chtlich abgelegt/", "tokens": ["Mein", "Haupt", "hat", "ei\u00b7ne", "Kron", "ver\u00b7\u00e4cht\u00b7lich", "ab\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer dieses rohte Gold auf seinen Wirbel tr\u00e4gt", "tokens": ["Wer", "die\u00b7ses", "roh\u00b7te", "Gold", "auf", "sei\u00b7nen", "Wir\u00b7bel", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mu\u00df stets in Sorgen stehn/ und sich mit Kummer kr\u00e4ncken/", "tokens": ["Mu\u00df", "stets", "in", "Sor\u00b7gen", "stehn", "/", "und", "sich", "mit", "Kum\u00b7mer", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVINF", "$(", "KON", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn Kronen k\u00f6nnen nichts als ", "tokens": ["Denn", "Kro\u00b7nen", "k\u00f6n\u00b7nen", "nichts", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "PIS", "KOKOM"], "meter": "-+-+-+-", "meas