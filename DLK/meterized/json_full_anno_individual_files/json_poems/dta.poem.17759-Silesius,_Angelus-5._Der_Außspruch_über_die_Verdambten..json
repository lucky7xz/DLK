{"dta.poem.17759": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "5.  Der Au\u00dfspruch \u00fcber die Verdambten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Geht ihr Verfluchten geht/ ihr Teuffels Rottgesellen/", "tokens": ["Geht", "ihr", "Ver\u00b7fluch\u00b7ten", "geht", "/", "ihr", "Teuf\u00b7fels", "Rott\u00b7ge\u00b7sel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr Raben die jhr mich nie habt getr\u00e4nkt/ gespeist/", "tokens": ["Ihr", "Ra\u00b7ben", "die", "jhr", "mich", "nie", "habt", "ge\u00b7tr\u00e4nkt", "/", "ge\u00b7speist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "PRF", "ADV", "VAFIN", "VVPP", "$(", "VVPP", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bekleidt/ besucht/ getr\u00f6ft/ noch eingen Dienst geleist:", "tokens": ["Be\u00b7kleidt", "/", "be\u00b7sucht", "/", "ge\u00b7tr\u00f6ft", "/", "noch", "ein\u00b7gen", "Dienst", "ge\u00b7leist", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVPP", "$(", "VVPP", "$(", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geht in das Ewge Feur und in den Schlund der H\u00f6llen.", "tokens": ["Geht", "in", "das", "Ew\u00b7ge", "Feur", "und", "in", "den", "Schlund", "der", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Empfahet euren Lohn in jhren grimmen Wellen/", "tokens": ["E\u00b7mpfa\u00b7het", "eu\u00b7ren", "Lohn", "in", "jhren", "grim\u00b7men", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Blitz/ Donner/ Pestilentz und alls was b\u00f6se heist.", "tokens": ["Blitz", "/", "Don\u00b7ner", "/", "Pes\u00b7ti\u00b7lentz", "und", "alls", "was", "b\u00f6\u00b7se", "heist", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "ADV", "PWS", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Gebt und bleibt ewiglich von meinem Reich verweist.", "tokens": ["Gebt", "und", "bleibt", "e\u00b7wig\u00b7lich", "von", "mei\u00b7nem", "Reich", "ver\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhr werdt nu Heuln und schreihn/ und wie die Hunde", "tokens": ["Ihr", "werdt", "nu", "Heuln", "und", "schreihn", "/", "und", "wie", "die", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "VVINF", "$(", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "bellen/", "tokens": ["bel\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "In Durst unnd Hunger stehn: Eur Wurm der", "tokens": ["In", "Durst", "unnd", "Hun\u00b7ger", "stehn", ":", "Eur", "Wurm", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$.", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "stirbet nicht/", "tokens": ["stir\u00b7bet", "nicht", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Das Feuer l\u00f6scht nicht au\u00df das euch ist zugericht.", "tokens": ["Das", "Feu\u00b7er", "l\u00f6scht", "nicht", "au\u00df", "das", "euch", "ist", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr m\u00fcsset ewiglich in Peineu sein gerochen/", "tokens": ["Ihr", "m\u00fcs\u00b7set", "e\u00b7wig\u00b7lich", "in", "Pei\u00b7neu", "sein", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NE", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie jhr verdienet habt: Denn was jhr habt gethan/", "tokens": ["Wie", "jhr", "ver\u00b7die\u00b7net", "habt", ":", "Denn", "was", "jhr", "habt", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VAFIN", "$.", "KON", "PWS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Gliedern meines Leibs/ nehm ich mich selber an.", "tokens": ["Den", "Glie\u00b7dern", "mei\u00b7nes", "Leibs", "/", "nehm", "ich", "mich", "sel\u00b7ber", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Geht jhr Verfluchten geht/ das Vrtheil ist gesprochen.", "tokens": ["Geht", "jhr", "Ver\u00b7fluch\u00b7ten", "geht", "/", "das", "Vrtheil", "ist", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}