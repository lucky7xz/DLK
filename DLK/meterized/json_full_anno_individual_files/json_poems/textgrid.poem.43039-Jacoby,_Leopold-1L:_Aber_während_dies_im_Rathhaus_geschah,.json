", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.17": {"text": "Gewandter und manierlicher.", "tokens": ["Ge\u00b7wand\u00b7ter", "und", "ma\u00b7nier\u00b7li\u00b7cher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.18": {"text": "Der andre, gar hager, doch markig und sehnig,", "tokens": ["Der", "and\u00b7re", ",", "gar", "ha\u00b7ger", ",", "doch", "mar\u00b7kig", "und", "seh\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Sa\u00df auf einer Bank, l\u00e4rmt auch nicht wenig,", "tokens": ["Sa\u00df", "auf", "ei\u00b7ner", "Bank", ",", "l\u00e4rmt", "auch", "nicht", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Hat eine M\u00f6nchskapuze angethan", "tokens": ["Hat", "ei\u00b7ne", "M\u00f6nchs\u00b7ka\u00b7pu\u00b7ze", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und Narrenschellen darangethan,", "tokens": ["Und", "Nar\u00b7ren\u00b7schel\u00b7len", "da\u00b7ran\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Die lie\u00df er klingen mit Jauchzen und Johlen", "tokens": ["Die", "lie\u00df", "er", "klin\u00b7gen", "mit", "Jauch\u00b7zen", "und", "Joh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und schnitt dazu n\u00e4rrische Kapriolen.", "tokens": ["Und", "schnitt", "da\u00b7zu", "n\u00e4r\u00b7ri\u00b7sche", "Kap\u00b7ri\u00b7o\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Der erste nahm von der Wand eine Laute,", "tokens": ["Der", "ers\u00b7te", "nahm", "von", "der", "Wand", "ei\u00b7ne", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Der er allerhand Singsang anvertraute;", "tokens": ["Der", "er", "al\u00b7ler\u00b7hand", "Sings\u00b7ang", "an\u00b7ver\u00b7trau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "--+-++-+-+-", "measure": "anapaest.init"}, "line.26": {"text": "Dann rief er zum zweiten: Halt einmal Ruh,", "tokens": ["Dann", "rief", "er", "zum", "zwei\u00b7ten", ":", "Halt", "ein\u00b7mal", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "$.", "VVIMP", "ADV", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "La\u00dft mich jetzt singen und du h\u00f6r' zu,", "tokens": ["La\u00dft", "mich", "jetzt", "sin\u00b7gen", "und", "du", "h\u00f6r'", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word