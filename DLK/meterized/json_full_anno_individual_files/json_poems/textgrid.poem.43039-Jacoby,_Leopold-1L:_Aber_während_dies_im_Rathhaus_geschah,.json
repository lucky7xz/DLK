{"textgrid.poem.43039": {"metadata": {"author": {"name": "Jacoby, Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aber w\u00e4hrend dies im Rathhaus geschah,", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber w\u00e4hrend dies im Rathhaus geschah,", "tokens": ["A\u00b7ber", "w\u00e4h\u00b7rend", "dies", "im", "Rath\u00b7haus", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da sah", "tokens": ["Da", "sah"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "In einem Zimmer der G\u00e4\u00dfchen, wirr und kraus,", "tokens": ["In", "ei\u00b7nem", "Zim\u00b7mer", "der", "G\u00e4\u00df\u00b7chen", ",", "wirr", "und", "kraus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im Erdgescho\u00df zum Hof hinaus", "tokens": ["Im", "Erd\u00b7ge\u00b7scho\u00df", "zum", "Hof", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wunderlich aus.", "tokens": ["Es", "wun\u00b7der\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "An den W\u00e4nden hingen da", "tokens": ["An", "den", "W\u00e4n\u00b7den", "hin\u00b7gen", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Instrumente der Musika", "tokens": ["Inst\u00b7ru\u00b7men\u00b7te", "der", "Mu\u00b7si\u00b7ka"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Und Tafeln standen auf Gestell'n,", "tokens": ["Und", "Ta\u00b7feln", "stan\u00b7den", "auf", "Ge\u00b7stell'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Im Zimmer aber waren zwei Gesell'n,", "tokens": ["Im", "Zim\u00b7mer", "a\u00b7ber", "wa\u00b7ren", "zwei", "Ge\u00b7sell'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die trieben mit halloh und holla", "tokens": ["Die", "trie\u00b7ben", "mit", "hal\u00b7loh", "und", "hol\u00b7la"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "XY"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Allerlei Allotria.", "tokens": ["Al\u00b7ler\u00b7lei", "Al\u00b7lo\u00b7tria", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Sie sangen unverfroren", "tokens": ["Sie", "san\u00b7gen", "un\u00b7ver\u00b7fro\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Einander Spottliedlein in die Ohren.", "tokens": ["Ein\u00b7an\u00b7der", "Spott\u00b7lied\u00b7lein", "in", "die", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Dabei \u00fcbte der eine die Fu\u00dfgelenke", "tokens": ["Da\u00b7bei", "\u00fcb\u00b7te", "der", "ei\u00b7ne", "die", "Fu\u00df\u00b7ge\u00b7len\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ART", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Und sprang beim Sang \u00fcber Schemel und B\u00e4nke.", "tokens": ["Und", "sprang", "beim", "Sang", "\u00fc\u00b7ber", "Sche\u00b7mel", "und", "B\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Es war ein h\u00fcbscher zierlicher,", "tokens": ["Es", "war", "ein", "h\u00fcb\u00b7scher", "zier\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.17": {"text": "Gewandter und manierlicher.", "tokens": ["Ge\u00b7wand\u00b7ter", "und", "ma\u00b7nier\u00b7li\u00b7cher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.18": {"text": "Der andre, gar hager, doch markig und sehnig,", "tokens": ["Der", "and\u00b7re", ",", "gar", "ha\u00b7ger", ",", "doch", "mar\u00b7kig", "und", "seh\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Sa\u00df auf einer Bank, l\u00e4rmt auch nicht wenig,", "tokens": ["Sa\u00df", "auf", "ei\u00b7ner", "Bank", ",", "l\u00e4rmt", "auch", "nicht", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Hat eine M\u00f6nchskapuze angethan", "tokens": ["Hat", "ei\u00b7ne", "M\u00f6nchs\u00b7ka\u00b7pu\u00b7ze", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und Narrenschellen darangethan,", "tokens": ["Und", "Nar\u00b7ren\u00b7schel\u00b7len", "da\u00b7ran\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Die lie\u00df er klingen mit Jauchzen und Johlen", "tokens": ["Die", "lie\u00df", "er", "klin\u00b7gen", "mit", "Jauch\u00b7zen", "und", "Joh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und schnitt dazu n\u00e4rrische Kapriolen.", "tokens": ["Und", "schnitt", "da\u00b7zu", "n\u00e4r\u00b7ri\u00b7sche", "Kap\u00b7ri\u00b7o\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Der erste nahm von der Wand eine Laute,", "tokens": ["Der", "ers\u00b7te", "nahm", "von", "der", "Wand", "ei\u00b7ne", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Der er allerhand Singsang anvertraute;", "tokens": ["Der", "er", "al\u00b7ler\u00b7hand", "Sings\u00b7ang", "an\u00b7ver\u00b7trau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "--+-++-+-+-", "measure": "anapaest.init"}, "line.26": {"text": "Dann rief er zum zweiten: Halt einmal Ruh,", "tokens": ["Dann", "rief", "er", "zum", "zwei\u00b7ten", ":", "Halt", "ein\u00b7mal", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "$.", "VVIMP", "ADV", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "La\u00dft mich jetzt singen und du h\u00f6r' zu,", "tokens": ["La\u00dft", "mich", "jetzt", "sin\u00b7gen", "und", "du", "h\u00f6r'", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Nachher wollen wir die Rollen tauschen,", "tokens": ["Nach\u00b7her", "wol\u00b7len", "wir", "die", "Rol\u00b7len", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Und du sollst singen und ich will lauschen;", "tokens": ["Und", "du", "sollst", "sin\u00b7gen", "und", "ich", "will", "lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Wer aber den andern besiegt im Gesang,", "tokens": ["Wer", "a\u00b7ber", "den", "an\u00b7dern", "be\u00b7siegt", "im", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.31": {"text": "Der soll Kaiser sein eine Stunde lang.", "tokens": ["Der", "soll", "Kai\u00b7ser", "sein", "ei\u00b7ne", "Stun\u00b7de", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPOSAT", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der zweite sprach: Ich bin's zufrieden,", "tokens": ["Der", "zwei\u00b7te", "sprach", ":", "Ich", "bin's", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fange du an, deinen Leim zu sieden.", "tokens": ["Fan\u00b7ge", "du", "an", ",", "dei\u00b7nen", "Leim", "zu", "sie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Gut, sagte der erste,", "tokens": ["Gut", ",", "sag\u00b7te", "der", "ers\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein Ohrspiel zum Vorspiel,", "tokens": ["Ein", "Ohr\u00b7spiel", "zum", "Vor\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und zum Nachspiel ein Sprachspiel.", "tokens": ["Und", "zum", "Nach\u00b7spiel", "ein", "Sprach\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dann stimmt' er die Saiten, spielte und sang", "tokens": ["Dann", "stimmt'", "er", "die", "Sai\u00b7ten", ",", "spiel\u00b7te", "und", "sang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit lust'gem Klang,", "tokens": ["Mit", "lust'\u00b7gem", "Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und der zweite mit droll'gem Humor", "tokens": ["Und", "der", "zwei\u00b7te", "mit", "droll'\u00b7gem", "Hu\u00b7mor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Machte den Chor.", "tokens": ["Mach\u00b7te", "den", "Chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Aber w\u00e4hrend dies im Rathhaus geschah,", "tokens": ["A\u00b7ber", "w\u00e4h\u00b7rend", "dies", "im", "Rath\u00b7haus", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da sah", "tokens": ["Da", "sah"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "In einem Zimmer der G\u00e4\u00dfchen, wirr und kraus,", "tokens": ["In", "ei\u00b7nem", "Zi