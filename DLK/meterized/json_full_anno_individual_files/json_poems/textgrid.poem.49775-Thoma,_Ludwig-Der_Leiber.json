{"textgrid.poem.49775": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Leiber", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist des Kriegers seligstes Vergn\u00fcgen,", "tokens": ["Was", "ist", "des", "Krie\u00b7gers", "se\u00b7ligs\u00b7tes", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was ist des Kriegers allerh\u00f6chste Lust?", "tokens": ["Was", "ist", "des", "Krie\u00b7gers", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als wie des Nachts beim M\u00e4digen zu liegen,", "tokens": ["Als", "wie", "des", "Nachts", "beim", "M\u00e4\u00b7di\u00b7gen", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die treue Liebe in der stolzen Brust?", "tokens": ["Die", "treu\u00b7e", "Lie\u00b7be", "in", "der", "stol\u00b7zen", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja, das erfreuet jeden, der es kennt,", "tokens": ["Ja", ",", "das", "er\u00b7freu\u00b7et", "je\u00b7den", ",", "der", "es", "kennt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und auch den Tapfern \u2013 aberi juhe!", "tokens": ["Und", "auch", "den", "Tap\u00b7fern", "\u2013", "a\u00b7be\u00b7ri", "ju\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$(", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Vom Leiberregiment.", "tokens": ["Vom", "Lei\u00b7ber\u00b7re\u00b7gi\u00b7ment", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Marschieren wir des Tages auf und nieder,", "tokens": ["Mar\u00b7schie\u00b7ren", "wir", "des", "Ta\u00b7ges", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn wir stehen auf der stillen Wacht,", "tokens": ["Und", "wenn", "wir", "ste\u00b7hen", "auf", "der", "stil\u00b7len", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann weihen wir der Liebsten unsre Lieder", "tokens": ["Dann", "wei\u00b7hen", "wir", "der", "Liebs\u00b7ten", "uns\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seufzen leise: morgen auf die Nacht,", "tokens": ["Und", "seuf\u00b7zen", "lei\u00b7se", ":", "mor\u00b7gen", "auf", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja morgen komm' ich wieder hochbegl\u00fcckt,", "tokens": ["Ja", "mor\u00b7gen", "komm'", "ich", "wie\u00b7der", "hoch\u00b7be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn Liebe ist es \u2013 aberi juhe!", "tokens": ["Denn", "Lie\u00b7be", "ist", "es", "\u2013", "a\u00b7be\u00b7ri", "ju\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "$(", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die wo das Leben schm\u00fcckt.", "tokens": ["Die", "wo", "das", "Le\u00b7ben", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O la\u00df uns heimlich in die K\u00fcche gehen", "tokens": ["O", "la\u00df", "uns", "heim\u00b7lich", "in", "die", "K\u00fc\u00b7che", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dort empfangen s\u00fc\u00dfen Liebeslohn!", "tokens": ["Und", "dort", "emp\u00b7fan\u00b7gen", "s\u00fc\u00b7\u00dfen", "Lie\u00b7bes\u00b7lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du nicht etwas in dem Kasten stehen?", "tokens": ["Hast", "du", "nicht", "et\u00b7was", "in", "dem", "Kas\u00b7ten", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Sch\u00e4tzigen, das andre wei\u00dft du schon.", "tokens": ["Mein", "Sch\u00e4t\u00b7zi\u00b7gen", ",", "das", "and\u00b7re", "wei\u00dft", "du", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir wollen essen und gar fr\u00f6hlich sein,", "tokens": ["Wir", "wol\u00b7len", "es\u00b7sen", "und", "gar", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn unsre Treue \u2013 aberi juhe!", "tokens": ["Denn", "uns\u00b7re", "Treu\u00b7e", "\u2013", "a\u00b7be\u00b7ri", "ju\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das ist kein leerer Schein.", "tokens": ["Das", "ist", "kein", "lee\u00b7rer", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So leben wir Soldaten stets in Freuden,", "tokens": ["So", "le\u00b7ben", "wir", "Sol\u00b7da\u00b7ten", "stets", "in", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VMFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nichts anderes w\u00fcnschen wir ja niemals nicht.", "tokens": ["Nichts", "an\u00b7de\u00b7res", "w\u00fcn\u00b7schen", "wir", "ja", "nie\u00b7mals", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ade, Feinsliebchen, und jetzt mu\u00df ich scheiden,", "tokens": ["A\u00b7de", ",", "Feins\u00b7lieb\u00b7chen", ",", "und", "jetzt", "mu\u00df", "ich", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Weil mich zu Bette ruft die harte Pflicht.", "tokens": ["Weil", "mich", "zu", "Bet\u00b7te", "ruft", "die", "har\u00b7te", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wenn uns auch die bittre Stunde trennt,", "tokens": ["Doch", "wenn", "uns", "auch", "die", "bitt\u00b7re", "Stun\u00b7de", "trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verbleib dein Schatz ich \u2013 aberi juhe!", "tokens": ["Ver\u00b7bleib", "dein", "Schatz", "ich", "\u2013", "a\u00b7be\u00b7ri", "ju\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PP