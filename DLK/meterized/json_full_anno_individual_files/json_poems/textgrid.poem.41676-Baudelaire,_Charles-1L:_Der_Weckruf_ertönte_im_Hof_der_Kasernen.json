{"textgrid.poem.41676": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Weckruf ert\u00f6nte im Hof der Kasernen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Weckruf ert\u00f6nte im Hof der Kasernen", "tokens": ["Der", "Weck\u00b7ruf", "er\u00b7t\u00f6n\u00b7te", "im", "Hof", "der", "Ka\u00b7ser\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und der Morgenwind blies auf die Laternen.", "tokens": ["Und", "der", "Mor\u00b7gen\u00b7wind", "blies", "auf", "die", "La\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es war die Stunde, da der Tr\u00e4ume b\u00f6sem Bann", "tokens": ["Es", "war", "die", "Stun\u00b7de", ",", "da", "der", "Tr\u00e4u\u00b7me", "b\u00f6\u00b7sem", "Bann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf seinem Bett der Knab sich nicht entwinden kann,", "tokens": ["Auf", "sei\u00b7nem", "Bett", "der", "Knab", "sich", "nicht", "ent\u00b7win\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da wie ein blutig Aug, das bebt in wehen Qualen,", "tokens": ["Da", "wie", "ein", "blu\u00b7tig", "Aug", ",", "das", "bebt", "in", "we\u00b7hen", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJD", "NN", "$,", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Lampen ihren Fleck rot in den Morgen malen,", "tokens": ["Die", "Lam\u00b7pen", "ih\u00b7ren", "Fleck", "rot", "in", "den", "Mor\u00b7gen", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da durch des K\u00f6rpers Last die Seele niederbricht", "tokens": ["Da", "durch", "des", "K\u00f6r\u00b7pers", "Last", "die", "See\u00b7le", "nie\u00b7der\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und gleiche K\u00e4mpfe ringt wie Tag und Lampenlicht.", "tokens": ["Und", "glei\u00b7che", "K\u00e4mp\u00b7fe", "ringt", "wie", "Tag", "und", "Lam\u00b7pen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie ein betr\u00e4nt Gesicht, das trocknet in den Winden,", "tokens": ["Wie", "ein", "be\u00b7tr\u00e4nt", "Ge\u00b7sicht", ",", "das", "trock\u00b7net", "in", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erschauern in der Luft die Dinge, die entschwinden.", "tokens": ["Er\u00b7schau\u00b7ern", "in", "der", "Luft", "die", "Din\u00b7ge", ",", "die", "ent\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des Schreibens ist der Mann, die Frau des Liebens satt.", "tokens": ["Des", "Schrei\u00b7bens", "ist", "der", "Mann", ",", "die", "Frau", "des", "Lie\u00b7bens", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schon stiegen hier und dort Rauchs\u00e4ulen aus der Stadt.", "tokens": ["Schon", "stie\u00b7gen", "hier", "und", "dort", "Rauch\u00b7s\u00e4u\u00b7len", "aus", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Freudenm\u00e4dchen, tiefgesenkt die bleichen Lider,", "tokens": ["Die", "Freu\u00b7den\u00b7m\u00e4d\u00b7chen", ",", "tief\u00b7ge\u00b7senkt", "die", "blei\u00b7chen", "Li\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie lagen offnen Munds in stierem Schlaf danieder;", "tokens": ["Sie", "la\u00b7gen", "off\u00b7nen", "Munds", "in", "stie\u00b7rem", "Schlaf", "da\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Arme, welk die Brust, die Lippen ohne Blut,", "tokens": ["Und", "Ar\u00b7me", ",", "welk", "die", "Brust", ",", "die", "Lip\u00b7pen", "oh\u00b7ne", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bliesen die Finger sich und bliesen in die Glut.", "tokens": ["Blie\u00b7sen", "die", "Fin\u00b7ger", "sich", "und", "blie\u00b7sen", "in", "die", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Es war die Stunde, da in K\u00e4lte und Entbehren", "tokens": ["Es", "war", "die", "Stun\u00b7de", ",", "da", "in", "K\u00e4l\u00b7te", "und", "Ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Wehn und N\u00f6te der Geb\u00e4renden sich mehren,", "tokens": ["Die", "Wehn", "und", "N\u00f6\u00b7te", "der", "Ge\u00b7b\u00e4\u00b7ren\u00b7den", "sich", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gleich einem Schluchzen, das ein Blutsturz j\u00e4h verschlang,", "tokens": ["Gleich", "ei\u00b7nem", "Schluch\u00b7zen", ",", "das", "ein", "Blut\u00b7sturz", "j\u00e4h", "ver\u00b7schlang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der fr\u00fche Ruf des Hahns durch Morgennebel drang;", "tokens": ["Der", "fr\u00fc\u00b7he", "Ruf", "des", "Hahns", "durch", "Mor\u00b7gen\u00b7ne\u00b7bel", "drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Um die Geb\u00e4ude schwamm das Nebelmeer, das fahle,", "tokens": ["Um", "die", "Ge\u00b7b\u00e4u\u00b7de", "schwamm", "das", "Ne\u00b7bel\u00b7meer", ",", "das", "fah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schwer keuchten Sterbende im Scho\u00df der Hospitale", "tokens": ["Schwer", "keuch\u00b7ten", "Ster\u00b7ben\u00b7de", "im", "Scho\u00df", "der", "Hos\u00b7pi\u00b7ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.12": {"text": "Und stie\u00dfen todesmatt ein letztes R\u00f6cheln aus.", "tokens": ["Und", "stie\u00b7\u00dfen", "to\u00b7des\u00b7matt", "ein", "letz\u00b7tes", "R\u00f6\u00b7cheln", "aus", "."], "to