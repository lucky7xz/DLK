{"textgrid.poem.41107": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "M\u00f6glichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man so sitzt im tr\u00fcben Mut", "tokens": ["Wenn", "man", "so", "sitzt", "im", "tr\u00fc\u00b7ben", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sich kann fassen kaum,", "tokens": ["Und", "sich", "kann", "fas\u00b7sen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VMFIN", "VVINF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denkt man oft: Wie w\u00e4r's so gut,", "tokens": ["So", "denkt", "man", "oft", ":", "Wie", "w\u00e4r's", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$.", "PWAV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4r' alles nur ein Traum!", "tokens": ["W\u00e4r'", "al\u00b7les", "nur", "ein", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ist denn kein Traum die bange Nacht,", "tokens": ["Ist", "denn", "kein", "Traum", "die", "ban\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schwere Herz? Vielleicht", "tokens": ["Das", "schwe\u00b7re", "Herz", "?", "