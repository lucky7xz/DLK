{"dta.poem.13532": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und aus solcher Schmerzen Schwellen,", "tokens": ["Und", "aus", "sol\u00b7cher", "Schmer\u00b7zen", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was so lange d\u00fcrstend rang,", "tokens": ["Was", "so", "lan\u00b7ge", "d\u00fcrs\u00b7tend", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will an's Licht nun rastlos quellen,", "tokens": ["Will", "an's", "Licht", "nun", "rast\u00b7los", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzend mit den Wasserf\u00e4llen,", "tokens": ["St\u00fcr\u00b7zend", "mit", "den", "Was\u00b7ser\u00b7f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelst\u00e4ubend, jubelnd, bang,", "tokens": ["Him\u00b7mel\u00b7st\u00e4u\u00b7bend", ",", "ju\u00b7belnd", ",", "bang", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der Ferne sanft zu ziehen,", "tokens": ["Nach", "der", "Fer\u00b7ne", "sanft", "zu", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo so himmlisch Rufen sang,", "tokens": ["Wo", "so", "himm\u00b7lisch", "Ru\u00b7fen", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach! nach tiefern Melodieen.", "tokens": ["Ach", "!", "nach", "tie\u00b7fern", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}