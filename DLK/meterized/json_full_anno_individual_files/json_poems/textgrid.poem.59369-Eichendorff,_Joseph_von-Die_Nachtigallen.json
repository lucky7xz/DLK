{"textgrid.poem.59369": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachtigallen", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6cht wissen, was sie schlagen", "tokens": ["M\u00f6cht", "wis\u00b7sen", ",", "was", "sie", "schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sch\u00f6n bei der Nacht,", "tokens": ["So", "sch\u00f6n", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "'s ist in der Welt ja doch niemand,", "tokens": ["'s", "ist", "in", "der", "Welt", "ja", "doch", "nie\u00b7mand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "PIS", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der mit ihnen wacht.", "tokens": ["Der", "mit", "ih\u00b7nen", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und die Wolken, die reisen,", "tokens": ["Und", "die", "Wol\u00b7ken", ",", "die", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und das Land ist so bla\u00df,", "tokens": ["Und", "das", "Land", "ist", "so", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], 