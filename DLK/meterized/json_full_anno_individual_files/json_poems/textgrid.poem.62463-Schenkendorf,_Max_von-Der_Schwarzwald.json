{"textgrid.poem.62463": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schwarzwald", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie fr\u00f6hlich hier im reichen Thal", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "hier", "im", "rei\u00b7chen", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieben B\u00e4ume stehn.", "tokens": ["Die", "lie\u00b7ben", "B\u00e4u\u00b7me", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gereift an Gottes mildem Strahl,", "tokens": ["Ge\u00b7reift", "an", "Got\u00b7tes", "mil\u00b7dem", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gesch\u00fctzt von jenen H\u00f6h'n.", "tokens": ["Ge\u00b7sch\u00fctzt", "von", "je\u00b7nen", "H\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VVPP", "APPR", "PDAT", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Kirschen und ihr K\u00e4sten sollt", "tokens": ["Ihr", "Kir\u00b7schen", "und", "ihr", "K\u00e4s\u00b7ten", "sollt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch manches Jahr gedeihn,", "tokens": ["Noch", "man\u00b7ches", "Jahr", "ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch du Gutedel, flie\u00dfend Gold,", "tokens": ["Auch", "du", "Gut\u00b7e\u00b7del", ",", "flie\u00b7\u00dfend", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "$,", "ADJD", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auch du, Markgrafenwein.", "tokens": ["Auch", "du", ",", "Mark\u00b7gra\u00b7fen\u00b7wein", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch h\u00f6her, immer h\u00f6her zieht,", "tokens": ["Doch", "h\u00f6\u00b7her", ",", "im\u00b7mer", "h\u00f6\u00b7her", "zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Walde zieht mich's hin,", "tokens": ["Zum", "Wal\u00b7de", "zieht", "mich's", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort nach dem dunkeln Gipfel sieht", "tokens": ["Dort", "nach", "dem", "dun\u00b7keln", "Gip\u00b7fel", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein liebetrunkner Sinn.", "tokens": ["Mein", "lie\u00b7be\u00b7trunk\u00b7ner", "Sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mich ziehet nach dem h\u00f6chsten Wald", "tokens": ["Mich", "zie\u00b7het", "nach", "dem", "h\u00f6chs\u00b7ten", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die h\u00f6chste Sehnsucht fort.", "tokens": ["Die", "h\u00f6chs\u00b7te", "Sehn\u00b7sucht", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht schrecket mich im H\u00f6llenthor,", "tokens": ["Nicht", "schre\u00b7cket", "mich", "im", "H\u00f6l\u00b7len\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der grause Felsensteg,", "tokens": ["Der", "grau\u00b7se", "Fel\u00b7sens\u00b7teg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weit \u00fcber Land und Fels empor", "tokens": ["Weit", "\u00fc\u00b7ber", "Land", "und", "Fels", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Gipfel geht mein Weg.", "tokens": ["Zum", "Gip\u00b7fel", "geht", "mein", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dein Wasser sch\u00f6pf' ich in der Hand,", "tokens": ["Dein", "Was\u00b7ser", "sch\u00f6pf'", "ich", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Donau, frohe Fahrt!", "tokens": ["O", "Do\u00b7nau", ",", "fro\u00b7he", "Fahrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verk\u00fcnde nur im Morgenland", "tokens": ["Ver\u00b7k\u00fcn\u00b7de", "nur", "im", "Mor\u00b7gen\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Deutschen Sinn und Art.", "tokens": ["Der", "Deut\u00b7schen", "Sinn", "und", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du mit dem wei\u00dfen W\u00e4lderhut", "tokens": ["Du", "mit", "dem", "wei\u00b7\u00dfen", "W\u00e4l\u00b7der\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit dem schwarzen Band,", "tokens": ["Und", "mit", "dem", "schwar\u00b7zen", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O M\u00e4gdlein sittig, sch\u00f6n und gut,", "tokens": ["O", "M\u00e4gd\u00b7lein", "sit\u00b7tig", ",", "sch\u00f6n", "und", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df mir das deutsche Land.", "tokens": ["Gr\u00fc\u00df", "mir", "das", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich mu\u00df hinauf zum schwarzen Wald,", "tokens": ["Ich", "mu\u00df", "hin\u00b7auf", "zum", "schwar\u00b7zen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So liebend und allein,", "tokens": ["So", "lie\u00b7bend", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort soll fortan mein Aufenthalt", "tokens": ["Dort", "soll", "for\u00b7tan", "mein", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Kirche sein.", "tokens": ["Und", "mei\u00b7ne", "Kir\u00b7che", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Euch B\u00e4ume hat kein Mensch gestreut,", "tokens": ["Euch", "B\u00e4u\u00b7me", "hat", "kein", "Mensch", "ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch s\u00e4'te Gottes Hand,", "tokens": ["Euch", "s\u00e4'\u00b7te", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr alten hohen Tannen seid", "tokens": ["Ihr", "al\u00b7ten", "ho\u00b7hen", "Tan\u00b7nen", "seid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir meines Gottes Pfand.", "tokens": ["Mir", "mei\u00b7nes", "Got\u00b7tes", "Pfand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Durch eure schlanken Wipfel geht", "tokens": ["Durch", "eu\u00b7re", "schlan\u00b7ken", "Wip\u00b7fel", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein wunderbarer Gang,", "tokens": ["Sein", "wun\u00b7der\u00b7ba\u00b7rer", "Gang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In euren gr\u00fcnen Zweigen weht", "tokens": ["In", "eu\u00b7ren", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schauervoller Klang.", "tokens": ["Ein", "schau\u00b7er\u00b7vol\u00b7ler", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das ist ein ferner Liebeston,", "tokens": ["Das", "ist", "ein", "fer\u00b7ner", "Lie\u00b7be\u00b7ston", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er klingt wol tausend Jahr,", "tokens": ["Er", "klingt", "wol", "tau\u00b7send", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Geistern, deren Zeit entflohn", "tokens": ["Von", "Geis\u00b7tern", ",", "de\u00b7ren", "Zeit", "ent\u00b7flohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deren Burg hier war.", "tokens": ["Und", "de\u00b7ren", "Burg", "hier", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wie schaurig hier und wie allein", "tokens": ["Wie", "schau\u00b7rig", "hier", "und", "wie", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "KON", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im h\u00f6chsten schwarzen Wald,", "tokens": ["Im", "h\u00f6chs\u00b7ten", "schwar\u00b7zen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht fern kann hier die Wohnung sein", "tokens": ["Nicht", "fern", "kann", "hier", "die", "Woh\u00b7nung", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VMFIN", "ADV", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der seligsten Gestalt.", "tokens": ["Der", "se\u00b7ligs\u00b7ten", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Der Freiheit, die mein Herz gewann,", "tokens": ["Der", "Frei\u00b7heit", ",", "die", "mein", "Herz", "ge\u00b7wann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der s\u00fc\u00dfen Heldenbraut,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Hel\u00b7den\u00b7braut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ich, ein liebentbrannter Mann,", "tokens": ["Der", "ich", ",", "ein", "lie\u00b7bent\u00b7brann\u00b7ter", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ewig mich vertraut.", "tokens": ["F\u00fcr", "e\u00b7wig", "mich", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "O Freiheit, Freiheit komm' heraus,", "tokens": ["O", "Frei\u00b7heit", ",", "Frei\u00b7heit", "komm'", "he\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kr\u00e4ftig und so fromm,", "tokens": ["So", "kr\u00e4f\u00b7tig", "und", "so", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus deinem gr\u00fcnen dunkeln Haus", "tokens": ["Aus", "dei\u00b7nem", "gr\u00fc\u00b7nen", "dun\u00b7keln", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du sch\u00f6ne Freiheit komm'.", "tokens": ["Du", "sch\u00f6\u00b7ne", "Frei\u00b7heit", "komm'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Dort unten la\u00df dich wieder schaun,", "tokens": ["Dort", "un\u00b7ten", "la\u00df", "dich", "wie\u00b7der", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im freien deutschen Land,", "tokens": ["Im", "frei\u00b7en", "deut\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bewahre du die treuen Gaun,", "tokens": ["Be\u00b7wah\u00b7re", "du", "die", "treu\u00b7en", "Gaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor welschem Sklavenstand.", "tokens": ["Vor", "wel\u00b7schem", "Skla\u00b7ven\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.16": {"line.1": {"text": "Wie fr\u00f6hlich hier im reichen Thal", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "hier", "im", "rei\u00b7chen", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieben B\u00e4ume stehn.", "tokens": ["Die", "lie\u00b7ben", "B\u00e4u\u00b7me", "stehn", "."], "token_info": ["word", "word", "wor