{"textgrid.poem.39121": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Motto", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist auf Erden wenig quer gegangen,", "tokens": ["Mir", "ist", "auf", "Er\u00b7den", "we\u00b7nig", "quer", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenig Gram hat meine Stirn verdunkelt;", "tokens": ["Und", "we\u00b7nig", "Gram", "hat", "mei\u00b7ne", "Stirn", "ver\u00b7dun\u00b7kelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets hat der Freude Rot aus meinen Wangen,", "tokens": ["Stets", "hat", "der", "Freu\u00b7de", "Rot", "aus", "mei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus meinen Augen Jugendmut gefunkelt.", "tokens": ["Aus", "mei\u00b7nen", "Au\u00b7gen", "Ju\u00b7gend\u00b7mut", "ge\u00b7fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich schminkte nie zum Spa\u00df die Wange blasser,", "tokens": ["Ich", "schmink\u00b7te", "nie", "zum", "Spa\u00df", "die", "Wan\u00b7ge", "blas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch quetscht' ich je mit affektierten Schmerzen", "tokens": ["Noch", "quetscht'", "ich", "je", "mit", "af\u00b7fek\u00b7tier\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In meine Augen k\u00fcnstlich Tr\u00e4nenwasser;", "tokens": ["In", "mei\u00b7ne", "Au\u00b7gen", "k\u00fcnst\u00b7lich", "Tr\u00e4\u00b7nen\u00b7was\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich leide wenig an zerri\u00dfnem Herzen.", "tokens": ["Ich", "lei\u00b7de", "we\u00b7nig", "an", "zer\u00b7ri\u00df\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mich freut der Becher noch, der Schmerzvertilger,", "tokens": ["Mich", "freut", "der", "Be\u00b7cher", "noch", ",", "der", "Schmerz\u00b7ver\u00b7til\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich freut der holde Zauber noch des Kusses;", "tokens": ["Mich", "freut", "der", "hol\u00b7de", "Zau\u00b7ber", "noch", "des", "Kus\u00b7ses", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich walle rasch, ein froher Liebespilger,", "tokens": ["Ich", "wal\u00b7le", "rasch", ",", "ein", "fro\u00b7her", "Lie\u00b7be\u00b7spil\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im schrankenlosen Garten des Genusses.", "tokens": ["Im", "schran\u00b7ken\u00b7lo\u00b7sen", "Gar\u00b7ten", "des", "Ge\u00b7nus\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn der Sturm der Zeit mein Haupt getroffen,", "tokens": ["Und", "wenn", "der", "Sturm", "der", "Zeit", "mein", "Haupt", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So meint' ich ihn am besten zu verstehen,", "tokens": ["So", "meint'", "ich", "ihn", "am", "bes\u00b7ten", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich mit Mut und meinem k\u00fchnsten Hoffen", "tokens": ["Wenn", "ich", "mit", "Mut", "und", "mei\u00b7nem", "k\u00fchns\u00b7ten", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem blauen Himmel wollt' entgegensehen.", "tokens": ["Dem"