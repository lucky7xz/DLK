{"textgrid.poem.46820": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ich, mit Augen und mit Ohren trunken]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, mit Augen und mit Ohren trunken,", "tokens": ["Wenn", "ich", ",", "mit", "Au\u00b7gen", "und", "mit", "Oh\u00b7ren", "trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "KON", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oft lauschte deinem Blicke, deinem Wort,", "tokens": ["Oft", "lauschte", "dei\u00b7nem", "Bli\u00b7cke", ",", "dei\u00b7nem", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Erschrocken nahm ich wahr an einem Funken", "tokens": ["Er\u00b7schro\u00b7cken", "nahm", "ich", "wahr", "an", "ei\u00b7nem", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Innern, welch ein Feuer brannte dort.", "tokens": ["Des", "In\u00b7nern", ",", "welch", "ein", "Feu\u00b7er", "brann\u00b7te", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und darf es nun ein Wunder mich bedunken,", "tokens": ["Und", "darf", "es", "nun", "ein", "Wun\u00b7der", "mich", "be\u00b7dun\u00b7ken", ","], "token_info": ["word"