{"textgrid.poem.43840": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Feiertagsfreuden", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Festag ist im ganzen Jahr,", "tokens": ["Kein", "Fes\u00b7tag", "ist", "im", "gan\u00b7zen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns nicht Etwas br\u00e4chte,", "tokens": ["Der", "uns", "nicht", "Et\u00b7was", "br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer am meisten Freude bringt,", "tokens": ["Und", "wer", "am", "meis\u00b7ten", "Freu\u00b7de", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist f\u00fcr uns der rechte.", "tokens": ["Das", "ist", "f\u00fcr", "uns", "der", "rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ist die Fastnacht wieder da,", "tokens": ["Und", "ist", "die", "Fast\u00b7nacht", "wie\u00b7der", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann kommt der G\u00e4stebitter:", "tokens": ["Dann", "kommt", "der", "G\u00e4s\u00b7t