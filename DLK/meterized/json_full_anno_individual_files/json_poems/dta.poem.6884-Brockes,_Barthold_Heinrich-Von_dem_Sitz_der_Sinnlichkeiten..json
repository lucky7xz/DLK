{"dta.poem.6884": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist das Gehirn von D\u00fcnsten eingenommen,", "tokens": ["Ist", "das", "Ge\u00b7hirn", "von", "D\u00fcns\u00b7ten", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in dasselbige durch Kranckheit und durch Pein", "tokens": ["Die", "in", "das\u00b7sel\u00b7bi\u00b7ge", "durch", "Kran\u00b7ck\u00b7heit", "und", "durch", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Entstanden und gekommen,", "tokens": ["Ent\u00b7stan\u00b7den", "und", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und theilt sich folgends nicht mehr mit der Geister Lauf;", "tokens": ["Und", "theilt", "sich", "fol\u00b7gends", "nicht", "mehr", "mit", "der", "Geis\u00b7ter", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So h\u00f6ret das Gef\u00fchl im gantzen C\u00f6rper auf.", "tokens": ["So", "h\u00f6\u00b7ret", "das", "Ge\u00b7f\u00fchl", "im", "gant\u00b7zen", "C\u00f6r\u00b7per", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann aber fern vom Hirn der Schmertz die Glieder trifft.", "tokens": ["Wann", "a\u00b7ber", "fern", "vom", "Hirn", "der", "Schmertz", "die", "Glie\u00b7der", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn Feur und Stahl uns Wunden macht und schl\u00e4get,", "tokens": ["Wenn", "Feur", "und", "Stahl", "uns", "Wun\u00b7den", "macht", "und", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird gleich in ihm ein scharffer Schmertz gestifft.", "tokens": ["Wird", "gleich", "in", "ihm", "ein", "scharf\u00b7fer", "Schmertz", "ge\u00b7stifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da sich der Geister Heer gewaltig schnell beweget;", "tokens": ["Da", "sich", "der", "Geis\u00b7ter", "Heer", "ge\u00b7wal\u00b7tig", "schnell", "be\u00b7we\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wird pl\u00f6tzlich im Gehirn ein scharffer Druck erreget.", "tokens": ["Wird", "pl\u00f6tz\u00b7lich", "im", "Ge\u00b7hirn", "ein", "scharf\u00b7fer", "Druck", "er\u00b7re\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}