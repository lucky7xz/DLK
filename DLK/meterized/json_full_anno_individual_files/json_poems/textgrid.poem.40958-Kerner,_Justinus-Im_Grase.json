{"textgrid.poem.40958": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Im Grase", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft mich im Gras und Blumen liegen", "tokens": ["La\u00dft", "mich", "im", "Gras", "und", "Blu\u00b7men", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaun dem blauen Himmel zu:", "tokens": ["Und", "schaun", "dem", "blau\u00b7en", "Him\u00b7mel", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie goldne Wolken ihn durchfliegen,", "tokens": ["Wie", "gold\u00b7ne", "Wol\u00b7ken", "ihn", "durch\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihm ein Falke kreist in Ruh'.", "tokens": ["In", "ihm", "ein", "Fal\u00b7ke", "kreist", "in", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die blaue Stille st\u00f6rt dort oben", "tokens": ["Die", "blau\u00b7e", "Stil\u00b7le", "st\u00f6rt", "dort", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Dampfer und kein Segelschiff,", "tokens": ["Kein", "Damp\u00b7fer", "und", "kein", "Se\u00b7gel\u00b7schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Menschentritt, kein Pferdetoben,", "tokens": ["Kein", "Men\u00b7schen\u00b7tritt", ",", "kein", "Pfer\u00b7de\u00b7to\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht des Dampfwagens wilder Pfiff.", "tokens": ["Nicht", "des", "Dampf\u00b7wa\u00b7gens", "wil\u00b7der", "Pfiff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "La\u00dft satt mich schauen in die Klarheit,", "tokens": ["La\u00dft", "satt", "mich", "schau\u00b7en", "in", "die", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesen keuschen, sel'gen Raum,", "tokens": ["In", "die\u00b7sen", "keu\u00b7schen", ",", "sel'\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn bald k\u00f6nnt' werden ja zur Wahrheit", "tokens": ["Denn", "bald", "k\u00f6nnt'", "wer\u00b7den", "ja", "zur", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Fliegen, der unsel'ge Traum.", "tokens": ["Das", "Flie\u00b7gen", ",", "der", "un\u00b7sel'\u00b7ge", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Dann flieht der Vogel aus den L\u00fcften", "tokens": ["Dann", "flieht", "der", "Vo\u00b7gel", "aus", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie aus dem Rhein der Salme schon,", "tokens": ["Wie", "aus", "dem", "Rhein", "der", "Sal\u00b7me", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NE", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo einst singend Lerchen schifften,", "tokens": ["Und", "wo", "einst", "sin\u00b7gend", "Ler\u00b7chen", "schiff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schifft gr\u00e4mlich stumm Britannias Sohn.", "tokens": ["Schifft", "gr\u00e4m\u00b7lich", "stumm", "Bri\u00b7tan\u00b7ni\u00b7as", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "NE", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Blick' ich gen Himmel, zu gewahren,", "tokens": ["Blick'", "ich", "gen", "Him\u00b7mel", ",", "zu", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum's so pl\u00f6tzlich dunkel sei,", "tokens": ["War\u00b7um's", "so", "pl\u00f6tz\u00b7lich", "dun\u00b7kel", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erschau' ich einen Zug von Waren,", "tokens": ["Er\u00b7schau'", "ich", "ei\u00b7nen", "Zug", "von", "Wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der an der Sonne schifft vorbei.", "tokens": ["Der", "an", "der", "Son\u00b7ne", "schifft", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fchl' Regen ich im Sonnenscheine,", "tokens": ["F\u00fchl'", "Re\u00b7gen", "ich", "im", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Such' ich den Regenbogen keck,", "tokens": ["Such'", "ich", "den", "Re\u00b7gen\u00b7bo\u00b7gen", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist es kein Regen, wie ich meine,", "tokens": ["Ist", "es", "kein", "Re\u00b7gen", ",", "wie", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ward in der Luft ein \u00d6lfa\u00df leck.", "tokens": ["Ward", "in", "der", "Luft", "ein", "\u00d6l\u00b7fa\u00df", "leck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00dft schaun mich von dem Erdget\u00fcmmel", "tokens": ["La\u00dft", "schaun", "mich", "von", "dem", "Erd\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Himmel, eh' es ist zu sp\u00e4t,", "tokens": ["Zum", "Him\u00b7mel", ",", "eh'", "es", "ist", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' wie vom Erdball so vom Himmel", "tokens": ["Eh'", "wie", "vom", "Erd\u00b7ball", "so", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPRART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Poesie still trauernd geht.", "tokens": ["Die", "Poe\u00b7sie", "still", "trau\u00b7ernd", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Verzeiht dies Lied des Dichters Grolle,", "tokens": ["Ver\u00b7zeiht", "dies", "Lied", "des", "Dich\u00b7ters", "Grol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4umt er von solchem Himmelsgraus,", "tokens": ["Tr\u00e4umt", "er", "von", "sol\u00b7chem", "Him\u00b7mels\u00b7graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er, den die Zeit, die dampfestolle,", "tokens": ["Er", ",", "den", "die", "Zeit", ",", "die", "damp\u00b7fe\u00b7stol\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlie\u00dft von der Erde lieblos aus.", "tokens": ["Schlie\u00dft", "von", "der", "Er\u00b7de", "lieb\u00b7los", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "La\u00dft mich im Gras und Blumen liegen", "tokens": ["La\u00dft", "mich", "im", "Gras", "und", "Blu\u00b7men", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaun dem blauen Himmel zu:", "tokens": ["Und", "schaun", "dem", "blau\u00b7en", "Him\u00b7mel", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie goldne Wolken ihn durchfliegen,", "tokens": ["Wie", "gold\u00b7ne", "Wol\u00b7ken", "ihn", "durch\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihm ein Falke kreist in Ruh'.", "tokens": ["In", "ihm", "ein", "