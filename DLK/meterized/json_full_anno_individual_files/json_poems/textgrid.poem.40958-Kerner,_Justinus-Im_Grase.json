k", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist es kein Regen, wie ich meine,", "tokens": ["Ist", "es", "kein", "Re\u00b7gen", ",", "wie", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ward in der Luft ein \u00d6lfa\u00df leck.", "tokens": ["Ward", "in", "der", "Luft", "ein", "\u00d6l\u00b7fa\u00df", "leck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "La\u00dft schaun mich von dem Erdget\u00fcmmel", "tokens": ["La\u00dft", "schaun", "mich", "von", "dem", "Erd\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Himmel, eh' es ist zu sp\u00e4t,", "tokens": ["Zum", "Him\u00b7mel", ",", "eh'", "es", "ist", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' wie vom Erdball so vom Himmel", "tokens": ["Eh'", "wie", "vom", "Erd\u00b7ball", "so", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPRART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Poesie still trauernd geht.", "tokens": ["Die", "Poe\u00b7sie", "still", "trau\u00b7ernd", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Verzeiht dies Lied des Dichters Grolle,", "tokens": ["Ver\u00b7zeiht", "dies", "Lied", "des", "Dich\u00b7ters", "Grol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4umt er von solchem Himmelsgraus,", "tokens": ["Tr\u00e4umt", "er", "von", "sol\u00b7chem", "Him\u00b7mels\u00b7graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er, den die Zeit, die dampfestolle,", "tokens": ["Er", ",", "den", "die", "Zeit", ",", "die", "damp\u00b7fe\u00b7stol\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlie\u00dft von der Erde lieblos aus.", "tokens": ["Schlie\u00dft", "von", "der", "Er\u00b7de", "lieb\u00b7los", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}