s": ["PIS", "VVFIN", "ADJD", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Aller Minschen Doent, Gedancken,", "tokens": ["Al\u00b7ler", "Min\u00b7schen", "Do\u00b7ent", ",", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rede, Mening, Sinn und Waen,", "tokens": ["Re\u00b7de", ",", "Me\u00b7ning", ",", "Sinn", "und", "Waen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein Wind und Wedderhaen", "tokens": ["Als", "ein", "Wind", "und", "Wed\u00b7der\u00b7ha\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hen und her vnstedig wancken.", "tokens": ["Hen", "und", "her", "vns\u00b7te\u00b7dig", "wan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wat dar was ein nie Gesanck,", "tokens": ["Wat", "dar", "was", "ein", "nie", "Ge\u00b7sanck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PWS", "ART", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dat is nu de olde Klanck.", "tokens": ["Dat", "is", "nu", "de", "ol\u00b7de", "Klanck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wat v\u00f6rm Jahr was Allemode,", "tokens": ["Wat", "v\u00f6rm", "Jahr", "was", "Al\u00b7le\u00b7mo\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PWS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd van jederm wart geehrt,", "tokens": ["Vnd", "van", "je\u00b7derm", "wart", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PIS", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dat is itzund nicht mehr werth", "tokens": ["Dat", "is", "it\u00b7zund", "nicht", "mehr", "werth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als dat schimmel van dem Brode:", "tokens": ["Als", "dat", "schim\u00b7mel", "van", "dem", "Bro\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie wert old, und old wert nie,", "tokens": ["Nie", "wert", "old", ",", "und", "old", "wert", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "KON", "ADJD", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kaken moet men frischen Brie.", "tokens": ["Ka\u00b7ken", "moet", "men", "fri\u00b7schen", "Brie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Solcke doerheit wert gehalet", "tokens": ["Sol\u00b7cke", "doer\u00b7heit", "wert", "ge\u00b7ha\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All uth Franckrick, darv\u00f6r is", "tokens": ["All", "uth", "Fran\u00b7ckrick", ",", "dar\u00b7v\u00f6r", "is"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "NE", "$,", "PAV", "FM"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Mennig Schilling, ja gewis", "tokens": ["Men\u00b7nig", "Schil\u00b7ling", ",", "ja", "ge\u00b7wis"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mennig tunne Gold betalet.", "tokens": ["Men\u00b7nig", "tun\u00b7ne", "Gold", "be\u00b7ta\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "V\u00f6r Vernufft und Wy\u00dfheit goet", "tokens": ["V\u00f6r", "Ver\u00b7nufft", "und", "Wy\u00df\u00b7heit", "goet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gifft men kuem ein st\u00fccke Broet.", "tokens": ["Gifft", "men", "ku\u00b7em", "ein", "st\u00fc\u00b7cke", "Broet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Nemand h\u00f6lt sick na dem Stande,", "tokens": ["Ne\u00b7mand", "h\u00f6lt", "sick", "na", "dem", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dar en Gott hefft tho gebracht,", "tokens": ["Dar", "en", "Gott", "hefft", "tho", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "NN", "VVFIN", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nemand blifft bi siner Dracht,", "tokens": ["Ne\u00b7mand", "blifft", "bi", "si\u00b7ner", "Dracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "De gebr\u00fccklick is im Lande,", "tokens": ["De", "ge\u00b7br\u00fcc\u00b7klick", "is", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlichtes Volck ein Levend f\u00f6rth,", "tokens": ["Schlich\u00b7tes", "Volck", "ein", "Le\u00b7vend", "f\u00f6rth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als dem Adelstand geb\u00f6rth.", "tokens": ["Als", "dem", "A\u00b7del\u00b7stand", "ge\u00b7b\u00f6rth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Vnderscheet der St\u00e4nd und Orden", "tokens": ["Vn\u00b7der\u00b7scheet", "der", "St\u00e4nd", "und", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Is den L\u00fcden man ein Spot,", "tokens": ["Is", "den", "L\u00fc\u00b7den", "man", "ein", "Spot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcker doch wy\u00dflick van Gott", "tokens": ["Wel\u00b7cker", "doch", "wy\u00df\u00b7lick", "van", "Gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "NE", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fclvest is gestifftet worden.", "tokens": ["S\u00fcl\u00b7vest", "is", "ge\u00b7stiff\u00b7tet", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "VVPP", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "B\u00f6rgers willen holden sick,", "tokens": ["B\u00f6r\u00b7gers", "wil\u00b7len", "hol\u00b7den", "sick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Na der hogen wise und schick.", "tokens": ["Na", "der", "ho\u00b7gen", "wi\u00b7se", "und", "schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Kleder, Sprake, Versche schriven,", "tokens": ["Kle\u00b7der", ",", "Spra\u00b7ke", ",", "Ver\u00b7sche", "schri\u00b7ven", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endert sick fast alle Jahr,", "tokens": ["En\u00b7dert", "sick", "fast", "al\u00b7le", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man ick achte idt nicht ein haer.", "tokens": ["Man", "ick", "ach\u00b7te", "idt", "nicht", "ein", "haer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADJA", "NN", "PTKNEG", "ART", "NN", "$."], "m