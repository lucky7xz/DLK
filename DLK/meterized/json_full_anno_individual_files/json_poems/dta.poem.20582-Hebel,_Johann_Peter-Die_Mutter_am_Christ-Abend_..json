{"dta.poem.20582": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Mutter  \n am Christ-Abend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Erschloft, er schloft! Do lit er, wie ne Grof!               ", "tokens": ["Er\u00b7schloft", ",", "er", "schloft", "!", "Do", "lit", "er", ",", "wie", "ne", "Grof", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du lieben Engel, was i bitt,", "tokens": ["Du", "lie\u00b7ben", "En\u00b7gel", ",", "was", "i", "bitt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "by Lib und Lebe verwach mer nit,", "tokens": ["by", "Lib", "und", "Le\u00b7be", "ver\u00b7wach", "mer", "nit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "KON", "NN", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gott gits de Siinen im Schlof!", "tokens": ["Gott", "gits", "de", "Si\u00b7i\u00b7nen", "im", "Schlof", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NE", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Verwachmer nit, verwachmer nit!", "tokens": ["Ver\u00b7wach\u00b7mer", "nit", ",", "ver\u00b7wach\u00b7mer", "nit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Di Mutter goht mit stillem Tritt,", "tokens": ["Di", "Mut\u00b7ter", "goht", "mit", "stil\u00b7lem", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie goht mit zartem Mutter-Sinu,", "tokens": ["sie", "goht", "mit", "zar\u00b7tem", "Mut\u00b7ter\u00b7Si\u00b7nu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und holt e Baum im Ch\u00e4mmerli d\u2019inn.", "tokens": ["und", "holt", "e", "Baum", "im", "Ch\u00e4m\u00b7mer\u00b7li", "d'", "in\u00b7n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "NE", "NE", "APPRART", "NE", "NE", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Was henki der denn dra?", "tokens": ["Was", "hen\u00b7ki", "der", "denn", "dra", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADV", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ne sch\u00f6ne Lebchueche-Ma;", "tokens": ["Ne", "sch\u00f6\u00b7ne", "Leb\u00b7chue\u00b7che\u00b7Ma", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "ne Gitzeli, ne Mummeli", "tokens": ["ne", "Git\u00b7ze\u00b7li", ",", "ne", "Mum\u00b7me\u00b7li"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "und Bl\u00fcemli wii\u00df und roth und gel,", "tokens": ["und", "Bl\u00fcem\u00b7li", "wii\u00df", "und", "roth", "und", "gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "KON", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "alles vo s\u00fcessem Zucker-Mehl.", "tokens": ["al\u00b7les", "vo", "s\u00fces\u00b7sem", "Zu\u00b7cke\u00b7rMehl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "\u2019s isch gnueg, du Mutter-Herz,", "tokens": ["'s", "isch", "gnu\u00b7eg", ",", "du", "Mut\u00b7ter\u00b7Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "viel S\u00fce\u00df macht numme Schmerz!", "tokens": ["viel", "S\u00fce\u00df", "macht", "num\u00b7me", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gib\u2019s sparsem, wie der liebi Gott,", "tokens": ["Gib's", "spar\u00b7sem", ",", "wie", "der", "lie\u00b7bi", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er helset nit alli Tag Zucker-Brod.", "tokens": ["er", "hel\u00b7set", "nit", "al\u00b7li", "Tag", "Zu\u00b7cke\u00b7rBrod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PIAT", "NN", "NN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Jez R\u00fcmmechr\u00fcsliger her,", "tokens": ["Jez", "R\u00fcm\u00b7mech\u00b7r\u00fcs\u00b7li\u00b7ger", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "die allersch\u00f6nste, woni ha,", "tokens": ["die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", ",", "wo\u00b7ni", "ha", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2019s isch nummen au kei M\u00f6seli dra!", "tokens": ["'s", "isch", "num\u00b7men", "au", "kei", "M\u00f6\u00b7se\u00b7li", "dra", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "NE", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wer het sie sch\u00f6ner, wer?", "tokens": ["Wer", "het", "sie", "sch\u00f6\u00b7ner", ",", "wer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "$,", "PWS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u2019s isch wohr, es isch e Pracht,", "tokens": ["'s", "isch", "wohr", ",", "es", "isch", "e", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "was so en Oepfel lacht;", "tokens": ["was", "so", "en", "O\u00b7ep\u00b7fel", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und isch der Zucker-Beck e Ma,", "tokens": ["und", "isch", "der", "Zu\u00b7cke\u00b7rBeck", "e", "Ma", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "se mach er so ein, wenn er cha!", "tokens": ["se", "mach", "er", "so", "ein", ",", "wenn", "er", "cha", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Der lieb Gott het en gmacht.", "tokens": ["Der", "lieb", "Gott", "het", "en", "gmacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "FM", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was hani echt no meh?", "tokens": ["Was", "ha\u00b7ni", "echt", "no", "meh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ne Fazenetli wii\u00df und roth,", "tokens": ["Ne", "Fa\u00b7ze\u00b7net\u00b7li", "wii\u00df", "und", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und das eis vo de sch\u00f6ne.", "tokens": ["und", "das", "eis", "vo", "de", "sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "FM", "FM", "FM", "FM", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "O Chind vor bittre Thr\u00e4ne", "tokens": ["O", "Chind", "vor", "bitt\u00b7re", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "biwahr di Gott, biwahr di Gott!", "tokens": ["bi\u00b7wahr", "di", "Gott", ",", "bi\u00b7wahr", "di", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "$,", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und was isch meh do inn?", "tokens": ["Und", "was", "isch", "meh", "do", "inn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ne B\u00fcechli, Chind! \u2019s isch au no di;", "tokens": ["ne", "B\u00fcech\u00b7li", ",", "Chind", "!", "'s", "isch", "au", "no", "di", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$.", "PPER", "ADJD", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "i leg der sch\u00f6ni Helgeli dri,", "tokens": ["i", "leg", "der", "sch\u00f6\u00b7ni", "Hel\u00b7ge\u00b7li", "dri", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "und sch\u00f6ni Gibetli sin selber drinn.", "tokens": ["und", "sch\u00f6\u00b7ni", "Gi\u00b7bet\u00b7li", "sin", "sel\u00b7ber", "drinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Jez ch\u00f6nnti, traui, goh;", "tokens": ["Jez", "ch\u00f6nn\u00b7ti", ",", "trau\u00b7i", ",", "goh", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "es fehlt n\u00fct meh zum Gute \u2014", "tokens": ["es", "fehlt", "n\u00fct", "meh", "zum", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Potz tausig, no ne Ruthe!", "tokens": ["Potz", "tau\u00b7sig", ",", "no", "ne", "Ru\u00b7the", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Do isch sie scho, do isch sie scho!", "tokens": ["Do", "isch", "sie", "scho", ",", "do", "isch", "sie", "scho", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u2019s cha sy, sie frent di nit,", "tokens": ["'s", "cha", "sy", ",", "sie", "frent", "di", "nit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "$,", "PPER", "VVFIN", "NE", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2019s cha sy, sie haut der \u2019s V\u00fcdeli wund;", "tokens": ["'s", "cha", "sy", ",", "sie", "haut", "der", "'s", "V\u00fc\u00b7de\u00b7li", "wund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "$,", "PPER", "VVFIN", "ART", "PPER", "NE", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "doch witt nit anderst, sen ischs der gsund,", "tokens": ["doch", "witt", "nit", "an\u00b7derst", ",", "sen", "ischs", "der", "gsund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "de muesch nit, wenn d\u2019 nit witt.", "tokens": ["de", "mu\u00b7esch", "nit", ",", "wenn", "d'", "nit", "witt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKNEG", "$,", "KOUS", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und willschs nit anderst ha,", "tokens": ["Und", "will\u00b7schs", "nit", "an\u00b7derst", "ha", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKNEG", "ADV", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "in Gottis Name seig es drum!", "tokens": ["in", "Got\u00b7tis", "Na\u00b7me", "seig", "es", "drum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Muetter-Liebe isch zart und frumm,", "tokens": ["Doch", "Mu\u00b7et\u00b7ter\u00b7Lie\u00b7be", "isch", "zart", "und", "frumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "sie windet rothi Bendeli dri,", "tokens": ["sie", "win\u00b7det", "ro\u00b7thi", "Ben\u00b7de\u00b7li", "dri", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "und macht e Letschli dra.", "tokens": ["und", "macht", "e", "Let\u00b7schli", "dra", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Jez w\u00e4r er usstaffirt,", "tokens": ["Jez", "w\u00e4r", "er", "us\u00b7staf\u00b7firt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "und wie ne May-Baum ziert,", "tokens": ["und", "wie", "ne", "May\u00b7Baum", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und wenn bis fr\u00fceih der Tag verwacht,", "tokens": ["und", "wenn", "bis", "fr\u00fc\u00b7eih", "der", "Tag", "ver\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "het \u2019s Wienecht-Chindli alles gmacht.", "tokens": ["het", "'s", "Wienecht\u00b7Chind\u00b7li", "al\u00b7les", "gmacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "De nimmschs und danksch mer\u2019s nit;", "tokens": ["De", "nimm\u00b7schs", "und", "danksch", "mer's", "nit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "PIS", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Drum weisch nit, wer ders git;", "tokens": ["Drum", "weisch", "nit", ",", "wer", "ders", "git", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PTKNEG", "$,", "PWS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch mach