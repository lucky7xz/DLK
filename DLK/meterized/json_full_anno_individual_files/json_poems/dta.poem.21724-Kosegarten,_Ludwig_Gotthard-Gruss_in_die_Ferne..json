{"dta.poem.21724": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Gruss in die Ferne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Woher, o laues Wehen,", "tokens": ["Wo\u00b7her", ",", "o", "lau\u00b7es", "We\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das schmeichelnd um mich kost?", "tokens": ["Das", "schmei\u00b7chelnd", "um", "mich", "kost", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Streifst du von Jasmunds H\u00f6hen", "tokens": ["Streifst", "du", "von", "Jas\u00b7munds", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Daher aus fernem Ost?", "tokens": ["Da\u00b7her", "aus", "fer\u00b7nem", "Ost", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Versch\u00f6nt das Land der H\u00fcnen", "tokens": ["Ver\u00b7sch\u00f6nt", "das", "Land", "der", "H\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht itzt Elwinens Fuss?", "tokens": ["Nicht", "itzt", "El\u00b7wi\u00b7nens", "Fuss", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NE", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Ach bringst du von Elwinen", "tokens": ["Ach", "bringst", "du", "von", "El\u00b7wi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir etwa s\u00fcssen Gruss?", "tokens": ["Mir", "et\u00b7wa", "s\u00fcs\u00b7sen", "Gruss", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Flog sie vielleicht den H\u00fcgel", "tokens": ["Flog", "sie", "viel\u00b7leicht", "den", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und staunt den Lasurspiegel", "tokens": ["Und", "staunt", "den", "La\u00b7sur\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Meers anbetend an?", "tokens": ["Des", "Meers", "an\u00b7be\u00b7tend", "an", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sieht sie im Abendschimmer", "tokens": ["Sieht", "sie", "im", "A\u00b7bend\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr heimisch Wittow gl\u00fchn,", "tokens": ["Ihr", "hei\u00b7misch", "Wit\u00b7tow", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und fern im Spatrothschimmer", "tokens": ["Und", "fern", "im", "Spat\u00b7roth\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Arkona Funken spr&#252;hn?", "tokens": ["Ar\u00b7ko\u00b7na", "Fun\u00b7ken", "spr", "&#252;", "hn", "?"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$(", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich seh', ich seh' die Holde", "tokens": ["Ich", "seh'", ",", "ich", "seh'", "die", "Hol\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihrer Sch\u00f6nheit Licht.", "tokens": ["In", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Hespers mattem Golde", "tokens": ["In", "Hes\u00b7pers", "mat\u00b7tem", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie gl\u00e4nzt ihr Angesicht!", "tokens": ["Wie", "gl\u00e4nzt", "ihr", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schwer rollen ihre Locken.", "tokens": ["Schwer", "rol\u00b7len", "ih\u00b7re", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wild schwirrt des Huthes Band.", "tokens": ["Wild", "schwirrt", "des", "Hu\u00b7thes", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Gleich frischgefallnen Flocken", "tokens": ["Gleich", "frischge\u00b7fall\u00b7nen", "Flo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Fliesst blendend ihr Gewand.", "tokens": ["Fliesst", "blen\u00b7dend", "ihr", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Hauch der Abendk\u00fchle,", "tokens": ["Im", "Hauch", "der", "A\u00b7bend\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im sanften Abendlicht,", "tokens": ["Im", "sanf\u00b7ten", "A\u00b7bend\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verkl\u00e4rt vom Hochgef\u00fchle,", "tokens": ["Ver\u00b7kl\u00e4rt", "vom", "Hoch\u00b7ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie strahlt ihr Angesicht.", "tokens": ["Wie", "strahlt", "ihr", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In ihren Wimpern zittert", "tokens": ["In", "ih\u00b7ren", "Wim\u00b7pern", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der R\u00fchrung heil'ger Thau,", "tokens": ["Der", "R\u00fch\u00b7rung", "heil'\u00b7ger", "Thau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und eine Thr\u00e4n' umflittert", "tokens": ["Und", "ei\u00b7ne", "Thr\u00e4n'", "um\u00b7flit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Augen heitres Blau.", "tokens": ["Der", "Au\u00b7gen", "heit\u00b7res", "Blau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zur\u00fcck, o schmeichelnd Wehen,", "tokens": ["Zu\u00b7r\u00fcck", ",", "o", "schmei\u00b7chelnd", "We\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "FM", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das freundlich um mich kost!", "tokens": ["Das", "freund\u00b7lich", "um", "mich", "kost", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur\u00fcck zu Jasmunds H\u00f6hen,", "tokens": ["Zu\u00b7r\u00fcck", "zu", "Jas\u00b7munds", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum sanftgef\u00e4rbten Ost!", "tokens": ["Zum", "sanft\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Ost", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ber\u00fchr' Elwinens Wange", "tokens": ["Be\u00b7r\u00fchr'", "El\u00b7wi\u00b7nens", "Wan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit leichtem Zephyrkuss,", "tokens": ["Mit", "leich\u00b7tem", "Ze\u00b7phyr\u00b7kuss", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und fl\u00fcstr' ihr leis' und bange:", "tokens": ["Und", "fl\u00fcstr'", "ihr", "leis'", "und", "ban\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es ist des Dichters Gruss!", "tokens": ["Es", "ist", "des", "Dich\u00b7ters", "Gruss", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}