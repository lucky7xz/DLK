{"textgrid.poem.42204": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Die Hunde", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren einmal zwei Hunde,", "tokens": ["Es", "wa\u00b7ren", "ein\u00b7mal", "zwei", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie war das Herz ihnen schwer!", "tokens": ["Wie", "war", "das", "Herz", "ih\u00b7nen", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie liefen wohl eine Stunde", "tokens": ["Sie", "lie\u00b7fen", "wohl", "ei\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hintereinander her.", "tokens": ["Hin\u00b7ter\u00b7ein\u00b7an\u00b7der", "her", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Sie hofften, in liebendem Bunde", "tokens": ["Sie", "hoff\u00b7ten", ",", "in", "lie\u00b7ben\u00b7dem", "Bun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Werd ihnen leicht und frei,", "tokens": ["Werd", "ih\u00b7nen", "leicht", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und waren doch nur zwei Hunde,", "tokens": ["Und", "wa\u00b7ren", "doch", "nur", "zwei", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und keine H\u00fcndin dabei.", "tokens": ["Und", "kei\u00b7ne", "H\u00fcn\u00b7din", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Das ist die soziale Misere,", "tokens": ["Das", "ist", "die", "so\u00b7zi\u00b7a\u00b7le", "Mi\u00b7se\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die Sphinx in der Hundewelt,", "tokens": ["Die", "Sphinx", "in", "der", "Hun\u00b7de\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df man vom Hundeverkehre", "tokens": ["Da\u00df", "man", "vom", "Hun\u00b7de\u00b7ver\u00b7keh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die H\u00fcndinnen ferne h\u00e4lt.", "tokens": ["Die", "H\u00fcn\u00b7din\u00b7nen", "fer\u00b7ne", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die H\u00fcndinnen werden ja h\u00e4ufig", "tokens": ["Die", "H\u00fcn\u00b7din\u00b7nen", "wer\u00b7den", "ja", "h\u00e4u\u00b7fig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gleich nach der Geburt ers\u00e4uft,", "tokens": ["Gleich", "nach", "der", "Ge\u00b7burt", "er\u00b7s\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wird eine H\u00fcndin l\u00e4ufig,", "tokens": ["Und", "wird", "ei\u00b7ne", "H\u00fcn\u00b7din", "l\u00e4u\u00b7fig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verhindert man, da\u00df sie l\u00e4uft.", "tokens": ["Ver\u00b7hin\u00b7dert", "man", ",", "da\u00df", "sie", "l\u00e4uft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Man l\u00e4\u00dft sie aus ihrem Kerker", "tokens": ["Man", "l\u00e4\u00dft", "sie", "aus", "ih\u00b7rem", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tag und Nacht nicht heraus;", "tokens": ["Tag", "und", "Nacht", "nicht", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Knurrend liegt ", "tokens": ["Knur\u00b7rend", "liegt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Zu F\u00fc\u00dfen der Tochter vom Haus.", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "der", "Toch\u00b7ter", "vom", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Und zittert wohl mit den Knien,", "tokens": ["Und", "zit\u00b7tert", "wohl", "mit", "den", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zuckt mit den Lippen bisweilen,", "tokens": ["Zuckt", "mit", "den", "Lip\u00b7pen", "bis\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und beide denken an ihn.", "tokens": ["Und", "bei\u00b7de", "den\u00b7ken", "an", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wallt man im Familienvereine", "tokens": ["Wallt", "man", "im", "Fa\u00b7mi\u00b7li\u00b7en\u00b7ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPRART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sonntags vors Tor hinaus,", "tokens": ["Sonn\u00b7tags", "vors", "Tor", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APZR", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bella geht an der Leine", "tokens": ["Be\u00b7lla", "geht", "an", "der", "Lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zugleich mit der Tochter vom Haus.", "tokens": ["Zu\u00b7gleich", "mit", "der", "Toch\u00b7ter", "vom", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Hier r\u00fccken heran die Studenten,", "tokens": ["Hier", "r\u00fc\u00b7cken", "he\u00b7ran", "die", "Stu\u00b7den\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort naht sich Nero galant;", "tokens": ["Dort", "naht", "sich", "Ne\u00b7ro", "ga\u00b7lant", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie wird von beiden Enden", "tokens": ["Wie", "wird", "von", "bei\u00b7den", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die arme Leine gespannt!", "tokens": ["Die", "ar\u00b7me", "Lei\u00b7ne", "ge\u00b7spannt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "In einem Rudel Hunde", "tokens": ["In", "ei\u00b7nem", "Ru\u00b7del", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam schlie\u00dflich man \u00fcberein,", "tokens": ["Kam", "schlie\u00df\u00b7lich", "man", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es m\u00f6ge nun in der Runde", "tokens": ["Es", "m\u00f6\u00b7ge", "nun", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jeder mal H\u00fcndin sein.", "tokens": ["Je\u00b7der", "mal", "H\u00fcn\u00b7din", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "NN", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Das Auge, angstvoll, tr\u00fcbe,", "tokens": ["Das", "Au\u00b7ge", ",", "angst\u00b7voll", ",", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schweift ferne zum Horizont,", "tokens": ["Schweift", "fer\u00b7ne", "zum", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als spr\u00e4ch's: Und das hat der Liebe", "tokens": ["Als", "spr\u00e4ch's", ":", "Und", "das", "hat", "der", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$.", "KON", "PDS", "VAFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Himmlische Macht gekonnt.", "tokens": ["Himm\u00b7li\u00b7sche", "Macht", "ge\u00b7konnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Der kleine Fritz ging vor\u00fcber", "tokens": ["Der", "klei\u00b7ne", "Fritz", "ging", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PTKVZ"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sagte: \u00bbLieber Papa,", "tokens": ["Und", "sag\u00b7te", ":", "\u00bb", "Lie\u00b7ber", "Pa\u00b7pa", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADJD", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sage mir doch, du Lieber,", "tokens": ["Sa\u00b7ge", "mir", "doch", ",", "du", "Lie\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Was machen die Hunde da?\u00ab", "tokens": ["Was", "ma\u00b7chen", "die", "Hun\u00b7de", "da", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Papa entgegnet: \u00bbDas nennt man,", "tokens": ["Pa\u00b7pa", "ent\u00b7geg\u00b7net", ":", "\u00bb", "Das", "nennt", "man", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "$(", "PDS", "VVFIN", "PIS", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Darf dir nicht sagen wie;", "tokens": ["Darf", "dir", "nicht", "sa\u00b7gen", "wie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "KOKOM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An diesen Greueln erkennt man", "tokens": ["An", "die\u00b7sen", "Greu\u00b7eln", "er\u00b7kennt", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PIS"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das lausige Hundevieh.\