{"dta.poem.12999": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leanders gegen-antwort.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.57", "af:0.28", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leanders poesie lag auf dem krancken-bette,", "tokens": ["Le\u00b7an\u00b7ders", "po\u00b7e\u00b7sie", "lag", "auf", "dem", "kran\u00b7cken\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sann, nach schwanen art, schon auf ein sterbe-lied:", "tokens": ["Und", "sann", ",", "nach", "schwa\u00b7nen", "art", ",", "schon", "auf", "ein", "ster\u00b7be\u00b7lied", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es drang der kalte schwei\u00df fast durch ein iedes glied:", "tokens": ["Es", "drang", "der", "kal\u00b7te", "schwei\u00df", "fast", "durch", "ein", "ie\u00b7des", "glied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "VVFIN", "ADV", "APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man wuste keinen artzt, der ihr gerathen h\u00e4tte.", "tokens": ["Man", "wus\u00b7te", "kei\u00b7nen", "artzt", ",", "der", "ihr", "ge\u00b7ra\u00b7then", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bi\u00df deine Muse sich, o himmlische Florette!", "tokens": ["Bi\u00df", "dei\u00b7ne", "Mu\u00b7se", "sich", ",", "o", "himm\u00b7li\u00b7sche", "Flo\u00b7ret\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit einer schnellen cur zu ihrem heil bem\u00fcht.", "tokens": ["Mit", "ei\u00b7ner", "schnel\u00b7len", "cur", "zu", "ih\u00b7rem", "heil", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn als dein schreiben ihr kaum in die hand gerieth,", "tokens": ["Denn", "als", "dein", "schrei\u00b7ben", "ihr", "kaum", "in", "die", "hand", "ge\u00b7rieth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So brach, ich wei\u00df nicht wie, Morbonens harte kette.", "tokens": ["So", "brach", ",", "ich", "wei\u00df", "nicht", "wie", ",", "Mor\u00b7bo\u00b7nens", "har\u00b7te", "ket\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Der nebel, so bereits der augen licht umfieng.", "tokens": ["Der", "ne\u00b7bel", ",", "so", "be\u00b7reits", "der", "au\u00b7gen", "licht", "um\u00b7fi\u00b7eng", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Die k\u00e4lte, so sich schon an hertz und adern hieng,", "tokens": ["Die", "k\u00e4l\u00b7te", ",", "so", "sich", "schon", "an", "hertz", "und", "a\u00b7dern", "hieng", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "PRF", "ADV", "APPR", "NN", "KON", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verlohr sich, als ein blitz, vor deinen sch\u00f6nen versen.", "tokens": ["Ver\u00b7lohr", "sich", ",", "als", "ein", "blitz", ",", "vor", "dei\u00b7nen", "sch\u00f6\u00b7nen", "ver\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Komm nun, und leugne mehr, Florette! da\u00df dein geist,", "tokens": ["Komm", "nun", ",", "und", "leug\u00b7ne", "mehr", ",", "Flo\u00b7ret\u00b7te", "!", "da\u00df", "dein", "geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "VVFIN", "ADV", "$,", "NN", "$.", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der krancken Musen hilfft und sie dem grab entrei\u00dft,", "tokens": ["Der", "kran\u00b7cken", "Mu\u00b7sen", "hilfft", "und", "sie", "dem", "grab", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie Ph\u00f6bus, f\u00e4hig sey den Pindus zu beherrschen.", "tokens": ["Wie", "Ph\u00f6\u00b7bus", ",", "f\u00e4\u00b7hig", "sey", "den", "Pin\u00b7dus", "zu", "be\u00b7herr\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ADJD", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}