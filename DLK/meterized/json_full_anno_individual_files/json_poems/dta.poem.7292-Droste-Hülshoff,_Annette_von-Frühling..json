{"dta.poem.7292": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Die Rebe bl\u00fcht, ihr linder Hauch", "tokens": ["Die", "Re\u00b7be", "bl\u00fcht", ",", "ihr", "lin\u00b7der", "Hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchzieht das thauige Revier,", "tokens": ["Durch\u00b7zieht", "das", "thau\u00b7i\u00b7ge", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und nah' und ferne wiegt die Luft", "tokens": ["Und", "nah'", "und", "fer\u00b7ne", "wiegt", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielfarb'ger Blumen bunte Zier.", "tokens": ["Viel\u00b7fa\u00b7rb'\u00b7ger", "Blu\u00b7men", "bun\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie's um mich gaukelt, wie es summt", "tokens": ["Wie's", "um", "mich", "gau\u00b7kelt", ",", "wie", "es", "summt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Vogel, Bien' und Schmetterling,", "tokens": ["Von", "Vo\u00b7gel", ",", "Bi\u00b7en'", "und", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie seine seidnen Wimpel regt", "tokens": ["Wie", "sei\u00b7ne", "seid\u00b7nen", "Wim\u00b7pel", "regt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zweig, so j\u00fcngst voll Reifen hing.", "tokens": ["Der", "Zweig", ",", "so", "j\u00fcngst", "voll", "Rei\u00b7fen", "hing", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch sucht man gern den Sonnenschein", "tokens": ["Noch", "sucht", "man", "gern", "den", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nimmt die trocknen Pl\u00e4tzchen ein;", "tokens": ["Und", "nimmt", "die", "trock\u00b7nen", "Pl\u00e4tz\u00b7chen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Nachts schleicht an die Gr\u00e4nze doch", "tokens": ["Denn", "Nachts", "schleicht", "an", "die", "Gr\u00e4n\u00b7ze", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der landesfl\u00fccht'ge Winter noch.", "tokens": ["Der", "lan\u00b7des\u00b7fl\u00fccht'\u00b7ge", "Win\u00b7ter", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O du mein ernst gewalt'ger Greis,", "tokens": ["O", "du", "mein", "ernst", "ge\u00b7walt'\u00b7ger", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein S\u00e4ntis mit der Locke wei\u00df!", "tokens": ["Mein", "S\u00e4n\u00b7tis", "mit", "der", "Lo\u00b7cke", "wei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Felsenbl\u00f6cke eingemauert,", "tokens": ["In", "Fel\u00b7sen\u00b7bl\u00f6\u00b7cke", "ein\u00b7ge\u00b7mau\u00b7ert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Schneegest\u00f6ber \u00fcberschauert,", "tokens": ["Von", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "\u00fc\u00b7bersc\u00b7hau\u00b7ert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Eisespanzer eingeschn\u00fcrt:", "tokens": ["In", "Ei\u00b7se\u00b7span\u00b7zer", "ein\u00b7ge\u00b7schn\u00fcrt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hu! wie dich schaudert, wie dich friert!", "tokens": ["Hu", "!", "wie", "dich", "schau\u00b7dert", ",", "wie", "dich", "friert", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}