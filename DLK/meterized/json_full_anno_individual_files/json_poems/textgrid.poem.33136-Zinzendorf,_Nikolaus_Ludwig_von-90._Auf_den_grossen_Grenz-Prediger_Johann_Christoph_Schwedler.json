{"textgrid.poem.33136": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "90. Auf den grossen Grenz-Prediger Johann Christoph Schwedler", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgens von der Arbeit gehn, das ist eines Menschen Weise,", "tokens": ["Mor\u00b7gens", "von", "der", "Ar\u00b7beit", "gehn", ",", "das", "ist", "ei\u00b7nes", "Men\u00b7schen", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der sich, wie der treue Schwedler, als ein Licht verzehren will:", "tokens": ["Der", "sich", ",", "wie", "der", "treu\u00b7e", "Schwed\u00b7ler", ",", "als", "ein", "Licht", "ver\u00b7zeh\u00b7ren", "will", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ihm war Tag und Nacht bequem, Gott zu Dienst, dem Herrn zum Preise;", "tokens": ["Ihm", "war", "Tag", "und", "Nacht", "be\u00b7quem", ",", "Gott", "zu", "Dienst", ",", "dem", "Herrn", "zum", "Prei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "ADJD", "$,", "NN", "APPR", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "K\u00fcrzlich ist er eingeschlafen, und liegt noch die Stunde still.", "tokens": ["K\u00fcrz\u00b7lich", "ist", "er", "ein\u00b7ge\u00b7schla\u00b7fen", ",", "und", "liegt", "noch", "die", "Stun\u00b7de", "still", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ohne Zweifel ist der Knecht unter seiner Last erlegen,", "tokens": ["Oh\u00b7ne", "Zwei\u00b7fel", "ist", "der", "Knecht", "un\u00b7ter", "sei\u00b7ner", "Last", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und der Schlaf wird lange w\u00e4hren, weil er eben viel gewacht.", "tokens": ["Und", "der", "Schlaf", "wird", "lan\u00b7ge", "w\u00e4h\u00b7ren", ",", "weil", "er", "e\u00b7ben", "viel", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schlaf, du m\u00fcder Arbeits-Mann! Schlaf Mit Jesu Christi Segen,", "tokens": ["Schlaf", ",", "du", "m\u00fc\u00b7der", "Ar\u00b7beits\u00b7Mann", "!", "Schlaf", "Mit", "Je\u00b7su", "Chris\u00b7ti", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$.", "NN", "APPR", "NE", "NE", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Deine Ruhe-Stunde schl\u00e4get. Schlaf! du hast genug gemacht.", "tokens": ["Dei\u00b7ne", "Ru\u00b7he\u00b7Stun\u00b7de", "schl\u00e4\u00b7get", ".", "Schlaf", "!", "du", "hast", "ge\u00b7nug", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Wie dann: Hat sich Schwedlers Haupt endlich in den Schlaf gefunden,", "tokens": ["Wie", "dann", ":", "Hat", "sich", "Schwed\u00b7lers", "Haupt", "end\u00b7lich", "in", "den", "Schlaf", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "VAFIN", "PRF", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-++-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sinkt sein ausgeh\u00e4rtet Leben auch einmal zur Ruhe hin?", "tokens": ["Sinkt", "sein", "aus\u00b7ge\u00b7h\u00e4r\u00b7tet", "Le\u00b7ben", "auch", "ein\u00b7mal", "zur", "Ru\u00b7he", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VVFIN", "NN", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Freilich! so hat auch der Held Jesus Christus \u00fcberwunden,", "tokens": ["Frei\u00b7lich", "!", "so", "hat", "auch", "der", "Held", "Je\u00b7sus", "Chris\u00b7tus", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VAFIN", "ADV", "ART", "NN", "NE", "NE", "VVPP", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Er, der Arbeitsleute Schaffner, Er, der