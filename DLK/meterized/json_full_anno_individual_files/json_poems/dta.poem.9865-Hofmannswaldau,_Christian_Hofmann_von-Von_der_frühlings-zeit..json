{"dta.poem.9865": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der fr\u00fchlings-zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kanst dein eigenthum ausbreiten/", "tokens": ["Du", "kanst", "dein", "ei\u00b7gen\u00b7thum", "aus\u00b7brei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O gr\u00fcner lentz! in voller pracht:", "tokens": ["O", "gr\u00fc\u00b7ner", "lentz", "!", "in", "vol\u00b7ler", "pracht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An der uns seine weise macht", "tokens": ["An", "der", "uns", "sei\u00b7ne", "wei\u00b7se", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sch\u00f6pffer zeigt auf allen seiten/", "tokens": ["Der", "Sch\u00f6pf\u00b7fer", "zeigt", "auf", "al\u00b7len", "sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn deine liebliche gestalt", "tokens": ["Wenn", "dei\u00b7ne", "lieb\u00b7li\u00b7che", "ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergn\u00fcgt das feld/ den berg und wald.", "tokens": ["Ver\u00b7gn\u00fcgt", "das", "feld", "/", "den", "berg", "und", "wald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast die grosse welt gebohren/", "tokens": ["Du", "hast", "die", "gros\u00b7se", "welt", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den anfang aller zeit gemacht:", "tokens": ["Den", "an\u00b7fang", "al\u00b7ler", "zeit", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das beyl den menschen wieder bracht/", "tokens": ["Das", "beyl", "den", "men\u00b7schen", "wie\u00b7der", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als zu der mutter ward erkohren", "tokens": ["Als", "zu", "der", "mut\u00b7ter", "ward", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die jungfrau/ deren wunder-sohn", "tokens": ["Die", "jung\u00b7frau", "/", "de\u00b7ren", "wun\u00b7der\u00b7sohn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "$(", "PRELAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sitzt auf des Allerh\u00f6chsten thron.", "tokens": ["Sitzt", "auf", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Du sahest ihn im grabe liegen/", "tokens": ["Du", "sa\u00b7hest", "ihn", "im", "gra\u00b7be", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der an dem creutze war verblast/", "tokens": ["Der", "an", "dem", "creut\u00b7ze", "war", "ver\u00b7blast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nach abgelegter todes-last/", "tokens": ["Nach", "ab\u00b7ge\u00b7leg\u00b7ter", "to\u00b7des\u00b7last", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hoch \u00fcber seine feinde siegen.", "tokens": ["Hoch", "\u00fc\u00b7ber", "sei\u00b7ne", "fein\u00b7de", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dir ist die reine seelen-lust", "tokens": ["Dir", "ist", "die", "rei\u00b7ne", "see\u00b7len\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der Christen heiligthum bewust.", "tokens": ["Der", "Chris\u00b7ten", "hei\u00b7lig\u00b7thum", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dein mertz wei\u00df alles zu erfreuen/", "tokens": ["Dein", "mertz", "wei\u00df", "al\u00b7les", "zu", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Was erde/ wafser/ lu", "tokens": ["Was", "er\u00b7de", "/", "waf\u00b7ser", "/", "lu"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PWS", "NN", "$(", "NN", "$(", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Ob dein april sich offt verstellt/", "tokens": ["Ob", "dein", "ap\u00b7ril", "sich", "offt", "ver\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Pflegt er die welt doch zu verneuen.", "tokens": ["Pflegt", "er", "die", "welt", "doch", "zu", "ver\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Lehrt nicht dein allersch\u00f6nster may/", "tokens": ["Lehrt", "nicht", "dein", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ter", "may", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Da\u00df er der beste mahler sey?", "tokens": ["Da\u00df", "er", "der", "bes\u00b7te", "mah\u00b7ler", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die warmen sonnen-strahlen m\u00fcssen", "tokens": ["Die", "war\u00b7men", "son\u00b7nen\u00b7strah\u00b7len", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Dir zu der wollust dienlich seyn/", "tokens": ["Dir", "zu", "der", "wol\u00b7lust", "dien\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wenn iedes thier f\u00fchlt s\u00fcsse pein/", "tokens": ["Wenn", "ie\u00b7des", "thier", "f\u00fchlt", "s\u00fcs\u00b7se", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Das/ vielmahl deiner zu geniessen/", "tokens": ["Das", "/", "viel\u00b7mahl", "dei\u00b7ner", "zu", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPOSAT", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die von natur erweckte brunst", "tokens": ["Die", "von", "na\u00b7tur", "er\u00b7weck\u00b7te", "brunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Lehrt die gemeine liebes-kunst.", "tokens": ["Lehrt", "die", "ge\u00b7mei\u00b7ne", "lie\u00b7bes\u00b7kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}