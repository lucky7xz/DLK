{"dta.poem.14332": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Auf meines Kindes Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das Kindlein spielt' drau\u00dfen im Fr\u00fchlingsschein", "tokens": ["Das", "Kin\u00b7dlein", "spielt'", "drau\u00b7\u00dfen", "im", "Fr\u00fch\u00b7lings\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und freut sich und hatte so viel zu sehen,", "tokens": ["Und", "freut", "sich", "und", "hat\u00b7te", "so", "viel", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KON", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie die Felder schimmern und die Str\u00f6me gehen \u2014", "tokens": ["Wie", "die", "Fel\u00b7der", "schim\u00b7mern", "und", "die", "Str\u00f6\u00b7me", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Da sah der Abend durch die B\u00e4ume herein,", "tokens": ["Da", "sah", "der", "A\u00b7bend", "durch", "die", "B\u00e4u\u00b7me", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Der alle die sch\u00f6nen Bilder verwirrt.", "tokens": ["Der", "al\u00b7le", "die", "sch\u00f6\u00b7nen", "Bil\u00b7der", "ver\u00b7wirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und wie es nun ringsum so stille wird,", "tokens": ["Und", "wie", "es", "nun", "ring\u00b7sum", "so", "stil\u00b7le", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ADV", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Beginnt aus den Th\u00e4lern ein heimlich Singen,", "tokens": ["Be\u00b7ginnt", "aus", "den", "Th\u00e4\u00b7lern", "ein", "heim\u00b7lich", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Als wollt's mit Wemuth die Welt umschlingen,", "tokens": ["Als", "wollt's", "mit", "We\u00b7muth", "die", "Welt", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Farben vergeh'n und die Erde wird bla\u00df.", "tokens": ["Die", "Far\u00b7ben", "ver\u00b7geh'n", "und", "die", "Er\u00b7de", "wird", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Voll Staunen fragt's Kindlein: ach, was ist das?", "tokens": ["Voll", "Stau\u00b7nen", "fragt's", "Kin\u00b7dlein", ":", "ach", ",", "was", "ist", "das", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJA", "NN", "$.", "XY", "$,", "PWS", "VAFIN", "PDS", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und legt sich tr\u00e4umend in's s\u00e4uselnde Gras;", "tokens": ["Und", "legt", "sich", "tr\u00e4u\u00b7mend", "in's", "s\u00e4u\u00b7seln\u00b7de", "Gras", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Da r\u00fchren die Blumen ihm k\u00fchle an's Herz", "tokens": ["Da", "r\u00fch\u00b7ren", "die", "Blu\u00b7men", "ihm", "k\u00fch\u00b7le", "an's", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Und l\u00e4chelnd f\u00fchlt es so s\u00fc\u00dfen Schmerz,", "tokens": ["Und", "l\u00e4\u00b7chelnd", "f\u00fchlt", "es", "so", "s\u00fc\u00b7\u00dfen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und die Erde, die Mutter so sch\u00f6n und bleich,", "tokens": ["Und", "die", "Er\u00b7de", ",", "die", "Mut\u00b7ter", "so", "sch\u00f6n", "und", "bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.15": {"text": "K\u00fc\u00dft das Kindlein und l\u00e4\u00dft's nicht los,", "tokens": ["K\u00fc\u00dft", "das", "Kin\u00b7dlein", "und", "l\u00e4\u00dft's", "nicht", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Zieht es herzinnig in ihren Schoo\u00df", "tokens": ["Zieht", "es", "her\u00b7zin\u00b7nig", "in", "ih\u00b7ren", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Und bettet es drunten gar warm und weich", "tokens": ["Und", "bet\u00b7tet", "es", "drun\u00b7ten", "gar", "warm", "und", "weich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Still unter Blumen und Moos. \u2014", "tokens": ["Still", "un\u00b7ter", "Blu\u00b7men", "und", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}