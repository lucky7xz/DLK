{"dta.poem.6614": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr wen geschicht nun dies? Der Himmel und das Licht,", "tokens": ["F\u00fcr", "wen", "ge\u00b7schicht", "nun", "dies", "?", "Der", "Him\u00b7mel", "und", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVPP", "ADV", "PDS", "$.", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr wen verbreiten sie den hellen Glantz und Schimmer?", "tokens": ["F\u00fcr", "wen", "ver\u00b7brei\u00b7ten", "sie", "den", "hel\u00b7len", "Glantz", "und", "Schim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie h\u00e4tten ihren Lauff ja ewiglich und immer", "tokens": ["Sie", "h\u00e4t\u00b7ten", "ih\u00b7ren", "Lauff", "ja", "e\u00b7wig\u00b7lich", "und", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In eitler Emsigkeit verricht\u2019t;", "tokens": ["In", "eit\u00b7ler", "Em\u00b7sig\u00b7keit", "ver\u00b7richt't", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man h\u00e4tte keinen Stoff und kein Bewegen", "tokens": ["Man", "h\u00e4t\u00b7te", "kei\u00b7nen", "Stoff", "und", "kein", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In ihnen sehn und unterscheiden m\u00f6gen.", "tokens": ["In", "ih\u00b7nen", "sehn", "und", "un\u00b7ter\u00b7schei\u00b7den", "m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ja selber die Natur, in allen ihren Wercken,", "tokens": ["Ja", "sel\u00b7ber", "die", "Na\u00b7tur", ",", "in", "al\u00b7len", "ih\u00b7ren", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "K\u00f6nnt ohne Geist kein\u2019 Aenderung vermercken:", "tokens": ["K\u00f6nnt", "oh\u00b7ne", "Geist", "kein'", "A\u00b7en\u00b7de\u00b7rung", "ver\u00b7mer\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Der gantze Welt-Creis l\u00e4g\u2019 in seiner ersten Nacht,", "tokens": ["Der", "gant\u00b7ze", "Welt\u00b7Creis", "l\u00e4g'", "in", "sei\u00b7ner", "ers\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als w\u00e4r er nie hervor gebracht.", "tokens": ["Als", "w\u00e4r", "er", "nie", "her\u00b7vor", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}