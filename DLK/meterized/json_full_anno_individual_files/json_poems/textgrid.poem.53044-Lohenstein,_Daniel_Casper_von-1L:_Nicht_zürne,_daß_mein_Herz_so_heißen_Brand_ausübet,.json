{"textgrid.poem.53044": {"metadata": {"author": {"name": "Lohenstein, Daniel Casper von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht z\u00fcrne, da\u00df mein Herz so hei\u00dfen Brand aus\u00fcbet,", "genre": "verse", "period": "N.A.", "pub_year": 1659, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht z\u00fcrne, da\u00df mein Herz so hei\u00dfen Brand aus\u00fcbet,", "tokens": ["Nicht", "z\u00fcr\u00b7ne", ",", "da\u00df", "mein", "Herz", "so", "hei\u00b7\u00dfen", "Brand", "aus\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil deine Sch\u00f6nheit selbst der Flammen Zunder hegt,", "tokens": ["Weil", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "selbst", "der", "Flam\u00b7men", "Zun\u00b7der", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schuld und Entschuldigung in ihren Augen tr\u00e4gt;", "tokens": ["Schuld", "und", "Ent\u00b7schul\u00b7di\u00b7gung", "in", "ih\u00b7ren", "Au\u00b7gen", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Das Meer kann nicht daf\u00fcr, da\u00df sich der Himmel tr\u00fcbet,", "tokens": ["Das", "Meer", "kann", "nicht", "da\u00b7f\u00fcr", ",", "da\u00df", "sich", "der", "Him\u00b7mel", "tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", 