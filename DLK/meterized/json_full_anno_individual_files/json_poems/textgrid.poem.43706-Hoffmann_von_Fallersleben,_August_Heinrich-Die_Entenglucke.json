{"textgrid.poem.43706": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Entenglucke", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Glucke geht am Ufer", "tokens": ["Die", "Glu\u00b7cke", "geht", "am", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ihrer Entenschaar,", "tokens": ["Mit", "ih\u00b7rer", "En\u00b7ten\u00b7schaar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmt ihr Amt als Mutter", "tokens": ["Und", "nimmt", "ihr", "Amt", "als", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach allen Seiten wahr.", "tokens": ["Nach", "al\u00b7len", "Sei\u00b7ten", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch mag sie immer gackern", "tokens": ["Doch", "mag", "sie", "im\u00b7mer", "ga\u00b7ckern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und noch so \u00e4ngstlich schrei'n,", "tokens": ["Und", "noch", "so", "\u00e4ngst\u00b7lich", "schrei'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Enten laufen alle", "tokens": ["Die", "E