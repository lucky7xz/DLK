{"textgrid.poem.52889": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort, wo kein Baum der frommen Trauer", "tokens": ["Dort", ",", "wo", "kein", "Baum", "der", "from\u00b7men", "Trau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verlassne H\u00fcgel gr\u00fcn belaubt,", "tokens": ["Ver\u00b7lass\u00b7ne", "H\u00fc\u00b7gel", "gr\u00fcn", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort ruht, dicht an der Kirchhofs-Mauer,", "tokens": ["Dort", "ruht", ",", "dicht", "an", "der", "Kirch\u00b7hofs\u00b7Mau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruht meines Vaters heilig' Haupt.", "tokens": ["Ruht", "mei\u00b7nes", "Va\u00b7ters", "hei\u00b7lig'", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum sie ihn so weit gebettet", "tokens": ["Wa\u00b7rum", "sie", "ihn", "so", "weit", "ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von guter Christen Lagerstatt?", "tokens": ["Von", "gu\u00b7ter", "Chris\u00b7ten", "La\u00b7ger\u00b7statt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil er, den andre nicht gerettet,", "tokens": ["Weil", "er", ",", "den", "and\u00b7re", "nicht", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuletzt sich selbst gerettet hat.", "tokens": ["Zu\u00b7letzt", "sich", "selbst", "ge\u00b7ret\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Weil er zum Dieb nicht werden mochte", "tokens": ["Weil", "er", "zum", "Dieb", "nicht", "wer\u00b7den", "moch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKNEG", "VAINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und weil dem Bettler niemand gab,", "tokens": ["Und", "weil", "dem", "Bett\u00b7ler", "nie\u00b7mand", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum schnitt er seinem Lebensdochte", "tokens": ["Drum", "schnitt", "er", "sei\u00b7nem", "Le\u00b7bens\u00b7doch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rasch selbst die tote Kohle ab.", "tokens": ["Rasch", "selbst", "die", "to\u00b7te", "Koh\u00b7le", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Selbstm\u00f6rdern streng den Stab zu brechen,", "tokens": ["Selbst\u00b7m\u00f6r\u00b7dern", "streng", "den", "Stab", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man warm sitzt im hohen Rat,", "tokens": ["Wenn", "man", "warm", "sitzt", "im", "ho\u00b7hen", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Von Feigheit und Verirrung sprechen,", "tokens": ["Von", "Feig\u00b7heit", "und", "Ver\u00b7ir\u00b7rung", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist, wahrlich! keine Heldentat.", "tokens": ["Ist", ",", "wahr\u00b7lich", "!", "kei\u00b7ne", "Hel\u00b7den\u00b7tat", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch w\u00fc\u00dftet Ihr, wie dem zu Mute,", "tokens": ["Doch", "w\u00fc\u00df\u00b7tet", "Ihr", ",", "wie", "dem", "zu", "Mu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "ART", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der, aller Erdenhoffnung quitt,", "tokens": ["Der", ",", "al\u00b7ler", "Er\u00b7den\u00b7hoff\u00b7nung", "quitt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fertig mit Gott, mit kaltem Blute", "tokens": ["Fer\u00b7tig", "mit", "Gott", ",", "mit", "kal\u00b7tem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In seine roten Adern schnitt:", "tokens": ["In", "sei\u00b7ne", "ro\u00b7ten", "A\u00b7dern", "schnitt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der nachts sich in die Wellen st\u00fcrzte,", "tokens": ["Der", "nachts", "sich", "in", "die", "Wel\u00b7len", "st\u00fcrz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem er lang am Ufer hing,", "tokens": ["Nach\u00b7dem", "er", "lang", "am", "U\u00b7fer", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der k\u00fcnstlich selbst die Schlinge sch\u00fcrzte,", "tokens": ["Der", "k\u00fcnst\u00b7lich", "selbst", "die", "Schlin\u00b7ge", "sch\u00fcrz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darin sein Atem sich verfing:", "tokens": ["Da\u00b7rin", "sein", "A\u00b7tem", "sich", "ver\u00b7fing", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "S\u00e4ht' Ihr, wie reuig und erstarrend", "tokens": ["S\u00e4ht'", "Ihr", ",", "wie", "reu\u00b7ig", "und", "er\u00b7star\u00b7rend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Hand nach einem Halme griff,", "tokens": ["Die", "Hand", "nach", "ei\u00b7nem", "Hal\u00b7me", "griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie die Kehle, rettungs-harrend,", "tokens": ["Und", "wie", "die", "Keh\u00b7le", ",", "ret\u00b7tungs\u00b7har\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach ferner Hilfe krampfhaft rief: \u2013 \u2013", "tokens": ["Nach", "fer\u00b7ner", "Hil\u00b7fe", "krampf\u00b7haft", "rief", ":", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$.", "$(", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Ihr w\u00e4ret lasser im Verdammen", "tokens": ["Ihr", "w\u00e4\u00b7ret", "las\u00b7ser", "im", "Ver\u00b7dam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und littet wohl in Majest\u00e4t,", "tokens": ["Und", "lit\u00b7tet", "wohl", "in", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df solche Blumen nah beisammen", "tokens": ["Da\u00df", "sol\u00b7che", "Blu\u00b7men", "nah", "bei\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Modern mit den', so Gott gem\u00e4ht!", "tokens": ["Mo\u00b7dern", "mit", "den'", ",", "so", "Gott", "ge\u00b7m\u00e4ht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDS", "$,", "ADV", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Sie haben keinen Stein gegeben,", "tokens": ["Sie", "ha\u00b7ben", "kei\u00b7nen", "Stein", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Mal, mein armer Vater, dir,", "tokens": ["Kein", "Mal", ",", "mein", "ar\u00b7mer", "Va\u00b7ter", ",", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch warest du im Leben", "tokens": ["Und", "den\u00b7noch", "wa\u00b7rest", "du", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Mann wie wenig M\u00e4nner hier.", "tokens": ["Ein", "Mann", "wie", "we\u00b7nig", "M\u00e4n\u00b7ner", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Gleichviel! Ich finde doch die Pfade", "tokens": ["Gleich\u00b7viel", "!", "Ich", "fin\u00b7de", "doch", "die", "Pfa\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deines Grabes Nesselbeet,", "tokens": ["Zu", "dei\u00b7nes", "Gra\u00b7bes", "Nes\u00b7sel\u00b7beet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn gleich kein Kreuz mit \u00bbGottes Gnade\u00ab", "tokens": ["Wenn", "gleich", "kein", "Kreuz", "mit", "\u00bb", "Got\u00b7tes", "Gna\u00b7de", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "APPR", "$(", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00bbSchlummre sanft\u00ab dar\u00fcbersteht.", "tokens": ["Und", "\u00bb", "Schlumm\u00b7re", "sanft", "\u00ab", "da\u00b7r\u00fc\u00b7bers\u00b7teht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$(", "NN", "ADJD", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dank deinem Leben, das gesch\u00e4ftig", "tokens": ["Dank", "dei\u00b7nem", "Le\u00b7ben", ",", "das", "ge\u00b7sch\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir keine Lehre schuldig blieb,", "tokens": ["Mir", "kei\u00b7ne", "Leh\u00b7re", "schul\u00b7dig", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dank deiner Hand, die allzukr\u00e4ftig", "tokens": ["Dank", "dei\u00b7ner", "Hand", ",", "die", "all\u00b7zu\u00b7kr\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie auf den jungen R\u00fccken schrieb!", "tokens": ["Sie", "auf", "den", "jun\u00b7gen", "R\u00fc\u00b7cken", "schrieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Dank deinem Tod, der ohne Worte", "tokens": ["Dank", "dei\u00b7nem", "Tod", ",", "der", "oh\u00b7ne", "Wor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir einen gro\u00dfen Trost verhie\u00df;", "tokens": ["Mir", "ei\u00b7nen", "gro\u00b7\u00dfen", "Trost", "ver\u00b7hie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er zeigt mir doch, an welchem Orte", "tokens": ["Er", "zeigt", "mir", "doch", ",", "an", "wel\u00b7chem", "Or\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Loch der Zimmermeister lie\u00df.", "tokens": ["Ein", "Loch", "der", "Zim\u00b7mer\u00b7meis\u00b7ter", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Dort, wo kein Baum der frommen Trauer", "tokens": ["Dort", ",", "wo", "kein", "Baum", "der", "from\u00b7men", "Trau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verlassne H\u00fcgel gr\u00fcn belaubt,", "tokens": ["Ver\u00b7lass\u00b7ne", "H\u00fc\u00b7gel", "gr\u00fcn", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort ruht, dicht an der Kirchhofs-Mauer,", "tokens": ["Dort", "ruht", ",", "dicht", "an", "der", "Kirch\u00b7hofs\u00b7Mau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruht meines Vaters heilig' Haupt.", "tokens": ["Ruht", "mei\u00b7nes", "Va\u00b7ters", "hei\u00b7lig'", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Warum sie ihn so weit gebettet", "tokens": ["Wa\u00b7rum", "sie", "ihn", "so", "weit", "ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von guter Christen Lagerstatt?", "tokens": ["Von", "gu\u00b7ter", "Chris\u00b7ten", "La\u00b7ger\u00b7statt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$