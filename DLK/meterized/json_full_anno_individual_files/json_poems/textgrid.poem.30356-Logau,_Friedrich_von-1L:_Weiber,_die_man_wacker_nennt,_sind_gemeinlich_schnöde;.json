{"textgrid.poem.30356": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weiber, die man wacker nennt, sind gemeinlich schn\u00f6de;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weiber, die man wacker nennt, sind gemeinlich schn\u00f6de;", "tokens": ["Wei\u00b7ber", ",", "die", "man", "wa\u00b7cker", "nennt", ",", "sind", "ge\u00b7mein\u00b7lich", "schn\u00f6\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Weiber, die man from beniemt, sind gemeinlich bl\u00f6de;", "tokens": ["Wei\u00b7ber", ",", "die", "man", "from", "be\u00b7niemt", ",", "sind", "ge\u00b7mein\u00b7lich", "bl\u00f6\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADJD", "VVPP", "$,", "VAFIN", "ADJD", "ADJA", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Weiber, die man wirthlich heist, sind gemeinlich b\u00f6se.", "tokens": ["Wei\u00b7ber", ",", "die", "man", "wirth\u00b7lich", "heist", ",", "sind", "ge\u00b7mein\u00b7lich", "b\u00f6\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADJD", "VAFIN", "$,", "VAFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Schwer ists, wie mans treffen soll, da\u00df mans recht aufl\u00f6se.", "tokens": ["Schwer", "ists", ",", "wie", "mans", "tref\u00b7fen", "soll", ",", "da\u00df", "mans", "recht", "auf\u00b7l\u00f6