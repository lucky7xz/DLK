{"textgrid.poem.61097": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es treibt mich stets ein wechselndes Verlangen:", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es treibt mich stets ein wechselndes Verlangen:", "tokens": ["Es", "treibt", "mich", "stets", "ein", "wech\u00b7seln\u00b7des", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald m\u00f6cht' ich unter meiner Heimat Linden", "tokens": ["Bald", "m\u00f6cht'", "ich", "un\u00b7ter", "mei\u00b7ner", "Hei\u00b7mat", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am eignen Herd ein schattig Pl\u00e4tzchen finden,", "tokens": ["Am", "eig\u00b7nen", "Herd", "ein", "schat\u00b7tig", "Pl\u00e4tz\u00b7chen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um dort zu rasten ohne Wunsch und Bangen;", "tokens": ["Um", "dort", "zu", "ras\u00b7ten", "oh\u00b7ne", "Wunsch", "und", "Ban\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PTKZU", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald wieder m\u00f6cht' ich, sonnverbrannt die Wangen,", "tokens": ["Bald", "wie\u00b7der", "m\u00f6cht'", "ich", ",", "sonn\u00b