{"textgrid.poem.45848": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll sich vor Dir des Baumes Stolz enth\u00fcllen,", "tokens": ["Soll", "sich", "vor", "Dir", "des", "Bau\u00b7mes", "Stolz", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nur allein sich selbsten aufw\u00e4rts strebet,", "tokens": ["Der", "nur", "al\u00b7lein", "sich", "selbs\u00b7ten", "auf\u00b7w\u00e4rts", "stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Busches Geist, der heil'ge Schatten webet,", "tokens": ["Des", "Bu\u00b7sches", "Geist", ",", "der", "heil'\u00b7ge", "Schat\u00b7ten", "we\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was der Blume zarte Kelche h\u00fcllen.", "tokens": ["Und", "was", "der", "Blu\u00b7me", "zar\u00b7te", "Kel\u00b7che", "h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So mu\u00dft Du alle laute Neugier stillen", "tokens": ["So", "mu\u00dft", "Du", "al\u00b7le", "lau\u00b7te", "Neu\u00b7gier", "stil\u