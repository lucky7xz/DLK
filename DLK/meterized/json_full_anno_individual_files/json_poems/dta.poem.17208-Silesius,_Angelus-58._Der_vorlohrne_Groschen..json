{"dta.poem.17208": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "58.  Der vorlohrne Groschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele GOttesbild ist der verlohrne Groschen/", "tokens": ["Die", "See\u00b7le", "Got\u00b7tes\u00b7bild", "ist", "der", "ver\u00b7lohr\u00b7ne", "Gro\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Kertze himmlisch Licht/ das durch den fall verloschen:", "tokens": ["Die", "Kert\u00b7ze", "himm\u00b7lisch", "Licht", "/", "das", "durch", "den", "fall", "ver\u00b7lo\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$(", "ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wei\u00dfheit ist das Weib die es aufs neu entz\u00fcndt:", "tokens": ["Die", "Wei\u00df\u00b7heit", "ist", "das", "Weib", "die", "es", "aufs", "neu", "ent\u00b7z\u00fcndt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "PPER", "APPRART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie seelig ist der Mensch den sie nu wider findt!", "tokens": ["Wie", "see\u00b7lig", "ist", "der", "Mensch", "den", "sie", "nu", "wi\u00b7der", "findt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "PPER", "ADV", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}