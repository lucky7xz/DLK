{"textgrid.poem.57278": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer erm\u00fcdet hinauf zu der Heerschaar der Gestirne,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer erm\u00fcdet hinauf zu der Heerschaar der Gestirne,", "tokens": ["Wer", "er\u00b7m\u00fc\u00b7det", "hin\u00b7auf", "zu", "der", "Heer\u00b7schaar", "der", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "In die H\u00f6hen zu schaun, wo der Lichtfuss sich herabsenkt,", "tokens": ["In", "die", "H\u00f6\u00b7hen", "zu", "schaun", ",", "wo", "der", "Licht\u00b7fuss", "sich", "her\u00b7ab\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wo den Blitzglanz Fomahant und Antar, wo des Leun Herz", "tokens": ["Wo", "den", "Blitz\u00b7glanz", "Fo\u00b7ma\u00b7hant", "und", "An\u00b7tar", ",", "wo", "des", "Leun", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-++-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Sich ergeusst, ins Gefild hin, wo die \u00c4hr' und die Winzerin strahlt!", "tokens": ["Sich", "er\u00b7geusst", ",", "ins", "Ge\u00b7fild", "hin", ",", "wo", "die", "\u00c4hr'", "und", "die", "Win\u00b7ze\u00b7rin", "strahlt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "APPRART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Mit Graun f\u00fcllt, und Ehrfurcht der Anblick, mit Entz\u00fcckung", "tokens": ["Mit", "Graun", "f\u00fcllt", ",", "und", "Ehr\u00b7furcht", "der", "An\u00b7blick", ",", "mit", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "NN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Herz dess, der sich da freut, wo Freud' ist, nicht allein ihn", "tokens": ["Das", "Herz", "dess", ",", "der", "sich", "da", "freut", ",", "wo", "Freud'", "ist", ",", "nicht", "al\u00b7lein", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,", "PWAV", "NN", "VAFIN", "$,", "PTKNEG", "ADV", "PPER"], "meter": "-+--+-+-+-++-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ihr Phantom t\u00e4uscht! Ich steh hier in dem Vorhof der Gottheit.", "tokens": ["Ihr", "Phan\u00b7tom", "t\u00e4uscht", "!", "Ich", "steh", "hier", "in", "dem", "Vor\u00b7hof", "der", "Got\u00b7theit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Befl\u00fcgelt von dem Tod' eilt mein Geist einst in den Tempel!", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", "von", "dem", "Tod'", "eilt", "mein", "Geist", "einst", "in", "den", "Tem\u00b7pel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Mitternacht, h\u00f6re du meinen Gesang, Morgenstern,", "tokens": ["Mit\u00b7ter\u00b7nacht", ",", "h\u00f6\u00b7re", "du", "mei\u00b7nen", "Ge\u00b7sang", ",", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Finde du preisend oft, dankend mich, Thr\u00e4nen im Blick,", "tokens": ["Fin\u00b7de", "du", "prei\u00b7send", "oft", ",", "dan\u00b7kend", "mich", ",", "Thr\u00e4\u00b7nen", "im", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ADV", "$,", "KOUS", "PPER", "$,", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Bote des Tags! Wirst du darauf Abendstern, find' auch dann", "tokens": ["Bo\u00b7te", "des", "Tags", "!", "Wirst", "du", "da\u00b7rauf", "A\u00b7bends\u00b7tern", ",", "find'", "auch", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "VAFIN", "PPER", "PAV", "NN", "$,", "VVFIN", "ADV", "ADV"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "\u00dcber Gott, den erstaunt, welcher sein Heil nie begreift!", "tokens": ["\u00dc\u00b7ber", "Gott", ",", "den", "er\u00b7staunt", ",", "wel\u00b7cher", "sein", "Heil", "nie", "be\u00b7greift", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Wer erm\u00fcdet hinauf zu der Heerschaar der Gestirne,", "tokens": ["Wer", "er\u00b7m\u00fc\u00b7det", "hin\u00b7auf", "zu", "der", "Heer\u00b7schaar", "der", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "In die H\u00f6hen zu schaun, wo der Lichtfuss sich herabsenkt,", "tokens": ["In", "die", "H\u00f6\u00b7hen", "zu", "schaun", ",", "wo", "der", "Licht\u00b7fuss", "sich", "her\u00b7ab\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wo den Blitzglanz Fomahant und Antar, wo des Leun Herz", "tokens": ["Wo", "den", "Blitz\u00b7glanz", "Fo\u00b7ma\u00b7hant", "und", "An\u00b7tar", ",", "wo", "des", "Leun", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-++-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Sich ergeusst, ins Gefild hin, wo die \u00c4hr' und die Winzerin strahlt!", "tokens": ["Sich", "er\u00b7geusst", ",", "ins", "Ge\u00b7fild", "hin", ",", "wo", "die", "\u00c4hr'", "und", "die", "Win\u00b7ze\u00b7rin", "strahlt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "wor