{"dta.poem.21304": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput III.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, ganz verschwand die Fuchtel nie,", "tokens": ["Ja", ",", "ganz", "ver\u00b7schwand", "die", "Fuch\u00b7tel", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie tragen sie jetzt im Innern;", "tokens": ["Sie", "tra\u00b7gen", "sie", "jetzt", "im", "In\u00b7nern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das trauliche Du wird immer noch", "tokens": ["Das", "trau\u00b7li\u00b7che", "Du", "wird", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An das alte Er erinnern.", "tokens": ["An", "das", "al\u00b7te", "Er", "e\u00b7rin\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der lange Schnurbart ist eigentlich nur", "tokens": ["Der", "lan\u00b7ge", "Schnur\u00b7bart", "ist", "ei\u00b7gent\u00b7lich", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Zopfthums neuere Phase:", "tokens": ["Des", "Zopf\u00b7thums", "neu\u00b7e\u00b7re", "Pha\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Zopf, der ehmals hinten hing,", "tokens": ["Der", "Zopf", ",", "der", "eh\u00b7mals", "hin\u00b7ten", "hing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der h\u00e4ngt jetzt unter der Nase.", "tokens": ["Der", "h\u00e4ngt", "jetzt", "un\u00b7ter", "der", "Na\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht \u00fcbel gefiel mir das neue Costum", "tokens": ["Nicht", "\u00fc\u00b7bel", "ge\u00b7fiel", "mir", "das", "neu\u00b7e", "Cos\u00b7tum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Reuter, das mu\u00df ich loben,", "tokens": ["Der", "Reu\u00b7ter", ",", "das", "mu\u00df", "ich", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Besonders die Pikkelhaube, den Helm,", "tokens": ["Be\u00b7son\u00b7ders", "die", "Pik\u00b7kel\u00b7hau\u00b7be", ",", "den", "Helm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit der st\u00e4hlernen Spitze nach oben.", "tokens": ["Mit", "der", "st\u00e4h\u00b7ler\u00b7nen", "Spit\u00b7ze", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Das ist so ritterth\u00fcmlich und mahnt", "tokens": ["Das", "ist", "so", "rit\u00b7tert\u00b7h\u00fcm\u00b7lich", "und", "mahnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "An der Vorzeit holde Romantik,", "tokens": ["An", "der", "Vor\u00b7zeit", "hol\u00b7de", "Ro\u00b7man\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "An die Burgfrau Johanna von Montfaucon,", "tokens": ["An", "die", "Burg\u00b7frau", "Jo\u00b7han\u00b7na", "von", "Mont\u00b7fau\u00b7con", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "An den Freyherrn Fouqu\u00e8, Uhland, Tieck.", "tokens": ["An", "den", "Frey\u00b7herrn", "Fou\u00b7qu\u00e8", ",", "Uh\u00b7land", ",", "Tieck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}