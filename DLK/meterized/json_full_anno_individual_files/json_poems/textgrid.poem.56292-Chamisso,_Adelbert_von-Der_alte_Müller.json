{"textgrid.poem.56292": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der alte M\u00fcller", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es w\u00fctet der Sturm mit entsetzlicher Macht,", "tokens": ["Es", "w\u00fc\u00b7tet", "der", "Sturm", "mit", "ent\u00b7setz\u00b7li\u00b7cher", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Windm\u00fchl schwankt, das Geb\u00e4lk erkracht.", "tokens": ["Die", "Wind\u00b7m\u00fchl", "schwankt", ",", "das", "Ge\u00b7b\u00e4lk", "er\u00b7kracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Der Meister ist nicht, der alte, zur Hand,", "tokens": ["Der", "Meis\u00b7ter", "ist", "nicht", ",", "der", "al\u00b7te", ",", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "$,", "ART", "ADJA", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er steht an der Felswand schwindlichem Rand.", "tokens": ["Er", "steht", "an", "der", "Fels\u00b7wand", "schwind\u00b7li\u00b7chem", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Da steht er allein, mit dem Winde vertraut,", "tokens": ["Da", "steht", "er", "al\u00b7lein", ",", "mit", "dem", "Win\u00b7de", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und spricht mit den L\u00fcften vernehmlich und laut.", "tokens": ["Und", "spricht", "mit", "den", "L\u00fcf\u00b7ten", "ver\u00b7nehm\u00b7lich", "und", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Er sch\u00fcttelt im Sturme sein wei\u00dfes Haar,", "tokens": ["Er", "sch\u00fct\u00b7telt", "im", "Stur\u00b7me", "sein", "wei\u00b7\u00dfes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und was er da spricht, klingt sonderbar.", "tokens": ["Und", "was", "er", "da", "spricht", ",", "klingt", "son\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Willkommen, willkommen, gro\u00dfm\u00e4chtiger Wind!", "tokens": ["Will\u00b7kom\u00b7men", ",", "will\u00b7kom\u00b7men", ",", "gro\u00df\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Wind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Was bringst du mir Neues, verk\u00fcnd es geschwind.", "tokens": ["Was", "bringst", "du", "mir", "Neu\u00b7es", ",", "ver\u00b7k\u00fcnd", "es", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Du hast mich gewiegt, du hast mich gen\u00e4hrt,", "tokens": ["Du", "hast", "mich", "ge\u00b7wiegt", ",", "du", "hast", "mich", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Du hast mich geliebt, du hast mich gelehrt.", "tokens": ["Du", "hast", "mich", "ge\u00b7liebt", ",", "du", "hast", "mich", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Du hast mir die Worte wohl hinterbracht,", "tokens": ["Du", "hast", "mir", "die", "Wor\u00b7te", "wohl", "hin\u00b7ter\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Worte der Weisheit, von Toren verlacht.", "tokens": ["Die", "Wor\u00b7te", "der", "Weis\u00b7heit", ",", "von", "To\u00b7ren", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Ihr Toren, ihr Toren, die fa\u00dftet ihr nicht,", "tokens": ["Ihr", "To\u00b7ren", ",", "ihr", "To\u00b7ren", ",", "die", "fa\u00df\u00b7tet", "ihr", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die fa\u00dfte der Wind auf, der gab mir Bericht.", "tokens": ["Die", "fa\u00df\u00b7te", "der", "Wind", "auf", ",", "der", "gab", "mir", "Be\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Das Wort wird Tat, das Kind wird Mann,", "tokens": ["Das", "Wort", "wird", "Tat", ",", "das", "Kind", "wird", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind wird Sturm, wer zweifelt daran?", "tokens": ["Der", "Wind", "wird", "Sturm", ",", "wer", "zwei\u00b7felt", "da\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "PWS", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Willkommen, willkommen, gro\u00dfm\u00e4chtiger Wind!", "tokens": ["Will\u00b7kom\u00b7men", ",", "will\u00b7kom\u00b7men", ",", "gro\u00df\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Wind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und was du auch bringest, vollend es geschwind.", "tokens": ["Und", "was", "du", "auch", "brin\u00b7gest", ",", "vol\u00b7lend", "es", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "VVPP", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Das Ma\u00df ist voll, die Zeit ist aus;", "tokens": ["Das", "Ma\u00df", "ist", "voll", ",", "die", "Zeit", "ist", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt kommt das Gericht in Zerst\u00f6rung und Graus.", "tokens": ["Jetzt", "kommt", "das", "Ge\u00b7richt", "in", "Zer\u00b7st\u00f6\u00b7rung", "und", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.12": {"line.1": {"text": "Ein Wirbelwind fa\u00dft den Alten zumal", "tokens": ["Ein", "Wir\u00b7bel\u00b7wind", "fa\u00dft", "den", "Al\u00b7ten", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schleudert zerschmettert ihn tief in das Tal.", "tokens": ["Und", "schleu\u00b7dert", "zer\u00b7schmet\u00b7tert", "ihn", "tief", "in", "das", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hilf, Himmel, erbarme dich unser!", "tokens": ["Hilf", ",", "Him\u00b7mel", ",", "er\u00b7bar\u00b7me", "dich", "un\