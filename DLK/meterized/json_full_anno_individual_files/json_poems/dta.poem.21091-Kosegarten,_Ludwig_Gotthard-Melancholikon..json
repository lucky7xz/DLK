{"dta.poem.21091": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Melancholikon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Himmelssonne,               ", "tokens": ["Sch\u00f6\u00b7ne", "Him\u00b7mels\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mild und hold und hehr,", "tokens": ["Mild", "und", "hold", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Urquell aller Wonne,", "tokens": ["Ur\u00b7quell", "al\u00b7ler", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wogend Flammenmeer!", "tokens": ["Wo\u00b7gend", "Flam\u00b7men\u00b7meer", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "So blass sind deine Schimmer!", "tokens": ["So", "blass", "sind", "dei\u00b7ne", "Schim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So matt sind deine Flimmer!", "tokens": ["So", "matt", "sind", "dei\u00b7ne", "Flim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Heldin, ist der K\u00f6cher", "tokens": ["Hel\u00b7din", ",", "ist", "der", "K\u00f6\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Deiner Pfeile leer?", "tokens": ["Dei\u00b7ner", "Pfei\u00b7le", "leer", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00d6de Stoppelfelder,", "tokens": ["\u00d6\u00b7de", "Stop\u00b7pel\u00b7fel\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Blumenarme Flur,", "tokens": ["Blu\u00b7men\u00b7ar\u00b7me", "Flur", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ausgestorbne W\u00e4lder,", "tokens": ["Aus\u00b7ge\u00b7storb\u00b7ne", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Siechende Natur,", "tokens": ["Sie\u00b7chen\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+", "measure": "dactylic.init"}, "line.5": {"text": "Woher so stumm und traurig?", "tokens": ["Wo\u00b7her", "so", "stumm", "und", "trau\u00b7rig", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Woher so bang' und schaurig?", "tokens": ["Wo\u00b7her", "so", "bang'", "und", "schau\u00b7rig", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Winket denn die Urne", "tokens": ["Win\u00b7ket", "denn", "die", "Ur\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Aller Kreatur?", "tokens": ["Al\u00b7ler", "Kre\u00b7a\u00b7tur", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auf den gr\u00fcnen Matten,", "tokens": ["Auf", "den", "gr\u00fc\u00b7nen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "L\u00e4ngs dem Wiesenbach,", "tokens": ["L\u00e4ngs", "dem", "Wie\u00b7sen\u00b7bach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo im Erlenschatten", "tokens": ["Wo", "im", "Er\u00b7len\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ich s\u00fcsstr\u00e4umend lag,", "tokens": ["Ich", "s\u00fcs\u00b7str\u00e4u\u00b7mend", "lag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wo Lieb' und Leben schwirrte,", "tokens": ["Wo", "Lieb'", "und", "Le\u00b7ben", "schwirr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und fl\u00f6tet, summt' und girrte \u2014", "tokens": ["Und", "fl\u00f6\u00b7tet", ",", "summt'", "und", "girr\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wimmert Todesklage,", "tokens": ["Wim\u00b7mert", "To\u00b7des\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "\u00c4chzt gebrochnes Ach!", "tokens": ["\u00c4chzt", "ge\u00b7broch\u00b7nes", "Ach", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schl\u00fcsselblumen schm\u00fcckten", "tokens": ["Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "schm\u00fcck\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Diess bescheidne Thal.", "tokens": ["Diess", "be\u00b7scheid\u00b7ne", "Thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wilde Rosen nickten", "tokens": ["Wil\u00b7de", "Ro\u00b7sen", "nick\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hier im lauen Strahl \u2014", "tokens": ["Hier", "im", "lau\u00b7en", "Strahl"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wo seyd ihr Trauten, Lieben,", "tokens": ["Wo", "seyd", "ihr", "Trau\u00b7ten", ",", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo seyd, wo seyd ihr blieben?", "tokens": ["Wo", "seyd", ",", "wo", "seyd", "ihr", "blie\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ehret eure Asche", "tokens": ["Eh\u00b7ret", "eu\u00b7re", "A\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Kein verk\u00fcndend Maal?", "tokens": ["Kein", "ver\u00b7k\u00fcn\u00b7dend", "Maal", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "VVPP", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Goldner Weizen kr\u00e4nzte", "tokens": ["Gold\u00b7ner", "Wei\u00b7zen", "kr\u00e4nz\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jene stolze H\u00f6h;", "tokens": ["Je\u00b7ne", "stol\u00b7ze", "H\u00f6h", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hier im Blachfeld gl\u00e4nzte", "tokens": ["Hier", "im", "Blach\u00b7feld", "gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Eine Halmensee.", "tokens": ["Ei\u00b7ne", "Hal\u00b7men\u00b7see", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich seh sie nicht mehr wallen.", "tokens": ["Ich", "seh", "sie", "nicht", "mehr", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gefallen, ach, gefallen", "tokens": ["Ge\u00b7fal\u00b7len", ",", "ach", ",", "ge\u00b7fal\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ITJ", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor dem Schwung der Sichel", "tokens": ["Vor", "dem", "Schwung", "der", "Si\u00b7chel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ist die Wogende.", "tokens": ["Ist", "die", "Wo\u00b7gen\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "Ahnung, die mich d\u00fcstert,", "tokens": ["Ah\u00b7nung", ",", "die", "mich", "d\u00fcs\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sprich, wo stammst du her?", "tokens": ["Sprich", ",", "wo", "stammst", "du", "her", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Stimme, die mir flistert,", "tokens": ["Stim\u00b7me", ",", "die", "mir", "flis\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sprich verst\u00e4ndlicher!", "tokens": ["Sprich", "ver\u00b7st\u00e4nd\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Die Sonn' ist untergangen;", "tokens": ["Die", "Sonn'", "ist", "un\u00b7ter\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Hespers kalten Wangen", "tokens": ["Von", "Hes\u00b7pers", "kal\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Tr\u00e4ufeln starre Thr\u00e4nen", "tokens": ["Tr\u00e4u\u00b7feln", "star\u00b7re", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Auf den Wanderer.", "tokens": ["Auf", "den", "Wan\u00b7de\u00b7rer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Unbekanntes Grausen", "tokens": ["Un\u00b7be\u00b7kann\u00b7tes", "Grau\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sch\u00fcttelt mein Gebein.", "tokens": ["Sch\u00fct\u00b7telt", "mein", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dumpfer Wetter Brausen", "tokens": ["Dum\u00b7pfer", "Wet\u00b7ter", "Brau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Donnert fern im Hain.", "tokens": ["Don\u00b7nert", "fern", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Es rasselt tausendstimmig;", "tokens": ["Es", "ras\u00b7selt", "tau\u00b7sends\u00b7tim\u00b7mig", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es fasst mich wild und grimmig \u2014", "tokens": ["Es", "fasst", "mich", "wild", "und", "grim\u00b7mig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Riesenarm, wer bist du?", "tokens": ["Rie\u00b7se\u00b7narm", ",", "wer", "bist", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Schrecklicher, halt ein!", "tokens": ["Schreck\u00b7li\u00b7cher", ",", "halt", "ein", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Nachtschwarz rauscht dein Fl\u00fcgel,", "tokens": ["Nacht\u00b7schwarz", "rauscht", "dein", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "W\u00fcrger Tod, um mich.", "tokens": ["W\u00fcr\u00b7ger", "Tod", ",", "um", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUI", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Deine Demantriegel,", "tokens": ["Dei\u00b7ne", "De\u00b7man\u00b7trie\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Grab, entriegeln sich.", "tokens": ["Grab", ",", "ent\u00b7rie\u00b7geln", "sich", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "\u201ehinunter aus der Schw\u00fcle!", "tokens": ["\u201e", "hin\u00b7un\u00b7ter", "aus", "der", "Schw\u00fc\u00b7le", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201ehinunter in die K\u00fchle!", "tokens": ["\u201e", "hin\u00b7un\u00b7ter", "in", "die", "K\u00fch\u00b7le", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201edrunten ists vertraulich,", "tokens": ["\u201e", "drun\u00b7ten", "ists", "ver\u00b7trau\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "\u201eeng und schauerlich.\u201c", "tokens": ["\u201e", "eng", "und", "schau\u00b7er\u00b7lich", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Deiner Fl\u00fcgel Sausen,", "tokens": ["Dei\u00b7ner", "Fl\u00fc\u00b7gel", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dr\u00e4uer, schreckt mich nicht.", "tokens": ["Dr\u00e4u\u00b7er", ",", "schreckt", "mich", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Deines Dunkels Grausen,", "tokens": ["Dei\u00b7nes", "Dun\u00b7kels", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Grab, entf\u00e4rbt mich nicht", "tokens": ["Grab", ",", "ent\u00b7f\u00e4rbt", "mich", "nicht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Hinunter aus der Schw\u00fcle!", 