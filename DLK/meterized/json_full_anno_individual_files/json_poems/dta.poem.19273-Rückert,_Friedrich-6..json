{"dta.poem.19273": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In sch\u00f6ne Leiblichkeit Gedanken eingeb\u00e4ren,", "tokens": ["In", "sch\u00f6\u00b7ne", "Leib\u00b7lich\u00b7keit", "Ge\u00b7dan\u00b7ken", "ein\u00b7ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in Gedankenduft ein leibliches verkl\u00e4ren,", "tokens": ["Und", "in", "Ge\u00b7dan\u00b7ken\u00b7duft", "ein", "leib\u00b7li\u00b7ches", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist beides Poesie nach zwei verschiednen Seiten;", "tokens": ["Ist", "bei\u00b7des", "Poe\u00b7sie", "nach", "zwei", "ver\u00b7schied\u00b7nen", "Sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der mag auf dieser Bahn, und der auf jener schreiten.", "tokens": ["Der", "mag", "auf", "die\u00b7ser", "Bahn", ",", "und", "der", "auf", "je\u00b7ner", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PDAT", "NN", "$,", "KON", "ART", "APPR", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das H\u00f6chste doch gelingt, Vollkommenstes entspringt,", "tokens": ["Das", "H\u00f6chs\u00b7te", "doch", "ge\u00b7lingt", ",", "Voll\u00b7kom\u00b7mens\u00b7tes", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wo ganz, urspr\u00fcnglich eins, sich beides rein durchdringt.", "tokens": ["Wo", "ganz", ",", "ur\u00b7spr\u00fcng\u00b7lich", "eins", ",", "sich", "bei\u00b7des", "rein", "durch\u00b7dringt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADJD", "PIS", "$,", "PRF", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}