{"textgrid.poem.41312": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Veit", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Veit, Schulz zu Michelsdorf, pflag immer zu verzeihn.", "tokens": ["Veit", ",", "Schulz", "zu", "Mi\u00b7chels\u00b7dorf", ",", "pflag", "im\u00b7mer", "zu", "ver\u00b7zeihn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "APPR", "NE", "$,", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald ward auch, unter ihm, die Bosheit allgemein,", "tokens": ["Bald", "ward", "auch", ",", "un\u00b7ter", "ihm", ",", "die", "Bos\u00b7heit", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "APPR", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Frevler lachten frei des Galgens und der Schande.", "tokens": ["Und", "Frev\u00b7ler", "lach\u00b7ten", "frei", "des", "Gal\u00b7gens", "und", "der", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Knecht war mit dem Hengst des Gastwirths fortgetrabt.", "tokens": ["Ein", "Knecht", "war", "mit", "dem", "Hengst", "des", "Gast\u00b7wirths", "fort\u00b7ge\u00b7trabt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man h\u00e4lt und klagt ihn an. Veit jammert seiner Bande.", "tokens": ["Man", "h\u00e4lt", "und", "klagt", "ihn", "an", ".", "Veit", "jam\u00b7mert", "sei\u00b7ner", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Kl\u00e4ger ruft ihm zu: Seid g\u00fctig mit Verstande!", "tokens": ["Der", "Kl\u00e4\u00b7ger", "ruft", "ihm", "zu", ":", "Seid", "g\u00fc\u00b7tig", "mit", "Ver\u00b7stan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "VAIMP", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00fcrwahr, Herr Schulz, wenn ihr mit Dieben Mitleid habt,", "tokens": ["F\u00fcr\u00b7wahr", ",", "Herr", "Schulz", ",", "wenn", "ihr", "mit", "Die\u00b7ben", "Mit\u00b7leid", "habt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NE", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So habt ihr keines mit dem Lande.", "tokens": ["So", "habt", "ihr", "kei\u00b7nes", "mit", "dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Veit, Schulz zu Michelsdorf, pflag immer zu verzeihn.", "tokens": ["Veit", ",", "Schulz", "zu", "Mi\u00b7