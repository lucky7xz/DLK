{"textgrid.poem.52921": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einen Strahl, eh' in dem Wogenbette", "tokens": ["Noch", "ei\u00b7nen", "Strahl", ",", "eh'", "in", "dem", "Wo\u00b7gen\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du deines Tages letzte Glut ertr\u00e4nkst,", "tokens": ["Du", "dei\u00b7nes", "Ta\u00b7ges", "letz\u00b7te", "Glut", "er\u00b7tr\u00e4nkst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und fern auf andre, gl\u00fccklichere St\u00e4dte,", "tokens": ["Und", "fern", "auf", "and\u00b7re", ",", "gl\u00fcck\u00b7li\u00b7che\u00b7re", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Belebende! dein Himmels-Auge lenkst!", "tokens": ["Be\u00b7le\u00b7ben\u00b7de", "!", "dein", "Him\u00b7mels\u00b7Au\u00b7ge", "lenkst", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Noch einmal webe um die rote Firne", "tokens": ["Noch", "ein\u00b7mal", "we\u00b7be", "um", "die", "ro\u00b7te", "Fir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Felsens deinen zauberischen Glast,", "tokens": ["Des", "Fel\u00b7sens", "dei\u00b7nen", "zau\u00b7be\u00b7ri\u00b7schen", "Glast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Diadem um eines Riesen Stirne,", "tokens": ["Ein", "Di\u00b7a\u00b7dem", "um", "ei\u00b7nes", "Rie\u00b7sen", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das hell der Falten grauen Ernst umfa\u00dft.", "tokens": ["Das", "hell", "der", "Fal\u00b7ten", "grau\u00b7en", "Ernst", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie winkt, die Sonne, freundliche Gew\u00e4hrung", "tokens": ["Sie", "winkt", ",", "die", "Son\u00b7ne", ",", "freund\u00b7li\u00b7che", "Ge\u00b7w\u00e4h\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lauscht aus Wolkenschleiern gro\u00df hervor;", "tokens": ["Und", "lauscht", "aus", "Wol\u00b7ken\u00b7schlei\u00b7ern", "gro\u00df", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es schwimmt das Meer, die Insel in Verkl\u00e4rung,", "tokens": ["Es", "schwimmt", "das", "Meer", ",", "die", "In\u00b7sel", "in", "Ver\u00b7kl\u00e4\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ganze Westen scheint ein flammend Tor.", "tokens": ["Der", "gan\u00b7ze", "Wes\u00b7ten", "scheint", "ein", "flam\u00b7mend", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus lauter Strahlen baut sich eine Br\u00fccke,", "tokens": ["Aus", "lau\u00b7ter", "Strah\u00b7len", "baut", "sich", "ei\u00b7ne", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Himmel einend mit dem dunklen Strand,", "tokens": ["Den", "Him\u00b7mel", "ei\u00b7nend", "mit", "dem", "dunk\u00b7len", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Fort strebt die Welle, strebt zum Land zur\u00fccke", "tokens": ["Fort", "strebt", "die", "Wel\u00b7le", ",", "strebt", "zum", "Land", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und spinnt so hin und her ihr funkelnd' Band.", "tokens": ["Und", "spinnt", "so", "hin", "und", "her", "ihr", "fun\u00b7kelnd'", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "KON", "ADV", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer wandeln k\u00f6nnte auf dem goldnen Pfade,", "tokens": ["Wer", "wan\u00b7deln", "k\u00f6nn\u00b7te", "auf", "dem", "gold\u00b7nen", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VMFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Lichte nach, in die Unendlichkeit!", "tokens": ["Dem", "Lich\u00b7te", "nach", ",", "in", "die", "Un\u00b7end\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wen der Delphin hintr\u00fcge, die Najade,", "tokens": ["Wen", "der", "Del\u00b7phin", "hin\u00b7tr\u00fc\u00b7ge", ",", "die", "Na\u00b7ja\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Wogen auf und ab, wer wei\u00df wie weit?", "tokens": ["Die", "Wo\u00b7gen", "auf", "und", "ab", ",", "wer", "wei\u00df", "wie", "weit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,", "PWS", "VVFIN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort, wo der Sonne Feuerball sich bettet", "tokens": ["Dort", ",", "wo", "der", "Son\u00b7ne", "Feu\u00b7er\u00b7ball", "sich", "bet\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Well', und Wolkenpf\u00fchle eingeh\u00fcllt,", "tokens": ["In", "Well'", ",", "und", "Wol\u00b7ken\u00b7pf\u00fch\u00b7le", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O wer dahin, dahin sich erst gerettet,", "tokens": ["O", "wer", "da\u00b7hin", ",", "da\u00b7hin", "sich", "erst", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "PAV", "$,", "PAV", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Gl\u00fccklichen w\u00e4r' Wunsch und Traum erf\u00fcllt!", "tokens": ["Dem", "Gl\u00fcck\u00b7li\u00b7chen", "w\u00e4r'", "Wunsch", "und", "Traum", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da fangen Br\u00fcck' und Band an zu zerrinnen,", "tokens": ["Da", "fan\u00b7gen", "Br\u00fcck", "und", "Band", "an", "zu", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bogen l\u00f6sen sich in Schaum und Duft,", "tokens": ["Die", "Bo\u00b7gen", "l\u00f6\u00b7sen", "sich", "in", "Schaum", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es dunkelt um des Eilands Felsenzinnen,", "tokens": ["Es", "dun\u00b7kelt", "um", "des", "Ei\u00b7lands", "Fel\u00b7sen\u00b7zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Nacht bew\u00e4ltigt Meer und Land und Luft.", "tokens": ["Die", "Nacht", "be\u00b7w\u00e4l\u00b7tigt", "Meer", "und", "Land", "und", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fahr wohl, fahr wohl! Noch seh' ich deinen Schimmer,", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", "!", "Noch", "seh'", "ich", "dei\u00b7nen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den sterbenden, der mir verhei\u00dfend winkt,", "tokens": ["Den", "ster\u00b7ben\u00b7den", ",", "der", "mir", "ver\u00b7hei\u00b7\u00dfend", "winkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch ach! erreichen kann ich dich ja nimmer,", "tokens": ["Doch", "ach", "!", "er\u00b7rei\u00b7chen", "kann", "ich", "dich", "ja", "nim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "VVINF", "VMFIN", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da mit dir auch der lichte Pfad versinkt.", "tokens": ["Da", "mit", "dir", "auch", "der", "lich\u00b7te", "Pfad", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So steht entt\u00e4uscht, die Arme ausgebreitet,", "tokens": ["So", "steht", "ent\u00b7t\u00e4uscht", ",", "die", "Ar\u00b7me", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Dichter an des Lebens nacktem Strand;", "tokens": ["Der", "Dich\u00b7ter", "an", "des", "Le\u00b7bens", "nack\u00b7tem", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das luft'ge Bild, das seinem Blick entgleitet,", "tokens": ["Das", "luft'\u00b7ge", "Bild", ",", "das", "sei\u00b7nem", "Blick", "ent\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergeblich w\u00e4hnt er's nah-ger\u00fcckt, gebannt.", "tokens": ["Ver\u00b7geb\u00b7lich", "w\u00e4hnt", "er's", "nah\u00b7ge\u00b7r\u00fcckt", ",", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nach Zielen schw\u00e4rmt er in der Weihe Stunden,", "tokens": ["Nach", "Zie\u00b7len", "schw\u00e4rmt", "er", "in", "der", "Wei\u00b7he", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu denen glanzvoll sich ein Weg ihm beut,", "tokens": ["Zu", "de\u00b7nen", "glanz\u00b7voll", "sich", "ein", "Weg", "ihm", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "PRF", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch mit dem Ziel ist auch der Weg verschwunden,", "tokens": ["Doch", "mit", "dem", "Ziel", "ist", "auch", "der", "Weg", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie jene goldne Sonnenbr\u00fccke heut'.", "tokens": ["Wie", "je\u00b7ne", "gold\u00b7ne", "Son\u00b7nen\u00b7br\u00fc\u00b7cke", "heut'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Geh heim! Es harret an dem Felsengange", "tokens": ["Geh", "heim", "!", "Es", "har\u00b7ret", "an", "dem", "Fel\u00b7sen\u00b7gan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im letzten H\u00e4uschen eine Zelle dein,", "tokens": ["Im", "letz\u00b7ten", "H\u00e4usc\u00b7hen", "ei\u00b7ne", "Zel\u00b7le", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort wiege bei dem n\u00e4chtlichen Gesange", "tokens": ["Dort", "wie\u00b7ge", "bei", "dem", "n\u00e4cht\u00b7li\u00b7chen", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Winds, der Welle dich getr\u00f6stet ein.", "tokens": ["Des", "Winds", ",", "der", "Wel\u00b7le", "dich", "ge\u00b7tr\u00f6s\u00b7tet", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sieh, ist auch die Sonne gleich versunken,", "tokens": ["Und", "sieh", ",", "ist", "auch", "die", "Son\u00b7ne", "gleich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du bist verlassen, du bist lichtlos nicht, \u2013", "tokens": ["Du", "bist", "ver\u00b7las\u00b7sen", ",", "du", "bist", "licht\u00b7los", "nicht", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "ADJD", "PTKNEG", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im Osten taucht ja eben, wehmut-trunken", "tokens": ["Im", "Os\u00b7ten", "taucht", "ja", "e\u00b7ben", ",", "weh\u00b7mut\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADV", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mild, empor des Mondes Angesicht.", "tokens": ["Und", "mild", ",", "em\u00b7por", "des", "Mon\u00b7des", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Noch einen Strahl, eh' in dem Wogenbette", "tokens": ["Noch", "ei\u00b7nen", "Strahl", ",", "eh'", "in", "dem", "Wo\u00b7gen\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du deines Tages letzte Glut ertr\u00e4nkst,", "tokens": ["Du", "dei\u00b7nes", "Ta\u00b7ges", "letz\u00b7te", "Glut", "er\u00b7tr\u00e4nkst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und fern auf andre, gl\u00fccklichere St\u00e4dte,", "tokens": ["Und", "fern", "auf", "and\u00b7re", ",", "gl\u00fcck\u00b7li\u00b7che\u00b7re", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Belebende! dein Himmels-Auge lenkst!", "tokens": ["Be\u00b7le\u00b7ben\u00b7de", "!", "dein", "Him\u00b7mels\u00b7Au\u00b7ge", "lenkst", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Noch einmal webe um die rote Firne", "tokens": ["Noch", "ein\u00b7mal", "we\u00b7be", "um", "die", "ro\u00b7te", "Fir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Felsens deinen zauberischen Glast,", "tokens": ["Des", "Fel\u00b7sens", "dei\u00b7nen", "zau\u00b7be\u00b7ri\u00b7schen", "Glast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Diadem um eines Riesen Stirne,", "tokens": ["Ein", "Di\u00b7a\u00b7dem", "um", "ei\u00b7nes", "Rie\u00b7sen", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das hell der Falten grauen Ernst umfa\u00dft.", "tokens": ["Das", "hell", "der", "Fal\u00b7ten", "grau\u00b7en", "Ernst", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sie winkt, die Sonne, freundliche Gew\u00e4hrung", "tokens": ["Sie", "winkt", ",", "die", "Son\u00b7ne", ",", "freund\u00b7li\u00b7che", "Ge\u00b7w\u00e4h\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lauscht aus Wolkenschleiern gro\u00df hervor;", "tokens": ["Und", "lauscht", "aus", "Wol\u00b7ken\u00b7schlei\u00b7ern", "gro\u00df", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es schw