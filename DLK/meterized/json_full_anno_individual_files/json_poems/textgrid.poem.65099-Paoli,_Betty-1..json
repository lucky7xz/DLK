{"textgrid.poem.65099": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Welt um uns ein Netz geschlungen,", "tokens": ["Es", "hat", "die", "Welt", "um", "uns", "ein", "Netz", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein dunkles Netz verwirrender Gestalten;", "tokens": ["Ein", "dunk\u00b7les", "Netz", "ver\u00b7wir\u00b7ren\u00b7der", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jedweder glaubt die Wahrheit festzuhalten,", "tokens": ["Jed\u00b7we\u00b7der", "glaubt", "die", "Wahr\u00b7heit", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Gott allein nur wei\u00df, wem es gelungen.", "tokens": ["Und", "Gott", "al\u00b7lein", "nur", "wei\u00df", ",", "wem", "es", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Was mit Begeist'rung dein Gem\u00fcth durchdrungen,", "tokens": ["Was", "mit", "Be\u00b7geist'\u00b7rung", "dein", "Ge\u00b7m\u00fcth", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Hasse macht es meine Brust erkalten,", "tokens": ["Im", "Has\u00b7se", "macht", "es", "mei\u00b7ne", "Brust", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was meines Hoffens freudigstes Entfalten", "tokens": ["Was", "mei\u00b7nes", "Hof\u00b7fens", "freu\u00b7digs\u00b7tes", "Ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Scheint aus der H\u00f6lle dir emporgedrungen.", "tokens": ["Scheint", "aus", "der", "H\u00f6l\u00b7le", "dir", "em\u00b7por\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Doch ob auch in den blutgetr\u00e4nkten Schranken", "tokens": ["Doch", "ob", "auch", "in", "den", "blut\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Geister sich in wildem Kampf erhitzen,", "tokens": ["Die", "Geis\u00b7ter", "sich", "in", "wil\u00b7dem", "Kampf", "er\u00b7hit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir si