{"textgrid.poem.46024": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[nun soll ich in die Fremde ziehen]", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun soll ich in die Fremde ziehen", "tokens": ["Nun", "soll", "ich", "in", "die", "Frem\u00b7de", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir hatte eine Himmelsbraut", "tokens": ["Mir", "hat\u00b7te", "ei\u00b7ne", "Him\u00b7mels\u00b7braut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Zweiglein aus dem Kranz geliehen", "tokens": ["Ein", "Zwei\u00b7glein", "aus", "dem", "Kranz", "ge\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hatte draus ein Haus erbaut", "tokens": ["Ich", "hat\u00b7te", "draus", "ein", "Haus", "er\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es gr\u00fcnte schon, es wollte bl\u00fchen", "tokens": ["Es", "gr\u00fcn\u00b7te", "schon", ",", "es", "woll\u00b7te", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von meiner Tr\u00e4nennut betaut", "tokens": ["Von", "mei\u00b7ner", "Tr\u00e4\u00b7nen\u00b7nut", "be\u00b7taut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da konnt' ich betend ruhig knieen", "tokens": ["Da", "konnt'", "ich", "be\u00b7tend", "ru\u00b7hig", "kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Da hatte ich so fest vertraut", "tokens": ["Da", "hat\u00b7te", "ich", "so", "fest", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und soll nun in die Fremde ziehen.", "tokens": ["Und", "soll", "nun", "in", "die", "Frem\u00b7de", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun soll ich in die Fremde ziehen", "tokens": ["Nun", "soll", "ich", "in", "die", "Frem\u00b7de", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie w\u00e4re ruhig, w\u00e4r' ich fort,", "tokens": ["Sie", "w\u00e4\u00b7re", "ru\u00b7hig", ",", "w\u00e4r'", "ich", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tempel, wo wir beide knieen,", "tokens": ["Der", "Tem\u00b7pel", ",", "wo", "wir", "bei\u00b7de", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Soll nun zerbrechen und der Ort", "tokens": ["Soll", "nun", "zer\u00b7bre\u00b7chen", "und", "der", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohin ich mit ihr sollte ziehen", "tokens": ["Wo\u00b7hin", "ich", "mit", "ihr", "soll\u00b7te", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Soll nun verschwinden und der Hort", "tokens": ["Soll", "nun", "ver\u00b7schwin\u00b7den", "und", "der", "Hort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des einen Gl\u00fccks, f\u00fcr das wir gl\u00fchen,", "tokens": ["Des", "ei\u00b7nen", "Gl\u00fccks", ",", "f\u00fcr", "das", "wir", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Soll sinken, auf ein hartes Wort", "tokens": ["Soll", "sin\u00b7ken", ",", "auf", "ein", "har\u00b7tes", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Soll ich nun in die Fremde ziehen.", "tokens": ["Soll", "ich", "nun", "in", "die", "Frem\u00b7de", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun soll ich in die Fremde ziehen", "tokens": ["Nun", "soll", "ich", "in", "die", "Frem\u00b7de", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich der die Heimat nie gekannt,", "tokens": ["Ich", "der", "die", "Hei\u00b7mat", "nie", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll meine erste Heimat fliehen", "tokens": ["Soll", "mei\u00b7ne", "ers\u00b7te", "Hei\u00b7mat", "flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Soll fallen in der R\u00e4uber Hand", "tokens": ["Soll", "fal\u00b7len", "in", "der", "R\u00e4u\u00b7ber", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was Sie mir schenkte war geliehen", "tokens": ["Was", "Sie", "mir", "schenk\u00b7te", "war", "ge\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Streng fodert sie das heil'ge Pfand", "tokens": ["Streng", "fo\u00b7dert", "sie", "das", "heil'\u00b7ge", "Pfand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu ihr hab' ich um H\u00fclf' geschrieen,", "tokens": ["Zu", "ihr", "hab'", "ich", "um", "H\u00fcl\u00b7f'", "ge\u00b7schri\u00b7een", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sie weist mich nach dem andern Land", "tokens": ["Sie", "weist", "mich", "nach", "dem", "an\u00b7dern", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich soll nun in die Fremde ziehen.", "tokens": ["Ich", "soll", "nun", "in", "die", "Frem\u00b7de", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun soll ich in die Fremde ziehen,", "tokens": ["Nun", "soll", "ich", "in", "die", "Frem\u00b7de", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df wohl, wie die Fremde tut", "tokens": ["Ich", "wei\u00df", "wohl", ",", "wie", "die", "Frem\u00b7de", "tut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Ankergrund ist mir gediehen", "tokens": ["Kein", "An\u00b7ker\u00b7grund", "ist", "mir", "ge\u00b7die\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich dem ungerechten Gut", "tokens": ["Weil", "ich", "dem", "un\u00b7ge\u00b7rech\u00b7ten", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf meinem Schiffe Schutz verliehen", "tokens": ["Auf", "mei\u00b7nem", "Schif\u00b7fe", "Schutz", "ver\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zerbrach es in des Sturmes Wut", "tokens": ["Zer\u00b7brach", "es", "in", "des", "Stur\u00b7mes", "Wut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Woge hat mich ausgespieen,", "tokens": ["Die", "Wo\u00b7ge", "hat", "mich", "aus\u00b7ge\u00b7spi\u00b7een", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und kaum hab' ich am Strand geruht", "tokens": ["Und", "kaum", "hab'", "ich", "am", "Strand", "ge\u00b7ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Soll ich schon in die Fremde ziehen.", "tokens": ["Soll", "ich", "schon", "in", "die", "Frem\u00b7de", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun soll ich in die Fremde ziehen", "tokens": ["Nun", "soll", "ich", "in", "die", "Frem\u00b7de", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohin, wohin, da\u00df Gott erbarm',", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", ",", "da\u00df", "Gott", "er\u00b7barm'", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht, wo die Friedensrosen bl\u00fchen,", "tokens": ["Nicht", ",", "wo", "die", "Frie\u00b7dens\u00b7ro\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht, wo im Geist so sonnenwarm", "tokens": ["Nicht", ",", "wo", "im", "Geist", "so", "son\u00b7nen\u00b7warm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PWAV", "APPRART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Worte wie Gebete gl\u00fchen", "tokens": ["Die", "Wor\u00b7te", "wie", "Ge\u00b7be\u00b7te", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nein in die Brust \u2013 den Wespenschwarm", "tokens": ["Nein", "in", "die", "Brust", "\u2013", "den", "Wes\u00b7pen\u00b7schwarm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "APPR", "ART", "NN", "$(", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vergeblicher erstarrter M\u00fchen", "tokens": ["Ver\u00b7geb\u00b7li\u00b7cher", "er\u00b7starr\u00b7ter", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"],