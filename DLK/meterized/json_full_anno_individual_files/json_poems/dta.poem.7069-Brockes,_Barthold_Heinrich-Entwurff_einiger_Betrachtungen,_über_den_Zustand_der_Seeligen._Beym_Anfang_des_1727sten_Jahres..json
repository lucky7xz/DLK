{"dta.poem.7069": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch eine sanffte Lust wird sie entz\u00fccken,", "tokens": ["Welch", "ei\u00b7ne", "sanff\u00b7te", "Lust", "wird", "sie", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie so kl\u00e4rlich sehn, so offenbar befinden,", "tokens": ["Wenn", "sie", "so", "kl\u00e4r\u00b7lich", "sehn", ",", "so", "of\u00b7fen\u00b7bar", "be\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie GOTT des Glaubens Licht, in ihnen anzuz\u00fcnden,", "tokens": ["Wie", "GoTT", "des", "Glau\u00b7bens", "Licht", ",", "in", "ih\u00b7nen", "an\u00b7zu\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "NN", "$,", "APPR", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und einen neuen Strahl in ihren Geist zu schicken,", "tokens": ["Und", "ei\u00b7nen", "neu\u00b7en", "Strahl", "in", "ih\u00b7ren", "Geist", "zu", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So offt sie wehrt geacht\u2019t;", "tokens": ["So", "offt", "sie", "wehrt", "ge\u00b7acht't", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wodurch Er Hertz und Willen", "tokens": ["Wo\u00b7durch", "Er", "Hertz", "und", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit Seinem Wollen zu erf\u00fcllen;", "tokens": ["Mit", "Sei\u00b7nem", "Wol\u00b7len", "zu", "er\u00b7f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erleuchtet und geschickt gemacht!", "tokens": ["Er\u00b7leuch\u00b7tet", "und", "ge\u00b7schickt", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}