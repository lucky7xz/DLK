{"dta.poem.4022": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die jungen Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Lieben Br\u00fcder! es reift unsere Kunst vielleicht,", "tokens": ["Lie\u00b7ben", "Br\u00fc\u00b7der", "!", "es", "reift", "un\u00b7se\u00b7re", "Kunst", "viel\u00b7leicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da, dem J\u00fcnglinge gleich, lange sie schon geg\u00e4hrt,", "tokens": ["Da", ",", "dem", "J\u00fcng\u00b7lin\u00b7ge", "gleich", ",", "lan\u00b7ge", "sie", "schon", "ge\u00b7g\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "ADV", "$,", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "--++-+---+-+", "measure": "anapaest.init"}, "line.3": {"text": "Bald zur Stille der Sch\u00f6nheit;", "tokens": ["Bald", "zur", "Stil\u00b7le", "der", "Sch\u00f6n\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Seyd nur fromm, wie der Grieche war!", "tokens": ["Seyd", "nur", "fromm", ",", "wie", "der", "Grie\u00b7che", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebt die G\u00f6tter und denkt freundlich der Sterb-", "tokens": ["Liebt", "die", "G\u00f6t\u00b7ter", "und", "denkt", "freund\u00b7lich", "der", "Ster\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "ART", "TRUNC"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "lichen!", "tokens": ["li\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ha\u00dft den Rausch wie den Frost! lehrt und be-", "tokens": ["Ha\u00dft", "den", "Rausch", "wie", "den", "Frost", "!", "lehrt", "und", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$.", "VVFIN", "KON", "TRUNC"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "schreibet nicht!", "tokens": ["schrei\u00b7bet", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wenn der Meister euch \u00e4ngstigt,", "tokens": ["Wenn", "der", "Meis\u00b7ter", "euch", "\u00e4ngs\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Fragt die gro\u00dfe Natur um Rath!", "tokens": ["Fragt", "die", "gro\u00b7\u00dfe", "Na\u00b7tur", "um", "Rath", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}