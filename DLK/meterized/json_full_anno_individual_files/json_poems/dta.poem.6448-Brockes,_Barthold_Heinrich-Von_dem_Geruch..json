{"dta.poem.6448": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf solche Weise nun (doch dieses ausgenommen,", "tokens": ["Auf", "sol\u00b7che", "Wei\u00b7se", "nun", "(", "doch", "die\u00b7ses", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$(", "ADV", "PDAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn wir um den Geschmack durch einen Zufall kommen.)", "tokens": ["Wenn", "wir", "um", "den", "Ge\u00b7schmack", "durch", "ei\u00b7nen", "Zu\u00b7fall", "kom\u00b7men", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat eine Handlung nur das Riechen und das Schmecken,", "tokens": ["Hat", "ei\u00b7ne", "Hand\u00b7lung", "nur", "das", "Rie\u00b7chen", "und", "das", "Schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da sie in Nas\u2019 und Zung denselben Druck erwecken.", "tokens": ["Da", "sie", "in", "Nas'", "und", "Zung", "den\u00b7sel\u00b7ben", "Druck", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es bleibet mehrentheils die Regel fest gestellt,", "tokens": ["Es", "blei\u00b7bet", "meh\u00b7ren\u00b7theils", "die", "Re\u00b7gel", "fest", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Speise, die wohl riecht, auch dem Geschmack gef\u00e4llt.", "tokens": ["Da\u00df", "Spei\u00b7se", ",", "die", "wohl", "riecht", ",", "auch", "dem", "Ge\u00b7schmack", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}