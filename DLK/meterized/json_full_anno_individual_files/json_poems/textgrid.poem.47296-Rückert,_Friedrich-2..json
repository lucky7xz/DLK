{"textgrid.poem.47296": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Fr\u00fchling, ew'ge Liebesmelodie,", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "ew'\u00b7ge", "Lie\u00b7bes\u00b7me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unausget\u00f6nt von allen Nachtigallen,", "tokens": ["Un\u00b7aus\u00b7ge\u00b7t\u00f6nt", "von", "al\u00b7len", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unausgebl\u00fcht von allen Rosen, wie", "tokens": ["Un\u00b7aus\u00b7ge\u00b7bl\u00fcht", "von", "al\u00b7len", "Ro\u00b7sen", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unausgef\u00fchlt von Menschenherzen allen!", "tokens": ["Un\u00b7aus\u00b7ge\u00b7f\u00fchlt", "von", "Men\u00b7schen\u00b7her\u00b7zen", "al\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So Fr\u00fchling, wie du's nun bist, warst du nie,", "tokens": ["So", "Fr\u00fch\u00b7ling", ",", "wie", "du's", "nun", "bist", ",", "warst", "du", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PWAV", "PIS", "ADV", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "$,"