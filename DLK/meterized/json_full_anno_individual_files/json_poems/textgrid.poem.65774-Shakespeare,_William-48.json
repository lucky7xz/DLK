{"textgrid.poem.65774": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "48", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sorgsam barg ich jeden kleinen Tand,", "tokens": ["Wie", "sorg\u00b7sam", "barg", "ich", "je\u00b7den", "klei\u00b7nen", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als ich auf Reisen ging, in Kofferw\u00e4nde,", "tokens": ["Als", "ich", "auf", "Rei\u00b7sen", "ging", ",", "in", "Kof\u00b7fer\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit ich unber\u00fchrt von falscher Hand", "tokens": ["Da\u00b7mit", "ich", "un\u00b7be\u00b7r\u00fchrt", "von", "fal\u00b7scher", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu eignem Zweck ihn sicher wiederf\u00e4nde!", "tokens": ["Zu", "eig\u00b7nem", "Zweck", "ihn", "si\u00b7cher", "wie\u00b7der\u00b7f\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und du, dem Tand nur meine Perlen sind,", "tokens": ["Und", "du", ",", "dem", "Tand", "nur", "mei\u00b7ne", "Per\u00b7len", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein teurer Trost, und nun mein gr\u00f6\u00dfter Gram auf Erden,", "tokens": ["Mein", "teu\u00b7rer", "Trost", ",", "und", "nun", "mein", "gr\u00f6\u00df\u00b7ter", "Gram", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du einzig h\u00f6chstes Gut, das meine Seele minnt,", "tokens": ["Du", "ein\u00b7zig", "h\u00f6chs\u00b7tes", "Gut", ",", "das", "mei\u00b7ne", "See\u00b7le", "minnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kannst jedes schn\u00f6den Diebes Beute werden!", "tokens": ["Kannst", "je\u00b7des", "schn\u00f6\u00b7den", "Die\u00b7bes", "Beu\u00b7te", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dich schlie\u00dft kein Koffer mir noch Kasten ein,", "tokens": ["Dich", "schlie\u00dft", "kein", "Kof\u00b7fer", "mir", "noch", "Kas\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPER", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als der, wo du nicht bist \u2013 und doch f\u00fchl ich dich drinnen \u2013", "tokens": ["Als", "der", ",", "wo", "du", "nicht", "bist", "\u2013", "und", "doch", "f\u00fchl", "ich", "dich", "drin\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PWAV", "PPER", "PTKNEG", "VAFIN", "$(", "KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Hier in der Brust, dem trauten K\u00e4mmerlein,", "tokens": ["Hier", "in", "der", "Brust", ",", "dem", "trau\u00b7ten", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wo du, nach freier Lust, kannst kommen und entrinnen:", "tokens": ["Wo", "du", ",", "nach", "frei\u00b7er", "Lust", ",", "kannst", "kom\u00b7men", "und", "ent\u00b7rin\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Denn um so teuern Preis wird Treue selbst zum Dieb.", "tokens": ["Denn", "um", "so", "teu\u00b7ern", "Preis", "wird", "Treu\u00b7e", "selbst", "zum", "Dieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "VAFIN", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sorgsam barg ich jeden kleinen Tand,", "tokens": ["Wie", "sorg\u00b7sam", "barg", "ich", "je\u00b7den", "klei\u00b7nen", "Tand", ","],