{"textgrid.poem.44559": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[das Ministerium, h\u00f6r ich, war schwach]", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Ministerium, h\u00f6r ich, war schwach,", "tokens": ["Das", "Mi\u00b7nis\u00b7te\u00b7ri\u00b7um", ",", "h\u00f6r", "ich", ",", "war", "schwach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADJD", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der eine sagts, der andre sagts nach.", "tokens": ["Der", "ei\u00b7ne", "sagts", ",", "der", "and\u00b7re", "sagts", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es sei denn schwach! Wir aber warens nicht,", "tokens": ["Es", "sei", "denn", "schwach", "!", "Wir", "a\u00b7ber", "wa\u00b7rens", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "ADV", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die lachten, wenn der P\u00f6bel hielt Gericht.", "tokens": ["Die", "lach\u00b7ten", ",", "wenn", "der", "P\u00f6\u00b7bel", "hielt", "Ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Eltern warens nicht, die ihren Knaben", "tokens": ["Die", "El\u00b7tern", "wa\u00b7rens", "nicht", ",", "die", "ih\u00b7ren", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein Wort der Mahnung zugedonnert haben,", "tokens": ["Kein", "Wort", "der", "Mah\u00b7nung", "zu\u00b7ge\u00b7don\u00b7nert", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Garde war es nicht, die, als es galt,", "tokens": ["Die", "Gar\u00b7de", "war", "es", "nicht", ",", "die", ",", "als", "es", "galt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,", "PRELS", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem Staat versagte ihres Beistands Halt,", "tokens": ["Dem", "Staat", "ver\u00b7sag\u00b7te", "ih\u00b7res", "Bei\u00b7stands", "Halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die B\u00fcrgertruppe nicht, die selbst zur Tat", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7trup\u00b7pe", "nicht", ",", "die", "selbst", "zur", "Tat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Frei auf die Seite der Emp\u00f6rer trat!", "tokens": ["Frei", "auf", "die", "Sei\u00b7te", "der", "Em\u00b7p\u00f6\u00b7rer", "trat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"