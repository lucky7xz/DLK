{"textgrid.poem.26433": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da ich in Schweigen versunken", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich in Schweigen versunken", "tokens": ["Da", "ich", "in", "Schwei\u00b7gen", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf meine Dichtung zur\u00fccksah, lang \u00fcberlegend und z\u00f6gernd,", "tokens": ["Auf", "mei\u00b7ne", "Dich\u00b7tung", "zu\u00b7r\u00fcck\u00b7sah", ",", "lang", "\u00fc\u00b7berl\u00b7e\u00b7gend", "und", "z\u00f6\u00b7gernd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADJD", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Stieg ein Phantom vor mir auf, mi\u00dftrauisch, fragw\u00fcrdiger Art,", "tokens": ["Stieg", "ein", "Phan\u00b7tom", "vor", "mir", "auf", ",", "mi\u00df\u00b7trau\u00b7isch", ",", "frag\u00b7w\u00fcr\u00b7di\u00b7ger", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Schrecklich in Sch\u00f6nheit, Alter und Kraft,", "tokens": ["Schreck\u00b7lich", "in", "Sch\u00f6n\u00b7heit", ",", "Al\u00b7ter", "und", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der Genius der Dichter aller L\u00e4nder,", "tokens": ["Der", "Ge\u00b7nius", "der", "Dich\u00b7ter", "al\u00b7ler", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Richtete seine Augen wie Flammen auf mich,", "tokens": ["Rich\u00b7te\u00b7te", "sei\u00b7ne", "Au\u00b7gen", "wie", "Flam\u00b7men", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KOKOM", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Wies mit dem Finger auf manche unsterbliche Lieder", "tokens": ["Wies", "mit", "dem", "Fin\u00b7ger", "auf", "man\u00b7che", "uns\u00b7terb\u00b7li\u00b7che", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.8": {"text": "Und sprach mit drohender Stimme: ", "tokens": ["Und", "sprach", "mit", "dro\u00b7hen\u00b7der", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da ich in Schweigen versunken", "tokens": ["Da", "ich", "in", "Schwei\u00b7gen", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf meine Dichtung zur\u00fccksah, lang \u00fcberlegend und z\u00f6gernd,", "tokens": ["Auf", "mei\u00b7ne", "Dich\u00b7tung", "zu\u00b7r\u00fcck\u00b7sah", ",", "lang", "\u00fc\u00b7berl\u00b7e\u00b7gend", "und", "z\u00f6\u00b7gernd"