{"dta.poem.6675": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hitze wird im Blut gelindert und erreget:", "tokens": ["Die", "Hit\u00b7ze", "wird", "im", "Blut", "ge\u00b7lin\u00b7dert", "und", "er\u00b7re\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichwie ein C\u00f6rper Kr\u00e4ffte heget,", "tokens": ["Gleich\u00b7wie", "ein", "C\u00f6r\u00b7per", "Kr\u00e4ff\u00b7te", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er den andern hemmt, und auch beweget.", "tokens": ["Da\u00df", "er", "den", "an\u00b7dern", "hemmt", ",", "und", "auch", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Also erregt die Laute, die die Hand", "tokens": ["Al\u00b7so", "er\u00b7regt", "die", "Lau\u00b7te", ",", "die", "die", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Des Meisters k\u00fcnstlich greifft, ein solch Ersch\u00fcttern", "tokens": ["Des", "Meis\u00b7ters", "k\u00fcnst\u00b7lich", "greifft", ",", "ein", "solch", "Er\u00b7sch\u00fct\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jm Blut\u2019, das sich zu langsam regt:", "tokens": ["Jm", "Blut'", ",", "das", "sich", "zu", "lang\u00b7sam", "regt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PRF", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie f\u00fcllt mit einem regen Zittern", "tokens": ["Sie", "f\u00fcllt", "mit", "ei\u00b7nem", "re\u00b7gen", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein anders, dessen Lauff sich feuriger bewegt:", "tokens": ["Ein", "an\u00b7ders", ",", "des\u00b7sen", "Lauff", "sich", "feu\u00b7ri\u00b7ger", "be\u00b7wegt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELAT", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und unsre Seele f\u00fchlt die Eigenschafft so bald", "tokens": ["Und", "uns\u00b7re", "See\u00b7le", "f\u00fchlt", "die", "Ei\u00b7gen\u00b7schafft", "so", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von einer sanfteren und strengeren Gewalt.", "tokens": ["Von", "ei\u00b7ner", "sanf\u00b7te\u00b7ren", "und", "stren\u00b7ge\u00b7ren", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}