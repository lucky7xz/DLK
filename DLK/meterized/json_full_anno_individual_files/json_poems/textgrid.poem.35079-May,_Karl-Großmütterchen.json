{"textgrid.poem.35079": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfm\u00fctterchen", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie trug mich stets auf ihren Armen;", "tokens": ["Sie", "trug", "mich", "stets", "auf", "ih\u00b7ren", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie lehrte mich den ersten Schritt,", "tokens": ["Sie", "lehr\u00b7te", "mich", "den", "ers\u00b7ten", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weinte ich zum Herzerbarmen,", "tokens": ["Und", "wein\u00b7te", "ich", "zum", "Her\u00b7zer\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So weinte sie erbarmend mit.", "tokens": ["So", "wein\u00b7te", "sie", "er\u00b7bar\u00b7mend", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sie des Abends mich ins Nestchen", "tokens": ["Wenn", "sie", "des", "A\u00b7bends", "mich", "ins", "Nest\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit linder Segenshand gebracht,", "tokens": ["Mit", "lin\u00b7der", "Se\u00b7gens\u00b7hand", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So bat ich: \u00bbBleibe noch ein Restchen\u00ab,", "tokens": ["So", "bat", "ich", ":", "\u00bb", "Blei\u00b7be", "noch", "ein", "Rest\u00b7chen", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "ADV", "ART", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meinte da \u00bbdie ganze Nacht\u00ab.", "tokens": ["Und", "mein\u00b7te", "da", "\u00bb", "die", "gan\u00b7ze", "Nacht", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "$(", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn ein b\u00f6ser Traum mich schreckte,", "tokens": ["Und", "wenn", "ein", "b\u00f6\u00b7ser", "Traum", "mich", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sa\u00df sie da beim kleinen Licht,", "tokens": ["So", "sa\u00df", "sie", "da", "beim", "klei\u00b7nen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nahm weg den Schirm, der es bedeckte,", "tokens": ["Nahm", "weg", "den", "Schirm", ",", "der", "es", "be\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah mir liebend ins Gesicht.", "tokens": ["Und", "sah", "mir", "lie\u00b7bend", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz ihrer hellen Augensterne", "tokens": ["Trotz", "ih\u00b7rer", "hel\u00b7len", "Au\u00b7gens\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "That ich sodann die Frage doch:", "tokens": ["That", "ich", "so\u00b7dann", "die", "Fra\u00b7ge", "doch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbich tr\u00e4ume ohne dich nicht gerne;", "tokens": ["\u00bb", "ich", "tr\u00e4u\u00b7me", "oh\u00b7ne", "dich", "nicht", "ger\u00b7ne", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gro\u00dfm\u00fctterchen, sag, wachst du noch?\u00ab", "tokens": ["Gro\u00df\u00b7m\u00fct\u00b7ter\u00b7chen", ",", "sag", ",", "wachst", "du", "noch", "?", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar ist sie l\u00e4ngst von mir gegangen;", "tokens": ["Zwar", "ist", "sie", "l\u00e4ngst", "von", "mir", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich selbst bin alt, fast schon ein Greis,", "tokens": ["Ich", "selbst", "bin", "alt", ",", "fast", "schon", "ein", "Greis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchl mich doch von ihr umfangen,", "tokens": ["Und", "f\u00fchl", "mich", "doch", "von", "ihr", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mich noch jetzt zu segnen wei\u00df.", "tokens": ["Die", "mich", "noch", "jetzt", "zu", "seg\u00b7nen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stets ist es mir, geh ich zur Ruhe,", "tokens": ["Stets", "ist", "es", "mir", ",", "geh", "ich", "zur", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Als setze sie sich zu mir hin,", "tokens": ["Als", "set\u00b7ze", "sie", "sich", "zu", "mir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und wenn ich etwas Wichtges thue,", "tokens": ["Und", "wenn", "ich", "et\u00b7was", "Wicht\u00b7ges", "thue", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kommt sie mir hilfreich in den Sinn.", "tokens": ["Kommt", "sie", "mir", "hilf\u00b7reich", "in", "den", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So oft ich Sterne leuchten sehe,", "tokens": ["So", "oft", "ich", "Ster\u00b7ne", "leuch\u00b7ten", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hell wie in meiner Jugendzeit,", "tokens": ["Hell", "wie", "in", "mei\u00b7ner", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6r ich ihr Wort: \u00bbWas auch geschehe,", "tokens": ["H\u00f6r", "ich", "ihr", "Wort", ":", "\u00bb", "Was", "auch", "ge\u00b7sche\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$.", "$(", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du und dein Gl\u00fcck, ihr seid gefeit.\u00ab", "tokens": ["Du", "und", "dein", "Gl\u00fcck", ",", "ihr", "seid", "ge\u00b7feit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measur