{"textgrid.poem.65112": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: In deinen Tempel soll ich treten,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In deinen Tempel soll ich treten,", "tokens": ["In", "dei\u00b7nen", "Tem\u00b7pel", "soll", "ich", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinen G\u00f6ttern gl\u00e4ubig beten,", "tokens": ["Zu", "dei\u00b7nen", "G\u00f6t\u00b7tern", "gl\u00e4u\u00b7big", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was jemals mein Gem\u00fcth durbebt,", "tokens": ["Was", "je\u00b7mals", "mein", "Ge\u00b7m\u00fcth", "dur\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll ich mit muth'ger Hand verwischen,", "tokens": ["Soll", "ich", "mit", "muth'\u00b7ger", "Hand", "ver\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann, meinst du, werde mich erfrischen", "tokens": ["Dann", ",", "meinst", "du", ",", "wer\u00b7de", "mich", "er\u00b7fri\u00b7schen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Frieden, welcher dich umschwebt.", "tokens": ["Der", "Frie\u00b7den", ",", "wel\u00b7cher", "dich", "um\u00b7schwebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gewi\u00df! gewi\u00df! wie freudig w\u00fcrde", "tokens": ["Ge\u00b7wi\u00df", "!", "ge\u00b7wi\u00df", "!", "wie", "freu\u00b7dig", "w\u00fcr\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "$.", "PWAV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich dann ertragen jede B\u00fcrde,", "tokens": ["Ich", "dann", "er\u00b7tra\u00b7gen", "je\u00b7de", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie froh begr\u00fc\u00dfen jeden Tag,", "tokens": ["Wie", "froh", "be\u00b7gr\u00fc\u00b7\u00dfen", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie schiene mir dann Leid und Gr\u00e4men", "tokens": ["Wie", "schie\u00b7ne", "mir", "dann", "Leid", "und", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts als ein wesenloser Schemen \u2013 \u2013", "tokens": ["Nichts", "als", "ein", "we\u00b7sen\u00b7lo\u00b7ser", "Sche\u00b7men", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "ia