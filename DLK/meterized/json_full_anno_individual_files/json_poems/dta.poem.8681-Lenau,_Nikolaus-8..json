{"dta.poem.8681": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Noch immer lag ein tiefes Schweigen", "tokens": ["Noch", "im\u00b7mer", "lag", "ein", "tie\u00b7fes", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rings auf den H\u00f6h'n; doch pl\u00f6tzlich fuhr", "tokens": ["Rings", "auf", "den", "H\u00f6h'n", ";", "doch", "pl\u00f6tz\u00b7lich", "fuhr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wind nun auf zum wilden Reigen,", "tokens": ["Der", "Wind", "nun", "auf", "zum", "wil\u00b7den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sausende Gewitterspur.", "tokens": ["Die", "sau\u00b7sen\u00b7de", "Ge\u00b7wit\u00b7ter\u00b7spur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Himmel eilt mit dumpfem Klange", "tokens": ["Am", "Him\u00b7mel", "eilt", "mit", "dum\u00b7pfem", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herauf der finstre Wolkenzug:", "tokens": ["Her\u00b7auf", "der", "finst\u00b7re", "Wol\u00b7ken\u00b7zug", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So nimmt der Zorn im hei\u00dfen Drange", "tokens": ["So", "nimmt", "der", "Zorn", "im", "hei\u00b7\u00dfen", "Dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den n\u00e4chtlichen Gedankenflug. \u2014", "tokens": ["Den", "n\u00e4cht\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken\u00b7flug", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Himmel donnert seinen Hader;", "tokens": ["Der", "Him\u00b7mel", "don\u00b7nert", "sei\u00b7nen", "Ha\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seiner dunklen Stirne gl\u00fcht", "tokens": ["Auf", "sei\u00b7ner", "dunk\u00b7len", "Stir\u00b7ne", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Blitz hervor, die Zornesader,", "tokens": ["Der", "Blitz", "her\u00b7vor", ",", "die", "Zor\u00b7ne\u00b7sa\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schrecken auf die Erde spr\u00fcht.", "tokens": ["Die", "Schre\u00b7cken", "auf", "die", "Er\u00b7de", "spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Regen st\u00fcrzt in lauten G\u00fcssen;", "tokens": ["Der", "Re\u00b7gen", "st\u00fcrzt", "in", "lau\u00b7ten", "G\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit B\u00e4umen, die der Sturm zerbrach,", "tokens": ["Mit", "B\u00e4u\u00b7men", ",", "die", "der", "Sturm", "zer\u00b7brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erbraust der Strom zu meinen F\u00fc\u00dfen; \u2014", "tokens": ["Er\u00b7braust", "der", "Strom", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch schweigt der Donner allgemach.", "tokens": ["Doch", "schweigt", "der", "Don\u00b7ner", "all\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Sturm l\u00e4\u00dft seine Fl\u00fcgel sinken,", "tokens": ["Der", "Sturm", "l\u00e4\u00dft", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Regen s\u00e4uselt milde Ruh:", "tokens": ["Der", "Re\u00b7gen", "s\u00e4u\u00b7selt", "mil\u00b7de", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sah ich froh ein H\u00fcttlein winken,", "tokens": ["Da", "sah", "ich", "froh", "ein", "H\u00fctt\u00b7lein", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und eilte seiner Pforte zu.", "tokens": ["Und", "eil\u00b7te", "sei\u00b7ner", "Pfor\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}