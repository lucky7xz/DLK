{"dta.poem.8031": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Nimmer mochten ihn verwunden", "tokens": ["Nim\u00b7mer", "moch\u00b7ten", "ihn", "ver\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebesblicke s\u00fc\u00dfer Sch\u00f6nen,", "tokens": ["Lie\u00b7bes\u00b7bli\u00b7cke", "s\u00fc\u00b7\u00dfer", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer mochten ihn bezwingen", "tokens": ["Nim\u00b7mer", "moch\u00b7ten", "ihn", "be\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwerdterschl\u00e4ge, Lanzenst\u00f6\u00dfe.", "tokens": ["Schwerd\u00b7ter\u00b7schl\u00e4\u00b7ge", ",", "Lan\u00b7zen\u00b7st\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als er einsam ritt auf Bergen,", "tokens": ["Als", "er", "ein\u00b7sam", "ritt", "auf", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fuhr ein Blitz aus dem Gew\u00f6lke;", "tokens": ["Fuhr", "ein", "Blitz", "aus", "dem", "Ge\u00b7w\u00f6l\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so ist er unterlegen", "tokens": ["Und", "so", "ist", "er", "un\u00b7ter\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur dem Stral von Himmelsh\u00f6hen.", "tokens": ["Nur", "dem", "Stral", "von", "Him\u00b7mels\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}