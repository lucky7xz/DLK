{"dta.poem.11436": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun glaub ich, was man sonst als eine fabel liest:", "tokens": ["Nun", "glaub", "ich", ",", "was", "man", "sonst", "als", "ei\u00b7ne", "fa\u00b7bel", "liest", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PIS", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Themis in der that nicht mehr auf erden ist.", "tokens": ["Da\u00df", "The\u00b7mis", "in", "der", "that", "nicht", "mehr", "auf", "er\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Glaubst du es nicht? So sag: Ob auch zu dieser zeit", "tokens": ["Glaubst", "du", "es", "nicht", "?", "So", "sag", ":", "Ob", "auch", "zu", "die\u00b7ser", "zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$.", "ADV", "VVFIN", "$.", "KOUS", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ungerechters ist, als die gerechtigkeit?", "tokens": ["Was", "un\u00b7ge\u00b7rech\u00b7ters", "ist", ",", "als", "die", "ge\u00b7rech\u00b7tig\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV"