: "Macht uns bereit,", "tokens": ["Macht", "uns", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Barmherzigkeit", "tokens": ["Barm\u00b7her\u00b7zig\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den Armen zu erzeigen,", "tokens": ["Den", "Ar\u00b7men", "zu", "er\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie auch beherzt,", "tokens": ["Wie", "auch", "be\u00b7herzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Das was uns schmerzt", "tokens": ["Das", "was", "uns", "schmerzt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Zu eifern und zu l\u00e4stern,", "tokens": ["Zu", "ei\u00b7fern", "und", "zu", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ertheilt die Kunst", "tokens": ["Er\u00b7theilt", "die", "Kunst"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und alle Gunst", "tokens": ["Und", "al\u00b7le", "Gunst"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Der dreymal dreyen Schwestern.", "tokens": ["Der", "drey\u00b7mal", "drey\u00b7en", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Daher man sieht,", "tokens": ["Da\u00b7her", "man", "sieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wenn wir hiemit", "tokens": ["Wenn", "wir", "hie\u00b7mit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Nase schon begossen,", "tokens": ["Die", "Na\u00b7se", "schon", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie dann der Flu\u00df", "tokens": ["Wie", "dann", "der", "Flu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Des Pegasus", "tokens": ["Des", "Pe\u00b7ga\u00b7sus"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kommt auf uns zugeschossen,", "tokens": ["Kommt", "auf", "uns", "zu\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der will dann ein", "tokens": ["Der", "will", "dann", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Poete seyn,", "tokens": ["Poe\u00b7te", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Der kann viel Streitens machen", "tokens": ["Der", "kann", "viel", "Strei\u00b7tens", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Von der Natur,", "tokens": ["Von", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Der redet nur", "tokens": ["Der", "re\u00b7det", "nur"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Von Gottes hohen Sachen.", "tokens": ["Von", "Got\u00b7tes", "ho\u00b7hen", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dort hat ein Paar", "tokens": ["Dort", "hat", "ein", "Paar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Sich bey dem Haar,", "tokens": ["Sich", "bey", "dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der greift nach seinem Degen,", "tokens": ["Der", "greift", "nach", "sei\u00b7nem", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der steht und speit,", "tokens": ["Der", "steht", "und", "speit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der jauchzt und schreit", "tokens": ["Der", "jauchzt", "und", "schreit"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und kann sich kaum noch regen.", "tokens": ["Und", "kann", "sich", "kaum", "noch", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der s\u00e4uft dem zu", "tokens": ["Der", "s\u00e4uft", "dem", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "PTKZU"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Auf einen Du,", "tokens": ["Auf", "ei\u00b7nen", "Du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der schwatzt von seinen Kriegen,", "tokens": ["Der", "schwatzt", "von", "sei\u00b7nen", "Krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der sitzt und weist", "tokens": ["Der", "sitzt", "und", "weist"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Wo er gereist,", "tokens": ["Wo", "er", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und scheut sich nicht zu l\u00fcgen.", "tokens": ["Und", "scheut", "sich", "nicht", "zu", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Auch mir wird izt", "tokens": ["Auch", "mir", "wird", "izt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Kopf erhitzt,", "tokens": ["Der", "Kopf", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O Wein, von deinen Gaben,", "tokens": ["O", "Wein", ",", "von", "dei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zunge singt", "tokens": ["Die", "Zun\u00b7ge", "singt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Seele springt,", "tokens": ["Die", "See\u00b7le", "springt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die F\u00fcsse wollen traben,", "tokens": ["Die", "F\u00fcs\u00b7se", "wol\u00b7len", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohlan noch ba\u00df", "tokens": ["Wo\u00b7hlan", "noch", "ba\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Durch dieses Glas", "tokens": ["Durch", "die\u00b7ses", "Glas"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Will ich auf dich jezt zielen.", "tokens": ["Will", "ich", "auf", "dich", "jezt", "zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Du deutsches Blut", "tokens": ["Du", "deut\u00b7sches", "Blut"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "La\u00df mir ein gut", "tokens": ["La\u00df", "mir", "ein", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Rundadinella spielen.", "tokens": ["Run\u00b7da\u00b7di\u00b7nel\u00b7la", "spie\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}