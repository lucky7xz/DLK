{"dta.poem.14887": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Zieht Gesichter ihm und rauschet,", "tokens": ["Zieht", "Ge\u00b7sich\u00b7ter", "ihm", "und", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterh\u00e4lt aus dunklem Grunde", "tokens": ["Un\u00b7ter\u00b7h\u00e4lt", "aus", "dunk\u00b7lem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich mit ihm als wie im Traume.", "tokens": ["Sich", "mit", "ihm", "als", "wie", "im", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "KOUS", "KOKOM", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und er spricht und sinnt und sinnet,", "tokens": ["Und", "er", "spricht", "und", "sinnt", "und", "sin\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bunt verwirrend alle Zeiten,", "tokens": ["Bunt", "ver\u00b7wir\u00b7rend", "al\u00b7le", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weinet bitterlich und lachet,", "tokens": ["Wei\u00b7net", "bit\u00b7ter\u00b7lich", "und", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Seele ist so heiter.", "tokens": ["Sei\u00b7ne", "See\u00b7le", "ist", "so", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}