{"textgrid.poem.67102": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00fcngling", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sollte ruhn? Ich soll die Liebe zwingen,", "tokens": ["Ich", "soll\u00b7te", "ruhn", "?", "Ich", "soll", "die", "Lie\u00b7be", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die feurigfroh nach hoher Sch\u00f6ne strebt?", "tokens": ["Die", "feu\u00b7rig\u00b7froh", "nach", "ho\u00b7her", "Sch\u00f6\u00b7ne", "strebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich soll mein Schwanenlied am Grabe singen,", "tokens": ["Ich", "soll", "mein", "Schwa\u00b7nen\u00b7lied", "am", "Gra\u00b7be", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo ihr so gern lebendig uns begr\u00e4bt?", "tokens": ["Wo", "ihr", "so", "gern", "le\u00b7ben\u00b7dig", "uns", "be\u00b7gr\u00e4bt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O schonet mein! Allm\u00e4chtig fortgezogen,", "tokens": ["O", "scho\u00b7net", "mein", "!", "All\u00b7m\u00e4ch\u00b7tig", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "$.", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mu\u00df immerhin des Lebens frische Flut", "tokens": ["Mu\u00df", "im\u00b7mer\u00b7hin", "des", "Le\u00b7bens", "fri\u00b7sche", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit Ungeduld im engen Bette wogen,", "tokens": ["Mit", "Un\u00b7ge\u00b7duld", "im", "en\u00b7gen", "Bet\u00b7te", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bis sie im heimatlichen Meere ruht.", "tokens": ["Bis", "sie", "im", "hei\u00b7mat\u00b7li\u00b7chen", "Mee\u00b7re", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Weins Gew\u00e4chs verschm\u00e4ht die k\u00fchlen Tale,", "tokens": ["Des", "Weins", "Ge\u00b7w\u00e4chs", "ver\u00b7schm\u00e4ht", "die", "k\u00fch\u00b7len", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hesperiens begl\u00fcckter Garten bringt", "tokens": ["Hes\u00b7pe\u00b7ri\u00b7ens", "be\u00b7gl\u00fcck\u00b7ter", "Gar\u00b7ten", "bringt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die goldnen Fr\u00fcchte nur im hei\u00dfen Strahle,", "tokens": ["Die", "gold\u00b7nen", "Fr\u00fcch\u00b7te", "nur", "im", "hei\u00b7\u00dfen", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der, wie ein Pfeil, ins Herz der Erde dringt.", "tokens": ["Der", ",", "wie", "ein", "Pfeil", ",", "ins", "Herz", "der", "Er\u00b7de", "dringt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was s\u00e4nftiget ihr dann, wenn in den Ketten", "tokens": ["Was", "s\u00e4nf\u00b7ti\u00b7get", "ihr", "dann", ",", "wenn", "in", "den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der ehrnen Zeit die Seele mir entbrennt,", "tokens": ["Der", "ehr\u00b7nen", "Zeit", "die", "See\u00b7le", "mir", "ent\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was nimmt ihr mir, den nur die K\u00e4mpfe retten,", "tokens": ["Was", "nimmt", "ihr", "mir", ",", "den", "nur", "die", "K\u00e4mp\u00b7fe", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr Weichlinge! mein gl\u00fchend Element?", "tokens": ["Ihr", "Weich\u00b7lin\u00b7ge", "!", "mein", "gl\u00fc\u00b7hend", "E\u00b7le\u00b7ment", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "ADJD", "NN", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Leben ist zum Tode nicht erkoren,", "tokens": ["Das", "Le\u00b7ben", "ist", "zum", "To\u00b7de", "nicht", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Schlafe nicht der Gott, der uns entflammt,", "tokens": ["Zum", "Schla\u00b7fe", "nicht", "der", "Gott", ",", "der", "uns", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Joch ist nicht der Herrliche geboren,", "tokens": ["Zum", "Joch", "ist", "nicht", "der", "Herr\u00b7li\u00b7che", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Genius, der aus dem Aether stammt;", "tokens": ["Der", "Ge\u00b7nius", ",", "der", "aus", "dem", "A\u00b7e\u00b7ther", "stammt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Er kommt herab; er taucht sich, wie zum Bade,", "tokens": ["Er", "kommt", "her\u00b7ab", ";", "er", "taucht", "sich", ",", "wie", "zum", "Ba\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PRF", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In des Jahrhunderts Strom und gl\u00fccklich raubt", "tokens": ["In", "des", "Jahr\u00b7hun\u00b7derts", "Strom", "und", "gl\u00fcck\u00b7lich", "raubt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "ADJD", "VVFIN"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Auf eine Zeit den Schwimmer die Najade,", "tokens": ["Auf", "ei\u00b7ne", "Zeit", "den", "Schwim\u00b7mer", "die", "Na\u00b7ja\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch hebt er heitrer bald sein leuchtend Haupt.", "tokens": ["Doch", "hebt", "er", "hei\u00b7trer", "bald", "sein", "leuch\u00b7tend", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum la\u00dft die Lust, das Gro\u00dfe zu verderben,", "tokens": ["Drum", "la\u00dft", "die", "Lust", ",", "das", "Gro\u00b7\u00dfe", "zu", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und geht und sprecht von eurem Gl\u00fccke nicht!", "tokens": ["Und", "geht", "und", "sprecht", "von", "eu\u00b7rem", "Gl\u00fc\u00b7cke", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pflanzt keinen Zedernbaum in eure Scherben!", "tokens": ["Pflanzt", "kei\u00b7nen", "Ze\u00b7dern\u00b7baum", "in", "eu\u00b7re", "Scher\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nimmt keinen Geist in eure S\u00f6ldnerspflicht!", "tokens": ["Nimmt", "kei\u00b7nen", "Geist", "in", "eu\u00b7re", "S\u00f6ld\u00b7ners\u00b7pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Versucht es nicht, das Sonnenro\u00df zu l\u00e4hmen!", "tokens": ["Ver\u00b7sucht", "es", "nicht", ",", "das", "Son\u00b7nen\u00b7ro\u00df", "zu", "l\u00e4h\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "La\u00dft immerhin den Sternen ihre Bahn!", "tokens": ["La\u00dft", "im\u00b7mer\u00b7hin", "den", "Ster\u00b7nen", "ih\u00b7re", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mir, mir ratet nicht, mich zu bequemen,", "tokens": ["Und", "mir", ",", "mir", "ra\u00b7tet", "nicht", ",", "mich", "zu", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und macht mich nicht den Knechten untertan.", "tokens": ["Und", "macht", "mich", "nicht", "den", "Knech\u00b7ten", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und k\u00f6nnt ihr ja das Sch\u00f6ne nicht ertragen,", "tokens": ["Und", "k\u00f6nnt", "ihr", "ja", "das", "Sch\u00f6\u00b7ne", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So f\u00fchrt den Krieg mit offner Kraft und Tat!", "tokens": ["So", "f\u00fchrt", "den", "Krieg", "mit", "off\u00b7ner", "Kraft", "und", "Tat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sonst ward der Schw\u00e4rmer doch ans Kreuz geschlagen,", "tokens": ["Sonst", "ward", "der", "Schw\u00e4r\u00b7mer", "doch", "ans", "Kreuz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt mordet ihn der sanfte kluge Rat;", "tokens": ["Jetzt", "mor\u00b7det", "ihn", "der", "sanf\u00b7te", "klu\u00b7ge", "Rat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie manchen habt ihr herrlich zubereitet", "tokens": ["Wie", "man\u00b7chen", "habt", "ihr", "herr\u00b7lich", "zu\u00b7be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcrs Reich der Not! wie oft auf euern Sand", "tokens": ["F\u00fcrs", "Reich", "der", "Not", "!", "wie", "oft", "auf", "eu\u00b7ern", "Sand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "$.", "PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den hoffnungsfrohen Steuermann verleitet", "tokens": ["Den", "hoff\u00b7nungs\u00b7fro\u00b7hen", "Steu\u00b7er\u00b7mann", "ver\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf k\u00fchner Fahrt ins warme Morgenland!", "tokens": ["Auf", "k\u00fch\u00b7ner", "Fahrt", "ins", "war\u00b7me", "Mor\u00b7gen\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Umsonst! mich h\u00e4lt die d\u00fcrre Zeit vergebens,", "tokens": ["Um\u00b7sonst", "!", "mich", "h\u00e4lt", "die", "d\u00fcr\u00b7re", "Zeit", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mein Jahrhundert ist mir Z\u00fcchtigung;", "tokens": ["Und", "mein", "Jahr\u00b7hun\u00b7dert", "ist", "mir", "Z\u00fcch\u00b7ti\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sehne mich ins gr\u00fcne Feld des Lebens", "tokens": ["Ich", "seh\u00b7ne", "mich", "ins", "gr\u00fc\u00b7ne", "Feld", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in den Himmel der Begeisterung;", "tokens": ["Und", "in", "den", "Him\u00b7mel", "der", "Be\u00b7geis\u00b7te\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Begrabt sie nur, ihr Toten, eure Toten,", "tokens": ["Be\u00b7grabt", "sie", "nur", ",", "ihr", "To\u00b7ten", ",", "eu\u00b7re", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und preist das Menschenwerk und scheltet nur!", "tokens": ["Und", "preist", "das", "Men\u00b7schen\u00b7werk", "und", "schel\u00b7tet", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch reift in mir, so wie mein Herz geboten,", "tokens": ["Doch", "reift", "in", "mir", ",", "so", "wie", "mein", "Herz", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], 