{"dta.poem.12144": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Wetter wechselt, und es wechseln Menschenkaunen,", "tokens": ["Das", "Wet\u00b7ter", "wech\u00b7selt", ",", "und", "es", "wech\u00b7seln", "Men\u00b7schen\u00b7kau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Landschaft wechselt auch; was ist da gro\u00df zu staunen,", "tokens": ["Die", "Land\u00b7schaft", "wech\u00b7selt", "auch", ";", "was", "ist", "da", "gro\u00df", "zu", "stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "PWS", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Wanderer, wenn du bist dreifach launenhaft,", "tokens": ["O", "Wan\u00b7de\u00b7rer", ",", "wenn", "du", "bist", "drei\u00b7fach", "lau\u00b7nen\u00b7haft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach der Natur, der Reis' und deiner Eigenschaft!", "tokens": ["Nach", "der", "Na\u00b7tur", ",", "der", "Reis'", "und", "dei\u00b7ner", "Ei\u00b7gen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "KON", "PPOSAT