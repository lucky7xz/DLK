{"dta.poem.20065": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Rauh ging der Wind, der Regen troff,", "tokens": ["Rauh", "ging", "der", "Wind", ",", "der", "Re\u00b7gen", "troff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon war ich na\u00df und kalt;", "tokens": ["Schon", "war", "ich", "na\u00df", "und", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich macht\u2019 auf einem Bauerhof", "tokens": ["Ich", "macht'", "auf", "ei\u00b7nem", "Bau\u00b7er\u00b7hof"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schutz des Zaunes Halt.", "tokens": ["Im", "Schutz", "des", "Zau\u00b7nes", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit abgestutzten Fl\u00fcgeln schritt", "tokens": ["Mit", "ab\u00b7ge\u00b7stutz\u00b7ten", "Fl\u00fc\u00b7geln", "schritt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kranich drin umher,", "tokens": ["Ein", "Kra\u00b7nich", "drin", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur seine Sehnsucht trug ihn mit", "tokens": ["Nur", "sei\u00b7ne", "Sehn\u00b7sucht", "trug", "ihn", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Br\u00fcdern \u00fcber\u2019s Meer;", "tokens": ["Den", "Br\u00fc\u00b7dern", "\u00fc\u00b7ber's", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit seinen Br\u00fcdern, deren Zug", "tokens": ["Mit", "sei\u00b7nen", "Br\u00fc\u00b7dern", ",", "de\u00b7ren", "Zug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt hoch in L\u00fcften stockt,", "tokens": ["Jetzt", "hoch", "in", "L\u00fcf\u00b7ten", "stockt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deren Schrei auch ihn zum Flug", "tokens": ["Und", "de\u00b7ren", "Schrei", "auch", "ihn", "zum", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELAT", "NN", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gen S\u00fcden ruft und lockt.", "tokens": ["Gen", "S\u00fc\u00b7den", "ruft", "und", "lockt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und sieh, er hat sich aufgerafft,", "tokens": ["Und", "sieh", ",", "er", "hat", "sich", "auf\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gilt ja Lenz und Gl\u00fcck;", "tokens": ["Es", "gilt", "ja", "Lenz", "und", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umsonst, der Schwinge fehlt die Kraft", "tokens": ["Um\u00b7sonst", ",", "der", "Schwin\u00b7ge", "fehlt", "die", "Kraft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ach, er sinkt zur\u00fcck.", "tokens": ["Und", "ach", ",", "er", "sinkt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nur Hahn und Huhn zum Schabernack", "tokens": ["Nur", "Hahn", "und", "Huhn", "zum", "Scha\u00b7ber\u00b7nack"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umkr\u00e4hn ihn jetzt voll Freud: \u2014", "tokens": ["Um\u00b7kr\u00e4hn", "ihn", "jetzt", "voll", "Freud", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PPER", "ADV", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es jubelt stets das H\u00fchnerpack", "tokens": ["Es", "ju\u00b7belt", "stets", "das", "H\u00fch\u00b7ner\u00b7pack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei eines Kranichs Leid.", "tokens": ["Bei", "ei\u00b7nes", "Kra\u00b7nichs", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}