{"dta.poem.16347": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "8.  GOtt lebt nicht ohne mich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df da\u00df ohne mich GOtt nicht ein Nun kan leben/", "tokens": ["Ich", "wei\u00df", "da\u00df", "oh\u00b7ne", "mich", "Gott", "nicht", "ein", "Nun", "kan", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "APPR", "PPER", "NN", "PTKNEG", "ART", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Werd' ich zu nicht Er mu\u00df von Noth den Geist auff-", "tokens": ["Werd'", "ich", "zu", "nicht", "Er", "mu\u00df", "von", "Noth", "den", "Geist", "auf\u00b7f"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PTKNEG", "PPER", "VMFIN", "APPR", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "geben.", "tokens": ["ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}