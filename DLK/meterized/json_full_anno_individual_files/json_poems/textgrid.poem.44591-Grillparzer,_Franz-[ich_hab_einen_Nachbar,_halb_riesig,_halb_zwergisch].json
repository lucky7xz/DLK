{"textgrid.poem.44591": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ich hab einen Nachbar, halb riesig, halb zwergisch]", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab einen Nachbar, halb riesig, halb zwergisch,", "tokens": ["Ich", "hab", "ei\u00b7nen", "Nach\u00b7bar", ",", "halb", "rie\u00b7sig", ",", "halb", "zwer\u00b7gisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD", "ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das kommt daher: der Mann ist energisch.", "tokens": ["Das", "kommt", "da\u00b7her", ":", "der", "Mann", "ist", "e\u00b7ner\u00b7gisch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er will das H\u00f6chste, was keiner getan,", "tokens": ["Er", "will", "das", "H\u00f6chs\u00b7te", ",", "was", "kei\u00b7ner", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit jedem Nachbar f\u00e4ngt er H\u00e4ndel an,", "tokens": ["Mit", "je\u00b7dem", "Nach\u00b7bar", "f\u00e4ngt", "er", "H\u00e4n\u00b7del", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er kennt nur Verweigern und Widerstreben,", "tokens": ["Er", "kennt", "nur", "Ver\u00b7wei\u00b7gern", "und", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Einem Knaben hat er j\u00fcngst einen Fu\u00dftritt gegeben,", "tokens": ["Ei\u00b7nem", "Kna\u00b7ben", "hat", "er", "j\u00fcngst", "ei\u00b7nen", "Fu\u00df\u00b7tritt", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Ein mildes Wort, das die Feindschaft br\u00e4che,", "tokens": ["Ein", "mil\u00b7des", "Wort", ",", "das", "die", "Feind\u00b7schaft", "br\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Verschm\u00e4ht er als ein Zeichen der Schw\u00e4che.", "tokens": ["Ver\u00b7schm\u00e4ht", "er", "als", "ein", "Zei\u00b7chen", "der", "Schw\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Gehts nun nicht weiter, ja r\u00fcckw\u00e4rts kl\u00e4glich,", "tokens": ["Gehts", "nun", "nicht", "wei\u00b7ter", ",", "ja", "r\u00fcck\u00b7w\u00e4rts", "kl\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "PTKVZ", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "So tr\u00f6stet er sich, das Ding sei unm\u00f6glich.", "tokens": ["So", "tr\u00f6s\u00b7tet", "er", "sich", ",", "das", "Ding", "sei", "un\u00b7m\u00f6g\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Es ist auch so, doch hat er nicht acht,", "tokens": ["Es", "ist", "auch", "so", ",", "doch", "hat", "er", "nicht", "acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "CARD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Da\u00df nur er selbst es unm\u00f6glich gemacht.", "tokens": ["Da\u00df", "nur", "er", "selbst", "es", "un\u00b7m\u00f6g\u00b7lich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich hab einen Nachbar, halb riesig, halb zwergisch,", "tokens": ["Ich", "hab", "ei\u00b7nen", "Nach\u00b7bar", ",", "halb", "rie\u00b7sig", ",", "halb", "zwer\u00b7gisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD", "ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das kommt daher: der Mann ist energisch.", "tokens": ["Das", "kommt", "da\u00b7her", ":", "der", "Mann", "ist", "e\u00b7ner\u00b7gisch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.