{"dta.poem.3200": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in dem Winter stirbt, warum sollt' er nicht sterben,", "tokens": ["Wer", "in", "dem", "Win\u00b7ter", "stirbt", ",", "wa\u00b7rum", "sollt'", "er", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo alle Bl\u00e4tter von des Frostes Hauch verderben?", "tokens": ["Wo", "al\u00b7le", "Bl\u00e4t\u00b7ter", "von", "des", "Fros\u00b7tes", "Hauch", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wer im Sommer stirbt, wo alle Blumen bl\u00fchn,", "tokens": ["Und", "wer", "im", "Som\u00b7mer", "stirbt", ",", "wo", "al\u00b7le", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie w\u00e4r' er todt? sein Grab macht Lebenshoffnung gr\u00fcn.", "tokens": ["Wie", "w\u00e4r'", "er", "todt", "?", "sein", "Grab", "macht", "Le\u00b7bens\u00b7hoff\u00b7nung", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$.", "PPOSAT", "NN", "VVFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum wer im Sommer dir und wer im Winter starb,", "tokens": ["Drum", "wer", "im", "Som\u00b7mer", "dir", "und", "wer", "im", "Win\u00b7ter", "starb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "APPRART", "NN", "PPER", "KON", "PWS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Natur hat einen Trost, Heil dem, der ihn erwarb.", "tokens": ["Na\u00b7tur", "hat", "ei\u00b7nen", "Trost", ",", "Heil", "dem", ",", "der", "ihn", "er\u00b7warb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}