 "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es zog mich unaufhaltsam.", "tokens": ["Es", "zog", "mich", "un\u00b7auf\u00b7halt\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sein Lied ist stark als wie der Tod,", "tokens": ["Sein", "Lied", "ist", "stark", "als", "wie", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lockt in Nacht und Verderben.", "tokens": ["Es", "lockt", "in", "Nacht", "und", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch brennt mir im Herzen die t\u00f6nende Glut;", "tokens": ["Noch", "brennt", "mir", "im", "Her\u00b7zen", "die", "t\u00f6\u00b7nen\u00b7de", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich wei\u00df, jetzt mu\u00df ich sterben.\u00ab \u2013", "tokens": ["Ich", "wei\u00df", ",", "jetzt", "mu\u00df", "ich", "ster\u00b7ben", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": 