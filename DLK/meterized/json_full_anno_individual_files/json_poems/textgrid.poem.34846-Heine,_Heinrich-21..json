{"textgrid.poem.34846": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Peter und Bender sa\u00dfen beim Wein,", "tokens": ["Herr", "Pe\u00b7ter", "und", "Ben\u00b7der", "sa\u00b7\u00dfen", "beim", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Herr Bender sprach: \u00bbIch wette,", "tokens": ["Herr", "Ben\u00b7der", "sprach", ":", "\u00bb", "Ich", "wet\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bezw\u00e4nge dein Singen die ganze Welt,", "tokens": ["Be\u00b7zw\u00e4n\u00b7ge", "dein", "Sin\u00b7gen", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Doch nimmer bezwingt es Frau Mette.\u00ab", "tokens": ["Doch", "nim\u00b7mer", "be\u00b7zwingt", "es", "Frau", "Met\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "NE", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Herr Peter sprach: \u00bbIch wette mein Ro\u00df,", "tokens": ["Herr", "Pe\u00b7ter", "sprach", ":", "\u00bb", "Ich", "wet\u00b7te", "mein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wohl gegen deine Hunde,", "tokens": ["Wohl", "ge\u00b7gen", "dei\u00b7ne", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frau Mette sing ich nach meinem Hof,", "tokens": ["Frau", "Met\u00b7te", "sing", "ich", "nach", "mei\u00b7nem", "Hof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch heut, in der Mitternachtstunde.\u00ab", "tokens": ["Noch", "heut", ",", "in", "der", "Mit\u00b7ter\u00b7nachts\u00b7tun\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und als die Mitternachtstunde kam,", "tokens": ["Und", "als", "die", "Mit\u00b7ter\u00b7nachts\u00b7tun\u00b7de", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Herr Peter hub an zu singen;", "tokens": ["Herr", "Pe\u00b7ter", "hub", "an", "zu", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wohl \u00fcber den Flu\u00df, wohl \u00fcber den Wald", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "Flu\u00df", ",", "wohl", "\u00fc\u00b7ber", "den", "Wald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die s\u00fc\u00dfen T\u00f6ne dringen.", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7ne", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Tannenb\u00e4ume horchen so still,", "tokens": ["Die", "Tan\u00b7nen\u00b7b\u00e4u\u00b7me", "hor\u00b7chen", "so", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Flut h\u00f6rt auf zu rauschen,", "tokens": ["Die", "Flut", "h\u00f6rt", "auf", "zu", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Himmel zittert der blasse Mond,", "tokens": ["Am", "Him\u00b7mel", "zit\u00b7tert", "der", "blas\u00b7se", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die klugen Sterne lauschen.", "tokens": ["Die", "klu\u00b7gen", "Ster\u00b7ne", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Frau Mette erwacht aus ihrem Schlaf:", "tokens": ["Frau", "Met\u00b7te", "er\u00b7wacht", "aus", "ih\u00b7rem", "Schlaf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbwer singt vor meiner Kammer?\u00ab", "tokens": ["\u00bb", "wer", "singt", "vor", "mei\u00b7ner", "Kam\u00b7mer", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie achselt ihr Kleid, sie schreitet hinaus; \u2013", "tokens": ["Sie", "ach\u00b7selt", "ihr", "Kleid", ",", "sie", "schrei\u00b7tet", "hin\u00b7aus", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ward zu gro\u00dfem Jammer.", "tokens": ["Das", "ward", "zu", "gro\u00b7\u00dfem", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl durch den Wald, wohl durch den Flu\u00df", "tokens": ["Wohl", "durch", "den", "Wald", ",", "wohl", "durch", "den", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schreitet unaufhaltsam;", "tokens": ["Sie", "schrei\u00b7tet", "un\u00b7auf\u00b7halt\u00b7sam", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr Peter zog sie nach seinem Hof", "tokens": ["Herr", "Pe\u00b7ter", "zog", "sie", "nach", "sei\u00b7nem", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit seinem Liede gewaltsam.", "tokens": ["Mit", "sei\u00b7nem", "Lie\u00b7de", "ge\u00b7walt\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und als sie morgens nach Hause kam,", "tokens": ["Und", "als", "sie", "mor\u00b7gens", "nach", "Hau\u00b7se", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor der T\u00fcre stand Herr Bender:", "tokens": ["Vor", "der", "T\u00fc\u00b7re", "stand", "Herr", "Ben\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbfrau Mette, wo bist du gewesen zur Nacht,", "tokens": ["\u00bb", "frau", "Met\u00b7te", ",", "wo", "bist", "du", "ge\u00b7we\u00b7sen", "zur", "Nacht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "PWAV", "VAFIN", "PPER", "VAPP", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es triefen deine Gew\u00e4nder?\u00ab", "tokens": ["Es", "trie\u00b7fen", "dei\u00b7ne", "Ge\u00b7w\u00e4n\u00b7der", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbich war heut nacht am Nixenflu\u00df,", "tokens": ["\u00bb", "ich", "war", "heut", "nacht", "am", "Ni\u00b7xen\u00b7flu\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort h\u00f6rt ich prophezeien,", "tokens": ["Dort", "h\u00f6rt", "ich", "pro\u00b7phe\u00b7zei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es pl\u00e4tscherten und bespritzten mich", "tokens": ["Es", "pl\u00e4t\u00b7scher\u00b7ten", "und", "be\u00b7spritz\u00b7ten", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die neckenden Wasserfeien.\u00ab", "tokens": ["Die", "ne\u00b7cken\u00b7den", "Was\u00b7ser\u00b7fei\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbam Nixenflu\u00df ist feiner Sand,", "tokens": ["\u00bb", "am", "Ni\u00b7xen\u00b7flu\u00df", "ist", "fei\u00b7ner", "Sand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort bist du nicht gegangen,", "tokens": ["Dort", "bist", "du", "nicht", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zerrissen und blutig sind deine F\u00fc\u00df',", "tokens": ["Zer\u00b7ris\u00b7sen", "und", "blu\u00b7tig", "sind", "dei\u00b7ne", "F\u00fc\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auch bluten deine Wangen.\u00ab", "tokens": ["Auch", "blu\u00b7ten", "dei\u00b7ne", "Wan\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbich war heut nacht im Elfenwald,", "tokens": ["\u00bb", "ich", "war", "heut", "nacht", "im", "El\u00b7fen\u00b7wald", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu schauen den Elfenreigen,", "tokens": ["Zu", "schau\u00b7en", "den", "El\u00b7fen\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich hab mir verwundet Fu\u00df und Gesicht,", "tokens": ["Ich", "hab", "mir", "ver\u00b7wun\u00b7det", "Fu\u00df", "und", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An Dornen und Tannenzweigen.\u00ab", "tokens": ["An", "Dor\u00b7nen", "und", "Tan\u00b7nen\u00b7zwei\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbdie Elfen tanzen im Monat Mai,", "tokens": ["\u00bb", "die", "El\u00b7fen", "tan\u00b7zen", "im", "Mo\u00b7nat", "Mai", ","], "token_info": ["punct", "word", 