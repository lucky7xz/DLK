{"dta.poem.15574": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die eine Hoffnung hast du kaum zu Grab getragen,", "tokens": ["Die", "ei\u00b7ne", "Hoff\u00b7nung", "hast", "du", "kaum", "zu", "Grab", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und andre Knosp' am Strauch beginnt schon auszuschlagen.", "tokens": ["Und", "and\u00b7re", "Knosp'", "am", "Strauch", "be\u00b7ginnt", "schon", "aus\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Oh doppelt theuer ist die also neugeborne,", "tokens": ["Oh", "dop\u00b7pelt", "theu\u00b7er", "ist", "die", "al\u00b7so", "neu\u00b7ge\u00b7bor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "ADJD", "VAFIN", "ART", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der du zwei nun hast, sie selbst und die verlorne.", "tokens": ["In", "der", "du", "zwei", "nun", "hast", ",", "sie", "selbst", "und", "die", "ver\u00b7lor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "CARD", "ADV", "VAFIN", "$,", "PPER", "ADV", "KON", "ART", "ADJA", "$."], "m