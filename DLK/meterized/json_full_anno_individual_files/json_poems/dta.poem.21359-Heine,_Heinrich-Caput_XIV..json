{"dta.poem.21359": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Marstall ist der erste Saal,", "tokens": ["Ein", "Mar\u00b7stall", "ist", "der", "ers\u00b7te", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dorten kann man sehen", "tokens": ["Und", "dor\u00b7ten", "kann", "man", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel tausend Pferde, blankgeschirrt,", "tokens": ["Viel", "tau\u00b7send", "Pfer\u00b7de", ",", "blank\u00b7ge\u00b7schirrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die an den Krippen stehen.", "tokens": ["Die", "an", "den", "Krip\u00b7pen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sind gesattelt und gez\u00e4umt,", "tokens": ["Sie", "sind", "ge\u00b7sat\u00b7telt", "und", "ge\u00b7z\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch von diesen Rossen", "tokens": ["Je\u00b7doch", "von", "die\u00b7sen", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein einziges wiehert, kein einziges stampft,", "tokens": ["Kein", "ein\u00b7zi\u00b7ges", "wie\u00b7hert", ",", "kein", "ein\u00b7zi\u00b7ges", "stampft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "$,", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sind still, wie aus Eisen gegossen.", "tokens": ["Sind", "still", ",", "wie", "aus", "Ei\u00b7sen", "ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Im zweiten Saale, auf der Streu,", "tokens": ["Im", "zwei\u00b7ten", "Saa\u00b7le", ",", "auf", "der", "Streu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht man Soldaten liegen,", "tokens": ["Sieht", "man", "Sol\u00b7da\u00b7ten", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel tausend Soldaten, b\u00e4rtiges Volk,", "tokens": ["Viel", "tau\u00b7send", "Sol\u00b7da\u00b7ten", ",", "b\u00e4r\u00b7ti\u00b7ges", "Volk", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit kriegerisch trotzigen Z\u00fcgen.", "tokens": ["Mit", "krie\u00b7ge\u00b7risch", "trot\u00b7zi\u00b7gen", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Sie sind ger\u00fcstet von Kopf bis Fu\u00df,", "tokens": ["Sie", "sind", "ge\u00b7r\u00fcs\u00b7tet", "von", "Kopf", "bis", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch alle diese Braven,", "tokens": ["Doch", "al\u00b7le", "die\u00b7se", "Bra\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie r\u00fchren sich nicht, bewegen sich nicht,", "tokens": ["Sie", "r\u00fch\u00b7ren", "sich", "nicht", ",", "be\u00b7we\u00b7gen", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$,", "VVFIN", "PRF", "PTKNEG", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie liegen fest und schlafen.", "tokens": ["Sie", "lie\u00b7gen", "fest", "und", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}