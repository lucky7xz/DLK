{"dta.poem.12229": {"metadata": {"author": {"name": "Nietzsche, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das n\u00e4chtliche Geheimniss", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-30172-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "             Gestern Nachts, als Alles schlief,", "tokens": ["Ge\u00b7stern", "Nachts", ",", "als", "Al\u00b7les", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "             Kaum der Wind mit ungewissen", "tokens": ["Kaum", "der", "Wind", "mit", "un\u00b7ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "             Seufzern durch die Gassen lief,", "tokens": ["Seuf\u00b7zern", "durch", "die", "Gas\u00b7sen", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "             Gab mir Ruhe nicht das Kissen,", "tokens": ["Gab", "mir", "Ru\u00b7he", "nicht", "das", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "         Noch der Mohn, noch, was sonst tief", "tokens": ["Noch", "der", "Mohn", ",", "noch", ",", "was", "sonst", "tief"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "         Schlafen macht \u2014 ein gut Gewissen.", "tokens": ["Schla\u00b7fen", "macht", "ein", "gut", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ART", "ADJD", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "             Endlich schlug ich mir den Schlaf", "tokens": ["End\u00b7lich", "schlug", "ich", "mir", "den", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Aus dem Sinn und lief zum Strande.", "tokens": ["Aus", "dem", "Sinn", "und", "lief", "zum", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "             Mondhell war\u2019s und mild \u2014 ich traf", "tokens": ["Mond\u00b7hell", "wa\u00b7r's", "und", "mild", "ich", "traf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "KON", "ADJD", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "         Mann und Kahn auf warmem Sande,", "tokens": ["Mann", "und", "Kahn", "auf", "war\u00b7mem", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "         Schl\u00e4frig beide, Hirt und Schaf: \u2014", "tokens": ["Schl\u00e4f\u00b7rig", "bei\u00b7de", ",", "Hirt", "und", "Schaf", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PIS", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "             Schl\u00e4frig stiess der Kahn vom Lande.", "tokens": ["Schl\u00e4f\u00b7rig", "stiess", "der", "Kahn", "vom", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "             Eine Stunde, leicht auch zwei,", "tokens": ["Ei\u00b7ne", "Stun\u00b7de", ",", "leicht", "auch", "zwei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Oder war\u2019s ein Jahr? \u2014 da sanken", "tokens": ["O\u00b7der", "wa\u00b7r's", "ein", "Jahr", "?", "da", "san\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "$.", "$(", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "         Pl\u00f6tzlich mir Sinn und Gedanken", "tokens": ["Pl\u00f6tz\u00b7lich", "mir", "Sinn", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "         In ein ew\u2019ges Einerlei,", "tokens": ["In", "ein", "ew'\u00b7ges", "Ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "             Und ein Abgrund ohne Schranken", "tokens": ["Und", "ein", "Ab\u00b7grund", "oh\u00b7ne", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "                 That sich auf: \u2014 da war\u2019s vorbei! \u2014", "tokens": ["That", "sich", "auf", ":", "da", "wa\u00b7r's", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "PTKVZ", "$.", "$(", "ADV", "VAFIN", "PTKVZ", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "             Morgen kam: auf schwarzen Tiefen", "tokens": ["Mor\u00b7gen", "kam", ":", "auf", "schwar\u00b7zen", "Tie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "         Steht ein Kahn und ruht und ruht \u2014", "tokens": ["Steht", "ein", "Kahn", "und", "ruht", "und", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was geschah? so riefs, so riefen", "tokens": ["Was", "ge\u00b7schah", "?", "so", "riefs", ",", "so", "rie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "ADV", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Hundert bald \u2014 was gab es? Blut? \u2014", "tokens": ["Hun\u00b7dert", "bald", "was", "gab", "es", "?", "Blut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["CARD", "ADV", "$(", "PWS", "VVFIN", "PPER", "$.", "NN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "             Nichts geschah! Wir schliefen, schliefen", "tokens": ["Nichts", "ge\u00b7schah", "!", "Wir", "schlie\u00b7fen", ",", "schlie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "             Alle \u2014 ach, so gut! so gut!", "tokens": ["Al\u00b7le", "ach", ",", "so", "gut", "!", "so", "gut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "$(", "ITJ", "$,", "ADV", "ADJD", "$.", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}