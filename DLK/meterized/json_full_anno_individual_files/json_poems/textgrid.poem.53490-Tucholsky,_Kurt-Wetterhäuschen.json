{"textgrid.poem.53490": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Wetterh\u00e4uschen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mal gehts uns gut. Dann br\u00fcllt der Chor der Rache.", "tokens": ["Mal", "gehts", "uns", "gut", ".", "Dann", "br\u00fcllt", "der", "Chor", "der", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Weltenunterjocher werden wild.", "tokens": ["Die", "Wel\u00b7ten\u00b7un\u00b7ter\u00b7jo\u00b7cher", "wer\u00b7den", "wild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Bizeps steigt. Der Kluge ist der Schwache.", "tokens": ["Der", "Bi\u00b7zeps", "steigt", ".", "Der", "Klu\u00b7ge", "ist", "der", "Schwa\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Macht ist Recht, die Mannessehne schwillt \u2013", "tokens": ["Nur", "Macht", "ist", "Recht", ",", "die", "Man\u00b7nes\u00b7seh\u00b7ne", "schwillt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NN", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mal gehts uns gut.", "tokens": ["Mal", "gehts", "uns", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"