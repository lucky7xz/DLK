{"dta.poem.2308": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die geheime Sprache.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mich und meine Sch\u00f6ne", "tokens": ["Wenn", "ich", "mich", "und", "mei\u00b7ne", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der gelben Nelke kr\u00f6ne,", "tokens": ["Mit", "der", "gel\u00b7ben", "Nel\u00b7ke", "kr\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich ihr mit Efen winke,", "tokens": ["Wenn", "ich", "ihr", "mit", "E\u00b7fen", "win\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr zeige, wie ich trinke,", "tokens": ["Und", "ihr", "zei\u00b7ge", ",", "wie", "ich", "trin\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ich lustig guter Dinge", "tokens": ["Wenn", "ich", "lus\u00b7tig", "gu\u00b7ter", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr vergn\u00fcgt entgegen springe,", "tokens": ["Ihr", "ver\u00b7gn\u00fcgt", "ent\u00b7ge\u00b7gen", "sprin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich, da ich ihr begegne", "tokens": ["Wenn", "ich", ",", "da", "ich", "ihr", "be\u00b7geg\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren vollen Busen seegne,", "tokens": ["Ih\u00b7ren", "vol\u00b7len", "Bu\u00b7sen", "seeg\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn ich ruf\u2019: ich will im Garten", "tokens": ["Wenn", "ich", "ruf'", ":", "ich", "will", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bei der Sonnenblume warten,", "tokens": ["Bei", "der", "Son\u00b7nen\u00b7blu\u00b7me", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn ich sie ins Thal begleite,", "tokens": ["Wenn", "ich", "sie", "ins", "Thal", "be\u00b7glei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wei\u00df sie schon was es bedeute;", "tokens": ["Wei\u00df", "sie", "schon", "was", "es", "be\u00b7deu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und weil wir uns f\u00fcrchten m\u00fcssen", "tokens": ["Und", "weil", "wir", "uns", "f\u00fcrch\u00b7ten", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVINF", "VMINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Mu\u00df sie\u2019s nur alleine wissen.", "tokens": ["Mu\u00df", "sie's", "nur", "al\u00b7lei\u00b7ne", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}