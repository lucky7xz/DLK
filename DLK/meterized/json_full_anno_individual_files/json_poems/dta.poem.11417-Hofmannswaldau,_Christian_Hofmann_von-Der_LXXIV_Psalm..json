{"dta.poem.11417": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  LXXIV  Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! warum m\u00fcssen wir so gar verstossen seyn?", "tokens": ["Herr", "!", "wa\u00b7rum", "m\u00fcs\u00b7sen", "wir", "so", "gar", "ver\u00b7stos\u00b7sen", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erbarmest du dich nicht der schafe deiner weide?", "tokens": ["Er\u00b7bar\u00b7mest", "du", "dich", "nicht", "der", "scha\u00b7fe", "dei\u00b7ner", "wei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ART", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kommt dir dein eigenthum, dein Zion, nicht mehr ein?", "tokens": ["Kommt", "dir", "dein", "ei\u00b7gen\u00b7thum", ",", "dein", "Zi\u00b7on", ",", "nicht", "mehr", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist unser untergang ein aufgang deiner freude?", "tokens": ["Ist", "un\u00b7ser", "un\u00b7ter\u00b7gang", "ein", "auf\u00b7gang", "dei\u00b7ner", "freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verst\u00f6ssest du das volck, so du erwehlet hast?", "tokens": ["Ver\u00b7st\u00f6s\u00b7sest", "du", "das", "volck", ",", "so", "du", "er\u00b7weh\u00b7let", "hast", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Tritst du Jerusalem und Jsrael mit f\u00fcssen?", "tokens": ["Tritst", "du", "Je\u00b7ru\u00b7sa\u00b7lem", "und", "Js\u00b7rael", "mit", "f\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "KON", "NE", "APPR", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ach GOtt! dein heiligthum und unser lust-pallast", "tokens": ["Ach", "Gott", "!", "dein", "hei\u00b7lig\u00b7thum", "und", "un\u00b7ser", "lust\u00b7pal\u00b7last"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird von der feinde sturm erb\u00e4rmlich umgerissen.", "tokens": ["Wird", "von", "der", "fein\u00b7de", "sturm", "er\u00b7b\u00e4rm\u00b7lich", "um\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In deinen schulen br\u00fcllt der heyden l\u00e4ster-geist.", "tokens": ["In", "dei\u00b7nen", "schu\u00b7len", "br\u00fcllt", "der", "hey\u00b7den", "l\u00e4s\u00b7ter\u00b7geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kanst du den g\u00f6tzen-dienst in deinem tempel schauen?", "tokens": ["Kanst", "du", "den", "g\u00f6t\u00b7zen\u00b7dienst", "in", "dei\u00b7nem", "tem\u00b7pel", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein nahme wird entweiht, und dein altar zerreist.", "tokens": ["Dein", "nah\u00b7me", "wird", "ent\u00b7weiht", ",", "und", "dein", "al\u00b7tar", "zer\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Schau! wie die rasenden in b\u00e4nck und pfosten hauen.", "tokens": ["Schau", "!", "wie", "die", "ra\u00b7sen\u00b7den", "in", "b\u00e4nck", "und", "pfos\u00b7ten", "hau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "ART", "ADJA", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Die freche grausamkeit brennt alle h\u00e4user aus,", "tokens": ["Die", "fre\u00b7che", "grau\u00b7sam\u00b7keit", "brennt", "al\u00b7le", "h\u00e4u\u00b7ser", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wo vor kein ander wort, als dein gesetz, erschollen:", "tokens": ["Wo", "vor", "kein", "an\u00b7der", "wort", ",", "als", "dein", "ge\u00b7setz", ",", "er\u00b7schol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der schulen hohe pracht ist nichts als ziegel-graus:", "tokens": ["Der", "schu\u00b7len", "ho\u00b7he", "pracht", "ist", "nichts", "als", "zie\u00b7gel\u00b7graus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "PIS", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wir sehn kein zeichen mehr, auf das wir mercken sollen.", "tokens": ["Wir", "sehn", "kein", "zei\u00b7chen", "mehr", ",", "auf", "das", "wir", "mer\u00b7cken", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$,", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die priester bleiben stumm, und der prophete schweigt.", "tokens": ["Die", "pries\u00b7ter", "blei\u00b7ben", "stumm", ",", "und", "der", "pro\u00b7phe\u00b7te", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "$,", "KON", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "O GOtt! wie lange soll der widersacher toben?", "tokens": ["O", "Gott", "!", "wie", "lan\u00b7ge", "soll", "der", "wi\u00b7der\u00b7sa\u00b7cher", "to\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADV", "VMFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein knecht mu\u00df untergehn, und, der dich l\u00e4stert, steigt,", "tokens": ["Dein", "knecht", "mu\u00df", "un\u00b7ter\u00b7gehn", ",", "und", ",", "der", "dich", "l\u00e4s\u00b7tert", ",", "steigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,", "KON", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und hat sich \u00fcber dich und deinen berg erhoben.", "tokens": ["Und", "hat", "sich", "\u00fc\u00b7ber", "dich", "und", "dei\u00b7nen", "berg", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Warum entzeuchst du uns den beystand deiner hand?", "tokens": ["Wa\u00b7rum", "ent\u00b7zeuchst", "du", "uns", "den", "beys\u00b7tand", "dei\u00b7ner", "hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was soll der g\u00f6tzen-schwarm in deinem tempel machen?", "tokens": ["Was", "soll", "der", "g\u00f6t\u00b7zen\u00b7schwarm", "in", "dei\u00b7nem", "tem\u00b7pel", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wiewohl! du bleibst doch GOtt, der das bedr\u00e4ngte land", "tokens": ["Wie\u00b7wohl", "!", "du", "bleibst", "doch", "Gott", ",", "der", "das", "be\u00b7dr\u00e4ng\u00b7te", "land"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$.", "PPER", "VVFIN", "ADV", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Durch treuen arm erh\u00e4lt, wenn gleich die pfeiler krachen.", "tokens": ["Durch", "treu\u00b7en", "arm", "er\u00b7h\u00e4lt", ",", "wenn", "gleich", "die", "pfei\u00b7ler", "kra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du bist von alters her mein k\u00f6nig, der mich sch\u00fctzt:", "tokens": ["Du", "bist", "von", "al\u00b7ters", "her", "mein", "k\u00f6\u00b7nig", ",", "der", "mich", "sch\u00fctzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADV", "PPOSAT", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du kanst auch alles thun, du kanst der see gebieten:", "tokens": ["Du", "kanst", "auch", "al\u00b7les", "thun", ",", "du", "kanst", "der", "see", "ge\u00b7bie\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "VVINF", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wenn dein starcker zorn mit seinem donner blitzt,", "tokens": ["Und", "wenn", "dein", "star\u00b7cker", "zorn", "mit", "sei\u00b7nem", "don\u00b7ner", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So mu\u00df der drachen-kopff so blut als gifft versch\u00fctten.", "tokens": ["So", "mu\u00df", "der", "dra\u00b7chen\u00b7kopff", "so", "blut", "als", "gifft", "ver\u00b7sch\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ADJD", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kein wallsisch steht f\u00fcr dir, er mu\u00df zu grunde gehn,", "tokens": ["Kein", "wall\u00b7sisch", "steht", "f\u00fcr", "dir", ",", "er", "mu\u00df", "zu", "grun\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "$,", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und in der w\u00fcsteney dem volcke speise bringen.", "tokens": ["Und", "in", "der", "w\u00fcs\u00b7te\u00b7ney", "dem", "vol\u00b7cke", "spei\u00b7se", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So bald dein mund gebeut, so m\u00fcssen str\u00f6hme stehn,", "tokens": ["So", "bald", "dein", "mund", "ge\u00b7beut", ",", "so", "m\u00fcs\u00b7sen", "str\u00f6h\u00b7me", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VMFIN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und aus der felsen h\u00f6h die sch\u00f6nsten b\u00e4che springen.", "tokens": ["Und", "aus", "der", "fel\u00b7sen", "h\u00f6h", "die", "sch\u00f6ns\u00b7ten", "b\u00e4\u00b7che", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Du giebst uns tag und nacht, und l\u00e4ssest in der welt", "tokens": ["Du", "giebst", "uns", "tag", "und", "nacht", ",", "und", "l\u00e4s\u00b7sest", "in", "der", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die sternen ihren lauff, die l\u00e4nder grentzen haben:", "tokens": ["Die", "ster\u00b7nen", "ih\u00b7ren", "lauff", ",", "die", "l\u00e4n\u00b7der", "grent\u00b7zen", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Machst, da\u00df der warme lentz des winters ei\u00df zerschellt,", "tokens": ["Machst", ",", "da\u00df", "der", "war\u00b7me", "lentz", "des", "win\u00b7ters", "ei\u00df", "zer\u00b7schellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und da\u00df die menschen sich im herbst und sommer laben.", "tokens": ["Und", "da\u00df", "die", "men\u00b7schen", "sich", "im", "herbst", "und", "som\u00b7mer", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "PRF", "APPRART", "VVFIN", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nun, so gedencke doch, HErr! an dein eigenthum,", "tokens": ["Nun", ",", "so", "ge\u00b7den\u00b7cke", "doch", ",", "Herr", "!", "an", "dein", "ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADV", "$,", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Und la\u00df den stoltzen feind dich nicht vergebens spotten!", "tokens": ["Und", "la\u00df", "den", "stolt\u00b7zen", "feind", "dich", "nicht", "ver\u00b7ge\u00b7bens", "spot\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vertheidige dein volck, errette deinen ruhm,", "tokens": ["Ver\u00b7thei\u00b7di\u00b7ge", "dein", "volck", ",", "er\u00b7ret\u00b7te", "dei\u00b7nen", "ruhm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Und s\u00e4ume dich nicht mehr, die g\u00f6tzen auszurotten!", "tokens": ["Und", "s\u00e4u\u00b7me", "dich",