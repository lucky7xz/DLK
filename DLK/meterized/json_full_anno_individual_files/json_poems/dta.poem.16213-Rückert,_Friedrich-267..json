{"dta.poem.16213": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "267.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In seinem eignen Kreis wer l\u00e4\u00dft sich gerne st\u00f6ren?", "tokens": ["In", "sei\u00b7nem", "eig\u00b7nen", "Kreis", "wer", "l\u00e4\u00dft", "sich", "ger\u00b7ne", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PWS", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedweder hat ein Recht sich selbst anzugeh\u00f6ren.", "tokens": ["Jed\u00b7we\u00b7der", "hat", "ein", "Recht", "sich", "selbst", "an\u00b7zu\u00b7ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "ADV", "VVIZU", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und also hast auch du dein Recht, zur\u00fcckzuweisen,", "tokens": ["Und", "al\u00b7so", "hast", "auch", "du", "dein", "Recht", ",", "zu\u00b7r\u00fcck\u00b7zu\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PPER", "PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer irgend oder was dich st\u00f6rt in deinen Kreisen.", "tokens": ["Wer", "ir\u00b7gend", "o\u00b7der", "was", "dich", "st\u00f6rt", "in", "dei\u00b7nen", "Krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nur wie sich's geh\u00f6rt, fein still und unentp\u00f6rt!", "tokens": ["Doch", "nur", "wie", "sich's", "ge\u00b7h\u00f6rt", ",", "fein", "still", "und", "un\u00b7ent\u00b7p\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "PIS", "VVFIN", "$,", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst hat nicht Fremdes dich, du hast dich selbst verst\u00f6rt.", "tokens": ["Sonst", "hat", "nicht", "Frem\u00b7des", "dich", ",", "du", "hast", "dich", "selbst", "ver\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gib lieber etwas preis, und zieh zur\u00fcck dich leise", "tokens": ["Gib", "lie\u00b7ber", "et\u00b7was", "preis", ",", "und", "zieh", "zu\u00b7r\u00fcck", "dich", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PIS", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einen innern Kreis aus einem \u00e4u\u00dferm Kreise;", "tokens": ["In", "ei\u00b7nen", "in\u00b7nern", "Kreis", "aus", "ei\u00b7nem", "\u00e4u\u00b7\u00dferm", "Krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie ein Befehliger, der Vestung Au\u00dfenwerke", "tokens": ["Wie", "ein", "Be\u00b7feh\u00b7li\u00b7ger", ",", "der", "Ves\u00b7tung", "Au\u00b7\u00dfen\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufgebend, sich verl\u00e4\u00dft auf seines Hauptwerks St\u00e4rke:", "tokens": ["Auf\u00b7ge\u00b7bend", ",", "sich", "ver\u00b7l\u00e4\u00dft", "auf", "sei\u00b7nes", "Haupt\u00b7werks", "St\u00e4r\u00b7ke", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die \u00e4u\u00dfern Linien mu\u00dft du zu weit nicht dehnen,", "tokens": ["Die", "\u00e4u\u00b7\u00dfern", "Li\u00b7ni\u00b7en", "mu\u00dft", "du", "zu", "weit", "nicht", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PTKA", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst zu vertheidigen hast du zuviel an denen.", "tokens": ["Sonst", "zu", "ver\u00b7thei\u00b7di\u00b7gen", "hast", "du", "zu\u00b7viel", "an", "de\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "PPER", "PIS", "APPR", "PRELS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}