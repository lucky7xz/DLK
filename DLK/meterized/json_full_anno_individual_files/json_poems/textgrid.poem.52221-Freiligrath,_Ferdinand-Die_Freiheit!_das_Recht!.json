{"textgrid.poem.52221": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Freiheit! das Recht!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, glaubt nicht, sie ruhe fortan bei den Toten,", "tokens": ["O", ",", "glaubt", "nicht", ",", "sie", "ru\u00b7he", "for\u00b7tan", "bei", "den", "To\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "O, glaubt nicht, sie meide fortan dies Geschlecht,", "tokens": ["O", ",", "glaubt", "nicht", ",", "sie", "mei\u00b7de", "for\u00b7tan", "dies", "Ge\u00b7schlecht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "PDS", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Weil mutigen Sprechern das Wort man verboten", "tokens": ["Weil", "mu\u00b7ti\u00b7gen", "Spre\u00b7chern", "das", "Wort", "man", "ver\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "PIS", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Nichtdelatoren verweigert das Recht!", "tokens": ["Und", "Nicht\u00b7de\u00b7la\u00b7to\u00b7ren", "ver\u00b7wei\u00b7gert", "das", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nein, ob ins Exil auch die Eidfesten schritten;", "tokens": ["Nein", ",", "ob", "ins", "E\u00b7xil", "auch", "die", "Eid\u00b7fes\u00b7ten", "schrit\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Ob, m\u00fcde der Willk\u00fchr, die endlos sie litten,", "tokens": ["Ob", ",", "m\u00fc\u00b7de", "der", "Will\u00b7k\u00fchr", ",", "die", "end\u00b7los", "sie", "lit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sich andre im Kerker die Adern zerschnitten \u2013", "tokens": ["Sich", "and\u00b7re", "im", "Ker\u00b7ker", "die", "A\u00b7dern", "zer\u00b7schnit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Doch lebt noch die Freiheit, und mit ihr das Recht!", "tokens": ["Doch", "lebt", "noch", "die", "Frei\u00b7heit", ",", "und", "mit", "ihr", "das", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "APPR", "PPOSAT", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u2013 Die Freiheit! das Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Nicht mach' uns die einzelne Schlappe verlegen!", "tokens": ["Nicht", "mach'", "uns", "die", "ein\u00b7zel\u00b7ne", "Schlap\u00b7pe", "ver\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die f\u00f6rdert die Siege des Ganzen erst recht;", "tokens": ["Die", "f\u00f6r\u00b7dert", "die", "Sie\u00b7ge", "des", "Gan\u00b7zen", "erst", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die wirkt, da\u00df wir doppelt uns r\u00fchren und regen,", "tokens": ["Die", "wirkt", ",", "da\u00df", "wir", "dop\u00b7pelt", "uns", "r\u00fch\u00b7ren", "und", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "PPER", "VVINF", "KON", "ADJA", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Noch lauter es rufen: Die Freiheit! das Recht!", "tokens": ["Noch", "lau\u00b7ter", "es", "ru\u00b7fen", ":", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Denn ewig sind eins diese heiligen Zweie!", "tokens": ["Denn", "e\u00b7wig", "sind", "eins", "die\u00b7se", "hei\u00b7li\u00b7gen", "Zwei\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie halten zusammen in Trutz und in Treue;", "tokens": ["Sie", "hal\u00b7ten", "zu\u00b7sam\u00b7men", "in", "Trutz", "und", "in", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wo das Recht ist, da wohnen von selber schon Freie,", "tokens": ["Wo", "das", "Recht", "ist", ",", "da", "woh\u00b7nen", "von", "sel\u00b7ber", "schon", "Frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "ADV", "ADV", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Und immer, wo Freie sind, waltet das Recht!", "tokens": ["Und", "im\u00b7mer", ",", "wo", "Frei\u00b7e", "sind", ",", "wal\u00b7tet", "das", "Recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "VAFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "\u2013 Die Freiheit! das Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und auch das sei ein Trost uns: Nie flogen, wie heuer,", "tokens": ["Und", "auch", "das", "sei", "ein", "Trost", "uns", ":", "Nie", "flo\u00b7gen", ",", "wie", "heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "VAFIN", "ART", "NN", "PPER", "$.", "ADV", "VVFIN", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die freudigen Zwei von Gefecht zu Gefecht!", "tokens": ["Die", "freu\u00b7di\u00b7gen", "Zwei", "von", "Ge\u00b7fecht", "zu", "Ge\u00b7fecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nie flutete voller ihr Odem und freier,", "tokens": ["Nie", "flu\u00b7te\u00b7te", "vol\u00b7ler", "ihr", "O\u00b7dem", "und", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Durch die Seele selbst brausend dem niedrigsten Knecht!", "tokens": ["Durch", "die", "See\u00b7le", "selbst", "brau\u00b7send", "dem", "nied\u00b7rigs\u00b7ten", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Sie machen die Runde der Welt und der Lande,", "tokens": ["Sie", "ma\u00b7chen", "die", "Run\u00b7de", "der", "Welt", "und", "der", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie wecken und werben von Strande zu Strande,", "tokens": ["Sie", "we\u00b7cken", "und", "wer\u00b7ben", "von", "Stran\u00b7de", "zu", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VAFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Schon sprengten sie k\u00fchn des Leibeigenen Bande,", "tokens": ["Schon", "spreng\u00b7ten", "sie", "k\u00fchn", "des", "Leib\u00b7ei\u00b7ge\u00b7nen", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und sagten zu denen des Negers: Zerbrecht!", "tokens": ["Und", "sag\u00b7ten", "zu", "de\u00b7nen", "des", "Ne\u00b7gers", ":", "Zer\u00b7brecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRELS", "ART", "NN", "$.", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "\u2013 Die Freiheit! das Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Ja, ihr Banner entflattert und weht allerorten,", "tokens": ["Ja", ",", "ihr", "Ban\u00b7ner", "ent\u00b7flat\u00b7tert", "und", "weht", "al\u00b7le\u00b7ror\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "PIS", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Da\u00df die Unbill ges\u00fchnt sei, die Schande ger\u00e4cht!", "tokens": ["Da\u00df", "die", "Un\u00b7bill", "ge\u00b7s\u00fchnt", "sei", ",", "die", "Schan\u00b7de", "ge\u00b7r\u00e4cht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Ja, und siegen sie hier nicht, so siegen sie dorten,", "tokens": ["Ja", ",", "und", "sie\u00b7gen", "sie", "hier", "nicht", ",", "so", "sie\u00b7gen", "sie", "dor\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und am Ende doch siegen sie gr\u00fcndlich und echt!", "tokens": ["Und", "am", "En\u00b7de", "doch", "sie\u00b7gen", "sie", "gr\u00fcnd\u00b7lich", "und", "echt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "O Gott, welch ein Kranz wird sie glorreich dann zieren!", "tokens": ["O", "Gott", ",", "welch", "ein", "Kranz", "wird", "sie", "glor\u00b7reich", "dann", "zie\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAT", "ART", "NN", "VAFIN", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "All die L\u00e4uber, die V\u00f6lker im Fahnentuch f\u00fchren!", "tokens": ["All", "die", "L\u00e4u\u00b7ber", ",", "die", "V\u00f6l\u00b7ker", "im", "Fah\u00b7nen\u00b7tuch", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Die Olive des Griechen, das Kleeblatt des Iren,", "tokens": ["Die", "O\u00b7li\u00b7ve", "des", "Grie\u00b7chen", ",", "das", "Klee\u00b7blatt", "des", "I\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und vor allem germanisches Eichengeflecht!", "tokens": ["Und", "vor", "al\u00b7lem", "ger\u00b7ma\u00b7ni\u00b7sches", "Ei\u00b7chen\u00b7ge\u00b7flecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADJA", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.8": {"line.1": {"text": "\u2013 Die Freiheit! das Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Wohl ruhn dann schon manche, die jetzo noch leiden \u2013", "tokens": ["Wohl", "ruhn", "dann", "schon", "man\u00b7che", ",", "die", "jet\u00b7zo", "noch", "lei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PIS", "$,", "PRELS", "ADV", "ADV", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch ihr Schlummer ist s\u00fc\u00df, und ihr Ruhn ist gerecht!", "tokens": ["Doch", "ihr", "Schlum\u00b7mer", "ist", "s\u00fc\u00df", ",", "und", "ihr", "Ruhn", "ist", "ge\u00b7recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und licht an den Gr\u00e4bern stehen die beiden,", "tokens": ["Und", "licht", "an", "den", "Gr\u00e4\u00b7bern", "ste\u00b7hen", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "PIAT", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die wir ", "tokens": ["Die", "wir"], "token_info": ["word", "word"], "pos": ["ART", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die da stritten und mutig ins Elend drum schritten,", "tokens": ["Die", "da", "strit\u00b7ten", "und", "mu\u00b7tig", "ins", "E\u00b7lend", "drum", "schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "ADJD", "APPRART", "NN", "PAV", "VVFIN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Die das Recht uns verfochten und Unrecht drum litten!", "tokens": ["Die", "das", "Recht", "uns", "ver\u00b7foch\u00b7ten", "und", "Un\u00b7recht", "drum", "lit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Hoch ewig das Recht \u2013 und die Freiheit durchs Recht!", "tokens": ["Hoch", "e\u00b7wig", "das", "Recht", "\u2013", "und", "die", "Frei\u00b7heit", "durchs", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$(", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "\u2013 Die Freiheit durchs Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "durchs", "Recht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "O, glaubt nicht, sie ruhe fortan bei den Toten,", "tokens": ["O", ",", "glaubt", "nicht", ",", "sie", "ru\u00b7he", "for\u00b7tan", "bei", "den", "To\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "O, glaubt nicht, sie meide fortan dies Geschlecht,", "tokens": ["O", ",", "glaubt", "nicht", ",", "sie", "mei\u00b7de", "for\u00b7tan", "dies", "Ge\u00b7schlecht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "PDS", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Weil mutigen Sprechern das Wort man verboten", "tokens": ["Weil", "mu\u00b7ti\u00b7gen", "Spre\u00b7chern", "das", "Wort", "man", "ver\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "PIS", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Nichtdelatoren verweigert das Recht!", "tokens": ["Und", "Nicht\u00b7de\u00b7la\u00b7to\u00b7ren", "ver\u00b7wei\u00b7gert", "das", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nein, ob ins Exil auch die Eidfesten schritten;", "tokens": ["Nein", ",", "ob", "ins", "E\u00b7xil", "auch", "die", "Eid\u00b7fes\u00b7ten", "schrit\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Ob, m\u00fcde der Willk\u00fchr, die endlos sie litten,", "tokens": ["Ob", ",", "m\u00fc\u00b7de", "der", "Will\u00b7k\u00fchr", ",", "die", "end\u00b7los", "sie", "lit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sich andre im Kerker die Adern zerschnitten \u2013", "tokens": ["Sich", "and\u00b7re", "im", "Ker\u00b7ker", "die", "A\u00b7dern", "zer\u00b7schnit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Doch lebt noch die Freiheit, und mit ihr das Recht!", "tokens": ["Doch", "lebt", "noch", "die", "Frei\u00b7heit", ",", "und", "mit", "ihr", "das", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "APPR", "PPOSAT", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "\u2013 Die Freiheit! das Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Nicht mach' uns die einzelne Schlappe verlegen!", "tokens": ["Nicht", "mach'", "uns", "die", "ein\u00b7zel\u00b7ne", "Schlap\u00b7pe", "ver\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die f\u00f6rdert die Siege des Ganzen erst recht;", "tokens": ["Die", "f\u00f6r\u00b7dert", "die", "Sie\u00b7ge", "des", "Gan\u00b7zen", "erst", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die wirkt, da\u00df wir doppelt uns r\u00fchren und regen,", "tokens": ["Die", "wirkt", ",", "da\u00df", "wir", "dop\u00b7pelt", "uns", "r\u00fch\u00b7ren", "und", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "PPER", "VVINF", "KON", "ADJA", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Noch lauter es rufen: Die Freiheit! das Recht!", "tokens": ["Noch", "lau\u00b7ter", "es", "ru\u00b7fen", ":", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Denn ewig sind eins diese heiligen Zweie!", "tokens": ["Denn", "e\u00b7wig", "sind", "eins", "die\u00b7se", "hei\u00b7li\u00b7gen", "Zwei\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie halten zusammen in Trutz und in Treue;", "tokens": ["Sie", "hal\u00b7ten", "zu\u00b7sam\u00b7men", "in", "Trutz", "und", "in", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wo das Recht ist, da wohnen von selber schon Freie,", "tokens": ["Wo", "das", "Recht", "ist", ",", "da", "woh\u00b7nen", "von", "sel\u00b7ber", "schon", "Frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "ADV", "ADV", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Und immer, wo Freie sind, waltet das Recht!", "tokens": ["Und", "im\u00b7mer", ",", "wo", "Frei\u00b7e", "sind", ",", "wal\u00b7tet", "das", "Recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "VAFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.14": {"line.1": {"text": "\u2013 Die Freiheit! das Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.15": {"line.1": {"text": "Und auch das sei ein Trost uns: Nie flogen, wie heuer,", "tokens": ["Und", "auch", "das", "sei", "ein", "Trost", "uns", ":", "Nie", "flo\u00b7gen", ",", "wie", "heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "VAFIN", "ART", "NN", "PPER", "$.", "ADV", "VVFIN", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die freudigen Zwei von Gefecht zu Gefecht!", "tokens": ["Die", "freu\u00b7di\u00b7gen", "Zwei", "von", "Ge\u00b7fecht", "zu", "Ge\u00b7fecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nie flutete voller ihr Odem und freier,", "tokens": ["Nie", "flu\u00b7te\u00b7te", "vol\u00b7ler", "ihr", "O\u00b7dem", "und", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Durch die Seele selbst brausend dem niedrigsten Knecht!", "tokens": ["Durch", "die", "See\u00b7le", "selbst", "brau\u00b7send", "dem", "nied\u00b7rigs\u00b7ten", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Sie machen die Runde der Welt und der Lande,", "tokens": ["Sie", "ma\u00b7chen", "die", "Run\u00b7de", "der", "Welt", "und", "der", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie wecken und werben von Strande zu Strande,", "tokens": ["Sie", "we\u00b7cken", "und", "wer\u00b7ben", "von", "Stran\u00b7de", "zu", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VAFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Schon sprengten sie k\u00fchn des Leibeigenen Bande,", "tokens": ["Schon", "spreng\u00b7ten", "sie", "k\u00fchn", "des", "Leib\u00b7ei\u00b7ge\u00b7nen", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und sagten zu denen des Negers: Zerbrecht!", "tokens": ["Und", "sag\u00b7ten", "zu", "de\u00b7nen", "des", "Ne\u00b7gers", ":", "Zer\u00b7brecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRELS", "ART", "NN", "$.", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.16": {"line.1": {"text": "\u2013 Die Freiheit! das Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.17": {"line.1": {"text": "Ja, ihr Banner entflattert und weht allerorten,", "tokens": ["Ja", ",", "ihr", "Ban\u00b7ner", "ent\u00b7flat\u00b7tert", "und", "weht", "al\u00b7le\u00b7ror\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "PIS", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Da\u00df die Unbill ges\u00fchnt sei, die Schande ger\u00e4cht!", "tokens": ["Da\u00df", "die", "Un\u00b7bill", "ge\u00b7s\u00fchnt", "sei", ",", "die", "Schan\u00b7de", "ge\u00b7r\u00e4cht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Ja, und siegen sie hier nicht, so siegen sie dorten,", "tokens": ["Ja", ",", "und", "sie\u00b7gen", "sie", "hier", "nicht", ",", "so", "sie\u00b7gen", "sie", "dor\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und am Ende doch siegen sie gr\u00fcndlich und echt!", "tokens": ["Und", "am", "En\u00b7de", "doch", "sie\u00b7gen", "sie", "gr\u00fcnd\u00b7lich", "und", "echt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "O Gott, welch ein Kranz wird sie glorreich dann zieren!", "tokens": ["O", "Gott", ",", "welch", "ein", "Kranz", "wird", "sie", "glor\u00b7reich", "dann", "zie\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAT", "ART", "NN", "VAFIN", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "All die L\u00e4uber, die V\u00f6lker im Fahnentuch f\u00fchren!", "tokens": ["All", "die", "L\u00e4u\u00b7ber", ",", "die", "V\u00f6l\u00b7ker", "im", "Fah\u00b7nen\u00b7tuch", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Die Olive des Griechen, das Kleeblatt des Iren,", "tokens": ["Die", "O\u00b7li\u00b7ve", "des", "Grie\u00b7chen", ",", "das", "Klee\u00b7blatt", "des", "I\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und vor allem germanisches Eichengeflecht!", "tokens": ["Und", "vor", "al\u00b7lem", "ger\u00b7ma\u00b7ni\u00b7sches", "Ei\u00b7chen\u00b7ge\u00b7flecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADJA", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.18": {"line.1": {"text": "\u2013 Die Freiheit! das Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7heit", "!", "das", "Recht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.19": {"line.1": {"text": "Wohl ruhn dann schon manche, die jetzo noch leiden \u2013", "tokens": ["Wohl", "ruhn", "dann", "schon", "man\u00b7che", ",", "die", "jet\u00b7zo", "noch", "lei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PIS", "$,", "PRELS", "ADV", "ADV", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch ihr Schlummer ist s\u00fc\u00df, und ihr Ruhn ist gerecht!", "tokens": ["Doch", "ihr", "Schlum\u00b7mer", "ist", "s\u00fc\u00df", ",", "und", "ihr", "Ruhn", "ist", "ge\u00b7recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und licht an den Gr\u00e4bern stehen die beiden,", "tokens": ["Und", "licht", "an", "den", "Gr\u00e4\u00b7bern", "ste\u00b7hen", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "PIAT", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die wir ", "tokens": ["Die", "wir"], "token_info": ["word", "word"], "pos": ["ART", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die da stritten und mutig ins Elend drum schritten,", "tokens": ["Die", "da", "strit\u00b7ten", "und", "mu\u00b7tig", "ins", "E\u00b7lend", "drum", "schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "ADJD", "APPRART", "NN", "PAV", "VVFIN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Die das Recht uns verfochten und Unrecht drum litten!", "tokens": ["Die", "das", "Recht", "uns", "ver\u00b7foch\u00b7ten", "und", "Un\u00b7recht", "drum", "lit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Hoch ewig das Recht \u2013 und die Freiheit durchs Recht!", "tokens": ["Hoch", "e\u00b7wig", "das", "Recht", "\u2013", "und", "die", "Frei\u00b7heit", "durchs", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$(", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.20": {"line.1": {"text": "\u2013 Die Freiheit durchs Recht!", "tokens": ["\u2013", "Die", "Frei\u00b7hei