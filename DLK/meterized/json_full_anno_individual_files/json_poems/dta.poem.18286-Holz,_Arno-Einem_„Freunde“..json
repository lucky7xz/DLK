{"dta.poem.18286": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Einem \u201eFreunde\u201c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du mich hinterr\u00fccks begeifert;", "tokens": ["Wenn", "du", "mich", "hin\u00b7ter\u00b7r\u00fccks", "be\u00b7gei\u00b7fert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Grund ist jedenfalls sehr triftig,", "tokens": ["Dein", "Grund", "ist", "je\u00b7den\u00b7falls", "sehr", "trif\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}