{"dta.poem.12525": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxXV.  \n  Bestiale Poesie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was erlebt man doch Geschichten!", "tokens": ["Was", "er\u00b7lebt", "man", "doch", "Ge\u00b7schich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tolle Zeiten, tolle Moden!", "tokens": ["Tol\u00b7le", "Zei\u00b7ten", ",", "tol\u00b7le", "Mo\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkt doch: deutsche Hasen dichten", "tokens": ["Denkt", "doch", ":", "deut\u00b7sche", "Ha\u00b7sen", "dich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzund auf die L\u00f6wen Oden.", "tokens": ["Je\u00b7tzund", "auf", "die", "L\u00f6\u00b7wen", "O\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}