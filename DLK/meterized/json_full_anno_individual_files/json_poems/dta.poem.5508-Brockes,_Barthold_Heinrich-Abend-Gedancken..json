{"dta.poem.5508": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe, leider! diesen Tag nicht sonders n\u00fctzlich zu", "tokens": ["Ich", "ha\u00b7be", ",", "lei\u00b7der", "!", "die\u00b7sen", "Tag", "nicht", "son\u00b7ders", "n\u00fctz\u00b7lich", "zu"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "ADV", "$.", "PDAT", "NN", "PTKNEG", "ADV", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "gebracht,", "tokens": ["ge\u00b7bracht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich habe nichts zu GOttes Ruhm gethan, geschrieben,", "tokens": ["Ich", "ha\u00b7be", "nichts", "zu", "Got\u00b7tes", "Ruhm", "ge\u00b7than", ",", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "NN", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "noch gelesen,", "tokens": ["noch", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Da er jedoch, wie nicht zu leugnen, ein Theil von meiner", "tokens": ["Da", "er", "je\u00b7doch", ",", "wie", "nicht", "zu", "leug\u00b7nen", ",", "ein", "Theil", "von", "mei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "PTKNEG", "PTKZU", "VVINF", "$,", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Zeit gewesen!", "tokens": ["Zeit", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Jedoch, mein Hertz, gieb dich zufrieden! Ich sahe ja der", "tokens": ["Je\u00b7doch", ",", "mein", "Hertz", ",", "gieb", "dich", "zu\u00b7frie\u00b7den", "!", "Ich", "sa\u00b7he", "ja", "der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Sonnen Pracht,", "tokens": ["Son\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Und habe die\u00df dabey gedacht:", "tokens": ["Und", "ha\u00b7be", "die\u00df", "da\u00b7bey", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie gro\u00df ist GOTT der sie gemacht.", "tokens": ["Wie", "gro\u00df", "ist", "GoTT", "der", "sie", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NE", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn ich auch sonsten nichts gethan, so ist der Tag doch", "tokens": ["Wenn", "ich", "auch", "sons\u00b7ten", "nichts", "ge\u00b7than", ",", "so", "ist", "der", "Tag", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIS", "VVPP", "$,", "ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "nicht verlohren:", "tokens": ["nicht", "ver\u00b7loh\u00b7ren", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Die\u00df ist der gr\u00f6sten Pflichten eine, zu der wir auf die Welt", "tokens": ["Die\u00df", "ist", "der", "gr\u00f6s\u00b7ten", "Pflich\u00b7ten", "ei\u00b7ne", ",", "zu", "der", "wir", "auf", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "$,", "APPR", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "gebohren.", "tokens": ["ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}