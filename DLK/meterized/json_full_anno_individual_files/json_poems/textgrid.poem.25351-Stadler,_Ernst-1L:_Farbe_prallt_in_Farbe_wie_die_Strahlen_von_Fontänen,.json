{"textgrid.poem.25351": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Farbe prallt in Farbe wie die Strahlen von Font\u00e4nen,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Farbe prallt in Farbe wie die Strahlen von Font\u00e4nen,", "tokens": ["Far\u00b7be", "prallt", "in", "Far\u00b7be", "wie", "die", "Strah\u00b7len", "von", "Fon\u00b7t\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "die ihr Feuer ineinanderschie\u00dfen,", "tokens": ["die", "ihr", "Feu\u00b7er", "in\u00b7ein\u00b7an\u00b7der\u00b7schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Im Geflitter hochgeraffter R\u00f6cke", "tokens": ["Im", "Ge\u00b7flit\u00b7ter", "hoch\u00b7ge\u00b7raff\u00b7ter", "R\u00f6\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und dem Bausch der bunten Sommerblusen.", "tokens": ["und", "dem", "Bausch", "der", "bun\u00b7ten", "Som\u00b7mer\u00b7blu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Rings von allen W\u00e4nden, hundertf\u00e4ltig", "tokens": ["Rings", "von", "al\u00b7len", "W\u00e4n\u00b7den", ",", "hun\u00b7dert\u00b7f\u00e4l\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ausgeteilt, str\u00f6mt Licht.", "tokens": ["Aus\u00b7ge\u00b7teilt", ",", "str\u00f6mt", "Licht", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die Flammen, die sich zuckend in den Wirbel gie\u00dfen,", "tokens": ["Die", "Flam\u00b7men", ",", "die", "sich", "zu\u00b7ckend", "in", "den", "Wir\u00b7bel", "gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Stehen, h\u00f6her, eingesammelt,", "tokens": ["Ste\u00b7hen", ",", "h\u00f6\u00b7her", ",", "ein\u00b7ge\u00b7sam\u00b7melt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "in den goldgefa\u00dften Spiegeln, fremd und hinterh\u00e4ltig,", "tokens": ["in", "den", "gold\u00b7ge\u00b7fa\u00df\u00b7ten", "Spie\u00b7geln", ",", "fremd", "und", "hin\u00b7ter\u00b7h\u00e4l\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Wie erstarrt und Regung doch in grenzenlose Tiefen", "tokens": ["Wie", "er\u00b7starrt", "und", "Re\u00b7gung", "doch", "in", "gren\u00b7zen\u00b7lo\u00b7se", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "KON", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "weiterleitend, Leben, abgel\u00f6st und fern", "tokens": ["wei\u00b7ter\u00b7lei\u00b7tend", ",", "Le\u00b7ben", ",", "ab\u00b7ge\u00b7l\u00f6st", "und", "fern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "NN", "$,", "VVPP", "KON", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "und wieder eins und einig mit den Paaren,", "tokens": ["und", "wie\u00b7der", "eins", "und", "ei\u00b7nig", "mit", "den", "Paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die im Bann der immer gleichen Melodien,", "tokens": ["Die", "im", "Bann", "der", "im\u00b7mer", "glei\u00b7chen", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "engverschmiegt, mit losgelassnen Gliedern schreitend,", "tokens": ["eng\u00b7ver\u00b7schmiegt", ",", "mit", "los\u00b7ge\u00b7lass\u00b7nen", "Glie\u00b7dern", "schrei\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Durcheinanderquirlen: Frauen, die geschminkten", "tokens": ["Durch\u00b7ein\u00b7an\u00b7der\u00b7quir\u00b7len", ":", "Frau\u00b7en", ",", "die", "ge\u00b7schmink\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Wangen rot behaucht, mit halb gel\u00f6sten Haaren,", "tokens": ["Wan\u00b7gen", "rot", "be\u00b7haucht", ",", "mit", "halb", "ge\u00b7l\u00f6s\u00b7ten", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Taumelnd, nur die Augen ganz im Grund ein", "tokens": ["Tau\u00b7melnd", ",", "nur", "die", "Au\u00b7gen", "ganz", "im", "Grund", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "wenig matt, die in das Dunkel leerer Stunden laden,", "tokens": ["we\u00b7nig", "matt", ",", "die", "in", "das", "Dun\u00b7kel", "lee\u00b7rer", "Stun\u00b7den", "la\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "W\u00e4hrend ihre K\u00f6rper sich im Takt", "tokens": ["W\u00e4h\u00b7rend", "ih\u00b7re", "K\u00f6r\u00b7per", "sich", "im", "Takt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "unkeuscher Gesten ineinanderneigen,", "tokens": ["un\u00b7keu\u00b7scher", "Ges\u00b7ten", "in\u00b7ein\u00b7an\u00b7der\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "Ernsthaft und voll Andacht:", "tokens": ["Ernst\u00b7haft", "und", "voll", "An\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "und sie tanzen, gl\u00e4ubig blickend, die Balladen", "tokens": ["und", "sie", "tan\u00b7zen", ",", "gl\u00e4u\u00b7big", "bli\u00b7ckend", ",", "die", "Bal\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "ADJD", "ADJD", "$,", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "M\u00fcd gebrannter Herzen, l\u00fcstern und verspielt,", "tokens": ["M\u00fcd", "ge\u00b7brann\u00b7ter", "Her\u00b7zen", ",", "l\u00fcs\u00b7tern", "und", "ver\u00b7spielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.24": {"text": "und vom Gepl\u00e4rr der Geigen", "tokens": ["und", "vom", "Ge\u00b7pl\u00e4rr", "der", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Wie von einer z\u00e4hen lauen Flut umschwemmt.", "tokens": ["Wie", "von", "ei\u00b7ner", "z\u00e4\u00b7hen", "lau\u00b7en", "Flut", "um\u00b7schwemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.26": {"text": "Zuweilen kreischt ein Schrei.", "tokens": ["Zu\u00b7wei\u00b7len", "kreischt", "ein", "Schrei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Ein Lachen gellt. Die Schwebe,", "tokens": ["Ein", "La\u00b7chen", "gellt", ".", "Die", "Schwe\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "In der die Paare, unsichtbar gehalten,", "tokens": ["In", "der", "die", "Paa\u00b7re", ",", "un\u00b7sicht\u00b7bar", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "schaukeln, schwankt.", "tokens": ["schau\u00b7keln", ",", "schwankt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "Doch immer, wie in traumhaft irrem Schwung", "tokens": ["Doch", "im\u00b7mer", ",", "wie", "in", "traum\u00b7haft", "ir\u00b7rem", "Schwung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Schnurrt der Rhythmus weiter", "tokens": ["Schnurrt", "der", "Rhyth\u00b7mus", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "durch den \u00fcberhitzten Saal ...", "tokens": ["durch", "den", "\u00fc\u00b7berh\u00b7itz\u00b7ten", "Saal", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Da\u00df nur kein Windzug jetzt", "tokens": ["Da\u00df", "nur", "kein", "Wind\u00b7zug", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "die roten Samtportieren hebe,", "tokens": ["die", "ro\u00b7ten", "Samt\u00b7por\u00b7tie\u00b7ren", "he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Hinter denen schon der Morgen wartet,", "tokens": ["Hin\u00b7ter", "de\u00b7nen", "schon", "der", "Mor\u00b7gen", "war\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "grau, hager, fahl ...", "tokens": ["grau", ",", "ha\u00b7ger", ",", "fahl", "..."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KOUS", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "bereit, in kaltem Sprung,", "tokens": ["be\u00b7reit", ",", "in", "kal\u00b7tem", "Sprung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Die Br\u00fcstung \u00fcbergreifend, ins Parkett zu gleiten,", "tokens": ["Die", "Br\u00fcs\u00b7tung", "\u00fc\u00b7berg\u00b7rei\u00b7fend", ",", "ins", "Par\u00b7kett", "zu", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "da\u00df die hei\u00dfgetanzten Reihen j\u00e4hlings stocken,", "tokens": ["da\u00df", "die", "hei\u00df\u00b7ge\u00b7tanz\u00b7ten", "Rei\u00b7hen", "j\u00e4h\u00b7lings", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.40": {"text": "Traum und Tanz zerbricht,", "tokens": ["Traum", "und", "Tanz", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.41": {"text": "Und w\u00e4hrend noch die Walzerweise", "tokens": ["Und", "w\u00e4h\u00b7rend", "noch", "die", "Wal\u00b7zer\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "sinnlos leiernd weitert\u00f6nt,", "tokens": ["sinn\u00b7los", "lei\u00b7ernd", "wei\u00b7ter\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Tag einstr\u00f6mt und die dicke Luft von Schwei\u00df,", "tokens": ["Tag", "eins\u00b7tr\u00f6mt", "und", "die", "di\u00b7cke", "Luft", "von", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "Parfum und umgegossnem Wein zerrei\u00dft,", "tokens": ["Par\u00b7fum", "und", "um\u00b7ge\u00b7goss\u00b7nem", "Wein", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "und durch das harte Licht,", "tokens": ["und", "durch", "das", "har\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Fernher rollend, ehern, stark und klar,", "tokens": ["Fern\u00b7her", "rol\u00b7lend", ",", "e\u00b7hern", ",", "stark", "und", "klar", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "das Arbeitslied der gro\u00dfen Stadt", "tokens": ["das", "Ar\u00b7beits\u00b7lied", "der", "gro\u00b7\u00dfen", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "durch pl\u00f6tzlich aufgerissene Fenster dr\u00f6hnt.", "tokens": ["durch", "pl\u00f6tz\u00b7lich", "auf\u00b7ge\u00b7ris\u00b7se\u00b7ne", "Fens\u00b7ter", "dr\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Farbe prallt in Farbe wie die Strahlen von Font\u00e4nen,", "tokens": ["Far\u00b7be", "prallt", "in", "Far\u00b7be", "wie", "die", "Strah\u00b7len", "von", "Fon\u00b7t\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "die ihr Feuer ineinanderschie\u00dfen,", "tokens": ["die", "ihr", "Feu\u00b7er", "in\u00b7ein\u00b7an\u00b7der\u00b7schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Im Geflitter hochgeraffter R\u00f6cke", "tokens": ["Im", "Ge\u00b7flit\u00b7ter", "hoch\u00b7ge\u00b7raff\u00b7ter", "R\u00f6\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und dem Bausch der bunten Sommerblusen.", "tokens": ["und", "dem", "Bausch", "der", "bun\u00b7ten", "Som\u0