{"textgrid.poem.51184": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Dichter seht, der immerdar erz\u00e4hlt von Lerchensang,", "tokens": ["Den", "Dich\u00b7ter", "seht", ",", "der", "im\u00b7mer\u00b7dar", "er\u00b7z\u00e4hlt", "von", "Ler\u00b7chen\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wie er nun bald ein Dutzend schon gebratner Lerchen schlang!", "tokens": ["Wie", "er", "nun", "bald", "ein", "Dut\u00b7zend", "schon", "ge\u00b7brat\u00b7ner", "Ler\u00b7chen", "schlang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Bei Sonnenaufgang, als der Tag in Blau und Gold ergl\u00fcht',", "tokens": ["Bei", "Son\u00b7nen\u00b7auf\u00b7gang", ",", "als", "der", "Tag", "in", "Blau", "und", "Gold", "er\u00b7gl\u00fcht'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Da war es, da\u00df sein Morgenlied vom Lob der Lerchen klang;", "tokens": ["Da", "war", "es", ",", "da\u00df", "sein", "Mor\u00b7gen\u00b7lied", "vom", "Lob", "der", "Ler\u00b7chen", "klang", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Und nun bei Sonnenuntergang mit seinem Gabelspie\u00df", "tokens": ["Und", "nun", "bei", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "mit", "sei\u00b7nem", "Ga\u00b7bel\u00b7spie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Er sehnend in die Liederbrust gebratner Lerchen drang!", "tokens": ["Er", "seh\u00b7nend", "in", "die", "Lie\u00b7der\u00b7brust", "ge\u00b7brat\u00b7ner", "Ler\u00b7chen", "drang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Das hei\u00df ich die Natur verstehn, allseitig, tief und k\u00fchn,", "tokens": ["Das", "hei\u00df", "ich", "die", "Na\u00b7tur", "ver\u00b7stehn", ",", "all\u00b7sei\u00b7tig", ",", "tief", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Wenn also auf und nieder sich sein Tag mit Lerchen schwang!", "tokens": ["Wenn", "al\u00b7so", "auf", "und", "nie\u00b7der", "sich", "sein", "Tag", "mit", "Ler\u00b7chen", "schwang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKVZ", "KON", "PTKVZ", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.se