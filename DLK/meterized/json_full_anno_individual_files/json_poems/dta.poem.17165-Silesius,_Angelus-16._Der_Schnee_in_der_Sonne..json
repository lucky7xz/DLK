{"dta.poem.17165": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "16.  Der Schnee in der Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6ne gl\u00e4ntzt der Schnee wann jhn der Sonnen-", "tokens": ["Wie", "sch\u00f6\u00b7ne", "gl\u00e4ntzt", "der", "Schnee", "wann", "jhn", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "VVFIN", "ART", "NN", "PWAV", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "strahlen", "tokens": ["strah\u00b7len"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mit Himmelischem Licht bestreichen und bemahlen!", "tokens": ["Mit", "Him\u00b7me\u00b7li\u00b7schem", "Licht", "be\u00b7strei\u00b7chen", "und", "be\u00b7mah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So gl\u00e4ntzt auch deine Seel/ so sie ist wei\u00df wie Schnee:", "tokens": ["So", "gl\u00e4ntzt", "auch", "dei\u00b7ne", "Seel", "/", "so", "sie", "ist", "wei\u00df", "wie", "Schnee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "ADV", "PPER", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann sie beschienen wird vom Aufgang au\u00df der H\u00f6h.", "tokens": ["Wann", "sie", "be\u00b7schie\u00b7nen", "wird", "vom", "Auf\u00b7gang", "au\u00df", "der", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VAFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}