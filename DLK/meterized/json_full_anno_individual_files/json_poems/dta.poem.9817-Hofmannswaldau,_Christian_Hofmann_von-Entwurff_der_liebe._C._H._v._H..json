{"dta.poem.9817": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff der liebe.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die lieb ist unvernunfft/ die mit vernunfft vermengt/", "tokens": ["Die", "lieb", "ist", "un\u00b7ver\u00b7nunfft", "/", "die", "mit", "ver\u00b7nunfft", "ver\u00b7mengt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "$(", "ART", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein fried gesellter ha\u00df/ betrug vermischt mit glauben/", "tokens": ["Ein", "fried", "ge\u00b7sell\u00b7ter", "ha\u00df", "/", "be\u00b7trug", "ver\u00b7mischt", "mit", "glau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "VVFIN", "VVFIN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein hoffnungs-volle furcht/ ein schiffbruch dessen rauben", "tokens": ["Ein", "hoff\u00b7nungs\u00b7vol\u00b7le", "furcht", "/", "ein", "schiff\u00b7bruch", "des\u00b7sen", "rau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJD", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uns dennoch s\u00fcsse d\u00fcnckt/ ein stein so uns bedr\u00e4ngt/", "tokens": ["Uns", "den\u00b7noch", "s\u00fcs\u00b7se", "d\u00fcnckt", "/", "ein", "stein", "so", "uns", "be\u00b7dr\u00e4ngt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "$(", "ART", "ADV", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein angenehm Charybd/ und ein gesundes krancken/", "tokens": ["Ein", "an\u00b7ge\u00b7nehm", "Cha\u00b7rybd", "/", "und", "ein", "ge\u00b7sun\u00b7des", "kran\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein hunger der sich mu\u00df mit seiner sattheit zancken/", "tokens": ["Ein", "hun\u00b7ger", "der", "sich", "mu\u00df", "mit", "sei\u00b7ner", "sat\u00b7theit", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRELS", "PRF", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein vollgezechter durst/ und trunckne n\u00fcchternheit/", "tokens": ["Ein", "voll\u00b7ge\u00b7zech\u00b7ter", "durst", "/", "und", "trunck\u00b7ne", "n\u00fcch\u00b7tern\u00b7heit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein sch\u00f6nes freuden-spiel/ das garstig ungl\u00fcck endet/", "tokens": ["Ein", "sch\u00f6\u00b7nes", "freu\u00b7den\u00b7spiel", "/", "das", "gars\u00b7tig", "un\u00b7gl\u00fcck", "en\u00b7det", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein port der uns verschlingt/ wenn man schon angelendet/", "tokens": ["Ein", "port", "der", "uns", "ver\u00b7schlingt", "/", "wenn", "man", "schon", "an\u00b7ge\u00b7len\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "PPER", "VVFIN", "$(", "KOUS", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein s\u00fcsser \u00fcbelstand/ und \u00fcble s\u00fc\u00dfigkeit/", "tokens": ["Ein", "s\u00fcs\u00b7ser", "\u00fc\u00b7bel\u00b7stand", "/", "und", "\u00fcb\u00b7le", "s\u00fc\u00b7\u00dfig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein bittrer honigsafft/ der von geruch beliebet/", "tokens": ["Ein", "bit\u00b7trer", "ho\u00b7nigs\u00b7afft", "/", "der", "von", "ge\u00b7ruch", "be\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und der uns im geschmack gifft/ pest und galle giebet/", "tokens": ["Und", "der", "uns", "im", "ge\u00b7schmack", "gifft", "/", "pest", "und", "gal\u00b7le", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "APPRART", "NN", "VVFIN", "$(", "VVFIN", "KON", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein wetter das man w\u00fcnscht/ und eine lichte nacht/", "tokens": ["Ein", "wet\u00b7ter", "das", "man", "w\u00fcnscht", "/", "und", "ei\u00b7ne", "lich\u00b7te", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "VVFIN", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein dick verfinstert licht/ ein abgestorbnes leben", "tokens": ["Ein", "dick", "ver\u00b7fins\u00b7tert", "licht", "/", "ein", "ab\u00b7ge\u00b7storb\u00b7nes", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "NN", "$(", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und ein belebter tod; ein fehler der vergeben/", "tokens": ["Und", "ein", "be\u00b7leb\u00b7ter", "tod", ";", "ein", "feh\u00b7ler", "der", "ver\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "ART", "NN", "ART", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch nicht vergessen wird. Ein schandfleck/ der mit pracht", "tokens": ["Doch", "nicht", "ver\u00b7ges\u00b7sen", "wird", ".", "Ein", "schand\u00b7fleck", "/", "der", "mit", "pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVPP", "VAFIN", "$.", "ART", "ADJD", "$(", "ART", "APPR", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Und schmincke sich bestreicht. Ein tugendhafftes lafter", "tokens": ["Und", "schmin\u00b7cke", "sich", "be\u00b7streicht", ".", "Ein", "tu\u00b7gend\u00b7haff\u00b7tes", "laf\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und schn\u00f6der missethat gelindes artzney-pflaster/", "tokens": ["Und", "schn\u00f6\u00b7der", "mis\u00b7se\u00b7that", "ge\u00b7lin\u00b7des", "artz\u00b7ney\u00b7pflas\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein unbest\u00e4ndig spiel und ein best\u00e4ndig trug/", "tokens": ["Ein", "un\u00b7be\u00b7st\u00e4n\u00b7dig", "spiel", "und", "ein", "be\u00b7st\u00e4n\u00b7dig", "trug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein\u2019 ausgekr\u00e4ffte krafft/ ein gantz beweglich festes/", "tokens": ["Ein'", "aus\u00b7ge\u00b7kr\u00e4ff\u00b7te", "krafft", "/", "ein", "gantz", "be\u00b7weg\u00b7lich", "fes\u00b7tes", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "ADJD", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein allgemeiner schlu\u00df/ den narrheit nennt sein bestes/", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7ner", "schlu\u00df", "/", "den", "nar\u00b7rheit", "nennt", "sein", "bes\u00b7tes", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein rath der urtheil spricht gantz ohne recht und fug/", "tokens": ["Ein", "rath", "der", "ur\u00b7theil", "spricht", "gantz", "oh\u00b7ne", "recht", "und", "fug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "APPR", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein wolstand der betr\u00fcbt/ ein gl\u00fcck das nicht erscheinet/", "tokens": ["Ein", "wol\u00b7stand", "der", "be\u00b7tr\u00fcbt", "/", "ein", "gl\u00fcck", "das", "nicht", "er\u00b7schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVPP", "$(", "ART", "NN", "PDS", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ein lust-haus/ da die seel den freyen stand beweinet.", "tokens": ["Ein", "lust\u00b7haus", "/", "da", "die", "seel", "den", "frey\u00b7en", "stand", "be\u00b7wei\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "ART", "NN", "ART", "ADJA", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}