: "Neigend mild,", "tokens": ["Nei\u00b7gend", "mild", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Meine schlanken Lohen wend ich:", "tokens": ["Mei\u00b7ne", "schlan\u00b7ken", "Lo\u00b7hen", "wend", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KOUS", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Komm nicht nach mir, ich verbrenn dich!", "tokens": ["Komm", "nicht", "nach", "mir", ",", "ich", "ver\u00b7brenn", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo die wilden B\u00e4che rauschen", "tokens": ["Wo", "die", "wil\u00b7den", "B\u00e4\u00b7che", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die hohen Palmen stehn,", "tokens": ["Und", "die", "ho\u00b7hen", "Pal\u00b7men", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die J\u00e4ger heimlich lauschen,", "tokens": ["Wenn", "die", "J\u00e4\u00b7ger", "heim\u00b7lich", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Viele Rehe einsam gehn.", "tokens": ["Vie\u00b7le", "Re\u00b7he", "ein\u00b7sam", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+