{"textgrid.poem.31446": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begreiffestu schon nicht / mein Herz / das was du glaubest;", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begreiffestu schon nicht / mein Herz / das was du glaubest;", "tokens": ["Be\u00b7greif\u00b7fes\u00b7tu", "schon", "nicht", "/", "mein", "Herz", "/", "das", "was", "du", "glau\u00b7best", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "$(", "PPOSAT", "NN", "$(", "PDS", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "schadt nicht; de\u00df Glaubens Art / ist unbegreifflich seyn.", "tokens": ["schadt", "nicht", ";", "de\u00df", "Glau\u00b7bens", "Art", "/", "ist", "un\u00b7be\u00b7greif\u00b7flich", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$.", "ART", "NN", "NN", "$(", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ist sein gegenstand / was wider allen schein.", "tokens": ["Das", "ist", "sein", "ge\u00b7gen\u00b7stand", "/", "was", "wi\u00b7der", "al\u00b7len", "schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$(", "PWS", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch ursach aus vernunfft / du ihm sein wesen raubest /", "tokens": ["Durch", "ur\u00b7sach", "aus", "ver\u00b7nunfft", "/", "du", "ihm", "sein", "we\u00b7sen", "rau\u00b7best", "/"], "token_info": ["word", "word"