{"dta.poem.2285": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Wahl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nt ich malen, wie Apelles,", "tokens": ["K\u00f6nt", "ich", "ma\u00b7len", ",", "wie", "A\u00b7pel\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "PWAV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauter M\u00e4dchen wolt ich malen;", "tokens": ["Lau\u00b7ter", "M\u00e4d\u00b7chen", "wolt", "ich", "ma\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nt ich nur wie Orpheus spielen,", "tokens": ["K\u00f6nt", "ich", "nur", "wie", "Or\u00b7pheus", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOKOM", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauter M\u00e4dchen solten tanzen;", "tokens": ["Lau\u00b7ter", "M\u00e4d\u00b7chen", "sol\u00b7ten", "tan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nt ich Todte lebend machen,", "tokens": ["K\u00f6nt", "ich", "Tod\u00b7te", "le\u00b7bend", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lauter M\u00e4dchen solten leben;", "tokens": ["Lau\u00b7ter", "M\u00e4d\u00b7chen", "sol\u00b7ten", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber k\u00f6nt ich, wie ich wolte", "tokens": ["A\u00b7ber", "k\u00f6nt", "ich", ",", "wie", "ich", "wol\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Viele wieder sterben lassen,", "tokens": ["Vie\u00b7le", "wie\u00b7der", "ster\u00b7ben", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Viele solten wieder sterben,", "tokens": ["Vie\u00b7le", "sol\u00b7ten", "wie\u00b7der", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Viele wolt ich \u00fcberstreichen,", "tokens": ["Vie\u00b7le", "wolt", "ich", "\u00fc\u00b7bers\u00b7trei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df sie ungemalet blieben,", "tokens": ["Da\u00df", "sie", "un\u00b7ge\u00b7ma\u00b7let", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und, vom ersten Tanz erm\u00fcdet,", "tokens": ["Und", ",", "vom", "ers\u00b7ten", "Tanz", "er\u00b7m\u00fc\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Solten viele nicht mehr tanzen.", "tokens": ["Sol\u00b7ten", "vie\u00b7le", "nicht", "mehr", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}