{"dta.poem.102": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Selindens  Antwort an  Almidorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du schreibst Almidor dreist/ und etwas l\u00e4cherlich/", "tokens": ["Du", "schreibst", "Al\u00b7mi\u00b7dor", "dreist", "/", "und", "et\u00b7was", "l\u00e4\u00b7cher\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "VVFIN", "$(", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mir gef\u00e4lt dein Brief/ die Worte kitzeln mich/", "tokens": ["Doch", "mir", "ge\u00b7f\u00e4lt", "dein", "Brief", "/", "die", "Wor\u00b7te", "kit\u00b7zeln", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Selinde ist mit dir/ Almidor, schon vers\u00f6hnet/", "tokens": ["Se\u00b7lin\u00b7de", "ist", "mit", "dir", "/", "Al\u00b7mi\u00b7dor", ",", "schon", "ver\u00b7s\u00f6h\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPER", "$(", "NE", "$,", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie denckt nicht mehr daran/ da\u00df du sie hast verh\u00f6hnet/", "tokens": ["Sie", "denckt", "nicht", "mehr", "da\u00b7ran", "/", "da\u00df", "du", "sie", "hast", "ver\u00b7h\u00f6h\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PAV", "$(", "KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Brief ist l\u00e4cherlich.", "tokens": ["Dein", "Brief", "ist", "l\u00e4\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nun stell dich diese Nacht ja bey ", "tokens": ["Nun", "stell", "dich", "die\u00b7se", "Nacht", "ja", "bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PDAT", "NN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es soll ihr weicher Leib dein Unter-Bette seyn/", "tokens": ["Es", "soll", "ihr", "wei\u00b7cher", "Leib", "dein", "Un\u00b7ter\u00b7Bet\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da will ich meinen Schimpff an dich gar s\u00fcsse r\u00e4chen/", "tokens": ["Da", "will", "ich", "mei\u00b7nen", "Schimpff", "an", "dich", "gar", "s\u00fcs\u00b7se", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da wollen wir die Frucht der sch\u00f6nsten Zweige brechen/", "tokens": ["Da", "wol\u00b7len", "wir", "die", "Frucht", "der", "sch\u00f6ns\u00b7ten", "Zwei\u00b7ge", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Worte kitzeln mich.", "tokens": ["Die", "Wor\u00b7te", "kit\u00b7zeln", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ob gleich die Mutter wacht/ ob sie schon Achtung giebt/", "tokens": ["Ob", "gleich", "die", "Mut\u00b7ter", "wacht", "/", "ob", "sie", "schon", "Ach\u00b7tung", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dennoch ", "tokens": ["Den\u00b7noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Sie h\u00fcte wie sie will/ sie wache alle Stunden/", "tokens": ["Sie", "h\u00fc\u00b7te", "wie", "sie", "will", "/", "sie", "wa\u00b7che", "al\u00b7le", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du solt doch/ wehrtes Kind/ verbinden meine Wunden.", "tokens": ["Du", "solt", "doch", "/", "wehr\u00b7tes", "Kind", "/", "ver\u00b7bin\u00b7den", "mei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$(", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dein Brief ist l\u00e4cherlich.", "tokens": ["Dein", "Brief", "ist", "l\u00e4\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "So bald der blasse Mond mit seinem Heer auffgeht/", "tokens": ["So", "bald", "der", "blas\u00b7se", "Mond", "mit", "sei\u00b7nem", "Heer", "auff\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Alsdann ", "tokens": ["Als\u00b7dann"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "So nach ", "tokens": ["So", "nach"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Und in dem Feder-Grab kan neben dich erwarmeu.", "tokens": ["Und", "in", "dem", "Fe\u00b7der\u00b7Grab", "kan", "ne\u00b7ben", "dich", "er\u00b7war\u00b7meu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Die Worte kitzeln mich.", "tokens": ["Die", "Wor\u00b7te", "kit\u00b7zeln", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Drum stell dich diese Nacht ja bey ", "tokens": ["Drum", "stell", "dich", "die\u00b7se", "Nacht", "ja", "bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "NN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So bald ", "tokens": ["So", "bald"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Da\u00df dich ", "tokens": ["Da\u00df", "dich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Und dein gehabtes Leid mit einer Straffe b\u00fcssen.", "tokens": ["Und", "dein", "ge\u00b7hab\u00b7tes", "Leid", "mit", "ei\u00b7ner", "Straf\u00b7fe", "b\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein Brief ist l\u00e4cherlich", "tokens": ["Dein", "Brief", "ist", "l\u00e4\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Dis schreibt mit eigner Hand die l\u00e4chtzende ", "tokens": ["Dis", "schreibt", "mit", "eig\u00b7ner", "Hand", "die", "l\u00e4cht\u00b7zen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.27": {"text": "Ach! stelle dich ja ein/ ", "tokens": ["Ach", "!", "stel\u00b7le", "dich", "ja", "ein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Und lehre meine Brust wie sie soll br\u00fcnstig lieben/", "tokens": ["Und", "leh\u00b7re", "mei\u00b7ne", "Brust", "wie", "sie", "soll", "br\u00fcns\u00b7tig", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KOKOM", "PPER", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Selinde hat zwar so noch nicht an dich geschrieben;", "tokens": ["Se\u00b7lin\u00b7de", "hat", "zwar", "so", "noch", "nicht", "an", "dich", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Was schadts? es kitzelt mich.", "tokens": ["Was", "schadts", "?", "es", "kit\u00b7zelt", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}