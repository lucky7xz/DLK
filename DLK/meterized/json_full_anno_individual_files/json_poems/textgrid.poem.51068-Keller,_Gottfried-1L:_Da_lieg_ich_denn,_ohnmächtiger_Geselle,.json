{"textgrid.poem.51068": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da lieg ich denn, ohnm\u00e4chtiger Geselle,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da lieg ich denn, ohnm\u00e4chtiger Geselle,", "tokens": ["Da", "lieg", "ich", "denn", ",", "ohn\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Loch geworfen, wie ein Stra\u00dfenheld,", "tokens": ["Ins", "Loch", "ge\u00b7wor\u00b7fen", ",", "wie", "ein", "Stra\u00b7\u00dfen\u00b7held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein l\u00e4rmender, von der Emp\u00f6rung Welle;", "tokens": ["Ein", "l\u00e4r\u00b7men\u00b7der", ",", "von", "der", "Em\u00b7p\u00f6\u00b7rung", "Wel\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein blinder Maulwurf im zerw\u00fchlten Feld!", "tokens": ["Ein", "blin\u00b7der", "Maul\u00b7wurf", "im", "zer\u00b7w\u00fchl\u00b7ten", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohlan, ich will, was kommen soll, erwarten,", "tokens": ["Wo\u00b7hlan", ",", "ich", "will", ",", "was", "kom\u00b7men", "soll", ",", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "$,", "PWS", "VVINF", "VMFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist am End ein friedlich Wohnen hier;", "tokens": ["Es", "ist", "am", "End", "ein", "fried\u00b7lich", "Woh\u00b7nen", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchle nicht die Glieder, die erstarrten,", "tokens": ["Ich", "f\u00fch\u00b7le", "nicht", "die", "Glie\u00b7der", ",", "die", "er\u00b7starr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch heiter glimmt die stille Seele mir!", "tokens": ["Doch", "hei\u00b7ter", "glimmt", "die", "stil\u00b7le", "See\u00b7le", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt ich nun einen ewigen Gedanken,", "tokens": ["H\u00e4tt", "ich", "nun", "ei\u00b7nen", "e\u00b7wi\u00b7gen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An dem man endlos sich erproben mag,", "tokens": ["An", "dem", "man", "end\u00b7los", "sich", "er\u00b7pro\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADJD", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00f6cht ich liegen in den engen Schranken,", "tokens": ["So", "m\u00f6cht", "ich", "lie\u00b7gen", "in", "den", "en\u00b7gen", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Behaglich sinnend bis zum J\u00fcngsten Tag.", "tokens": ["Be\u00b7hag\u00b7lich", "sin\u00b7nend", "bis", "zum", "J\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vielleicht, wer wei\u00df, w\u00fcchs er zu solcher Gr\u00f6\u00dfe,", "tokens": ["Viel\u00b7leicht", ",", "wer", "wei\u00df", ",", "w\u00fcchs", "er", "zu", "sol\u00b7cher", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er, in Kraft sich wandelnd, ein Vulkan,", "tokens": ["Da\u00df", "er", ",", "in", "Kraft", "sich", "wan\u00b7delnd", ",", "ein", "Vul\u00b7kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "PRF", "VVPP", "$,", "ART", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Flammenausbruch dieses Grab erschl\u00f6sse,", "tokens": ["Im", "Flam\u00b7men\u00b7aus\u00b7bruch", "die\u00b7ses", "Grab", "er\u00b7schl\u00f6s\u00b7se", ","], "token_inf