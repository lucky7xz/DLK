{"dta.poem.6770": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele sucht offt aus den Ketten,", "tokens": ["Die", "See\u00b7le", "sucht", "offt", "aus", "den", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die sie umschlingen, sich zu retten;", "tokens": ["Die", "sie", "um\u00b7schlin\u00b7gen", ",", "sich", "zu", "ret\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie selber, wenn sie offt durch Jrrthum fortgerissen,", "tokens": ["Sie", "sel\u00b7ber", ",", "wenn", "sie", "offt", "durch", "Jrr\u00b7thum", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df aus Schwachheit sie den Sinnen folgen m\u00fcssen,", "tokens": ["Und", "da\u00df", "aus", "Schwach\u00b7heit", "sie", "den", "Sin\u00b7nen", "fol\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PPER", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erleuchtet sie sie doch gar offt hingegen,", "tokens": ["Er\u00b7leuch\u00b7tet", "sie", "sie", "doch", "gar", "offt", "hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und leitet sie auf rechten Wegen,", "tokens": ["Und", "lei\u00b7tet", "sie", "auf", "rech\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja richtet ihre Handlung ein.", "tokens": ["Ja", "rich\u00b7tet", "ih\u00b7re", "Hand\u00b7lung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Jrrthum wird durch sie vertilget und verbessert,", "tokens": ["Der", "Jrr\u00b7thum", "wird", "durch", "sie", "ver\u00b7til\u00b7get", "und", "ver\u00b7bes\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So, da\u00df was Anfangs schien ein blinder Sinn zu seyn,", "tokens": ["So", ",", "da\u00df", "was", "An\u00b7fangs", "schien", "ein", "blin\u00b7der", "Sinn", "zu", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hernachmahls pl\u00f6tzlich wird erkannt,", "tokens": ["Her\u00b7nach\u00b7mahls", "pl\u00f6tz\u00b7lich", "wird", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df es ein w\u00fcrcklicher Verstand.", "tokens": ["Da\u00df", "es", "ein", "w\u00fcrck\u00b7li\u00b7cher", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}