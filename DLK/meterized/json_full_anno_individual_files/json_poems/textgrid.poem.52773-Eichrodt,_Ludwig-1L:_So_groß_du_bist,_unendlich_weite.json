{"textgrid.poem.52773": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: So gro\u00df du bist, unendlich weite ", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gro\u00df du bist, unendlich weite ", "tokens": ["So", "gro\u00df", "du", "bist", ",", "un\u00b7end\u00b7lich", "wei\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du vor dir selbst erschreckender Kolo\u00df,", "tokens": ["Du", "vor", "dir", "selbst", "er\u00b7schre\u00b7cken\u00b7der", "Ko\u00b7lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist doch nur, wof\u00fcr der Mensch dich h\u00e4lt,", "tokens": ["Du", "bist", "doch", "nur", ",", "wo\u00b7f\u00fcr", "der", "Mensch", "dich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Gro\u00dfen klein und nur dem Kleinen gro\u00df.", "tokens": ["Dem", "Gro\u00b7\u00dfen", "klein", "und", "nur", "dem", "Klei\u00b7nen", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erfahre du, nach reiflichster Betrachtung,", "tokens": ["Er\u00b7fah\u00b7re", "du", ",", "nach", "reif\u00b7lichs\u00b7ter", "Be\u00b7trach\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geistlose, meine g\u00fcndlichste Verachtung.", "tokens": ["Geist\u00b7lo\u00b7se", ",", "mei\u00b7ne", "g\u00fcnd\u00b7lichs\u00b7te", "Ver\u00b7ach\u00b7tung", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Man sagt, ein Gott hat dich aus ", "tokens": ["Man", "sagt", ",", "ein", "Gott", "hat", "dich", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VAFIN", "PRF", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wahr mu\u00df es sein, noch heut' bezeugt's der Schlamm;", "tokens": ["Wahr", "mu\u00df", "es", "sein", ",", "noch", "heut'", "be\u00b7zeugt's", "der", "Schlamm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$,", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was bist du? Nichts. Mit diesem Fluch bedacht:", "tokens": ["Was", "bist", "du", "?", "Nichts", ".", "Mit", "die\u00b7sem", "Fluch", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PIS", "$.", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Apfel f\u00e4llt nicht weit von seinem ", "tokens": ["Der", "Ap\u00b7fel", "f\u00e4llt", "nicht", "weit", "von", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Menschengeists erhebendes Bewu\u00dftsein,", "tokens": ["Des", "Men\u00b7schen\u00b7geists", "er\u00b7he\u00b7ben\u00b7des", "Be\u00b7wu\u00df\u00b7tsein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es sollte dir ein ewiger Verlust sein.", "tokens": ["Es", "soll\u00b7te", "dir", "ein", "e\u00b7wi\u00b7ger", "Ver\u00b7lust", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Einst betete im Bett des alten Nil", "tokens": ["Einst", "be\u00b7te\u00b7te", "im", "Bett", "des", "al\u00b7ten", "Nil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das bl\u00f6de Volk dich, Welt, mit Inbrunst an,", "tokens": ["Das", "bl\u00f6\u00b7de", "Volk", "dich", ",", "Welt", ",", "mit", "In\u00b7brunst", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$,", "NN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit dem Begriffe trieb es Affenspiel,", "tokens": ["Mit", "dem", "Be\u00b7grif\u00b7fe", "trieb", "es", "Af\u00b7fen\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Inde\u00df um Wahrheit sich verzehrt der Mann.", "tokens": ["In\u00b7de\u00df", "um", "Wahr\u00b7heit", "sich", "ver\u00b7zehrt", "der", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Raum, dem Stoff, in schmutziger Beth\u00f6rung,", "tokens": ["Dem", "Raum", ",", "dem", "Stoff", ",", "in", "schmut\u00b7zi\u00b7ger", "Be\u00b7th\u00f6\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erwies der Wahnmensch g\u00f6ttliche Verehrung.", "tokens": ["Er\u00b7wies", "der", "Wahn\u00b7mensch", "g\u00f6tt\u00b7li\u00b7che", "Ver\u00b7eh\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es ist genug. Die frische Gegenwart", "tokens": ["Es", "ist", "ge\u00b7nug", ".", "Die", "fri\u00b7sche", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entri\u00df sich l\u00e4ngst dem schn\u00f6den G\u00e4ngelband,", "tokens": ["Ent\u00b7ri\u00df", "sich", "l\u00e4ngst", "dem", "schn\u00f6\u00b7den", "G\u00e4n\u00b7gel\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von der Natur in finstrer Zeit genarrt,", "tokens": ["Von", "der", "Na\u00b7tur", "in", "finst\u00b7rer", "Zeit", "ge\u00b7narrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat sich der Mensch zum H\u00f6heren gewandt.", "tokens": ["Hat", "sich", "der", "Mensch", "zum", "H\u00f6\u00b7he\u00b7ren", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sieh! In wissenschaftlicher Verkunstung", "tokens": ["Und", "sieh", "!", "In", "wis\u00b7sen\u00b7schaft\u00b7li\u00b7cher", "Ver\u00b7kun\u00b7stung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entflieht er fr\u00f6hlich alles Seins Umdunstung.", "tokens": ["Ent\u00b7flieht", "er", "fr\u00f6h\u00b7lich", "al\u00b7les", "Seins", "Um\u00b7dun\u00b7stung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Popanz warst du, Welt, von Anbeginn,", "tokens": ["Ein", "Po\u00b7panz", "warst", "du", ",", "Welt", ",", "von", "An\u00b7be\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O, man durchschaut jetzt den ber\u00fchmten ", "tokens": ["O", ",", "man", "durch\u00b7schaut", "jetzt", "den", "be\u00b7r\u00fchm\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PIS", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Du bist erkannt, Geheimni\u00dfkr\u00e4merin,", "tokens": ["Du", "bist", "er\u00b7kannt", ",", "Ge\u00b7heim\u00b7ni\u00df\u00b7kr\u00e4\u00b7me\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Copernicus hob dir den Schleier auf.", "tokens": ["Co\u00b7per\u00b7ni\u00b7cus", "hob", "dir", "den", "Schlei\u00b7er", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Noch hundert Jahr' und auf des Archimedes", "tokens": ["Noch", "hun\u00b7dert", "Jahr'", "und", "auf", "des", "Ar\u00b7chi\u00b7me\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weltmordpunkt steht von unsern Kindern Jedes.", "tokens": ["Welt\u00b7mord\u00b7punkt", "steht", "von", "un\u00b7sern", "Kin\u00b7dern", "Je\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ha, schauder' vor dir selbst, Erb\u00e4rmliche,", "tokens": ["Ha", ",", "schau\u00b7der'", "vor", "dir", "selbst", ",", "Er\u00b7b\u00e4rm\u00b7li\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "APPR", "PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "An uns'rer Gnade zappelst du nur noch,", "tokens": ["An", "un\u00b7s'\u00b7rer", "Gna\u00b7de", "zap\u00b7pelst", "du", "nur", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur eine Galgenfrist, rein f\u00f6rmliche,", "tokens": ["Nur", "ei\u00b7ne", "Gal\u00b7gen\u00b7frist", ",", "rein", "f\u00f6rm\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und du erf\u00fcllst nicht mehr das eitle Loch.", "tokens": ["Und", "du", "er\u00b7f\u00fcllst", "nicht", "mehr", "das", "eit\u00b7le", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bis dahin br\u00fcste dich, als Somnamb\u00fcle,", "tokens": ["Bis", "da\u00b7hin", "br\u00fcs\u00b7te", "dich", ",", "als", "Som\u00b7nam\u00b7b\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PAV", "VVFIN", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In deines ", "tokens": ["In", "dei\u00b7nes"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "So gro\u00df du bist, unendlich weite ", "tokens": ["So", "gro\u00df", "du", "bist", ",", "un\u00b7end\u00b7lich", "wei\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du vor dir selbst erschreckender Kolo\u00df,", "tokens": ["Du", "vor", "dir", "selbst", "er\u00b7schre\u00b7cken\u00b7der",