{"textgrid.poem.52212": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Mit raschen Pferden jagt die Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit raschen Pferden jagt die Zeit,", "tokens": ["Mit", "ra\u00b7schen", "Pfer\u00b7den", "jagt", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein hei\u00dfes Weib, nach Freiheit lechzend;", "tokens": ["Ein", "hei\u00b7\u00dfes", "Weib", ",", "nach", "Frei\u00b7heit", "lech\u00b7zend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die halbbewu\u00dfte Menge schreit,", "tokens": ["Die", "halb\u00b7be\u00b7wu\u00df\u00b7te", "Men\u00b7ge", "schreit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gedankenlos als Vorspann \u00e4chzend.", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7los", "als", "Vor\u00b7spann", "\u00e4ch\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das tappt und tastet, wie man's lenkt;", "tokens": ["Das", "tappt", "und", "tas\u00b7tet", ",", "wie", "man's", "lenkt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie l\u00e4\u00dft den blinden Tro\u00df gew\u00e4hren", "tokens": ["Sie", "l\u00e4\u00dft", "den", "blin\u00b7den", "Tro\u00df", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00e4lt die Z\u00fcgel straff und denkt:", "tokens": ["Und", "h\u00e4lt", "die", "Z\u00fc\u00b7gel", "straff", "und", "denkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbweh mir, wenn das die einz'gen w\u00e4ren!\u00ab", "tokens": ["\u00bb", "weh", "mir", ",", "wenn", "das", "die", "einz'\u00b7gen", "w\u00e4\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "KOUS", "PDS", "ART", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Gottweib! Ernst verehr' ich sie,", "tokens": ["Ein", "Gott\u00b7weib", "!", "Ernst", "ver\u00b7ehr'", "ich", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NE", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und geh' ihr nach mit Schwert und Schilde,", "tokens": ["Und", "geh'", "ihr", "nach", "mit", "Schwert", "und", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und jauchz' ihr zu; \u2013 doch nun und nie", "tokens": ["Und", "jauch\u00b7z'", "ihr", "zu", ";", "\u2013", "doch", "nun", "und", "nie"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "ADV", "ADV", "KON", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Entweih' ich sie zum G\u00f6tzenbilde!", "tokens": ["Ent\u00b7weih'", "ich", "sie", "zum", "G\u00f6t\u00b7zen\u00b7bil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich denk' an das zu Dschagernat,", "tokens": ["Ich", "denk'", "an", "das", "zu", "Dscha\u00b7ger\u00b7nat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRELS", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor dem das Volk in langer Gasse", "tokens": ["Vor", "dem", "das", "Volk", "in", "lan\u00b7ger", "Gas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dickstirnig hinkniet, da\u00df vom Rad", "tokens": ["Dicks\u00b7tir\u00b7nig", "hin\u00b7kniet", ",", "da\u00df", "vom", "Rad"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Es jubelnd sich zermalmen lasse!", "tokens": ["Es", "ju\u00b7belnd", "sich", "zer\u00b7mal\u00b7men", "las\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit raschen Pferden jagt die Zeit,", "tokens": ["Mit", "ra\u00b7schen", "Pfer\u00b7den", "jagt", "die", "Zeit", ","], "token_