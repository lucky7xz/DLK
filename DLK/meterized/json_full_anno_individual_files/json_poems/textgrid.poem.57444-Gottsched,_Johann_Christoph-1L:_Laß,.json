{"textgrid.poem.57444": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df, ", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, ", "tokens": ["La\u00df", ","], "token_info": ["word", "punct"], "pos": ["VVIMP", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Aus Salems Stern-Gew\u00f6lben schiessen,", "tokens": ["Aus", "Sa\u00b7lems", "Stern\u00b7Ge\u00b7w\u00f6l\u00b7ben", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieh, wie viel hier Thr\u00e4nen fliessen,", "tokens": ["Und", "sieh", ",", "wie", "viel", "hier", "Thr\u00e4\u00b7nen", "flies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sieh Dein hohes Ehrenmahl!", "tokens": ["Und", "sieh", "Dein", "ho\u00b7hes", "Eh\u00b7ren\u00b7mahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Sachsen, Dein best\u00fcrtztes Mei\u00dfen,", "tokens": ["Dein", "Sach\u00b7sen", ",", "Dein", "be\u00b7st\u00fcrtz\u00b7tes", "Mei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erstaunt bey Deiner Todten-Grufft;", "tokens": ["Er\u00b7staunt", "bey", "Dei\u00b7ner", "Tod\u00b7ten\u00b7G\u00b7rufft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Auge thr\u00e4nt, die Zunge rufft:", "tokens": ["Das", "Au\u00b7ge", "thr\u00e4nt", ",", "die", "Zun\u00b7ge", "rufft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Schmertz mu\u00df unaussprechlich hei\u00dfen.", "tokens": ["Mein", "Schmertz", "mu\u00df", "un\u00b7aus\u00b7sprech\u00b7lich", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier klagt ", "tokens": ["Hier", "klagt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der Adel \u00e4chzt, der B\u00fcrger trauert,", "tokens": ["Der", "A\u00b7del", "\u00e4chzt", ",", "der", "B\u00fcr\u00b7ger", "trau\u00b7ert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat Dich nicht das Volck bedauert,", "tokens": ["Wie", "hat", "Dich", "nicht", "das", "Volck", "be\u00b7dau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald es Deinen Fall empfand?", "tokens": ["So\u00b7bald", "es", "Dei\u00b7nen", "Fall", "emp\u00b7fand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verstummt! verstummt ihr holden Seyten!", "tokens": ["Ver\u00b7stummt", "!", "ver\u00b7stummt", "ihr", "hol\u00b7den", "Sey\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Thon vermag der L\u00e4nder Noth,", "tokens": ["Kein", "Thon", "ver\u00b7mag", "der", "L\u00e4n\u00b7der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um ", "tokens": ["Um"], "token_info": ["word"], "pos": ["KOUI"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "O Schmertzens-Wort! recht anzudeuten.", "tokens": ["O", "Schmert\u00b7zens\u00b7Wort", "!", "recht", "an\u00b7zu\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Glocken bebendes Geth\u00f6n,", "tokens": ["Der", "Glo\u00b7cken", "be\u00b7ben\u00b7des", "Ge\u00b7th\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll der betr\u00fcbten Seelen Schrecken,", "tokens": ["Soll", "der", "be\u00b7tr\u00fcb\u00b7ten", "See\u00b7len", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch ihr geschwungnes Ertz entdecken,", "tokens": ["Durch", "ihr", "ge\u00b7schwung\u00b7nes", "Ertz", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und uns durch Marck und Adern gehn.", "tokens": ["Und", "uns", "durch", "Marck", "und", "A\u00b7dern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O k\u00f6nnte nur ihr banges Klingen,", "tokens": ["O", "k\u00f6nn\u00b7te", "nur", "ihr", "ban\u00b7ges", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Davon das Ohr uns t\u00e4glich gellt,", "tokens": ["Da\u00b7von", "das", "Ohr", "uns", "t\u00e4g\u00b7lich", "gellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der gantzen Europ\u00e4er-Welt,", "tokens": ["Der", "gant\u00b7zen", "Eu\u00b7ro\u00b7p\u00e4\u00b7er\u00b7Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Zeugni\u00df unsers Jammers bringen!", "tokens": ["Ein", "Zeug\u00b7ni\u00df", "un\u00b7sers", "Jam\u00b7mers", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie starb die ", "tokens": ["Wie", "starb", "die"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wie muthig hat Ihr Geist gerungen,", "tokens": ["Wie", "mut\u00b7hig", "hat", "Ihr", "Geist", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis Sie des Todes Arm bezwungen,", "tokens": ["Bis", "Sie", "des", "To\u00b7des", "Arm", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch eh er Ihre Brust besiegt.", "tokens": ["Noch", "eh", "er", "Ih\u00b7re", "Brust", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Leben lie\u00df die Kunst zu sterben", "tokens": ["Ihr", "Le\u00b7ben", "lie\u00df", "die", "Kunst", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In unverr\u00fcckter Ubung sehn:", "tokens": ["In", "un\u00b7ver\u00b7r\u00fcck\u00b7ter", "U\u00b7bung", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Unm\u00f6glich konnt es dann geschehn,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "konnt", "es", "dann", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich vor dem Tode zu entf\u00e4rben.", "tokens": ["Sich", "vor", "dem", "To\u00b7de", "zu", "ent\u00b7f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach seelig! wessen grosser Geist,", "tokens": ["Ach", "see\u00b7lig", "!", "wes\u00b7sen", "gros\u00b7ser", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "$.", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich \u00fcber die Natur erhebet,", "tokens": ["Sich", "\u00fc\u00b7ber", "die", "Na\u00b7tur", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Grufft und S\u00e4rgen nicht erbebet,", "tokens": ["Vor", "Grufft", "und", "S\u00e4r\u00b7gen", "nicht", "er\u00b7be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihn sein Sch\u00f6pfer scheiden hei\u00dft.", "tokens": ["Wenn", "ihn", "sein", "Sch\u00f6p\u00b7fer", "schei\u00b7den", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Dir, ", "tokens": ["An", "Dir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "An Dir, ", "tokens": ["An", "Dir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "An Dir, ", "tokens": ["An", "Dir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "War dieser Gro\u00dfmuth Bild zu schauen.", "tokens": ["War", "die\u00b7ser", "Gro\u00df\u00b7muth", "Bild", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Ewigkeit Saphirnes Haus", "tokens": ["Der", "E\u00b7wig\u00b7keit", "Sa\u00b7phir\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht Deiner heitern Augen Blicke,", "tokens": ["Zieht", "Dei\u00b7ner", "hei\u00b7tern", "Au\u00b7gen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von der verschm\u00e4hten Welt zur\u00fccke,", "tokens": ["Von", "der", "ver\u00b7schm\u00e4h\u00b7ten", "Welt", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tilgt der Erden Denckbild aus.", "tokens": ["Und", "tilgt", "der", "Er\u00b7den", "Denck\u00b7bild", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein heller Glantz, gleicht hundert Sonnen,", "tokens": ["Dein", "hel\u00b7ler", "Glantz", ",", "gleicht", "hun\u00b7dert", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Licht das unsern Tag zur Nacht", "tokens": ["Ein", "Licht", "das", "un\u00b7sern", "Tag", "zur", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und unsre Sonne finster macht,", "tokens": ["Und", "uns\u00b7re", "Son\u00b7ne", "fins\u00b7ter", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat Dein verkl\u00e4rtes Haupt gewonnen.", "tokens": ["Hat", "Dein", "ver\u00b7kl\u00e4r\u00b7tes", "Haupt", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was Wunder ists? Du bist es werth,", "tokens": ["Was", "Wun\u00b7der", "ists", "?", "Du", "bist", "es", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du F\u00fcrbild aller K\u00f6niginnen!", "tokens": ["Du", "F\u00fcr\u00b7bild", "al\u00b7ler", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du mustest allen Schmuck gewinnen,", "tokens": ["Du", "mus\u00b7test", "al\u00b7len", "Schmuck", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Deine Scheitel itzt verkl\u00e4rt.", "tokens": ["Der", "Dei\u00b7ne", "Schei\u00b7tel", "itzt", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun stehst Du vor des Lammes-Throne,", "tokens": ["Nun", "stehst", "Du", "vor", "des", "Lam\u00b7mes\u00b7Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verschm\u00e4hst des Purpurs Eitelkeit", "tokens": ["Ver\u00b7schm\u00e4hst", "des", "Pur\u00b7purs", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor Deiner Unschuld Perlen-Kleid,", "tokens": ["Vor", "Dei\u00b7ner", "Un\u00b7schuld", "Per\u00b7len\u00b7Kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und spottest der verla\u00dfnen Krone.", "tokens": ["Und", "spot\u00b7test", "der", "ver\u00b7la\u00df\u00b7nen", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So weit der volle Weichsel-Strand,", "tokens": ["So", "weit", "der", "vol\u00b7le", "Weich\u00b7sel\u00b7Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Niester und die Warte fliesset,", "tokens": ["Der", "Nies\u00b7ter", "und", "die", "War\u00b7te", "flies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": 