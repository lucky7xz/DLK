{"textgrid.poem.46258": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der hochheit schein ist stark, gro\u00df ist der weisheit macht,", "tokens": ["Der", "hoch\u00b7heit", "schein", "ist", "stark", ",", "gro\u00df", "ist", "der", "weis\u00b7heit", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der sterblichen gem\u00fct zu ihrer bai\u00df zu bringen:", "tokens": ["der", "sterb\u00b7li\u00b7chen", "ge\u00b7m\u00fct", "zu", "ih\u00b7rer", "bai\u00df", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "vil gr\u00f6\u00dfer aber ist der sch\u00f6nheit macht und pracht,", "tokens": ["vil", "gr\u00f6\u00b7\u00dfer", "a\u00b7ber", "ist", "der", "sch\u00f6n\u00b7heit", "macht", "und", "pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VAFIN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als die die weisheit selbs und hochheit kan bezwingen.", "tokens": ["als", "die", "die", "weis\u00b7heit", "selbs", "und", "hoch\u00b7heit", "kan", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "ADV", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gro\u00df ist gewi\u00df des golds und der hochheit gewalt,", "tokens": ["Gro\u00df", "ist", "ge\u00b7wi\u00df", "des", "golds", "und", "der", "hoch\u00b7heit", "ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "so mag die weisheit auch den menschen wolgefallen:", "tokens": ["so", "mag", "die", "weis\u00b7heit", "auch", "den", "men\u00b7schen", "wol\u00b7ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "jedoch der reich und weis mu\u00df, elend, der gestalt,", "tokens": ["je\u00b7doch", "der", "reich", "und", "weis", "mu\u00df", ",", "e\u00b7lend", ",", "der", "ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "KON", "PTKVZ", "VMFIN", "$,", "ADJD", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "dadurch die sch\u00f6nheit ihn versehret, zu fu\u00df fallen.", "tokens": ["da\u00b7durch", "die", "sch\u00f6n\u00b7heit", "ihn", "ver\u00b7seh\u00b7ret", ",", "zu", "fu\u00df", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "VVFIN", "$,", "APPR", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du, Venus, kanst allein sie aller s\u00fc\u00dfigkeit", "tokens": ["Du", ",", "Ve\u00b7nus", ",", "kanst", "al\u00b7lein", "sie", "al\u00b7ler", "s\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "ADV", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "erg\u00f6tzung, sch\u00f6nheit, lieb und lieblichkeit gewehren,", "tokens": ["er\u00b7g\u00f6t\u00b7zung", ",", "sch\u00f6n\u00b7heit", ",", "lieb", "und", "lieb\u00b7lich\u00b7keit", "ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "ADJD", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und wie durch deine kraft kein herz ohn freindlichkeit,", "tokens": ["und", "wie", "durch", "dei\u00b7ne", "kraft", "kein", "herz", "ohn", "freind\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "also kan kein geschlecht ohn deine gnad lang wehren.", "tokens": ["al\u00b7so", "kan", "kein", "ge\u00b7schlecht", "ohn", "dei\u00b7ne", "gnad", "lang", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.13": {"text": "Du, g\u00f6ttin, bist allein voll sch\u00f6nheit, lust und lieb,", "tokens": ["Du", ",", "g\u00f6t\u00b7tin", ",", "bist", "al\u00b7lein", "voll", "sch\u00f6n\u00b7heit", ",", "lust", "und", "lieb", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VAFIN", "ADV", "ADJD", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "luft, himmel, meer und erd wird nu von dir erhalten,", "tokens": ["luft", ",", "him\u00b7mel", ",", "meer", "und", "erd", "wird", "nu", "von", "dir", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "und dises alles w\u00fcrd ohn dich bald \u00f6d und tr\u00fcb,", "tokens": ["und", "di\u00b7ses", "al\u00b7les", "w\u00fcrd", "ohn", "dich", "bald", "\u00f6d", "und", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "ohn dich w\u00fcrd alles bald verkalten und veralten.", "tokens": ["ohn", "dich", "w\u00fcrd", "al\u00b7les", "bald", "ver\u00b7kal\u00b7ten", "und", "ver\u00b7al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dir dienet nicht aus zwang, sondern mit freud und scherz", "tokens": ["Dir", "die\u00b7net", "nicht", "aus", "zwang", ",", "son\u00b7dern", "mit", "freud", "und", "scherz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "VVFIN", "$,", "KON", "APPR", "VVFIN", "KON", "NN"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "was immer in der welt kan kriechen, fliegen, gehen;", "tokens": ["was", "im\u00b7mer", "in", "der", "welt", "kan", "krie\u00b7chen", ",", "flie\u00b7gen", ",", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "dan durch dein s\u00fc\u00dfe brunst ein jede seel und herz", "tokens": ["dan", "durch", "dein", "s\u00fc\u00b7\u00dfe", "brunst", "ein", "je\u00b7de", "seel", "und", "herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "ART", "PIAT", "NN", "KON", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.20": {"text": "entz\u00fcndet mutiglich kan f\u00fcr und f\u00fcr bestehen.", "tokens": ["ent\u00b7z\u00fcn\u00b7det", "mu\u00b7tig\u00b7lich", "kan", "f\u00fcr", "und", "f\u00fcr", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VMFIN", "APPR", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die luft erf\u00fcllest du mit der lieb saft und kraft,", "tokens": ["Die", "luft", "er\u00b7f\u00fcl\u00b7lest", "du", "mit", "der", "lieb", "saft", "und", "kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-++-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "mit der lieb danz und glanz die himmel du erleuchtest,", "tokens": ["mit", "der", "lieb", "danz", "und", "glanz", "die", "him\u00b7mel", "du", "er\u00b7leuch\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADV", "KON", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "die erd bereichest du mit der lieb wissenschaft,", "tokens": ["die", "erd", "be\u00b7rei\u00b7chest", "du", "mit", "der", "lieb", "wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJD", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "und mit liebreicher frucht das meer du \u00fcberfeuchtest.", "tokens": ["und", "mit", "lieb\u00b7rei\u00b7cher", "frucht", "das", "meer", "du", "\u00fc\u00b7ber\u00b7feuch\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du stillend allen zank, du legend allen streit", "tokens": ["Du", "stil\u00b7lend", "al\u00b7len", "zank", ",", "du", "le\u00b7gend", "al\u00b7len", "streit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PIAT", "NN", "$,", "PPER", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "und d\u00e4mpfend allen ha\u00df, wirst billich hochgeehret;", "tokens": ["und", "d\u00e4mp\u00b7fend", "al\u00b7len", "ha\u00df", ",", "wirst", "bil\u00b7lich", "hoch\u00b7geeh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "$,", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "so gro\u00df der sch\u00f6nheit kraft, so s\u00fc\u00df ist der lieb beut,", "tokens": ["so", "gro\u00df", "der", "sch\u00f6n\u00b7heit", "kraft", ",", "so", "s\u00fc\u00df", "ist", "der", "lieb", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "da\u00df dadurch alle welt, verliebet, sich vermehret.", "tokens": ["da\u00df", "da\u00b7durch", "al\u00b7le", "welt", ",", "ver\u00b7lie\u00b7bet", ",", "sich", "ver\u00b7meh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PAV", "PIAT", "NN", "$,", "VVFIN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Stark zwar der hochheit schein, gro\u00df ist der weisheit macht", "tokens": ["Stark", "zwar", "der", "hoch\u00b7heit", "schein", ",", "gro\u00df", "ist", "der", "weis\u00b7heit", "macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$,", "ADJD", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "der sterblichen gem\u00fct zu ihrer baiz zu bringen:", "tokens": ["der", "sterb\u00b7li\u00b7chen", "ge\u00b7m\u00fct", "zu", "ih\u00b7rer", "baiz", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "vil gr\u00f6\u00dfer aber ist der sch\u00f6nheit macht und pracht,", "tokens"