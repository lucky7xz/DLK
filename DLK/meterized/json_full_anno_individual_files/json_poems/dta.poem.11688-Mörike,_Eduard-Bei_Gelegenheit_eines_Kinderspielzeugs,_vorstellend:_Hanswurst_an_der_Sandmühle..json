{"dta.poem.11688": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Bei Gelegenheit eines Kinderspielzeugs,  \n vorstellend:  \n  Hanswurst an der Sandm\u00fchle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ich glaub's ihm gern; der Sand ist nur so nebenher.", "tokens": ["Ich", "glaub's", "ihm", "gern", ";", "der", "Sand", "ist", "nur", "so", "ne\u00b7ben\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}