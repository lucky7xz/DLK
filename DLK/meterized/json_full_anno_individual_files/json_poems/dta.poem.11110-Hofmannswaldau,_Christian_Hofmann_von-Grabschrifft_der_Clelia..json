{"dta.poem.11110": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft der Clelia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer lieget Clelia. Mehr darffst du wohl nicht wissen,", "tokens": ["Hjer", "lie\u00b7get", "Cle\u00b7lia", ".", "Mehr", "darffst", "du", "wohl", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$.", "PIS", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn dieser nahme schleust verstand und tugend ein.", "tokens": ["Denn", "die\u00b7ser", "nah\u00b7me", "schleust", "ver\u00b7stand", "und", "tu\u00b7gend", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "VVFIN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr geist war viel zu gro\u00df, der c\u00f6rper viel zu klein,", "tokens": ["Ihr", "geist", "war", "viel", "zu", "gro\u00df", ",", "der", "c\u00f6r\u00b7per", "viel", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "ART", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und also hat der tod sie beyde trennen m\u00fcssen.", "tokens": ["Und", "al\u00b7so", "hat", "der", "tod", "sie", "bey\u00b7de", "tren\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PPER", "PIS", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}