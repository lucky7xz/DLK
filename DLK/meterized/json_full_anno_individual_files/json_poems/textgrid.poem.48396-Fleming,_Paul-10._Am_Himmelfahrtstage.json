{"textgrid.poem.48396": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10. Am Himmelfahrtstage", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahr auf, du Siegesf\u00fcrst, in aller Himmel Himmel,", "tokens": ["Fahr", "auf", ",", "du", "Sie\u00b7ges\u00b7f\u00fcrst", ",", "in", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und la\u00df dich holen ein mit pr\u00e4chtigem Get\u00fcmmel,", "tokens": ["und", "la\u00df", "dich", "ho\u00b7len", "ein", "mit", "pr\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie dein Triumph erheischt! Zehntausent Engel stehn,", "tokens": ["wie", "dein", "Tri\u00b7umph", "er\u00b7heischt", "!", "Zehn\u00b7tau\u00b7sent", "En\u00b7gel", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$.", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "zehnmal zehntausent stehn, bis da\u00df du ein wirst gehn", "tokens": ["zehn\u00b7mal", "zehn\u00b7tau\u00b7sent", "stehn", ",", "bis", "da\u00df", "du", "ein", "wirst", "gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "$,", "KON", "KOUS", "PPER", "PTK