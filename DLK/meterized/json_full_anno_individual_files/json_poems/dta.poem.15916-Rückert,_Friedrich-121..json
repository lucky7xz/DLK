{"dta.poem.15916": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "121.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit meinen S\u00f6hnen ging ich wandernd \u00fcber Land,", "tokens": ["Mit", "mei\u00b7nen", "S\u00f6h\u00b7nen", "ging", "ich", "wan\u00b7dernd", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und es war wunderbar, wie ich mich da empfand.", "tokens": ["Und", "es", "war", "wun\u00b7der\u00b7bar", ",", "wie", "ich", "mich", "da", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So reizend zweifelhaft war es mir nie erschienen,", "tokens": ["So", "rei\u00b7zend", "zwei\u00b7fel\u00b7haft", "war", "es", "mir", "nie", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ob ich ihr F\u00fchrer sei, ob selbst gef\u00fchrt von ihnen.", "tokens": ["Ob", "ich", "ihr", "F\u00fch\u00b7rer", "sei", ",", "ob", "selbst", "ge\u00b7f\u00fchrt", "von", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,", "KOUS", "ADV", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie m\u00f6gen nun so fort stets unbed\u00fcrft'ger schreiten,", "tokens": ["Sie", "m\u00f6\u00b7gen", "nun", "so", "fort", "stets", "un\u00b7be\u00b7d\u00fcrft'\u00b7ger", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PTKVZ", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00e4higer, mich gern bed\u00fcrfenden zu leiten.", "tokens": ["Und", "f\u00e4\u00b7hi\u00b7ger", ",", "mich", "gern", "be\u00b7d\u00fcr\u00b7fen\u00b7den", "zu", "lei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "ADV", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}