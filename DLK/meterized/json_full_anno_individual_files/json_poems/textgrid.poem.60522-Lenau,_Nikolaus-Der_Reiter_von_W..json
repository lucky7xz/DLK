{"textgrid.poem.60522": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Reiter von W.", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem krit'schen Schusterb\u00e4nklein", "tokens": ["Auf", "dem", "krit'\u00b7schen", "Schus\u00b7ter\u00b7b\u00e4n\u00b7klein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nahmst du dich noch aus ertr\u00e4glich,", "tokens": ["Nahmst", "du", "dich", "noch", "aus", "er\u00b7tr\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hattest manchmal ein Ged\u00e4nklein;", "tokens": ["Hat\u00b7test", "manch\u00b7mal", "ein", "Ge\u00b7d\u00e4nk\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch als Dichter bist du kl\u00e4glich!", "tokens": ["Doch", "als", "Dich\u00b7ter", "bist", "du", "kl\u00e4g\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rezensenten sind fast alle", "tokens": ["Re\u00b7zen\u00b7sen\u00b7ten", "sind", "fast", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Obenleichthindr\u00fcberhuscher,", "tokens": ["O\u00b7ben\u00b7leich\u00b7thin\u00b7dr\u00fc\u00b7ber\u00b7hu\u00b7scher", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die d\u00fcmmsten mit Gelalle", "tokens": ["Und", "die", "d\u00fcmms\u00b7ten", "mit", "Ge\u00b7lal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch versifikante Pfuscher.", "tokens": ["Auch", "ver\u00b7si\u00b7fi\u00b7kan\u00b7te", "Pfu\u00b7scher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kommt der Bursch in seinem Streitwahn,", "tokens": ["Kommt", "der", "Bursch", "in", "sei\u00b7nem", "Streit\u00b7wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter tausend St\u00fcmper\u00e4ngsten,", "tokens": ["Un\u00b7ter", "tau\u00b7send", "St\u00fcm\u00b7pe\u00b7r\u00e4ngs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief zu Esel auf die Reitbahn,", "tokens": ["Tief", "zu", "E\u00b7sel", "auf", "die", "Reit\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00f6hnend von arabschen Hengsten.", "tokens": ["Dr\u00f6h\u00b7nend", "von", "a\u00b7rab\u00b7schen", "Hengs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Hei! hei! heil du krit'scher Bummler,", "tokens": ["Hei", "!", "hei", "!", "heil", "du", "krit'\u00b7scher", "Bumm\u00b7ler", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "$.", "KOUS", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeige dich nun selbst als Reiter!", "tokens": ["Zei\u00b7ge", "dich", "nun", "selbst", "als", "Rei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeige dich als kecker Tummler!", "tokens": ["Zei\u00b7ge", "dich", "als", "ke\u00b7cker", "Tumm\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sporne! peitsche! vorw\u00e4rts!! weiter!!!", "tokens": ["Spor\u00b7ne", "!", "peit\u00b7sche", "!", "vor\u00b7w\u00e4rts", "!!", "wei\u00b7ter", "!!!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "ADJA", "$.", "ADV", "$.", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6rst du's wiehern? h\u00f6rst du's rufen?", "tokens": ["H\u00f6rst", "du's", "wie\u00b7hern", "?", "h\u00f6rst", "du's", "ru\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$.", "VVFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch dein Graugaul str\u00e4ubt die Ohren,", "tokens": ["Doch", "dein", "Grau\u00b7gaul", "str\u00e4ubt", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stampfend m\u00f6cht er mit den Hufen", "tokens": ["Stamp\u00b7fend", "m\u00f6cht", "er", "mit", "den", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Erde sich verbohren.", "tokens": ["In", "die", "Er\u00b7de", "sich", "ver\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die Reiter nehmens Kr\u00e4nzlein,", "tokens": ["Und", "die", "Rei\u00b7ter", "neh\u00b7mens", "Kr\u00e4nz\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das du ihnen gabst zur Ehre,", "tokens": ["Das", "du", "ih\u00b7nen", "gabst", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie bindens an das Schw\u00e4nzlein", "tokens": ["Und", "sie", "bin\u00b7dens", "an", "das", "Schw\u00e4nz\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lachend deiner grauen M\u00e4hre.", "tokens": ["La\u00b7chend", "dei\u00b7ner", "grau\u00b7en", "M\u00e4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Raschelnd mit den Lorbeerbauschen", "tokens": ["Ra\u00b7schelnd", "mit", "den", "Lor\u00b7beer\u00b7bau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Peitscht der Esel sich die Flanken,", "tokens": ["Peitscht", "der", "E\u00b7sel", "sich", "die", "Flan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Spottgel\u00e4chters Rauschen", "tokens": ["Un\u00b7ter", "Spott\u00b7ge\u00b7l\u00e4ch\u00b7ters", "Rau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bricht er scheu aus unsern Schranken.", "tokens": ["Bricht", "er", "scheu", "aus", "un\u00b7sern", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Die zerzauste Panegyrik", "tokens": ["Die", "zer\u00b7zaus\u00b7te", "Pa\u00b7ne\u00b7gy\u00b7rik"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Hat der Wind davongetragen,", "tokens": ["Hat", "der", "Wind", "da\u00b7von\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lachend denkt man nur der Lyrik,", "tokens": ["La\u00b7chend", "denkt", "man", "nur", "der", "Ly\u00b7rik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die dein Esel aufgeschlagen.", "tokens": ["Die", "dein", "E\u00b7sel", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Reiter, die dir nicht gefallen,", "tokens": ["Rei\u00b7ter", ",", "die", "dir", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du j\u00fcngst so scharf gescholten,", "tokens": ["Die", "du", "j\u00fcngst", "so", "scharf", "ge\u00b7schol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben spottend jetzt vor allen", "tokens": ["Ha\u00b7ben", "spot\u00b7tend", "jetzt", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schadenfreudig dirs vergolten.", "tokens": ["Scha\u00b7den\u00b7freu\u00b7dig", "dirs", "ver\u00b7gol\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Willst du richten unser Dichten,", "tokens": ["Willst", "du", "rich\u00b7ten", "un\u00b7ser", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die Vers' im Halse stecken;", "tokens": ["La\u00df", "die", "Ver\u00b7s'", "im", "Hal\u00b7se", "ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zernichten dir dein Richten!", "tokens": ["Sie", "zer\u00b7nich\u00b7ten", "dir", "dein", "Rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df den Grauen bei den S\u00e4cken!", "tokens": ["La\u00df", "den", "Grau\u00b7en", "bei", "den", "S\u00e4\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "La\u00df als M\u00fcller du dein Fohlen", "tokens": ["La\u00df", "als", "M\u00fcl\u00b7ler", "du", "dein", "Foh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOKOM", "NE", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immerhin zur M\u00fchle gehen;", "tokens": ["Im\u00b7mer\u00b7hin", "zur", "M\u00fch\u00b7le", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und als Schuster flick die Sohlen", "tokens": ["Und", "als", "Schus\u00b7ter", "flick", "die", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlechtbeschlagnen Renommeen!", "tokens": ["Schlecht\u00b7be\u00b7schlag\u00b7nen", "Re\u00b7nom\u00b7meen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Auf dem krit'schen Schusterb\u00e4nklein", "tokens": ["Auf",