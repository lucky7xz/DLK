{"textgrid.poem.53393": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Innigliche Frewde", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da lieg du Volck der finstern Erden,", "tokens": ["Da", "lieg", "du", "Volck", "der", "fins\u00b7tern", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gram, Kranckheit, Thr\u00e4nen, Hertzeleid,", "tokens": ["Gram", ",", "Kran\u00b7ck\u00b7heit", ",", "Thr\u00e4\u00b7nen", ",", "Hert\u00b7ze\u00b7leid", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tyrannen meiner Lebens-Zeit,", "tokens": ["Ty\u00b7ran\u00b7nen", "mei\u00b7ner", "Le\u00b7bens\u00b7Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sclav ich muste t\u00e4glich werden,", "tokens": ["Der", "Sclav", "ich", "mus\u00b7te", "t\u00e4g\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Seelenfeind', und du o Leib,", "tokens": ["Ihr", "See\u00b7len\u00b7feind'", ",", "und", "du", "o", "Leib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Knechtschafft Auffenthalt, da bleib,", "tokens": ["Der", "Knecht\u00b7schafft", "Auf\u00b7fent\u00b7halt", ",", "da", "bleib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott holt mich heim aus ewren Banden,", "tokens": ["Gott", "holt", "mich", "heim", "aus", "ew\u00b7ren", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun bin ich himmlisch, ewig, mein,", "tokens": ["Nun", "bin", "ich", "himm\u00b7lisch", ",", "e\u00b7wig", ",", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reich, sieghafft, \u00fcber alle Pein,", "tokens": ["Reich", ",", "sieg\u00b7hafft", ",", "\u00fc\u00b7ber", "al\u00b7le", "Pein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ich so viel hab' ausgestanden,", "tokens": ["Der", "ich", "so", "viel", "hab'", "aus\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ewigheit ihr Glantz und Pracht", "tokens": ["Die", "E\u00b7wig\u00b7heit", "ihr", "Glantz", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sampt allem ist in meiner Macht.", "tokens": ["Sampt", "al\u00b7lem", "ist", "in", "mei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein ist die Gn\u00fcg in allen Dingen", "tokens": ["Mein", "ist", "die", "Gn\u00fcg", "in", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Lust ohn Ende wehren sol,", "tokens": ["Der", "Lust", "ohn", "En\u00b7de", "weh\u00b7ren", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein freyer Mund ist lachens voll", "tokens": ["Mein", "frey\u00b7er", "Mund", "ist", "la\u00b7chens", "voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Zunge kan nur singen,", "tokens": ["Und", "mei\u00b7ne", "Zun\u00b7ge", "kan", "nur", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Abgrund meines Hertzens brennt", "tokens": ["Der", "Ab\u00b7grund", "mei\u00b7nes", "Hert\u00b7zens", "brennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Frewden, die mein Aug' erkennt.", "tokens": ["F\u00fcr", "Frew\u00b7den", ",", "die", "mein", "Aug'", "er\u00b7kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gegr\u00fcsset seyd ihr sch\u00f6nen Awen,", "tokens": ["Ge\u00b7gr\u00fcs\u00b7set", "seyd", "ihr", "sch\u00f6\u00b7nen", "A\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Lebens-Brunnen allzumal,", "tokens": ["Ihr", "Le\u00b7bens\u00b7Brun\u00b7nen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd du der Frommen grosser Saal", "tokens": ["Vnd", "du", "der", "From\u00b7men", "gros\u00b7ser", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Daran kein Mensch hat m\u00fcssen bawen,", "tokens": ["Da\u00b7ran", "kein", "Mensch", "hat", "m\u00fcs\u00b7sen", "ba\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Vnd den der H\u00f6chste weit und breit", "tokens": ["Vnd", "den", "der", "H\u00f6chs\u00b7te", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erf\u00fcllt mit seiner Herrligheit.", "tokens": ["Er\u00b7f\u00fcllt", "mit", "sei\u00b7ner", "Herr\u00b7lig\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr V\u00e4ter und ihr Seraphinen", "tokens": ["Ihr", "V\u00e4\u00b7ter", "und", "ihr", "Se\u00b7ra\u00b7phi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du grosse Himmels-B\u00fcrgerey", "tokens": ["Du", "gros\u00b7se", "Him\u00b7mels\u00b7B\u00fcr\u00b7ge\u00b7rey"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr die ihr theils durch grosse Trew", "tokens": ["Ihr", "die", "ihr", "theils", "durch", "gros\u00b7se", "Trew"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Menschen euch bem\u00fcht zu dienen", "tokens": ["Den", "Men\u00b7schen", "euch", "be\u00b7m\u00fcht", "zu", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Theils Gott ohn Ende loben m\u00fcsst,", "tokens": ["Theils", "Gott", "ohn", "En\u00b7de", "lo\u00b7ben", "m\u00fcsst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Wahrheit-Zeugen seyd gegr\u00fcsst.", "tokens": ["Ihr", "Wahr\u00b7heit\u00b7Zeu\u00b7gen", "seyd", "ge\u00b7gr\u00fcs\u00b7st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O macht mich theilhafft ewrer Frewden", "tokens": ["O", "macht", "mich", "theil\u00b7hafft", "ew\u00b7rer", "Frew\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd nehmt in ewre Zunfft mich ein,", "tokens": ["Vnd", "nehmt", "in", "ew\u00b7re", "Zunfft", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich werd euch nicht ver\u00e4chtlich seyn,", "tokens": ["Ich", "werd", "euch", "nicht", "ver\u00b7\u00e4cht\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn seht, ich komm aus grossem Leiden,", "tokens": ["Denn", "seht", ",", "ich", "komm", "aus", "gros\u00b7sem", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lammes Blut und Todes-Schwei\u00df", "tokens": ["Des", "Lam\u00b7mes", "Blut", "und", "To\u00b7des\u00b7Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht meine Kleider hell und wei\u00df", "tokens": ["Macht", "mei\u00b7ne", "Klei\u00b7der", "hell", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Ich wil zum Tempel Gottes wallen", "tokens": ["Ich", "wil", "zum", "Tem\u00b7pel", "Got\u00b7tes", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit heisser Andacht und mit Danck,", "tokens": ["Mit", "heis\u00b7ser", "An\u00b7dacht", "und", "mit", "Danck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da sol Tag und Nacht der Klang", "tokens": ["Und", "da", "sol", "Tag", "und", "Nacht", "der", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch meiner Stimmen widerschallen,", "tokens": ["Auch", "mei\u00b7ner", "Stim\u00b7men", "wi\u00b7der\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Lamm im Saal nimmt immerdar", "tokens": ["Das", "Lamm", "im", "Saal", "nimmt", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch volle Weide meiner wahr.", "tokens": ["Durch", "vol\u00b7le", "Wei\u00b7de", "mei\u00b7ner", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es f\u00fchrt mich zu den k\u00fchlen Bronnen", "tokens": ["Es", "f\u00fchrt", "mich", "zu", "den", "k\u00fch\u00b7len", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Daraus ohn Ende leben quillt,", "tokens": ["Da\u00b7raus", "ohn", "En\u00b7de", "le\u00b7ben", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist das meine Thr\u00e4nen stillt,", "tokens": ["Es", "ist", "das", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "stillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hie f\u00fchl ich keine Glut der Sonnen,", "tokens": ["Hie", "f\u00fchl", "ich", "kei\u00b7ne", "Glut", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht Durst noch Hunger kan mir zu", "tokens": ["Nicht", "Durst", "noch", "Hun\u00b7ger", "kan", "mir", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "NN", "VMFIN", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn meine Gn\u00fcge Gott bist du.", "tokens": ["Denn", "mei\u00b7ne", "Gn\u00fc\u00b7ge", "Gott", "bist", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du hast uns von dem Bann der Erden", "tokens": ["Du", "hast", "uns", "von", "dem", "Bann", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erkaufft durch deinen eignen Sohn,", "tokens": ["Er\u00b7kaufft", "durch", "dei\u00b7nen", "eig\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd l\u00e4ssest jetzt vor deinem Thron", "tokens": ["Vnd", "l\u00e4s\u00b7sest", "jetzt", "vor", "dei\u00b7nem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vns K\u00f6nig' und auch Priester werden.", "tokens": ["Vns", "K\u00f6\u00b7nig'", "und", "auch", "Pries\u00b7ter", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr solche Trew solst du allein", "tokens": ["F\u00fcr", "sol\u00b7che", "Trew", "solst", "du", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Ewigheit mein Dancklied seyn.", "tokens": ["In", "E\u00b7wig\u00b7heit", "mein", "Danck\u00b7lied", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da lieg du Volck der finstern Erden,", "tokens": ["Da", "lieg", "du", "Volck", "der", "fins\u00b7tern", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "ADJA",