{"dta.poem.18271": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An die Conventionellen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ich k\u00e4u nicht wieder wie das liebe Vieh;", "tokens": ["Ich", "k\u00e4u", "nicht", "wie\u00b7der", "wie", "das", "lie\u00b7be", "Vieh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4ngst hab ich von den Schuhen ihn gesch\u00fcttelt,", "tokens": ["L\u00e4ngst", "hab", "ich", "von", "den", "Schu\u00b7hen", "ihn", "ge\u00b7sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den grauen Schulstaub eurer Poesie!", "tokens": ["Den", "grau\u00b7en", "Schul\u00b7staub", "eu\u00b7rer", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich hab mich umgesehn in meinem Volke", "tokens": ["Ich", "hab", "mich", "um\u00b7ge\u00b7sehn", "in", "mei\u00b7nem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meiner Zeit bis tief ins Herz geschaut", "tokens": ["Und", "mei\u00b7ner", "Zeit", "bis", "tief", "ins", "Herz", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und n\u00e4chtlich ist aus dunkler Wetterwolke", "tokens": ["Und", "n\u00e4cht\u00b7lich", "ist", "aus", "dunk\u00b7ler", "Wet\u00b7ter\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein heilig Feuer in mein Lied gethaut.", "tokens": ["Ein", "hei\u00b7lig", "Feu\u00b7er", "in", "mein", "Lied", "ge\u00b7thaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun ruf ich zu des Himmels goldnen Kronen:", "tokens": ["Nun", "ruf", "ich", "zu", "des", "Him\u00b7mels", "gold\u00b7nen", "Kro\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dreimal verflucht sei jegliche Dressur!", "tokens": ["Drei\u00b7mal", "ver\u00b7flucht", "sei", "jeg\u00b7li\u00b7che", "Dres\u00b7sur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Teufel eure kindischen Schablonen!", "tokens": ["Zum", "Teu\u00b7fel", "eu\u00b7re", "kin\u00b7di\u00b7schen", "Scha\u00b7blo\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}