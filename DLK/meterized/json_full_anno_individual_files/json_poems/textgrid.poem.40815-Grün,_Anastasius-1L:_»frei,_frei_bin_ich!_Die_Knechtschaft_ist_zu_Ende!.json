{"textgrid.poem.40815": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbfrei, frei bin ich! Die Knechtschaft ist zu Ende!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbfrei, frei bin ich! Die Knechtschaft ist zu Ende!", "tokens": ["\u00bb", "frei", ",", "frei", "bin", "ich", "!", "Die", "Knecht\u00b7schaft", "ist", "zu", "En\u00b7de", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "$.", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das offne Thor, ha, wie mich's fast erschreckte!", "tokens": ["Das", "off\u00b7ne", "Thor", ",", "ha", ",", "wie", "mich's", "fast", "er\u00b7schreck\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ITJ", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie ungelenk jetzt fesselfrei die H\u00e4nde,", "tokens": ["Wie", "un\u00b7ge\u00b7lenk", "jetzt", "fes\u00b7sel\u00b7frei", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die einst in Ketten leicht zu Gott ich streckte!", "tokens": ["Die", "einst", "in", "Ket\u00b7ten", "leicht", "zu", "Gott", "ich", "streck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "ADJD", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Frei, frei bin ich! Die Fesseln sind gefallen,", "tokens": ["Frei", ",", "frei", "bin", "ich", "!", "Die", "Fes\u00b7seln", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PPER", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Licht, wie blend'st du meine Augenlider!", "tokens": ["O", "Licht", ",", "wie", "blen\u00b7d'st", "du", "mei\u00b7ne", "Au\u00b7gen\u00b7li\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Frei darf ich durch den Garten Gottes wallen", "tokens": ["Frei", "darf", "ich", "durch", "den", "Gar\u00b7ten", "Got\u00b7tes", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "VVINF"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Und st\u00fcrzen an die Herzen meiner Br\u00fcder!", "tokens": ["Und", "st\u00fcr\u00b7zen", "an", "die", "Her\u00b7zen", "mei\u00b7ner", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Reicht eure H\u00e4nde mir! \u2013 Doch, ach, wie sollen", "tokens": ["Reicht", "eu\u00b7re", "H\u00e4n\u00b7de", "mir", "!", "\u2013", "Doch", ",", "ach", ",", "wie", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$.", "$(", "KON", "$,", "ITJ", "$,", "PWAV", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie dringen durch der Gr\u00e4ber gr\u00fcne Decken!", "tokens": ["Sie", "drin\u00b7gen", "durch", "der", "Gr\u00e4\u00b7ber", "gr\u00fc\u00b7ne", "De\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die Lebend'gen flieh'n, denn nimmer wollen", "tokens": ["Und", "die", "Le\u00b7ben\u00b7d'\u00b7gen", "flieh'n", ",", "denn", "nim\u00b7mer", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "KON", "ADV", "VMFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Sie mit des Sklaven Handschlag sich beflecken!", "tokens": ["Sie", "mit", "des", "Skla\u00b7ven", "Hand\u00b7schlag", "sich", "be\u00b7fle\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohlan, so will ich selber denn erringen", "tokens": ["Wo\u00b7hlan", ",", "so", "will", "ich", "sel\u00b7ber", "denn", "er\u00b7rin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir neue Liebe und ein neues Leben!", "tokens": ["Mir", "neu\u00b7e", "Lie\u00b7be", "und", "ein", "neu\u00b7es", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch f\u00fchl' ich Jugendkraft den Arm beschwingen,", "tokens": ["Noch", "f\u00fchl'", "ich", "Ju\u00b7gend\u00b7kraft", "den", "Arm", "be\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Jugend Locken noch ums Haupt mir schweben!", "tokens": ["Der", "Ju\u00b7gend", "Lo\u00b7cken", "noch", "ums", "Haupt", "mir", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da nahm mein Todfeind schweigend mich am Arme", "tokens": ["Da", "nahm", "mein", "Tod\u00b7feind", "schwei\u00b7gend", "mich", "am", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stellte mich vor einer Quelle Spiegel:", "tokens": ["Und", "stell\u00b7te", "mich", "vor", "ei\u00b7ner", "Quel\u00b7le", "Spie\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O weh, mein Haupt eisgrau, da\u00df Gott erbarme!", "tokens": ["O", "weh", ",", "mein", "Haupt", "eis\u00b7grau", ",", "da\u00df", "Gott", "er\u00b7bar\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "ADJD", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Wang' 