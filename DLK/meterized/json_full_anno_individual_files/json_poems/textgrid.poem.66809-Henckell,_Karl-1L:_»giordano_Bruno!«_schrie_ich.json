, "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des M\u00e4rtyrers ins Auge,", "tokens": ["Des", "M\u00e4r\u00b7ty\u00b7rers", "ins", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wehm\u00fctig l\u00e4chelnd.", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "l\u00e4\u00b7chelnd", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "In k\u00fchnen Ringeln fielen", "tokens": ["In", "k\u00fch\u00b7nen", "Rin\u00b7geln", "fie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Um sein Haupt die Locken,", "tokens": ["Um", "sein", "Haupt", "die", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Sinnlich keck", "tokens": ["Sinn\u00b7lich", "keck"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Und \u00fcberm\u00fctig bl\u00fchte", "tokens": ["Und", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der kleine Schnurrbart,", "tokens": ["Der", "klei\u00b7ne", "Schnurr\u00b7bart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Frei lag der Hals,", "tokens": ["Frei", "lag", "der", "Hals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Und um die breite Brust", "tokens": ["Und", "um", "die", "brei\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Bauschte sich voll die weite Gewandung.", "tokens": ["Bauschte", "sich", "voll", "die", "wei\u00b7te", "Ge\u00b7wan\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Glutlodernd wieder,", "tokens": ["Glut\u00b7lo\u00b7dernd", "wie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Von wirklichen Blitzen entz\u00fcndet,", "tokens": ["Von", "wirk\u00b7li\u00b7chen", "Blit\u00b7zen", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.20": {"text": "Empordr\u00e4ute mein Traumschreck.", "tokens": ["Em\u00b7por\u00b7dr\u00e4u\u00b7te", "mein", "Traum\u00b7schreck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+++", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Auf gr\u00e4\u00dflichem Scheiterhaufen", "tokens": ["Auf", "gr\u00e4\u00df\u00b7li\u00b7chem", "Schei\u00b7ter\u00b7hau\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Der qualmumw\u00f6lkte", "tokens": ["Der", "qual\u00b7mum\u00b7w\u00f6lk\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Herrliche Held und Denker der Welt hing.", "tokens": ["Herr\u00b7li\u00b7che", "Held", "und", "Den\u00b7ker", "der", "Welt", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Die b\u00f6sen Idioten,", "tokens": ["Die", "b\u00f6\u00b7sen", "I\u00b7dio\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Verkappte Masken", "tokens": ["Ver\u00b7kapp\u00b7te", "Mas\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "In langen, wei\u00dfen,", "tokens": ["In", "lan\u00b7gen", ",", "wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Schleppenden Laken", "tokens": ["Schlep\u00b7pen\u00b7den", "La\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Tanzten wie Orang-Utans", "tokens": ["Tanz\u00b7ten", "wie", "O\u00b7rang\u00b7U\u00b7tans"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOKOM", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Kreischend im Kreise,", "tokens": ["Krei\u00b7schend", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Und die gefr\u00e4\u00dfigste Bestie", "tokens": ["Und", "die", "ge\u00b7fr\u00e4\u00b7\u00dfigs\u00b7te", "Be\u00b7stie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-++--+", "