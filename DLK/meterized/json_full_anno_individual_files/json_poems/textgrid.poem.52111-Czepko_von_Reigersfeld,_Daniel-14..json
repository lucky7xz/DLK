{"textgrid.poem.52111": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nechst sah' ich vor das Thor den armen S\u00fcnder f\u00fchren,", "tokens": ["Nechst", "sah'", "ich", "vor", "das", "Thor", "den", "ar\u00b7men", "S\u00fcn\u00b7der", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Pater schry ihm zu: Das Beil wird dich zuvieren,", "tokens": ["Der", "Pa\u00b7ter", "schry", "ihm", "zu", ":", "Das", "Beil", "wird", "dich", "zu\u00b7vie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Pfahl durchbohrn, das Rad zerschelln, der Rost verzehrn,", "tokens": ["Der", "Pfahl", "durch\u00b7bohrn", ",", "das", "Rad", "zer\u00b7schelln", ",", "der", "Rost", "ver\u00b7zehrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Teuffel hole mich wirst du dich nicht bekehrn.", "tokens": ["Der", "Teuf\u00b7fel", "ho\u00b7le", "mich", "wirst", "du", "dich", "nicht", "be\u00b7kehrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VAFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der arme Tropp erschrack: nahm an den rechten Glauben,", "tokens": ["Der", "ar\u00b7me", "Tropp", "er\u00b7schrack", ":", "nahm", "an", "den", "rech\u00b7ten", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Pater wuste hoch die sch\u00f6ne That zu schrauben:", "tokens": ["Der", "Pa\u00b7ter", "wus\u00b7te", "hoch", "die", "sch\u00f6\u00b7ne", "That", "zu", "schrau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was r\u00fchmst du dich, aus Furcht, aus Angst nimmt er ihn an,", "tokens": ["Was", "r\u00fchmst", "du", "dich", ",", "aus", "Furcht", ",", "aus", "Angst", "nimmt", "er", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$,", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du hast ihn nicht bekehrt, der Hencker hats gethan.", "tokens": ["Du", "hast", "ihn", "nicht", "be\u00b7kehrt", ",", "der", "Hen\u00b7cker", "hats", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "ART", "NN", "VAFIN", "VVPP"