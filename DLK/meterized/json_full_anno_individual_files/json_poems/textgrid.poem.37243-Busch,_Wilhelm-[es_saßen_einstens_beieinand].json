{"textgrid.poem.37243": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es sa\u00dfen einstens beieinand]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00dfen einstens beieinand", "tokens": ["Es", "sa\u00b7\u00dfen", "eins\u00b7tens", "bei\u00b7ei\u00b7nand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Knaben, Fritz und Ferdinand.", "tokens": ["Zwei", "Kna\u00b7ben", ",", "Fritz", "und", "Fer\u00b7di\u00b7nand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NE", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sprach der Fritz: Nun gib mal acht,", "tokens": ["Da", "sprach", "der", "Fritz", ":", "Nun", "gib", "mal", "acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$.", "ADV", "VVIMP", "ADV", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich getr\u00e4umt vergangne Nacht.", "tokens": ["Was", "ich", "ge\u00b7tr\u00e4umt", "ver\u00b7gang\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich stieg in einen sch\u00f6nen Wagen,", "tokens": ["Ich", "stieg", "in", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wagen war mit Gold beschlagen.", "tokens": ["Der", "Wa\u00b7gen", "war", "mit", "Gold", "be\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zwei Englein spannten sich davor,", "tokens": ["Zwei", "En\u00b7glein", "spann\u00b7ten", "sich", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zogen mich zum Himmelstor.", "tokens": ["Die", "zo\u00b7gen", "mich", "zum", "Him\u00b7mels\u00b7tor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleich kamst du auch und wolltest mit", "tokens": ["Gleich", "kamst", "du", "auch", "und", "woll\u00b7test", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VMFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprangest auf den Kutschentritt,", "tokens": ["Und", "spran\u00b7gest", "auf", "den", "Kut\u00b7schen\u00b7tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch ein Teufel, schwarz und gro\u00df,", "tokens": ["Je\u00b7doch", "ein", "Teu\u00b7fel", ",", "schwarz", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der nahm dich hinten bei der Hos", "tokens": ["Der", "nahm", "dich", "hin\u00b7ten", "bei", "der", "Hos"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und hat dich in die H\u00f6ll getragen.", "tokens": ["Und", "hat", "dich", "in", "die", "H\u00f6ll", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es war sehr lustig, mu\u00df ich sagen. \u2013", "tokens": ["Es", "war", "sehr", "lus\u00b7tig", ",", "mu\u00df", "ich", "sa\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So h\u00fcbsch nun dieses Traumgesicht,", "tokens": ["So", "h\u00fcbsch", "nun", "die\u00b7ses", "Traum\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Ferdinand gefiel es nicht.", "tokens": ["Dem", "Fer\u00b7di\u00b7nand", "ge\u00b7fiel", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schlapp! schlug er Fritzen an das Ohr,", "tokens": ["Schlapp", "!", "schlug", "er", "Frit\u00b7zen", "an", "das", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er die Zippelm\u00fctz verlor.",