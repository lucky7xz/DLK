{"dta.poem.8527": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Gesegnet .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du doch, o M&#228;dchen?", "tokens": ["Wer", "bist", "du", "doch", ",", "o", "M", "&#228;", "dchen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "XML_entity", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "FM", "NE", "$(", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du mit dem schwarzen Schleier,", "tokens": ["Du", "mit", "dem", "schwar\u00b7zen", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit dem schwarzen Sklaven?", "tokens": ["Und", "mit", "dem", "schwar\u00b7zen", "Skla\u00b7ven", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der wei\u00dfen Sklavin du?", "tokens": ["Der", "wei\u00b7\u00dfen", "Skla\u00b7vin", "du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie Sterne deine Augen", "tokens": ["Wie", "Ster\u00b7ne", "dei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch deines Schleiers N\u00e4chte,", "tokens": ["Durch", "dei\u00b7nes", "Schlei\u00b7ers", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Gang wie der Gazelle,", "tokens": ["Dein", "Gang", "wie", "der", "Ga\u00b7zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Palme die Gestalt.", "tokens": ["Wie", "Pal\u00b7me", "die", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gesegnet sind die Wellen", "tokens": ["Ge\u00b7seg\u00b7net", "sind", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Bades, die dich k\u00fchlen,", "tokens": ["Des", "Ba\u00b7des", ",", "die", "dich", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesegnet die Gew\u00e4nder,", "tokens": ["Ge\u00b7seg\u00b7net", "die", "Ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umschlie\u00dfend deine Huld.", "tokens": ["Um\u00b7schlie\u00b7\u00dfend", "dei\u00b7ne", "Huld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und siebenfach gesegnet", "tokens": ["Und", "sie\u00b7ben\u00b7fach", "ge\u00b7seg\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sklave, dem du winkest,", "tokens": ["Der", "Skla\u00b7ve", ",", "dem", "du", "win\u00b7kest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der deinen Tritten lauschet,", "tokens": ["Der", "dei\u00b7nen", "Trit\u00b7ten", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der deine Stimme h\u00f6rt.", "tokens": ["Der", "dei\u00b7ne", "Stim\u00b7me", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und tausendfach gesegnet", "tokens": ["Und", "tau\u00b7send\u00b7fach", "ge\u00b7seg\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sklavin, der du l\u00e4chelst,", "tokens": ["Die", "Skla\u00b7vin", ",", "der", "du", "l\u00e4\u00b7chelst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An ihre Schulter lehnend", "tokens": ["An", "ih\u00b7re", "Schul\u00b7ter", "leh\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein unverschleiert Haupt.", "tokens": ["Dein", "un\u00b7ver\u00b7schlei\u00b7ert", "Haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}