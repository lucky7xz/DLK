{"textgrid.poem.34906": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Richard", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl durch der W\u00e4lder ein\u00f6dige Pracht", "tokens": ["Wohl", "durch", "der", "W\u00e4l\u00b7der", "ei\u00b7n\u00f6\u00b7di\u00b7ge", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Jagt ungest\u00fcm ein Reiter;", "tokens": ["Jagt", "un\u00b7ge\u00b7st\u00fcm", "ein", "Rei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er bl\u00e4st ins Horn, er singt und lacht", "tokens": ["Er", "bl\u00e4st", "ins", "Horn", ",", "er", "singt", "und", "lacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar seelenvergn\u00fcgt und heiter.", "tokens": ["Gar", "see\u00b7len\u00b7ver\u00b7gn\u00fcgt", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sein Harnisch ist von starkem Erz,", "tokens": ["Sein", "Har\u00b7nisch", "ist", "von", "star\u00b7kem", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch st\u00e4rker ist sein Gem\u00fcte,", "tokens": ["Noch", "st\u00e4r\u00b7ker", "ist", "sein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das ist Herr Richard L\u00f6wenherz,", "tokens": ["Das", "ist", "Herr", "Ric\u00b7hard", "L\u00f6\u00b7wen\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der christlichen Ritterschaft Bl\u00fcte.", "tokens": ["Der", "christ\u00b7li\u00b7chen", "Rit\u00b7ter\u00b7schaft", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbwillkommen in England!\u00ab rufen ihm zu", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", "in", "En\u00b7gland", "!", "\u00ab", "ru\u00b7fen", "ihm", "zu"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "APPR", "NE", "$.", "$(", "VVFIN", "PPER", "PTKZU"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die B\u00e4ume mit gr\u00fcnen Zungen \u2013", "tokens": ["Die", "B\u00e4u\u00b7me", "mit", "gr\u00fc\u00b7nen", "Zun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbwir freuen uns, o K\u00f6nig, da\u00df du", "tokens": ["\u00bb", "wir", "freu\u00b7en", "uns", ",", "o", "K\u00f6\u00b7nig", ",", "da\u00df", "du"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u00d6streichischer Haft entsprungen.\u00ab", "tokens": ["\u00d6s\u00b7trei\u00b7chi\u00b7scher", "Haft", "ent\u00b7sprun\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dem K\u00f6nig ist wohl in der freien Luft,", "tokens": ["Dem", "K\u00f6\u00b7nig", "ist", "wohl", "in", "der", "frei\u00b7en", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er f\u00fchlt sich wie neugeboren,", "tokens": ["Er", "f\u00fchlt", "sich", "wie", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er denkt an \u00d6streichs Festungsduft \u2013", "tokens": ["Er", "denkt", "an", "\u00d6s\u00b7treichs", "Fes\u00b7tungs\u00b7duft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gibt seinem Pferde die Sporen.", "tokens": ["Und", "gibt", "sei\u00b7nem", "Pfer\u00b7de", "die", "Spo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Wohl durch der W\u00e4lder ein\u00f6dige Pracht", "tokens": ["Wohl", "durch", "der", "W\u00e4l\u00b7der", "ei\u00b7n\u00f6\u00b7di\u00b7ge", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "