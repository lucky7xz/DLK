{"dta.poem.18791": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hatem .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Nun wer weiss was sie erf\u00fcllet!", "tokens": ["Nun", "wer", "weiss", "was", "sie", "er\u00b7f\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennt ihr solcher Tiefe Grund?", "tokens": ["Kennt", "ihr", "sol\u00b7cher", "Tie\u00b7fe", "Grund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbstgef\u00fchltes Lied entquillet,", "tokens": ["Selbst\u00b7ge\u00b7f\u00fchl\u00b7tes", "Lied", "ent\u00b7quil\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbstgedichtetes dem Mund.", "tokens": ["Selbst\u00b7ge\u00b7dich\u00b7te\u00b7tes", "dem", "Mund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von euch Dichterinnen allen", "tokens": ["Von", "euch", "Dich\u00b7te\u00b7rin\u00b7nen", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ihr eben keine gleich:", "tokens": ["Ist", "ihr", "e\u00b7ben", "kei\u00b7ne", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie singt mir zu gefallen,", "tokens": ["Denn", "sie", "singt", "mir", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr singt und liebt nur euch.", "tokens": ["Und", "ihr", "singt", "und", "liebt", "nur", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}