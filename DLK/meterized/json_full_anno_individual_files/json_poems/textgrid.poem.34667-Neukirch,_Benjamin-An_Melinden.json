{"textgrid.poem.34667": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "An Melinden", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste der seelen/ ich mu\u00df es bekennen:", "tokens": ["Sch\u00f6ns\u00b7te", "der", "see\u00b7len", "/", "ich", "mu\u00df", "es", "be\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df mich dein augen-blitz endlich besiegt/", "tokens": ["Da\u00df", "mich", "dein", "au\u00b7gen\u00b7blitz", "end\u00b7lich", "be\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Da\u00df ich vor liebe wie feuer mu\u00df brennen;", "tokens": ["Da\u00df", "ich", "vor", "lie\u00b7be", "wie", "feu\u00b7er", "mu\u00df", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VVFIN", "KOKOM", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weil mir mein hertze selbst wunden zuf\u00fcgt;", "tokens": ["Weil", "mir", "mein", "hert\u00b7ze", "selbst", "wun\u00b7den", "zu\u00b7f\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wieder mich kriegt.", "tokens": ["Und", "wie\u00b7der", "mich", "kriegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Drum setze mir/ o meine zier!", "tokens": ["Drum", "set\u00b7ze", "mir", "/", "o", "mei\u00b7ne", "zier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "FM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die schaalen der himmlischen lebens-krafft f\u00fcr/", "tokens": ["Die", "schaa\u00b7len", "der", "himm\u00b7li\u00b7schen", "le\u00b7bens\u00b7krafft", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und la\u00df mir die morgen", "tokens": ["Und", "la\u00df", "mir", "die", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der traurigen sorgen/", "tokens": ["Der", "trau\u00b7ri\u00b7gen", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die t\u00e4glich in meinen gedancken entstehn/", "tokens": ["Die", "t\u00e4g\u00b7lich", "in", "mei\u00b7nen", "ge\u00b7dan\u00b7cken", "ent\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Mit rosen auffgehn.", "tokens": ["Mit", "ro\u00b7sen", "auff\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Zwar ich betadle mein k\u00fchnes verlangen;", "tokens": ["Zwar", "ich", "be\u00b7tad\u00b7le", "mein", "k\u00fch\u00b7nes", "ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn ich bin gegen dir freylich zu schwach:", "tokens": ["Denn", "ich", "bin", "ge\u00b7gen", "dir", "frey\u00b7lich", "zu", "schwach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aber der purpur der lachenden wangen", "tokens": ["A\u00b7ber", "der", "pur\u00b7pur", "der", "la\u00b7chen\u00b7den", "wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Mehret mein winseln und seuffzendes ach/", "tokens": ["Meh\u00b7ret", "mein", "win\u00b7seln", "und", "seuff\u00b7zen\u00b7des", "ach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVINF", "KON", "XY", "XY", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Und zieht mich dir nach.", "tokens": ["Und", "zieht", "mich", "dir", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "St\u00fcrtzt nun dein schertz mein treues hertz/", "tokens": ["St\u00fcrtzt", "nun", "dein", "schertz", "mein", "treu\u00b7es", "hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie flammen die motten in t\u00f6dlichen schmertz;", "tokens": ["Wie", "flam\u00b7men", "die", "mot\u00b7ten", "in", "t\u00f6d\u00b7li\u00b7chen", "schmertz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wie hast du Melinde/", "tokens": ["Wie", "hast", "du", "Me\u00b7lin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Denn was ich empfinde/", "tokens": ["Denn", "was", "ich", "emp\u00b7fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Und was mich noch t\u00e4glich mit dornen verletzt/", "tokens": ["Und", "was", "mich", "noch", "t\u00e4g\u00b7lich", "mit", "dor\u00b7nen", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "APPR", "NN", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "So gringe gesch\u00e4tzt?", "tokens": ["So", "grin\u00b7ge", "ge\u00b7sch\u00e4tzt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Schau/ wie ich armer in thr\u00e4nen zerfliesse/", "tokens": ["Schau", "/", "wie", "ich", "ar\u00b7mer", "in", "thr\u00e4\u00b7nen", "zer\u00b7flies\u00b7se", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PPER", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wie mir mein hertze vor trauren verschmacht;", "tokens": ["Wie", "mir", "mein", "hert\u00b7ze", "vor", "trau\u00b7ren", "ver\u00b7schmacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weilen dein safftiger balsam der k\u00fcsse/", "tokens": ["Wei\u00b7len", "dein", "saff\u00b7ti\u00b7ger", "bal\u00b7sam", "der", "k\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Andern die lippen zu nectar-thau macht/", "tokens": ["An\u00b7dern", "die", "lip\u00b7pen", "zu", "nec\u00b7ta\u00b7r\u00b7thau", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ART", "NN", "APPR", "NE", "VVFIN", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Mich aber veracht.", "tokens": ["Mich", "a\u00b7ber", "ver\u00b7acht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Doch schmertz und leid wird mit der zeit", "tokens": ["Doch", "schmertz", "und", "leid", "wird", "mit", "der", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht anders als nebel durch sonnen zerstreut;", "tokens": ["Nicht", "an\u00b7ders", "als", "ne\u00b7bel", "durch", "son\u00b7nen", "zer\u00b7streut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ADV", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Drum hoff ich auff lachen", "tokens": ["Drum", "hoff", "ich", "auff", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Auch mitten im krachen/", "tokens": ["Auch", "mit\u00b7ten", "im", "kra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wie l\u00e4chzende muscheln in w\u00e4\u00dfriger grufft/", "tokens": ["Wie", "l\u00e4ch\u00b7zen\u00b7de", "mu\u00b7scheln", "in", "w\u00e4\u00df\u00b7ri\u00b7ger", "grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Auff morgen und lufft.", "tokens": ["Auff", "mor\u00b7gen", "und", "lufft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Nun ach! mein engel/ du kanst mich erretten;", "tokens": ["Nun", "ach", "!", "mein", "en\u00b7gel", "/", "du", "kanst", "mich", "er\u00b7ret\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "H\u00f6re doch endlich mein liebes-geschrey!", "tokens": ["H\u00f6\u00b7re", "doch", "end\u00b7lich", "mein", "lie\u00b7bes\u00b7ge\u00b7schrey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+++-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Trenne die stricke/ zermalme die ketten/", "tokens": ["Tren\u00b7ne", "die", "stri\u00b7cke", "/", "zer\u00b7mal\u00b7me", "die", "ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVFIN", "$(", "VVFIN", "ART", "ADJA", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Rei\u00df die beschwerlichen fessel entzwey/", "tokens": ["Rei\u00df", "die", "be\u00b7schwer\u00b7li\u00b7chen", "fes\u00b7sel", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Und mache mich frey.", "tokens": ["Und", "ma\u00b7che", "mich", "frey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Du hast gesiegt. Ich bin bekriegt.", "tokens": ["Du", "hast", "ge\u00b7siegt", ".", "Ich", "bin", "be\u00b7kriegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schau/ wie dir dein diener zun f\u00fcssen hier liegt.", "tokens": ["Schau", "/", "wie", "dir", "dein", "die\u00b7ner", "zun", "f\u00fcs\u00b7sen", "hier", "liegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PPER", "PPOSAT", "NN", "VVINF", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ach la\u00df dich des armen", "tokens": ["Ach", "la\u00df", "dich", "des", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Doch endlich erbarmen/", "tokens": ["Doch", "end\u00b7lich", "er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und k\u00fchle durch qvellen bezuckerter flut", "tokens": ["Und", "k\u00fch\u00b7le", "durch", "qvel\u00b7len", "be\u00b7zu\u00b7cker\u00b7ter", "flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die traurige glut.", "tokens": ["Die", "trau\u00b7ri\u00b7ge", "glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wilst du denn lebenslang eisern verbleiben?", "tokens": ["Wilst", "du", "denn", "le\u00b7bens\u00b7lang", "ei\u00b7sern", "ver\u00b7blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ADJD", "VVINF", "$."], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ist denn dein hertze von stahl oder stein?", "tokens": ["Ist", "denn", "dein", "hert\u00b7ze", "von", "stahl", "o\u00b7der", "stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "ADV", "KON", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Soll ich mich ewig zum sclaven verschreiben?", "tokens": ["Soll", "ich", "mich", "e\u00b7wig", "zum", "scla\u00b7ven", "ver\u00b7schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "APPRART", "ADJA", "VVINF", "$."], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Oder soll diese verbitterte pein", "tokens": ["O\u00b7der", "soll", "die\u00b7se", "ver\u00b7bit\u00b7ter\u00b7te", "pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PDAT", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Mein liebes-lohn seyn?", "tokens": ["Mein", "lie\u00b7bes\u00b7lohn", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ich liebe dich! du hassest mich!", "tokens": ["Ich", "lie\u00b7be", "dich", "!", "du", "has\u00b7sest", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch deine gedancken verfinsteren sich/", "tokens": ["Doch", "dei\u00b7ne", "ge\u00b7dan\u00b7cken", "ver\u00b7fins\u00b7te\u00b7ren", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und werden von fernen/", "tokens": ["Und", "wer\u00b7den", "von", "fer\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Noch endlich erlernen/", "tokens": ["Noch", "end\u00b7lich", "er\u00b7ler\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Da\u00df du mich unschuldig zum tode verdammt/", "tokens": ["Da\u00df", "du", "mich", "un\u00b7schul\u00b7dig", "zum", "to\u00b7de", "ve