{"dta.poem.3001": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Allgemeinen wird der Geist mir schwindeldumpf,", "tokens": ["Im", "All\u00b7ge\u00b7mei\u00b7nen", "wird", "der", "Geist", "mir", "schwin\u00b7del\u00b7dumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vorm Besondern gar ist jeder Sinn mir stumpf.", "tokens": ["Und", "vorm", "Be\u00b7son\u00b7dern", "gar", "ist", "je\u00b7der", "Sinn", "mir", "stumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VAFIN", "PIAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo bleibt ein Spielraum mir, von hier und dort vertrieben?", "tokens": ["Wo", "bleibt", "ein", "Spiel\u00b7raum", "mir", ",", "von", "hier", "und", "dort", "ver\u00b7trie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "$,", "APPR", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein artig Grenzgebiet in Mitten ist geblieben,", "tokens": ["Ein", "ar\u00b7tig", "Grenz\u00b7ge\u00b7biet", "in", "Mit\u00b7ten", "ist", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo Allgemeines im Besondern Farben spielt,", "tokens": ["Wo", "All\u00b7ge\u00b7mei\u00b7nes", "im", "Be\u00b7son\u00b7dern", "Far\u00b7ben", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein Besonderes auf's Allgemeine zielt.", "tokens": ["Und", "ein", "Be\u00b7son\u00b7de\u00b7res", "auf's", "All\u00b7ge\u00b7mei\u00b7ne", "zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}