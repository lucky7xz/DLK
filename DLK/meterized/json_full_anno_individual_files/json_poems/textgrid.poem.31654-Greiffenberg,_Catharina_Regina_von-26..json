{"textgrid.poem.31654": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das / was man von Gott soll sagen / fl\u00f6sset uns derselbig ein.", "tokens": ["Das", "/", "was", "man", "von", "Gott", "soll", "sa\u00b7gen", "/", "fl\u00f6s\u00b7set", "uns", "der\u00b7sel\u00b7big", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PIS", "APPR", "NN", "VMFIN", "VVINF", "$(", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Was den Himmel soll erheben / mu\u00df aus seinem Kunst-Schatz seyn.", "tokens": ["Was", "den", "Him\u00b7mel", "soll", "er\u00b7he\u00b7ben", "/", "mu\u00df", "aus", "sei\u00b7nem", "Kunst\u00b7Schatz", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$(", "VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Was zu seinen Ehren zielt / nimmt den Vrsprung her von oben.", "tokens": ["Was", "zu", "sei\u00b7nen", "Eh\u00b7ren", "zielt", "/", "nimmt", "den", "Vr\u00b7sprung", "her", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "ART", "NN", "ADV", "APPR", "ADV", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Dieses Liecht erleuchten mu\u00df / dessen Klarheit man soll loben.", "tokens": ["Die\u00b7ses", "Liecht", "er\u00b7leuch\u00b7ten", "mu\u00df", "/", "des\u00b7sen", "Klar\u00b7heit", "man", "soll", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "VMFIN", "$(", "PRELAT", "NN", "PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Drum mein alles guten Anfang / Ziel und End mein A und O!", "tokens": ["Drum", "mein", "al\u00b7les", "gu\u00b7ten", "An\u00b7fang", "/", "Ziel", "und", "End", "mein", "A", "und", "O", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "PIAT", "ADJA", "NN", "$(", "NN", "KON", "NN", "PPOSAT", "NE", "KON", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "flie\u00df und gie\u00df / bestrahl / erleuchte mich mit deiner Krafft also:", "tokens": ["flie\u00df", "und", "gie\u00df", "/", "be\u00b7strahl", "/", "er\u00b7leuch\u00b7te", "mich", "mit", "dei\u00b7ner", "Krafft", "al\u00b7so", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$(", "ADV", "$(", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "da\u00df ich von dir sing' und sag / dich erhebe / lob' und prei\u00dfe", "tokens": ["da\u00df", "ich", "von", "dir", "sing'", "und", "sag", "/", "dich", "er\u00b7he\u00b7be", "/", "lob'", "und", "prei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$(", "PPER", "VVFIN", "$(", "VVFIN", "KON", "NN"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"te