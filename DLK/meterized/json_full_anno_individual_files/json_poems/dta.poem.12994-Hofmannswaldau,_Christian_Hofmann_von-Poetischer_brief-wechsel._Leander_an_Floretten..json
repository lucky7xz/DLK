{"dta.poem.12994": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Poetischer brief-wechsel.  \n Leander an Floretten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich halte zwar mein wort, annehmliche Florette!", "tokens": ["Ich", "hal\u00b7te", "zwar", "mein", "wort", ",", "an\u00b7nehm\u00b7li\u00b7che", "Flo\u00b7ret\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es stellet sich mein brief in tieffer demuth ein:", "tokens": ["Es", "stel\u00b7let", "sich", "mein", "brief", "in", "tief\u00b7fer", "de\u00b7muth", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Allein die poesie liegt gleichsam an der kette,", "tokens": ["Al\u00b7lein", "die", "po\u00b7e\u00b7sie", "liegt", "gleich\u00b7sam", "an", "der", "ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die verse wollen mir nicht recht gehorsam seyn.", "tokens": ["Die", "ver\u00b7se", "wol\u00b7len", "mir", "nicht", "recht", "ge\u00b7hor\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das macht, ich leb\u2019 itzund an einem rauhen orte,", "tokens": ["Das", "macht", ",", "ich", "leb'", "it\u00b7zund", "an", "ei\u00b7nem", "rau\u00b7hen", "or\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo nichts als herbe flut aus tieffen brunnen steigt,", "tokens": ["Wo", "nichts", "als", "her\u00b7be", "flut", "aus", "tief\u00b7fen", "brun\u00b7nen", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KOKOM", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und kenne keine pracht, kein blumwerck netter worte,", "tokens": ["Und", "ken\u00b7ne", "kei\u00b7ne", "pracht", ",", "kein", "blum\u00b7werck", "net\u00b7ter", "wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das sich auf deinem mund als wie ein fr\u00fchling zeigt.", "tokens": ["Das", "sich", "auf", "dei\u00b7nem", "mund", "als", "wie", "ein", "fr\u00fch\u00b7ling", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPOSAT", "NN", "KOUS", "KOKOM", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indessen schreib ich doch, weil du es selbst befohlen.", "tokens": ["In\u00b7des\u00b7sen", "schreib", "ich", "doch", ",", "weil", "du", "es", "selbst", "be\u00b7foh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vielleichte wird mein geist durch deinen aufgeweckt.", "tokens": ["Viel\u00b7leich\u00b7te", "wird", "mein", "geist", "durch", "dei\u00b7nen", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer deine verse liest, der kan sich leicht erhohlen,", "tokens": ["Wer", "dei\u00b7ne", "ver\u00b7se", "liest", ",", "der", "kan", "sich", "leicht", "er\u00b7hoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil lauter kern und safft in solchen schalen steckt.", "tokens": ["Weil", "lau\u00b7ter", "kern", "und", "safft", "in", "sol\u00b7chen", "scha\u00b7len", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "VVFIN", "APPR", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und darum freu ich mich schon auf dein antwort-schreiben;", "tokens": ["Und", "da\u00b7rum", "freu", "ich", "mich", "schon", "auf", "dein", "ant\u00b7wor\u00b7tschrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wo diese freude nicht allzu verwegen ist.", "tokens": ["Wo", "die\u00b7se", "freu\u00b7de", "nicht", "all\u00b7zu", "ver\u00b7we\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "PTKNEG", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jedoch Florette l\u00e4st den hoffnungs-baum bekleiben,", "tokens": ["Je\u00b7doch", "Flo\u00b7ret\u00b7te", "l\u00e4st", "den", "hoff\u00b7nungs\u00b7baum", "be\u00b7klei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Damit Leander einst was auserlesnes liest.", "tokens": ["Da\u00b7mit", "Le\u00b7an\u00b7der", "einst", "was", "au\u00b7ser\u00b7les\u00b7nes", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "ADV", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn ist man gleich bi\u00dfher fast \u00fcberf\u00fcllt gewesen,", "tokens": ["Denn", "ist", "man", "gleich", "bi\u00df\u00b7her", "fast", "\u00fc\u00b7berf\u00b7\u00fcllt", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "ADV", "ADV", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nachdem ein ieder Mops, den dichter-krantz begehrt,", "tokens": ["Nach\u00b7dem", "ein", "ie\u00b7der", "Mops", ",", "den", "dich\u00b7ter\u00b7krantz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So ist der beste vers, den ich bi\u00dfher gelesen,", "tokens": ["So", "ist", "der", "bes\u00b7te", "vers", ",", "den", "ich", "bi\u00df\u00b7her", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.20": {"text": "Doch nicht einmal so viel, als dein geringster, werth:", "tokens": ["Doch", "nicht", "ein\u00b7mal", "so", "viel", ",", "als", "dein", "ge\u00b7rings\u00b7ter", ",", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wofern dein hoher kiel auch iemals was geringes", "tokens": ["Wo\u00b7fern", "dein", "ho\u00b7her", "kiel", "auch", "ie\u00b7mals", "was", "ge\u00b7rin\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "PWS", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf das papier gesetzt. Zum minsten ist dein geist", "tokens": ["Auf", "das", "pa\u00b7pier", "ge\u00b7setzt", ".", "Zum", "mins\u00b7ten", "ist", "dein", "geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "APPRART", "ADJA", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein abgesagter feind des abgeschmackten dinges,", "tokens": ["Ein", "ab\u00b7ge\u00b7sag\u00b7ter", "feind", "des", "ab\u00b7ge\u00b7schmack\u00b7ten", "din\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Womit der g\u00e4nse schwarm den albern p\u00f6vel speist.", "tokens": ["Wo\u00b7mit", "der", "g\u00e4n\u00b7se", "schwarm", "den", "al\u00b7bern", "p\u00f6\u00b7vel", "speist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJD", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was aber will ich viel an das geschnatter dencken,", "tokens": ["Was", "a\u00b7ber", "will", "ich", "viel", "an", "das", "ge\u00b7schnat\u00b7ter", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da ich ein s\u00fc\u00dfes lied von schwanen hoffen kan?", "tokens": ["Da", "ich", "ein", "s\u00fc\u00b7\u00dfes", "lied", "von", "schwa\u00b7nen", "hof\u00b7fen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn wen die Musen selbst aus ihren brunnen tr\u00e4ncken,", "tokens": ["Denn", "wen", "die", "Mu\u00b7sen", "selbst", "aus", "ih\u00b7ren", "brun\u00b7nen", "tr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der bringet allerdings nichts schlechtes auf die bahn.", "tokens": ["Der", "brin\u00b7get", "al\u00b7ler\u00b7dings", "nichts", "schlech\u00b7tes", "auf", "die", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIS", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Florett\u2019! ich zweiffle nicht, da\u00df dich die Musen lieben:", "tokens": ["Flo\u00b7rett'", "!", "ich", "zweiff\u00b7le", "nicht", ",", "da\u00df", "dich", "die", "Mu\u00b7sen", "lie\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich wei\u00df, was ich n\u00e4chsthin von deiner hand gesehn.", "tokens": ["Ich", "wei\u00df", ",", "was", "ich", "n\u00e4chst\u00b7hin", "von", "dei\u00b7ner", "hand", "ge\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn Bav\u2019 und Mevius dergleichen verse schrieben,", "tokens": ["Wenn", "Ba\u00b7v'", "und", "Me\u00b7vius", "derg\u00b7lei\u00b7chen", "ver\u00b7se", "schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NE", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So w\u00fcrde dem Parna\u00df ein gro\u00dfer dienst geschehn.", "tokens": ["So", "w\u00fcr\u00b7de", "dem", "Par\u00b7na\u00df", "ein", "gro\u00b7\u00dfer", "dienst", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und darum sch\u00e4m\u2019 ich mich, da\u00df ich nicht besser schreibe,", "tokens": ["Und", "da\u00b7rum", "sch\u00e4m'", "ich", "mich", ",", "da\u00df", "ich", "nicht", "bes\u00b7ser", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Indem mein schlechter brief nicht einen blick verdient,", "tokens": ["In\u00b7dem", "mein", "schlech\u00b7ter", "brief", "nicht", "ei\u00b7nen", "blick", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "VVFIN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df ich durchgehends nur ein leer geschw\u00e4tze treibe,", "tokens": ["Da\u00df", "ich", "durch\u00b7ge\u00b7hends", "nur", "ein", "leer", "ge\u00b7schw\u00e4t\u00b7ze", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Und als ein Phaeton mich allzu viel erk\u00fchnt.", "tokens": ["Und", "als", "ein", "Phae\u00b7ton", "mich", "all\u00b7zu", "viel", "er\u00b7k\u00fchnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Di\u00df eben dringt mich auch, so gut ich kan, zu schl\u00fcssen,", "tokens": ["Di\u00df", "e\u00b7ben", "dringt", "mich", "auch", ",", "so", "gut", "ich", "kan", ",", "zu", "schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Denn der, so \u00fcbel schreibt, schreibt allezeit zu viel.", "tokens": ["Denn", "der", ",", "so", "\u00fc\u00b7bel", "schreibt", ",", "schreibt", "al\u00b7le\u00b7zeit", "zu", "viel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "L\u00e4st aber doch Florett ein antwort-schreiben fliessen,", "tokens": ["L\u00e4st", "a\u00b7ber", "doch", "Flo\u00b7rett", "ein", "ant\u00b7wor\u00b7tschrei\u00b7ben", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So glaube, da\u00df ich es mit freuden k\u00fcssen will.", "tokens": ["So", "glau\u00b7be", ",", "da\u00df", "ich", "es", "mit", "freu\u00b7den", "k\u00fcs\u00b7sen", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}