{"dta.poem.12480": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Partei.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du dr\u00fcckst den Kranz auf eines Mannes Stirne,", "tokens": ["Du", "dr\u00fcckst", "den", "Kranz", "auf", "ei\u00b7nes", "Man\u00b7nes", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der wie ein Sch\u00e4cher j\u00fcngst sein Blut vergo\u00df,", "tokens": ["Der", "wie", "ein", "Sch\u00e4\u00b7cher", "j\u00fcngst", "sein", "Blut", "ver\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indessen hier die k\u00f6nigliche Dirne", "tokens": ["In\u00b7des\u00b7sen", "hier", "die", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Dir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die S\u00fcndenhefe ihrer Lust geno\u00df;", "tokens": ["Die", "S\u00fcn\u00b7den\u00b7he\u00b7fe", "ih\u00b7rer", "Lust", "ge\u00b7no\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich will ihm den Cypressenkranz gew\u00e4hren,", "tokens": ["Ich", "will", "ihm", "den", "Cyp\u00b7res\u00b7sen\u00b7kranz", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "D\u00fcngt auch sein Blut die Saat der Tyrannei \u2014", "tokens": ["D\u00fcngt", "auch", "sein", "Blut", "die", "Saat", "der", "Ty\u00b7ran\u00b7nei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Doch gegen ", "tokens": ["Doch", "ge\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Die noch die Mutter aller Siege war!", "tokens": ["Die", "noch", "die", "Mut\u00b7ter", "al\u00b7ler", "Sie\u00b7ge", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie mag ein Dichter solch ein Wort verfehmen,", "tokens": ["Wie", "mag", "ein", "Dich\u00b7ter", "solch", "ein", "Wort", "ver\u00b7feh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wort, das alles Herrliche gebar?", "tokens": ["Ein", "Wort", ",", "das", "al\u00b7les", "Herr\u00b7li\u00b7che", "ge\u00b7bar", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur offen wie ein Mann: F\u00fcr oder wider?", "tokens": ["Nur", "of\u00b7fen", "wie", "ein", "Mann", ":", "F\u00fcr", "o\u00b7der", "wi\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$.", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und die Parole: Sklave oder frei?", "tokens": ["Und", "die", "Pa\u00b7ro\u00b7le", ":", "Skla\u00b7ve", "o\u00b7der", "frei", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Selbst G\u00f6tter stiegen vom Olymp hernieder", "tokens": ["Selbst", "G\u00f6t\u00b7ter", "stie\u00b7gen", "vom", "O\u00b7lymp", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und k\u00e4mpften auf der Zinne der Partei!", "tokens": ["Und", "k\u00e4mpf\u00b7ten", "auf", "der", "Zin\u00b7ne", "der", "Par\u00b7tei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh hin! dein Volk will neue Bahnen wandeln,", "tokens": ["Sieh", "hin", "!", "dein", "Volk", "will", "neu\u00b7e", "Bah\u00b7nen", "wan\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "NN", "VMFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur des Signales harrt ein stattlich Heer;", "tokens": ["Nur", "des", "Sig\u00b7na\u00b7les", "harrt", "ein", "statt\u00b7lich", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die F\u00fcrsten tr\u00e4umen, la\u00dft die Dichter handeln!", "tokens": ["Die", "F\u00fcrs\u00b7ten", "tr\u00e4u\u00b7men", ",", "la\u00dft", "die", "Dich\u00b7ter", "han\u00b7deln", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Spielt Saul die Harfe, werfen ", "tokens": ["Spielt", "Saul", "die", "Har\u00b7fe", ",", "wer\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "NE", "ART", "NN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Panzer um \u2014 ge\u00f6ffnet sind die Schranken,", "tokens": ["Den", "Pan\u00b7zer", "um", "ge\u00b7\u00f6ff\u00b7net", "sind", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$(", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Brecht immer euer Saitenspiel entzwei,", "tokens": ["Brecht", "im\u00b7mer", "eu\u00b7er", "Sai\u00b7ten\u00b7spiel", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und f\u00fchrt ein F\u00e4hnlein ewiger Gedanken", "tokens": ["Und", "f\u00fchrt", "ein", "F\u00e4hn\u00b7lein", "e\u00b7wi\u00b7ger", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zur starken, stolzen Fahne der ", "tokens": ["Zur", "star\u00b7ken", ",", "stol\u00b7zen", "Fah\u00b7ne", "der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Gestern ist wie eine welke Blume \u2014", "tokens": ["Das", "Ge\u00b7stern", "ist", "wie", "ei\u00b7ne", "wel\u00b7ke", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Man legt sie wohl als Zeichen in ein Buch \u2014", "tokens": ["Man", "legt", "sie", "wohl", "als", "Zei\u00b7chen", "in", "ein", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "KOUS", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begrabt's mit seiner Schmach und seinem Ruhme", "tokens": ["Be\u00b7grabt's", "mit", "sei\u00b7ner", "Schmach", "und", "sei\u00b7nem", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und webt nicht l\u00e4nger an dem Leichentuch!", "tokens": ["Und", "webt", "nicht", "l\u00e4n\u00b7ger", "an", "dem", "Lei\u00b7chen\u00b7tuch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Leben gilt's ein Lebehoch zu singen,", "tokens": ["Dem", "Le\u00b7ben", "gilt's", "ein", "Le\u00b7be\u00b7hoch", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nicht ein Lied im Dienst der Schmeichelei;", "tokens": ["Und", "nicht", "ein", "Lied", "im", "Dienst", "der", "Schmei\u00b7che\u00b7lei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Menschheit gilt's ein Opfer darzubringen,", "tokens": ["Der", "Menschheit", "gilt's", "ein", "Op\u00b7fer", "dar\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Menschheit, auf dem Altar der ", "tokens": ["Der", "Menschheit", ",", "auf", "dem", "Al\u00b7tar", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O stellt sie ein die ungerechte Klage,", "tokens": ["O", "stellt", "sie", "ein", "die", "un\u00b7ge\u00b7rech\u00b7te", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihr die Angst so mancher Seele schaut;", "tokens": ["Wenn", "ihr", "die", "Angst", "so", "man\u00b7cher", "See\u00b7le", "schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ist das Bangen vor dem Hochzeittage,", "tokens": ["Es", "ist", "das", "Ban\u00b7gen", "vor", "dem", "Hoch\u00b7zeit\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das hoffnungsvolle Bangen einer Braut.", "tokens": ["Das", "hoff\u00b7nungs\u00b7vol\u00b7le", "Ban\u00b7gen", "ei\u00b7ner", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon dr\u00e4ngen aller Orten sich die Erben", "tokens": ["Schon", "dr\u00e4n\u00b7gen", "al\u00b7ler", "Or\u00b7ten", "sich", "die", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An's Krankenlager unsrer Zeit herbei;", "tokens": ["An's", "Kran\u00b7ken\u00b7la\u00b7ger", "uns\u00b7rer", "Zeit", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "La\u00dft, Dichter, la\u00dft auch ihr den Kranken sterben,", "tokens": ["La\u00dft", ",", "Dich\u00b7ter", ",", "la\u00dft", "auch", "ihr", "den", "Kran\u00b7ken", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "VVIMP", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr eures Volkes Zukunft nehmt ", "tokens": ["F\u00fcr", "eu\u00b7res", "Vol\u00b7kes", "Zu\u00b7kunft", "nehmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr m\u00fc\u00dft das Herz an Eine Karte wagen,", "tokens": ["Ihr", "m\u00fc\u00dft", "das", "Herz", "an", "Ei\u00b7ne", "Kar\u00b7te", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Ruhe \u00fcber Wolken ziemt euch nicht;", "tokens": ["Die", "Ru\u00b7he", "\u00fc\u00b7ber", "Wol\u00b7ken", "ziemt", "euch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr m\u00fc\u00dft euch mit in diesem Kampfe schlagen,", "tokens": ["Ihr", "m\u00fc\u00dft", "euch", "mit", "in", "die\u00b7sem", "Kamp\u00b7fe", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schwert in eurer Hand ist das Gedicht.", "tokens": ["Ein", "Schwert", "in", "eu\u00b7rer", "Hand", "ist", "das", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O w\u00e4hlt ein Banner, und ich bin zufrieden,", "tokens": ["O", "w\u00e4hlt", "ein", "Ban\u00b7ner", ",", "und", "ich", "bin", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ob's auch ein andres, denn das meine sei;", "tokens": ["Ob's", "auch", "ein", "and\u00b7res", ",", "denn", "das", "mei\u00b7ne", "sei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "PIS", "$,", "KON", "PDS", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich hab' gew\u00e4hlt, ich habe mich entschieden,", "tokens": ["Ich", "hab'", "ge\u00b7w\u00e4hlt", ",", "ich", "ha\u00b7be", "mich", "ent\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}