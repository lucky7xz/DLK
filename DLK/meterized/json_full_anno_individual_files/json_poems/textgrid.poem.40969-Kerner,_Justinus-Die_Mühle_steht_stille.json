{"textgrid.poem.40969": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die M\u00fchle steht stille", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Irrwing reitet nachts durchs Tal der M\u00fchle,", "tokens": ["Herr", "Irr\u00b7wing", "rei\u00b7tet", "nachts", "durchs", "Tal", "der", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Lichtstrahl folgt ihm und ein Windhauch k\u00fchle.", "tokens": ["Ein", "Licht\u00b7strahl", "folgt", "ihm", "und", "ein", "Wind\u00b7hauch", "k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herr Irrwing denkt: das ist des Mondes Licht;", "tokens": ["Herr", "Irr\u00b7wing", "denkt", ":", "das", "ist", "des", "Mon\u00b7des", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da haucht es hohl: \u00bbDer Mondstrahl redet nicht!\u00ab", "tokens": ["Da", "haucht", "es", "hohl", ":", "\u00bb", "Der", "Mond\u00b7strahl", "re\u00b7det", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "ART", "NN", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die M\u00fchle steht stille.", "tokens": ["Die", "M\u00fch\u00b7le", "steht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Herr Irrwing denkt: das ist des Baches T\u00f6nen!", "tokens": ["Herr", "Irr\u00b7wing", "denkt", ":", "das", "ist", "des", "Ba\u00b7ches", "T\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da haucht es hohl: \u00bbVom Bach aus Blut und Tr\u00e4nen!\u00ab", "tokens": ["Da", "haucht", "es", "hohl", ":", "\u00bb", "Vom", "Bach", "aus", "Blut", "und", "Tr\u00e4\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herr Irrwing spornt sein Ro\u00df zu schnellem Lauf,", "tokens": ["Herr", "Irr\u00b7wing", "spornt", "sein", "Ro\u00df", "zu", "schnel\u00b7lem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch pl\u00f6tzlich geht ihm innres Schauen auf.", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "geht", "ihm", "inn\u00b7res", "Schau\u00b7en", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die M\u00fchle steht stille.", "tokens": ["Die", "M\u00fch\u00b7le", "steht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbdas ist nicht Mondenstrahl, nicht Baches Wogen,", "tokens": ["\u00bb", "das", "ist", "nicht", "Mon\u00b7dens\u00b7trahl", ",", "nicht", "Ba\u00b7ches", "Wo\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gespenstig k\u00f6mmt ein Weib mir nachgeflogen,", "tokens": ["Ge\u00b7spens\u00b7tig", "k\u00f6mmt", "ein", "Weib", "mir", "nach\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Leichentuch getragen, bleich und wund,", "tokens": ["Vom", "Lei\u00b7chen\u00b7tuch", "ge\u00b7tra\u00b7gen", ",", "bleich", "und", "wund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein kalter Hauch entstr\u00f6met ihrem Mund.\u00ab", "tokens": ["Ein", "kal\u00b7ter", "Hauch", "ent\u00b7str\u00f6\u00b7met", "ih\u00b7rem", "Mund", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die M\u00fchle steht stille.", "tokens": ["Die", "M\u00fch\u00b7le", "steht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Herr Irrwing l\u00e4\u00dft dem scheuen Ro\u00df die Z\u00fcgel,", "tokens": ["Herr", "Irr\u00b7wing", "l\u00e4\u00dft", "dem", "scheu\u00b7en", "Ro\u00df", "die", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Geist doch auf des Leichentuches Fl\u00fcgel", "tokens": ["Der", "Geist", "doch", "auf", "des", "Lei\u00b7chen\u00b7tu\u00b7ches", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ereilt ihn bald und hauchet in die Luft:", "tokens": ["Er\u00b7eilt", "ihn", "bald", "und", "hau\u00b7chet", "in", "die", "Luft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbschnell wie kein Vogel fliegt ein Geist der Gruft.\u00ab", "tokens": ["\u00bb", "schnell", "wie", "kein", "Vo\u00b7gel", "fliegt", "ein", "Geist", "der", "Gruft", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "KOKOM", "PIAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die M\u00fchle steht stille.", "tokens": ["Die", "M\u00fch\u00b7le", "steht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wie Herr Irrwing schaut, sieht er gespalten", "tokens": ["Und", "wie", "Herr", "Irr\u00b7wing", "schaut", ",", "sieht", "er", "ge\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "NE", "VVFIN", "$,", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Geistes Haupt, er siehet in den kalten,", "tokens": ["Des", "Geis\u00b7tes", "Haupt", ",", "er", "sie\u00b7het", "in", "den", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gespenst'gen Sch\u00e4del, tief bis auf den Grund;", "tokens": ["Ge\u00b7spenst'\u00b7gen", "Sch\u00e4\u00b7del", ",", "tief", "bis", "auf", "den", "Grund", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da haucht also des Geistes kalter Mund:", "tokens": ["Da", "haucht", "al\u00b7so", "des", "Geis\u00b7tes", "kal\u00b7ter", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die M\u00fchle steht stille.", "tokens": ["Die", "M\u00fch\u00b7le", "steht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbschau' diese Spalte, draus entfloh mein Leben,", "tokens": ["\u00bb", "schau'", "die\u00b7se", "Spal\u00b7te", ",", "draus", "ent\u00b7floh", "mein", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "$,", "PAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sie hat mein Mann, John Mulling, mir gegeben,", "tokens": ["Sie", "hat", "mein", "Mann", ",", "John", "Mul\u00b7ling", ",", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "NE", "NE", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der M\u00fcller dort, den Sarg schlug selbst er zu", "tokens": ["Der", "M\u00fcl\u00b7ler", "dort", ",", "den", "Sarg", "schlug", "selbst", "er", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "$,", "ART", "NN", "VVFIN", "ADV", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sprach: \u203aEin Schlag gab ihr die ew'ge Ruh'!\u2039\u00ab", "tokens": ["Und", "sprach", ":", "\u203a", "Ein", "Schlag", "gab", "ihr", "die", "ew'\u00b7ge", "Ruh'", "!", "\u2039", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die M\u00fchle steht stille.", "tokens": ["Die", "M\u00fch\u00b7le", "steht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbnun irr' ich ungerochnes Weib als Schatte,", "tokens": ["\u00bb", "nun", "irr'", "ich", "un\u00b7ge\u00b7roch\u00b7nes", "Weib", "als", "Schat\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Johannens j\u00fcngern Leib umf\u00e4ngt mein Gatte,", "tokens": ["Jo\u00b7han\u00b7nens", "j\u00fcn\u00b7gern", "Leib", "um\u00b7f\u00e4ngt", "mein", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00fchle steht stille.", "tokens": ["Die", "M\u00fch\u00b7le", "steht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbdie schl\u00e4ft im Bette mein, hat all mein Habe,", "tokens": ["\u00bb", "die", "schl\u00e4ft", "im", "Bet\u00b7te", "mein", ",", "hat", "all", "mein", "Ha\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VVFIN", "APPRART", "NN", "PPOSAT", "$,", "VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hungrig mein Kn\u00e4blein weint auf meinem Grabe.", "tokens": ["Hung\u00b7rig", "mein", "Kn\u00e4\u00b7blein", "weint", "auf", "mei\u00b7nem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Herr Irrwing! da\u00df Ihr meinen Worten glaubt,", "tokens": ["Herr", "Irr\u00b7wing", "!", "da\u00df", "Ihr",