{"dta.poem.4003": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An unsere Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Des Ganges Ufer h\u00f6rten des Freudengotts", "tokens": ["Des", "Gan\u00b7ges", "U\u00b7fer", "h\u00f6r\u00b7ten", "des", "Freu\u00b7den\u00b7gotts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Triumph, als allerobernd vom Indus her", "tokens": ["Tri\u00b7umph", ",", "als", "al\u00b7le\u00b7ro\u00b7bernd", "vom", "In\u00b7dus", "her"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADJD", "APPRART", "NE", "APZR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der junge Bacchus kam, mit heil'gem", "tokens": ["Der", "jun\u00b7ge", "Bac\u00b7chus", "kam", ",", "mit", "heil'\u00b7gem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weine vom Schlafe die V\u00f6lker weckend.", "tokens": ["Wei\u00b7ne", "vom", "Schla\u00b7fe", "die", "V\u00f6l\u00b7ker", "we\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "O weckt, ihr Dichter! weckt sie vom Schlummer auf,", "tokens": ["O", "weckt", ",", "ihr", "Dich\u00b7ter", "!", "weckt", "sie", "vom", "Schlum\u00b7mer", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die jetzt noch schlafen, gebt die Gesetze, gebt", "tokens": ["Die", "jetzt", "noch", "schla\u00b7fen", ",", "gebt", "die", "Ge\u00b7set\u00b7ze", ",", "gebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "ADV", "VVINF", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Uns Leben, singt, Heroen! ihr nur", "tokens": ["Uns", "Le\u00b7ben", ",", "singt", ",", "He\u00b7roen", "!", "ihr", "nur"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "VVFIN", "$,", "NN", "$.", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Habt der Eroberung Recht, wie Bacchus.", "tokens": ["Habt", "der", "Er\u00b7o\u00b7be\u00b7rung", "Recht", ",", "wie", "Bac\u00b7chus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "PWAV", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}