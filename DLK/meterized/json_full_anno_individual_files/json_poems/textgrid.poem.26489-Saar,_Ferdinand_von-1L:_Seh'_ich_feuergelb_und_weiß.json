{"textgrid.poem.26489": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seh' ich feuergelb und wei\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich feuergelb und wei\u00df", "tokens": ["Seh'", "ich", "feu\u00b7er\u00b7gelb", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fccken euch des Sommers Fluren,", "tokens": ["Schm\u00fc\u00b7cken", "euch", "des", "Som\u00b7mers", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelt meine Seele leis'", "tokens": ["Wan\u00b7delt", "mei\u00b7ne", "See\u00b7le", "leis'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Sch\u00f6pfung heil'gen Spuren.", "tokens": ["Auf", "der", "Sch\u00f6p\u00b7fung", "heil'\u00b7gen", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn so wie in euch der Schnee,", "tokens": ["Denn", "so", "wie", "in", "euch", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanft erwarmend, sich gestaltet:", "tokens": ["Sanft", "er\u00b7war\u00b7mend", ",", "sich", "ge\u00b7stal\u00b7tet", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist in euch der Flamme Weh'", "tokens": ["Ist", "in", "euch", "der", "Flam\u00b7me", "Weh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch zur Blume still erkaltet.", "tokens": ["Auch", "zur", "Blu\u00b7me", "still", "er\u00b7kal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich ahn' in tiefster Brust,", "tokens": ["Und", "ich", "ahn'", "in", "tiefs\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Kr\u00e4fte sich durchdringen,", "tokens": ["Wie", "die", "Kr\u00e4f\u00b7te", "sich", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im All, hold unbewu\u00dft,", "tokens": ["Und", "im", "All", ",", "hold", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegens\u00e4tze sich bezwingen!", "tokens": ["Ge\u00b7gen\u00b7s\u00e4t\u00b7ze", "sich", "be\u00b7zwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seh' ich