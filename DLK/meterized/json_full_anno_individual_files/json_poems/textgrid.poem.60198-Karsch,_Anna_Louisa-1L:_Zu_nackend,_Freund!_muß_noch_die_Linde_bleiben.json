{"textgrid.poem.60198": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu nackend, Freund! mu\u00df noch die Linde bleiben", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu nackend, Freund! mu\u00df noch die Linde bleiben", "tokens": ["Zu", "na\u00b7ckend", ",", "Freund", "!", "mu\u00df", "noch", "die", "Lin\u00b7de", "blei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "NN", "$.", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ganz ihr gr\u00fcnes Kleid verlohr.", "tokens": ["Die", "ganz", "ihr", "gr\u00fc\u00b7nes", "Kleid", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rauh ist der Fr\u00fchlings-Tag. Die kleinen Wurzeln treiben", "tokens": ["Rauh", "ist", "der", "Fr\u00fch\u00b7lings\u00b7Tag", ".", "Die", "klei\u00b7nen", "Wur\u00b7zeln", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht junges Gra\u00df hervor.", "tokens": ["Nicht", "jun\u00b7ges", "Gra\u00df", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch lieblich ist der Lustgang an der Elbe", "tokens": ["Doch", "lieb\u00b7lich", "ist", "der", "Lust\u00b7gang", "an", "der", "El\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "ART", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf ihrer Oberfl\u00e4che schwimmt", "tokens": ["Auf", "ih\u00b7rer", "O\u00b7berf\u00b7l\u00e4\u00b7che", "schwimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne noch einmal, der an dem Luft-Gew\u00f6lbe", "tokens": ["Die", "Son\u00b7ne", "noch", "ein\u00b7mal", ",", "der", "an", "dem", "Luft\u00b7Ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott ihren Lauf bestimmt.", "tokens": ["Gott", "ih\u00b7ren", "Lauf", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr feyren bey dem ersten holden Blicke", "tokens": ["Ihr", "fey\u00b7ren", "bey", "dem", "ers\u00b7ten", "hol\u00b7den", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Fest, die Knaben mit dem Ball", "tokens": ["Ein", "Fest", ",", "die", "Kna\u00b7ben", "mit", "dem", "Ball"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nicht besorgt um Brod, und ihr zuk\u00fcnftig Gl\u00fccke", "tokens": ["Die", "nicht", "be\u00b7sorgt", "um", "Brod", ",", "und", "ihr", "zu\u00b7k\u00fcnf\u00b7tig", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADJD", "APPR", "NN", "$,", "KON", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Laut jauchzen auf dem Wall.", "tokens": ["Laut", "jauch\u00b7zen", "auf", "dem", "Wall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort st\u00fctzt ein Mann, die lahmgebliebne Rechte", "tokens": ["Dort", "st\u00fctzt", "ein", "Mann", ",", "die", "lahm\u00b7ge\u00b7blieb\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und krumme Schenkel, an ein Holz.", "tokens": ["Und", "krum\u00b7me", "Schen\u00b7kel", ",", "an", "ein", "Holz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schleicht und denkt sich noch das schreckliche Gefechte", "tokens": ["Er", "schleicht", "und", "denkt", "sich", "noch", "das", "schreck\u00b7li\u00b7che", "Ge\u00b7fech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist auf Narben stolz.", "tokens": ["Und", "ist", "auf", "Nar\u00b7ben", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Freund! ein Weib tr\u00e4gt voller Eymer Lasten;", "tokens": ["O", "Freund", "!", "ein", "Weib", "tr\u00e4gt", "vol\u00b7ler", "Ey\u00b7mer", "Las\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie steigt am Ufer auf, und keucht.", "tokens": ["Sie", "steigt", "am", "U\u00b7fer", "auf", ",", "und", "keucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich leb im Ueberflu\u00df, und ganze Tage fasten", "tokens": ["Ich", "leb", "im", "Ue\u00b7berf\u00b7lu\u00df", ",", "und", "gan\u00b7ze", "Ta\u00b7ge", "fas\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df sie; und ach! vielleicht", "tokens": ["Mu\u00df", "sie", ";", "und", "ach", "!", "viel\u00b7leicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$.", "KON", "XY", "$.", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Fiel in der Schlacht ihr bester Freund, und Kinder", "tokens": ["Fiel", "in", "der", "Schlacht", "ihr", "bes\u00b7ter", "Freund", ",", "und", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein traurig Denkmahl! lie\u00df er hier!", "tok