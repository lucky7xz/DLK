{"textgrid.poem.42034": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die Katzen", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sind sehr k\u00fchl und biegsam, wenn sie schreiten,", "tokens": ["Sie", "sind", "sehr", "k\u00fchl", "und", "bieg\u00b7sam", ",", "wenn", "sie", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihre Leiber flie\u00dfen sanft entlang.", "tokens": ["Und", "ih\u00b7re", "Lei\u00b7ber", "flie\u00b7\u00dfen", "sanft", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sie die blumenhaften F\u00fc\u00dfe breiten,", "tokens": ["Wenn", "sie", "die", "blu\u00b7men\u00b7haf\u00b7ten", "F\u00fc\u00b7\u00dfe", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schmiegt sich die Erde ihrem runden Gang.", "tokens": ["Schmiegt", "sich", "die", "Er\u00b7de", "ih\u00b7rem", "run\u00b7den", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr Blick ist demuthaft und manchmal etwas irr.", "tokens": ["Ihr", "Blick", "ist", "de\u00b7mut\u00b7haft", "und", "manch\u00b7mal", "et\u