{"dta.poem.11000": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4dliche Verabs\u00e4umung,  \n Gottes Wunder auf der Welt zu be-  \n trachten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen, die in dieser Welt, und in der gegenw\u00e4rt-\ngen Zeit,", "tokens": ["Die", "Men\u00b7schen", ",", "die", "in", "die\u00b7ser", "Welt", ",", "und", "in", "der", "ge\u00b7gen\u00b7w\u00e4r\u00b7t", "gen", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "$,", "KON", "APPR", "ART", "TRUNC", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Nicht ihres Sch\u00f6pfers Weisheit, Macht, samt seiner Vater-", "tokens": ["Nicht", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "Macht", ",", "samt", "sei\u00b7ner", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die, mit ihrem Geist, allein in einer k\u00fcnftgen Ewig-", "tokens": ["Und", "die", ",", "mit", "ih\u00b7rem", "Geist", ",", "al\u00b7lein", "in", "ei\u00b7ner", "k\u00fcnft\u00b7gen", "E\u00b7wig"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Jhn, als die Weisheit, Macht und Liebe, nur gleichsam in", "tokens": ["Jhn", ",", "als", "die", "Weis\u00b7heit", ",", "Macht", "und", "Lie\u00b7be", ",", "nur", "gleich\u00b7sam", "in"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "ADJD", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die scheinen, (da sie blind und taub, f\u00fcr Gottes gegenw\u00e4rt-", "tokens": ["Die", "schei\u00b7nen", ",", "(", "da", "sie", "blind", "und", "taub", ",", "f\u00fcr", "Got\u00b7tes", "ge\u00b7gen\u00b7w\u00e4r\u00b7t"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "$(", "KOUS", "PPER", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Fast keinen gegenw\u00e4rtigen, nur einen k\u00fcnftgen Gott zu haben.", "tokens": ["Fast", "kei\u00b7nen", "ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gen", ",", "nur", "ei\u00b7nen", "k\u00fcnft\u00b7gen", "Gott", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "$,", "ADV", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}