{"textgrid.poem.65047": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du willst mich tr\u00f6sten? La\u00df, o la\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst mich tr\u00f6sten? La\u00df, o la\u00df,", "tokens": ["Du", "willst", "mich", "tr\u00f6s\u00b7ten", "?", "La\u00df", ",", "o", "la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$.", "VVIMP", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was zwingst du dich zu falschem Scheine?", "tokens": ["Was", "zwingst", "du", "dich", "zu", "fal\u00b7schem", "Schei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Angesicht, ist es nicht bla\u00df,", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", ",", "ist", "es", "nicht", "bla\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Aug' nicht d\u00fcster wie das meine?", "tokens": ["Dein", "Aug'", "nicht", "d\u00fcs\u00b7ter", "wie", "das", "mei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADJD", "KOKOM", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was sagst du mir, wie sch\u00f6n es sei", "tokens": ["Was", "sagst", "du", "mir", ",", "wie", "sch\u00f6n", "es", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PWAV", "ADJD", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem Geschicke k\u00fchn zu ringen?", "tokens": ["Mit", "dem", "Ge\u00b7schi\u00b7cke", "k\u00fchn", "zu", "rin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch deine Worte h\u00f6r' den Schrei", "tokens": ["Durch", "dei\u00b7ne", "Wor\u00b7te", "h\u00f6r'", "den", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich deines eignen Jammers dringen!", "tokens": ["Ich", "dei\u00b7nes", "eig\u00b7nen", "Jam\u00b7mers", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, deines Auges verl\u00f6schend Licht,", "tokens": ["Ach", ",", "dei\u00b7nes", "Au\u00b7ges", "ver\u00b7l\u00f6\u00b7schend", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VVPP", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der stille Gram in deinen Z\u00fcgen,", "tokens": ["Der", "stil\u00b7le", "Gram", "in", "dei\u00b7nen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja selbst dein L\u00e4cheln widerspricht", "tokens": ["Ja", "selbst", "dein", "L\u00e4\u00b7cheln", "wi\u00b7der\u00b7spricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All' deinen frommgemeinten L\u00fcgen.", "tokens": ["All'", "dei\u00b7nen", "fromm\u00b7ge\u00b7mein\u00b7ten", "L\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geh! deinem Wesen hat der Feind,", "tokens": ["Geh", "!", "dei\u00b7nem", "We\u00b7sen", "hat", "der", "Feind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den zu bek\u00e4mpfen du dich r\u00fcstest,", "tokens": ["Den", "zu", "be\u00b7k\u00e4mp\u00b7fen", "du", "dich", "r\u00fcs\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Schmerz, sich allzu tief geeint,", "tokens": ["Der", "Schmerz", ",", "sich", "all\u00b7zu", "tief", "ge\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als da\u00df du ihn zu bannen w\u00fc\u00dftest.", "tokens": ["Als", "da\u00df", "du", "ihn", "zu", "ban\u00b7nen", "w\u00fc\u00df\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer ihn besiegen soll, der darf", "tokens": ["Wer", "ihn", "be\u00b7sie\u00b7gen", "soll", ",", "der", "darf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "PRELS", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des finstern Gegners Macht nicht kennen,", "tokens": ["Des", "fins\u00b7tern", "Geg\u00b7ners", "Macht", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem d\u00fcrfen Thr\u00e4nen \u00e4tzend scharf", "tokens": ["Dem", "d\u00fcr\u00b7fen", "Thr\u00e4\u00b7nen", "\u00e4t\u00b7zend", "scharf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht auf der eignen Wange brennen,", "tokens": ["Nicht", "auf", "der", "eig\u00b7nen", "Wan\u00b7ge", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der darf in seiner eignen Brust", "tokens": ["Der", "darf", "in", "sei\u00b7ner", "eig\u00b7nen", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Schaudern nicht empfunden haben,", "tokens": ["Mit", "Schau\u00b7dern", "nicht", "emp\u00b7fun\u00b7den", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was es bedeuten will, die Lust,", "tokens": ["Was", "es", "be\u00b7deu\u00b7ten", "will", ",", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Gl\u00fcck des Lebens zu begraben.", "tokens": ["Das", "Gl\u00fcck", "des", "Le\u00b7bens", "zu", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der beste Tr\u00f6ster ist ein Kind!", "tokens": ["Der", "bes\u00b7te", "Tr\u00f6s\u00b7ter", "ist", "ein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Kummers bittrer Quell versieget,", "tokens": ["Des", "Kum\u00b7mers", "bit\u00b7trer", "Quell", "ver\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn es mit sonn'gem L\u00e4cheln lind", "tokens": ["Wenn", "es", "mit", "sonn'\u00b7gem", "L\u00e4\u00b7cheln", "lind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An unsre Brust sich harmlos schmieget.", "tokens": ["An", "uns\u00b7re", "Brust", "sich", "harm\u00b7los", "schmie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein sanfter Sph\u00e4renton durchdringt", "tokens": ["Ein", "sanf\u00b7ter", "Sph\u00e4\u00b7ren\u00b7ton", "durch\u00b7dringt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es unsrer Qualen Schlachtget\u00fcmmel,", "tokens": ["Es", "uns\u00b7rer", "Qua\u00b7len", "Schlacht\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein freudig Morgenroth entspringt", "tokens": ["Ein", "freu\u00b7dig", "Mor\u00b7gen\u00b7roth", "ent\u00b7springt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es hell an unserm n\u00e4cht'gen Himmel!", "tokens": ["Es", "hell", "an", "un\u00b7serm", "n\u00e4cht'\u00b7gen", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sein Wort erquickt und labt und kos't", "tokens": ["Sein", "Wort", "er\u00b7quickt", "und", "labt", "und", "kos't"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Jubellied aus Rosenlauben,", "tokens": ["Ein", "Ju\u00b7bel\u00b7lied", "aus", "Ro\u00b7sen\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und macht den Geist, trotz Sturm und Frost,", "tokens": ["Und", "macht", "den", "Geist", ",", "trotz", "Sturm", "und", "Frost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An einen ew'gen Fr\u00fchling glauben.", "tokens": ["An", "ei\u00b7nen", "ew'\u00b7gen", "Fr\u00fch\u00b7ling", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es h\u00e4uft nicht eifrig Schlu\u00df auf Schlu\u00df", "tokens": ["Es", "h\u00e4uft", "nicht", "eif\u00b7rig", "Schlu\u00df", "auf", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es qu\u00e4lt dich nicht mit eitlen Gr\u00fcnden,", "tokens": ["Es", "qu\u00e4lt", "dich", "nicht", "mit", "eit\u00b7len", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit einem Blick, mit einem Ku\u00df", "tokens": ["Mit", "ei\u00b7nem", "Blick", ",", "mit", "ei\u00b7nem", "Ku\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Spricht es dich frei von Schmerz und S\u00fcnden!", "tokens": ["Spricht", "es", "dich", "frei", "von", "Schmerz", "und", "S\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "So sieht der blitzgetroffne Baum", "tokens": ["So", "sieht", "der", "blitz\u00b7ge\u00b7troff\u00b7ne", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An seinem Stamme Blumenranken", "tokens": ["An", "sei\u00b7nem", "Stam\u00b7me", "Blu\u00b7men\u00b7ran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich einem holden Elfentraum", "tokens": ["Gleich", "ei\u00b7nem", "hol\u00b7den", "El\u00b7fen\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Hauch des Fr\u00fchlings duftig schwanken.", "tokens": ["Im", "Hauch", "des", "Fr\u00fch\u00b7lings", "duf\u00b7tig", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, wie vergi\u00dft er da so gern", "tokens": ["O", ",", "wie", "ver\u00b7gi\u00dft", "er", "da", "so", "gern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein eigen Welken und Zersplittern,", "tokens": ["Sein", "ei\u00b7gen", "Wel\u00b7ken", "und", "Zer\u00b7split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sieht er in ihres Kelches Stern", "tokens": ["Sieht", "er", "in", "ih\u00b7res", "Kel\u00b7ches", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Morgenthau als Perle zittern! \u2013", "tokens": ["Den", "Mor\u00b7gent\u00b7hau", "als", "Per\u00b7le", "zit\u00b7tern", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du f\u00fchlst dich innerlich erhellt,", "tokens": ["Du", "f\u00fchlst", "dich", "in\u00b7ner\u00b7lich", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Tempel wird des Schmerzens Zelle,", "tokens": ["Zum", "Tem\u00b7pel", "wird", "des", "Schmer\u00b7zens", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kind setzt seine neue Welt", "tokens": ["Das", "Kind", "setzt", "sei\u00b7ne", "neu\u00b7e", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deiner eingest\u00fcrzten Stelle!", "tokens": ["An", "dei\u00b7ner", "ein\u00b7ge\u00b7st\u00fcrz\u00b7ten", "Stel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ist auch diese Welt nicht dein", "tokens": ["Und", "ist", "auch", "die\u00b7se", "Welt", "nicht", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "PDAT", "NN", "PTKNEG", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihrem wolkenlosen Gl\u00fccke,", "tokens": ["Mit", "ih\u00b7rem", "wol\u00b7ken\u00b7lo\u00b7sen", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct