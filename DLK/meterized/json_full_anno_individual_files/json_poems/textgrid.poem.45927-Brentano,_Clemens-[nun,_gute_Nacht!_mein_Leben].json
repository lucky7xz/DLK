{"textgrid.poem.45927": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[nun, gute Nacht! mein Leben]", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun, gute Nacht! mein Leben,", "tokens": ["Nun", ",", "gu\u00b7te", "Nacht", "!", "mein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du alter, treuer Rhein,", "tokens": ["Du", "al\u00b7ter", ",", "treu\u00b7er", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Deine Wellen schweben", "tokens": ["Dei\u00b7ne", "Wel\u00b7len", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schon im klaren Sternenschein;", "tokens": ["Schon", "im", "kla\u00b7ren", "Ster\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Welt ist rings entschlafen,", "tokens": ["Die", "Welt", "ist", "rings", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es singt den Wolkenschafen", "tokens": ["Es", "singt", "den", "Wol\u00b7ken\u00b7scha\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Mond ein Lied.", "tokens": ["Der", "Mond", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Schiffer schl\u00e4ft im Nachen", "tokens": ["Der", "Schif\u00b7fer", "schl\u00e4ft", "im", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und tr\u00e4umet von dem Meer,", "tokens": ["Und", "tr\u00e4u\u00b7met", "von", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du aber, du mu\u00dft wachen", "tokens": ["Du", "a\u00b7ber", ",", "du", "mu\u00dft", "wa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00e4gst das Schiff einher.", "tokens": ["Und", "tr\u00e4gst", "das", "Schiff", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du f\u00fchrst ein freies Leben,", "tokens": ["Du", "f\u00fchrst", "ein", "frei\u00b7es", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durchtanzest bei den Reben", "tokens": ["Durch\u00b7tan\u00b7zest", "bei", "den", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die ernste Nacht.", "tokens": ["Die", "erns\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wer dich gesehn, lernt lachen;", "tokens": ["Wer", "dich", "ge\u00b7sehn", ",", "lernt", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bist so freudenreich,", "tokens": ["Du", "bist", "so", "freu\u00b7den\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du labst das Herz der Schwachen", "tokens": ["Du", "labst", "das", "Herz", "der", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und machst den Armen reich,", "tokens": ["Und", "machst", "den", "Ar\u00b7men", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du spiegelst hohe Schl\u00f6sser,", "tokens": ["Du", "spie\u00b7gelst", "ho\u00b7he", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und f\u00fcllest gro\u00dfe F\u00e4sser", "tokens": ["Und", "f\u00fcl\u00b7lest", "gro\u00b7\u00dfe", "F\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit edlem Wein.", "tokens": ["Mit", "ed\u00b7lem", "Wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Auch manchen lehrst du weinen,", "tokens": ["Auch", "man\u00b7chen", "lehrst", "du", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem du sein Lieb entf\u00fchrt,", "tokens": ["Dem", "du", "sein", "Lieb", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wolle die vereinen,", "tokens": ["Gott", "wol\u00b7le", "die", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die solche Sehnsucht r\u00fchrt.", "tokens": ["Die", "sol\u00b7che", "Sehn\u00b7sucht", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie irren in den Hainen", "tokens": ["Sie", "ir\u00b7ren", "in", "den", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und von den Echosteinen", "tokens": ["Und", "von", "den", "E\u00b7chos\u00b7tei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erschallt ihr Weh.", "tokens": ["Er\u00b7schallt", "ihr", "Weh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und manchen lehret beten", "tokens": ["Und", "man\u00b7chen", "leh\u00b7ret", "be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein tiefer Felsengrund,", "tokens": ["Dein", "tie\u00b7fer", "Fel\u00b7sen\u00b7grund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer dich in Zorn betreten,", "tokens": ["Wer", "dich", "in", "Zorn", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ziehst du in den Schlund.", "tokens": ["Den", "ziehst", "du", "in", "den", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo deine Strudel brausen,", "tokens": ["Wo", "dei\u00b7ne", "Stru\u00b7del", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo deine Wirbel sausen,", "tokens": ["Wo", "dei\u00b7ne", "Wir\u00b7bel", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da beten sie.", "tokens": ["Da", "be\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Mich aber lehrst du singen,", "tokens": ["Mich", "a\u00b7ber", "lehrst", "du", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn dich mein Aug' ersieht,", "tokens": ["Wenn", "dich", "mein", "Aug'", "er\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein freudenselig Klingen", "tokens": ["Ein", "freu\u00b7den\u00b7se\u00b7lig", "Klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir durch den Busen zieht.", "tokens": ["Mir", "durch", "den", "Bu\u00b7sen", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Treib fromm nur meine M\u00fchle,", "tokens": ["Treib", "fromm", "nur", "mei\u00b7ne", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jetzt scheid' ich in der K\u00fchle", "tokens": ["Jetzt", "scheid'", "ich", "in", "der", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und schlummre ein.", "tokens": ["Und", "schlumm\u00b7re", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ihr lieben Sterne decket", "tokens": ["Ihr", "lie\u00b7ben", "Ster\u00b7ne", "de\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir meinen Vater zu.", "tokens": ["Mir", "mei\u00b7nen", "Va\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis mich die Sonne wecket,", "tokens": ["Bis", "mich", "die", "Son\u00b7ne", "we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis dahin mahle du.", "tokens": ["Bis", "da\u00b7hin", "mah\u00b7le", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wird's gut, will ich dich preisen,", "tokens": ["Wird's", "gut", ",", "will", "ich", "dich", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VMFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dann sing' in h\u00f6hern Weisen", "tokens": ["Dann", "sing'", "in", "h\u00f6\u00b7hern", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich dir ein Lied.", "tokens": ["Ich", "dir", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Nun werf' ich dir zum Spiele", "tokens": ["Nun", "wer\u00b7f'", "ich", "dir", "zum", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Kranz in deine Flut,", "tokens": ["Den", "Kranz", "in", "dei\u00b7ne", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trag' ihn zu seinem Ziele,", "tokens": ["Trag'", "ihn", "zu", "sei\u00b7nem", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo dieser Tag auch ruht.", "tokens": ["Wo", "die\u00b7ser", "Tag", "auch", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und nun mu\u00df ich mich wenden", "tokens": ["Und", "nun", "mu\u00df", "ich", "mich", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und segnend dich vollenden", "tokens": ["Und", "seg\u00b7nend", "dich", "voll\u00b7en\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Abendsang.", "tokens": ["Den", "A\u00b7bend\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Nun, gute Nacht! mein Leben,", "tokens": ["Nun", ",", "gu\u00b7te", "Nacht", "!", "mein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du alter, treuer Rhein,", "tokens": ["Du", "al\u00b7ter", ",", "treu\u00b7er", "Rhein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure