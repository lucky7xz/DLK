{"dta.poem.181": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er bittet sie zu k\u00fcssen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Darf ich den Perlen-Thau von euren Lippen nehmen?", "tokens": ["Darf", "ich", "den", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "von", "eu\u00b7ren", "Lip\u00b7pen", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Brunette, die ihr mehr als alle Sch\u00f6nen prahlt/", "tokens": ["Bru\u00b7net\u00b7te", ",", "die", "ihr", "mehr", "als", "al\u00b7le", "Sch\u00f6\u00b7nen", "prahlt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PIAT", "KOKOM", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn eurer Augen-Blitz was g\u00f6ttlichs von sich strahlt;", "tokens": ["Wenn", "eu\u00b7rer", "Au\u00b7gen\u00b7Blitz", "was", "g\u00f6tt\u00b7lichs", "von", "sich", "strahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PWS", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Wangen Purpur kan das Schnecken-Blut besch\u00e4men.", "tokens": ["Der", "Wan\u00b7gen", "Pur\u00b7pur", "kan", "das", "Schne\u00b7cken\u00b7Blut", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach darf mein brauner Mund das sch\u00f6ne Feld bes\u00e4men/", "tokens": ["Ach", "darf", "mein", "brau\u00b7ner", "Mund", "das", "sch\u00f6\u00b7ne", "Feld", "be\u00b7s\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo die Natur den Mund so angenehm bemahlt/", "tokens": ["Wo", "die", "Na\u00b7tur", "den", "Mund", "so", "an\u00b7ge\u00b7nehm", "be\u00b7mahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo ein ", "tokens": ["Wo", "ein"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Ich will mich gern dazu/ wenn ihr nur wolt/ bequehmen.", "tokens": ["Ich", "will", "mich", "gern", "da\u00b7zu", "/", "wenn", "ihr", "nur", "wolt", "/", "be\u00b7queh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PAV", "$(", "KOUS", "PPER", "ADV", "VMFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Brunette gebt es zu/ verg\u00f6nnt mir einen Ku\u00df/", "tokens": ["Bru\u00b7net\u00b7te", "gebt", "es", "zu", "/", "ver\u00b7g\u00f6nnt", "mir", "ei\u00b7nen", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "$(", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Euch ist der Lippen-Schmuck umsonst nicht zugestellet/", "tokens": ["Euch", "ist", "der", "Lip\u00b7pen\u00b7Schmuck", "um\u00b7sonst", "nicht", "zu\u00b7ge\u00b7stel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Selbst die Natur will/ da\u00df man sich dahin gesellet.", "tokens": ["Selbst", "die", "Na\u00b7tur", "will", "/", "da\u00df", "man", "sich", "da\u00b7hin", "ge\u00b7sel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "$(", "KOUS", "PIS", "PRF", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vergebens quillt da nicht der s\u00fcsse ", "tokens": ["Ver\u00b7ge\u00b7bens", "quillt", "da", "nicht", "der", "s\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die Liebe hei\u00dft es uns den sch\u00f6nen Ort zu suchen/", "tokens": ["Die", "Lie\u00b7be", "hei\u00dft", "es", "uns", "den", "sch\u00f6\u00b7nen", "Ort", "zu", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So kan die Keuschheit selbst dis Werck auch nicht verfluchen.", "tokens": ["So", "kan", "die", "Keuschheit", "selbst", "dis", "Werck", "auch", "nicht", "ver\u00b7flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PDS", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+----+-+-+-", "measure": "dactylic.init"}}}}}