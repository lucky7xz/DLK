{"textgrid.poem.46243": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Venedig gegen seiner liebsten verglichen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Witzlos war der f\u00fcrwitz, aufs\u00e4tzig der f\u00fcrsatz", "tokens": ["Witz\u00b7los", "war", "der", "f\u00fcr\u00b7witz", ",", "auf\u00b7s\u00e4t\u00b7zig", "der", "f\u00fcr\u00b7satz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADJD", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "kreuzgeizig der ehrgeiz, die mich so sehr beth\u00f6ret,", "tokens": ["kreuz\u00b7gei\u00b7zig", "der", "ehr\u00b7geiz", ",", "die", "mich", "so", "sehr", "be\u00b7th\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df eines f\u00fcrstens will (der sch\u00f6n und lieb gesatz", "tokens": ["da\u00df", "ei\u00b7nes", "f\u00fcrs\u00b7tens", "will", "(", "der", "sch\u00f6n", "und", "lieb", "ge\u00b7satz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "$(", "ART", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "zuwider) mich jetz ihm gehorsamen gelehret.", "tokens": ["zu\u00b7wi\u00b7der", ")", "mich", "jetz", "ihm", "ge\u00b7hor\u00b7sa\u00b7men", "ge\u00b7leh\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PPER", "ADV", "PPER", "VVPP", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Dan was seind doch die Brent, galleen, Marxenplatz,", "tokens": ["Dan", "was", "seind", "doch", "die", "Brent", ",", "gal\u00b7le\u00b7en", ",", "Mar\u00b7xen\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "ADV", "ART", "NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "die statliche pall\u00e4st, der schatz so weit vermehret,", "tokens": ["die", "stat\u00b7li\u00b7che", "pal\u00b7l\u00e4st", ",", "der", "schatz", "so", "weit", "ver\u00b7meh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "gegen der haaren strom von purem gold bewehret,", "tokens": ["ge\u00b7gen", "der", "haa\u00b7ren", "strom", "von", "pu\u00b7rem", "gold", "be\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und gegen der sch\u00f6nheit und tugend gr\u00f6\u00dferm schatz?", "tokens": ["und", "ge\u00b7gen", "der", "sch\u00f6n\u00b7heit", "und", "tu\u00b7gend", "gr\u00f6\u00b7\u00dferm", "schatz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Was ist des herzogs, rats, der curtisanen prangen", "tokens": ["Was", "ist", "des", "her\u00b7zogs", ",", "rats", ",", "der", "cur\u00b7ti\u00b7sa\u00b7nen", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "in purpur, scharlach, gold, in bestem saal und mal", "tokens": ["in", "pur\u00b7pur", ",", "schar\u00b7lach", ",", "gold", ",", "in", "bes\u00b7tem", "saal", "und", "mal"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "$,", "VVFIN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "verglichen mit dem schmuck der lippen und der wangen?", "tokens": ["ver\u00b7gli\u00b7chen", "mit", "dem", "schmuck", "der", "lip\u00b7pen", "und", "der", "wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was seind die m\u00fcnz, zeughaus, gesch\u00fctz und arsenal", "tokens": ["Was", "seind", "die", "m\u00fcnz", ",", "zeug\u00b7haus", ",", "ge\u00b7sch\u00fctz", "und", "ar\u00b7se\u00b7nal"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PTKVZ", "$,", "ADJD", "KON", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "gegen dem sch\u00f6nen aug, das billich, mein verlangen", "tokens": ["ge\u00b7gen", "dem", "sch\u00f6\u00b7nen", "aug", ",", "das", "bil\u00b7lich", ",", "mein", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,", "P