{"dta.poem.1259": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fcrcht/ es ist zu hoch: Doch besser hochgestiegen/", "tokens": ["Ich", "f\u00fcrcht", "/", "es", "ist", "zu", "hoch", ":", "Doch", "bes\u00b7ser", "hoch\u00b7ges\u00b7tie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "PTKA", "ADJD", "$.", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als unversucht zur Erde liegen.", "tokens": ["Als", "un\u00b7ver\u00b7sucht", "zur", "Er\u00b7de", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein hochgethaner Fall weist doch ein k\u00fchnes Wagen.", "tokens": ["Ein", "hoch\u00b7ge\u00b7tha\u00b7ner", "Fall", "weist", "doch", "ein", "k\u00fch\u00b7nes", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Manch Vorsatz mu\u00df zur\u00fccke schlagen.", "tokens": ["Manch", "Vor\u00b7satz", "mu\u00df", "zu\u00b7r\u00fc\u00b7cke", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Gl\u00fccke st\u00f6st dem/ der es sucht/ zu handen:", "tokens": ["Das", "Gl\u00fc\u00b7cke", "st\u00f6st", "dem", "/", "der", "es", "sucht", "/", "zu", "han\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "$(", "PRELS", "PPER", "VVFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wer ihm nicht traut/ wird ohne Ruhm zu Schanden.", "tokens": ["Wer", "ihm", "nicht", "traut", "/", "wird", "oh\u00b7ne", "Ruhm", "zu", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$(", "VAFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}