{"dta.poem.7034": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Gedancken \u00fcber  \n die Schmetterlinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan auch ein menschlicher Verstand,", "tokens": ["Kan", "auch", "ein", "menschli\u00b7cher", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Fiel mir hier\u00fcber ein, dies Wunderwerck begreiffen!", "tokens": ["Fiel", "mir", "hier\u00b7\u00fc\u00b7ber", "ein", ",", "dies", "Wun\u00b7der\u00b7werck", "be\u00b7greif\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "PTKVZ", "$,", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Kunstwerck dieser kleinen Pfeiffen", "tokens": ["Das", "Kunst\u00b7werck", "die\u00b7ser", "klei\u00b7nen", "Pfeif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwirret die Vernunfft.", "tokens": ["Ver\u00b7wir\u00b7ret", "die", "Ver\u00b7nunfft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}