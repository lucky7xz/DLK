{"dta.poem.5334": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ungl\u00fcck im Gl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie viel tausend Umst\u00e4nd', Ordnung und Bem\u00fchung", "tokens": ["Wie", "viel", "tau\u00b7send", "Um\u00b7st\u00e4nd'", ",", "Ord\u00b7nung", "und", "Be\u00b7m\u00fc\u00b7hung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "CARD", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "braucht man nicht", "tokens": ["braucht", "man", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Zur Gesundheit blos allein!", "tokens": ["Zur", "Ge\u00b7sund\u00b7heit", "blos", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so viele tausend F\u00e4lle reich zu werden, und zu seyn!", "tokens": ["Wie", "so", "vie\u00b7le", "tau\u00b7send", "F\u00e4l\u00b7le", "reich", "zu", "wer\u00b7den", ",", "und", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "CARD", "NN", "ADJD", "PTKZU", "VAINF", "$,", "KON", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wie viel tausend zu dem Wolstand! wie viel zu begl\u00fcck-", "tokens": ["Wie", "viel", "tau\u00b7send", "zu", "dem", "Wol\u00b7stand", "!", "wie", "viel", "zu", "be\u00b7gl\u00fc\u00b7ck"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "APPR", "ART", "NN", "$.", "PWAV", "PIS", "APPR", "TRUNC"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "ter Ehe!", "tokens": ["ter", "E\u00b7he", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Wie viel tausend zur Bef\u00f6rdrung, da\u00df uns nichts im We-", "tokens": ["Wie", "viel", "tau\u00b7send", "zur", "Be\u00b7f\u00f6r\u00b7drung", ",", "da\u00df", "uns", "nichts", "im", "We"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "APPRART", "NN", "$,", "KOUS", "PPER", "PIS", "APPRART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "ge stehe,", "tokens": ["ge", "ste\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Welches m\u00e4chtiger als wir! da\u00df es wol von Statten gehe,", "tokens": ["Wel\u00b7ches", "m\u00e4ch\u00b7ti\u00b7ger", "als", "wir", "!", "da\u00df", "es", "wol", "von", "Stat\u00b7ten", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "KOUS", "PPER", "$.", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Wenn wir, zu der unsrigen Nutz und Wolfahrt, uns be-", "tokens": ["Wenn", "wir", ",", "zu", "der", "uns\u00b7ri\u00b7gen", "Nutz", "und", "Wol\u00b7fahrt", ",", "uns", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "m\u00fchn!", "tokens": ["m\u00fchn", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Wie viel tausend Hindrungen m\u00fcssen sich zu rechte ziehn,", "tokens": ["Wie", "viel", "tau\u00b7send", "Hin\u00b7drun\u00b7gen", "m\u00fcs\u00b7sen", "sich", "zu", "rech\u00b7te", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "CARD", "NN", "VMFIN", "PRF", "APPR", "ADJA", "VVINF", "$,"], "meter": "+-+-++-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.13": {"text": "Eh\u2019 man alles, was man w\u00fcnscht, was man braucht, was", "tokens": ["Eh'", "man", "al\u00b7les", ",", "was", "man", "w\u00fcnscht", ",", "was", "man", "braucht", ",", "was"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,", "PWS"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "uns gef\u00e4llt", "tokens": ["uns", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word"], "pos": ["PPER", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Erst erh\u00e4lt;", "tokens": ["Erst", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Und, wenn wir, trotz aller Hindrung, alles dieses \u00fcber-", "tokens": ["Und", ",", "wenn", "wir", ",", "trotz", "al\u00b7ler", "Hin\u00b7drung", ",", "al\u00b7les", "die\u00b7ses", "\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "$,", "APPR", "PIAT", "NN", "$,", "PIS", "PDAT", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "kommen,", "tokens": ["kom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Wird es nicht in acht genommen,", "tokens": ["Wird", "es", "nicht", "in", "acht", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "CARD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Gott, als Geber, nicht gedanckt. Ja, man wendet alle", "tokens": ["Gott", ",", "als", "Ge\u00b7ber", ",", "nicht", "ge\u00b7danckt", ".", "Ja", ",", "man", "wen\u00b7det", "al\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "NN", "$,", "PTKNEG", "VVPP", "$.", "PTKANT", "$,", "PIS", "VVFIN", "PIS"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Sachen,", "tokens": ["Sa\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Die mit so viel M\u00fch\u2019 erhalten, ja sein Ticht- und Trach-", "tokens": ["Die", "mit", "so", "viel", "M\u00fch'", "er\u00b7hal\u00b7ten", ",", "ja", "sein", "Ticht", "und", "Trach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "PIAT", "NN", "VVPP", "$,", "ADV", "PPOSAT", "TRUNC", "KON", "TRUNC"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "ten an,", "tokens": ["ten", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "(welch ein\u2019 ungl\u00fcckseel\u2019ge Thorheit, die man nicht begreif-", "tokens": ["(", "welch", "ein'", "un\u00b7gl\u00fcck\u00b7seel'\u00b7ge", "Thor\u00b7heit", ",", "die", "man", "nicht", "be\u00b7greif"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAT", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.24": {"text": "fen kann)", "tokens": ["fen", "kann", ")"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Sich, an statt vergn\u00fcgt und gl\u00fccklich, ungl\u00fcckseelig selbst", "tokens": ["Sich", ",", "an", "statt", "ver\u00b7gn\u00fcgt", "und", "gl\u00fcck\u00b7lich", ",", "un\u00b7gl\u00fcck\u00b7see\u00b7lig", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "$,", "APPR", "ADJD", "VVPP", "KON", "ADJD", "$,", "ADJD", "ADV"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "zu machen.", "tokens": ["zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}