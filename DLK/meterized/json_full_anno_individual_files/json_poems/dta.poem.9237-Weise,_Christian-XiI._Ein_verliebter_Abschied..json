{"dta.poem.9237": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n Ein verliebter Abschied.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Meines hertzens k\u00f6nigin!", "tokens": ["Mei\u00b7nes", "hert\u00b7zens", "k\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich jetzo von dir scheiden/", "tokens": ["Soll", "ich", "jet\u00b7zo", "von", "dir", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die lieben \u00f6rter meiden/", "tokens": ["Und", "die", "lie\u00b7ben", "\u00f6r\u00b7ter", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da ich vor gewesen bin?", "tokens": ["Da", "ich", "vor", "ge\u00b7we\u00b7sen", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O/ du meines lebens leben!", "tokens": ["O", "/", "du", "mei\u00b7nes", "le\u00b7bens", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist denn die\u00df der strenge Schlu\u00df/", "tokens": ["Ist", "denn", "die\u00df", "der", "stren\u00b7ge", "Schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "PDS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich gute nacht sol geben?", "tokens": ["Da\u00df", "ich", "gu\u00b7te", "nacht", "sol", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja mein liebstes kind! ich mu\u00df.", "tokens": ["Ja", "mein", "liebs\u00b7tes", "kind", "!", "ich", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Hier ist doch kein ewig bleiben/", "tokens": ["Hier", "ist", "doch", "kein", "e\u00b7wig", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "ADJD", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wir m\u00fc\u00dfen uns verschreiben", "tokens": ["Und", "wir", "m\u00fc\u00b7\u00dfen", "uns", "ver\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ferner in die welt zu gehn/", "tokens": ["Fer\u00b7ner", "in", "die", "welt", "zu", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser leben und studieren", "tokens": ["Un\u00b7ser", "le\u00b7ben", "und", "stu\u00b7die\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "KON", "VVINF"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Giebet uns den guten rath/", "tokens": ["Gie\u00b7bet", "uns", "den", "gu\u00b7ten", "rath", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bester massen auszuf\u00fchren/", "tokens": ["Bes\u00b7ter", "mas\u00b7sen", "aus\u00b7zu\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was man angefangen hat.", "tokens": ["Was", "man", "an\u00b7ge\u00b7fan\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Drum so nimm die treue hand/", "tokens": ["Drum", "so", "nimm", "die", "treu\u00b7e", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVIMP", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gib ihr noch zum letzten mahle/", "tokens": ["Gib", "ihr", "noch", "zum", "letz\u00b7ten", "mah\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier bey diesem rosenthale/", "tokens": ["Hier", "bey", "die\u00b7sem", "ro\u00b7sen\u00b7tha\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein gewisses freundschaffts-pfand/", "tokens": ["Ein", "ge\u00b7wis\u00b7ses", "freund\u00b7schaffts\u00b7pfand", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dr\u00fccke die betr\u00fcbten glieder/", "tokens": ["Dr\u00fc\u00b7cke", "die", "be\u00b7tr\u00fcb\u00b7ten", "glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn es ist doch nun geschehn/", "tokens": ["Denn", "es", "ist", "doch", "nun", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wer wei\u00df/ wann ich dich wieder/", "tokens": ["Und", "wer", "wei\u00df", "/", "wann", "ich", "dich", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$(", "PWAV", "PPER", "PRF", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebste seele! werde sehn.", "tokens": ["Liebs\u00b7te", "see\u00b7le", "!", "wer\u00b7de", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Dencke mein geliebtes kind!", "tokens": ["Den\u00b7cke", "mein", "ge\u00b7lieb\u00b7tes", "kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erwege meine sorgen/", "tokens": ["Und", "er\u00b7we\u00b7ge", "mei\u00b7ne", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann der abend und der morgen/", "tokens": ["Wann", "der", "a\u00b7bend", "und", "der", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "KON", "ART", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine seufftzer/ durch den wind/", "tokens": ["Mei\u00b7ne", "seufft\u00b7zer", "/", "durch", "den", "wind", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "APPR", "ART", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird zu deiner seele f\u00fchren/", "tokens": ["Wird", "zu", "dei\u00b7ner", "see\u00b7le", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So wird mein vergn\u00fcgter geist", "tokens": ["So", "wird", "mein", "ver\u00b7gn\u00fcg\u00b7ter", "geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Dein ged\u00e4chtni\u00df nicht verliehren/", "tokens": ["Dein", "ge\u00b7d\u00e4cht\u00b7ni\u00df", "nicht", "ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bi\u00df der lebens-faden reist.", "tokens": ["Bi\u00df", "der", "le\u00b7bens\u00b7fa\u00b7den", "reist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Sch\u00f6nste! lebe dir und mir/", "tokens": ["Sch\u00f6ns\u00b7te", "!", "le\u00b7be", "dir", "und", "mir", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "KON", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! ich habe deinem leben", "tokens": ["Ach", "!", "ich", "ha\u00b7be", "dei\u00b7nem", "le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPOSAT", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles wohlergehn ergeben/", "tokens": ["Al\u00b7les", "woh\u00b7ler\u00b7gehn", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum gedencke f\u00fcr und f\u00fcr/", "tokens": ["Drum", "ge\u00b7den\u00b7cke", "f\u00fcr", "und", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "KON", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wilst du mir die freude g\u00f6nnen!", "tokens": ["Wilst", "du", "mir", "die", "freu\u00b7de", "g\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So erhalte deinen schein/", "tokens": ["So", "er\u00b7hal\u00b7te", "dei\u00b7nen", "schein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bi\u00df wir endlich wieder k\u00f6nnen", "tokens": ["Bi\u00df", "wir", "end\u00b7lich", "wie\u00b7der", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6chst-vergn\u00fcgt beysammen seyn.", "tokens": ["H\u00f6chst\u00b7ver\u00b7gn\u00fcgt", "bey\u00b7sam\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}