{"dta.poem.10952": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Th\u00f6richter Hochmuth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich aufs Ufer-lose Meer, und seiner Fluthen Last\ngedenke;", "tokens": ["Wenn", "ich", "aufs", "U\u00b7fer\u00b7lo\u00b7se", "Meer", ",", "und", "sei\u00b7ner", "Flut\u00b7hen", "Last", "ge\u00b7den\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wenn ich mich in die hohle Tiefe der ausgespannten Lust", "tokens": ["Wenn", "ich", "mich", "in", "die", "hoh\u00b7le", "Tie\u00b7fe", "der", "aus\u00b7ge\u00b7spann\u00b7ten", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ja, wenn ich aller Sternen Heer, ins unumschr\u00e4nkten Him-", "tokens": ["Ja", ",", "wenn", "ich", "al\u00b7ler", "Ster\u00b7nen", "Heer", ",", "ins", "un\u00b7um\u00b7schr\u00e4nk\u00b7ten", "Him"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PIAT", "NN", "NN", "$,", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Erstaunet schau, und denn zugleich auf mich und meine Klein-", "tokens": ["Er\u00b7stau\u00b7net", "schau", ",", "und", "denn", "zu\u00b7gleich", "auf", "mich", "und", "mei\u00b7ne", "Klein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "ADV", "ADV", "APPR", "PPER", "KON", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Erschreck ich, und begreife nicht, wie ein so kleines Wesen sich,", "tokens": ["Er\u00b7schreck", "ich", ",", "und", "be\u00b7grei\u00b7fe", "nicht", ",", "wie", "ein", "so", "klei\u00b7nes", "We\u00b7sen", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KON", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "ADV", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Da es sich kaum des Nichts erwehrt, so th\u00f6richt als ver-", "tokens": ["Da", "es", "sich", "kaum", "des", "Nichts", "er\u00b7wehrt", ",", "so", "th\u00f6\u00b7richt", "als", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "NN", "VVPP", "$,", "ADV", "ADJD", "KOKOM", "TRUNC"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.7": {"text": "Den Sch\u00f6pfer aller Welt zu fassen, und zu beschreiben, un-", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Welt", "zu", "fas\u00b7sen", ",", "und", "zu", "be\u00b7schrei\u00b7ben", ",", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}