{"dta.poem.11128": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft des Grobiani.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die grobheit war bey mir mein f\u00fcnfftes element,", "tokens": ["Die", "grob\u00b7heit", "war", "bey", "mir", "mein", "f\u00fcnff\u00b7tes", "e\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Noch gr\u00f6ber war der tod, der keine ", "tokens": ["Noch", "gr\u00f6\u00b7ber", "war", "der", "tod", ",", "der", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zwey st\u00e4dte sind, die mir doch ruhm erworben haben:", "tokens": ["Zwey", "st\u00e4d\u00b7te", "sind", ",", "die", "mir", "doch", "ruhm", "er\u00b7wor\u00b7ben", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu Schweinfurth ward ich jung, zu Ochsenfurth begraben.", "tokens": ["Zu", "Schwein\u00b7furth", "ward", "ich", "jung", ",", "zu", "Och\u00b7sen\u00b7furth", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexan