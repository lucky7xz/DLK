{"dta.poem.20507": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was denckt aus mir der himmel doch zu machen?", "tokens": ["Was", "denckt", "aus", "mir", "der", "him\u00b7mel", "doch", "zu", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df aller schmertz auff meinen scheitel f\u00e4llt;", "tokens": ["Da\u00df", "al\u00b7ler", "schmertz", "auff", "mei\u00b7nen", "schei\u00b7tel", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df stete qvaal und pein bey mir erwachen/", "tokens": ["Da\u00df", "ste\u00b7te", "qvaal", "und", "pein", "bey", "mir", "er\u00b7wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bin ich allein ein s\u00fcnder auff der welt?", "tokens": ["Bin", "ich", "al\u00b7lein", "ein", "s\u00fcn\u00b7der", "auff", "der", "welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein andrer speist sein hertz mit tausend freuden/", "tokens": ["Ein", "an\u00b7drer", "speist", "sein", "hertz", "mit", "tau\u00b7send", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich \u00e4rmster aber mu\u00df best\u00e4ndig leiden.", "tokens": ["Ich", "\u00e4rms\u00b7ter", "a\u00b7ber", "mu\u00df", "be\u00b7st\u00e4n\u00b7dig", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach Margaris! du f\u00fcrstin meiner seelen/", "tokens": ["Ach", "Mar\u00b7ga\u00b7ris", "!", "du", "f\u00fcrs\u00b7tin", "mei\u00b7ner", "see\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPER", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein englisch seyn hat meine seel entz\u00fcndt/", "tokens": ["Dein", "eng\u00b7lisch", "seyn", "hat", "mei\u00b7ne", "seel", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAINF", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich mu\u00df die glut in meiner brust verh\u00f6len/", "tokens": ["Ich", "mu\u00df", "die", "glut", "in", "mei\u00b7ner", "brust", "ver\u00b7h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil auch ein wort offt strenges urthel find/", "tokens": ["Weil", "auch", "ein", "wort", "offt", "stren\u00b7ges", "ur\u00b7thel", "find", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein reines hertze brennt von liebes-flammen/", "tokens": ["Mein", "rei\u00b7nes", "hert\u00b7ze", "brennt", "von", "lie\u00b7bes\u00b7flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mit gedult mu\u00df ich die glut verdammen.", "tokens": ["Und", "mit", "ge\u00b7dult", "mu\u00df", "ich", "die", "glut", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein sclave der in seinen ketten schmachtet/", "tokens": ["Ein", "scla\u00b7ve", "der", "in", "sei\u00b7nen", "ket\u00b7ten", "schmach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der tauben ohren seine seuffzer schickt/", "tokens": ["Der", "tau\u00b7ben", "oh\u00b7ren", "sei\u00b7ne", "seuff\u00b7zer", "schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der strang und pfahl vor seinen siegs-krantz achtet/", "tokens": ["Der", "strang", "und", "pfahl", "vor", "sei\u00b7nen", "siegs\u00b7krantz", "ach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist ja vielmehr als leyder! ich begl\u00fcckt:", "tokens": ["Ist", "ja", "viel\u00b7mehr", "als", "ley\u00b7der", "!", "ich", "be\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOUS", "NN", "$.", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn er darff seine noth noch sterbend zeigen/", "tokens": ["Denn", "er", "darff", "sei\u00b7ne", "noth", "noch", "ster\u00b7bend", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich aber ach! mu\u00df meine pein verschweigen.", "tokens": ["Ich", "a\u00b7ber", "ach", "!", "mu\u00df", "mei\u00b7ne", "pein", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$.", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun Margaris fiehstu mich gleich nicht brennen/", "tokens": ["Nun", "Mar\u00b7ga\u00b7ris", "fieh\u00b7stu", "mich", "gleich", "nicht", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Deckt schon die noth mein unberedter mund/", "tokens": ["Deckt", "schon", "die", "noth", "mein", "un\u00b7be\u00b7red\u00b7ter", "mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So soltu mich doch in der aschen kennen;", "tokens": ["So", "sol\u00b7tu", "mich", "doch", "in", "der", "asc\u00b7hen", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein seuffzend hertz macht meine liebe kund/", "tokens": ["Mein", "seuff\u00b7zend", "hertz", "macht", "mei\u00b7ne", "lie\u00b7be", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "VVFIN", "PPOSAT", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Besing ich st\u00fcndlich gleich nicht mein betr\u00fcben/", "tokens": ["Be\u00b7sing", "ich", "st\u00fcnd\u00b7lich", "gleich", "nicht", "mein", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "PTKNEG", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So denck Adon kan leiden/ schweigen/ lieben.", "tokens": ["So", "denck", "A\u00b7don", "kan", "lei\u00b7den", "/", "schwei\u00b7gen", "/", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "NE", "VMFIN", "VVINF", "$(", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er stellt die welt dem irrthum zum gesetze/", "tokens": ["Er", "stellt", "die", "welt", "dem", "irr\u00b7thum", "zum", "ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dieses ists/ was mich bestricket h\u00e4lt/", "tokens": ["Und", "die\u00b7ses", "ists", "/", "was", "mich", "be\u00b7stri\u00b7cket", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$(", "PWS", "PPER", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchrt mich nicht mein verh\u00e4ngni\u00df aus dem netze/", "tokens": ["F\u00fchrt", "mich", "nicht", "mein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "aus", "dem", "net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So/ da\u00df die hoffnung gleich mit mir verf\u00e4llt/", "tokens": ["So", "/", "da\u00df", "die", "hoff\u00b7nung", "gleich", "mit", "mir", "ver\u00b7f\u00e4llt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So soll mein hertz doch glut und flammen hegen/", "tokens": ["So", "soll", "mein", "hertz", "doch", "glut", "und", "flam\u00b7men", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bi\u00df man mich wird zu meinen v\u00e4tern legen.", "tokens": ["Bi\u00df", "man", "mich", "wird", "zu", "mei\u00b7nen", "v\u00e4\u00b7tern", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}