{"textgrid.poem.31358": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das letzte Lied", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern ab am Horizont, auf Felsenrissen,", "tokens": ["Fern", "ab", "am", "Ho\u00b7ri\u00b7zont", ",", "auf", "Fel\u00b7sen\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Liegt der gewitterschwarze Krieg get\u00fcrmt.", "tokens": ["Liegt", "der", "ge\u00b7wit\u00b7ter\u00b7schwar\u00b7ze", "Krieg", "ge\u00b7t\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Blitze zucken schon, die ungewissen,", "tokens": ["Die", "Blit\u00b7ze", "zu\u00b7cken", "schon", ",", "die", "un\u00b7ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Wandrer sucht das Laubdach, das ihn schirmt.", "tokens": ["Der", "Wand\u00b7rer", "sucht", "das", "Laub\u00b7dach", ",", "das", "ihn", "schirmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie ein Strom, geschwellt von Regeng\u00fcssen,", "tokens": ["Und", "wie", "ein", "Strom", ",", "ge\u00b7schwellt", "von", "Re\u00b7gen\u00b7g\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus seines Ufers Bette heulend st\u00fcrmt,", "tokens": ["Aus", "sei\u00b7nes", "U\u00b7fers", "Bet\u00b7te", "heu\u00b7lend", "st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kommt das Verderben, mit entbundnen Wogen,", "tokens": ["Kommt", "das", "Ver\u00b7der\u00b7ben", ",", "mit", "ent\u00b7bund\u00b7nen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf alles, was besteht, herangezogen.", "tokens": ["Auf", "al\u00b7les", ",", "was", "be\u00b7steht", ",", "her\u00b7an\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der alten Staaten graues Prachtger\u00fcste", "tokens": ["Der", "al\u00b7ten", "Staa\u00b7ten", "grau\u00b7es", "Pracht\u00b7ge\u00b7r\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sinkt donnernd ein, von ihm hinweggesp\u00fclt,", "tokens": ["Sinkt", "don\u00b7nernd", "ein", ",", "von", "ihm", "hin\u00b7weg\u00b7ge\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie, auf der Heide Grund, ein Wurmgeniste,", "tokens": ["Wie", ",", "auf", "der", "Hei\u00b7de", "Grund", ",", "ein", "Wurm\u00b7ge\u00b7nis\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von einem Knaben scharrend weggew\u00fchlt;", "tokens": ["Von", "ei\u00b7nem", "Kna\u00b7ben", "schar\u00b7rend", "weg\u00b7ge\u00b7w\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wo das Leben, um der Menschen Br\u00fcste,", "tokens": ["Und", "wo", "das", "Le\u00b7ben", ",", "um", "der", "Men\u00b7schen", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "KOUI", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In tausend Lichtern jauchzend hat gespielt,", "tokens": ["In", "tau\u00b7send", "Lich\u00b7tern", "jauch\u00b7zend", "hat", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist es so lautlos jetzt, wie in den Reichen,", "tokens": ["Ist", "es", "so", "laut\u00b7los", "jetzt", ",", "wie", "in", "den", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch die die Wellen des Kozytus schleichen.", "tokens": ["Durch", "die", "die", "Wel\u00b7len", "des", "Ko\u00b7zy\u00b7tus", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Und ein Geschlecht, von d\u00fcsterm Haar umflogen,", "tokens": ["Und", "ein", "Ge\u00b7schlecht", ",", "von", "d\u00fcs\u00b7term", "Haar", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tritt aus der Nacht, das keinen Namen f\u00fchrt,", "tokens": ["Tritt", "aus", "der", "Nacht", ",", "das", "kei\u00b7nen", "Na\u00b7men", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das, wie ein Hirngespinst der Mythologen,", "tokens": ["Das", ",", "wie", "ein", "Hirn\u00b7ge\u00b7spinst", "der", "My\u00b7tho\u00b7lo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hervor aus der Erschlagnen Knochen stiert;", "tokens": ["Her\u00b7vor", "aus", "der", "Er\u00b7schlag\u00b7nen", "Kno\u00b7chen", "stiert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Das ist geboren nicht und nicht erzogen", "tokens": ["Das", "ist", "ge\u00b7bo\u00b7ren", "nicht", "und", "nicht", "er\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "VVPP", "PTKNEG", "KON", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vom alten, das im deutschen Land regiert:", "tokens": ["Vom", "al\u00b7ten", ",", "das", "im", "deut\u00b7schen", "Land", "re\u00b7giert", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das l\u00e4\u00dft in T\u00f6nen, wie der Nord an Str\u00f6men,", "tokens": ["Das", "l\u00e4\u00dft", "in", "T\u00f6\u00b7nen", ",", "wie", "der", "Nord", "an", "Str\u00f6\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn er im Schilfrohr seufzet, sich vernehmen.", "tokens": ["Wenn", "er", "im", "Schilf\u00b7rohr", "seuf\u00b7zet", ",", "sich", "ver\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und du, o Lied, voll unnennbarer Wonnen,", "tokens": ["Und", "du", ",", "o", "Lied", ",", "voll", "un\u00b7nenn\u00b7ba\u00b7rer", "Won\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das das Gef\u00fchl so wunderbar erhebt,", "tokens": ["Das", "das", "Ge\u00b7f\u00fchl", "so", "wun\u00b7der\u00b7bar", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das, einer Himmelsurne wie entronnen,", "tokens": ["Das", ",", "ei\u00b7ner", "Him\u00b7mel\u00b7sur\u00b7ne", "wie", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu den entz\u00fcckten Ohren niederschwebt,", "tokens": ["Zu", "den", "ent\u00b7z\u00fcck\u00b7ten", "Oh\u00b7ren", "nie\u00b7der\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bei dessen Klang, empor ins Reich der Sonnen,", "tokens": ["Bei", "des\u00b7sen", "Klang", ",", "em\u00b7por", "ins", "Reich", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "PTKVZ", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von allen Banden frei die Seele strebt;", "tokens": ["Von", "al\u00b7len", "Ban\u00b7den", "frei", "die", "See\u00b7le", "strebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dich trifft der Todespfeil; die Parzen winken,", "tokens": ["Dich", "trifft", "der", "To\u00b7des\u00b7pfeil", ";", "die", "Par\u00b7zen", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und stumm ins Grab mu\u00dft du daniedersinken.", "tokens": ["Und", "stumm", "ins", "Grab", "mu\u00dft", "du", "da\u00b7nie\u00b7der\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Erschienen, festlich, in der V\u00f6lker Reigen,", "tokens": ["Er\u00b7schie\u00b7nen", ",", "fest\u00b7lich", ",", "in", "der", "V\u00f6l\u00b7ker", "Rei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird dir kein Beifall mehr entgegen bl\u00fchn,", "tokens": ["Wird", "dir", "kein", "Bei\u00b7fall", "mehr", "ent\u00b7ge\u00b7gen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Herz dir klopfen, keine Brust dir steigen,", "tokens": ["Kein", "Herz", "dir", "klop\u00b7fen", ",", "kei\u00b7ne", "Brust", "dir", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$,", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir keine Tr\u00e4ne mehr zur Erde gl\u00fchn,", "tokens": ["Dir", "kei\u00b7ne", "Tr\u00e4\u00b7ne", "mehr", "zur", "Er\u00b7de", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und nur wo einsam, unter Tannenzweigen,", "tokens": ["Und", "nur", "wo", "ein\u00b7sam", ",", "un\u00b7ter", "Tan\u00b7nen\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PWAV", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu Leichensteinen stille Pfade fliehn,", "tokens": ["Zu", "Lei\u00b7chen\u00b7stei\u00b7nen", "stil\u00b7le", "Pfa\u00b7de", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wird Wanderern, die bei den Toten leben,", "tokens": ["Wird", "Wan\u00b7de\u00b7rern", ",", "die", "bei", "den", "To\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Schatten deiner Sch\u00f6n' entgegenschweben.", "tokens": ["Ein", "Schat\u00b7ten", "dei\u00b7ner", "Sch\u00f6n'", "ent\u00b7ge\u00b7gen\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und st\u00e4rker rauscht der S\u00e4nger in die Saiten,", "tokens": ["Und", "st\u00e4r\u00b7ker", "rauscht", "der", "S\u00e4n\u00b7ger", "in", "die", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der T\u00f6ne ganze Macht lockt er hervor,", "tokens": ["Der", "T\u00f6\u00b7ne", "gan\u00b7ze", "Macht", "lockt", "er", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er singt die Lust, f\u00fcrs Vaterland zu streiten,", "tokens": ["Er", "singt", "die", "Lust", ",", "f\u00fcrs", "Va\u00b7ter\u00b7land", "zu", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und machtlos schl\u00e4gt sein Ruf an jedes Ohr, \u2013", "tokens": ["Und", "macht\u00b7los", "schl\u00e4gt", "sein", "Ruf", "an", "je\u00b7des", "Ohr", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und da sein Blick das Blutpanier der Zeiten", "tokens": ["Und", "da", "sein", "Blick", "das", "Blut\u00b7pa\u00b7nier", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Stets weiter flattern sieht, von Tor zu Tor,", "tokens": ["Stets", "wei\u00b7ter", "flat\u00b7tern", "sieht", ",", "von", "Tor", "zu", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VVFIN", "$,", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schlie\u00dft er sein Lied, er w\u00fcnscht mit ihm zu enden,", "tokens": ["Schlie\u00dft", "