{"dta.poem.4210": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das sch\u00f6ne Blau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich mein ger\u00fchrt Gesicht,", "tokens": ["In\u00b7dem", "ich", "mein", "ge\u00b7r\u00fchrt", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem ernsten Blick und mit Bedacht,", "tokens": ["Mit", "ei\u00b7nem", "erns\u00b7ten", "Blick", "und", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf die, an Farb\u2019 und Glanz, dem Himmel gleiche Pracht", "tokens": ["Auf", "die", ",", "an", "Fa\u00b7rb'", "und", "Glanz", ",", "dem", "Him\u00b7mel", "glei\u00b7che", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "APPR", "NN", "KON", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Der lieblichen ", "tokens": ["Der", "lieb\u00b7li\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "Bewundernd richte, sie betrachte,", "tokens": ["Be\u00b7wun\u00b7dernd", "rich\u00b7te", ",", "sie", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und von der Farbe dieses dachte:", "tokens": ["Und", "von", "der", "Far\u00b7be", "die\u00b7ses", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PDAT", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Unm\u00f6glich kann ein blauer Schein", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "kann", "ein", "blau\u00b7er", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So angenehm, so lieblich seyn,", "tokens": ["So", "an\u00b7ge\u00b7nehm", ",", "so", "lieb\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und unser Auge mehr vergn\u00fcgen;", "tokens": ["Und", "un\u00b7ser", "Au\u00b7ge", "mehr", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sah ich ein noch viel sch\u00f6ners fliegen.", "tokens": ["Sah", "ich", "ein", "noch", "viel", "sch\u00f6\u00b7ners", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Es flog, als recht von ungefehr,", "tokens": ["Es", "flog", ",", "als", "recht", "von", "un\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Grase-Pferdchen hin und her;", "tokens": ["Ein", "Gra\u00b7se\u00b7P\u00b7ferd\u00b7chen", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ja, recht als wenn es haben wollte,", "tokens": ["Ja", ",", "recht", "als", "wenn", "es", "ha\u00b7ben", "woll\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "KOKOM", "KOUS", "PPER", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df ich es auch bewundern sollte;", "tokens": ["Da\u00df", "ich", "es", "auch", "be\u00b7wun\u00b7dern", "soll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Hemmt es sein fl\u00fcchtiges Gefieder,", "tokens": ["Hemmt", "es", "sein", "fl\u00fcch\u00b7ti\u00b7ges", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Und setzte sich selbst auf das Bl\u00fchmchen nieder,", "tokens": ["Und", "setz\u00b7te", "sich", "selbst", "auf", "das", "Bl\u00fch\u00b7mchen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Das ich, an seinem Schmuck, worinn es spielte,", "tokens": ["Das", "ich", ",", "an", "sei\u00b7nem", "Schmuck", ",", "wo\u00b7rinn", "es", "spiel\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "F\u00fcr v\u00f6llig unvergleichlich hielte.", "tokens": ["F\u00fcr", "v\u00f6l\u00b7lig", "un\u00b7ver\u00b7gleich\u00b7lich", "hiel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Es schien, ob wollt\u2019 es dadurch zeigen,", "tokens": ["Es", "schien", ",", "ob", "wollt'", "es", "da\u00b7durch", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Da\u00df, in der bildenden Natur,", "tokens": ["Da\u00df", ",", "in", "der", "bil\u00b7den\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sowohl an Farben als Figur,", "tokens": ["So\u00b7wohl", "an", "Far\u00b7ben", "als", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die Kr\u00e4fte noch viel h\u00f6her steigen,", "tokens": ["Die", "Kr\u00e4f\u00b7te", "noch", "viel", "h\u00f6\u00b7her", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Als wir, ob selbe noch so sch\u00f6n", "tokens": ["Als", "wir", ",", "ob", "sel\u00b7be", "noch", "so", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ADJA", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Uns in die Augen fallen, sehn.", "tokens": ["Uns", "in", "die", "Au\u00b7gen", "fal\u00b7len", ",", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Er legte sein noch st\u00e4rker Himmel-Blau", "tokens": ["Er", "leg\u00b7te", "sein", "noch", "st\u00e4r\u00b7ker", "Him\u00b7mel\u00b7Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Voll Glanz, als wie mit Flei\u00df, indem es sich nicht regte,", "tokens": ["Voll", "Glanz", ",", "als", "wie", "mit", "Flei\u00df", ",", "in\u00b7dem", "es", "sich", "nicht", "reg\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "KOUS", "KOKOM", "APPR", "NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Recht, als im Gegenhalt, zur Schau.", "tokens": ["Recht", ",", "als", "im", "Ge\u00b7gen\u00b7halt", ",", "zur", "Schau", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Es ward, durch seinen Glanz bewogen,", "tokens": ["Es", "ward", ",", "durch", "sei\u00b7nen", "Glanz", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Mein Blick auf dieses Thier allein gezogen.", "tokens": ["Mein", "Blick", "auf", "die\u00b7ses", "Thier", "al\u00b7lein", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Ich stutzte recht, als ich den Glanz erwegte,", "tokens": ["Ich", "stutz\u00b7te", "recht", ",", "als", "ich", "den", "Glanz", "er\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Den sch\u00f6nen Schmelz, die Klarheit \u00fcberlegte,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Schmelz", ",", "die", "Klar\u00b7heit", "\u00fc\u00b7ber\u00b7leg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Die auf demselben mehr zu gl\u00fchn,", "tokens": ["Die", "auf", "dem\u00b7sel\u00b7ben", "mehr", "zu", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Als seine Haut zu f\u00e4rben, schien.", "tokens": ["Als", "sei\u00b7ne", "Haut", "zu", "f\u00e4r\u00b7ben", ",", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Mein GOtt! fing ich, voll Freuden, an,", "tokens": ["Mein", "Gott", "!", "fing", "ich", ",", "voll", "Freu\u00b7den", ",", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "ADJD", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ger\u00fchrt durch die\u00df sapphirne Gl\u00e4nzen,", "tokens": ["Ge\u00b7r\u00fchrt", "durch", "die\u00df", "sap\u00b7phir\u00b7ne", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PDS", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.36": {"text": "Wer ist, der Deiner Wunder Grenzen,", "tokens": ["Wer", "ist", ",", "der", "Dei\u00b7ner", "Wun\u00b7der", "Gren\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Jm Kleinsten nur, ergr\u00fcnden kann?", "tokens": ["Jm", "Kleins\u00b7ten", "nur", ",", "er\u00b7gr\u00fcn\u00b7den", "kann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Mir stellt nicht nur an diesem Thier", "tokens": ["Mir", "stellt", "nicht", "nur", "an", "die\u00b7sem", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Des C\u00f6rpers Zierlichkeit und Bau;", "tokens": ["Des", "C\u00f6r\u00b7pers", "Zier\u00b7lich\u00b7keit", "und", "Bau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Mir stellt sein wundersch\u00f6nes Blau", "tokens": ["Mir", "stellt", "sein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Ein tiefes Meer von Sch\u00f6nheit f\u00fcr,", "tokens": ["Ein", "tie\u00b7fes", "Meer", "von", "Sch\u00f6n\u00b7heit", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ein unersch\u00f6pflichs Meer von Sch\u00e4tzen,", "tokens": ["Ein", "un\u00b7er\u00b7sch\u00f6pf\u00b7lichs", "Meer", "von", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Die Creaturen zu ergetzen,", "tokens": ["Die", "Crea\u00b7tu\u00b7ren", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Das dies\u2019 und alle Welten f\u00fcllet,", "tokens": ["Das", "dies'", "und", "al\u00b7le", "Wel\u00b7ten", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Und welches aus der Gottheit quillet.", "tokens": ["Und", "wel\u00b7ches", "aus", "der", "Got\u00b7theit", "quil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Es f\u00fchret mich sein sch\u00f6nes Kleid", "tokens": ["Es", "f\u00fch\u00b7ret", "mich", "sein", "sch\u00f6\u00b7nes", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Auf eine neu- und klare Spur", "tokens": ["Auf", "ei\u00b7ne", "neu", "und", "kla\u00b7re", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Zu einer Unerm\u00e4\u00dflichkeit,", "tokens": ["Zu", "ei\u00b7ner", "Un\u00b7er\u00b7m\u00e4\u00df\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Auch selbst im Reiche der Natur,", "tokens": ["Auch", "selbst", "im", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Die mir in der Gesch\u00f6pfe Pracht,", "tokens": ["Die", "mir", "in", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Mit hellen Farben, deutlich zeiget,", "tokens": ["Mit", "hel\u00b7len", "Far\u00b7ben", ",", "deut\u00b7lich", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Wie weit des Sch\u00f6pfers Lieb\u2019 und Macht,", "tokens": ["Wie", "weit", "des", "Sch\u00f6p\u00b7fers", "Lieb'", "und", "Macht", ","], "token_info": ["word", "word", "word", "wor