ct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Treues Herz, das nimmer von mir l\u00e4\u00dft!", "tokens": ["Treu\u00b7es", "Herz", ",", "das", "nim\u00b7mer", "von", "mir", "l\u00e4\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Meine Liebe, sagst du, sei dein Leben?", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", ",", "sagst", "du", ",", "sei", "dein", "Le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O so frag', ob ich sie Dir gegeben,", "tokens": ["O", "so", "frag'", ",", "ob", "ich", "sie", "Dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Diese Thr\u00e4ne, die mein Auge n\u00e4\u00dft,", "tokens": ["Die\u00b7se", "Thr\u00e4\u00b7ne", ",", "die", "mein", "Au\u00b7ge", "n\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Treues Herz, das nimmer von mir l\u00e4\u00dft!", "tokens": ["Treu\u00b7es", "Herz", ",", "das", "nim\u00b7mer", "von", "mir", "l\u00e4\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "AD