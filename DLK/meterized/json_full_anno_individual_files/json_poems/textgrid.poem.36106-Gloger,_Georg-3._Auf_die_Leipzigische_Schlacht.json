{"textgrid.poem.36106": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf die Leipzigische Schlacht", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist die erste Schlacht, di\u00df ist das erste Siegen,", "tokens": ["Di\u00df", "ist", "die", "ers\u00b7te", "Schlacht", ",", "di\u00df", "ist", "das", "ers\u00b7te", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das auf den werthen Schlu\u00df zu ", "tokens": ["Das", "auf", "den", "wert\u00b7hen", "Schlu\u00df", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "Der in Italien vorhin nicht kunt erliegen;", "tokens": ["Der", "in", "I\u00b7ta\u00b7li\u00b7en", "vor\u00b7hin", "nicht", "kunt", "er\u00b7lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht, wo die k\u00fchne ", "tokens": ["Nicht", ",", "wo", "die", "k\u00fch\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der an dem ", "tokens": ["Der", "an", "dem"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Vnd holt ihm die Geb\u00fchr von seiner Thaten Schande.", "tokens": ["Vnd", "holt", "ihm", "die", "Ge\u00b7b\u00fchr", "von", "sei\u00b7ner", "Tha\u00b7ten", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er hat sonst nirgends nicht geschlagen sollen seyn,", "tokens": ["Er", "hat", "sonst", "nir\u00b7gends", "nicht", "ge\u00b7schla\u00b7gen", "sol\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVINF", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als nur bey Leipzig hier. Hier hat die gute Sache", "tokens": ["Als", "nur", "bey", "Leip\u00b7zig", "hier", ".", "Hier", "hat", "die", "gu\u00b7te", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NE", "ADV", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den ersten Grund gelegt, hier wuchse sie auch auf,", "tokens": ["Den", "ers\u00b7ten", "Grund", "ge\u00b7legt", ",", "hier", "wuch\u00b7se", "sie", "auch", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hier griffe sie zum Schwert, vnd suchte Schutz vnd Rache", "tokens": ["Hier", "grif\u00b7fe", "sie", "zum", "Schwert", ",", "vnd", "such\u00b7te", "Schutz", "vnd", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch vieler Tausent Blut, das beyden gienge drauf.", "tokens": ["Durch", "vie\u00b7ler", "Tau\u00b7sent", "Blut", ",", "das", "bey\u00b7den", "gien\u00b7ge", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "PRELS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Wahlstatt sagt es aus, die wie ein Blut vnd Eyter,", "tokens": ["Die", "Wahl\u00b7statt", "sagt", "es", "aus", ",", "die", "wie", "ein", "Blut", "vnd", "Ey\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "KOKOM", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein vielgestalter Tod, vnd w\u00fcstes Beinhaus steht.", "tokens": ["Ein", "viel\u00b7ge\u00b7stal\u00b7ter", "Tod", ",", "vnd", "w\u00fcs\u00b7tes", "Be\u00b7in\u00b7haus", "steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Geh hin nun, ", "tokens": ["Geh", "hin", "nun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Denn der die Sache sch\u00fctzt, dir auf den Fersen geht.", "tokens": ["Denn", "der", "die", "Sa\u00b7che", 