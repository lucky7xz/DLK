{"dta.poem.14883": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die deutsche Jungfrau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das Fr\u00e4ulein stie\u00df die Knecht' hinab,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "stie\u00df", "die", "Knecht'", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Liebsten auch in's hei\u00dfe Grab,", "tokens": ["Den", "Liebs\u00b7ten", "auch", "in's", "hei\u00b7\u00dfe", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie selbst dann in die Flamme sprang,", "tokens": ["Sie", "selbst", "dann", "in", "die", "Flam\u00b7me", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber ihnen die Burg zusammen sank.", "tokens": ["Ue\u00b7ber", "ih\u00b7nen", "die", "Burg", "zu\u00b7sam\u00b7men", "sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}