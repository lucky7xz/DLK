{"textgrid.poem.49687": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Gro\u00dfindustriellen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir lieben dieses Vaterland!", "tokens": ["Wir", "lie\u00b7ben", "die\u00b7ses", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch fesselt uns ein sch\u00f6n'res Band", "tokens": ["Doch", "fes\u00b7selt", "uns", "ein", "sch\u00f6n'\u00b7res", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel st\u00e4rker, unvergleichlich z\u00e4h", "tokens": ["Viel", "st\u00e4r\u00b7ker", ",", "un\u00b7ver\u00b7gleich\u00b7lich", "z\u00e4h"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ans Portemonnaie.", "tokens": ["Ans", "Por\u00b7te\u00b7mon\u00b7nai\u00b7e", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Treue unserm K\u00f6nigshaus,", "tokens": ["Die", "Treu\u00b7e", "un\u00b7serm", "K\u00f6\u00b7nigs\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir h\u00e4ngen sie beim Sekt heraus,", "tokens": ["Wir", "h\u00e4n\u00b7gen", "sie", "beim", "Sekt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART",