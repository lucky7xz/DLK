{"dta.poem.9241": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n Auf eine trauer-Hochzeit.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.57", "en:0.42"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ljebste braut! was soll ich sagen/", "tokens": ["Ljebs\u00b7te", "braut", "!", "was", "soll", "ich", "sa\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich das verlohrne gut", "tokens": ["Soll", "ich", "das", "ver\u00b7lohr\u00b7ne", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhrer jungferschafft beklagen/", "tokens": ["Ih\u00b7rer", "jung\u00b7fer\u00b7schafft", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder soll ich meinen muth", "tokens": ["O\u00b7der", "soll", "ich", "mei\u00b7nen", "muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mitten in den freuden spielen/", "tokens": ["Mit\u00b7ten", "in", "den", "freu\u00b7den", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem jungen weibgen k\u00fchlen.", "tokens": ["An", "dem", "jun\u00b7gen", "weib\u00b7gen", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Nein/ ich mag sie nicht vexieren/", "tokens": ["Nein", "/", "ich", "mag", "sie", "nicht", "ve\u00b7xie\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es ist mir nicht zuviel/", "tokens": ["Denn", "es", "ist", "mir", "nicht", "zu\u00b7viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch wofern ich klage f\u00fchren", "tokens": ["Auch", "wo\u00b7fern", "ich", "kla\u00b7ge", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die lust verderben will/", "tokens": ["Und", "die", "lust", "ver\u00b7der\u00b7ben", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Werd ich doch bey ihren lachen", "tokens": ["Werd", "ich", "doch", "bey", "ih\u00b7ren", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwerlich meinen anfang machen.", "tokens": ["Schwer\u00b7lich", "mei\u00b7nen", "an\u00b7fang", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Hier bey diesen ehren-feste", "tokens": ["Hier", "bey", "die\u00b7sen", "eh\u00b7ren\u00b7fes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Giebt es sonst gelegenheit/", "tokens": ["Giebt", "es", "sonst", "ge\u00b7le\u00b7gen\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die angenehmen g\u00e4ste", "tokens": ["Denn", "die", "an\u00b7ge\u00b7neh\u00b7men", "g\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden alsobald zerstreut/", "tokens": ["Wer\u00b7den", "al\u00b7so\u00b7bald", "zer\u00b7streut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die zincken und die geigen", "tokens": ["Weil", "die", "zin\u00b7cken", "und", "die", "gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den stillen platze schweigen.", "tokens": ["Auf", "den", "stil\u00b7len", "plat\u00b7ze", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Zwar man darff auffs musiciren", "tokens": ["Zwar", "man", "darff", "auffs", "mu\u00b7si\u00b7ci\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VMFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so sehr gebunden seyn/", "tokens": ["Nicht", "so", "sehr", "ge\u00b7bun\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den muth darumb verlieren;", "tokens": ["Und", "den", "muth", "da\u00b7rumb", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dann man schertze gleich so fein/", "tokens": ["Dann", "man", "schert\u00b7ze", "gleich", "so", "fein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als vor vielen tausend jahren", "tokens": ["Als", "vor", "vie\u00b7len", "tau\u00b7send", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine geigenmacher waren.", "tokens": ["Kei\u00b7ne", "gei\u00b7gen\u00b7ma\u00b7cher", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Doch sie wollens hier nicht gl\u00e4uben/", "tokens": ["Doch", "sie", "wol\u00b7lens", "hier", "nicht", "gl\u00e4u\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dieweil ein jederman", "tokens": ["Und", "die\u00b7weil", "ein", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will auf seinem sinne bleiben/", "tokens": ["Will", "auf", "sei\u00b7nem", "sin\u00b7ne", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht das zehnde spiel nicht an:", "tokens": ["Geht", "das", "zehn\u00b7de", "spiel", "nicht", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drumb mu\u00df eines nach dem andern", "tokens": ["Drumb", "mu\u00df", "ei\u00b7nes", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor der zeit nach hause wandern.", "tokens": ["Vor", "der", "zeit", "nach", "hau\u00b7se", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Sauer sehn und h\u00f6nisch lachen", "tokens": ["Sau\u00b7er", "sehn", "und", "h\u00f6\u00b7nisch", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnen alle leute wohl/", "tokens": ["K\u00f6n\u00b7nen", "al\u00b7le", "leu\u00b7te", "wohl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wer sich lustig machen", "tokens": ["A\u00b7ber", "wer", "sich", "lus\u00b7tig", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den spa\u00df erhalten soll/", "tokens": ["Und", "den", "spa\u00df", "er\u00b7hal\u00b7ten", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df sich unverdienter massen", "tokens": ["Mu\u00df", "sich", "un\u00b7ver\u00b7dien\u00b7ter", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Richten und verdammen lassen.", "tokens": ["Rich\u00b7ten", "und", "ver\u00b7dam\u00b7men", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Manche kan sich eckel stellen", "tokens": ["Man\u00b7che", "kan", "sich", "ec\u00b7kel", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und veracht\u2019t die compagnie/", "tokens": ["Und", "ver\u00b7acht't", "die", "com\u00b7pag\u00b7nie", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mit solchen junggesellen", "tokens": ["Denn", "mit", "sol\u00b7chen", "jung\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da verlohnt sichs nicht die m\u00fch;", "tokens": ["Da", "ver\u00b7lohnt", "sichs", "nicht", "die", "m\u00fch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "ART", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche wei\u00df an allen enden", "tokens": ["Man\u00b7che", "wei\u00df", "an", "al\u00b7len", "en\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon was anders einzuwenden.", "tokens": ["Schon", "was", "an\u00b7ders", "ein\u00b7zu\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Doch wir sind an unserm orte", "tokens": ["Doch", "wir", "sind", "an", "un\u00b7serm", "or\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht ein wenig schuld daran/", "tokens": ["Nicht", "ein", "we\u00b7nig", "schuld", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "PIS", "ADJD", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sind lauter leere worte", "tokens": ["Da", "sind", "lau\u00b7ter", "lee\u00b7re", "wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand giebt ein spiegel an/", "tokens": ["Nie\u00b7mand", "giebt", "ein", "spie\u00b7gel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wer lust hat anzufangen/", "tokens": ["Und", "wer", "lust", "hat", "an\u00b7zu\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "VAFIN", "VVIZU", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Kan nicht audienz erlangen.", "tokens": ["Kan", "nicht", "au\u00b7di\u00b7enz", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "9. Nun di\u00df ist/ was ich beklage/", "tokens": ["Nun", "di\u00df", "ist", "/", "was", "ich", "be\u00b7kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df so eine sch\u00f6ne braut/", "tokens": ["Da\u00df", "so", "ei\u00b7ne", "sch\u00f6\u00b7ne", "braut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt an ihrem ehren-tage", "tokens": ["Jetzt", "an", "ih\u00b7rem", "eh\u00b7ren\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine bessre freude schaut/", "tokens": ["Kei\u00b7ne", "bess\u00b7re", "freu\u00b7de", "schaut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und da\u00df wir mit unsern sachen", "tokens": ["Und", "da\u00df", "wir", "mit", "un\u00b7sern", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine trauer-hochzeit machen", "tokens": ["Ei\u00b7ne", "trau\u00b7e\u00b7rhoch\u00b7zeit", "ma\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "10. Doch/ sie kan auf andre f\u00e4lle/", "tokens": ["Doch", "/", "sie", "kan", "auf", "and\u00b7re", "f\u00e4l\u00b7le", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VMFIN", "APPR", "PIS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach belieben lustig seyn/", "tokens": ["Nach", "be\u00b7lie\u00b7ben", "lus\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der neue schlaff-geselle", "tokens": ["Denn", "der", "neu\u00b7e", "schlaff\u00b7ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6mmt herf\u00fcr und stellt sich ein/", "tokens": ["K\u00f6mmt", "her\u00b7f\u00fcr", "und", "stellt", "sich", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PRF", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser wird sie schon ergetzen", "tokens": ["Die\u00b7ser", "wird", "sie", "schon", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den mangel wohl ersetzen.", "tokens": ["Und", "den", "man\u00b7gel", "wohl", "er\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}