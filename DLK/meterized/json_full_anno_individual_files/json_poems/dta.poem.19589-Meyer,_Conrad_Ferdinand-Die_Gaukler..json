{"dta.poem.19589": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Gaukler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.85", "nl:0.14"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Am Strande des gelobten Lands", "tokens": ["Am", "Stran\u00b7de", "des", "ge\u00b7lob\u00b7ten", "Lands"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In gl\u00fchem Stich des Sonnenbrands", "tokens": ["In", "gl\u00fc\u00b7hem", "Stich", "des", "Son\u00b7nen\u00b7brands"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00e4mpft Ludowig der Fromme;", "tokens": ["K\u00e4mpft", "Lu\u00b7do\u00b7wig", "der", "From\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Er tr\u00e4gt in sich des Todes Keim,", "tokens": ["Er", "tr\u00e4gt", "in", "sich", "des", "To\u00b7des", "Keim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm ahnt es, da\u00df er nimmer heim", "tokens": ["Ihm", "ahnt", "es", ",", "da\u00df", "er", "nim\u00b7mer", "heim"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins sch\u00f6ne Frankreich komme.", "tokens": ["Ins", "sch\u00f6\u00b7ne", "Fran\u00b7kreich", "kom\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Scheu lauscht in Zeltes D\u00e4mmerschein", "tokens": ["Scheu", "lauscht", "in", "Zel\u00b7tes", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein junger Edelknecht herein", "tokens": ["Ein", "jun\u00b7ger", "E\u00b7del\u00b7knecht", "her\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hinter ihm die andern:", "tokens": ["Und", "hin\u00b7ter", "ihm", "die", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eherr K\u00f6nig, es sind Gaukler da,", "tokens": ["\u201e", "herr", "K\u00f6\u00b7nig", ",", "es", "sind", "Gauk\u00b7ler", "da", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "VAFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drei Br\u00fcder aus Armenia,", "tokens": ["Drei", "Br\u00fc\u00b7der", "aus", "Ar\u00b7me\u00b7nia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die nach dem Grabe wandern.", "tokens": ["Die", "nach", "dem", "Gra\u00b7be", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es hei\u00dft, sie spielen wundersch\u00f6n!", "tokens": ["Es", "hei\u00dft", ",", "sie", "spie\u00b7len", "wun\u00b7der\u00b7sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erlaubt ein frisches Hornget\u00f6n", "tokens": ["Er\u00b7laubt", "ein", "fri\u00b7sches", "Horn\u00b7ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns allen anzuh\u00f6ren!\u201c", "tokens": ["Uns", "al\u00b7len", "an\u00b7zu\u00b7h\u00f6\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "PIS", "VVIZU", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der K\u00f6nig seufzt: \u201eBetrug der Welt!", "tokens": ["Der", "K\u00f6\u00b7nig", "seufzt", ":", "\u201e", "Be\u00b7trug", "der", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bringt mir die Gaukler in das Zelt,", "tokens": ["Bringt", "mir", "die", "Gauk\u00b7ler", "in", "das", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie euch nicht beth\u00f6ren!\u201c", "tokens": ["Da\u00df", "sie", "euch", "nicht", "be\u00b7th\u00f6\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt heben an den Mund die Drei", "tokens": ["Jetzt", "he\u00b7ben", "an", "den", "Mund", "die", "Drei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Horn und spielen frank und frei,", "tokens": ["Das", "Horn", "und", "spie\u00b7len", "frank", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ging' es aus zum Jagen.", "tokens": ["Als", "ging'", "es", "aus", "zum", "Ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann wie ein Quell im Walde quillt,", "tokens": ["Dann", "wie", "ein", "Quell", "im", "Wal\u00b7de", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So rieselt sanft und w\u00e4chst und schwillt", "tokens": ["So", "rie\u00b7selt", "sanft", "und", "w\u00e4chst", "und", "schwillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Jubeln und ein Klagen.", "tokens": ["Ein", "Ju\u00b7beln", "und", "ein", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gemach vert\u00f6nt der H\u00f6rner Schall,", "tokens": ["Ge\u00b7mach", "ver\u00b7t\u00f6nt", "der", "H\u00f6r\u00b7ner", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut ruft Renaud von Reineval:", "tokens": ["Laut", "ruft", "Re\u00b7naud", "von", "Rei\u00b7ne\u00b7val", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edu Herzenstrost der Minne!", "tokens": ["\u201e", "du", "Her\u00b7zen\u00b7strost", "der", "Min\u00b7ne", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lucinden, die sich um mich kr\u00e4nkt,", "tokens": ["Lu\u00b7cin\u00b7den", ",", "die", "sich", "um", "mich", "kr\u00e4nkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Treuen ihres Pilgers denkt,", "tokens": ["In", "Treu\u00b7en", "ih\u00b7res", "Pil\u00b7gers", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sah ich auf stiller Zinne!\u201c", "tokens": ["Sah", "ich", "auf", "stil\u00b7ler", "Zin\u00b7ne", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201eich schaute\u201c, f\u00e4llt jung Walter ein,", "tokens": ["\u201e", "ich", "schau\u00b7te", "\u201c", ",", "f\u00e4llt", "jung", "Wal\u00b7ter", "ein", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "$,", "VVFIN", "ADJD", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ein meinem Teich den Wiederschein", "tokens": ["\u201e", "in", "mei\u00b7nem", "Teich", "den", "Wie\u00b7der\u00b7schein"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Eichen k\u00fchl und d\u00fcster,", "tokens": ["Von", "Ei\u00b7chen", "k\u00fchl", "und", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sah mein Boot, der Ruder bar,", "tokens": ["Ich", "sah", "mein", "Boot", ",", "der", "Ru\u00b7der", "bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das halb ans Land gezogen war,", "tokens": ["Das", "halb", "ans", "Land", "ge\u00b7zo\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umneigt von Schilfgefl\u00fcster!\u201c", "tokens": ["Um\u00b7neigt", "von", "Schilf\u00b7ge\u00b7fl\u00fcs\u00b7ter", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein Jeder hat im Horneslaut", "tokens": ["Ein", "Je\u00b7der", "hat", "im", "Hor\u00b7nes\u00b7laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Herz belauscht, sein Lieb geschaut,", "tokens": ["Sein", "Herz", "be\u00b7lauscht", ",", "sein", "Lieb", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Minnen und sein Sehnen.", "tokens": ["Sein", "Min\u00b7nen", "und", "sein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u2014 \u201eHerr K\u00f6nig, sagt, was sinnet Ihr?", "tokens": ["\u201e", "Herr", "K\u00f6\u00b7nig", ",", "sagt", ",", "was", "sin\u00b7net", "Ihr", "?"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "NN", "$,", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Was sehnet Ihr? Was minnet Ihr?", "tokens": ["Was", "seh\u00b7net", "Ihr", "?", "Was", "min\u00b7net", "Ihr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was rinnen Euch die Thr\u00e4nen?\u201c", "tokens": ["Was", "rin\u00b7nen", "Euch", "die", "Thr\u00e4\u00b7nen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Herr Ludwig fl\u00fcstert: \u201eSel'ger Traum!", "tokens": ["Herr", "Lud\u00b7wig", "fl\u00fcs\u00b7tert", ":", "\u201e", "Sel'\u00b7ger", "Traum", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich hoben durch den Himmelsraum", "tokens": ["Mich", "ho\u00b7ben", "durch", "den", "Him\u00b7mels\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Angelische Gestalten.", "tokens": ["An\u00b7ge\u00b7li\u00b7sche", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u201egetreuer Knecht, willkomm!\u201c erscholl", "tokens": ["\u201e", "ge\u00b7treu\u00b7er", "Knecht", ",", "will\u00b7komm", "!", "\u201c", "er\u00b7scholl"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word"], "pos": ["$(", "ADJA", "NN", "$,", "ADJD", "$.", "$(", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Ruf \u2014 ich konnte wonnevoll", "tokens": ["Ein", "Ruf", "ich", "konn\u00b7te", "won\u00b7ne\u00b7voll"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PPER", "VMFIN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Thr\u00e4nen nicht verhalten.\u201c", "tokens": ["Die", "Thr\u00e4\u00b7nen", "nicht", "ver\u00b7hal\u00b7ten", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}