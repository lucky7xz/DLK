{"textgrid.poem.47443": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich m\u00f6chte dir so gern die Seele geben]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte dir so gern die Seele geben;", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dir", "so", "gern", "die", "See\u00b7le", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch hast du sie durch ewigen Beschlu\u00df", "tokens": ["Doch", "hast", "du", "sie", "durch", "e\u00b7wi\u00b7gen", "Be\u00b7schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon ohnehin, und nicht bin ich im Stande", "tokens": ["Schon", "oh\u00b7ne\u00b7hin", ",", "und", "nicht", "bin", "ich", "im", "Stan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KON", "PTKNEG", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu sagen, da\u00df sie je mein eigen war.", "tokens": ["Zu", "sa\u00b7gen", ",", "da\u00df", "sie", "je", "mein", "ei\u00b7gen", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geschaffen hat, so viel ich mich besinne,", "tokens": ["Ge\u00b7schaf\u00b7fen", "hat", ",", "so", "viel", "ich", "mich", "be\u00b