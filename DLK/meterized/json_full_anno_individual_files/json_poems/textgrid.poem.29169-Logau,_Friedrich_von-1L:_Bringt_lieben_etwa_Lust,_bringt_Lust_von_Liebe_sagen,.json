{"textgrid.poem.29169": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bringt lieben etwa Lust, bringt Lust von Liebe sagen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bringt lieben etwa Lust, bringt Lust von Liebe sagen,", "tokens": ["Bringt", "lie\u00b7ben", "et\u00b7wa", "Lust", ",", "bringt", "Lust", "von", "Lie\u00b7be", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ADV", "NN", "$,", "VVFIN", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bringt beydes dennoch mir nichts als nur Bittrigkeit;", "tokens": ["Bringt", "bey\u00b7des", "den\u00b7noch", "mir", "nichts", "als", "nur", "Bitt\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPER", "PIS", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was andren Hertzens-Wonn ist mir nur Hertzens-Leid;", "tokens": ["Was", "an\u00b7dren", "Hert\u00b7zens\u00b7Wonn", "ist", "mir", "nur", "Hert\u00b7zens\u00b7Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann meine Lieb ist l\u00e4ngst zu Grabe weg getragen,", "tokens": ["Dann", "mei\u00b7ne", "Lieb", "ist", "l\u00e4ngst", "zu", "Gra\u00b7be", "weg", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "