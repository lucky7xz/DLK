{"textgrid.poem.59665": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An die Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein, V\u00f6glein in den L\u00fcften,", "tokens": ["V\u00f6\u00b7glein", ",", "V\u00f6\u00b7glein", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lerche, die zum Himmel schwebt,", "tokens": ["Ler\u00b7che", ",", "die", "zum", "Him\u00b7mel", "schwebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unten still in Blumend\u00fcften", "tokens": ["Un\u00b7ten", "still", "in", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Gr\u00fcn der Wiesen lebt,", "tokens": ["Und", "im", "Gr\u00fcn", "der", "Wie\u00b7sen", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du bist mein, du s\u00fc\u00dfe Kehle,", "tokens": ["Du", "bist", "mein", ",", "du", "s\u00fc\u00b7\u00dfe", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Sehnsucht, meine Lust,", "tokens": ["Mei\u00b7ne", "Sehn\u00b7sucht", ",",