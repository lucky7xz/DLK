{"textgrid.poem.54261": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die H\u00f6hnische Lisette", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Frauen-Volck pflegt zwar mit Hecheln umzugehen,", "tokens": ["Das", "Frau\u00b7en\u00b7Volck", "pflegt", "zwar", "mit", "He\u00b7cheln", "um\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man sie h\u00e4u\u00dflich sieht bey Flachs besch\u00e4fftget stehen,", "tokens": ["Wenn", "man", "sie", "h\u00e4u\u00df\u00b7lich", "sieht", "bey", "Flachs", "be\u00b7sch\u00e4fft\u00b7get", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVFIN", "APPR", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch Lisetten kommt hierinnen keine bey,", "tokens": ["Je\u00b7doch", "Li\u00b7set\u00b7ten", "kommt", "hie\u00b7rin\u00b7nen", "kei\u00b7ne", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PAV", "PIAT", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So flei\u00dfig und bem\u00fcht sie auch im Hause sey.", "tokens": ["So", "flei\u00b7\u00dfig", "und", "be\u00b7m\u00fcht", "sie", "auch", "im", "Hau\u00b7se", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Warum? Ihr Mund mu\u00df ihr statt einer Hechel dienen,", "tokens": ["Wa\u00b7rum", "?", "Ihr", "Mund", "mu\u00df", "ihr", "statt", "ei\u00b7ner", "He\u00b7chel", "die\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "H\u00f6rt nur das L\u00e4ster-Maul, gebt Acht auf ihre Minen,", "tokens": ["H\u00f6rt", "nur", "das", "L\u00e4s\u00b7ter\u00b7Maul", ",", "gebt", "Acht", "auf", "ih\u00b7re", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "VVFIN", "CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sagt mir, ist wohl ein Mensch, in unsrer gantzen Stadt,", "tokens": ["Sagt", "mir", ",", "ist", "wohl", "ein", "Mensch", ",", "in", "uns\u00b7rer", "gant\u00b7zen", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "ADV", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der ihre Hechel nicht mit Schmertz gef\u00fchlet hat?", "tokens": ["Der", "ih\u00b7re", "He\u00b7chel", "nicht", "mit", "Schmertz", "ge\u00b7f\u00fch\u00b7let", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein was n\u00fctzt ihr denn ihr gifftger Schlund und Rachen?", "tokens": ["Al\u00b7lein", "was", "n\u00fctzt", "ihr", "denn", "ihr", "gifft\u00b7ger", "Schlund", "und", "Ra\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So viel, da\u00df ieder wird vor ihr ein Creutze machen.", "tokens": ["So", "viel", ",", "da\u00df", "ie\u00b7der", "wird", "vor", "ihr", "ein", "Creut\u00b7ze", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIS", "VAFIN", "APPR", "PPOSAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es l\u00e4st aus Furcht mit ihr sich niemand weiter ein.", "tokens": ["Es", "l\u00e4st", "aus", "Furcht", "mit", "ihr", "sich", "nie\u00b7mand", "wei\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPER", "PRF", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Lisette, warlich, ist ein sch\u00e4dlich Stachel-Schwein.", "tokens": ["Li\u00b7set\u00b7te", ",", "war\u00b7lich", ",", "ist", "ein", "sch\u00e4d\u00b7lich", "Sta\u00b7chel\u00b7Schwein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "$,", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Frauen-Volck pflegt zwar mit Hecheln umzugehen,", "tokens": ["Das", "Frau\u00b7en\u00b7Volck", "pflegt", "zwar", "mit", "He\u00b7cheln", "um\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word