{"dta.poem.18240": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling wieder die harrende Welt;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wie\u00b7der", "die", "har\u00b7ren\u00b7de", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wo er sich zeigt, da singt es,", "tokens": ["Und", "wo", "er", "sich", "zeigt", ",", "da", "singt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wo er nur wandert, da klingt es,", "tokens": ["Und", "wo", "er", "nur", "wan\u00b7dert", ",", "da", "klingt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Jauchzend zum Himmelszelt.", "tokens": ["Jauch\u00b7zend", "zum", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}