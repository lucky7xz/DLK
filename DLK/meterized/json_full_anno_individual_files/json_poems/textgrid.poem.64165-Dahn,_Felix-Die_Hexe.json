{"textgrid.poem.64165": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Hexe", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du ein Hexlein richten soll't, blick' nicht ihr in die Augen,", "tokens": ["Wenn", "du", "ein", "Hex\u00b7lein", "rich\u00b7ten", "soll't", ",", "blick'", "nicht", "ihr", "in", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,", "VVFIN", "PTKNEG", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sonst wird dein t\u00f6richt Herz ihr hold, kann nicht zum Richten taugen.", "tokens": ["Sonst", "wird", "dein", "t\u00f6\u00b7richt", "Herz", "ihr", "hold", ",", "kann", "nicht", "zum", "Rich\u00b7ten", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "PPER", "ADJD", "$,", "VMFIN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Das hat den Burggraf von Tirol gef\u00fchrt in Tod und Schande:", "tokens": ["Das", "hat", "den", "Burg\u00b7graf", "von", "Ti\u00b7rol", "ge\u00b7f\u00fchrt", "in", "Tod", "und", "Schan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der war ein junger Ritter wohl und Richter in dem Lande.", "tokens": ["Der", "war", "ein", "jun\u00b7ger", "Rit\u00b7ter", "wohl", "und", "Rich\u00b7ter", "in", "dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Zu Bozen an dem schwarzen Stein, da sa\u00dfen Sch\u00f6ffen elfe: \u2013", "tokens": ["Zu", "Bo\u00b7zen", "an", "dem", "schwar\u00b7zen", "Stein", ",", "da", "sa\u00b7\u00dfen", "Sch\u00f6f\u00b7fen", "el\u00b7fe", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdie Hexe mu\u00df verbronnen sein\u00ab \u2013 sprach er \u2013 \u00bbso Gott mir helfe.", "tokens": ["\u00bb", "die", "He\u00b7xe", "mu\u00df", "ver\u00b7bron\u00b7nen", "sein", "\u00ab", "\u2013", "sprach", "er", "\u2013", "\u00bb", "so", "Gott", "mir", "hel\u00b7fe", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVPP", "VAINF", "$(", "$(", "VVFIN", "PPER", "$(", "$(", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Du Kl\u00e4gerin, sag' an geschwind, wes willst du sie bezichten?\u00ab", "tokens": ["Du", "Kl\u00e4\u00b7ge\u00b7rin", ",", "sag'", "an", "ge\u00b7schwind", ",", "wes", "willst", "du", "sie", "be\u00b7zich\u00b7ten", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "APPR", "ADJD", "$,", "PWS", "VMFIN", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbsie ist ein h\u00f6llisch Wechselkind, ihr Trachten b\u00f6s und Dichten.", "tokens": ["\u00bb", "sie", "ist", "ein", "h\u00f6l\u00b7lisch", "Wech\u00b7sel\u00b7kind", ",", "ihr", "Trach\u00b7ten", "b\u00f6s", "und", "Dich\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJD", "VAFIN", "$,", "PPOSAT", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Sie hat eine scheue stille Art, das Mannsvolk zu bet\u00f6ren,", "tokens": ["Sie", "hat", "ei\u00b7ne", "scheu\u00b7e", "stil\u00b7le", "Art", ",", "das", "Manns\u00b7volk", "zu", "be\u00b7t\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und wen sie anblickt stumm und zart, der mu\u00df ihr angeh\u00f6ren.", "tokens": ["Und", "wen", "sie", "an\u00b7blickt", "stumm", "und", "zart", ",", "der", "mu\u00df", "ihr", "an\u00b7ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "ART", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Meinem Eh'herrn hat sie's angetan mit ihrem schwarzen Blicke:", "tokens": ["Mei\u00b7nem", "Eh'\u00b7herrn", "hat", "sie's", "an\u00b7ge\u00b7tan", "mit", "ih\u00b7rem", "schwar\u00b7zen", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Er folgt ihr nach auf Weg und Bahn, als f\u00fchrt' sie ihn am Stricke.", "tokens": ["Er", "folgt", "ihr", "nach", "auf", "Weg", "und", "Bahn", ",", "als", "f\u00fchrt'", "sie", "ihn", "am", "Stri\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Der Fischer Kurt sprang in den See, \u2013 so wild mu\u00df't er sie lieben,", "tokens": ["Der", "Fi\u00b7scher", "Kurt", "sprang", "in", "den", "See", ",", "\u2013", "so", "wild", "mu\u00df't", "er", "sie", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,", "$(", "ADV", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Den Sch\u00fctz von Klausen hat's vor Weh' in Kampf und Tod getrieben;", "tokens": ["Den", "Sch\u00fctz", "von", "Klau\u00b7sen", "hat's", "vor", "Weh'", "in", "Kampf", "und", "Tod", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "In Kirch' und Messe geht sie nicht, ein Greu'l sind ihr die Glocken,", "tokens": ["In", "Kirch'", "und", "Mes\u00b7se", "geht", "sie", "nicht", ",", "ein", "Greu'l", "sind", "ihr", "die", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und gr\u00fcnes Zauberkraut sie flicht in ihre schwarzen Locken.", "tokens": ["Und", "gr\u00fc\u00b7nes", "Zau\u00b7ber\u00b7kraut", "sie", "flicht", "in", "ih\u00b7re", "schwar\u00b7zen", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Man wei\u00df es nicht, woher sie kam, fremd ist ihr bunt Gewande,", "tokens": ["Man", "wei\u00df", "es", "nicht", ",", "wo\u00b7her", "sie", "kam", ",", "fremd", "ist", "ihr", "bunt", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ihre Sprach' ist fremd und wundersam, sie hat kein Recht im Lande.\u00ab", "tokens": ["Ih\u00b7re", "Sprach'", "ist", "fremd", "und", "wun\u00b7der\u00b7sam", ",", "sie", "hat", "kein", "Recht", "im", "Lan\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "\u00bbihr Sch\u00f6ffen, die das Recht ihr kennt, nun heisch' ich eure Stimmen!\u00ab \u2013", "tokens": ["\u00bb", "ihr", "Sch\u00f6f\u00b7fen", ",", "die", "das", "Recht", "ihr", "kennt", ",", "nun", "heisch'", "ich", "eu\u00b7re", "Stim\u00b7men", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdas Recht ist: eh' die Hexe brennt, soll erst die Hexe schwimmen;", "tokens": ["\u00bb", "das", "Recht", "ist", ":", "eh'", "die", "He\u00b7xe", "brennt", ",", "soll", "erst", "die", "He\u00b7xe", "schwim\u00b7men", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "$.", "KOUS", "ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.11": {"line.1": {"text": "Werft sie gebunden in den Teich, die Hexe kann nicht sinken,", "tokens": ["Werft", "sie", "ge\u00b7bun\u00b7den", "in", "den", "Teich", ",", "die", "He\u00b7xe", "kann", "nicht", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Der Teufel tr\u00e4gt sie federgleich und l\u00e4\u00dft sie nicht ertrinken.\u00ab \u2013", "tokens": ["Der", "Teu\u00b7fel", "tr\u00e4gt", "sie", "fe\u00b7derg\u00b7leich", "und", "l\u00e4\u00dft", "sie", "nicht", "er\u00b7trin\u00b7ken", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.12": {"line.1": {"text": "Und von dem Stein der Burggraf schritt mit allem Volk zum Weiher:", "tokens": ["Und", "von", "dem", "Stein", "der", "Burg\u00b7graf", "schritt", "mit", "al\u00b7lem", "Volk", "zum", "Wei\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "PIS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Zwei Schergen schleppten die Hexe mit, geh\u00fcllt in dunkle Schleier.", "tokens": ["Zwei", "Scher\u00b7gen", "schlepp\u00b7ten", "die", "He\u00b7xe", "mit", ",", "ge\u00b7h\u00fcllt", "in", "dunk\u00b7le", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbhalt \u2013 la\u00dft mich erst dem Teufelskind in die Koboldaugen schauen:", "tokens": ["\u00bb", "halt", "\u2013", "la\u00dft", "mich", "erst", "dem", "Teu\u00b7fels\u00b7kind", "in", "die", "Ko\u00b7bol\u00b7dau\u00b7gen", "schau\u00b7en", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "VVIMP", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+--+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Und ob sie Zauberkohlen sind, \u2013 mir soll davor nicht grauen.\u00ab \u2013", "tokens": ["Und", "ob", "sie", "Zau\u00b7ber\u00b7koh\u00b7len", "sind", ",", "\u2013", "mir", "soll", "da\u00b7vor", "nicht", "grau\u00b7en", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "$(", "PPER", "VMFIN", "PAV", "PTKNEG", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.14": {"line.1": {"text": "Er rei\u00dft den Schleier fort mit Macht: \u2013 da war's um ihn geschehen: \u2013", "tokens": ["Er", "rei\u00dft", "den", "Schlei\u00b7er", "fort", "mit", "Macht", ":", "\u2013", "da", "wa\u00b7r's", "um", "ihn", "ge\u00b7sche\u00b7hen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "NN", "$.", "$(", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Zwei schwarze Augen voll s\u00fc\u00dfer Nacht, die haben ihn angesehen.", "tokens": ["Zwei", "schwar\u00b7ze", "Au\u00b7gen", "voll", "s\u00fc\u00b7\u00dfer", "Nacht", ",", "die", "ha\u00b7ben", "ihn", "an\u00b7ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,", "PRELS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.15": {"line.1": {"text": "Sie kreuzt auf ihrer Brust die Arm', ihr dunkles Haar wallt pr\u00e4chtig,", "tokens": ["Sie", "kreuzt", "auf", "ih\u00b7rer", "Brust", "die", "Arm'", ",", "ihr", "dunk\u00b7les", "Haar", "wallt", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sie blicket auf in Todesharm: \u2013 der Blick war zauberm\u00e4chtig!", "tokens": ["Sie", "bli\u00b7cket", "auf", "in", "To\u00b7des\u00b7harm", ":", "\u2013", "der", "Blick", "war", "zau\u00b7ber\u00b7m\u00e4ch\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "NN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.16": {"line.1": {"text": "Er hielt die Hand vors Angesicht, er t\u00e4t sich ba\u00df verf\u00e4rben:", "tokens": ["Er", "hielt", "die", "Hand", "vors", "An\u00b7ge\u00b7sicht", ",", "er", "t\u00e4t", "sich", "ba\u00df", "ver\u00b7f\u00e4r\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbhalt! \u2013 Sie ist keine Hexe nicht! \u2013 Sie ist rein! \u2013 Sie soll nicht sterben!\u00ab \u2013", "tokens": ["\u00bb", "halt", "!", "\u2013", "Sie", "ist", "kei\u00b7ne", "He\u00b7xe", "nicht", "!", "\u2013", "Sie", "ist", "rein", "!", "\u2013", "Sie", "soll", "nicht", "ster\u00b7ben", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$.", "$(", "PPER", "VAFIN", "ADJD", "$.", "$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "$(", "$("], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}}, "stanza.17": {"line.1": {"text": "\u00bbdie Hexe mu\u00df verbronnen sein!\u00ab \u2013 So sprachen da die Elfe \u2013", "tokens": ["\u00bb", "die", "He\u00b7xe", "mu\u00df", "ver\u00b7bron\u00b7nen", "sein", "!", "\u00ab", "\u2013", "So", "spra\u00b7chen", "da", "die", "El\u00b7fe", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVPP", "VAINF", "$.", "$(", "$(", "ADV", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdu bist behext: \u2013 gedenke fein: du schwurst, so Gott dir helfe!\u00ab", "tokens": ["\u00bb", "du", "bist", "be\u00b7hext", ":", "\u2013", "ge\u00b7den\u00b7ke", "fein", ":", "du", "schwurst", ",", "so", "Gott", "dir", "hel\u00b7fe", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$.", "$(", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "$,", "ADV", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.18": {"line.1": {"text": "Sie halten dem Grafen Schwert und Hand, sie zerren sie fort zum Weiher \u2013", "tokens": ["Sie", "hal\u00b7ten", "dem", "Gra\u00b7fen", "Schwert", "und", "Hand", ",", "sie", "zer\u00b7ren", "sie", "fort", "zum", "Wei\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$("], "meter": "-+--+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und als er sich zornig losgewandt, \u2013 im Wasser schwamm ihr Schleier.", "tokens": ["Und", "als", "er", "sich", "zor\u00b7nig", "los\u00b7ge\u00b7wandt", ",", "\u2013", "im", "Was\u00b7ser", "schwamm", "ihr", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,", "$(", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "----+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.19": {"line.1": {"text": "Er springt ihr nach, er fa\u00dft sie wohl: \u2013 da t\u00e4ten sie beide sinken: \u2013", "tokens": ["Er", "springt", "ihr", "nach", ",", "er", "fa\u00dft", "sie", "wohl", ":", "\u2013", "da", "t\u00e4\u00b7ten", "sie", "bei\u00b7de", "sin\u00b7ken", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "So mu\u00dfte der Burggraf von Tirol um eine Hex' ertrinken. \u2013", "tokens": ["So", "mu\u00df\u00b7te", "der", "Burg\u00b7graf", "von", "Ti\u00b7rol", "um", "ei\u00b7ne", "He\u00b7x'", "er\u00b7trin\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "NE", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+--+--+-+-", "measure": "amphibrach.penta.plus"}}, "stanza.20": {"line.1": {"text": "Wenn du ein Hexlein richten soll't, blick' nicht ihr in die Augen,", "tokens": ["Wenn", "du", "ein", "Hex\u00b7lein", "rich\u00b7ten", "soll't", ",", "blick'", "nicht", "ihr", "in", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,", "VVFIN", "PTKNEG", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sonst wird dein t\u00f6richt Herz ihr hold, kann nicht zum Richten taugen.", "tokens": ["Sonst", "wird", "dein", "t\u00f6\u00b7richt", "Herz", "ihr", "hold", ",", "kann", "nicht", "zum", "Rich\u00b7ten", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "PPER", "ADJD", "$,", "VMFIN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.21": {"line.1": {"text": "Das hat den Burggraf von Tirol gef\u00fchrt in Tod und Schande:", "tokens": ["Das", "hat", "den", "Burg\u00b7graf", "von", "Ti\u00b7rol", "ge\u00b7f\u00fchrt", "in", "Tod", "und", "Schan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der war ein junger Ritter wohl und Richter in dem Lande.", "tokens": ["Der", "war", "ein", "jun\u00b7ger", "Rit\u00b7ter", "wohl", "und", "Rich\u00b7ter", "in", "dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.22": {"line.1": {"text": "Zu Bozen an dem schwarzen Stein, da sa\u00dfen Sch\u00f6ffen elfe: \u2013", "tokens": ["Zu", "Bo\u00b7zen", "an", "dem", "schwar\u00b7zen", "Stein", ",", "da", "sa\u00b7\u00dfen", "Sch\u00f6f\u00b7fen", "el\u00b7fe", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdie Hexe mu\u00df verbronnen sein\u00ab \u2013 sprach er \u2013 \u00bbso Gott mir helfe.", "tokens": ["\u00bb", "die", "He\u00b7xe", "mu\u00df", "ver\u00b7bron\u00b7nen", "sein", "\u00ab", "\u2013", "sprach", "er", "\u2013", "\u00bb", "so", "Gott", "mir", "hel\u00b7fe", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVPP", "VAINF", "$(", "$(", "VVFIN", "PPER", "$(", "$(", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.23": {"line.1": {"text": "Du Kl\u00e4gerin, sag' an geschwind, wes willst du sie bezichten?\u00ab", "tokens": ["Du", "Kl\u00e4\u00b7ge\u00b7rin", ",", "sag'", "an", "ge\u00b7schwind", ",", "wes", "willst", "du", "sie", "be\u00b7zich\u00b7ten", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "APPR", "ADJD", "$,", "PWS", "VMFIN", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbsie ist ein h\u00f6llisch Wechselkind, ihr Trachten b\u00f6s und Dichten.", "tokens": ["\u00bb", "sie", "ist", "ein", "h\u00f6l\u00b7lisch", "Wech\u00b7sel\u00b7kind", ",", "ihr", "Trach\u00b7ten", "b\u00f6s", "und", "Dich\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJD", "VAFIN", "$,", "PPOSAT", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.24": {"line.1": {"text": "Sie hat eine scheue stille Art, das Mannsvolk zu bet\u00f6ren,", "tokens": ["Sie", "hat", "ei\u00b7ne", "scheu\u00b7e", "stil\u00b7le", "Art", ",", "das", "Manns\u00b7volk", "zu", "be\u00b7t\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und wen sie anblickt stumm und zart, der mu\u00df ihr angeh\u00f6ren.", "tokens": ["Und", "wen", "sie", "an\u00b7blickt", "stumm", "und", "zart", ",", "der", "mu\u00df", "ihr", "an\u00b7ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "ART", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.25": {"line.1": {"text": "Meinem Eh'herrn hat sie's angetan mit ihrem schwarzen Blicke:", "tokens": ["Mei\u00b7nem", "Eh'\u00b7herrn", "hat", "sie's", "an\u00b7ge\u00b7tan", "mit", "ih\u00b7rem", "schwar\u00b7zen", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Er folgt ihr nach auf Weg und Bahn, als f\u00fchrt' sie ihn am Stricke.", "tokens": ["Er", "folgt", "ihr", "nach", "auf", "Weg", "und", "Bahn", ",", "als", "f\u00fchrt'", "sie", "ihn", "am", "Stri\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.26": {"line.1": {"text": "Der Fischer Kurt sprang in den See, \u2013 so wild mu\u00df't er sie lieben,", "tokens": ["Der", "Fi\u00b7scher", "Kurt", "sprang", "in", "den", "See", ",", "\u2013", "so", "wild", "mu\u00df't", "er", "sie", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,", "$(", "ADV", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Den Sch\u00fctz von Klausen hat's vor Weh' in Kampf und Tod getrieben;", "tokens": ["Den", "Sch\u00fctz", "von", "Klau\u00b7sen", "hat's", "vor", "Weh'", "in", "Kampf", "und", "Tod", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.27": {"line.1": {"text": "In Kirch' und Messe geht sie nicht, ein Greu'l sind ihr die Glocken,", "tokens": ["In", "Kirch'", "und", "Mes\u00b7se", "geht", "sie", "nicht", ",", "ein", "Greu'l", "sind", "ihr", "die", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und gr\u00fcnes Zauberkraut sie flicht in ihre schwarzen Locken.", "tokens": ["Und", "gr\u00fc\u00b7nes", "Zau\u00b7ber\u00b7kraut", "sie", "flicht", "in", "ih\u00b7re", "schwar\u00b7zen", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.28": {"line.1": {"text": "Man wei\u00df es nicht, woher sie kam, fremd ist ihr bunt Gewande,", "tokens": ["Man", "wei\u00df", "es", "nicht", ",", "wo\u00b7her", "sie", "kam", ",", "fremd", "ist", "ihr", "bunt", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ihre Sprach' ist fremd und wundersam, sie hat kein Recht im Lande.\u00ab", "tokens": ["Ih\u00b7re", "Sprach'", "ist", "fremd", "und", "wun\u00b7der\u00b7sam", ",", "sie", "hat", "kein", "Recht", "im", "Lan\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.29": {"line.1": {"text": "\u00bbihr Sch\u00f6ffen, die das Recht ihr kennt, nun heisch' ich eure Stimmen!\u00ab \u2013", "tokens": ["\u00bb", "ihr", "Sch\u00f6f\u00b7fen", ",", "die", "das", "Recht", "ihr", "kennt", ",", "nun", "heisch'", "ich", "eu\u00b7re", "Stim\u00b7men", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdas Recht ist: eh' die Hexe brennt, soll erst die Hexe schwimmen;", "tokens": ["\u00bb", "das", "Recht", "ist", ":", "eh'", "die", "He\u00b7xe", "brennt", ",", "soll", "erst", "die", "He\u00b7xe", "schwim\u00b7men", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "$.", "KOUS", "ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.30": {"line.1": {"text": "Werft sie gebunden in den Teich, die Hexe kann nicht sinken,", "tokens": ["Werft", "sie", "ge\u00b7bun\u00b7den", "in", "den", "Teich", ",", "die", "He\u00b7xe", "kann", "nicht", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Der Teufel tr\u00e4gt sie federgleich und l\u00e4\u00dft sie nicht ertrinken.\u00ab \u2013", "tokens": ["Der", "Teu\u00b7fel", "tr\u00e4gt", "sie", "fe\u00b7derg\u00b7leich", "und", "l\u00e4\u00dft", "sie", "nicht", "er\u00b7trin\u00b7ken", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.31": {"line.1": {"text": "Und von dem Stein der Burggraf schritt mit allem Volk zum Weiher:", "tokens": ["Und", "von", "dem", "Stein", "der", "Burg\u00b7graf", "schritt", "mit", "al\u00b7lem", "Volk", "zum", "Wei\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "PIS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Zwei Schergen schleppten die Hexe mit, geh\u00fcllt in dunkle Schleier.", "tokens": ["Zwei", "Scher\u00b7gen", "schlepp\u00b7ten", "die", "He\u00b7xe", "mit", ",", "ge\u00b7h\u00fcllt", "in", "dunk\u00b7le", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.32": {"line.1": {"text": "\u00bbhalt \u2013 la\u00dft mich erst dem Teufelskind in die Koboldaugen schauen:", "tokens": ["\u00bb", "halt", "\u2013", "la\u00dft", "mich", "erst", "dem", "Teu\u00b7fels\u00b7kind", "in", "die", "Ko\u00b7bol\u00b7dau\u00b7gen", "schau\u00b7en", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "VVIMP", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+--+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Und ob sie Zauberkohlen sind, \u2013 mir soll davor nicht grauen.\u00ab \u2013", "tokens": ["Und", "ob", "sie", "Zau\u00b7ber\u00b7koh\u00b7len", "sind", ",", "\u2013", "mir", "soll", "da\u00b7vor", "nicht", "grau\u00b7en", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "$(", "PPER", "VMFIN", "PAV", "PTKNEG", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.33": {"line.1": {"text": "Er rei\u00dft den Schleier fort mit Macht: \u2013 da war's um ihn geschehen: \u2013", "tokens": ["Er", "rei\u00dft", "den", "Schlei\u00b7er", "fort", "mit", "Macht", ":", "\u2013", "da", "wa\u00b7r's", "um", "ihn", "ge\u00b7sche\u00b7hen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "NN", "$.", "$(", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Zwei schwarze Augen voll s\u00fc\u00dfer Nacht, die haben ihn angesehen.", "tokens": ["Zwei", "schwar\u00b7ze", "Au\u00b7gen", "voll", "s\u00fc\u00b7\u00dfer", "Nacht", ",", "die", "ha\u00b7ben", "ihn", "an\u00b7ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,", "PRELS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.34": {"line.1": {"text": "Sie kreuzt auf ihrer Brust die Arm', ihr dunkles Haar wallt pr\u00e4chtig,", "tokens": ["Sie", "kreuzt", "auf", "ih\u00b7rer", "Brust", "die", "Arm'", ",", "ihr", "dunk\u00b7les", "Haar", "wallt", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sie blicket auf in Todesharm: \u2013 der Blick war zauberm\u00e4chtig!", "tokens": ["Sie", "bli\u00b7cket", "auf", "in", "To\u00b7des\u00b7harm", ":", "\u2013", "der", "Blick", "war", "zau\u00b7ber\u00b7m\u00e4ch\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "NN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.35": {"line.1": {"text": "Er hielt die Hand vors Angesicht, er t\u00e4t sich ba\u00df verf\u00e4rben:", "tokens": ["Er", "hielt", "die", "Hand", "vors", "An\u00b7ge\u00b7sicht", ",", "er", "t\u00e4t", "sich", "ba\u00df", "ver\u00b7f\u00e4r\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbhalt! \u2013 Sie ist keine Hexe nicht! \u2013 Sie ist rein! \u2013 Sie soll nicht sterben!\u00ab \u2013", "tokens": ["\u00bb", "halt", "!", "\u2013", "Sie", "ist", "kei\u00b7ne", "He\u00b7xe", "nicht", "!", "\u2013", "Sie", "ist", "rein", "!", "\u2013", "Sie", "soll", "nicht", "ster\u00b7ben", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$.", "$(", "PPER", "VAFIN", "ADJD", "$.", "$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "$(", "$("], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}}, "stanza.36": {"line.1": {"text": "\u00bbdie Hexe mu\u00df verbronnen sein!\u00ab \u2013 So sprachen da die Elfe \u2013", "tokens": ["\u00bb", "die", "He\u00b7xe", "mu\u00df", "ver\u00b7bron\u00b7nen", "sein", "!", "\u00ab", "\u2013", "So", "spra\u00b7chen", "da", "die", "El\u00b7fe", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVPP", "VAINF", "$.", "$(", "$(", "ADV", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdu bist behext: \u2013 gedenke fein: du schwurst, so Gott dir helfe!\u00ab", "tokens": ["\u00bb", "du", "bist", "be\u00b7hext", ":", "\u2013", "ge\u00b7den\u00b7ke", "fein", ":", "du", "schwurst", ",", "so", "Gott", "dir", "hel\u00b7fe", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$.", "$(", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "$,", "ADV", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.37": {"line.1": {"text": "Sie halten dem Grafen Schwert und Hand, sie zerren sie fort zum Weiher \u2013", "tokens": ["Sie", "hal\u00b7ten", "dem", "Gra\u00b7fen", "Schwert", "und", "Hand", ",", "sie", "zer\u00b7ren", "sie", "fort", "zum", "Wei\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$("], "meter": "-+--+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und als er sich zornig losgewandt, \u2013 im Wasser schwamm ihr Schleier.", "tokens": ["Und", "als", "er", "sich", "zor\u00b7nig", "los\u00b7ge\u00b7wandt", ",", "\u2013", "im", "Was\u00b7ser", "schwamm", "ihr", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,", "$(", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "----+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.38": {"line.1": {"text": "Er springt ihr nach, er fa\u00dft sie wohl: \u2013 da t\u00e4ten sie beide sinken: \u2013", "tokens": ["Er", "springt", "ihr", "nach", ",", "er", "fa\u00dft", "sie", "wohl", ":", "\u2013", "da", "t\u00e4\u00b7ten", "sie", "bei\u00b7de", "sin\u00b7ken", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "So mu\u00dfte der Burggraf von Tirol um eine Hex' ertrinken. \u2013", "tokens": ["So", "mu\u00df\u00b7te", "der", "Burg\u00b7graf", "von", "Ti\u00b7rol", "um", "ei\u00b7ne", "He\u00b7x'", "er\u00b7trin\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "NE", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+--+--+-+-", "measure": "amphibrach.penta.plus"}}}}}