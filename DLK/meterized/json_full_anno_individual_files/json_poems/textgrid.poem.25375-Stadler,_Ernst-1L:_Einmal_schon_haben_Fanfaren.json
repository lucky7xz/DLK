{"textgrid.poem.25375": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einmal schon haben Fanfaren", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal schon haben Fanfaren", "tokens": ["Ein\u00b7mal", "schon", "ha\u00b7ben", "Fan\u00b7fa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "mein ungeduldiges Herz blutig gerissen,", "tokens": ["mein", "un\u00b7ge\u00b7dul\u00b7di\u00b7ges", "Herz", "blu\u00b7tig", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df es, aufsteigend wie ein Pferd,", "tokens": ["Da\u00df", "es", ",", "auf\u00b7stei\u00b7gend", "wie", "ein", "Pferd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sich w\u00fctend ins Gez\u00e4um verbissen.", "tokens": ["sich", "w\u00fc\u00b7tend", "ins", "Ge\u00b7z\u00e4um", "ver\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Damals schlug Tambourmarsch", "tokens": ["Da\u00b7mals", "schlug", "Tam\u00b7bour\u00b7marsch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "den Sturm auf allen Wegen,", "tokens": ["den", "Sturm", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und herrlichste Musik der Erde", "tokens": ["Und", "herr\u00b7lichs\u00b7te", "Mu\u00b7sik", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "hie\u00df uns Kugelregen.", "tokens": ["hie\u00df", "uns", "Ku\u00b7gel\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Dann, pl\u00f6tzlich, stand Leben stille.", "tokens": ["Dann", ",", "pl\u00f6tz\u00b7lich", ",", "stand", "Le\u00b7ben", "stil\u00b7le", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wege f\u00fchrten zwischen alten B\u00e4umen.", "tokens": ["We\u00b7ge", "f\u00fchr\u00b7ten", "zwi\u00b7schen", "al\u00b7ten", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Gem\u00e4cher lockten.", "tokens": ["Ge\u00b7m\u00e4\u00b7cher", "lock\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Es war s\u00fc\u00df, zu weilen und sich vers\u00e4umen,", "tokens": ["Es", "war", "s\u00fc\u00df", ",", "zu", "wei\u00b7len", "und", "sich", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PTKZU", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Von Wirklichkeit den Leib", "tokens": ["Von", "Wirk\u00b7lich\u00b7keit", "den", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "so wie von staubiger R\u00fcstung zu entketten,", "tokens": ["so", "wie", "von", "stau\u00b7bi\u00b7ger", "R\u00fcs\u00b7tung", "zu", "ent\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Woll\u00fcstig sich in Daunen", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "sich", "in", "Dau\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PRF", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.16": {"text": "weicher Traumstunden einzubetten.", "tokens": ["wei\u00b7cher", "Traum\u00b7stun\u00b7den", "ein\u00b7zu\u00b7bet\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Aber eines Morgens", "tokens": ["A\u00b7ber", "ei\u00b7nes", "Mor\u00b7gens"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "rollte durch Nebelluft das Echo von Signalen,", "tokens": ["roll\u00b7te", "durch", "Ne\u00b7bel\u00b7luft", "das", "E\u00b7cho", "von", "Sig\u00b7na\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Hart, scharf, wie Schwerthieb pfeifend. Es war", "tokens": ["Hart", ",", "scharf", ",", "wie", "Schwert\u00b7hieb", "pfei\u00b7fend", ".", "Es", "war"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "PWAV", "NN", "VVPP", "$.", "PPER", "VAFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "wie wenn im Dunkel pl\u00f6tzlich Lichter aufstrahlen.", "tokens": ["wie", "wenn", "im", "Dun\u00b7kel", "pl\u00f6tz\u00b7lich", "Lich\u00b7ter", "auf\u00b7strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPRART", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.21": {"text": "Es war wie wenn durch Biwakfr\u00fche", "tokens": ["Es", "war", "wie", "wenn", "durch", "Bi\u00b7wak\u00b7fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Trompetenst\u00f6\u00dfe klirren,", "tokens": ["Trom\u00b7pe\u00b7ten\u00b7st\u00f6\u00b7\u00dfe", "klir\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die Schlafenden aufspringen und die Zelte abschlagen", "tokens": ["Die", "Schla\u00b7fen\u00b7den", "auf\u00b7sprin\u00b7gen", "und", "die", "Zel\u00b7te", "ab\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-+--", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "und die Pferde schirren.", "tokens": ["und", "die", "Pfer\u00b7de", "schir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Ich war in Reihen eingeschient,", "tokens": ["Ich", "war", "in", "Rei\u00b7hen", "ein\u00b7ge\u00b7schient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "die in den Morgen stie\u00dfen, Feuer \u00fcber Helm und B\u00fcgel,", "tokens": ["die", "in", "den", "Mor\u00b7gen", "stie\u00b7\u00dfen", ",", "Feu\u00b7er", "\u00fc\u00b7ber", "Helm", "und", "B\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.27": {"text": "Vorw\u00e4rts, in Blick und Blut die Schlacht,", "tokens": ["Vor\u00b7w\u00e4rts", ",", "in", "Blick", "und", "Blut", "die", "Schlacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "mit vorgehaltnem Z\u00fcgel.", "tokens": ["mit", "vor\u00b7ge\u00b7halt\u00b7nem", "Z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Vielleicht w\u00fcrden uns", "tokens": ["Viel\u00b7leicht", "w\u00fcr\u00b7den", "uns"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.30": {"text": "am Abend Siegesm\u00e4rsche umstreichen,", "tokens": ["am", "A\u00b7bend", "Sie\u00b7ges\u00b7m\u00e4r\u00b7sche", "um\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Vielleicht l\u00e4gen wir irgendwo ausgestreckt", "tokens": ["Viel\u00b7leicht", "l\u00e4\u00b7gen", "wir", "ir\u00b7gend\u00b7wo", "aus\u00b7ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "unter Leichen.", "tokens": ["un\u00b7ter", "Lei\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "Aber vor dem Erraffen", "tokens": ["A\u00b7ber", "vor", "dem", "Er\u00b7raf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.34": {"text": "und vor dem Versinken", "tokens": ["und", "vor", "dem", "Ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "W\u00fcrden unsre Augen sich an Welt und Sonne satt", "tokens": ["W\u00fcr\u00b7den", "uns\u00b7re", "Au\u00b7gen", "sich", "an", "Welt", "und", "Son\u00b7ne", "satt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "APPR", "NN", "KON", "NN", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.36": {"text": "und gl\u00fchend trinken.", "tokens": ["und", "gl\u00fc\u00b7hend", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Einmal schon haben Fanfaren", "tokens": ["Ein\u00b7mal", "schon", "ha\u00b7ben", "Fan\u00b7fa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "mein ungeduldiges Herz blutig gerissen,", "tokens": ["mein", "un\u00b7ge\u00b7dul\u0