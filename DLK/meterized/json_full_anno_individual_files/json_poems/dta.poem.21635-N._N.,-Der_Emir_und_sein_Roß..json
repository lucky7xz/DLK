{"dta.poem.21635": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Der Emir und sein Ro\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Blutbestr\u00f6mt und voller Wunden,", "tokens": ["Blut\u00b7be\u00b7str\u00f6mt", "und", "vol\u00b7ler", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihm Christen-Schwerter schlugen,", "tokens": ["Die", "ihm", "Chris\u00b7ten\u00b7Schwer\u00b7ter", "schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trugen Mauren ihren Emir,", "tokens": ["Tru\u00b7gen", "Mau\u00b7ren", "ih\u00b7ren", "E\u00b7mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der da k\u00e4mpfte, der da siegte", "tokens": ["Der", "da", "k\u00e4mpf\u00b7te", ",", "der", "da", "sieg\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Hundertmal in hundert Schlachten,", "tokens": ["Hun\u00b7dert\u00b7mal", "in", "hun\u00b7dert", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heimw\u00e4rts von des Ebro Strand.", "tokens": ["Heim\u00b7w\u00e4rts", "von", "des", "Eb\u00b7ro", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tief gesenkt das Haupt, das edle,", "tokens": ["Tief", "ge\u00b7senkt", "das", "Haupt", ",", "das", "ed\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Blutspur des Gebieters,", "tokens": ["Zu", "der", "Blut\u00b7spur", "des", "Ge\u00b7bie\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst aus tiefer Wunde blutend,", "tokens": ["Selbst", "aus", "tie\u00b7fer", "Wun\u00b7de", "blu\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kam das Ro\u00df, das ihn getragen,", "tokens": ["Kam", "das", "Ro\u00df", ",", "das", "ihn", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hundertmal in hundert Schlachten,", "tokens": ["Hun\u00b7dert\u00b7mal", "in", "hun\u00b7dert", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "El Mahran, der wei\u00dfe Hengst.", "tokens": ["El", "Mah\u00b7ran", ",", "der", "wei\u00b7\u00dfe", "Hengst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von dem Burgthor, dem gew\u00f6lbten,", "tokens": ["Von", "dem", "Burg\u00b7thor", ",", "dem", "ge\u00b7w\u00f6lb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schritt herab das Weib des Helden,", "tokens": ["Schritt", "her\u00b7ab", "das", "Weib", "des", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00fclnahar, die vielgeliebte,", "tokens": ["G\u00fcl\u00b7na\u00b7har", ",", "die", "viel\u00b7ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlang um ihn die wei\u00dfen Arme,", "tokens": ["Schlang", "um", "ihn", "die", "wei\u00b7\u00dfen", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dunkel, flossen ihre Locken,", "tokens": ["Dun\u00b7kel", ",", "flos\u00b7sen", "ih\u00b7re", "Lo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201erettet,\u201c rief sie, \u201emeinen Herrn!\u201c", "tokens": ["\u201e", "ret\u00b7tet", ",", "\u201c", "rief", "sie", ",", "\u201e", "mei\u00b7nen", "Herrn", "!", "\u201c"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und es sprach Medschnun der alte,", "tokens": ["Und", "es", "sprach", "Medsc\u00b7hnun", "der", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der der Heilkunst wohl erfahrne:", "tokens": ["Der", "der", "Heil\u00b7kunst", "wohl", "er\u00b7fahr\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201eschwer und tief sind seine Wunden,", "tokens": ["\u201e", "schwer", "und", "tief", "sind", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie zum Kampf mehr wird er reiten,", "tokens": ["Nie", "zum", "Kampf", "mehr", "wird", "er", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber willst Du, da\u00df er lebe,", "tokens": ["A\u00b7ber", "willst", "Du", ",", "da\u00df", "er", "le\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leben wird er, folge mir:", "tokens": ["Le\u00b7ben", "wird", "er", ",", "fol\u00b7ge", "mir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Von den Pfeilern, von den W\u00e4nden,", "tokens": ["Von", "den", "Pfei\u00b7lern", ",", "von", "den", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm die Waffen, die ihn schm\u00fcckten,", "tokens": ["Nimm", "die", "Waf\u00b7fen", ",", "die", "ihn", "schm\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Gef\u00e4hrten alter Tage,", "tokens": ["Die", "Ge\u00b7f\u00e4hr\u00b7ten", "al\u00b7ter", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sein Blick sie nie mehr finde,", "tokens": ["Da\u00df", "sein", "Blick", "sie", "nie", "mehr", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie sein Auge ihn erinn\u2019re", "tokens": ["Nie", "sein", "Au\u00b7ge", "ihn", "er\u00b7inn'\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An den Glanz ruhmvoller Zeit.", "tokens": ["An", "den", "Glanz", "ruhm\u00b7vol\u00b7ler", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Banne ferne vom Palaste", "tokens": ["Ban\u00b7ne", "fer\u00b7ne", "vom", "Pa\u00b7las\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Posaunen, die Drommeten,", "tokens": ["Die", "Po\u00b7sau\u00b7nen", ",", "die", "Drom\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Verk\u00fcnder einst\u2019ger Thaten,", "tokens": ["Die", "Ver\u00b7k\u00fcn\u00b7der", "einst'\u00b7ger", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie nie mehr ihn erwecken,", "tokens": ["Da\u00df", "sie", "nie", "mehr", "ihn", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie sein Ohr ihn mehr erinn\u2019re", "tokens": ["Nie", "sein", "Ohr", "ihn", "mehr", "er\u00b7inn'\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An den Glanz ruhmvoller Zeit.", "tokens": ["An", "den", "Glanz", "ruhm\u00b7vol\u00b7ler", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mische dann in seinen Becher", "tokens": ["Mi\u00b7sche", "dann", "in", "sei\u00b7nen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese tief geheimen Tropfen,", "tokens": ["Die\u00b7se", "tief", "ge\u00b7hei\u00b7men", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Kraft ist, da\u00df sie l\u00f6schen", "tokens": ["De\u00b7ren", "Kraft", "ist", ",", "da\u00df", "sie", "l\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, was uns je betr\u00fcbte,", "tokens": ["Al\u00b7les", ",", "was", "uns", "je", "be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles, was uns je erfreute,", "tokens": ["Al\u00b7les", ",", "was", "uns", "je", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "T\u00f6dtend die Erinnerung.\u201c", "tokens": ["T\u00f6d\u00b7tend", "die", "E\u00b7rin\u00b7ne\u00b7rung", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "$("], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Und sie mischte ihm die Tropfen \u2014", "tokens": ["Und", "sie", "mischte", "ihm", "die", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo am schattigsten die B\u00e4ume,", "tokens": ["Wo", "am", "schat\u00b7tigs\u00b7ten", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wo am duftigsten die Blumen,", "tokens": ["Wo", "am", "duf\u00b7tigs\u00b7ten", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dort im stillen Gartenhause,", "tokens": ["Dort", "im", "stil\u00b7len", "Gar\u00b7ten\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern der Welt und fern den Menschen,", "tokens": ["Fern", "der", "Welt", "und", "fern", "den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pflegte sie den wunden Herrn.", "tokens": ["Pfleg\u00b7te", "sie", "den", "wun\u00b7den", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "M\u00e4hlich schlossen sich die Wunden \u2014", "tokens": ["M\u00e4h\u00b7lich", "schlos\u00b7sen", "sich", "die", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweimal ging der lichte Fr\u00fchling", "tokens": ["Zwei\u00b7mal", "ging", "der", "lich\u00b7te", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch das Thal von Barcelona;", "tokens": ["Durch", "das", "Thal", "von", "Bar\u00b7ce\u00b7lo\u00b7na", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als er kam zum drittenmale,", "tokens": ["Als", "er", "kam", "zum", "drit\u00b7ten\u00b7ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fand er, unter Blumen wandelnd,", "tokens": ["Fand", "er", ",", "un\u00b7ter", "Blu\u00b7men", "wan\u00b7delnd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Friedlich l\u00e4chelnd einen Greis.", "tokens": ["Fried\u00b7lich", "l\u00e4\u00b7chelnd", "ei\u00b7nen", "Greis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und das war der k\u00fchne Emir \u2014", "tokens": ["Und", "das", "war", "der", "k\u00fch\u00b7ne", "E\u00b7mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene Hand, die einst am Ebro", "tokens": ["Je\u00b7ne", "Hand", ",", "die", "einst", "am", "Eb\u00b7ro"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "APPRART", "NE"], "meter": "+-+-+-+-", "me