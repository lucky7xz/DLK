{"textgrid.poem.59233": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor mir liegen deine Zeilen,", "tokens": ["Vor", "mir", "lie\u00b7gen", "dei\u00b7ne", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind nicht Worte, Schriften nicht,", "tokens": ["Sind", "nicht", "Wor\u00b7te", ",", "Schrif\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfeile, die verwundend heilen,", "tokens": ["Pfei\u00b7le", ",", "die", "ver\u00b7wun\u00b7dend", "hei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundesaugen, treu und schlicht.", "tokens": ["Freun\u00b7des\u00b7au\u00b7gen", ",", "treu", "und", "schlicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Niemals konnte so mich r\u00fchren", "tokens": ["Nie\u00b7mals", "konn\u00b7te", "so", "mich", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch der Liebsten Angesicht,", "tokens": ["Noch", "der", "Liebs\u00b7ten", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word",