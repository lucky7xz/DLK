{"textgrid.poem.57471": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein wohlgetroffenes Hochzeitfest in Stade", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer weicht, der Herbst f\u00e4llt ein,", "tokens": ["Der", "Som\u00b7mer", "weicht", ",", "der", "Herbst", "f\u00e4llt", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gelbe ", "tokens": ["Die", "gel\u00b7be"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Selbst ", "tokens": ["Selbst"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und will den frischen Most nicht schonen,", "tokens": ["Und", "will", "den", "fri\u00b7schen", "Most", "nicht", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Bl\u00e4tter werden welk und fahl,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "wer\u00b7den", "welk", "und", "fahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fallen von den starren Zweigen;", "tokens": ["Und", "fal\u00b7len", "von", "den", "star\u00b7ren", "Zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die sich zum Theil schon nackt und kahl", "tokens": ["Die", "sich", "zum", "Theil", "schon", "nackt", "und", "kahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf den entlaubten Gipfeln zeigen.", "tokens": ["Auf", "den", "ent\u00b7laub\u00b7ten", "Gip\u00b7feln", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was lehrt euch die\u00df, verlobtes Paar!", "tokens": ["Was", "lehrt", "euch", "die\u00df", ",", "ver\u00b7lob\u00b7tes", "Paar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was gilts, ihr habt es wohl verstanden?", "tokens": ["Was", "gilts", ",", "ihr", "habt", "es", "wohl", "ver\u00b7stan\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rufft nicht das abgelebte Jahr:", "tokens": ["Rufft", "nicht", "das", "ab\u00b7ge\u00b7leb\u00b7te", "Jahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeit zum Freyen sey vorhanden!", "tokens": ["Die", "Zeit", "zum", "Frey\u00b7en", "sey", "vor\u00b7han\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne brennt nicht mehr so scharf,", "tokens": ["Die", "Son\u00b7ne", "brennt", "nicht", "mehr", "so", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum k\u00f6mmt des Menschen Leib zu Kr\u00e4ften,", "tokens": ["Drum", "k\u00f6mmt", "des", "Men\u00b7schen", "Leib", "zu", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wird geschickter, als er darf,", "tokens": ["Und", "wird", "ge\u00b7schick\u00b7ter", ",", "als", "er", "darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Amors z\u00e4rtlichen Gesch\u00e4fften.", "tokens": ["Zu", "A\u00b7mors", "z\u00e4rt\u00b7li\u00b7chen", "Ge\u00b7sch\u00e4ff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar pflegt der Fr\u00fchling auch der Welt", "tokens": ["Zwar", "pflegt", "der", "Fr\u00fch\u00b7ling", "auch", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Lieben Trieb und Kraft zu geben;", "tokens": ["Zum", "Lie\u00b7ben", "Trieb", "und", "Kraft", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Es lacht manch buntes Tulpenbeet,", "tokens": ["Es", "lacht", "manch", "bun\u00b7tes", "Tul\u00b7pen\u00b7beet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Wald von silbernen Narcissen:", "tokens": ["Ein", "Wald", "von", "sil\u00b7ber\u00b7nen", "Nar\u00b7cis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer da gepaart spazieren geht,", "tokens": ["Wer", "da", "ge\u00b7paart", "spa\u00b7zie\u00b7ren", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bek\u00f6mmt ohnfehlbar Lust zum k\u00fcssen.", "tokens": ["Be\u00b7k\u00f6mmt", "ohn\u00b7fehl\u00b7bar", "Lust", "zum", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man leugnet solches freylich nicht;", "tokens": ["Man", "leug\u00b7net", "sol\u00b7ches", "frey\u00b7lich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wie? wenn angenehme Wangen,", "tokens": ["Doch", "wie", "?", "wenn", "an\u00b7ge\u00b7neh\u00b7me", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Herbste, durch ein st\u00e4rker Licht,", "tokens": ["Im", "Herbs\u00b7te", ",", "durch", "ein", "st\u00e4r\u00b7ker", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als alle Fr\u00fchlingskinder, prangen?", "tokens": ["Als", "al\u00b7le", "Fr\u00fch\u00b7lings\u00b7kin\u00b7der", ",", "pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ist ein Herz so wild und hart,", "tokens": ["Wo", "ist", "ein", "Herz", "so", "wild", "und", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das hier nicht gleiche Regung f\u00fchlte?", "tokens": ["Das", "hier", "nicht", "glei\u00b7che", "Re\u00b7gung", "f\u00fchl\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als dort in ", "tokens": ["Als", "dort", "in"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Man siehts an dir, o Br\u00e4utigam!", "tokens": ["Man", "siehts", "an", "dir", ",", "o", "Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann ich es schon entfernt nicht sehen:", "tokens": ["Kann", "ich", "es", "schon", "ent\u00b7fernt", "nicht", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn bin ich gleich der L\u00fcgen gram,", "tokens": ["Denn", "bin", "ich", "gleich", "der", "L\u00fc\u00b7gen", "gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So glaub ich doch, es sey geschehen.", "tokens": ["So", "glaub", "ich", "doch", ",", "es", "sey", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich r\u00fchrt die Sch\u00f6nheit deiner Braut,", "tokens": ["Dich", "r\u00fchrt", "die", "Sch\u00f6n\u00b7heit", "dei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr s\u00fc\u00dfer Scherz, ihr holdes Lachen,", "tokens": ["Ihr", "s\u00fc\u00b7\u00dfer", "Scherz", ",", "ihr", "hol\u00b7des", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weit mehr, als alles, was man schaut,", "tokens": ["Weit", "mehr", ",", "als", "al\u00b7les", ",", "was", "man", "schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn Wild und V\u00f6gel Hochzeit machen.", "tokens": ["Wenn", "Wild", "und", "V\u00f6\u00b7gel", "Hoch\u00b7zeit", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du liebest sie, und das mit Recht;", "tokens": ["Du", "lie\u00b7best", "sie", ",", "und", "das", "mit", "Recht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PDS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist es werth, und liebt dich wieder:", "tokens": ["Sie", "ist", "es", "werth", ",", "und", "liebt", "dich", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00f6digkeit ist schon geschw\u00e4cht,", "tokens": ["Die", "Bl\u00f6\u00b7dig\u00b7keit", "ist", "schon", "ge\u00b7schw\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schl\u00e4gt nicht mehr die Augen nieder.", "tokens": ["Sie", "schl\u00e4gt", "nicht", "mehr", "die", "Au\u00b7gen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie reicht dir willig Mund und Hand,", "tokens": ["Sie", "reicht", "dir", "wil\u00b7lig", "Mund", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Herbstluft kann sie gar nicht st\u00f6ren:", "tokens": ["Die", "Herbst\u00b7luft", "kann", "sie", "gar", "nicht", "st\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und w\u00fcrd es kalt: der Liebe Brand", "tokens": ["Und", "w\u00fcrd", "es", "kalt", ":", "der", "Lie\u00b7be", "Brand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird euch die Geister schon vermehren.", "tokens": ["Wird", "euch", "die", "Geis\u00b7ter", "schon", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wird Sturm und Regen, Reif und Schnee", "tokens": ["Wird", "Sturm", "und", "Re\u00b7gen", ",", "Reif", "und", "Schnee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allm\u00e4hlich Wald und Feld bestreiten;", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "Wald", "und", "Feld", "be\u00b7strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wird dir Amor Laub und Klee,", "tokens": ["So", "wird", "dir", "A\u00b7mor", "Laub", "und", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja Rosenbl\u00e4tter gnug bereiten.", "tokens": ["Ja", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "gnug", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Liebsten Arm und Brust und Schoo\u00df", "tokens": ["Der", "Liebs\u00b7ten", "Arm", "und", "Brust", "und", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird jenen Mangel leicht ersetzen:", "tokens": ["Wird", "je\u00b7nen", "Man\u00b7gel", "leicht", "er\u00b7set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was achtest du des Winters gro\u00df,", "tokens": ["Was", "ach\u00b7test", "du", "des", "Win\u00b7ters", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn dich die Liebe kann ergetzen?", "tokens": ["Wenn", "d