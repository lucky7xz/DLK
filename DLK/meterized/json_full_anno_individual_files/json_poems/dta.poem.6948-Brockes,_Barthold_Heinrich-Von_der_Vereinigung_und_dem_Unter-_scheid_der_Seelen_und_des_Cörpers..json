{"dta.poem.6948": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein reger Stoff, der sich in kleine Theilchen bricht,", "tokens": ["Ein", "re\u00b7ger", "Stoff", ",", "der", "sich", "in", "klei\u00b7ne", "Theil\u00b7chen", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Formirt der Weisheit ewigs Licht,", "tokens": ["For\u00b7mirt", "der", "Weis\u00b7heit", "e\u00b7wigs", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Will, da\u00df die Eigenschafft des Geists ihm eigen werde,", "tokens": ["Will", ",", "da\u00df", "die", "Ei\u00b7gen\u00b7schafft", "des", "Geists", "ihm", "ei\u00b7gen", "wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Begreiffet was geheim im Himmel, auf der Erde.", "tokens": ["Be\u00b7greif\u00b7fet", "was", "ge\u00b7heim", "im", "Him\u00b7mel", ",", "auf", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "mea