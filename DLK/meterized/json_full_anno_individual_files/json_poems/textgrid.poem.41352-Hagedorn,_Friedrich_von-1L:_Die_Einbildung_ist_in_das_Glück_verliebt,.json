{"textgrid.poem.41352": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Einbildung ist in das Gl\u00fcck verliebt,", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Einbildung ist in das Gl\u00fcck verliebt,", "tokens": ["Die", "Ein\u00b7bil\u00b7dung", "ist", "in", "das", "Gl\u00fcck", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das sie so oft gesucht, das ihr so oft entgangen:", "tokens": ["Das", "sie", "so", "oft", "ge\u00b7sucht", ",", "das", "ihr", "so", "oft", "ent\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVPP", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Gl\u00fcckes Spr\u00f6digkeit, die ihren F\u00fcrwitz \u00fcbt,", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "Spr\u00f6\u00b7dig\u00b7keit", ",", "die", "ih\u00b7ren", "F\u00fcr\u00b7witz", "\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Reizt ihre Hoffnung stets, und t\u00e4uscht stets ihr Verlangen.", "tokens": ["Reizt", "ih\u00b7re", "Hoff\u00b7nung", "stets", ",", "und", "t\u00e4uscht", "stets", "ihr", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als sie noch jung und unerfahren war,", "tokens": ["Als", "sie", "noch", "jung", "und", "un\u00b7er\u00b7fah\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging sie ihm seufzend nach bis in das Reich der Liebe.", "tokens": ["Ging", "sie", "ihm", "seuf\u00b7zend", "nach", "bis", "in", "das", "Reich", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "APPR", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch hier entfernten es bald schl\u00fcpfrige Gefahr,", "tokens": ["Doch", "hier", "ent\u00b7fern\u00b7ten", "es", "bald", "schl\u00fcpf\u00b7ri\u00b7ge", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald leichter Wankelmuth, bald eifers\u00fccht'ge Triebe.", "tokens": ["Bald", "leich\u00b7ter", "Wan\u00b7kel\u00b7muth", ",", "bald", "ei\u00b7fer\u00b7s\u00fccht'\u00b7ge", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Arme w\u00e4chst, die Leidenschaft nimmt zu:", "tokens": ["Die", "Ar\u00b7me", "w\u00e4chst", ",", "die", "Lei\u00b7den\u00b7schaft", "nimmt", "zu", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie wagt sich an den Hof, zu den geschm\u00fcckten H\u00f6hen,", "tokens": ["Sie", "wagt", "sich", "an", "den", "Hof", ",", "zu", "den", "ge\u00b7schm\u00fcck\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Pracht und Ehrgeiz rauscht. Dort fehlen Treu' und Ruh',", "tokens": ["Wo", "Pracht", "und", "Ehr\u00b7geiz", "rauscht", ".", "Dort", "feh\u00b7len", "Treu'", "und", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$.", "ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Titel lassen sich, anstatt des Gl\u00fcckes, sehen.", "tokens": ["Und", "Ti\u00b7tel", "las\u00b7sen", "sich", ",", "an\u00b7statt", "des", "Gl\u00fc\u00b7ckes", ",", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "$,", "KOUI", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie eilt darauf ins Land der Ueppigkeit,", "tokens": ["Sie", "eilt", "da\u00b7rauf", "ins", "Land", "der", "Uep\u00b7pig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort mit dem Gl\u00fccke sich durch Reichthum zu verbinden;", "tokens": ["Dort", "mit", "dem", "Gl\u00fc\u00b7cke", "sich", "durch", "Reicht\u00b7hum", "zu", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Dort war auch Ueberflu\u00df, Gepr\u00e4nge, Schwelgen, Neid,", "tokens": ["Dort", "war", "auch", "Ue\u00b7berf\u00b7lu\u00df", ",", "Ge\u00b7pr\u00e4n\u00b7ge", ",", "Schwel\u00b7gen", ",", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der b\u00fcrgerliche Stolz, doch nicht das Gl\u00fcck, zu finden.", "tokens": ["Der", "b\u00fcr\u00b7ger\u00b7li\u00b7che", "Stolz", ",", "doch", "nicht", "das", "Gl\u00fcck", ",", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PTKNEG", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie rennt zur\u00fcck, und k\u00f6mmt auf eine Bahn,", "tokens": ["Sie", "rennt", "zu\u00b7r\u00fcck", ",", "und", "k\u00f6mmt", "auf", "ei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihren m\u00fcden Fu\u00df in niedre Gr\u00fcnde f\u00fchret.", "tokens": ["Die", "ih\u00b7ren", "m\u00fc\u00b7den", "Fu\u00df", "in", "nied\u00b7re", "Gr\u00fcn\u00b7de", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die stille Gegend ist der Sch\u00f6nen unterthan,", "tokens": ["Die", "stil\u00b7le", "Ge\u00b7gend", "ist", "der", "Sch\u00f6\u00b7nen", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sich mit keinem Schmuck, als Zucht und Demuth, zieret.", "tokens": ["Die", "sich", "mit", "kei\u00b7nem", "Schmuck", ",", "als", "Zucht", "und", "De\u00b7muth", ",", "zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Gottesfurcht hat dort ihr Heiligthum,", "tokens": ["Die", "Got\u00b7tes\u00b7furcht", "hat", "dort", "ihr", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Weisheit holdes Kind, die Lust der Ewigkeiten.", "tokens": ["Der", "Weis\u00b7heit", "hol\u00b7des", "Kind", ",", "die", "Lust", "der", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der milde Himmel kennt und sch\u00fctzet ihren Ruhm,", "tokens": ["Der", "mil\u00b7de", "Him\u00b7mel", "kennt", "und", "sch\u00fct\u00b7zet", "ih\u00b7ren", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Wahrheit, Lieb' und Recht weicht nicht von ihren Seiten.", "tokens": ["Und", "Wahr\u00b7heit", ",", "Lieb'", "und", "Recht", "weicht", "nicht", "von", "ih\u00b7ren", "Sei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Einbildung fragt nach dem Gl\u00fcck allhier;", "tokens": ["Die", "Ein\u00b7bil\u00b7dung", "fragt", "nach", "dem", "Gl\u00fcck", "all\u00b7hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die fromme Sch\u00f6ne spricht: ich will dir Rath ertheilen.", "tokens": ["Die", "from\u00b7me", "Sch\u00f6\u00b7ne", "spricht", ":", "ich", "will", "dir", "Rath", "er\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erwart' es; such' es nicht; geselle dich zu mir:", "tokens": ["Er\u00b7wart'", "es", ";", "such'", "es", "nicht", ";", "ge\u00b7sel\u00b7le", "dich", "zu", "mir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird dir schon das Gl\u00fcck von selbst entgegeneilen.", "tokens": ["So", "wird", "dir", "schon", "das", "Gl\u00fcck", "von", "selbst", "ent\u00b7ge\u00b7ge\u00b7nei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihr wird gefolgt; nichts konnte besser sein.", "tokens": ["Ihr", "wird", "ge\u00b7folgt", ";", "nichts", "konn\u00b7te", "bes\u00b7ser", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PIS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald sieht man einen Glanz das Heiligthum verkl\u00e4ren.", "tokens": ["Bald", "sieht", "man", "ei\u00b7nen", "Glanz", "das", "Hei\u00b7lig\u00b7thum", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es stellet sich das Gl\u00fcck mit offnen Armen ein,", "tokens": ["Es", "stel\u00b7let", "sich", "das", "Gl\u00fcck", "mit", "off\u00b7nen", "Ar\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umf\u00e4ngt die Hoffende, und s\u00e4ttigt ihr Begehren.", "tokens": ["Um\u00b7f\u00e4ngt", "die", "Hof\u00b7fen\u00b7de", ",", "und", "s\u00e4t\u00b7tigt", "ihr", "Be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.9": {"line.1": {"text": "Die Einbildung ist in das Gl\u00fcck verliebt,", "tokens": ["Die", "Ein\u00b7bil\u00b7dung", "ist", "in", "das", "Gl\u00fcck", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das sie so oft gesucht, das ihr so oft entgangen:", "tokens": ["Das", "sie", "so", "oft", "ge\u00b7sucht", ",", "das", "ihr", "so", "oft", "ent\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVPP", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Gl\u00fcckes Spr\u00f6digkeit, die ihren F\u00fcrwitz \u00fcbt,", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "Spr\u00f6\u00b7dig\u00b7keit", ",", "die", "ih\u00b7ren", "F\u00fcr\u00b7witz", "\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Reizt ihre Hoffnung stets, und t\u00e4uscht stets ihr Verlangen.", "tokens": ["Reizt", "ih\u00b7re", "Hoff\u00b7nung", "stets", ",", "und", "t\u00e4uscht", "stets", "ihr", "Ver\u00b7lan\u00b7gen", "."], "token_info": [