{"textgrid.poem.63008": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Philosoph, des Ordens Ehre,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Philosoph, des Ordens Ehre,", "tokens": ["Ein", "Phi\u00b7lo\u00b7soph", ",", "des", "Or\u00b7dens", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ritt einst nach einem Doctorschmaus,", "tokens": ["Ritt", "einst", "nach", "ei\u00b7nem", "Doc\u00b7tor\u00b7schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bewehrt mit S\u00e4bel, Schild und Speere,", "tokens": ["Be\u00b7wehrt", "mit", "S\u00e4\u00b7bel", ",", "Schild", "und", "Spee\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einer fl\u00fcchtigen Chim\u00e4re,", "tokens": ["Auf", "ei\u00b7ner", "fl\u00fcch\u00b7ti\u00b7gen", "Chi\u00b7m\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Voll Muths auf Abendtheuer aus.", "tokens": ["Voll", "Muths", "auf", "A\u00b7bendt\u00b7heu\u00b7er", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wahrheit, diese spr\u00f6de Dame,", "tokens": ["Die", "Wahr\u00b7heit", ",", "die\u00b7se", "spr\u00f6\u00b7de", "Da\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Magier, Sophist und Brame", "tokens": ["Der", "Ma\u00b7gier", ",", "So\u00b7phist", "und", "Bra\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zu allen Zeiten nachgesetzt,", "tokens": ["Zu", "al\u00b7len", "Zei\u00b7ten", "nach\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist auch das Wildpret das er hetzt.", "tokens": ["Ist", "auch", "das", "Wild\u00b7pret", "das", "er", "hetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allein zu seinem gr\u00f6sten Grame,", "tokens": ["Al\u00b7lein", "zu", "sei\u00b7nem", "gr\u00f6s\u00b7ten", "Gra\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Verlor er stets die rechte Spur:", "tokens": ["Ver\u00b7lor", "er", "stets", "die", "rech\u00b7te", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er tummelte die rasche M\u00e4hre,", "tokens": ["Er", "tum\u00b7mel\u00b7te", "die", "ra\u00b7sche", "M\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Bald in das Kreuz bald in die Queere,", "tokens": ["Bald", "in", "das", "Kreuz", "bald", "in", "die", "Que\u00b7e\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Erbost, durch eine stille Flur,", "tokens": ["Er\u00b7bost", ",", "durch", "ei\u00b7ne", "stil\u00b7le", "Flur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als er ein Hirtenweib erblickte,", "tokens": ["Als", "er", "ein", "Hir\u00b7ten\u00b7weib", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das unter Disteln Beeren pfl\u00fcckte;", "tokens": ["Das", "un\u00b7ter", "Dis\u00b7teln", "Bee\u00b7ren", "pfl\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die G\u00f6ttin wars, die unerkannt,", "tokens": ["Die", "G\u00f6t\u00b7tin", "wars", ",", "die", "un\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mit scharfem Blick und offnen Mienen,", "tokens": ["Mit", "schar\u00b7fem", "Blick", "und", "off\u00b7nen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "In einem weissen Flachsgewand", "tokens": ["In", "ei\u00b7nem", "weis\u00b7sen", "Flachs\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wie sie dem Sokrates erschienen,", "tokens": ["Wie", "sie", "dem", "Sok\u00b7ra\u00b7tes", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Vor seinem tr\u00fcben Auge stand:", "tokens": ["Vor", "sei\u00b7nem", "tr\u00fc\u00b7ben", "Au\u00b7ge", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wohin? sprach sie mit ernstem Spotte.", "tokens": ["Wo\u00b7hin", "?", "sprach", "sie", "mit", "erns\u00b7tem", "Spot\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Je! nach der Wahrheit Zauberschlo\u00df,", "tokens": ["Je", "!", "nach", "der", "Wahr\u00b7heit", "Zau\u00b7ber\u00b7schlo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Erwiederte der Don Quixotte.", "tokens": ["Er\u00b7wie\u00b7der\u00b7te", "der", "Don", "Qui\u00b7xot\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Das findst du nicht auf diesem Ro\u00df;", "tokens": ["Das", "findst", "du", "nicht", "auf", "die\u00b7sem", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sie wohnt in einer stillen Grotte,", "tokens": ["Sie", "wohnt", "in", "ei\u00b7ner", "stil\u00b7len", "Grot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Zu der ein rauher Pfad sich kr\u00fcmmt,", "tokens": ["Zu", "der", "ein", "rau\u00b7her", "Pfad", "sich", "kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Den, wie die alten Sagen melden,", "tokens": ["Den", ",", "wie", "die", "al\u00b7ten", "Sa\u00b7gen", "mel\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Auch selbst der tapferste der Helden", "tokens": ["Auch", "selbst", "der", "tap\u00b7fers\u00b7te", "der", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "ART", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.30": {"text": "Nur schwer und nur zu Fu\u00df erklimmt.", "tokens": ["Nur", "schwer", "und", "nur", "zu", "Fu\u00df", "er\u00b7klimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Zu Fu\u00dfe? sprach mit bitterm Hohne", "tokens": ["Zu", "Fu\u00b7\u00dfe", "?", "sprach", "mit", "bit\u00b7term", "Hoh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der weise Ritter zur Matrone,", "tokens": ["Der", "wei\u00b7se", "Rit\u00b7ter", "zur", "Mat\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ha, ha! so dumm sind wir nicht mehr:", "tokens": ["Ha", ",", "ha", "!", "so", "dumm", "sind", "wir", "nicht", "mehr", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "ADV", "ADJD", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Mein gutes Weib, la\u00df dir bedeuten,", "tokens": ["Mein", "gu\u00b7tes", "Weib", ",", "la\u00df", "dir", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Da\u00df schon seit vielen Jahren her", "tokens": ["Da\u00df", "schon", "seit", "vie\u00b7len", "Jah\u00b7ren", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PIAT", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die Philosophen alle reiten.", "tokens": ["Die", "Phi\u00b7lo\u00b7so\u00b7phen", "al\u00b7le", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+--+--+-", "measu