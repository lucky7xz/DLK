{"textgrid.poem.35047": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Gottesmahnung", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gieb mir, o Mensch, was mir geh\u00f6rt,", "tokens": ["Gieb", "mir", ",", "o", "Mensch", ",", "was", "mir", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gieb der Welt, was sie dir borgte,", "tokens": ["Und", "gieb", "der", "Welt", ",", "was", "sie", "dir", "borg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ist sofort der Wahn zerst\u00f6rt,", "tokens": ["So", "ist", "so\u00b7fort", "der", "Wahn", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie mehr als ich f\u00fcr dich sorgte.", "tokens": ["Da\u00df", "sie", "mehr", "als", "ich", "f\u00fcr", "dich", "sorg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist aus deinem Vaterland", "tokens": ["Du", "bist", "aus", "dei\u00b7nem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Gast zu ihr hinabgegangen", "tokens": ["Als", "Gast", "zu", "ihr", "hin\u00b7ab\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hast daf\u00fcr aus ihrer Hand", "tokens": ["Und", "hast", "da\u00b7f\u00fcr", "aus", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts als die S\u00fcnde nur empfangen.", "tokens": ["Nichts", "als", "die", "S\u00fcn\u00b7de", "nur", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun will sie durch die Gleisnerin", "tokens": ["Nun", "will", "sie", "durch", "die", "Gleis\u00b7ne\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich fest und fester an sich binden.", "tokens": ["Dich", "fest", "und", "fes\u00b7ter", "an", "sich", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es soll des Kindes Heimatssinn", "tokens": ["Es", "soll", "des", "Kin\u00b7des", "Hei\u00b7mats\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Vaterhaus nicht wiederfinden.", "tokens": ["Das", "Va\u00b7te