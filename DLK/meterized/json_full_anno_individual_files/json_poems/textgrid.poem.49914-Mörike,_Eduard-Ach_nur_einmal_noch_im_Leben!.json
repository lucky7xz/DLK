{"textgrid.poem.49914": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Ach nur einmal noch im Leben!", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fenster jenes alt verblichnen Gartensaals", "tokens": ["Im", "Fens\u00b7ter", "je\u00b7nes", "alt", "ver\u00b7blich\u00b7nen", "Gar\u00b7ten\u00b7sa\u00b7als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PDAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Die Harfe, die, vom leisen Windhauch angeregt,", "tokens": ["Die", "Har\u00b7fe", ",", "die", ",", "vom", "lei\u00b7sen", "Wind\u00b7hauch", "an\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lang ausgezogne T\u00f6ne traurig wechseln l\u00e4\u00dft", "tokens": ["Lang", "aus\u00b7ge\u00b7zog\u00b7ne", "T\u00f6\u00b7ne", "trau\u00b7rig", "wech\u00b7seln", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ADJD", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In ungepflegter Sp\u00e4therbst-Blumen-Einsamkeit,", "tokens": ["In", "un\u00b7ge\u00b7pfleg\u00b7ter", "Sp\u00e4t\u00b7herbs\u00b7tBlu\u00b7men\u00b7Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist sch\u00f6n zu h\u00f6ren einen langen Nachmittag.", "tokens": ["Ist", "sch\u00f6n", "zu", "h\u00f6\u00b7ren", "ei\u00b7nen", "lan\u00b7gen", "Nach\u00b7mit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht v\u00f6llig unwert ihrer holden Nachbarschaft", "tokens": ["Nicht", "v\u00f6l\u00b7lig", "un\u00b7wert", "ih\u00b7rer", "hol\u00b7den", "Nach\u00b7bar\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "St\u00f6hnt auf dem grauen Zwingerturm die Fahne dort,", "tokens": ["St\u00f6hnt", "auf", "dem", "grau\u00b7en", "Zwin\u00b7ger\u00b7turm", "die", "Fah\u00b7ne", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn st\u00fcrmischer oft die Wolken ziehen \u00fcberhin.", "tokens": ["Wenn", "st\u00fcr\u00b7mi\u00b7scher", "oft", "die", "Wol\u00b7ken", "zie\u00b7hen", "\u00fc\u00b7berh\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "In meinem Garten aber (hie\u00df' er nur noch mein!)", "tokens": ["In", "mei\u00b7nem", "Gar\u00b7ten", "a\u00b7ber", "(", "hie\u00df'", "er", "nur", "noch", "mein", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$(", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ging so ein Hinterpf\u00f6rtchen frei ins Feld hinaus,", "tokens": ["Ging", "so", "ein", "Hin\u00b7ter\u00b7pf\u00f6rt\u00b7chen", "frei", "ins", "Feld", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Abseits vom Dorf. Wie manches liebe Mal stie\u00df ich", "tokens": ["Ab\u00b7seits", "vom", "Dorf", ".", "Wie", "man\u00b7ches", "lie\u00b7be", "Mal", "stie\u00df", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$.", "PWAV", "PIAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Den Riegel auf an der geschw\u00e4rzten Gattert\u00fcr", "tokens": ["Den", "Rie\u00b7gel", "auf", "an", "der", "ge\u00b7schw\u00e4rz\u00b7ten", "Gat\u00b7ter\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und bog das \u00fcberh\u00e4ngende Gestr\u00e4uch zur\u00fcck,", "tokens": ["Und", "bog", "das", "\u00fc\u00b7ber\u00b7h\u00e4n\u00b7gen\u00b7de", "Ge\u00b7str\u00e4uch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem sie sich auf rostgen Angeln schwer gedreht! \u2013", "tokens": ["In\u00b7dem", "sie", "sich", "auf", "rost\u00b7gen", "An\u00b7geln", "schwer", "ge\u00b7dreht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die T\u00fcr nun, musikalisch mannigfach begabt,", "tokens": ["Die", "T\u00fcr", "nun", ",", "mu\u00b7si\u00b7ka\u00b7lisch", "man\u00b7nig\u00b7fach", "be\u00b7gabt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "F\u00fcr ihre Jahre noch ein ganz annehmlicher", "tokens": ["F\u00fcr", "ih\u00b7re", "Jah\u00b7re", "noch", "ein", "ganz", "an\u00b7nehm\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "ADV", "ADJA"], "meter": "-+-+-+++-+--", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Sopran (wenn sie nicht eben wetterlaunisch war),", "tokens": ["Sop\u00b7ran", "(", "wenn", "sie", "nicht", "e\u00b7ben", "wet\u00b7ter\u00b7lau\u00b7nisch", "war", ")", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VAFIN", "$(", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verriet mir eines Tages \u2013 pl\u00f6tzlich, wie es schien,", "tokens": ["Ver\u00b7riet", "mir", "ei\u00b7nes", "Ta\u00b7ges", "\u2013", "pl\u00f6tz\u00b7lich", ",", "wie", "es", "schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erweckt aus einer lieblichen Erinnerung \u2013", "tokens": ["Er\u00b7weckt", "aus", "ei\u00b7ner", "lieb\u00b7li\u00b7chen", "E\u00b7rin\u00b7ne\u00b7rung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein sch\u00f6neres Empfinden, h\u00f6here F\u00e4higkeit.", "tokens": ["Ein", "sch\u00f6\u00b7ne\u00b7res", "Emp\u00b7fin\u00b7den", ",", "h\u00f6\u00b7he\u00b7re", "F\u00e4\u00b7hig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Ich \u00f6ffne sie gewohnter Weise, da beginnt", "tokens": ["Ich", "\u00f6ff\u00b7ne", "sie", "ge\u00b7wohn\u00b7ter", "Wei\u00b7se", ",", "da", "be\u00b7ginnt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie z\u00e4rtlich eine Arie, die mein Ohr sogleich", "tokens": ["Sie", "z\u00e4rt\u00b7lich", "ei\u00b7ne", "A\u00b7rie", ",", "die", "mein", "Ohr", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bekannt ansprach. Wie? rief ich staunend: tr\u00e4um ich denn?", "tokens": ["Be\u00b7kannt", "an\u00b7sprach", ".", "Wie", "?", "rief", "ich", "stau\u00b7nend", ":", "tr\u00e4um", "ich", "denn", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "PWAV", "$.", "VVFIN", "PPER", "VVPP", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "War das nicht \u00bbAch nur einmal noch im Leben\u00ab ganz?", "tokens": ["War", "das", "nicht", "\u00bb", "Ach", "nur", "ein\u00b7mal", "noch", "im", "Le\u00b7ben", "\u00ab", "ganz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "$(", "ITJ", "ADV", "ADV", "ADV", "APPRART", "NN", "$(", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Aus Titus, wenn mir recht ist? \u2013 Alsbald lie\u00df ich sie", "tokens": ["Aus", "Ti\u00b7tus", ",", "wenn", "mir", "recht", "ist", "?", "\u2013", "Als\u00b7bald", "lie\u00df", "ich", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Stelle wiederholen; und ich irrte nicht!", "tokens": ["Die", "Stel\u00b7le", "wie\u00b7der\u00b7ho\u00b7len", ";", "und", "ich", "irr\u00b7te", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn langsamer, bestimmter, seelenvoller nun", "tokens": ["Denn", "lang\u00b7sa\u00b7mer", ",", "be\u00b7stimm\u00b7ter", ",", "see\u00b7len\u00b7vol\u00b7ler", "nun"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da capo sang die Alte: \u00bbAch nur einmal noch!\u00ab", "tokens": ["Da", "ca\u00b7po", "sang", "die", "Al\u00b7te", ":", "\u00bb", "Ach", "nur", "ein\u00b7mal", "noch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NE", "VVFIN", "ART", "NN", "$.", "$(", "ITJ", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die f\u00fcnf, sechs ersten Noten n\u00e4mlich, weiter kaum,", "tokens": ["Die", "f\u00fcnf", ",", "sechs", "ers\u00b7ten", "No\u00b7ten", "n\u00e4m\u00b7lich", ",", "wei\u00b7ter", "kaum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "CARD", "ADJA", "NN", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hingegen war auch dieser Anfang tadellos.", "tokens": ["Hin\u00b7ge\u00b7gen", "war", "auch", "die\u00b7ser", "An\u00b7fang", "ta\u00b7del\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "\u2013 Und was, frug ich nach einer kurzen Stille sie,", "tokens": ["\u2013", "Und", "was", ",", "frug", "ich", "nach", "ei\u00b7ner", "kur\u00b7zen", "Stil\u00b7le", "sie", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was denn noch einmal? Sprich, woher, Elegische,", "tokens": ["Was", "denn", "noch", "ein\u00b7mal", "?", "Sprich", ",", "wo\u00b7her", ",", "E\u00b7le\u00b7gi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADV", "$.", "NN", "$,", "PWAV", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Hast du das Lied? Ging etwa denn zu deiner Zeit", "tokens": ["Hast", "du", "das", "Lied", "?", "Ging", "et\u00b7wa", "denn", "zu", "dei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "(die neunziger Jahre meint ich) hier ein sch\u00f6nes Kind,", "tokens": ["(", "die", "neun\u00b7zi\u00b7ger", "Jah\u00b7re", "meint", "ich", ")", "hier", "ein", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PPER", "$(", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Des Pfarrers Enkeltochter, sittsam aus und ein,", "tokens": ["Des", "Pfar\u00b7rers", "En\u00b7kel\u00b7toch\u00b7ter", ",", "sitt\u00b7sam", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und h\u00f6rtest du sie durch das offne Fenster oft", "tokens": ["Und", "h\u00f6r\u00b7test", "du", "sie", "durch", "das", "off\u00b7ne", "Fens\u00b7ter", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Am gr\u00fcnlackierten, goldbebl\u00fcmten Pantalon", "tokens": ["Am", "gr\u00fcn\u00b7la\u00b7ckier\u00b7ten", ",", "gold\u00b7be\u00b7bl\u00fcm\u00b7ten", "Pan\u00b7ta\u00b7lon"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Hellstimmig singen? Des gestrengen M\u00fctterchens", "tokens": ["Hells\u00b7tim\u00b7mig", "sin\u00b7gen", "?", "Des", "ge\u00b7stren\u00b7gen", "M\u00fct\u00b7ter\u00b7chens"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gedenkst du auch, der Hausfrau, die so reinlich stets", "tokens": ["Ge\u00b7denkst", "du", "auch", ",", "der", "Haus\u00b7frau", ",", "die", "so", "rein\u00b7lich", "stets"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Den Garten hielt, gleich wie sie selber war, wann sie", "tokens": ["Den", "Gar\u00b7ten", "hielt", ",", "gleich", "wie", "sie", "sel\u00b7ber", "war", ",", "wann", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "KOKOM", "PPER", "ADV", "VAFIN", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Nach schw\u00fclem Tag am Abend ihren Kohl bego\u00df,", "tokens": ["Nach", "schw\u00fc\u00b7lem", "Tag", "am", "A\u00b7bend", "ih\u00b7ren", "Kohl", "be\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Derweil der Pfarrherr ein paar Freunden aus der Stadt,", "tokens": ["Der\u00b7weil", "der", "Pfar\u00b7rherr", "ein", "paar", "Freun\u00b7