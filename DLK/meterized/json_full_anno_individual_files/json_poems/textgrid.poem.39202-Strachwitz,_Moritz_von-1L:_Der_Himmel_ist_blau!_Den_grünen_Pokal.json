{"textgrid.poem.39202": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel ist blau! Den gr\u00fcnen Pokal", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel ist blau! Den gr\u00fcnen Pokal", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", "!", "Den", "gr\u00fc\u00b7nen", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit rinnendem Golde befeuchtet!", "tokens": ["Mit", "rin\u00b7nen\u00b7dem", "Gol\u00b7de", "be\u00b7feuch\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wer trinkt nicht gern, wenn der Sonnenstrahl", "tokens": ["Wer", "trinkt", "nicht", "gern", ",", "wenn", "der", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Rheinweinperlen leuchtet! \u2013", "tokens": ["In", "Rhein\u00b7wein\u00b7per\u00b7len", "leuch\u00b7tet", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zerschmettre den R\u00f6mer an der Wand,", "tokens": ["Zer\u00b7schmett\u00b7re", "den", "R\u00f6\u00b7mer", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit Tr\u00e4nen die Lippen wasche,", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "die", "Lip\u00b7pen", "wa\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und traure um Dein Vaterland", "tokens": ["Und", "trau\u00b7re", "um", "Dein", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Asche, in Asche!", "tokens": ["In", "A\u00b7sche", ",", "in", "A\u00b7sche", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Himmel ist blau! Wir sind noch jung,", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", "!", "Wir", "sind", "noch", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Viel Lieder verborgen fluten;", "tokens": ["Viel", "Lie\u00b7der", "ver\u00b7bor\u00b7gen", "flu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer l\u00e4\u00dft nicht gern die Begeisterung", "tokens": ["Wer", "l\u00e4\u00dft", "nicht", "gern", "die", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In klingender Woge bluten! \u2013", "tokens": ["In", "klin\u00b7gen\u00b7der", "Wo\u00b7ge", "blu\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "La\u00df weinen die Harfe unter der Hand", "tokens": ["La\u00df", "wei\u00b7nen", "die", "Har\u00b7fe", "un\u00b7ter", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein Grablied, t\u00f6richter Knabe,", "tokens": ["Ein", "Grab\u00b7lied", ",", "t\u00f6\u00b7rich\u00b7ter", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sie schleppen indes Dein Vaterland", "tokens": ["Sie", "schlep\u00b7pen", "in\u00b7des", "Dein", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu Grabe, zu Grabe!", "tokens": ["Zu", "Gra\u00b7be", ",", "zu", "Gra\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Himmel ist blau! Holdselige Frau,", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", "!", "Hold\u00b7se\u00b7li\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gepriesen sei Dein Name!", "tokens": ["Ge\u00b7prie\u00b7sen", "sei", "Dein", "Na\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer k\u00fc\u00dft nicht gerne den Wonnetau", "tokens": ["Wer", "k\u00fc\u00dft", "nicht", "ger\u00b7ne", "den", "Won\u00b7ne\u00b7tau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Auge seiner Dame! \u2013", "tokens": ["Vom", "Au\u00b7ge", "sei\u00b7ner", "Da\u00b7me", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus dem Herzen schneide den s\u00fc\u00dfen Tand,", "tokens": ["Aus", "dem", "Her\u00b7zen", "schnei\u00b7de", "den", "s\u00fc\u00b7\u00dfen", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Minne wende den R\u00fccken,", "tokens": ["Der", "Min\u00b7ne", "wen\u00b7de", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sie rei\u00dfen indes Dein Vaterland", "tokens": ["Sie", "rei\u00b7\u00dfen", "in\u00b7des", "Dein", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu St\u00fccken, zu St\u00fccken!", "tokens": ["Zu", "St\u00fc\u00b7cken", ",", "zu", "St\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Himmel ist blau! Die Jagd ist laut,", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", "!", "Die", "Jagd", "ist", "laut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ha, f\u00fcrstliche Freude der M\u00e4nner!", "tokens": ["Ha", ",", "f\u00fcrst\u00b7li\u00b7che", "Freu\u00b7de", "der", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wer reitet nicht gerne durchs Heidekraut", "tokens": ["Wer", "rei\u00b7tet", "nicht", "ger\u00b7ne", "durchs", "Hei\u00b7de\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den lang sich streckenden Renner! \u2013", "tokens": ["Den", "lang", "sich", "stre\u00b7cken\u00b7den", "Ren\u00b7ner", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PRF", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "La\u00df fallen die Z\u00fcgel aus der Hand,", "tokens": ["La\u00df", "fal\u00b7len", "die", "Z\u00fc\u00b7gel", "aus", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von der Ferse schlage die Sporen,", "tokens": ["Von", "der", "Fer\u00b7se", "schla\u00b7ge", "die", "Spo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Es geht indessen Dein Vaterland", "tokens": ["Es", "geht", "in\u00b7des\u00b7sen", "Dein", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Verloren, verloren! \u2013", "tokens": ["Ver\u00b7lo\u00b7ren", ",", "ver\u00b7lo\u00b7ren", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "VVPP", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Himmel ist blau! Er f\u00e4llt nicht ein", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", "!", "Er", "f\u00e4llt", "nicht", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PTKNEG", "ART"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Sturme irdischer Schmerzen,", "tokens": ["Vom", "Stur\u00b7me", "ir\u00b7di\u00b7scher", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es hungert das Volk, und die B\u00f6sen schrein", "tokens": ["Es", "hun\u00b7gert", "das", "Volk", ",", "und", "die", "B\u00f6\u00b7sen", "schrein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Aufruhr ihm in die Herzen! \u2013", "tokens": ["Den", "Auf\u00b7ruhr", "ihm", "in", "die", "Her\u00b7zen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Da ist kein Glaubens-, kein Liebesband,", "tokens": ["Da", "ist", "kein", "Glau\u00b7bens", ",", "kein", "Lie\u00b7bes\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "TRUNC", "$,", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie rei\u00dfen's mit frechen H\u00e4nden;", "tokens": ["Sie", "rei\u00b7\u00dfen's", "mit", "fre\u00b7chen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wie soll, o Herr, mit dem Vaterland", "tokens": ["Wie", "soll", ",", "o", "Herr", ",", "mit", "dem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "$,", "FM", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Das enden, das enden!", "tokens": ["Das", "en\u00b7den", ",", "das", "en\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PDS", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Himmel ist blau! Den gr\u00fcnen Pokal", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", "!", "Den", "gr\u00fc\u00b7nen", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit rinnendem Golde befeuchtet!", "tokens": ["Mi