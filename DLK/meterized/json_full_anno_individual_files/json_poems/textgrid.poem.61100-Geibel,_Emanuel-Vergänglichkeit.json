{"textgrid.poem.61100": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Verg\u00e4nglichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df alles uns so rasch vor\u00fcbereilet", "tokens": ["Da\u00df", "al\u00b7les", "uns", "so", "rasch", "vor\u00b7\u00fc\u00b7be\u00b7rei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sich die Zeit nicht l\u00e4\u00dft in Fesseln schlagen,", "tokens": ["Und", "sich", "die", "Zeit", "nicht", "l\u00e4\u00dft", "in", "Fes\u00b7seln", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "PTKNEG", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war mir nimmermehr ein Grund zu klagen,", "tokens": ["Es", "war", "mir", "nim\u00b7mer\u00b7mehr", "ein", "Grund", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich im Kreis der Fr\u00f6hlichen verweilet.", "tokens": ["Wenn", "ich", "im", "Kreis", "der", "Fr\u00f6h\u00b7li\u00b7chen", "ver\u00b7wei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn \u00f6fter noch hat mir es Trost erteilet,", "tokens": ["Denn", "\u00f6f\u00b7ter", "noch", "hat", "mir", "es", "Trost", "er\u00b7tei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn auf der Seele tiefe Schatten lagen;", "tokens": ["Wenn", "auf", "der", "See\u00b7le", "tie\u00b7fe", "Schat\u00b7ten", "la\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der bangen durft' ich dann vertrauend sagen:", "tokens": ["Der", "ban\u00b7gen", "durft'", "ich", "dann", "ver\u00b7trau\u00b7end", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Getrost! Der Sand verrinnt, die Wunde heilet.", "tokens": ["Ge\u00b7trost", "!", "Der", "Sand", "ver\u00b7rinnt", ",", "die", "Wun\u00b7de", "hei\u00b7let", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So hofft' ich stets dem jungen Lenz entgegen,", "tokens": ["So", "hofft'", "ich", "stets", "dem", "jun\u00b7gen", "Lenz", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War ich vom Frost des Winters kalt umschauert,", "tokens": ["War", "ich", "vom", "Frost", "des", "Win\u00b7ters", "kalt", "um\u00b7schau\u00b7ert", ","], "token_info": 