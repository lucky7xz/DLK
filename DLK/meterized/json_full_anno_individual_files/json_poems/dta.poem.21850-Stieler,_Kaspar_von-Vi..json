{"dta.poem.21850": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem Kusse kahm mir alles Leiden/", "tokens": ["Von", "ei\u00b7nem", "Kus\u00b7se", "kahm", "mir", "al\u00b7les", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "au\u00df einem Ku\u00df\u2019 entstunden mir die Freuden/", "tokens": ["au\u00df", "ei\u00b7nem", "Ku\u00df'", "ent\u00b7stun\u00b7den", "mir", "die", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dergleichen ich mein Tage nie geno\u00df.", "tokens": ["derg\u00b7lei\u00b7chen", "ich", "mein", "Ta\u00b7ge", "nie", "ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Lust erregt\u2019 ihr Lenz der roten Wangen.", "tokens": ["Die", "Lust", "er\u00b7regt'", "ihr", "Lenz", "der", "ro\u00b7ten", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es hat der Nord das Leid erst auffgefangen.", "tokens": ["Es", "hat", "der", "Nord", "das", "Leid", "erst", "auff\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So ist die Freude niemahl Leiden-blo\u00df.", "tokens": ["So", "ist", "die", "Freu\u00b7de", "nie\u00b7mahl", "Lei\u00b7den\u00b7blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}