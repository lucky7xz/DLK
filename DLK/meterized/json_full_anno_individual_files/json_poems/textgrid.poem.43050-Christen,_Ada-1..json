{"textgrid.poem.43050": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6nnt' ich Alles geben,", "tokens": ["O", "k\u00f6nnt'", "ich", "Al\u00b7les", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was dieses Herz bewegt,", "tokens": ["Was", "die\u00b7ses", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und all die tausend Gedanken,", "tokens": ["Und", "all", "die", "tau\u00b7send", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "CARD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die w\u00fcst mein Sch\u00e4del hegt! \u2013", "tokens": ["Die", "w\u00fcst", "mein", "Sch\u00e4\u00b7del", "hegt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es dr\u00e4nget hei\u00df zur Lippe,", "tokens": ["Es", "dr\u00e4n\u00b7get", "hei\u00df", "zur", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir das Herz zerbricht;", "tokens": ["Was", "mir", "das", "Herz", "zer\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kenn' es, ach, ich f\u00fchl' es \u2013", "tokens": ["Ich", "kenn'", "es", ",", "ach", ",", "ich", "f\u00fchl'", "es", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ITJ", "$,", "PPER", "VVFIN", "PPER", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch sagen kann ich's nicht!", "tokens": ["Doch", "sa\u00b7gen", "kann", "ich's", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O k\u00f6nnt' ich Alles geben,", "tokens": ["O", "k\u00f6nnt'", "ich", "Al\u00b7les", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was dieses Herz bewegt,", "tokens": ["Was", "die\u00b7ses", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und all die tausend Gedanken,", "tokens": ["Und", "all", "die", "tau\u00b7send", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "CARD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die w\u00fcst mein Sch\u00e4del hegt! \u2013", "tokens": ["Die", "w\u00fcst", "mein", "Sch\u00e4\u00b7del", "hegt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es dr\u00e4nget hei\u00df zur Lippe,", "tok