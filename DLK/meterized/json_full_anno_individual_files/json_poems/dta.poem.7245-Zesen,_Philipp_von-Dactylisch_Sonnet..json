{"dta.poem.7245": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Dactylisch  Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heilige Hertzen vnd heilige ", "tokens": ["Hei\u00b7li\u00b7ge", "Hert\u00b7zen", "vnd", "hei\u00b7li\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Schm\u00e4cket vnd sehet die geistliche Lust/", "tokens": ["Schm\u00e4\u00b7cket", "vnd", "se\u00b7het", "die", "geist\u00b7li\u00b7che", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Welche soll Jhnen seyn billich bewust;", "tokens": ["Wel\u00b7che", "soll", "Jh\u00b7nen", "seyn", "bil\u00b7lich", "be\u00b7wust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Salomons Geistlichen ", "tokens": ["Sa\u00b7lo\u00b7mons", "Geist\u00b7li\u00b7chen"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Lesset das heilige Br\u00fcnnelein ", "tokens": ["Les\u00b7set", "das", "hei\u00b7li\u00b7ge", "Br\u00fcn\u00b7nel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nectar/ vnd labet die durstige brust:", "tokens": ["Nec\u00b7tar", "/", "vnd", "la\u00b7bet", "die", "durs\u00b7ti\u00b7ge", "brust", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Reichlich ergie\u00dfet sich selbiger must", "tokens": ["Reich\u00b7lich", "er\u00b7gie\u00b7\u00dfet", "sich", "sel\u00b7bi\u00b7ger", "must"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Lasset vns Diesen mit liebe ", "tokens": ["Las\u00b7set", "vns", "Die\u00b7sen", "mit", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "APPR", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "welchen vns Salomon artlich beschreibt/", "tokens": ["wel\u00b7chen", "vns", "Sa\u00b7lo\u00b7mon", "art\u00b7lich", "be\u00b7schreibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "NE", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "welcher vns schmertzen im hertzen vertreibt:", "tokens": ["wel\u00b7cher", "vns", "schmert\u00b7zen", "im", "hert\u00b7zen", "ver\u00b7treibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Dencket doch aber vnd mercket darneben/", "tokens": ["Den\u00b7cket", "doch", "a\u00b7ber", "vnd", "mer\u00b7cket", "dar\u00b7ne\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KON", "VVFIN", "PAV", "$("], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Da\u00df Jhn entz\u00fcndet der Heilige Geist/", "tokens": ["Da\u00df", "Jhn", "ent\u00b7z\u00fcn\u00b7det", "der", "Hei\u00b7li\u00b7ge", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Solche geheimn\u00fc\u00df kein Mensche beweist/", "tokens": ["Sol\u00b7che", "ge\u00b7heim\u00b7n\u00fc\u00df", "kein", "Men\u00b7sche", "be\u00b7weist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PIAT", "NN", "VVFIN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Da\u00df er ergr\u00fcnde das G\u00f6ttliche Leben.", "tokens": ["Da\u00df", "er", "er\u00b7gr\u00fcn\u00b7de", "das", "G\u00f6tt\u00b7li\u00b7che", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}