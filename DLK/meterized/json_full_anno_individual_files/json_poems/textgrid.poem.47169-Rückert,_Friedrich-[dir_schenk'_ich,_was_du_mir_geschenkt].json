{"textgrid.poem.47169": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[dir schenk' ich, was du mir geschenkt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir schenk' ich, was du mir geschenkt;", "tokens": ["Dir", "schenk'", "ich", ",", "was", "du", "mir", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich dir schenkte, schenk' ich wieder:", "tokens": ["Was", "ich", "dir", "schenk\u00b7te", ",", "schenk'", "ich", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Herz wird jung, so oft es denkt", "tokens": ["Mein", "Herz", "wird", "jung", ",", "so", "oft", "es", "denkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir gesung'nen Jugendlieder.", "tokens": ["Der", "dir", "ge\u00b7sung'\u00b7nen", "Ju\u00b7gend\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir alterten, sie blieben jung,", "tokens": ["Wir", "al\u00b7ter\u00b7ten", ",", "sie", "blie\u00b7ben", "jung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PPER", "VVFIN", "ADJD", "$,"],