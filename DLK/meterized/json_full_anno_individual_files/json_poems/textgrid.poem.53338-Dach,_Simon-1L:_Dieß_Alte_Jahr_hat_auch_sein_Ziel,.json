, "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Aus unserm Land'", "tokens": ["Aus", "un\u00b7serm", "Land'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Vnd was vns kan verderben,", "tokens": ["Vnd", "was", "vns", "kan", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "K\u00f6mmt aber dann", "tokens": ["K\u00f6mmt", "a\u00b7ber", "dann"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der Todt vns an,", "tokens": ["Der", "Todt", "vns", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "So hilff vns seelig sterben.", "tokens": ["So", "hilff", "vns", "see\u00b7lig", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}