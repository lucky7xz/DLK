{"textgrid.poem.53338": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die\u00df Alte Jahr hat auch sein Ziel,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df Alte Jahr hat auch sein Ziel,", "tokens": ["Die\u00df", "Al\u00b7te", "Jahr", "hat", "auch", "sein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das New' ist angetretten:", "tokens": ["Das", "Ne\u00b7w'", "ist", "an\u00b7ge\u00b7tret\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wach auff mein Lob- vnd Seiten-Spiel", "tokens": ["Wach", "auff", "mein", "Lob", "vnd", "Sei\u00b7ten\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Danck vnd mit Gebehten,", "tokens": ["Mit", "Danck", "vnd", "mit", "Ge\u00b7beh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Rh\u00fcm den allein,", "tokens": ["Rh\u00fcm", "den", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Der alle Pein,", "tokens": ["Der", "al\u00b7le", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So vns bi\u00dfher getroffen,", "tokens": ["So", "vns", "bi\u00df\u00b7her", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit starcker Hand", "tokens": ["Mit", "star\u00b7cker", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Hat abgewand,", "tokens": ["Hat", "ab\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Vnd heisst vns bessrung hoffen.", "tokens": ["Vnd", "heisst", "vns", "bess\u00b7rung", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer h\u00e4tte kurtz zuvor gemeint,", "tokens": ["Wer", "h\u00e4t\u00b7te", "kurtz", "zu\u00b7vor", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem H\u00f6chsten Lob-zu-singen?", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "Lob\u00b7zu\u00b7sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Furcht war, da\u00df der wilde Feind", "tokens": ["Als", "Furcht", "war", ",", "da\u00df", "der", "wil\u00b7de", "Feind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht' vns auch zu verschlingen,", "tokens": ["Sucht'", "vns", "auch", "zu", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Sebel fra\u00df", "tokens": ["Sein", "Se\u00b7bel", "fra\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ohn alle Ma\u00df", "tokens": ["Ohn", "al\u00b7le", "Ma\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Nachbarn Gut vnd Leben,", "tokens": ["Der", "Nach\u00b7barn", "Gut", "vnd", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir schryen: Ach", "tokens": ["Wir", "schry\u00b7en", ":", "Ach"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Jetzt wird die Rach", "tokens": ["Jetzt", "wird", "die", "Rach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Jetzt \u00fcber vns auch schweben!", "tokens": ["Jetzt", "\u00fc\u00b7ber", "vns", "auch", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn vnsers Hauptes Krohne war", "tokens": ["Denn", "vn\u00b7sers", "Haup\u00b7tes", "Kroh\u00b7ne", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vns Armen abgefallen,", "tokens": ["Vns", "Ar\u00b7men", "ab\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daher entspann sich die Gefahr,", "tokens": ["Da\u00b7her", "ent\u00b7spann", "sich", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd w\u00fctet vnter allen,", "tokens": ["Vnd", "w\u00fc\u00b7tet", "vn\u00b7ter", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am meisten drewt'", "tokens": ["Am", "meis\u00b7ten", "drewt'"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Vneinigheit", "tokens": ["Vnei\u00b7nig\u00b7heit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Auch innerliche Kriege,", "tokens": ["Auch", "in\u00b7ner\u00b7li\u00b7che", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df \u00fcberall", "tokens": ["Da\u00df", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Furcht, Angst vnd Fall", "tokens": ["Furcht", ",", "Angst", "vnd", "Fall"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Behielten Pl\u00e4tz' vnd Siege.", "tokens": ["Be\u00b7hiel\u00b7ten", "Pl\u00e4tz'", "vnd", "Sie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo bleibt der Nahrung Abgang? Wo", "tokens": ["Wo", "bleibt", "der", "Nah\u00b7rung", "Ab\u00b7gang", "?", "Wo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$.", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Armut hin vnd wieder?", "tokens": ["Die", "Ar\u00b7mut", "hin", "vnd", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Eins klagt sich so, das andre so,", "tokens": ["Eins", "klagt", "sich", "so", ",", "das", "and\u00b7re", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "$,", "PRELS", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es weis sein Creutz ein jeder,", "tokens": ["Es", "weis", "sein", "Creutz", "ein", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PPOSAT", "NN", "ART", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So manches Hertz", "tokens": ["So", "man\u00b7ches", "Hertz"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "So mancher Schmertz.", "tokens": ["So", "man\u00b7cher", "Schmertz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nun hat es Gott gewendet,", "tokens": ["Nun", "hat", "es", "Gott", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd vnser Leydt", "tokens": ["Vnd", "vn\u00b7ser", "Leydt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Gleich mit der Zeit", "tokens": ["Gleich", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Des Alten Jahrs geendet.", "tokens": ["Des", "Al\u00b7ten", "Jahrs", "ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er hat den K\u00f6niglichen Stul", "tokens": ["Er", "hat", "den", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Stul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vns wieder auffgerichtet,", "tokens": ["Vns", "wie\u00b7der", "auff\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erhalten Rahthau\u00df, Kirch vnd Schul", "tokens": ["Er\u00b7hal\u00b7ten", "Rah\u00b7thau\u00df", ",", "Kirch", "vnd", "Schul"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd manchen Sturm geschlichtet,", "tokens": ["Vnd", "man\u00b7chen", "Sturm", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Creutzes Last", "tokens": ["Des", "Creut\u00b7zes", "Last"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mit angefasst,", "tokens": ["Mit", "an\u00b7ge\u00b7fasst", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Vns nie zu hart geschlagen,", "tokens": ["Vns", "nie", "zu", "hart", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd stets Gedult", "tokens": ["Vnd", "stets", "Ge\u00b7dult"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mit vnsrer Schuld", "tokens": ["Mit", "vns\u00b7rer", "Schuld"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Nach Vaters Art getragen.", "tokens": ["Nach", "Va\u00b7ters", "Art", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drumb werd' Er auch von vns erh\u00f6ht,", "tokens": ["Drumb", "werd'", "Er", "auch", "von", "vns", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprecht: Vater, deine G\u00fcte,", "tokens": ["Sprecht", ":", "Va\u00b7ter", ",", "dei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die \u00fcber Erd vnd Himmel geht,", "tokens": ["Die", "\u00fc\u00b7ber", "Erd", "vnd", "Him\u00b7mel", "geht", ","], "token_info": ["word", "word", 