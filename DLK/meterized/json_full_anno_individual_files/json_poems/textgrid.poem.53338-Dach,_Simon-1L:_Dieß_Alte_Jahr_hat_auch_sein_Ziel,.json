o": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist stets bey vns geblieben,", "tokens": ["Bist", "stets", "bey", "vns", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das Wetter so vns sehr erschreckt,", "tokens": ["Das", "Wet\u00b7ter", "so", "vns", "sehr", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du gew\u00fcnscht vertrieben:", "tokens": ["Hast", "du", "ge\u00b7w\u00fcnscht", "ver\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd allen Brod,", "tokens": ["Vnd", "al\u00b7len", "Brod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "So viel vns Noht,", "tokens": ["So", "viel", "vns", "Noht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu richter Zeit bescheret,", "tokens": ["Zu", "rich\u00b7ter", "Zeit", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie wird hief\u00fcr", "tokens": ["Wie", "wird", "hie\u00b7f\u00fcr"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VAFIN", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": 