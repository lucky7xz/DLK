{"textgrid.poem.31355": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Erzherzog Karl", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4ttest du T\u00fcrenne besiegt,", "tokens": ["H\u00e4t\u00b7test", "du", "T\u00fc\u00b7ren\u00b7ne", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der, an dem Z\u00fcgel der Einsicht,", "tokens": ["Der", ",", "an", "dem", "Z\u00fc\u00b7gel", "der", "Ein\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Leicht, den ehernen Wagen des Kriegs,", "tokens": ["Leicht", ",", "den", "e\u00b7her\u00b7nen", "Wa\u00b7gen", "des", "Kriegs", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie ein M\u00e4dchen ruhige Rosse, lenkte;", "tokens": ["Wie", "ein", "M\u00e4d\u00b7chen", "ru\u00b7hi\u00b7ge", "Ros\u00b7se", ",", "lenk\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "--+-++-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Oder jenen Gustav der Schweden,", "tokens": ["O\u00b7der", "je\u00b7nen", "Gus\u00b7tav", "der", "Schwe\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Der, an dem Tage der Schlacht,", "tokens": ["Der", ",", "an", "dem", "Ta\u00b7ge", "der", "Schlacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Seraphische Streiter zu H\u00fclfe rief;", "tokens": ["Se\u00b7ra\u00b7phi\u00b7sche", "Strei\u00b7ter", "zu", "H\u00fcl\u00b7fe", "rief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Oder den Suwarow, oder den Soltikow,", "tokens": ["O\u00b7der", "den", "Su\u00b7wa\u00b7row", ",", "o\u00b7der", "den", "Sol\u00b7ti\u00b7kow", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "$,", "KON", "ART", "NE", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Die, bei der Drommete Klang,", "tokens": ["Die", ",", "bei", "der", "Drom\u00b7me\u00b7te", "Klang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Alle D\u00e4mme der Streitlust niedertraten,", "tokens": ["Al\u00b7le", "D\u00e4m\u00b7me", "der", "Streit\u00b7lust", "nie\u00b7der\u00b7tra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Und mit B\u00e4chen von Blut,", "tokens": ["Und", "mit", "B\u00e4\u00b7chen", "von", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Die granitene Bahn des Siegs sich sprengten:", "tokens": ["Die", "gra\u00b7ni\u00b7te\u00b7ne", "Bahn", "des", "Siegs", "sich", "spreng\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Siehe, die Jungfraun rief' ich herbei des Landes,", "tokens": ["Sie\u00b7he", ",", "die", "Jung\u00b7fraun", "rie\u00b7f'", "ich", "her\u00b7bei", "des", "Lan\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df sie zum Kranz den Lorbeer fl\u00f6chten,", "tokens": ["Da\u00df", "sie", "zum", "Kranz", "den", "Lor\u00b7beer", "fl\u00f6ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dir die Scheitel, o Herr, zu kr\u00f6nen!", "tokens": ["Dir", "die", "Schei\u00b7tel", ",", "o", "Herr", ",", "zu", "kr\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "FM", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber wen ruf ich (o Herz, was klopfst du?),", "tokens": ["A\u00b7ber", "wen", "ruf", "ich", "(", "o", "Herz", ",", "was", "klopfst", "du", "?", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$(", "FM", "NN", "$,", "PWS", "VVFIN", "PPER", "$.", "$(", "$,"], "