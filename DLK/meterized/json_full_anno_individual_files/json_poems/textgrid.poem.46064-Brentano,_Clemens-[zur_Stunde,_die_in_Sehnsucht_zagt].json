{"textgrid.poem.46064": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[zur Stunde, die in Sehnsucht zagt]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Stunde, die in Sehnsucht zagt,", "tokens": ["Zur", "Stun\u00b7de", ",", "die", "in", "Sehn\u00b7sucht", "zagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schiffer tief das Herz beweget", "tokens": ["Dem", "Schif\u00b7fer", "tief", "das", "Herz", "be\u00b7we\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Freunden heut Lebwohl gesagt,", "tokens": ["Der", "Freun\u00b7den", "heut", "Leb\u00b7wohl", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Liebe in dem Pilger reget,", "tokens": ["Und", "Lie\u00b7be", "in", "dem", "Pil\u00b7ger", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6rt er, wie ferne Abendglockenkl\u00e4nge scheinen", "tokens": ["H\u00f6rt", "er", ",", "wie", "fer\u00b7ne", "A\u00b7bend\u00b7glo\u00b7cken\u00b7kl\u00e4n\u00b7ge", "schei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.