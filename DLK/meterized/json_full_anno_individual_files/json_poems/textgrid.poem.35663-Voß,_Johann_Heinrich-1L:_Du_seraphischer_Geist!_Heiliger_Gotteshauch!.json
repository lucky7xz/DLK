{"textgrid.poem.35663": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du seraphischer Geist! Heiliger Gotteshauch!", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du seraphischer Geist! Heiliger Gotteshauch!", "tokens": ["Du", "se\u00b7ra\u00b7phi\u00b7scher", "Geist", "!", "Hei\u00b7li\u00b7ger", "Got\u00b7tes\u00b7hauch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "---+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wird mein sterblicher Blick nimmer gew\u00fcrdiget,", "tokens": ["Wird", "mein", "sterb\u00b7li\u00b7cher", "Blick", "nim\u00b7mer", "ge\u00b7w\u00fcr\u00b7di\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Deiner Herrlichkeit Abglanz,", "tokens": ["Dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "Ab\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Jene Himmelsgestalt, zu schaun?", "tokens": ["Je\u00b7ne", "Him\u00b7mels\u00b7ge\u00b7stalt", ",", "zu", "schaun", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "O so wandelte Fluch, als ich geboren ward!", "tokens": ["O", "so", "wan\u00b7del\u00b7te", "Fluch", ",", "als", "ich", "ge\u00b7bo\u00b7ren", "ward", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Nacht, so keimten in dir l\u00e4ndervergiftende", "tokens": ["Nacht", ",", "so", "ke\u00b7im\u00b7ten", "in", "dir", "l\u00e4n\u00b7der\u00b7ver\u00b7gif\u00b7ten\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "APPR", "PPER", "ADJA"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.3": {"text": "Schandges\u00e4ng', und ein K\u00f6nig", "tokens": ["Schand\u00b7ge\u00b7s\u00e4ng'", ",", "und", "ein", "K\u00f6\u00b7nig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sann der Freiheit die Fessel aus!", "tokens": ["Sann", "der", "Frei\u00b7heit", "die", "Fes\u00b7sel", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "So gab Gott mir im Zorn dieses phantastische", "tokens": ["So", "gab", "Gott", "mir", "im", "Zorn", "die\u00b7ses", "phan\u00b7tas\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPER", "APPRART", "NN", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herz, das geniusk\u00fchn zaubernde Tr\u00e4ume schafft,", "tokens": ["Herz", ",", "das", "ge\u00b7nius\u00b7k\u00fchn", "zau\u00b7bern\u00b7de", "Tr\u00e4u\u00b7me", "schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dann abg\u00f6ttische Thr\u00e4nen", "tokens": ["Dann", "ab\u00b7g\u00f6t\u00b7ti\u00b7sche", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Vor dem eignen Gesch\u00f6pfe weint! \u2013", "tokens": ["Vor", "dem", "eig\u00b7nen", "Ge\u00b7sch\u00f6p\u00b7fe", "weint", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Traum war, t\u00e4uschender Traum, dieser erhabne Blick?", "tokens": ["Traum", "war", ",", "t\u00e4u\u00b7schen\u00b7der", "Traum", ",", "die\u00b7ser", "er\u00b7hab\u00b7ne", "Blick", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADJA", "NN", "$,", "PDAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dieses Beben der Brust? dieser edenische", "tokens": ["Die\u00b7ses", "Be\u00b7ben", "der", "Brust", "?", "die\u00b7ser", "e\u00b7de\u00b7ni\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN", "$.", "PDAT", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fr\u00fchling l\u00e4chelnder Wangen?", "tokens": ["Fr\u00fch\u00b7ling", "l\u00e4\u00b7cheln\u00b7der", "Wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ganz der himmlischen Seele Bild?", "tokens": ["Ganz", "der", "himm\u00b7li\u00b7schen", "See\u00b7le", "Bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Nein! so wahr er im Sturm freudiger Schauer mich", "tokens": ["Nein", "!", "so", "wahr", "er", "im", "Sturm", "freu\u00b7di\u00b7ger", "Schau\u00b7er", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "ADJD", "PPER", "APPRART", "NN", "ADJA", "NN", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Drauf, durch Sph\u00e4rengesang, unter die Bl\u00fcten ri\u00df,", "tokens": ["Drauf", ",", "durch", "Sph\u00e4\u00b7ren\u00b7ge\u00b7sang", ",", "un\u00b7ter", "die", "Bl\u00fc\u00b7ten", "ri\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo in goldenen Schalen", "tokens": ["Wo", "in", "gol\u00b7de\u00b7nen", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mir Unsterblichkeit funkelte:", "tokens": ["Mir", "U\u00b7nsterb\u00b7lich\u00b7keit", "fun\u00b7kel\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Diesen g\u00f6ttlichen Traum schuf mir ihr Genius!", "tokens": ["Die\u00b7sen", "g\u00f6tt\u00b7li\u00b7chen", "Traum", "schuf", "mir", "ihr", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihren ahndenden Wunsch h\u00fcllt er in Morgenglanz,", "tokens": ["Ih\u00b7ren", "ahn\u00b7den\u00b7den", "Wunsch", "h\u00fcllt", "er", "in", "Mor\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Bracht' in Schlummergew\u00f6lken", "tokens": ["Bracht'", "in", "Schlum\u00b7mer\u00b7ge\u00b7w\u00f6l\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dann die heilige Bildung mir!", "tokens": ["Dann", "die", "hei\u00b7li\u00b7ge", "Bil\u00b7dung", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Uns, zur Liebe bestimmt, ach! zu der feurigsten", "tokens": ["Uns", ",", "zur", "Lie\u00b7be", "be\u00b7stimmt", ",", "ach", "!", "zu", "der", "feu\u00b7rigs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPRART", "NN", "VVPP", "$,", "ITJ", "$.", "APPR", "ART", "ADJA"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Reinsten Liebe bestimmt! warum, o Selma, schrieb", "tokens": ["Reins\u00b7ten", "Lie\u00b7be", "be\u00b7stimmt", "!", "wa\u00b7rum", ",", "o", "Sel\u00b7ma", ",", "schrieb"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVPP", "$.", "PWAV", "$,", "FM", "NE", "$,", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dort ein schwarzes Verh\u00e4ngnis", "tokens": ["Dort", "ein", "schwar\u00b7zes", "Ver\u00b7h\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Unsre Trennung mit Sternenschrift?", "tokens": ["Uns\u00b7re", "Tren\u00b7nung", "mit", "Ster\u00b7nen\u00b7schrift", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Ach! ich f\u00fchl' es, sie feufzt! Eile, gefl\u00fcgelter!", "tokens": ["Ach", "!", "ich", "f\u00fchl'", "es", ",", "sie", "feufzt", "!", "Ei\u00b7le", ",", "ge\u00b7fl\u00fc\u00b7gel\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "ADJD", "$.", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Selma seufzet dir auch! Eile, begl\u00fcckter Tag,", "tokens": ["Sel\u00b7ma", "seuf\u00b7zet", "dir", "auch", "!", "Ei\u00b7le", ",", "be\u00b7gl\u00fcck\u00b7ter", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der in Thr\u00e4nen der Liebe", "tokens": ["Der", "in", "Thr\u00e4\u00b7nen", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meinem zitternden Arm sie schenkt!", "tokens": ["Mei\u00b7nem", "zit\u00b7tern\u00b7den", "Arm", "sie", "schenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Flamme Gottes, du strahlst, Liebe! der Sonne gleich,", "tokens": ["Flam\u00b7me", "Got\u00b7tes", ",", "du", "strahlst", ",", "Lie\u00b7be", "!", "der", "Son\u00b7ne", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "$,", "NN", "$.", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Auf des Todes Gefild Leben und Sch\u00f6nheit aus!", "tokens": ["Auf", "des", "To\u00b7des", "Ge\u00b7fild", "Le\u00b7ben", "und", "Sch\u00f6n\u00b7heit", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Gleich dem Liede Sionas,", "tokens": ["Gleich", "dem", "Lie\u00b7de", "Si\u00b7o\u00b7nas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrmst du Seelen zu Gott empor!", "tokens": ["St\u00fcrmst", "du", "See\u00b7len", "zu", "Gott", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Oft durchbebtest du mich, Liebe! doch unerkannt,", "tokens": ["Oft", "durch\u00b7beb\u00b7test", "du", "mich", ",", "Lie\u00b7be", "!", "doch", "un\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "$.", "ADV", "ADJD", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schien dein Odem mir jetzt Balsam der Sommernacht,", "tokens": ["Schien", "dein", "O\u00b7dem", "mir", "jetzt", "Bal\u00b7sam", "der", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jetzt ein S\u00e4useln des Fr\u00fchlings,", "tokens": ["Jetzt", "ein", "S\u00e4u\u00b7seln", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Jetzt ein Seufzen der Nachtigall!", "tokens": ["Jetzt", "ein", "Seuf\u00b7zen", "der", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Schon im schattigen Thal, wo wir, noch Seelen nur,", "tokens": ["Schon", "im", "schat\u00b7ti\u00b7gen", "Thal", ",", "wo", "wir", ",", "noch", "See\u00b7len", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "NN", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Tr\u00e4umten, spielten wir stets unter demselben Strauch,", "tokens": ["Tr\u00e4um\u00b7ten", ",", "spiel\u00b7ten", "wir", "stets", "un\u00b7ter", "dem\u00b7sel\u00b7ben", "Strauch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Pfl\u00fcckten einerlei Blumen,", "tokens": ["Pfl\u00fcck\u00b7ten", "ei\u00b7ner\u00b7lei", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Horchten einerlei Harmonie.", "tokens": ["Horch\u00b7ten", "ei\u00b7ner\u00b7lei", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Doch die Seraphim, einst unserer Pilgerschaft", "tokens": ["Doch", "die", "Se\u00b7ra\u00b7phim", ",", "einst", "un\u00b7se\u00b7rer", "Pil\u00b7ger\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu Geleitern gesellt, senkten den Psalterton", "tokens": ["Zu", "Ge\u00b7lei\u00b7tern", "ge\u00b7sellt", ",", "senk\u00b7ten", "den", "Psal\u00b7ter\u00b7ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "ART", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oft zum Lispel der Wehmut,", "tokens": ["Oft", "zum", "Lis\u00b7pel", "der", "Weh\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Blickten seitw\u00e4rts, und weineten.", "tokens": ["Blick\u00b7ten", "seit\u00b7w\u00e4rts", ",", "und", "wei\u00b7ne\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "VVFIN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Jetzo weinen auch wir! Dumpfere Trauer bebt", "tokens": ["Jet\u00b7zo", "wei\u00b7nen", "auch", "wir", "!", "Dum\u00b7pfe\u00b7re", "Trau\u00b7er", "bebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Diese Saiten herab! Denn in der Mitternacht", "tokens": ["Die\u00b7se", "Sai\u00b7ten", "her\u00b7ab", 