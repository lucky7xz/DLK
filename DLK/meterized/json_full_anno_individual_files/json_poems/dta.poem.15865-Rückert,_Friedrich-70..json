{"dta.poem.15865": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh wie den Zweck erreicht, und der Gefahr entweicht", "tokens": ["Sieh", "wie", "den", "Zweck", "er\u00b7reicht", ",", "und", "der", "Ge\u00b7fahr", "ent\u00b7weicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ART", "NN", "VVPP", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Efeu, der empor am Stamm der Buche schleicht.", "tokens": ["Der", "E\u00b7feu", ",", "der", "em\u00b7por", "am", "Stamm", "der", "Bu\u00b7che", "schleicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKVZ", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Nicht um den ganzen Stamm rings flicht er seine Str\u00e4nge,", "tokens": ["Nicht", "um", "den", "gan\u00b7zen", "Stamm", "rings", "flicht", "er", "sei\u00b7ne", "Str\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht der Baum, wenn er sich wachsend dehnt, sie sprenge.", "tokens": ["Da\u00df", "nicht", "der", "Baum", ",", "wenn", "er", "sich", "wach\u00b7send", "dehnt", ",", "sie", "spren\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gradaufwerts kriecht er nur; villeicht in k\u00fcnft'gen Tagen,", "tokens": ["Grad\u00b7auf\u00b7werts", "kriecht", "er", "nur", ";", "vil\u00b7leicht", "in", "k\u00fcnft'\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann nicht der Baum mehr w\u00e4chst, wird er sich rundum wagen.", "tokens": ["Wann", "nicht", "der", "Baum", "mehr", "w\u00e4chst", ",", "wird", "er", "sich", "run\u00b7dum", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dagegen dis Gerank, das nur den Sommer lebt,", "tokens": ["Da\u00b7ge\u00b7gen", "dis", "Ge\u00b7rank", ",", "das", "nur", "den", "Som\u00b7mer", "lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PDS", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von allen Seiten um den Stamm sich sorglos webt.", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "um", "den", "Stamm", "sich", "sorg\u00b7los", "webt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vom heur'gen Safttrieb ist sein Wachsthum nicht bedroht;", "tokens": ["Vom", "heur'\u00b7gen", "Saft\u00b7trieb", "ist", "sein", "Wach\u00b7sthum", "nicht", "be\u00b7droht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eh der n\u00e4chste schwillt, ist es schon selber todt.", "tokens": ["Und", "eh", "der", "n\u00e4chs\u00b7te", "schwillt", ",", "ist", "es", "schon", "sel\u00b7ber", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}