{"textgrid.poem.48334": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "3. Grabschrift", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kamst nur, um dein heilig Amt zu schaun,", "tokens": ["Du", "kamst", "nur", ",", "um", "dein", "hei\u00b7lig", "Amt", "zu", "schaun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUI", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du fand'st nicht Zeit, zu bilden und zu baun,", "tokens": ["Du", "fan\u00b7d'st", "nicht", "Zeit", ",", "zu", "bil\u00b7den", "und", "zu", "baun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht Zeit, der Zeit den Stempel aufzudr\u00fccken,", "tokens": ["Nicht", "Zeit", ",", "der", "Zeit", "den", "Stem\u00b7pel", "auf\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du fand'st nur eben Zeit noch, zu begl\u00fccken;", "tokens": ["Du", "fan\u00b7d'st", "nur", "e\u00b7ben", "Zeit", "noch", ",", "zu", "be\u00b7gl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "ADV", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Du sahst