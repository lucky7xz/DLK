{"textgrid.poem.40789": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dicht umwogt von Volkesmenge ragt ein luftig farbig Zelt;", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dicht umwogt von Volkesmenge ragt ein luftig farbig Zelt;", "tokens": ["Dicht", "um\u00b7wogt", "von", "Vol\u00b7kes\u00b7men\u00b7ge", "ragt", "ein", "luf\u00b7tig", "far\u00b7big", "Zelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "VVFIN", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ei, was doch die bunte H\u00fclle wohl f\u00fcr einen Schatz enth\u00e4lt?", "tokens": ["Ei", ",", "was", "doch", "die", "bun\u00b7te", "H\u00fcl\u00b7le", "wohl", "f\u00fcr", "ei\u00b7nen", "Schatz", "ent\u00b7h\u00e4lt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Birgt sie nicht die sch\u00f6nste Perle, Muscheln gleich, in schlichtem Schrein?", "tokens": ["Birgt", "sie", "nicht", "die", "sch\u00f6ns\u00b7te", "Per\u00b7le", ",", "Mu\u00b7scheln", "gleich", ",", "in", "schlich\u00b7tem", "Schrein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "H\u00fcllt sie nicht das sch\u00f6nste Antlitz, wie ein neid'scher Schleier, ein?", "tokens": ["H\u00fcllt", "sie", "nicht", "das", "sch\u00f6ns\u00b7te", "Ant\u00b7litz", ",", "wie", "ein", "nei\u00b7d'\u00b7scher", "Schlei\u00b7er", ",", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Glockenklang, Kanonendonner! \u2013 Sieh, des Zeltes H\u00fclle sank,", "tokens": ["Glo\u00b7cken\u00b7klang", ",", "Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", "!", "\u2013", "Sieh", ",", "des", "Zel\u00b7tes", "H\u00fcl\u00b7le", "sank", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "$(", "NE", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und enth\u00fcllt' ein riesig Standbild, erzgegossen, hell und blank!", "tokens": ["Und", "ent\u00b7h\u00fcllt'", "ein", "rie\u00b7sig", "Stand\u00b7bild", ",", "erz\u00b7ge\u00b7gos\u00b7sen", ",", "hell", "und", "blank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "$,", "VVPP", "$,", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie zur Huld'gung, trat die Sonne jetzt auch aus dem Nebelflor!", "tokens": ["Wie", "zur", "Huld'\u00b7gung", ",", "trat", "die", "Son\u00b7ne", "jetzt", "auch", "aus", "dem", "Ne\u00b7bel\u00b7flor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Jauchzend, da\u00df die Sterne bebten, schlug des Volkes Ruf empor!", "tokens": ["Jauch\u00b7zend", ",", "da\u00df", "die", "Ster\u00b7ne", "beb\u00b7ten", ",", "schlug", "des", "Vol\u00b7kes", "Ruf", "em\u00b7por", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Ruhig auf granitnem Sockel schwebt das Kaiserbild voll Glanz,", "tokens": ["Ru\u00b7hig", "auf", "gra\u00b7nit\u00b7nem", "So\u00b7ckel", "schwebt", "das", "Kai\u00b7ser\u00b7bild", "voll", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Um die Schl\u00e4fen keine Krone, nur den selbsterrungnen Kranz!", "tokens": ["Um", "die", "Schl\u00e4\u00b7fen", "kei\u00b7ne", "Kro\u00b7ne", ",", "nur", "den", "selbs\u00b7ter\u00b7rung\u00b7nen", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PIAT", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hoch zu Ro\u00df, das Antlitz l\u00e4chelnd, und empor die rechte Hand", "tokens": ["Hoch", "zu", "Ro\u00df", ",", "das", "Ant\u00b7litz", "l\u00e4\u00b7chelnd", ",", "und", "em\u00b7por", "die", "rech\u00b7te", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "$,", "ART", "NN", "ADJD", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sanft erhoben, wie zum Segen \u00fcber sein geliebtes Land.", "tokens": ["Sanft", "er\u00b7ho\u00b7ben", ",", "wie", "zum", "Se\u00b7gen", "\u00fc\u00b7ber", "sein", "ge\u00b7lieb\u00b7tes", "Land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Ja, du bist es, weiser Joseph! \u2013 Voll von Kraft und Mark und Klang,", "tokens": ["Ja", ",", "du", "bist", "es", ",", "wei\u00b7ser", "Jo\u00b7se\u00b7ph", "!", "\u2013", "Voll", "von", "Kraft", "und", "Mark", "und", "Klang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "$,", "ADJA", "NE", "$.", "$(", "ADJD", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "So im Bilde von Metalle, wie dein Leben all' entlang!", "tokens": ["So", "im", "Bil\u00b7de", "von", "Me\u00b7tal\u00b7le", ",", "wie", "dein", "Le\u00b7ben", "all'", "ent\u00b7lang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "NE", "$,", "PWAV", "PPOSAT", "NN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dem getreu und k\u00fchn beharrlich, was als edel du erkannt,", "tokens": ["Dem", "ge\u00b7treu", "und", "k\u00fchn", "be\u00b7harr\u00b7lich", ",", "was", "als", "e\u00b7del", "du", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ADJD", "$,", "PWS", "KOKOM", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und an deinem gro\u00dfen Werke bauend fest mit ehrner Hand!", "tokens": ["Und", "an", "dei\u00b7nem", "gro\u00b7\u00dfen", "Wer\u00b7ke", "bau\u00b7end", "fest", "mit", "ehr\u00b7ner", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Ein Despot bist du gewesen! Doch ein solcher, wie der Tag,", "tokens": ["Ein", "Des\u00b7pot", "bist", "du", "ge\u00b7we\u00b7sen", "!", "Doch", "ein", "sol\u00b7cher", ",", "wie", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VAPP", "$.", "KON", "ART", "PIAT", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dessen Sonne Nacht und Nebel neben sich nicht dulden mag,", "tokens": ["Des\u00b7sen", "Son\u00b7ne", "Nacht", "und", "Ne\u00b7bel", "ne\u00b7ben", "sich", "nicht", "dul\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "KON", "NN", "APPR", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Der zu dunklen Diebesschl\u00fcften die verha\u00dfte Leuchte tr\u00e4gt,", "tokens": ["Der", "zu", "dunk\u00b7len", "Die\u00b7bes\u00b7schl\u00fcf\u00b7ten", "die", "ver\u00b7ha\u00df\u00b7te", "Leuch\u00b7te", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und mit goldner Hand ans Fenster langer Schl\u00e4fer rastlos schl\u00e4gt.", "tokens": ["Und", "mit", "gold\u00b7ner", "Hand", "ans", "Fens\u00b7ter", "lan\u00b7ger", "Schl\u00e4\u00b7fer", "rast\u00b7los", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPRART", "NN", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Ein Despot bist du gewesen! Doch, f\u00fcrwahr, ein solcher blos,", "tokens": ["Ein", "Des\u00b7pot", "bist", "du", "ge\u00b7we\u00b7sen", "!", "Doch", ",", "f\u00fcr\u00b7wahr", ",", "ein", "sol\u00b7cher", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VAPP", "$.", "KON", "$,", "ADV", "$,", "ART", "PIAT", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie der Lenz, der Schnee und K\u00e4lte treibt zur Flucht erbarmungslos;", "tokens": ["Wie", "der", "Lenz", ",", "der", "Schnee", "und", "K\u00e4l\u00b7te", "treibt", "zur", "Flucht", "er\u00b7bar\u00b7mungs\u00b7los", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Der den \u00e4rgsten Griesgram lustig mit dem hellsten Thau besprengt,", "tokens": ["Der", "den", "\u00e4rgs\u00b7ten", "Gries\u00b7gram", "lus\u00b7tig", "mit", "dem", "hells\u00b7ten", "Thau", "be\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und mit seinen Festeskr\u00e4nzen selbst den \u00e4rmsten Strauch beh\u00e4ngt.", "tokens": ["Und", "mit", "sei\u00b7nen", "Fes\u00b7tes\u00b7kr\u00e4n\u00b7zen", "selbst", "den", "\u00e4rms\u00b7ten", "Strauch", "be\u00b7h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Drum mit Recht gab dir der Bildner Brust und Stirn' und Hand von Erz!", "tokens": ["Drum", "mit", "Recht", "gab", "dir", "der", "Bild\u00b7ner", "Brust", "und", "Stirn'", "und", "Hand", "von", "Erz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "--+---+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Aber k\u00fcssen, br\u00fcnstig k\u00fcssen m\u00f6cht' ich diese Hand von Erz! \u2013", "tokens": ["A\u00b7ber", "k\u00fcs\u00b7sen", ",", "br\u00fcns\u00b7tig", "k\u00fcs\u00b7sen", "m\u00f6cht'", "ich", "die\u00b7se", "Hand", "von", "Erz", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "$,", "ADJD", "VVINF", "VMFIN", "PPER", "PDAT", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch ich wei\u00df nicht, ist es Laune, ist es kind'scher Unverstand,", "tokens": ["Doch", "ich", "wei\u00df", "nicht", ",", "ist", "es", "Lau\u00b7ne", ",", "ist", "es", "kin\u00b7d'\u00b7scher", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Aber eine Rose gerne s\u00e4h' ich in der ehrnen Hand!", "tokens": ["A\u00b7ber", "ei\u00b7ne", "Ro\u00b7se", "ger\u00b7ne", "s\u00e4h'", "ich", "in", "der", "ehr\u00b7nen", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "All dein Ringen nach dem Lichte, all dein Thun in ernster Zeit,", "tokens": ["All", "dein", "Rin\u00b7gen", "nach", "dem", "Lich\u00b7te", ",", "all", "dein", "Thun", "in", "erns\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "PIAT", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Glich's nicht einer Hand von Eisen, die uns eine Rose beut?", "tokens": ["Glich's", "nicht", "ei\u00b7ner", "Hand", "von", "Ei\u00b7sen", ",", "die", "uns", "ei\u00b7ne", "Ro\u00b7se", "beut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ein beharrlich ernstes K\u00e4mpfen um ein morgenrothes Land!", "tokens": ["Ein", "be\u00b7harr\u00b7lich", "erns\u00b7tes", "K\u00e4mp\u00b7fen", "um", "ein", "mor\u00b7gen\u00b7ro\u00b7thes", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Drum, o legt ihm weich die Rose in die harte ehrne Hand!", "tokens": ["Drum", ",", "o", "legt", "ihm", "weich", "die", "Ro\u00b7se", "in", "die", "har\u00b7te", "ehr\u00b7ne", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Was er seinem Volk geboten, war's des Fr\u00fchlings Bote nicht?", "tokens": ["Was", "er", "sei\u00b7nem", "Volk", "ge\u00b7bo\u00b7ten", ",", "wa\u00b7r's", "des", "Fr\u00fch\u00b7lings", "Bo\u00b7te", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,", "VAFIN", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Drum im Kampf er ausgedauert, stammt' es nicht aus Morgenlicht?", "tokens": ["Drum", "im", "Kampf", "er", "aus\u00b7ge\u00b7dau\u00b7ert", ",", "stammt'", "es", "nicht", "aus", "Mor\u00b7gen\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Drauf einst unverr\u00fcckt sein Auge, war's nicht ros'ger Freiheit Pfand?", "tokens": ["Drauf", "einst", "un\u00b7ver\u00b7r\u00fcckt", "sein", "Au\u00b7ge", ",", "wa\u00b7r's", "nicht", "ros'\u00b7ger", "Frei\u00b7heit", "Pfand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "PPOSAT", "NN", "$,", "VAFIN", "PTKNEG", "ADJA", "NN", "NN", "$."], "meter": "--+-+-+----+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Drum die Rose allzugerne s\u00e4h' ich in der ehrnen Hand!", "tokens": ["Drum", "die", "Ro\u00b7se", "all\u00b7zu\u00b7ger\u00b7ne", "s\u00e4h'", "ich", "in", "der", "ehr\u00b7nen", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Ach, es will der Freiheit Rose uns im Garten nicht gedeihn!", "tokens": ["Ach", ",", "es", "will", "der", "Frei\u00b7heit", "Ro\u00b7se", "uns", "im", "Gar\u00b7ten", "nicht", "ge\u00b7deihn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "ART", "NN", "NE", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ohne Rose doch kannst nimmer, Erzkolo\u00df, sein Bild du sein!", "tokens": ["Oh\u00b7ne", "Ro\u00b7se", "doch", "kannst", "nim\u00b7mer", ",", "Erz\u00b7ko\u00b7lo\u00df", ",", "sein", "Bild", "du", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VMFIN", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "PPER", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.