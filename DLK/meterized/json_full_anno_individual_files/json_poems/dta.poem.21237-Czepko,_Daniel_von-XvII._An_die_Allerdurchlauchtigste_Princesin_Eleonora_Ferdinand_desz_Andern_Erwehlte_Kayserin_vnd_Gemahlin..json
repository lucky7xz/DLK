{"dta.poem.21237": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XvII.  \n  An die Allerdurchlauchtigste Princesin  \n Eleonora/  \n Ferdinand desz Andern  \n Erwehlte Kayserin vnd Gemahlin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "O Glantz au\u00df Mantua/ was flehn Sie in der Nacht/", "tokens": ["O", "Glantz", "au\u00df", "Man\u00b7tua", "/", "was", "flehn", "Sie", "in", "der", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NE", "$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "In die die Thr\u00e4nen ", "tokens": ["In", "die", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sie dencken an den Tag/ da Sie der Herr wird zihren/", "tokens": ["Sie", "den\u00b7cken", "an", "den", "Tag", "/", "da", "Sie", "der", "Herr", "wird", "zih\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Vns ", "tokens": ["Der", "Vns"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Ja der Verlust ist gro\u00df; Doch ", "tokens": ["Ja", "der", "Ver\u00b7lust", "ist", "gro\u00df", ";", "Doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["