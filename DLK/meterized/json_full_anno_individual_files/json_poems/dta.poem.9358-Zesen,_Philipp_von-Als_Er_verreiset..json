{"dta.poem.9358": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Als Er verreiset.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr B\u00fccher meine lust und freude/", "tokens": ["Ihr", "B\u00fc\u00b7cher", "mei\u00b7ne", "lust", "und", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "du leichte feder du/", "tokens": ["du", "leich\u00b7te", "fe\u00b7der", "du", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die ich zu meinen Liedern schneide/", "tokens": ["die", "ich", "zu", "mei\u00b7nen", "Lie\u00b7dern", "schnei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "h\u00f6rt meinen seuftzen zu.", "tokens": ["h\u00f6rt", "mei\u00b7nen", "seuft\u00b7zen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich soll Euch nun verla\u00dfen/", "tokens": ["Ich", "soll", "Euch", "nun", "ver\u00b7la\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wie fang ichs doch nur an/", "tokens": ["wie", "fang", "ichs", "doch", "nur", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "soll zihen meine stra\u00dfen/", "tokens": ["soll", "zi\u00b7hen", "mei\u00b7ne", "stra\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ein\u2019 ungeb\u00e4hnte bahn?", "tokens": ["ein'", "un\u00b7ge\u00b7b\u00e4hn\u00b7te", "bahn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Weg wird mir zu lange/", "tokens": ["Der", "Weg", "wird", "mir", "zu", "lan\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das scheiden ist zu schwer/", "tokens": ["das", "schei\u00b7den", "ist", "zu", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist mir angst und bange", "tokens": ["Es", "ist", "mir", "angst", "und", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und bin bek\u00fcmmert sehr.", "tokens": ["und", "bin", "be\u00b7k\u00fcm\u00b7mert", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer will die zeit vertreiben/", "tokens": ["Wer", "will", "die", "zeit", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die allzulange zeit/", "tokens": ["die", "all\u00b7zu\u00b7lan\u00b7ge", "zeit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wann ich nicht mehr kann schreiben", "tokens": ["wann", "ich", "nicht", "mehr", "kann", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "von meiner Adelheit.", "tokens": ["von", "mei\u00b7ner", "A\u00b7del\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wann ich nicht mehr kann lesen/", "tokens": ["Wann", "ich", "nicht", "mehr", "kann", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "den Edlen Opitz da/", "tokens": ["den", "Ed\u00b7len", "O\u00b7pitz", "da", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wie jhm sey lieb gewesen", "tokens": ["wie", "jhm", "sey", "lieb", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "ADJD", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die braune Flavia.", "tokens": ["die", "brau\u00b7ne", "Fla\u00b7via", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wann Flaccus sch\u00f6ne Lieder/", "tokens": ["Wann", "Flac\u00b7cus", "sch\u00f6\u00b7ne", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wann Maro nicht bey mier/", "tokens": ["wann", "Ma\u00b7ro", "nicht", "bey", "mier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PTKNEG", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "so ist mier nur zuwieder", "tokens": ["so", "ist", "mier", "nur", "zu\u00b7wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die sch\u00f6nste lust und zier.", "tokens": ["die", "sch\u00f6ns\u00b7te", "lust", "und", "zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die zeit wil nicht verflie\u00dfen/", "tokens": ["Die", "zeit", "wil", "nicht", "ver\u00b7flie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wann Sappho schweigen mu\u00df/", "tokens": ["wann", "Sap\u00b7pho", "schwei\u00b7gen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wenn mich nicht kann durchs\u00fc\u00dfen", "tokens": ["wenn", "mich", "nicht", "kann", "durch\u00b7s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der schwere Pindarus.", "tokens": ["der", "schwe\u00b7re", "Pin\u00b7da\u00b7rus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch weil ich ja soll scheiden", "tokens": ["Doch", "weil", "ich", "ja", "soll", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und euch nicht l\u00e4nger sehn/", "tokens": ["und", "euch", "nicht", "l\u00e4n\u00b7ger", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "so hoff ich/ soll mit freuden", "tokens": ["so", "hoff", "ich", "/", "soll", "mit", "freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "mein w\u00fcndschen auch geschehn.", "tokens": ["mein", "w\u00fcnd\u00b7schen", "auch", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In kurtzen komm ich wieder", "tokens": ["In", "kurt\u00b7zen", "komm", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und dessen tr\u00f6st ich mich/", "tokens": ["und", "des\u00b7sen", "tr\u00f6st", "ich", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PRF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da sollen denn die Lieder", "tokens": ["da", "sol\u00b7len", "denn", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "erst recht anheben sich.", "tokens": ["erst", "recht", "an\u00b7he\u00b7ben", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}