{"dta.poem.18449": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Artzt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es r\u00fchmte sich ein artzt/ und macht ein gro\u00df geprahl", "tokens": ["Es", "r\u00fchm\u00b7te", "sich", "ein", "artzt", "/", "und", "macht", "ein", "gro\u00df", "ge\u00b7prahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$(", "KON", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von seiner tiefen kunst/ von seiner cur zumahl/", "tokens": ["Von", "sei\u00b7ner", "tie\u00b7fen", "kunst", "/", "von", "sei\u00b7ner", "cur", "zu\u00b7mahl", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df kein/ kein patient f\u00fchr \u00fcber ihn beschwerde:", "tokens": ["Da\u00df", "kein", "/", "kein", "pa\u00b7ti\u00b7ent", "f\u00fchr", "\u00fc\u00b7ber", "ihn", "be\u00b7schwer\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "$(", "PIAT", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja/ sprach ein schalck/ du stopfst ihm bald das maul mit erde.", "tokens": ["Ja", "/", "sprach", "ein", "schalck", "/", "du", "stopfst", "ihm", "bald", "das", "maul", "mit", "er\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word