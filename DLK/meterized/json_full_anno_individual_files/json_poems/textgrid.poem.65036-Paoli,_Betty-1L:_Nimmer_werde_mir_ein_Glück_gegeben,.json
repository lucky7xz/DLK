{"textgrid.poem.65036": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimmer werde mir ein Gl\u00fcck gegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimmer werde mir ein Gl\u00fcck gegeben,", "tokens": ["Nim\u00b7mer", "wer\u00b7de", "mir", "ein", "Gl\u00fcck", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das nicht Alle, Alle die da leben", "tokens": ["Das", "nicht", "Al\u00b7le", ",", "Al\u00b7le", "die", "da", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "PIS", "$,", "PIS", "ART", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ueberstr\u00f6mt, mit gleichem tiefem Heil!", "tokens": ["Ue\u00b7ber\u00b7str\u00f6mt", ",", "mit", "glei\u00b7chem", "tie\u00b7fem", "Heil", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Tragen will ich, dulden und vermissen", "tokens": ["Tra\u00b7gen", "will", "ich", ",", "dul\u00b7den", "und", "ver\u00b7mis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lieber, als um einen Segen wissen,", "tokens": ["Lie\u00b7ber", ",", "als", "um", "ei\u00b7nen", "Se\u00b7gen", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der nicht aller Kreatur zu Theil!", "tokens": ["Der", "nicht", "al\u00b7ler", "Kre\u00b7a\u00b7tur", "zu", "Theil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Keinen Vorzug will ich vor den Andern,", "tokens": ["Kei\u00b7nen", "Vor\u00b7zug", "will", "ich", "vor", "den", "An\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht auf weichen Blumenpfaden wandern,", "tokens": ["Nicht", "auf", "wei\u00b7chen", "Blu\u00b7men\u00b7pfa\u00b7den", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4hrend ihre Bahn durch W\u00fcsten geht,", "tokens": ["W\u00e4h\u00b7rend", "ih\u00b7re", "Bahn", "durch", "W\u00fcs\u00b7ten", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und nicht treten in die Himmelshalle", "tokens": ["Und", "nicht", "tre\u00b7ten", "in", "die", "Him\u00b7mels\u00b7hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn die helle Pforte nicht f\u00fcr Alle", "tokens": ["Wenn", "die", "hel\u00b7le", "Pfor\u00b7te", "nicht", "f\u00fcr", "Al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"