{"textgrid.poem.56119": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: O ihr Z\u00e4rtlichen, tretet zuweilen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ihr Z\u00e4rtlichen, tretet zuweilen", "tokens": ["O", "ihr", "Z\u00e4rt\u00b7li\u00b7chen", ",", "tre\u00b7tet", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN", "ADV"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "in den Atem, der euch nicht meint,", "tokens": ["in", "den", "A\u00b7tem", ",", "der", "euch", "nicht", "meint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "la\u00dft ihn an eueren Wangen sich teilen,", "tokens": ["la\u00dft", "ihn", "an", "eu\u00b7e\u00b7ren", "Wan\u00b7gen", "sich", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "hinter euch zittert er, wieder vereint.", "tokens": ["hin\u00b7ter", "euch", "zit\u00b7tert", "er", ",", "wie\u00b7der", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "O ihr Seligen, o ihr Heilen,", "tokens": ["O", "ihr", "Se\u00b7li\u00b7gen", ",", "o", "ihr", "Hei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "die ihr der Anfang der Herzen scheint.", "tokens": ["die", "ihr", "der", "An\u00b7fang", "der", "Her\u00b7zen", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bogen der Pfeile und Ziele von Pfeilen,", "tokens": ["Bo\u00b7gen", "der", "Pfei\u00b7le", "und", "Zie\u00b7le", "von", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "ewiger gl\u00e4nzt euer L\u00e4cheln verweint.", "tokens": ["e\u00b7wi\u00b7ger", "gl\u00e4nzt", "eu\u00b7er", "L\u00e4\u00b7cheln", "ver\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcrchtet euch nicht zu leiden, die Schwere,", "tokens": ["F\u00fcrch\u00b7tet", "euch", "nicht", "zu", "lei\u00b7den", ",", "die", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "gebt sie zur\u00fcck an der Erde Gewicht;", "tokens": ["gebt", "sie", "zu\u00b7r\u00fcck", "an", "der", "Er\u00b7de", "Ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "