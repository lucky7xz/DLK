{"textgrid.poem.32892": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr G\u00f6tter! h\u00f6rt, was ietzt geschehen!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr G\u00f6tter! h\u00f6rt, was ietzt geschehen!", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "h\u00f6rt", ",", "was", "ietzt", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00f6nnt ihr ihn unbestrafet sehen?", "tokens": ["K\u00f6nnt", "ihr", "ihn", "un\u00b7be\u00b7stra\u00b7fet", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn, der die unerh\u00f6rte That", "tokens": ["Ihn", ",", "der", "die", "un\u00b7er\u00b7h\u00f6r\u00b7te", "That"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit frecher Faust begangen hat!", "tokens": ["Mit", "fre\u00b7cher", "Faust", "be\u00b7gan\u00b7gen", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es wird die Nachwelt zwar emp\u00f6ren;", "tokens": ["Es", "wird", "die", "Nach\u00b7welt", "zwar", "em\u00b7p\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measur