{"textgrid.poem.32653": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich, o bl\u00fchende Natur!", "tokens": ["K\u00f6nnt'", "ich", ",", "o", "bl\u00fc\u00b7hen\u00b7de", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All deinen Reiz besingen,", "tokens": ["All", "dei\u00b7nen", "Reiz", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jedem Hain und jeder Flur", "tokens": ["Und", "je\u00b7dem", "Hain", "und", "je\u00b7der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein dankbar Liedchen bringen!", "tokens": ["Ein", "dank\u00b7bar", "Lied\u00b7chen", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "S\u00e4ng' ich die Morgensonne, die", "tokens": ["S\u00e4ng'", "ich", "die", "Mor\u00b7gen\u00b7son\u00b7ne", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Auge sich verstecket,", "tokens": ["Dem", "Au\u00b7ge", "sich", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn schon der junge Sch\u00e4fer sie", "tokens": ["Wenn", "schon", "der", "jun\u00b7ge", "Sch\u00e4\u00b7fer", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seiner Fl\u00f6te wecket.", "tokens": ["Mit", "sei\u00b7ner", "Fl\u00f6\u00b7te", "we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Freude, die mit einemmal", "tokens": ["Die", "Freu\u00b7de", ",", "die", "mit", "ei\u00b7nem\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus seinen Augen schimmert,", "tokens": ["Aus", "sei\u00b7nen", "Au\u00b7gen", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sobald der erste Sonnenstrahl", "tokens": ["So\u00b7bald", "der", "ers\u00b7te", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Eichenwipfel flimmert.", "tokens": ["Am", "Ei\u00b7chen\u00b7wip\u00b7fel", "flim\u00b7mert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Blumen, die mit Gelb und Blau", "tokens": ["Die", "Blu\u00b7men", ",", "die", "mit", "Gelb", "und", "Blau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Flur umher bemalen,", "tokens": ["Die", "Flur", "um\u00b7her", "be\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch den jungen Morgentau", "tokens": ["Und", "durch", "den", "jun\u00b7gen", "Mor\u00b7gen\u00b7tau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In h\u00f6hern Farben strahlen.", "tokens": ["In", "h\u00f6\u00b7hern", "Far\u00b7ben", "strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das W\u00e4ldchen, das, der Unschuld gleich,", "tokens": ["Das", "W\u00e4ld\u00b7chen", ",", "das", ",", "der", "Un\u00b7schuld", "gleich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In wei\u00dfen Flor sich h\u00fcllet,", "tokens": ["In", "wei\u00b7\u00dfen", "Flor", "sich", "h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und den beschilften Silberteich", "tokens": ["Und", "den", "be\u00b7schilf\u00b7ten", "Sil\u00b7ber\u00b7teich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Bl\u00fcten \u00fcberf\u00fcllet.", "tokens": ["Mit", "Bl\u00fc\u00b7ten", "\u00fc\u00b7berf\u00b7\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Pappel gr\u00fcne Nacht, aus der", "tokens": ["Der", "Pap\u00b7pel", "gr\u00fc\u00b7ne", "Nacht", ",", "aus", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Turtelt\u00e4ubchen girret.", "tokens": ["Ein", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", "gir\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Apfelbaum, um den ein Heer", "tokens": ["Den", "Ap\u00b7fel\u00b7baum", ",", "um", "den", "ein", "Heer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Maienk\u00e4fern schwirret.", "tokens": ["Von", "Mai\u00b7en\u00b7k\u00e4\u00b7fern", "schwir\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Den Abend, der mit einemmal", "tokens": ["Den", "A\u00b7bend", ",", "der", "mit", "ei\u00b7nem\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den halben Himmel r\u00f6tet.", "tokens": ["Den", "hal\u00b7ben", "Him\u00b7mel", "r\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Hirten, der im letzten Strahl", "tokens": ["Den", "Hir\u00b7ten", ",", "der", "im", "letz\u00b7ten", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Tag zu Grabe fl\u00f6tet.", "tokens": ["Dem", "Tag", "zu", "Gra\u00b7be", "fl\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Den Stern der Liebesk\u00f6nigin,", "tokens": ["Den", "Stern", "der", "Lie\u00b7bes\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der aus dem Westen blinket", "tokens": ["Der", "aus", "dem", "Wes\u00b7ten", "blin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ins Geb\u00fcsch die Sch\u00e4ferin", "tokens": ["Und", "ins", "Ge\u00b7b\u00fcsch", "die", "Sch\u00e4\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum trauten Sch\u00e4fer winket.", "tokens": ["Zum", "trau\u00b7ten", "Sch\u00e4\u00b7fer", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dianen, die das s\u00fc\u00dfe Gl\u00fcck", "tokens": ["Di\u00b7a\u00b7nen", ",", "die", "das", "s\u00fc\u00b7\u00dfe", "Gl\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebenden betrachtet", "tokens": ["Der", "Lie\u00b7ben\u00b7den", "be\u00b7trach\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit hinweggewandtem Blick", "tokens": ["Und", "mit", "hin\u00b7weg\u00b7ge\u00b7wand\u00b7tem", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach gleichen Freuden schmachtet.", "tokens": ["Nach", "glei\u00b7chen", "Freu\u00b7den", "schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und tausend Scenen \u2013 k\u00f6nnt' ich sie,", "tokens": ["Und", "tau\u00b7send", "Sce\u00b7nen", "\u2013", "k\u00f6nnt'", "ich", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$(", "VMFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich sie f\u00fchle, singen,", "tokens": ["Wie", "ich", "sie", "f\u00fch\u00b7le", ",", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und allen Reiz der Harmonie", "tokens": ["Und", "al\u00b7len", "Reiz", "der", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins leichte Liedchen bringen:", "tokens": ["Ins", "leich\u00b7te", "Lied\u00b7chen", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dann w\u00fcrde doch ein J\u00fcngling mich", "tokens": ["Dann", "w\u00fcr\u00b7de", "doch", "ein", "J\u00fcng\u00b7ling", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daf\u00fcr an Busen dr\u00fccken,", "tokens": ["Da\u00b7f\u00fcr", "an", "Bu\u00b7sen", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und manches M\u00e4dchen dankbarlich", "tokens": ["Und", "man\u00b7ches", "M\u00e4d\u00b7chen", "dank\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ihren Beifall nicken.", "tokens": ["Mir", "ih\u00b7ren", "Bei\u00b7fall", "ni\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Doch nicht die bl\u00fchende Natur,", "tokens": ["Doch", "nicht", "die", "bl\u00fc\u00b7hen\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit allem Reiz umgeben;", "tokens": ["Mit", "al\u00b7lem", "Reiz", "um\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach G\u00f6tter! Daphnen kann ich nur", "tokens": ["Ach", "G\u00f6t\u00b7ter", "!", "Daph\u00b7nen", "kann", "ich", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihren Reiz erheben.", "tokens": ["Und", "ih\u00b7ren", "Reiz", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sie aber l\u00e4chelt h\u00f6hnisch, flieht,", "tokens": ["Sie", "a\u00b7ber", "l\u00e4\u00b7chelt", "h\u00f6h\u00b7nisch", ",", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und will der Liebe Lehren,", "tokens": ["Und", "will", "der", "Lie\u00b7be", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und will das minnigliche Lied", "tokens": ["Und", "will", "das", "min\u00b7nig\u00b7li\u00b7che", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihrem Reiz nicht h\u00f6ren.", "tokens": ["Von", "ih\u00b7rem", "Reiz", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "O G\u00f6tter! lehrt sie doch, wie ich,", "tokens": ["O", "G\u00f6t\u00b7ter", "!", "lehrt", "sie", "doch", ",", "wie", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lieb' und Sehnsucht schmachten;", "tokens": ["Von", "Lieb'", "und", "Sehn\u00b7sucht", "schmach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo nicht, so lehrt die Spr\u00f6de mich,", "tokens": ["Wo", "nicht", ",", "so", "lehrt", "die", "Spr\u00f6\u00b7de", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihren Stolz verachten!", "tokens": ["Und", "ih\u00b7ren", "Stolz", "ver\u00b7ach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "K\u00f6nnt' ich, o bl\u00fchende Natur!", "tokens": ["K\u00f6nnt'", "ich", ",", "o", "bl\u00fc\u00b7hen\u00b7de", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All deinen Reiz besingen,", "tokens": ["All", "dei\u00b7nen", "Reiz", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jedem Hain und jeder Flur", "tokens": ["Und", "je\u00b7dem", "Hain", "und", "je\u00b7der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein dankbar Liedchen bringen!", "tokens": ["Ein", "dank\u00b7bar", "Lied\u00b7chen", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word"