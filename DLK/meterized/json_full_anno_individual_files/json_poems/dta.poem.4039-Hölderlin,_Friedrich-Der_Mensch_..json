{"dta.poem.4039": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mensch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum spro\u00dften aus den Wassern, o Erde, dir", "tokens": ["Kaum", "spro\u00df\u00b7ten", "aus", "den", "Was\u00b7sern", ",", "o", "Er\u00b7de", ",", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "FM", "NN", "$,", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der alten Berge Gipfel; und dufteten,", "tokens": ["Der", "al\u00b7ten", "Ber\u00b7ge", "Gip\u00b7fel", ";", "und", "duf\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Voll junger W\u00e4lder, durch die Mailuft,", "tokens": ["Voll", "jun\u00b7ger", "W\u00e4l\u00b7der", ",", "durch", "die", "Mai\u00b7luft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber den Ocean hin, lustathmend,", "tokens": ["Ue\u00b7ber", "den", "O\u00b7cean", "hin", ",", "lus\u00b7tath\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Die ersten gr\u00fcnen Inseln; und freudig sah", "tokens": ["Die", "ers\u00b7ten", "gr\u00fc\u00b7nen", "In\u00b7seln", ";", "und", "freu\u00b7dig", "sah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Sonnengottes Auge die Erstlinge,", "tokens": ["Des", "Son\u00b7nen\u00b7got\u00b7tes", "Au\u00b7ge", "die", "Erst\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die B\u00e4um und Blumen, seiner Jugend", "tokens": ["Die", "B\u00e4um", "und", "Blu\u00b7men", ",", "sei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4chelnde Kinder, aus Dir geboren.", "tokens": ["L\u00e4\u00b7cheln\u00b7de", "Kin\u00b7der", ",", "aus", "Dir", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Da auf den Inseln sch\u00f6nsten,", "tokens": ["Da", "auf", "den", "In\u00b7seln", "sch\u00f6ns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.3": {"text": "Lag unter Trauben einst, nach lauer", "tokens": ["Lag", "un\u00b7ter", "Trau\u00b7ben", "einst", ",", "nach", "lau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nacht, in der d\u00e4mmernden Morgenstunde,", "tokens": ["Nacht", ",", "in", "der", "d\u00e4m\u00b7mern\u00b7den", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Geboren dir, o Erde, dein sch\u00f6nstes Kind;", "tokens": ["Ge\u00b7bo\u00b7ren", "dir", ",", "o", "Er\u00b7de", ",", "dein", "sch\u00f6ns\u00b7tes", "Kind", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und auf zum Vater Helios sieht bekannt", "tokens": ["Und", "auf", "zum", "Va\u00b7ter", "He\u00b7li\u00b7os", "sieht", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "APPRART", "NN", "NE", "VVFIN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Knab' und weiht und w\u00e4hlt, die s\u00fcssen", "tokens": ["Der", "Knab'", "und", "weiht", "und", "w\u00e4hlt", ",", "die", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beere versuchend, die heil'ge Rebe", "tokens": ["Bee\u00b7re", "ver\u00b7su\u00b7chend", ",", "die", "heil'\u00b7ge", "Re\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Zur Amme sich. Und bald ist er gro\u00df; ihn scheun", "tokens": ["Zur", "Am\u00b7me", "sich", ".", "Und", "bald", "ist", "er", "gro\u00df", ";", "ihn", "scheun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PRF", "$.", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVINF"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Thiere, denn ein Anderer ist, wie sie,", "tokens": ["Die", "Thie\u00b7re", ",", "denn", "ein", "An\u00b7de\u00b7rer", "ist", ",", "wie", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Mensch; nicht dir und nicht dem Vater", "tokens": ["Der", "Mensch", ";", "nicht", "dir", "und", "nicht", "dem", "Va\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PTKNEG", "PPER", "KON", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleicht er . . . . . . . . . . . . . . . . . . .", "tokens": ["Gleicht", "er", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Ach! darum treibt ihn, Erde! vom Herzen dir", "tokens": ["Ach", "!", "da\u00b7rum", "treibt", "ihn", ",", "Er\u00b7de", "!", "vom", "Her\u00b7zen", "dir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PAV", "VVFIN", "PPER", "$,", "NN", "$.", "APPRART", "NN", "PPER"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Uebermuth, und deine Geschenke sind", "tokens": ["Sein", "Ue\u00b7ber\u00b7muth", ",", "und", "dei\u00b7ne", "Ge\u00b7schen\u00b7ke", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umsonst, die z\u00e4rtlichen, zu hoch schl\u00e4gt", "tokens": ["Um\u00b7sonst", ",", "die", "z\u00e4rt\u00b7li\u00b7chen", ",", "zu", "hoch", "schl\u00e4gt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "$,", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Immer und immer der stolze Busen.", "tokens": ["Im\u00b7mer", "und", "im\u00b7mer", "der", "stol\u00b7ze", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Von seines Ufers duftender Wiese mu\u00df", "tokens": ["Von", "sei\u00b7nes", "U\u00b7fers", "duf\u00b7ten\u00b7der", "Wie\u00b7se", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ins bl\u00fcthenlose Wasser hinaus der Mensch,", "tokens": ["Ins", "bl\u00fc\u00b7then\u00b7lo\u00b7se", "Was\u00b7ser", "hin\u00b7aus", "der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gl\u00e4nzte auch, wie die Sternennacht, von", "tokens": ["Und", "gl\u00e4nz\u00b7te", "auch", ",", "wie", "die", "Ster\u00b7nen\u00b7nacht", ",", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "$,", "APPR"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Goldenen Fr\u00fcchten sein Hain, doch gr\u00e4bt er", "tokens": ["Gol\u00b7de\u00b7nen", "Fr\u00fcch\u00b7ten", "sein", "Hain", ",", "doch", "gr\u00e4bt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Sich H\u00f6hlen in den Bergen und sp\u00e4ht im Schacht,", "tokens": ["Sich", "H\u00f6h\u00b7len", "in", "den", "Ber\u00b7gen", "und", "sp\u00e4ht", "im", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von seines Vaters heiligem Strale fern,", "tokens": ["Von", "sei\u00b7nes", "Va\u00b7ters", "hei\u00b7li\u00b7gem", "Stra\u00b7le", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Sonnengott auch ungetreu, der", "tokens": ["Dem", "Son\u00b7nen\u00b7gott", "auch", "un\u00b7ge\u00b7treu", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Knechte nicht liebt und der Sorgen spottet.", "tokens": ["Knech\u00b7te", "nicht", "liebt", "und", "der", "Sor\u00b7gen", "spot\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Ach! freier athmen V\u00f6gel des Walds, wenn schon", "tokens": ["Ach", "!", "frei\u00b7er", "ath\u00b7men", "V\u00f6\u00b7gel", "des", "Walds", ",", "wenn", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ADJD", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Menschen Brust sich wilder und stolzer hebt,", "tokens": ["Des", "Men\u00b7schen", "Brust", "sich", "wil\u00b7der", "und", "stol\u00b7zer", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "ADJA", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Trotz wird Angst, und seines Friedens", "tokens": ["Sein", "Trotz", "wird", "Angst", ",", "und", "sei\u00b7nes", "Frie\u00b7dens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blume, die z\u00e4rtliche, bl\u00fcht nicht lange.", "tokens": ["Blu\u00b7me", ",", "die", "z\u00e4rt\u00b7li\u00b7che", ",", "bl\u00fcht", "nicht", "lan\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}