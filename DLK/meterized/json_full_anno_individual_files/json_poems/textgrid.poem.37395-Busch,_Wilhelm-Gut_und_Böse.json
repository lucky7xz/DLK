{"textgrid.poem.37395": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Gut und B\u00f6se", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tugend will, man soll sie holen,", "tokens": ["Tu\u00b7gend", "will", ",", "man", "soll", "sie", "ho\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungern ist sie gegenw\u00e4rtig;", "tokens": ["Un\u00b7gern", "ist", "sie", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Laster ist auch unbefohlen", "tokens": ["Las\u00b7ter", "ist", "auch", "un\u00b7be\u00b7foh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dienstbereit und fix und fertig.", "tokens": ["Dienst\u00b7be\u00b7reit", "und", "fix", "und", "fer\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gute Tiere, spricht der Weise,", "tokens": ["Gu\u00b7te", "Tie\u00b7re", ",", "spricht", "der", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft du z\u00fcchten, mu\u00dft du kaufen;", "tokens": ["Mu\u00dft", "du", "z\u00fcch\u00b7ten", ",", "mu\u00dft", "du", "kau\u00b7fen", ";"