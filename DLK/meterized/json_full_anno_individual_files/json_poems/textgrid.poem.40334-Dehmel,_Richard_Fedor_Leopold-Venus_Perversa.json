{"textgrid.poem.40334": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Perversa", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort sitz nieder! sieben Kreuze", "tokens": ["Dort", "sitz", "nie\u00b7der", "!", "sie\u00b7ben", "Kreu\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$.", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zwischen uns! und g\u00f6nn mir's: sei nicht Tier!", "tokens": ["zwi\u00b7schen", "uns", "!", "und", "g\u00f6nn", "mir's", ":", "sei", "nicht", "Tier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "KON", "NE", "NE", "$.", "VAFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sondern ich suche andere Reize:", "tokens": ["Son\u00b7dern", "ich", "su\u00b7che", "an\u00b7de\u00b7re", "Rei\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dich: komm, liebe dich vor mir!", "tokens": ["Dich", ":", "komm", ",", "lie\u00b7be", "dich", "vor", "mir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich nur, Dich! nur deine verschmachtenden Blicke", "tokens": ["Dich", "nur", ",", "Dich", "!", "nur", "dei\u00b7ne", "ver\u00b7schmach\u00b7ten\u00b7den", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPER", "$.", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und deine zuckende Scham und deine scheuen", "tokens": ["und", "dei\u00b7ne", "zu\u00b7cken\u00b7de", "Scham", "und", "dei\u00b7ne", "scheu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Seufzer g\u00f6nn mir \u2013 ja, entz\u00fccke", "tokens": ["Seuf\u00b7zer", "g\u00f6nn", "mir", "\u2013", "ja", ",", "ent\u00b7z\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "$(", "PTKANT", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mich mit Deinen Rasereien!", "tokens": ["mich", "mit", "Dei\u00b7nen", "Ra\u00b7se\u00b7rei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oh du, wenn die Knospen deiner welken", "tokens": ["Oh", "du", ",", "wenn", "die", "Knos\u00b7pen", "dei\u00b7ner", "wel\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Br\u00fcste unter deinen tastenden Fingern", "tokens": ["Br\u00fcs\u00b7te", "un\u00b7ter", "dei\u00b7nen", "tas\u00b7ten\u00b7den", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "wieder schwellen wie in j\u00fcngern", "tokens": ["wie\u00b7der", "schwel\u00b7len", "wie", "in", "j\u00fcn\u00b7gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KOKOM", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4chten \u2013 oh, dies Schwelgen \u2013", "tokens": ["N\u00e4ch\u00b7ten", "\u2013", "oh", ",", "dies", "Schwel\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "$,", "PDS", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "g\u00f6nn mir's, g\u00f6nn mir's! Meine eigenen Freuden", "tokens": ["g\u00f6nn", "mir's", ",", "g\u00f6nn", "mir's", "!", "Mei\u00b7ne", "ei\u00b7ge\u00b7nen", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "NE", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "sind mir Schaum, der bitter ist \u2013", "tokens": ["sind", "mir", "Schaum", ",", "der", "bit\u00b7ter", "ist", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aber Du, wenn Du so st\u00f6hnst und gl\u00fchst,", "tokens": ["a\u00b7ber", "Du", ",", "wenn", "Du", "so", "st\u00f6hnst", "und", "gl\u00fchst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "will ich mich an Deiner Wildheit weiden:", "tokens": ["will", "ich", "mich", "an", "Dei\u00b7ner", "Wild\u00b7heit", "wei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "wie du gleich entt\u00e4uschten Br\u00e4uten", "tokens": ["wie", "du", "gleich", "ent\u00b7t\u00e4uschten", "Br\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "deine einsame Sehnsucht stilltest,", "tokens": ["dei\u00b7ne", "ein\u00b7sa\u00b7me", "Sehn\u00b7sucht", "still\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "deine heimlichen Seligkeiten", "tokens": ["dei\u00b7ne", "heim\u00b7li\u00b7chen", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "mit berauschten H\u00e4nden f\u00fchltest \u2013", "tokens": ["mit", "be\u00b7rauschten", "H\u00e4n\u00b7den", "f\u00fchl\u00b7test", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "f\u00fchlst \u2013 stillst \u2013 \u2013 Seele, bricht dein Blick?", "tokens": ["f\u00fchlst", "\u2013", "stillst", "\u2013", "\u2013", "See\u00b7le", ",", "bricht", "dein", "Blick", "?"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "$(", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oh du, la\u00df mich diesen Blick genie\u00dfen!", "tokens": ["Oh", "du", ",", "la\u00df", "mich", "die\u00b7sen", "Blick", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "$,", "VVIMP", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "dies Verr\u00f6cheln von Lippen bis zu F\u00fc\u00dfen!", "tokens": ["dies", "Ver\u00b7r\u00f6\u00b7cheln", "von", "Lip\u00b7pen", "bis", "zu", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "recke dich nicht so starr zur\u00fcck \u2013", "tokens": ["re\u00b7cke", "dich", "nicht", "so", "starr", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und wir schaudern: wir begreifen", "tokens": ["Und", "wir", "schau\u00b7dern", ":", "wir", "be\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$.", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "den Triumph der Unnatur.", "tokens": ["den", "Tri\u00b7umph", "der", "Un\u00b7na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.9": {"line.1": {"text": "Wohin fliehn nach solchen Wonnen?", "tokens": ["Wo\u00b7hin", "fliehn", "nach", "sol\u00b7chen", "Won\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Damals lernt'ich die Ekstasen", "tokens": ["Da\u00b7mals", "lernt'\u00b7ich", "die", "Eks\u00b7ta\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der entbehrungss\u00fcchtigen Nonnen", "tokens": ["der", "ent\u00b7beh\u00b7rungs\u00b7s\u00fcch\u00b7ti\u00b7gen", "Non\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "w\u00fcrdigen, und das geistige Rasen", "tokens": ["w\u00fcr\u00b7di\u00b7gen", ",", "und", "das", "geis\u00b7ti\u00b7ge", "Ra\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "derer, die vor lauter Br\u00fcnsten", "tokens": ["de\u00b7rer", ",", "die", "vor", "lau\u00b7ter", "Br\u00fcns\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "nach der reinen Inbrunst schreien,", "tokens": ["nach", "der", "rei\u00b7nen", "In\u00b7brunst", "schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "w\u00e4hrend sie mit Marterk\u00fcnsten", "tokens": ["w\u00e4h\u00b7rend", "sie", "mit", "Mar\u00b7ter\u00b7k\u00fcns\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bis zum Rausch ihr Fleisch kasteien.", "tokens": ["bis", "zum", "Rausch", "ihr", "Fleisch", "kas\u00b7tei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Warlich, wenn der Heiligen Einer", "tokens": ["War\u00b7lich", ",", "wenn", "der", "Hei\u00b7li\u00b7gen", "Ei\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "jetzt vor meinem Bett erschiene,", "tokens": ["jetzt", "vor", "mei\u00b7nem", "Bett", "er\u00b7schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "br\u00fcnstiger als ich rang keiner!", "tokens": ["br\u00fcns\u00b7ti\u00b7ger", "als", "ich", "rang", "kei\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit eingeweihter Miene", "tokens": ["Und", "mit", "ein\u00b7ge\u00b7weih\u00b7ter", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "d\u00fcrft ich ihm die H\u00e4nde reichen:", "tokens