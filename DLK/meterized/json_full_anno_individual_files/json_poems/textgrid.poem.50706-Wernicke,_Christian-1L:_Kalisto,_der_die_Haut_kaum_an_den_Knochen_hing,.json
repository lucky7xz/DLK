ord", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So fand er nichts, als \u2013 d\u00fcrre Knochen.\u00ab", "tokens": ["So", "fand", "er", "nichts", ",", "als", "\u2013", "d\u00fcr\u00b7re", "Kno\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "KOUS", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ich schwieg; sie aber sprach: Freund, ich versteh' Euch nicht. \u2013", "tokens": ["Ich", "schwieg", ";", "sie", "a\u00b7ber", "sprach", ":", "Freund", ",", "ich", "ver\u00b7steh'", "Euch", "nicht", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "ADV", "VVFIN", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u00bbnein?\u00ab sagt' ich; \u00bb", "tokens": ["\u00bb", "nein", "?", "\u00ab", "sagt'", "ich", ";", "\u00bb"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$.", "$(", "VVFIN", "PPER", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}