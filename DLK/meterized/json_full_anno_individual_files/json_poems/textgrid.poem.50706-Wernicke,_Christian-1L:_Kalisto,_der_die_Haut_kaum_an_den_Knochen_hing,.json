{"textgrid.poem.50706": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kalisto, der die Haut kaum an den Knochen hing,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kalisto, der die Haut kaum an den Knochen hing,", "tokens": ["Ka\u00b7lis\u00b7to", ",", "der", "die", "Haut", "kaum", "an", "den", "Kno\u00b7chen", "hing", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die traf ich an, als sie vermummt im Gr\u00fcnen ging.", "tokens": ["Die", "traf", "ich", "an", ",", "als", "sie", "ver\u00b7mummt", "im", "Gr\u00fc\u00b7nen", "ging", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich naht' herzu und, gleich als ob ich sie nicht kennte", "tokens": ["Ich", "naht'", "her\u00b7zu", "und", ",", "gleich", "als", "ob", "ich", "sie", "nicht", "kenn\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "$,", "ADV", "KOKOM", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihr Gesicht zu sehen brennte:", "tokens": ["Und", "ihr", "Ge\u00b7sicht", "zu", "se\u00b7hen", "brenn\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbdie Sonne m\u00f6cht' ich sehn, die eine Wolke deckt,\u00ab", "tokens": ["\u00bb", "die", "Son\u00b7ne", "m\u00f6cht'", "ich", "sehn", ",", "die", "ei\u00b7ne", "Wol\u00b7ke", "deckt", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sagt' ich, als ich die Hand nach ihrer Maske streckt'. \u2013", "tokens": ["Sagt'", "ich", ",", "als", "ich", "die", "Hand", "nach", "ih\u00b7rer", "Mas\u00b7ke", "streckt'", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u00bbglaubt Ihr\u00ab, versetzte sie, \u00bbda\u00df insgemein auch meine", "tokens": ["\u00bb", "glaubt", "Ihr", "\u00ab", ",", "ver\u00b7setz\u00b7te", "sie", ",", "\u00bb", "da\u00df", "ins\u00b7ge\u00b7mein", "auch", "mei\u00b7ne"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "KOUS", "ADV", "ADV", "PPOSAT"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Den Thoren wie den Klugen scheine?\u00ab \u2013", "tokens": ["Den", "Tho\u00b7ren", "wie", "den", "Klu\u00b7gen", "schei\u00b7ne", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Erz\u00fcrnt durch dieses Wort \u00bbEi, ist Euch nicht bekannt,\u00ab", "tokens": ["Er\u00b7z\u00fcrnt", "durch", "die\u00b7ses", "Wort", "\u00bb", "Ei", ",", "ist", "Euch", "nicht", "be\u00b7kannt", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPR", "PDAT", "NN", "$(", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sprach ich, \u00bbein schlaues Weib, Semiramis genannt?", "tokens": ["Sprach", "ich", ",", "\u00bb", "ein", "schlau\u00b7es", "Weib", ",", "Se\u00b7mi\u00b7ra\u00b7mis", "ge\u00b7nannt", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "$(", "ART", "ADJA", "NN", "$,", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie lie\u00df, als sie noch lebt', auf ihren Leichstein graben:", "tokens": ["Sie", "lie\u00df", ",", "als", "sie", "noch", "lebt'", ",", "auf", "ih\u00b7ren", "Leichs\u00b7tein", "gra\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer einen Schatz verlangt zu haben,", "tokens": ["Wer", "ei\u00b7nen", "Schatz", "ver\u00b7langt", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der findet ihn gewi\u00df hier unter diesem Stein.", "tokens": ["Der", "fin\u00b7det", "ihn", "ge\u00b7wi\u00df", "hier", "un\u00b7ter", "die\u00b7sem", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Fremdling fand sich drauf nach vielen Jahren ein;", "tokens": ["Ein", "Fremd\u00b7ling", "fand", "sich", "drauf", "nach", "vie\u00b7len", "Jah\u00b7ren", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PAV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er las und dachte: Geld verachten nur die Narren,", "tokens": ["Er", "las", "und", "dach\u00b7te", ":", "Geld", "ver\u00b7ach\u00b7ten", "nur", "die", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und fing die Gruft an aufzuscharren.", "tokens": ["Und", "fing", "die", "Gruft", "an", "auf\u00b7zu\u00b7schar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Es kostet' ihm viel M\u00fch', und oftmals sch\u00f6pft' er Luft,", "tokens": ["Es", "kos\u00b7tet'", "ihm", "viel", "M\u00fch'", ",", "und", "oft\u00b7mals", "sch\u00f6pft'", "er", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.1