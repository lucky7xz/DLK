{"textgrid.poem.26307": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der welkende Kapokbaum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor dem Fensterrahmen, in der Leere des Himmelsraumes,", "tokens": ["Vor", "dem", "Fens\u00b7ter\u00b7rah\u00b7men", ",", "in", "der", "Lee\u00b7re", "des", "Him\u00b7mels\u00b7rau\u00b7mes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Steht drau\u00dfen die d\u00fcnne Krone eines Kapokbaumes.", "tokens": ["Steht", "drau\u00b7\u00dfen", "die", "d\u00fcn\u00b7ne", "Kro\u00b7ne", "ei\u00b7nes", "Ka\u00b7pok\u00b7bau\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Das St\u00e4mmlein h\u00e4lt seine wagrechten Zweige von sich wie Sprossen,", "tokens": ["Das", "St\u00e4mm\u00b7lein", "h\u00e4lt", "sei\u00b7ne", "wag\u00b7rech\u00b7ten", "Zwei\u00b7ge", "von", "sich", "wie", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PRF", "KOKOM", "NN", "$,"], "meter": "-+-+---+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Seine Bl\u00e4tter gilben und winken; sie haben ihr Leben genossen.", "tokens": ["Sei\u00b7ne", "Bl\u00e4t\u00b7ter", "gil\u00b7ben", "und", "win\u00b7ken", ";", "sie", "ha\u00b7ben", "ihr", "Le\u00b7ben", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", 