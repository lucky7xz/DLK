{"textgrid.poem.36609": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Sch\u00f6pfung des Weibes", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Anfang, als die Welt begann,", "tokens": ["Am", "An\u00b7fang", ",", "als", "die", "Welt", "be\u00b7gann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah Jupiter den ersten Mann,", "tokens": ["Sah", "Ju\u00b7pi\u00b7ter", "den", "ers\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wie einsam, wie voll Ernst er sann:", "tokens": ["Wie", "ein\u00b7sam", ",", "wie", "voll", "Ernst", "er", "sann", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von wem doch das, was ist, den Ursprung h\u00e4tte;", "tokens": ["Von", "wem", "doch", "das", ",", "was", "ist", ",", "den", "Ur\u00b7sprung", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "ADV", "PDS", "$,", "PWS", "VAFIN", "$,", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie er, den Grund von jedem Ding", "tokens": ["Wie", "er", ",", "den", "Grund", "von", "je\u00b7dem", "Ding"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ART