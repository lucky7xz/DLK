{"dta.poem.5935": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Anfang stellt man sich ein schwartzes Chaos vor.", "tokens": ["Jm", "An\u00b7fang", "stellt", "man", "sich", "ein", "schwart\u00b7zes", "Chaos", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein wild Gemisch verwirrt die Elementen alle,", "tokens": ["Ein", "wild", "Ge\u00b7misch", "ver\u00b7wirrt", "die", "E\u00b7le\u00b7men\u00b7ten", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So da\u00df der Geist sonst nichts erblickt, als nur,", "tokens": ["So", "da\u00df", "der", "Geist", "sonst", "nichts", "er\u00b7blickt", ",", "als", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "PIS", "VVPP", "$,", "KOUS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie alles durch einander walle.", "tokens": ["Wie", "al\u00b7les", "durch", "ein\u00b7an\u00b7der", "wal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn alles ist verh\u00fcllt in dunckler Wolcken Flohr.", "tokens": ["Denn", "al\u00b7les", "ist", "ver\u00b7h\u00fcllt", "in", "dunck\u00b7ler", "Wol\u00b7cken", "Flohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein, so bald der Elementen Schaaren,", "tokens": ["Al\u00b7lein", ",", "so", "bald", "der", "E\u00b7le\u00b7men\u00b7ten", "Schaa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Vom Chaos abgetrennt, in ihrer Ordnung waren;", "tokens": ["Vom", "Chaos", "ab\u00b7ge\u00b7trennt", ",", "in", "ih\u00b7rer", "Ord\u00b7nung", "wa\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "So bald als das Gestirn in seinem Lichte stund;", "tokens": ["So", "bald", "als", "das", "Ge\u00b7stirn", "in", "sei\u00b7nem", "Lich\u00b7te", "stund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bekam dadurch die Welt den festen Grund;", "tokens": ["Be\u00b7kam", "da\u00b7durch", "die", "Welt", "den", "fes\u00b7ten", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es zeigte sich die Sch\u00f6nheit, Ordnung, Pracht,", "tokens": ["Es", "zeig\u00b7te", "sich", "die", "Sch\u00f6n\u00b7heit", ",", "Ord\u00b7nung", ",", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Theile waren all\u2019 fest in einander bracht.", "tokens": ["Die", "Thei\u00b7le", "wa\u00b7ren", "all'", "fest", "in", "ein\u00b7an\u00b7der", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es war nur in den kleinen St\u00fccken,", "tokens": ["Es", "war", "nur", "in", "den", "klei\u00b7nen", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Worinn man Aendrung konnt\u2019 erblicken.", "tokens": ["Wo\u00b7rinn", "man", "A\u00b7en\u00b7drung", "konnt'", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}