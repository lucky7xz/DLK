{"textgrid.poem.53733": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Figurinen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal war ich schon achtzig Jahr.", "tokens": ["Ein\u00b7mal", "war", "ich", "schon", "acht\u00b7zig", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "CARD", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Einmal, in einem fr\u00fchem Leben,", "tokens": ["Ein\u00b7mal", ",", "in", "ei\u00b7nem", "fr\u00fc\u00b7hem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da hat sich dieses mit mir begeben", "tokens": ["da", "hat", "sich", "die\u00b7ses", "mit", "mir", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "PDAT", "APPR", "PPER", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u2013 und ich hatte ganz wei\u00dfes Haar \u2013:", "tokens": ["\u2013", "und", "ich", "hat\u00b7te", "ganz", "wei\u00b7\u00dfes", "Haar", "\u2013", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$(", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich sa\u00df im Lehnstuhl, nett und beschaulich,", "tokens": ["Ich", "sa\u00df", "im", "Lehn\u00b7stuhl", ",", "nett", "und", "be\u00b7schau\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "so kurz nach Tisch \u2013 mir war so verdaulich.", "tokens": ["so", "kurz", "nach", "Tisch", "\u2013", "mir", "war", "so", "ver\u00b7dau\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Blumen im Fenster. Im K\u00e4fig ein Matz \u2013", "tokens": ["Blu\u00b7men", "im", "Fens\u00b7ter", ".", "Im", "K\u00e4\u00b7fig", "ein", "Matz", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "APPRART", "NN", "ART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "auf dem Tisch eine Tasse mit Untersatz . . .", "tokens": ["auf", "dem", "Tisch", "ei\u00b7ne", "Tas\u00b7se", "mit", "Un\u00b7ter\u00b7satz", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "$.", "$.", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und vor mir hielt ich auf meinen Knien", "tokens": ["Und", "vor", "mir", "hielt", "ich", "auf", "mei\u00b7nen", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "ein Album mit alten Fotografien.", "tokens": ["ein", "Al\u00b7bum", "mit", "al\u00b7ten", "Fo\u00b7to\u00b7gra\u00b7fi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und ich machte ein Nickerchen . . .", "tokens": ["Und", "ich", "mach\u00b7te", "ein", "Ni\u00b7cker\u00b7chen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "Aus ihren Rahmen", "tokens": ["Aus", "ih\u00b7ren", "Rah\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "stiegen alle vergessenen Damen.", "tokens": ["stie\u00b7gen", "al\u00b7le", "ver\u00b7ges\u00b7se\u00b7nen", "Da\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Eine war schlank und klug und bescheiden.", "tokens": ["Ei\u00b7ne", "war", "schlank", "und", "klug", "und", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die mochte ich immer am liebsten leiden.", "tokens": ["Die", "moch\u00b7te", "ich", "im\u00b7mer", "am", "liebs\u00b7ten", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie roch die Menschen. Sie wu\u00dfte immer,", "tokens": ["Sie", "roch", "die", "Men\u00b7schen", ".", "Sie", "wu\u00df\u00b7te", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "betrat sie nur einmal ein fremdes Zimmer:", "tokens": ["be\u00b7trat", "sie", "nur", "ein\u00b7mal", "ein", "frem\u00b7des", "Zim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "die hat mit dem da \u2013 der ist stolz \u2013", "tokens": ["die", "hat", "mit", "dem", "da", "\u2013", "der", "ist", "stolz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "ADV", "$(", "PDS", "VAFIN", "ADJD", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "und die Frau ist falsch wie Galgenholz.", "tokens": ["und", "die", "Frau", "ist", "falsch", "wie", "Gal\u00b7gen\u00b7holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sie erschien, wie im Nebel. Ich streckte die Hand", "tokens": ["Sie", "er\u00b7schien", ",", "wie", "im", "Ne\u00b7bel", ".", "Ich", "streck\u00b7te", "die", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "nach ihr \u2013 sie wich zur\u00fcck und verschwand.", "tokens": ["nach", "ihr", "\u2013", "sie", "wich", "zu\u00b7r\u00fcck", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$(", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und sie sprach, indes ich, wie vertr\u00e4umt,", "tokens": ["Und", "sie", "sprach", ",", "in\u00b7des", "ich", ",", "wie", "ver\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "ein Gl\u00fcck zerschlagen \u2013 ein Leben vers\u00e4umt:", "tokens": ["ein", "Gl\u00fcck", "zer\u00b7schla\u00b7gen", "\u2013", "ein", "Le\u00b7ben", "ver\u00b7s\u00e4umt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "\u00bbich war die Nettste.\u00ab", "tokens": ["\u00bb", "ich", "war", "die", "Netts\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Dann kam eine, ein dickes Paket,", "tokens": ["Dann", "kam", "ei\u00b7ne", ",", "ein", "di\u00b7ckes", "Pa\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wie es gar nicht in eine Corsage geht.", "tokens": ["wie", "es", "gar", "nicht", "in", "ei\u00b7ne", "Cor\u00b7sa\u00b7ge", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wenn sie mir abends entgegenschwoll,", "tokens": ["Wenn", "sie", "mir", "a\u00b7bends", "ent\u00b7ge\u00b7gen\u00b7schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "war das ganze Schlafzimmer voll.", "tokens": ["war", "das", "gan\u00b7ze", "Schlaf\u00b7zim\u00b7mer", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und sie trank zwischendurch \u2013", "tokens": ["Und", "sie", "trank", "zwi\u00b7schen\u00b7durch", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "wie ich das noch seh! \u2013", "tokens": ["wie", "ich", "das", "noch", "seh", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PDS", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "immer Kaffee und Selter und Tee.", "tokens": ["im\u00b7mer", "Kaf\u00b7fee", "und", "Sel\u00b7ter", "und", "Tee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und w\u00e4hrend in weichen Kissen sie kraucht,", "tokens": ["Und", "w\u00e4h\u00b7rend", "in", "wei\u00b7chen", "Kis\u00b7sen", "sie", "kraucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "hat das Schwergewicht mir entgegengehaucht:", "tokens": ["hat", "das", "Schwer\u00b7ge\u00b7wicht", "mir", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7haucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "\u00bbich war die Fettste.\u00ab", "tokens": ["\u00bb", "ich", "war", "die", "Fetts\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Dann h\u00f6rt ich im Halbschlaf einen Chor", "tokens": ["Dann", "h\u00f6rt", "ich", "im", "Halb\u00b7schlaf", "ei\u00b7nen", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "von Stimmen. Und eine tauchte empor,", "tokens": ["von", "Stim\u00b7men", ".", "Und", "ei\u00b7ne", "tauch\u00b7te", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "KON", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "ein ganz junges M\u00e4dchen, wei\u00df, ganz weich \u2013", "tokens": ["ein", "ganz", "jun\u00b7ges", "M\u00e4d\u00b7chen", ",", "wei\u00df", ",", "ganz", "weich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "VVFIN", "$,", "ADV", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "sie z\u00f6gerte, n\u00e4herte sich nicht gleich . . .", "tokens": ["sie", "z\u00f6\u00b7ger\u00b7te", ",", "n\u00e4\u00b7her\u00b7te", "sich", "nicht", "gleich", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PRF", "PTKNEG", "ADV", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich streckte nach ihr die Arme aus.", "tokens": ["Ich", "streck\u00b7te", "nach", "ihr", "die", "Ar\u00b7me", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie stand vor einem Schifferhaus.", "tokens": ["Sie", "stand", "vor", "ei\u00b7nem", "Schif\u00b7fer\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und man h\u00f6rte das Meer . . .", "tokens": ["Und", "man", "h\u00f6r\u00b7te", "das", "Meer", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und sie sprach und sah mich dabei an \u2013", "tokens": ["Und", "sie", "sprach", "und", "sah", "mich", "da\u00b7bei", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PRF", "PAV", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "und da weinte ich alter Mann \u2013:", "tokens": ["und", "da", "wein\u00b7te", "ich", "al\u00b7ter", "Mann", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$(", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "\u00bbich war die Letzte.\u00ab", "tokens": ["\u00bb", "ich", "war", "die", "Letz\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Auf den Tod zu warten, ist so schwer . . .", "tokens": ["Auf", "den", "Tod", "zu", "war\u00b7ten", ",", "ist", "so", "schwer", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "ADJD", "$.", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Aber das ist schon lange her.", "tokens": ["A\u00b7ber", "das", "ist", "schon", "lan\u00b7ge", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Einmal war ich schon achtzig Jahr.", "tokens": ["Ein\u00b7mal", "war", "ich", "schon", "acht\u00b7zig", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "CARD", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Einmal, in einem fr\u00fchem Leben,", "tokens": ["Ein\u00b7mal", ",", "in", "ei\u00b7nem", "fr\u00fc\u00b7hem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da hat sich dieses mit mir begeben", "tokens": ["da", "hat", "sich", "die\u00b7ses", "mit", "mir", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "PDAT", "APPR", "PPER", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u2013 und ich hatte ganz wei\u00dfes Haar \u2013:", "tokens": ["\u2013", "und", "ich", "hat\u00b7te", "ganz", "wei\u00b7\u00dfes", "Haar", "\u2013", ":"], "token_info": ["punct", "word", "word",