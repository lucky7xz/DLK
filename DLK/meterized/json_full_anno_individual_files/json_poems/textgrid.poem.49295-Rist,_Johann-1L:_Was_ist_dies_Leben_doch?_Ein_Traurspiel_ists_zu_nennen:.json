{"textgrid.poem.49295": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist dies Leben doch? Ein Traurspiel ists zu nennen:", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist dies Leben doch? Ein Traurspiel ists zu nennen:", "tokens": ["Was", "ist", "dies", "Le\u00b7ben", "doch", "?", "Ein", "Traur\u00b7spiel", "ists", "zu", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "NN", "ADV", "$.", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist der Anfang gut, und wie wirs w\u00fcnschen k\u00f6nnen,", "tokens": ["Da", "ist", "der", "An\u00b7fang", "gut", ",", "und", "wie", "wirs", "w\u00fcn\u00b7schen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "KON", "PWAV", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Mittel voller Angst, das End ist Herzeleid,", "tokens": ["Das", "Mit\u00b7tel", "vol\u00b7ler", "Angst", ",", "das", "End", "ist", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja, wol der bittre Tod. O kurze Fr\u00f6lichkeit!", "tokens": ["Ja", ",", "wol", "der", "bitt\u00b7re", "Tod", ".", "O", "kur\u00b7ze", "Fr\u00f6\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "ADJA", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die\u00df thut uns Wallenstein in seinem Spiel erweisen:", "tokens": ["Die\u00df", "thut", "uns", "Wal\u00b7len\u00b7stein", "in", "sei\u00b7nem", "Spiel", "er\u00b7wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Kaiser pflag ihn selbst anf\u00e4nglich hoch zu preisen", "tokens": ["Der", "Kai\u00b7ser", "pflag", "ihn", "selbst", "an\u00b7f\u00e4ng\u00b7lich", "hoch", "zu", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als eine Seul des Reichs (so nant' ihn Ferdinand),", "tokens": ["Als", "ei\u00b7ne", "Seul", "des", "Reichs", "(", "so", "nant'", "ihn", "Fer\u00b7di\u00b7nand", ")", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "NE", "$(", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Teutschen Furcht und Zwang, des Kaisers rechter Hand.", "tokens": ["Der", "Teut\u00b7schen", "Furcht", "und", "Zwang", ",", "des", "Kai\u00b7sers", "rech\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bald aber