{"textgrid.poem.59521": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Lied, gesungen zu Edeby am M\u00e4lare im Herbstmond 1807 f\u00fcr den Freiherrn Otto Magnus Munck", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr krausen Grillen weg! Hinweg, ihr d\u00fcstern Sorgen!", "tokens": ["Ihr", "krau\u00b7sen", "Gril\u00b7len", "weg", "!", "Hin\u00b7weg", ",", "ihr", "d\u00fcs\u00b7tern", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$.", "ADV", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tief in das Meer hinab! Heut la\u00dft uns fr\u00f6hlich sein!", "tokens": ["Tief", "in", "das", "Meer", "hin\u00b7ab", "!", "Heut", "la\u00dft", "uns", "fr\u00f6h\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein weises Schicksal deckt den Sterblichen den Morgen \u2013", "tokens": ["Ein", "wei\u00b7ses", "Schick\u00b7sal", "deckt", "den", "Sterb\u00b7li\u00b7chen", "den", "Mor\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf! halt die Stunde fest! Das Heute nur ist dein.", "tokens": ["Auf", "!", "halt", "die", "Stun\u00b7de", "fest", "!", "Das", "Heu\u00b7te", "nur", "ist", "dein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es spielt Verg\u00e4nglichkeit mit H\u00fctten und mit Thronen:", "tokens": ["Es", "spielt", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "mit", "H\u00fct\u00b7ten", "und", "mit", "Thro\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denk' Alexanders Los, der weiland Magnus hie\u00df;", "tokens": ["Denk'", "A\u00b7lex\u00b7an\u00b7ders", "Los", ",", "der", "wei\u00b7land", "Mag\u00b7nus", "hie\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "PRELS", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo ist der Heldenschritt gewaltiger Ottonen?", "tokens": ["Wo", "ist", "der", "Hel\u00b7den\u00b7schritt", "ge\u00b7wal\u00b7ti\u00b7ger", "Ot\u00b7to\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geh, frage ihren Staub, den l\u00e4ngst der Wind zerblies.", "tokens": ["Geh", ",", "fra\u00b7ge", "ih\u00b7ren", "Staub", ",", "den", "l\u00e4ngst", "der", "Wind", "zer\u00b7blies", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei du in Liebe gro\u00df, sei du in Freude Kaiser!", "tokens": ["Sei", "du", "in", "Lie\u00b7be", "gro\u00df", ",", "sei", "du", "in", "Freu\u00b7de", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADJD", "$,", "VAFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bau' in den Herzen dir den ewig sichern Thron.", "tokens": ["Bau'", "in", "den", "Her\u00b7zen", "dir", "den", "e\u00b7wig", "si\u00b7chern", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Dies Heldentum gilt mehr als alle Lorbeerreiser,", "tokens": ["Dies", "Hel\u00b7den\u00b7tum", "gilt", "mehr", "als", "al\u00b7le", "Lor\u00b7beer\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NE", "VVFIN", "PIS", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Trotzt der Verg\u00e4nglichkeit und beut Tyrannen Hohn.", "tokens": ["Trotzt", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "und", "beut", "Ty\u00b7ran\u00b7nen", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Herbei des alten Weins! und lustig angeklungen!", "tokens": ["Her\u00b7bei", "des", "al\u00b7ten", "Weins", "!", "und", "lus\u00b7tig", "an\u00b7ge\u00b7klun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schling Rosen um das Haar, schling Rosen um den Wein!", "tokens": ["Schling", "Ro\u00b7sen", "um", "das", "Haar", ",", "schling", "Ro\u00b7sen", "um", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mann ist auch ein Held und hat die Welt bezwungen,", "tokens": ["Der", "Mann", "ist", "auch", "ein", "Held", "und", "hat", "die", "Welt", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem ernste Weisheit hei\u00dft, mit Freunden froh zu sein.", "tokens": ["Dem", "erns\u00b7te", "Weis\u00b7heit", "hei\u00dft", ",", "mit", "Freun\u00b7den", "froh", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum Liebe lebe hoch, die K\u00f6nigin des Lebens,", "tokens": ["Drum", "Lie\u00b7be", "le\u00b7be", "hoch", ",", "die", "K\u00f6\u00b7ni\u00b7gin", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Freundschaft, die den Ha\u00df an M\u00e4nnerbrust erdr\u00fcckt!", "tokens": ["Und", "Freund\u00b7schaft", ",", "die", "den", "Ha\u00df", "an", "M\u00e4n\u00b7ner\u00b7brust", "er\u00b7dr\u00fcckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer diese zwei gewann, der lebte nicht vergebens,", "tokens": ["Wer", "die\u00b7se", "zwei", "ge\u00b7wann", ",", "der", "leb\u00b7te", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "CARD", "VVFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der hat den h\u00f6chsten Glanz der Erde sich gepfl\u00fcckt.", "tokens": ["Der", "hat", "den", "h\u00f6chs\u00b7ten", "Glanz", "der", "Er\u00b7de", "sich", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Auch dir, o Vaterland, dir Land der tapfern Goten!", "tokens": ["Auch", "dir", ",", "o", "Va\u00b7ter\u00b7land", ",", "dir", "Land", "der", "tap\u00b7fern", "Go\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Klingt, Freunde, dreimal hoch! Das Vaterland! es gilt!", "tokens": ["Klingt", ",", "Freun\u00b7de", ",", "drei\u00b7mal", "hoch", "!", "Das", "Va\u00b7ter\u00b7land", "!", "es", "gilt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "ADJD", "$.", "ART", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Leben steht nur hoch auf Leben edler Toten \u2013", "tokens": ["Das", "Le\u00b7ben", "steht", "nur", "hoch", "auf", "Le\u00b7ben", "ed\u00b7ler", "To\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf V\u00e4tertugend denn die Becher frisch gef\u00fcllt!", "tokens": ["Auf", "V\u00e4\u00b7ter\u00b7tu\u00b7gend", "denn", "die", "Be\u00b7cher", "frisch", "ge\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und unserm K\u00f6nig auch, dem Heldensohn, zu Ehren,", "tokens": ["Und", "un\u00b7serm", "K\u00f6\u00b7nig", "auch", ",", "dem", "Hel\u00b7den\u00b7sohn", ",", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mit dem Schwerte blitzt und mit dem Zepter dr\u00e4ut,", "tokens": ["Der", "mit", "dem", "Schwer\u00b7te", "blitzt", "und", "mit", "dem", "Zep\u00b7ter", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Will ich das vollste Glas im vollen Jubel leeren:", "tokens": ["Will", "ich", "das", "volls\u00b7te", "Glas", "im", "vol\u00b7len", "Ju\u00b7bel", "lee\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Heil sei sein K\u00f6nigswort und Sieg sein M\u00e4nnerstreit!", "tokens": ["Heil", "sei", "sein", "K\u00f6\u00b7nigs\u00b7wort", "und", "Sieg", "sein", "M\u00e4n\u00b7ner\u00b7streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Reicht, Freunde, euch die Hand! Stolz schl\u00e4gt die Brust gehoben \u2013", "tokens": ["Reicht", ",", "Freun\u00b7de", ",", "euch", "die", "Hand", "!", "Stolz", "schl\u00e4gt", "die", "Brust", "ge\u00b7ho\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "ART", "NN", "$.", "NN", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Freude f\u00fchret auch auf zur Unsterblichkeit \u2013", "tokens": ["Die", "Freu\u00b7de", "f\u00fch\u00b7ret", "auch", "auf", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "APPRART", "NN", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den letzten vollen Klang dem Ewigen dort oben", "tokens": ["Den", "letz\u00b7ten", "vol\u00b7len", "Klang", "dem", "E\u00b7wi\u00b7gen", "dort", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "ADJA", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr manchen sch\u00f6nen Tag, f\u00fcr diesen sch\u00f6nen heut!", "tokens": ["F\u00fcr", "man\u00b7chen", "sch\u00f6\u00b7nen", "Tag", ",", "f\u00fcr", "die\u00b7sen", "sch\u00f6\u00b7nen", "heut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ihr krausen Grillen weg! Hinweg, ihr d\u00fcstern Sorgen!", "tokens": ["Ihr", "krau\u00b7sen", "Gril\u00b7len", "weg", "!", "Hin\u00b7weg", ",", "ihr", "d\u00fcs\u00b7tern", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$.", "ADV", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tief in das Meer hinab! Heut la\u00dft uns fr\u00f6hlich sein!", "tokens": ["Tief", "in", "das", "Meer", "hin\u00b7ab", "!", "Heut", "la\u00dft", "uns", "fr\u00f6h\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein weises Schicksal deckt den Sterblichen den Morgen \u2013", "tokens": ["Ein", "wei\u00b7ses", "Schick\u00b7sal", "deckt", "den", "Sterb\