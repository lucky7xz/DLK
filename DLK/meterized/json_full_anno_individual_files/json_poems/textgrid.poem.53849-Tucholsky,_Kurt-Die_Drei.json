{"textgrid.poem.53849": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Drei", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Gutsherrn mit den fetten Backen,", "tokens": ["Den", "Guts\u00b7herrn", "mit", "den", "fet\u00b7ten", "Ba\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "den Ringen und dem Speck im Nacken,", "tokens": ["den", "Rin\u00b7gen", "und", "dem", "Speck", "im", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit Haus und Hof und mit Gesinde,", "tokens": ["mit", "Haus", "und", "Hof", "und", "mit", "Ge\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit junger Frau und gutgen\u00e4hrtem Kinde;", "tokens": ["mit", "jun\u00b7ger", "Frau", "und", "gut\u00b7ge\u00b7n\u00e4hr\u00b7tem", "Kin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "den Gutsherrn mit dem Schlo\u00df in Fliederranken,", "tokens": ["den", "Guts\u00b7herrn", "mit", "dem", "Schlo\u00df", "in", "Flie\u00b7der\u00b7ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "m