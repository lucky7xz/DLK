{"textgrid.poem.57940": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Umgehungsgesang", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab'n wir nicht was Sch\u00f6nes angerichtet?", "tokens": ["Hab'n", "wir", "nicht", "was", "Sch\u00f6\u00b7nes", "an\u00b7ge\u00b7rich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PWS", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hab'n wir nicht was Herrliches erreicht?", "tokens": ["Hab'n", "wir", "nicht", "was", "Herr\u00b7li\u00b7ches", "er\u00b7reicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PWS", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Haben endlich mit Geduld und Spucke", "tokens": ["Ha\u00b7ben", "end\u00b7lich", "mit", "Ge\u00b7duld", "und", "Spu\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Des Ministers hartes Herz erweicht.", "tokens": ["Des", "Mi\u00b7nis\u00b7ters", "har\u00b7tes", "Herz", "er\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Gro\u00dfe D\u00e4mme werden aufgeworfen", "tokens": ["Gro\u00b7\u00dfe", "D\u00e4m\u00b7me", "wer\u00b7den", "auf\u00b7ge\u00b7wor\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Landschaft mitten durch halbiert,", "tokens": ["Und", "die", "Land\u00b7schaft", "mit\u00b7ten", "durch", "hal\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "VVFIN", "$,"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wenn es nicht ein gro\u00dfer Vorteil w\u00e4re,", "tokens": ["Wenn", "es", "nicht", "ein", "gro\u00b7\u00dfer", "Vor\u00b7teil", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00f6nnt' man glauben, man sei angeschmiert.", "tokens": ["K\u00f6nnt'", "man", "glau\u00b7ben", ",", "man", "sei", "an\u00b7ge\u00b7schmiert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Mit der Aussicht, damit ist es Essig,", "tokens": ["Mit", "der", "Aus\u00b7sicht", ",", "da\u00b7mit", "ist", "es", "Es\u00b7sig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auch wird der Verkehr sehr stark gehemmt,", "tokens": ["Auch", "wird", "der", "Ver\u00b7kehr", "sehr", "stark", "ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die Stadt wird zwischen hohe W\u00e4lle,", "tokens": ["Und", "die", "Stadt", "wird", "zwi\u00b7schen", "ho\u00b7he", "W\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie 'ne Katze in der T\u00fcr geklemmt.", "tokens": ["Wie", "'ne", "Kat\u00b7ze", "in", "der", "T\u00fcr", "ge\u00b7klemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Die \u00c4sthetik ist total zum Kuckuck", "tokens": ["Die", "\u00c4s\u00b7the\u00b7tik", "ist", "to\u00b7tal", "zum", "Ku\u00b7ckuck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und das Bild der Stadt, das ist verhunzt,", "tokens": ["Und", "das", "Bild", "der", "Stadt", ",", "das", "ist", "ver\u00b7hunzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PDS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als Erfolg die Sache aufzufassen,", "tokens": ["Als", "Er\u00b7folg", "die", "Sa\u00b7che", "auf\u00b7zu\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dazu geh\u00f6rt wahrhaftig eine Kunst.", "tokens": ["Da\u00b7zu", "ge\u00b7h\u00f6rt", "wahr\u00b7haf\u00b7tig", "ei\u00b7ne", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Uns wird bang und uns wird immer b\u00e4nger,", "tokens": ["Uns", "wird", "bang", "und", "uns", "wird", "im\u00b7mer", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unertr\u00e4glich war der Status quo,", "tokens": ["Un\u00b7er\u00b7tr\u00e4g\u00b7lich", "war", "der", "Sta\u00b7tus", "quo", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "FM", "FM", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch bedenkt man sich den Fall genauer,", "tokens": ["Doch", "be\u00b7denkt", "man", "sich", "den", "Fall", "ge\u00b7nau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denkt man, es war doch noch besser so.", "tokens": ["Denkt", "man", ",", "es", "war", "doch", "noch", "bes\u00b7ser", "so", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Doch was hilft's? Die Sache ist geschehen,", "tokens": ["Doch", "was", "hilft's", "?", "Die", "Sa\u00b7che", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Siebzig Millionen die sind hin,", "tokens": ["Sieb\u00b7zig", "Mil\u00b7lion\u00b7en", "die", "sind", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "VAFIN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hin ist hin, verloren ist verloren,", "tokens": ["Hin", "ist", "hin", ",", "ver\u00b7lo\u00b7ren", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tun wir so, als sei es ein Gewinn.", "tokens": ["Tun", "wir", "so", ",", "als", "sei", "es", "ein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Hauptsach ist: Der B\u00fcrger darf nicht merken,", "tokens": ["Haupt\u00b7sach", "ist", ":", "Der", "B\u00fcr\u00b7ger", "darf", "nicht", "mer\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df die Sache eine Pleite war,", "tokens": ["Da\u00df", "die", "Sa\u00b7che", "ei\u00b7ne", "Plei\u00b7te", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als unfehlbar m\u00fcssen wir erscheinen", "tokens": ["Als", "un\u00b7fehl\u00b7bar", "m\u00fcs\u00b7sen", "wir", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Diesem guten Tiere immerdar.", "tokens": ["Die\u00b7sem", "gu\u00b7ten", "Tie\u00b7re", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Darum lasset uns Verz\u00fcckung heucheln", "tokens": ["Da\u00b7rum", "las\u00b7set", "uns", "Ver\u00b7z\u00fc\u00b7ckung", "heu\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ob dem gro\u00dfen unverhofften Gl\u00fcck,", "tokens": ["Ob", "dem", "gro\u00b7\u00dfen", "un\u00b7ver\u00b7hoff\u00b7ten", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn wir sind nun einmal 'reingeschliddert", "tokens": ["Denn", "wir", "sind", "nun", "ein\u00b7mal", "'rein\u00b7ge\u00b7schlid\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und wir k\u00f6nnen nun nicht mehr zur\u00fcck.", "tokens": ["Und", "wir", "k\u00f6n\u00b7nen", "nun", "nicht", "mehr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Drum erhebet m\u00e4chtig eure Stimme,", "tokens": ["Drum", "er\u00b7he\u00b7bet", "m\u00e4ch\u00b7tig", "eu\u00b7re", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stimmt die hehre Lobeweise an:", "tokens": ["Stimmt", "die", "heh\u00b7re", "Lo\u00b7be\u00b7wei\u00b7se", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heil dem Fiskus, Heil dem Magistrate,", "tokens": ["Heil", "dem", "Fis\u00b7kus", ",", "Heil", "dem", "Ma\u00b7gist\u00b7ra\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die uns gaben die Umgehungsbahn!", "tokens": ["Die", "uns", "ga\u00b7ben", "die", "Um\u00b7ge\u00b7hungs\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Hab'n wir nicht was Sch\u00f6nes angerichtet?", "tokens": ["Hab'n", "wir", "nicht", "was", "Sch\u00f6\u00b7nes", "an\u00b7ge\u00b7rich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PWS", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hab'n wir nicht was Herrliches erreicht?", "tokens": ["Hab'n", "wir", "nicht", "was", "Herr\u00b7li\u00b7ches", "er\u00b7reicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PWS", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Haben endlich mit Geduld und Spucke", "tokens": ["Ha\u00b7ben", "end\u00b7lich", "mit", "Ge\u00b7duld", "und", "Spu\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Des Ministers hartes Herz erweicht.", "tokens": ["Des", 