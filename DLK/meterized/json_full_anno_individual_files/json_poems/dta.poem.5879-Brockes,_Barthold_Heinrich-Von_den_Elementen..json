{"dta.poem.5879": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Damit nun der Begriff recht deutlich, klar und rein", "tokens": ["Da\u00b7mit", "nun", "der", "Be\u00b7griff", "recht", "deut\u00b7lich", ",", "klar", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ADV", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Von der Materie Vermischung m\u00f6ge seyn;", "tokens": ["Von", "der", "Ma\u00b7te\u00b7rie", "Ver\u00b7mi\u00b7schung", "m\u00f6\u00b7ge", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "So lasset uns durch theilen, durch bewegen,", "tokens": ["So", "las\u00b7set", "uns", "durch", "thei\u00b7len", ",", "durch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "VVINF", "$,", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Elementen jetzt bem\u00fchet seyn zu finden,", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7ten", "jetzt", "be\u00b7m\u00fc\u00b7het", "seyn", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAINF", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wodurch sich alle C\u00f6rper binden.", "tokens": ["Wo\u00b7durch", "sich", "al\u00b7le", "C\u00f6r\u00b7per", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies ist zu der Natur Geheimni\u00df Th\u00fcr\u2019 und Grund.", "tokens": ["Dies", "ist", "zu", "der", "Na\u00b7tur", "Ge\u00b7heim\u00b7ni\u00df", "Th\u00fcr'", "und", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man nehme ", "tokens": ["Man", "neh\u00b7me"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Gespitzet, biegsam, rauh, gezackt, und welche sich", "tokens": ["Ge\u00b7spit\u00b7zet", ",", "bieg\u00b7sam", ",", "rauh", ",", "ge\u00b7zackt", ",", "und", "wel\u00b7che", "sich"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "$,", "VVPP", "$,", "KON", "PRELS", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auf hundert tausend Arten f\u00fcgen,", "tokens": ["Auf", "hun\u00b7dert", "tau\u00b7send", "Ar\u00b7ten", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und deren Aendrungen ohn\u2019 End\u2019 ver\u00e4nderlich;", "tokens": ["Und", "de\u00b7ren", "A\u00b7en\u00b7drun\u00b7gen", "ohn'", "End'", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "So wird man gleich, statt der ", "tokens": ["So", "wird", "man", "gleich", ",", "statt", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "$,", "KOUI", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Die blo\u00df ein Nahme nur voll Eitelkeit,", "tokens": ["Die", "blo\u00df", "ein", "Nah\u00b7me", "nur", "voll", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Schon den Begriff von ihren ", "tokens": ["Schon", "den", "Be\u00b7griff", "von", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.14": {"text": "Man sieht viel deutlicher die Eigenschafften an;", "tokens": ["Man", "sieht", "viel", "deut\u00b7li\u00b7cher", "die", "Ei\u00b7gen\u00b7schaff\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wie sie w\u00fcrcken, mercket man.", "tokens": ["Und", "wie", "sie", "w\u00fcr\u00b7cken", ",", "mer\u00b7cket", "man", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "$,", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ach! wie viel Freude, welche Lust", "tokens": ["Ach", "!", "wie", "viel", "Freu\u00b7de", ",", "wel\u00b7che", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "PIAT", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Erwecken diese klein-geformt-und rege Dinge", "tokens": ["Er\u00b7we\u00b7cken", "die\u00b7se", "klein\u00b7ge\u00b7form\u00b7t\u00b7und", "re\u00b7ge", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "In eines Weisen Aug\u2019 und Brust!", "tokens": ["In", "ei\u00b7nes", "Wei\u00b7sen", "Aug'", "und", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wie wird dasjenige, was sich in ihnen reget,", "tokens": ["Wie", "wird", "das\u00b7je\u00b7ni\u00b7ge", ",", "was", "sich", "in", "ih\u00b7nen", "re\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PDS", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das sonst unsichtbar ist, und so geringe,", "tokens": ["Das", "sonst", "un\u00b7sicht\u00b7bar", "ist", ",", "und", "so", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VAFIN", "$,", "KON", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wenn man es recht erweget,", "tokens": ["Wenn", "man", "es", "recht", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "So klar, so hell und f\u00fchlbar dargeleget!", "tokens": ["So", "klar", ",", "so", "hell", "und", "f\u00fchl\u00b7bar", "dar\u00b7ge\u00b7le\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und wie sind nicht die Elemente,", "tokens": ["Und", "wie", "sind", "nicht", "die", "E\u00b7le\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "So bis daher die unachtsame Welt", "tokens": ["So", "bis", "da\u00b7her", "die", "u\u00b7nacht\u00b7sa\u00b7me", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Noch nicht erkennte,", "tokens": ["Noch", "nicht", "er\u00b7kenn\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Dem Geist so deutlich vorgestellt!", "tokens": ["Dem", "Geist", "so", "deut\u00b7lich", "vor\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}