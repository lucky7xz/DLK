{"dta.poem.2946": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "145.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6pferische Geist f\u00fchlt sich nicht in der Welt", "tokens": ["Der", "sch\u00f6p\u00b7fe\u00b7ri\u00b7sche", "Geist", "f\u00fchlt", "sich", "nicht", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Befriedigt, wo er nicht sich sch\u00f6pferisch verh\u00e4lt.", "tokens": ["Be\u00b7frie\u00b7digt", ",", "wo", "er", "nicht", "sich", "sch\u00f6p\u00b7fe\u00b7risch", "ver\u00b7h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "PTKNEG", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Arbeiten mu\u00df er drum entweder alle Frist,", "tokens": ["Ar\u00b7bei\u00b7ten", "mu\u00df", "er", "drum", "ent\u00b7we\u00b7der", "al\u00b7le", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PAV", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Arbeit eine Art von Sch\u00f6pfung immer ist;", "tokens": ["Weil", "Ar\u00b7beit", "ei\u00b7ne", "Art", "von", "Sch\u00f6p\u00b7fung", "im\u00b7mer", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo nicht, so tr\u00e4umen wird er, denken oder dichten,", "tokens": ["Wo", "nicht", ",", "so", "tr\u00e4u\u00b7men", "wird", "er", ",", "den\u00b7ken", "o\u00b7der", "dich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVINF", "VAFIN", "PPER", "$,", "VVINF", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sch\u00f6pfungen aus sich selbst vorrufen und vernichten.", "tokens": ["Sch\u00f6p\u00b7fun\u00b7gen", "aus", "sich", "selbst", "vor\u00b7ru\u00b7fen", "und", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch nur ein Zeitvertreib ist dieses und ein Spiel,", "tokens": ["Doch", "nur", "ein", "Zeit\u00b7ver\u00b7treib", "ist", "die\u00b7ses", "und", "ein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PDAT", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Wirken h\u00f6hrer Art ist sein gestecktes Ziel,", "tokens": ["Ein", "Wir\u00b7ken", "h\u00f6h\u00b7rer", "Art", "ist", "sein", "ge\u00b7steck\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wo nicht die Wirklichkeit einengend mich umringt,", "tokens": ["Wo", "nicht", "die", "Wirk\u00b7lich\u00b7keit", "ei\u00b7nen\u00b7gend", "mich", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVPP", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Geschaffenheitsgef\u00fchl die Sch\u00f6pferkraft bedingt.", "tokens": ["Ge\u00b7schaf\u00b7fen\u00b7heits\u00b7ge\u00b7f\u00fchl", "die", "Sch\u00f6p\u00b7fer\u00b7kraft", "be\u00b7dingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Willst du der Sch\u00f6pfer seyn? Nein, aber dem Verein", "tokens": ["Willst", "du", "der", "Sch\u00f6p\u00b7fer", "seyn", "?", "Nein", ",", "a\u00b7ber", "dem", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$.", "PTKANT", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00f6pfungsgeister mitbeseligt mich anreihn.", "tokens": ["Der", "Sch\u00f6p\u00b7fungs\u00b7geis\u00b7ter", "mit\u00b7be\u00b7se\u00b7ligt", "mich", "an\u00b7reihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wo ist der Weg dazu? In Demut hin zu wallen,", "tokens": ["Wo", "ist", "der", "Weg", "da\u00b7zu", "?", "In", "De\u00b7mut", "hin", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis aus der Pr\u00fcfung dich ruft Gottes Wohlgefallen.", "tokens": ["Bis", "aus", "der", "Pr\u00fc\u00b7fung", "dich", "ruft", "Got\u00b7tes", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPER", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Im Kleinen wirke recht und bilde treu das Sch\u00f6ne,", "tokens": ["Im", "Klei\u00b7nen", "wir\u00b7ke", "recht", "und", "bil\u00b7de", "treu", "das", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit an H\u00f6heres sich sanft der Trieb gew\u00f6hne.", "tokens": ["Da\u00b7mit", "an", "H\u00f6\u00b7he\u00b7res", "sich", "sanft", "der", "Trieb", "ge\u00b7w\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "PRF", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}