{"dta.poem.16436": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "97.  Mit GOtt vereinigt seyn/ ist  \n  gut f\u00fcr Ewge Pein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer GOtt vereinigt ist/ den kan Er nicht verdammen:", "tokens": ["Wer", "Gott", "ver\u00b7ei\u00b7nigt", "ist", "/", "den", "kan", "Er", "nicht", "ver\u00b7dam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "VAFIN", "$(", "ART", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst\u00fcrtze sich dann selbst mit jhm in Tod und Flam\u0304en.", "tokens": ["Er\u00b7st\u00fcrt\u00b7ze", "sich", "dann", "selbst", "mit", "jhm", "in", "Tod", "und", "Flam\u0304en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}