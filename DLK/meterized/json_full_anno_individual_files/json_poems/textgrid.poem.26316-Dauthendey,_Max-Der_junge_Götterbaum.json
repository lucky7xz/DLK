{"textgrid.poem.26316": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der junge G\u00f6tterbaum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger G\u00f6tterbaum hat heut zum Gru\u00df entboten.", "tokens": ["Ein", "jun\u00b7ger", "G\u00f6t\u00b7ter\u00b7baum", "hat", "heut", "zum", "Gru\u00df", "ent\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eben aufgeschlossen Tulpenbl\u00fcten, die scharlachroten.", "tokens": ["Die", "e\u00b7ben", "auf\u00b7ge\u00b7schlos\u00b7sen", "Tul\u00b7pen\u00b7bl\u00fc\u00b7ten", ",", "die", "schar\u00b7la\u00b7ch\u00b7ro\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und liege ich zur hei\u00dfen Stunde auf dem Bett,", "tokens": ["Und", "lie\u00b7ge", "ich", "zur", "hei\u00b7\u00dfen", "Stun\u00b7de", "auf", "dem", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "D\u00fcnkt mich, er h\u00e4lt die Blumen hin aufs Fensterbrett.", "tokens": ["D\u00fcnkt", "mich", ",", "er", "h\u00e4lt", "die", "Blu\u00b7men", "hin", "aufs", "Fens\u00b7ter\u00b7brett", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER