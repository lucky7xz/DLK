{"textgrid.poem.47479": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[horch' h\u00f6rst du nicht vom Himmel her] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erschlagen ist Hafis. Wer ihn erschlagen,", "tokens": ["Er\u00b7schla\u00b7gen", "ist", "Ha\u00b7fis", ".", "Wer", "ihn", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "$.", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das mu\u00dft du, Kind, dein schwarzes Auge fragen.", "tokens": ["Das", "mu\u00dft", "du", ",", "Kind", ",", "dein", "schwar\u00b7zes", "Au\u00b7ge", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der b\u00f6se Mohr! Wie grimmig ist sein Eisen!", "tokens": ["Der", "b\u00f6\u00b7se", "Mohr", "!", "Wie", "grim\u00b7mig", "ist", "sein", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Tod umr\u00f6chelt ihn in tausend Lagen.", "tokens": ["Der", "Tod", "um\u00b7r\u00f6\u00b7chelt", "ihn", "in", "tau\u00b7send", "La\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach hat der Vogel Edens eine Seele,", "tokens": ["Ach", "hat", "der", "Vo\u00b7gel", "E\u00b7dens", "ei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NE", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Strebt nicht zu dir sein hei\u00dfes Fl\u00fcgelschlagen? \u2013", "tokens": ["Strebt", "nicht", "zu", "dir", "sein", "hei\u00b7\u00dfes", "Fl\u00fc\u00b7gel\u00b7schla\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht schrecke mich mit der Vernunft! Denn diese,", "tokens": ["Nicht", "schre\u00b7cke", "mich", "mit", "der", "Ver\u00b7nunft", "!", "Denn", "die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "KON", "PDS", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nichts hat sie hier zu schaffen und zu sagen.", "tokens": ["Nichts", "hat", "sie", "hier", "zu", "schaf\u00b7fen", "und", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Liebe Weg ist unbegrenzt, ", "tokens": ["Der", "Lie\u00b7be", "Weg", "ist", "un\u00b7be\u00b7grenzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kalte Schranken nicht und enge Hagen.", "tokens": ["So", "kal\u00b7te", "Schran\u00b7ken", "nicht", "und", "en\u00b7ge", "Ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Barmherzigkeit \u2013 welch edle Tugend\u00fcbung!", "tokens": ["Barm\u00b7her\u00b7zig\u00b7keit", "\u2013", "welch", "ed\u00b7le", "Tu\u00b7gend\u00b7\u00fc\u00b7bung", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was hast du hier zu zaudern und zu zagen?", "tokens": ["Was", "hast", "du", "hier", "zu", "zau\u00b7dern", "und", "zu", "za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Du bist aus einem eignen Stoff gebildet;", "tokens": ["Du", "bist", "aus", "ei\u00b7nem", "eig\u00b7nen", "Stoff", "ge\u00b7bil\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich r\u00fchren nicht Hafisens sch\u00f6ne Klagen.", "tokens": ["Dich", "r\u00fch\u00b7ren", "nicht", "Ha\u00b7fi\u00b7sens", "sch\u00f6\u00b7ne", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Erschlagen ist Hafis. Wer ihn erschlagen,", "tokens": ["Er\u00b7schla\u00b7gen", "ist", "Ha\u00b7fis", ".", "Wer", "ihn", "er\u00b7schla\u00b7gen", ","], "token