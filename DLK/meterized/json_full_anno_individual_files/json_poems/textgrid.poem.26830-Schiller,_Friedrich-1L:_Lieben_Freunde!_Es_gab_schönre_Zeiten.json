{"textgrid.poem.26830": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lieben Freunde! Es gab sch\u00f6nre Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieben Freunde! Es gab sch\u00f6nre Zeiten", "tokens": ["Lie\u00b7ben", "Freun\u00b7de", "!", "Es", "gab", "sch\u00f6n\u00b7re", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Als die unsern \u2013 das ist nicht zu streiten!", "tokens": ["Als", "die", "un\u00b7sern", "\u2013", "das", "ist", "nicht", "zu", "strei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "$(", "PDS", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ein edler Volk hat einst gelebt.", "tokens": ["Und", "ein", "ed\u00b7ler", "Volk", "hat", "einst", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00f6nnte die Geschichte davon schweigen,", "tokens": ["K\u00f6nn\u00b7te", "die", "Ge\u00b7schich\u00b7te", "da\u00b7von", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Tausend Steine w\u00fcrden redend zeugen,", "tokens": ["Tau\u00b7send", "Stei\u00b7ne", "w\u00fcr\u00b7den", "re\u00b7dend", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die man aus dem Scho\u00df der Erde gr\u00e4bt.", "tokens": ["Die", "man", "aus", "dem", "Scho\u00df", "der", "Er\u00b7de", "gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Doch es ist dahin, es ist verschwunden,", "tokens": ["Doch", "es", "ist", "da\u00b7hin", ",", "es", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PAV", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Dieses hochbeg\u00fcnstigte Geschlecht.", "tokens": ["Die\u00b7ses", "hoch\u00b7be\u00b7g\u00fcns\u00b7tig\u00b7te", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Wir, wir ", "tokens": ["Wir", ",", "wir"], "token_info": ["word", "punct", "word"], "pos": ["PPER", "$,", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Und der Lebende hat recht.", "tokens": ["Und", "der", "Le\u00b7ben\u00b7de", "hat", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Freunde! Es gibt gl\u00fccklichere Zonen", "tokens": ["Freun\u00b7de", "!", "Es", "gibt", "gl\u00fcck\u00b7li\u00b7che\u00b7re", "Zo\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als das Land, worin wir leidlich wohnen,", "tokens": ["Als", "das", "Land", ",", "wo\u00b7rin", "wir", "leid\u00b7lich", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie der weitgereiste Wandrer spricht.", "tokens": ["Wie", "der", "weit\u00b7ge\u00b7reis\u00b7te", "Wand\u00b7rer", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aber hat ", "tokens": ["A\u00b7ber", "hat"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "War die ", "tokens": ["War", "die"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Unser Herz erwarmt an ", "tokens": ["Un\u00b7ser", "Herz", "er\u00b7warmt", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Will der Lorbeer hier sich nicht gew\u00f6hnen,", "tokens": ["Will", "der", "Lor\u00b7beer", "hier", "sich", "nicht", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wird die Myrte unsers Winters Raub,", "tokens": ["Wird", "die", "Myr\u00b7te", "un\u00b7sers", "Win\u00b7ters", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Gr\u00fcnet doch, die Schl\u00e4fe zu bekr\u00f6nen,", "tokens": ["Gr\u00fc\u00b7net", "doch", ",", "die", "Schl\u00e4\u00b7fe", "zu", "be\u00b7kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Uns der Rebe muntres Laub.", "tokens": ["Uns", "der", "Re\u00b7be", "mun\u00b7tres", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl von gr\u00f6\u00dferm Leben mag es rauschen,", "tokens": ["Wohl", "von", "gr\u00f6\u00b7\u00dferm", "Le\u00b7ben", "mag", "es", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo vier Welten ihre Sch\u00e4tze tauschen,", "tokens": ["Wo", "vier", "Wel\u00b7ten", "ih\u00b7re", "Sch\u00e4t\u00b7ze", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "An der Themse, auf dem Markt der Welt.", "tokens": ["An", "der", "Them\u00b7se", ",", "auf", "dem", "Markt", "der", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Tausend Schiffe landen an und gehen,", "tokens": ["Tau\u00b7send", "Schif\u00b7fe", "lan\u00b7den", "an", "und", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "PTKVZ", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da ist jedes K\u00f6stliche zu sehen,", "tokens": ["Da", "ist", "je\u00b7des", "K\u00f6st\u00b7li\u00b7che", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und es herrscht der Erde Gott, das Geld.", "tokens": ["Und", "es", "herrscht", "der", "Er\u00b7de", "Gott", ",", "das", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aber nicht im tr\u00fcben Schlamm der B\u00e4che,", "tokens": ["A\u00b7ber", "nicht", "im", "tr\u00fc\u00b7ben", "Schlamm", "der", "B\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Der von wilden Regeng\u00fcssen schwillt,", "tokens": ["Der", "von", "wil\u00b7den", "Re\u00b7gen\u00b7g\u00fcs\u00b7sen", "schwillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Auf des stillen Baches ebner Fl\u00e4che", "tokens": ["Auf", "des", "stil\u00b7len", "Ba\u00b7ches", "eb\u00b7ner", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Spiegelt sich das Sonnenbild.", "tokens": ["Spie\u00b7gelt", "sich", "das", "Son\u00b7nen\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Pr\u00e4chtiger als wir in unserm Norden", "tokens": ["Pr\u00e4ch\u00b7ti\u00b7ger", "als", "wir", "in", "un\u00b7serm", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wohnt der Bettler an der Engelspforten,", "tokens": ["Wohnt", "der", "Bett\u00b7ler", "an", "der", "En\u00b7gel\u00b7spfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denn er sieht das ewig einzge Rom!", "tokens": ["Denn", "er", "sieht", "das", "e\u00b7wig", "einz\u00b7ge", "Rom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihn umgibt der Sch\u00f6nheit Glanzgewimmel,", "tokens": ["Ihn", "um\u00b7gibt", "der", "Sch\u00f6n\u00b7heit", "Glanz\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und ein zweiter Himmel in den Himmel", "tokens": ["Und", "ein", "zwei\u00b7ter", "Him\u00b7mel", "in", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Steigt Sankt Peters wunderbarer Dom.", "tokens": ["Steigt", "Sankt", "Pe\u00b7ters", "wun\u00b7der\u00b7ba\u00b7rer", "Dom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aber Rom in allem seinem Glanze", "tokens": ["A\u00b7ber", "Rom", "in", "al\u00b7lem", "sei\u00b7nem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ist ein Grab nur der Vergangenheit,", "tokens": ["Ist", "ein", "Grab", "nur", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Leben duftet nur die frische Pflanze,", "tokens": ["Le\u00b7ben", "duf\u00b7tet", "nur", "die", "fri\u00b7sche", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Die die gr\u00fcne Stunde streut.", "tokens": ["Die", "die", "gr\u00fc\u00b7ne", "Stun\u00b7de", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gr\u00f6\u00dfres mag sich anderswo begeben,", "tokens": ["Gr\u00f6\u00df\u00b7res", "mag", "sich", "an\u00b7ders\u00b7wo", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als bei uns in unserm kleinen Leben,", "tokens": ["Als", "bei", "uns", "in", "un\u00b7serm", "klei\u00b7nen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Neues \u2013 hat die Sonne nie gesehn.", "tokens": ["Neu\u00b7es", "\u2013", "hat", "die", "Son\u00b7ne", "nie", "ge\u00b7sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sehn wir doch das Gro\u00dfe ", "tokens": ["Sehn", "wir", "doch", "das", "Gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf den Brettern, die die Welt bedeuten,", "tokens": ["Auf", "den", "Bret\u00b7tern", ",", "die", "die", "Welt", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sinnvoll, still an uns vor\u00fcbergehn.", "tokens": ["Sinn\u00b7voll", ",", "still", "an", "uns", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Alles wiederholt sich nur im Leben,", "tokens": ["Al\u00b7les", "wie\u00b7der\u00b7holt", "sich", "nur", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ewig jung ist nur die Phantasie,", "tokens": ["E\u00b7wig", "jung", "ist", "nur", "die", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Was sich nie und nirgends hat begeben,", "tokens": ["Was", "sich", "nie", "und", "nir\u00b7gends", "hat", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Das allein veraltet nie!", "tokens": ["Das", "al\u00b7lein", "ver\u00b7al\u00b7tet", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lieben Freunde! Es gab sch\u00f6nre Zeiten", "tokens": ["Lie\u00b7ben", "Freun\u00b7de", "!", "Es", "gab", "sch\u00f6n\u00b7re", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Als die unsern \u2013 das ist nicht zu streiten!", "tokens": ["Als", "die", "un\u00b7sern", "\u2013", "das", "ist", "nicht", "zu", "strei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "$(", "PDS", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ein edler Volk hat einst gelebt.", "tokens": ["Und", "ein", "ed\u00b7ler", "Volk", "hat", "einst", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00f6nnte die Gesch