": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Alles wiederholt sich nur im Leben,", "tokens": ["Al\u00b7les", "wie\u00b7der\u00b7holt", "sich", "nur", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ewig jung ist nur die Phantasie,", "tokens": ["E\u00b7wig", "jung", "ist", "nur", "die", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Was sich nie und nirgends hat begeben,", "tokens": ["Was", "sich", "nie", "und", "nir\u00b7gends", "hat", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Das allein veraltet nie!", "tokens": ["Das", "al\u00b7lein", "ver\u00b7al\u00b7tet", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}