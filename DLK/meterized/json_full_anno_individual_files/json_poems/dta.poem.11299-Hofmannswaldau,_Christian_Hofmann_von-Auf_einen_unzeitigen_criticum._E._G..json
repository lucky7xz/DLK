{"dta.poem.11299": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen unzeitigen  criticum.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' im leben stets viel urthel abgefa\u00dft,", "tokens": ["Ich", "hab'", "im", "le\u00b7ben", "stets", "viel", "ur\u00b7thel", "ab\u00b7ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "VVFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeden, der was schrieb, verwegen angetast.", "tokens": ["Und", "je\u00b7den", ",", "der", "was", "schrieb", ",", "ver\u00b7we\u00b7gen", "an\u00b7ge\u00b7tast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PRELS", "PIS", "VVFIN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein weil ich das recht gar selten recht gesprochen,", "tokens": ["Al\u00b7lein", "weil", "ich", "das", "recht", "gar", "sel\u00b7ten", "recht", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PDS", "ADV", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat itzt \u00fcber mir der tod den stab gebrochen.", "tokens": ["So", "hat", "itzt", "\u00fc\u00b7ber", "mir", "der", "tod", "den", "stab", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}