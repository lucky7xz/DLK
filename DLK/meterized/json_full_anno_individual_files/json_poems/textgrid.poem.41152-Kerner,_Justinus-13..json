{"textgrid.poem.41152": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gar eine Puppe, jenes Zwitterding", "tokens": ["Gar", "ei\u00b7ne", "Pup\u00b7pe", ",", "je\u00b7nes", "Zwit\u00b7ter\u00b7ding"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zwischen der Raupe und dem Schmetterling,", "tokens": ["Zwi\u00b7schen", "der", "Rau\u00b7pe", "und", "dem", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stieg aus dem Hades auf ganz fl\u00fcgellos.", "tokens": ["Stieg", "aus", "dem", "Ha\u00b7des", "auf", "ganz", "fl\u00fc\u00b7gel\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADV", "ADJD", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "\u00bbzur\u00fcck mit dir in Schattenreiches Scho\u00df,", "tokens": ["\u00bb", "zu\u00b7r\u00fcck", "mit", "dir", "in", "Schat\u00b7ten\u00b7rei\u00b7ches", "Scho\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bis Fl\u00fcgel dir gewachsen licht und gro\u00df!\u00ab \u2013", "tokens": ["Bis", "Fl\u00fc\u00b7gel", "dir", "ge\u00b7wach\u00b7sen", "licht", "und", "gro\u00df", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "PPER", "ADJA", "NN", "KON", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbdie kommen nicht, ich lie\u00df schon lang mich narren,", "tokens": ["\u00bb", "die", "kom\u00b7men", "nicht", ",", "ich", "lie\u00df", "schon", "lang", "mich", "nar\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht l\u00e4nger will ich in der Nacht mehr harren,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "will", "ich", "in", "der", "Nacht", "mehr", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Dummkopf ist, der spricht: \u203aDurch Nacht zum Licht!\u2039", "tokens": ["Ein", "Dumm\u00b7kopf", "ist", ",", "der", "spricht", ":", "\u203a", "Durch", "Nacht", "zum", "Licht", "!", "\u2039"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "$.", "$(", "APPR", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Durch den Verstand zum Licht, nicht durch die Nacht!", "tokens": ["Durch", "den", "Ver\u00b7stand", "zum", "Licht", ",", "nicht", "durch", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "So hat's mein lichter Kopf sich stets gedacht.\u00ab", "tokens": ["So", "hat's", "mein", "lich\u00b7ter", "Kopf", "sich", "stets", "ge\u00b7dacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Also die irre Seele zu mir spricht.", "tokens": ["Al\u00b7so", "die", "ir\u00b7re", "See\u00b7le", "zu", "mir", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Ich aber sprach zu ihr: \u00bbDein trotziges Gesicht", "tokens": ["Ich", "a\u00b7ber", "sprach", "zu", "ihr", ":", "\u00bb", "Dein", "trot\u00b7zi\u00b7ges", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "$.", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Schaut aus der Puppe noch, wie's ehmals war,", "tokens": ["Schaut", "aus", "der", "Pup\u00b7pe", "noch", ",", "wie's", "eh\u00b7mals", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$,", "VVFIN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und jene schwere M\u00fctze, die sogar", "tokens": ["Und", "je\u00b7ne", "schwe\u00b7re", "M\u00fct\u00b7ze", ",", "die", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Du noch im Hades nicht hast abgestreift,", "tokens": ["Du", "noch", "im", "Ha\u00b7des", "nicht", "hast", "ab\u00b7ge\u00b7streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Beweisen, da\u00df zum Flug du nicht gereift,", "tokens": ["Be\u00b7wei\u00b7sen", ",", "da\u00df", "zum", "Flug", "du", "nicht", "ge\u00b7reift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "APPRART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dein Kopf es ist, dein Stolz, dein Selbstbetrug,", "tokens": ["Dein", "Kopf", "es", "ist", ",", "dein", "Stolz", ",", "dein", "Selbst\u00b7be\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Was dir noch lange hemmt den leichten Flug.\u00ab", "tokens": ["Was", "dir", "noch", "lan\u00b7ge", "hemmt", "den", "leich\u00b7ten", "Flug", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So sprach zur Puppe ich, die eine Hand,", "tokens": ["So", "sprach", "zur", "Pup\u00b7pe", "ich", ",", "die", "ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Unsichtbar mir, zur\u00fcc