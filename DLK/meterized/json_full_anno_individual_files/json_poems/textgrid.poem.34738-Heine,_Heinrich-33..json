{"textgrid.poem.34738": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgens send ich dir die Veilchen,", "tokens": ["Mor\u00b7gens", "send", "ich", "dir", "die", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich fr\u00fch im Wald gefunden,", "tokens": ["Die", "ich", "fr\u00fch", "im", "Wald", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Abends bring ich Rosen,", "tokens": ["Und", "des", "A\u00b7bends", "bring", "ich", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ich brach in D\u00e4mmrungstunden.", "tokens": ["Die", "ich", "brach", "in", "D\u00e4mm\u00b7rungs\u00b7tun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wei\u00dft du, was die h\u00fcbschen Blumen", "tokens": ["Wei\u00dft", "du", ",", "was", "die", "h\u00fcb\u00b7schen", "Blu\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir Verbl\u00fcmtes sagen m\u00f6chten?", "tokens": ["Dir", "Ver\