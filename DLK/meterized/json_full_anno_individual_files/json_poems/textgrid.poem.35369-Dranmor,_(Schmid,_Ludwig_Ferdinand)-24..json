{"textgrid.poem.35369": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich klage nicht. \u2013 Zwar ist mein Lebensbuch,", "tokens": ["Ich", "kla\u00b7ge", "nicht", ".", "\u2013", "Zwar", "ist", "mein", "Le\u00b7bens\u00b7buch", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich andern, kein vergoldetes gewesen;", "tokens": ["Gleich", "an\u00b7dern", ",", "kein", "ver\u00b7gol\u00b7de\u00b7tes", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "PIAT", "ADJA", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf mancher Seite ist das Wort zu lesen:", "tokens": ["Auf", "man\u00b7cher", "Sei\u00b7te", "ist", "das", "Wort", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, soviel Arbeit um ein Leichentuch!", "tokens": ["Ach", ",", "so\u00b7viel", "Ar\u00b7beit", "um", "ein", "Lei\u00b7chen\u00b7tuch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Nicht jene Ruhe, die Horaz besungen,", "tokens": ["Nicht", "je\u00b7ne", "Ru\u00b7he", ",", "die", "Ho\u00b7raz", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gen\u00fcgt mir ganz \u2013 mit l\u00e4ndlichen Idyllen", "tokens": ["Ge\u00b7n\u00fcgt", "mir", "ganz", "\u2013", "mit", "l\u00e4nd\u00b7li\u00b7chen", "I\u00b7dyl\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vermag ich meine Sehnsucht nicht zu stillen:", "tokens": ["Ver\u00b7mag", "ich", "mei\u00b7ne", "Sehn\u00b7sucht", "nicht", "zu", "stil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch ein gerechtes Urteil ist errungen.", "tokens": ["Doch", "ein", "ge\u00b7rech\u00b7tes", "Ur\u00b7teil", "ist", "er\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn Leidenschaften, w\u00fctende Despoten,", "tokens": ["Wenn", "Lei\u00b7den\u00b7schaf\u00b7ten", ",", "w\u00fc\u00b7ten\u00b7de", "Des\u00b7po\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mir einst das Herz zerfleischt, in jungen Jahren,", "tokens": ["Mir", "einst", "das", "Herz", "zer\u00b7fleischt", ",", "in", "jun\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sein hei\u00dfes Pochen hat Ersatz geboten", "tokens": ["Sein", "hei\u00b7\u00dfes", "Po\u00b7chen", "hat", "Er\u00b7satz", "ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcr jener Zeiten Leiden und Gefahren.", "tokens": ["F\u00fcr", "je\u00b7ner", "Zei\u00b7ten", "Lei\u00b7den", "und", "Ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn statt des Friedens, den ich stets erfleht,", "tokens": ["Wenn", "statt", "des", "Frie\u00b7dens", ",", "den", "ich", "stets", "er\u00b7fleht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Gedanken mich erf\u00fcllten, kaum zu z\u00fcgeln,", "tokens": ["Ge\u00b7dan\u00b7ken", "mich", "er\u00b7f\u00fcll\u00b7ten", ",", "kaum", "zu", "z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Liebe hat mit ihren Engelsfl\u00fcgeln", "tokens": ["Die", "Lie\u00b7be", "hat", "mit", "ih\u00b7ren", "En\u00b7gels\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mir immer neue Hoffnung zugeweht.", "tokens": ["Mir", "im\u00b7mer", "neu\u00b7e", "Hoff\u00b7nung", "zu\u00b7ge\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "O fern von hier, verloren in der Wildnis,", "tokens": ["O", "fern", "von", "hier", ",", "ver\u00b7lo\u00b7ren", "in", "der", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ADV", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sah ich manch s\u00fc\u00dfes, manch geliebtes Bildnis,", "tokens": ["Sah", "ich", "manch", "s\u00fc\u00b7\u00dfes", ",", "manch", "ge\u00b7lieb\u00b7tes", "Bild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Sah holder Augen Glanz her\u00fcberleuchten", "tokens": ["Sah", "hol\u00b7der", "Au\u00b7gen", "Glanz", "her\u00b7\u00fc\u00b7berl\u00b7euch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "In N\u00e4chte, halb durchtr\u00e4umte, halb durchweinte,", "tokens": ["In", "N\u00e4ch\u00b7te", ",", "halb", "durch\u00b7tr\u00e4um\u00b7te", ",", "halb", "durch\u00b7wein\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und f\u00fchlte Thr\u00e4nen, treu' und gutgemeinte,", "tokens": ["Und", "f\u00fchl\u00b7te", "Thr\u00e4\u00b7nen", ",", "treu'", "und", "gut\u00b7ge\u00b7mein\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die fieberhafte Stirne mir befeuchten.", "tokens": ["Die", "fie\u00b7ber\u00b7haf\u00b7te", "Stir\u00b7ne", "mir", "be\u00b7feuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "So kann ich das Verlorne nun verschmerzen", "tokens": ["So", "kann", "ich", "das", "Ver\u00b7lor\u00b7ne", "nun", "ver\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und mich in das, was unabwendbar, schicken", "tokens": ["Und", "mich", "in", "das", ",", "was", "un\u00b7ab\u00b7wend\u00b7bar", ",", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "APPR", "PDS", "$,", "PRELS", "ADJD", "$,", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Und an Erinnerungen mich erquicken", "tokens": ["Und", "an", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "mich", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Mit Todeshymnen im Poetenherzen.", "tokens": ["Mit", "To\u00b7des\u00b7hym\u00b7nen", "im", "Poe\u00b7ten\u00b7her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich klage nicht. \u2013 Zwar ist mein Lebe