{"dta.poem.2066": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Jlti\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dem Marter und der Wiesel hat er einerley", "tokens": ["Mit", "dem", "Mar\u00b7ter", "und", "der", "Wie\u00b7sel", "hat", "er", "ei\u00b7ner\u00b7ley"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "VAFIN", "PPER", "PIAT"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Natur,", "tokens": ["Na\u00b7tur", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ist auch meist von ihrer Gr\u00f6\u00dfe und derselbigen Figur,", "tokens": ["Ist", "auch", "meist", "von", "ih\u00b7rer", "Gr\u00f6\u00b7\u00dfe", "und", "der\u00b7sel\u00b7bi\u00b7gen", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Au\u00dfer, da\u00df sein Balg viel schlechter, und im schlechtern", "tokens": ["Au\u00b7\u00dfer", ",", "da\u00df", "sein", "Balg", "viel", "schlech\u00b7ter", ",", "und", "im", "schlech\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,", "KON", "APPRART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Preise nur", "tokens": ["Prei\u00b7se", "nur"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Insgemein verkaufet wird; wodurch denn auch armen", "tokens": ["Ins\u00b7ge\u00b7mein", "ver\u00b7kau\u00b7fet", "wird", ";", "wo\u00b7durch", "denn", "auch", "ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "$.", "PWAV", "ADV", "ADV", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Leuten", "tokens": ["Leu\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "In dem Frost geholfen ist, allerley sich zu bereiten:", "tokens": ["In", "dem", "Frost", "ge\u00b7hol\u00b7fen", "ist", ",", "al\u00b7ler\u00b7ley", "sich", "zu", "be\u00b7rei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "$,", "PIAT", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Um sich vor die strenge K\u00e4lte zu bedecken und zu sch\u00fctzen,", "tokens": ["Um", "sich", "vor", "die", "stren\u00b7ge", "K\u00e4l\u00b7te", "zu", "be\u00b7de\u00b7cken", "und", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "K\u00f6nnen also Jltiss\u2019 auch den verlass\u2019nen Armen n\u00fctzen.", "tokens": ["K\u00f6n\u00b7nen", "al\u00b7so", "Jl\u00b7tiss'", "auch", "den", "ver\u00b7lass'\u00b7nen", "Ar\u00b7men", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}}}}}