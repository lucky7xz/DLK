{"textgrid.poem.46235": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An meinen brudern Ludwig Weckerlin", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem dein gr\u00f6ste sorg durch deiner zungen ton", "tokens": ["In\u00b7dem", "dein", "gr\u00f6s\u00b7te", "sorg", "durch", "dei\u00b7ner", "zun\u00b7gen", "ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der wilden w\u00fcsten welt stolz, torheit und mutwillen", "tokens": ["der", "wil\u00b7den", "w\u00fcs\u00b7ten", "welt", "stolz", ",", "tor\u00b7heit", "und", "mut\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "NN", "ADJD", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "(gleichwie in Griechenland der Calliopen sohn", "tokens": ["(", "gleich\u00b7wie", "in", "Grie\u00b7chen\u00b7land", "der", "Cal\u00b7li\u00b7o\u00b7pen", "sohn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPR", "NE", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "thier, b\u00e4um und stein bezwang) zu z\u00e4hmen und zu stillen;", "tokens": ["thier", ",", "b\u00e4um", "und", "stein", "be\u00b7zwang", ")", "zu", "z\u00e4h\u00b7men", "und", "zu", "stil\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NE", "KON", "ADJD", "VVFIN", "$(", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indem, mein bruder, du, ein wahrer Amphion,", "tokens": ["In\u00b7dem", ",", "mein", "bru\u00b7der", ",", "du", ",", "ein", "wah\u00b7rer", "Am\u00b7phi\u00b7on", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPOSAT", "NN", "$,", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "bem\u00fchest, selig, dich nach gottes gutem willen", "tokens": ["be\u00b7m\u00fc\u00b7hest", ",", "se\u00b7lig", ",", "dich", "nach", "got\u00b7tes", "gu\u00b7tem", "wil\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "$,", "PRF", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "sein himmelische stat, alda er seinen thron,", "tokens": ["sein", "him\u00b7me\u00b7li\u00b7sche", "stat", ",", "al\u00b7da", "er", "sei\u00b7nen", "thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "mit lebendigem zeug zu gr\u00f6\u00dfen und zu f\u00fcllen;", "tokens": ["mit", "le\u00b7ben\u00b7di\u00b7gem", "zeug", "zu", "gr\u00f6\u00b7\u00dfen", "und", "zu", "f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Ich, sehend wie die leut allein geld, ehr und pracht,", "tokens": ["Ich", ",", "se\u00b7hend", "wie", "die", "leut", "al\u00b7lein", "geld", ",", "ehr", "und", "pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "KOKOM", "ART", "NN", "ADV", "NN", "$,", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "des kriegs geschenk und raub, hochachten und begehren,", "tokens": ["des", "kriegs", "ge\u00b7schenk", "und", "raub", ",", "hoch\u00b7ach\u00b7ten", "und", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$,", "ADJA", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ha\u00df solche narretei und klag des Teutschlands nacht;", "tokens": ["ha\u00df", "sol\u00b7che", "nar\u00b7re\u00b7tei", "und", "klag", "des", "Teutschlands", "nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Doch wie ich seufz, da\u00df auch die w\u00f6lf die kirch zerst\u00f6ren,", "tokens": ["Doch", "wie", "ich", "seufz", ",", "da\u00df", "auch", "die", "w\u00f6lf", "die", "kirch", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "also erfreu ich mich ab guter lehrer wacht", "tokens": ["al\u00b7so", "er\u00b7freu", "ich", "mich", "ab", "gu\u00b7ter", "leh\u00b7rer", "wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN"], "meter