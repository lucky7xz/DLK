d", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Pappelstr\u00e4uche r\u00fchrt kein Bliz,", "tokens": ["Pap\u00b7pel\u00b7str\u00e4u\u00b7che", "r\u00fchrt", "kein", "Bliz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Eichen schl\u00e4gt das Wetter;", "tokens": ["In", "die", "Ei\u00b7chen", "schl\u00e4gt", "das", "Wet\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, der Demuth Schattensiz", "tokens": ["Ja", ",", "der", "De\u00b7muth", "Schat\u00b7ten\u00b7siz"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trozt die sichern Lorbeerbl\u00e4tter,", "tokens": ["Trozt", "die", "si\u00b7chern", "Lor\u00b7beer\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der Himmel brennt und kracht", "tokens": ["Wenn", "der", "Him\u00b7mel", "brennt", "und", "kracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Erde furchtsam macht.", "tokens": ["Und", "die", "Er\u00b7de", "furcht\u00b7sam", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Edle Demuth, wer dich hat,", "tokens": ["Ed\u00