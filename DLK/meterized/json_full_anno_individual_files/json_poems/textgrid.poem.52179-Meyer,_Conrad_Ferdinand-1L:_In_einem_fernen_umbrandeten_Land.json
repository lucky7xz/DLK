{"textgrid.poem.52179": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einem fernen umbrandeten Land", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem fernen umbrandeten Land", "tokens": ["In", "ei\u00b7nem", "fer\u00b7nen", "um\u00b7bran\u00b7de\u00b7ten", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Spielen die M\u00e4dchen ein Spiel an dem Strand,", "tokens": ["Spie\u00b7len", "die", "M\u00e4d\u00b7chen", "ein", "Spiel", "an", "dem", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Schreiten im Reigen, heiter gesinnt,", "tokens": ["Schrei\u00b7ten", "im", "Rei\u00b7gen", ",", "hei\u00b7ter", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wann zu steigen die Flut beginnt,", "tokens": ["Wann", "zu", "stei\u00b7gen", "die", "Flut", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKZU", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Weichen zur\u00fcck in geme\u00dfner Flucht", "tokens": ["Wei\u00b7chen", "zu\u00b7r\u00fcck", "in", "ge\u00b7me\u00df\u00b7ner", "Flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Aus der schwellenden Meeresbucht.", "tokens": ["Aus", "der", "schwel\u00b7len\u00b7den", "Mee\u00b7res\u00b7bucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "In den Gew\u00e4ssern ruhigklar", "tokens": ["In", "den", "Ge\u00b7w\u00e4s\u00b7sern", "ru\u00b7hig\u00b7klar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Werden sie krause Gestalten gewahr,", "tokens": ["Wer\u00b7den", "sie", "krau\u00b7se", "Ge\u00b7stal\u00b7ten", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Rollt eine Woge, sie sehen ein Ro\u00df,", "tokens": ["Rollt", "ei\u00b7ne", "Wo\u00b7ge", ",", "sie", "se\u00b7hen", "ein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Sehn einen Reiter, bis er zerflo\u00df.", "tokens": ["Sehn", "ei\u00b7nen", "Rei\u00b7ter", ",", "bis", "er", "zer\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "\u00bbschauet den Meermann! Garstig Gesicht!", "tokens": ["\u00bb", "schau\u00b7et", "den", "Meer\u00b7mann", "!", "Gars\u00b7tig", "Ge\u00b7sicht", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$.", "ADJD", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Grinzende Larve, du haschest mich nicht!\u00ab", "tokens": ["Grin\u00b7zen\u00b7de", "Lar\u00b7ve", ",", "du", "ha\u00b7schest", "mich", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Aber das Meer es w\u00e4chst und naht \u2013", "tokens": ["A\u00b7ber", "das", "Meer", "es", "w\u00e4chst", "und", "naht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "\u00bbfliehet, ihr Schwestern! Sonst wird's zu spat!\u00ab", "tokens": ["\u00bb", "flie\u00b7het", ",", "ihr", "Schwes\u00b7tern", "!", "Sonst", "wird's", "zu", "spat", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PTKA", "ADJD", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Alle sie st\u00fcrzen in hastigem Lauf,", "tokens": ["Al\u00b7le", "sie", "st\u00fcr\u00b7zen", "in", "has\u00b7ti\u00b7gem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.16": {"text": "Gleiten, und rei\u00dfen die Strauchelnden auf", "tokens": ["Glei\u00b7ten", ",", "und", "rei\u00b7\u00dfen", "die", "Strau\u00b7cheln\u00b7den", "auf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "ART", "NN", "APPR"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.17": {"text": "Bis zu der Bank, wo die Ebbe beginnt,", "tokens": ["Bis", "zu", "der", "Bank", ",", "wo", "die", "Eb\u00b7be", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.18": {"text": "Wo, wie sie wissen, das Wasser zerrinnt.", "tokens": ["Wo", ",", "wie", "sie", "wis\u00b7sen", ",", "das", "Was\u00b7ser", "zer\u00b7rinnt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "PPER", "VVINF", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Dort ist gelagert der fl\u00fcchtige Chor,", "tokens": ["Dort", "ist", "ge\u00b7la\u00b7gert", "der", "fl\u00fcch\u00b7ti\u00b7ge", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Zieht an dem Felsen die F\u00fc\u00dfe empor,", "tokens": ["Zieht", "an", "dem", "Fel\u00b7sen", "die", "F\u00fc\u00b7\u00dfe", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.21": {"text": "Fleht in den Himmel mit br\u00fcnstigem Schrein:", "tokens": ["Fleht", "in", "den", "Him\u00b7mel", "mit", "br\u00fcns\u00b7ti\u00b7gem", "Schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.22": {"text": "\u00bbg\u00f6tter! ihr lasset die Unschuld allein?\u00ab", "tokens": ["\u00bb", "g\u00f6t\u00b7ter", "!", "ihr", "las\u00b7set", "die", "Un\u00b7schuld", "al\u00b7lein", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Aber die Flut, da den Raub sie ber\u00fchrt,", "tokens": ["A\u00b7ber", "die", "Flut", ",", "da", "den", "Raub", "sie", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.24": {"text": "Hat das Verh\u00e4ngnis des Ebbens gesp\u00fcrt,", "tokens": ["Hat", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "des", "Eb\u00b7bens", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.25": {"text": "Und, wie erschreckt durch das maidliche Ach,", "tokens": ["Und", ",", "wie", "er\u00b7schreckt", "durch", "das", "maid\u00b7li\u00b7che", "Ach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Gleitet sie nieder und f\u00e4llt gemach! \u2013", "tokens": ["Glei\u00b7tet", "sie", "nie\u00b7der", "und", "f\u00e4llt", "ge\u00b7mach", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Gegen die Ziehnde mit drohendem Arm", "tokens": ["Ge\u00b7gen", "die", "Ziehn\u00b7de", "mit", "dro\u00b7hen\u00b7dem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.28": {"text": "Hebt sich verfolgend der bl\u00fchende Schwarm:", "tokens": ["Hebt", "sich", "ver\u00b7fol\u00b7gend", "der", "bl\u00fc\u00b7hen\u00b7de", "Schwarm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "\u00bbh\u00f6hnet die Feigen! Sie fliehn aus dem Krieg!", "tokens": ["\u00bb", "h\u00f6h\u00b7net", "die", "Fei\u00b7gen", "!", "Sie", "fliehn", "aus", "dem", "Krieg", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.30": {"text": "Kr\u00e4nzet die Locken und feiert den Sieg!\u00ab", "tokens": ["Kr\u00e4n\u00b7zet", "die", "Lo\u00b7cken", "und", "fei\u00b7ert", "den", "Sieg", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Also vergn\u00fcgt sich das sterbliche Heer", "tokens": ["Al\u00b7so", "ver\u00b7gn\u00fcgt", "sich", "das", "sterb\u00b7li\u00b7che", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Mit dem gela\u00dfnen, dem ewigen Meer.", "tokens": ["Mit", "dem", "ge\u00b7la\u00df\u00b7nen", ",", "dem", "e\u00b7wi\u00b7gen", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "A