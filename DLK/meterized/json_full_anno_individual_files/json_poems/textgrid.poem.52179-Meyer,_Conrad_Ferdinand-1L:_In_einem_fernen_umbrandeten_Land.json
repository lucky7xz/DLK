pos": ["APPR", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.18": {"text": "Wo, wie sie wissen, das Wasser zerrinnt.", "tokens": ["Wo", ",", "wie", "sie", "wis\u00b7sen", ",", "das", "Was\u00b7ser", "zer\u00b7rinnt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "PPER", "VVINF", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Dort ist gelagert der fl\u00fcchtige Chor,", "tokens": ["Dort", "ist", "ge\u00b7la\u00b7gert", "der", "fl\u00fcch\u00b7ti\u00b7ge", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Zieht an dem Felsen die F\u00fc\u00dfe empor,", "tokens": ["Zieht", "an", "dem", "Fel\u00b7sen", "die", "F\u00fc\u00b7\u00dfe", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.21": {"text": "Fleht in den Himmel mit br\u00fcnstigem Schrein:", "tokens": ["Fleht", "in", "den", "Him\u00b7mel", "mit", "br\u00fcns\u00b7ti\u00b7gem", "Schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.22": {"text": "\u00bbg\u00f6tter! ihr lasset die Unschuld allein?\u00ab", "tokens": ["\u00bb", "g\u00f6t\u00b7ter", "!", "ihr", "las\u00b7set", "die", "Un\u00b7schuld", "al\u00b7lein", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Aber die Flut, da den Raub sie ber\u00fchrt,", "tokens": ["A\u00b7ber", "die", "Flut", ",", "da", "den", "Raub", "sie", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.24": {"text": "Hat das Verh\u00e4ngnis des Ebbens gesp\u00fcrt,", "tokens": ["Hat", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "des", "Eb\u00b7bens", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.25": {"text": "Und, wie erschreckt durch das maidliche Ach,", "tokens": ["Und", ",", "wie", "er\u00b7schreckt", "durch", "das", "maid\u00b7li\u00b7che", "Ach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Gleitet sie nieder und f\u00e4llt gemach! \u2013", "tokens": ["Glei\u00b7tet", "sie", "nie\u00b7der", "und", "f\u00e4llt", "ge\u00b7mach", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Gegen die Ziehnde mit drohendem Arm", "tokens": ["Ge\u00b7gen", "die", "Ziehn\u00b7de", "mit", "dro\u00b7hen\u00b7dem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.28": {"text": "Hebt sich verfolgend der bl\u00fchende Schwarm:", "tokens": ["Hebt", "sich", "ver\u00b7fol\u00b7gend", "der", "bl\u00fc\u00b7hen\u00b7de", "Schwarm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "\u00bbh\u00f6hnet die Feigen! Sie fliehn aus dem Krieg!", "tokens": ["\u00bb", "h\u00f6h\u00b7net", "die", "Fei\u00b7gen", "!", "Sie", "fliehn", "aus", "dem", "Krieg", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.30": {"text": "Kr\u00e4nzet die Locken und feiert den Sieg!\u00ab", "tokens": ["Kr\u00e4n\u00b7zet", "die", "Lo\u00b7cken", "und", "fei\u00b7ert", "den", "Sieg", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Also vergn\u00fcgt sich das sterbliche Heer", "tokens": ["Al\u00b7so", "ver\u00b7gn\u00fcgt", "sich", "das", "sterb\u00b7li\u00b7che", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Mit dem gela\u00dfnen, dem ewigen Meer.", "tokens": ["Mit", "dem", "ge\u00b7la\u00df\u00b7nen", ",", "dem", "e\u00b7wi\u00b7gen", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}