{"textgrid.poem.60443": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An einem Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchl herbstlicher Abend, es weht der Wind,", "tokens": ["K\u00fchl", "herbst\u00b7li\u00b7cher", "A\u00b7bend", ",", "es", "weht", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Am Grabe der Mutter weint das Kind,", "tokens": ["Am", "Gra\u00b7be", "der", "Mut\u00b7ter", "weint", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Freunde, Verwandten umdr\u00e4ngen dicht", "tokens": ["Die", "Freun\u00b7de", ",", "Ver\u00b7wand\u00b7ten", "um\u00b7dr\u00e4n\u00b7gen", "dicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "VVFIN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Prediger, der so r\u00fchrend spricht.", "tokens": ["Den", "Pre\u00b7di\u00b7ger", ",", "der", "so", "r\u00fch\u00b7rend", "spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er gedenkt, wie fromm die Tote war,", "tokens": ["Er", "ge\u00b7denkt", ",", "wie", "fromm", "die", "To\u00b7te", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["P