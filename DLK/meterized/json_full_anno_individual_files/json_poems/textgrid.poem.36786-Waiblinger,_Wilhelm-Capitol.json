{"textgrid.poem.36786": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Capitol", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Berg der G\u00f6tter und Helden, Triumphatoren und Sieger,", "tokens": ["Berg", "der", "G\u00f6t\u00b7ter", "und", "Hel\u00b7den", ",", "Tri\u00b7um\u00b7pha\u00b7to\u00b7ren", "und", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Welche Gedanken du mir, welche Erinn'rungen weckst!", "tokens": ["Wel\u00b7che", "Ge\u00b7dan\u00b7ken", "du", "mir", ",", "wel\u00b7che", "Er\u00b7inn'\u00b7run\u00b7gen", "weckst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "PPER", "$,", "PWAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Denk' ich der Scipionen und all' der alten Heroen,", "tokens": ["Denk'", "ich", "der", "Sci\u00b7pi\u00b7o\u00b7nen", "und", "all'", "der", "al\u00b7ten", "He\u00b7roen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie der Feldherr, das Heer dich im Triumphe betrat!", "tokens": ["Wie", "der", "Feld\u00b7herr", ",", "das", "Heer", "dich", "im", "Tri\u00b7um\u00b7phe", "be\u00b7trat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "