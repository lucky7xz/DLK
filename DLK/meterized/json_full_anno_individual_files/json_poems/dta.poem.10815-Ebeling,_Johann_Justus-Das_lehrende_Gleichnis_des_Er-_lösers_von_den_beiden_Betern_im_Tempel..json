", "von", "sei\u00b7ner", "Tu\u00b7gend", "prah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.92": {"text": "Der ist mit leeren Schein bemahlet;", "tokens": ["Der", "ist", "mit", "lee\u00b7ren", "Schein", "be\u00b7mah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.93": {"text": "Er ging auch wiederum hinaus,", "tokens": ["Er", "ging", "auch", "wie\u00b7de\u00b7rum", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.94": {"text": "Als ungerecht zu seinem Haus.", "tokens": ["Als", "un\u00b7ge\u00b7recht", "zu", "sei\u00b7nem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.95": {"text": "Er konnte bei dem Heuchelwesen,", "tokens": ["Er", "konn\u00b7te", "bei", "dem", "Heu\u00b7chel\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.96": {"text": "Von seiner Krankheit nicht genesen.", "tokens": ["Von", "sei\u00b7ner", "Krank\u00b7heit", "nicht", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.97": {"text": "Der Z\u00f6llner der im Demut kam,", "tokens": ["Der", "Z\u00f6ll\u00b7ner", "der", "im", "De\u00b7mut", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.98": {"text": "Voll heilger Reue, Bu\u00df und Schaam,", "tokens": ["Voll", "heil\u00b7ger", "Reu\u00b7e", ",", "Bu\u00df", "und", "Schaam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.99": {"text": "Erkandte was er hat verbrochen,", "tokens": ["Er\u00b7kand\u00b7te", "was", "er", "hat", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.100": {"text": "Bat Gnade, und ward frei gesprochen.", "tokens": ["Bat", "Gna\u00b7de", ",", "und", "ward", "frei", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KON", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}