{"dta.poem.10815": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das lehrende Gleichnis des Er-  \n l\u00f6sers von den beiden Betern  \n im Tempel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es gingen zwey zu beten hin,", "tokens": ["Es", "gin\u00b7gen", "zwey", "zu", "be\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "PTKZU", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch mit ganz unterschiednen Sinn:", "tokens": ["Doch", "mit", "ganz", "un\u00b7ter\u00b7schied\u00b7nen", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Mensch von denen, die da meinen,", "tokens": ["Ein", "Mensch", "von", "de\u00b7nen", ",", "die", "da", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRELS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie w\u00e4ren heilig, weil sie scheinen.", "tokens": ["Sie", "w\u00e4\u00b7ren", "hei\u00b7lig", ",", "weil", "sie", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein andrer der ein S\u00fcnder war,", "tokens": ["Ein", "an\u00b7drer", "der", "ein", "S\u00fcn\u00b7der", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und es bekandte offenbar,", "tokens": ["Und", "es", "be\u00b7kand\u00b7te", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der kam vor seine grossen S\u00fcnden,", "tokens": ["Der", "kam", "vor", "sei\u00b7ne", "gros\u00b7sen", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Seht erst den stolzen Heuchler an,", "tokens": ["Seht", "erst", "den", "stol\u00b7zen", "Heuch\u00b7ler", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O! was vor ein gerechter Mann;", "tokens": ["O", "!", "was", "vor", "ein", "ge\u00b7rech\u00b7ter", "Mann", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er stehet in dem Heiligthume,", "tokens": ["Er", "ste\u00b7het", "in", "dem", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Posaunet da von seinen Ruhme.", "tokens": ["Po\u00b7sau\u00b7net", "da", "von", "sei\u00b7nen", "Ruh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Er hat ein solches Angesicht,", "tokens": ["Er", "hat", "ein", "sol\u00b7ches", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Woraus der Andacht Flamme bricht,", "tokens": ["Wo\u00b7raus", "der", "An\u00b7dacht", "Flam\u00b7me", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da er von guten Werken prahlet,", "tokens": ["Da", "er", "von", "gu\u00b7ten", "Wer\u00b7ken", "prah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die nur ein blosser Schein gemahlet.", "tokens": ["Die", "nur", "ein", "blos\u00b7ser", "Schein", "ge\u00b7mah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "H\u00f6rt wie er betet, murmelnd sagt,", "tokens": ["H\u00f6rt", "wie", "er", "be\u00b7tet", ",", "mur\u00b7melnd", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Was seinen eitlen Stolz behagt:", "tokens": ["Was", "sei\u00b7nen", "eit\u00b7len", "Stolz", "be\u00b7hagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich danke dir O! GOtt und lobe,", "tokens": ["Ich", "dan\u00b7ke", "dir", "O", "!", "Gott", "und", "lo\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "$.", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Dich vor die grosse Gnaden Probe", "tokens": ["Dich", "vor", "die", "gros\u00b7se", "Gna\u00b7den", "Pro\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df ich nicht bin, wie andre sind,", "tokens": ["Da\u00df", "ich", "nicht", "bin", ",", "wie", "and\u00b7re", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "$,", "PWAV", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ein solch verfluchtes H\u00f6llen Kind;", "tokens": ["Ein", "solch", "ver\u00b7fluch\u00b7tes", "H\u00f6l\u00b7len", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df ich nicht bin wie solche Leute,", "tokens": ["Da\u00df", "ich", "nicht", "bin", "wie", "sol\u00b7che", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die sich mit r\u00e4uberischer Beute,", "tokens": ["Die", "sich", "mit", "r\u00e4u\u00b7be\u00b7ri\u00b7scher", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die gierge Diebesfaust gef\u00fcllt,", "tokens": ["Die", "gier\u00b7ge", "Die\u00b7bes\u00b7faust", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und keiner der da heimlich stielt.", "tokens": ["Und", "kei\u00b7ner", "der", "da", "heim\u00b7lich", "stielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich habe keinen je betrogen,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7nen", "je", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Durch Wucher andre ausgesogen.", "tokens": ["Durch", "Wu\u00b7cher", "and\u00b7re", "aus\u00b7ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die Geilheit hat mich nicht beflekt,", "tokens": ["Die", "Geil\u00b7heit", "hat", "mich", "nicht", "be\u00b7flekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Mich nicht in andrer Bett verstekt.", "tokens": ["Mich", "nicht", "in", "an\u00b7drer", "Bett", "vers\u00b7tekt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ich bin kein Z\u00f6llner der nichts tauget,", "tokens": ["Ich", "bin", "kein", "Z\u00f6ll\u00b7ner", "der", "nichts", "tau\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Und so, gleich wie ein Jgel sauget,", "tokens": ["Und", "so", ",", "gleich", "wie", "ein", "Jgel", "sau\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Das er aufschwillt von andrer Blut,", "tokens": ["Das", "er", "auf\u00b7schwillt", "von", "an\u00b7drer", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Auch an sich saugt der Fremden Gut.", "tokens": ["Auch", "an", "sich", "saugt", "der", "Frem\u00b7den", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Dies alles hab ich nicht verbrochen:", "tokens": ["Dies", "al\u00b7les", "hab", "ich", "nicht", "ver\u00b7bro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Ich faste zwier in der Wochen:", "tokens": ["Ich", "fas\u00b7te", "zwier", "in", "der", "Wo\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Und durch die strenge Lebens Art,", "tokens": ["Und", "durch", "die", "stren\u00b7ge", "Le\u00b7bens", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wird mein Fleisch vor der Lust verwahrt", "tokens": ["Wird", "mein", "Fleisch", "vor", "der", "Lust", "ver\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Der Zehnte ist die Andachtsgabe,", "tokens": ["Der", "Zehn\u00b7te", "ist", "die", "An\u00b7dachts\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Die reich ich dar von allem Haabe.", "tokens": ["Die", "reich", "ich", "dar", "von", "al\u00b7lem", "Haa\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "PTKVZ", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Das war sein murmelndes Gebet,", "tokens": ["Das", "war", "sein", "mur\u00b7meln\u00b7des", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Dadurch er sich, nicht ", "tokens": ["Da\u00b7durch", "er", "sich", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PAV", "PPER", "PRF", "$,", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.43": {"text": "Er wil bei diesem heilgen Gleissen,", "tokens": ["Er", "wil", "bei", "die\u00b7sem", "heil\u00b7gen", "Gleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Ein Frommer und Gerechter heissen.", "tokens": ["Ein", "From\u00b7mer", "und", "Ge\u00b7rech\u00b7ter", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Es soll sein \u00e4usrer Tugendschein,", "tokens": ["Es", "soll", "sein", "\u00e4us\u00b7rer", "Tu\u00b7gend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Die Larve seines Hochmuths seyn:", "tokens": ["Die", "Lar\u00b7ve", "sei\u00b7nes", "Hoch\u00b7muths", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und GOtt soll ihn mit Ehrenkronen,", "tokens": ["Und", "Gott", "soll", "ihn", "mit", "Eh\u00b7ren\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Vor seine Fr\u00f6mmigkeit belohnen.", "tokens": ["Vor", "sei\u00b7ne", "Fr\u00f6m\u00b7mig\u00b7keit", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Der andre der ein Z\u00f6lner war,", "tokens": ["Der", "and\u00b7re", "der", "ein", "Z\u00f6l\u00b7ner", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Der dachte, da\u00df vor ", "tokens": ["Der", "dach\u00b7te", ",", "da\u00df", "vor"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.51": {"text": "Was er als Bosheit ausge\u00fcbet,", "tokens": ["Was", "er", "als", "Bos\u00b7heit", "aus\u00b7ge\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Da er so ", "tokens": ["Da", "er", "so"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.53": {"text": "Er sah, da\u00df er ein B\u00f6sewicht,", "tokens": ["Er", "sah", ",", "da\u00df", "er", "ein", "B\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Drum wolte er auch sein Gesicht,", "tokens": ["Drum", "wol\u00b7te", "er", "auch", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Aus inrer Schaam nicht aufwerts drehen,", "tokens": ["Aus", "in\u00b7rer", "Schaam", "nicht", "auf\u00b7werts", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Noch nah zum Allerheilgen gehen.", "tokens": ["Noch", "nah", "zum", "Al\u00b7ler\u00b7heil\u00b7gen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Er stand von fern, schlug an die Brust,", "tokens": ["Er", "stand", "von", "fern", ",", "schlug", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Worin die Quelle b\u00f6ser Lust,", "tokens": ["Wo\u00b7rin", "die", "Quel\u00b7le", "b\u00f6\u00b7ser", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Das ganz verdorbne Herze stekket,", "tokens": ["Das", "ganz", "ver\u00b7dorb\u00b7ne", "Her\u00b7ze", "stek\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Wodurch sein ganzes Thun beflekket.", "tokens": ["Wo\u00b7durch", "sein", "gan\u00b7zes", "Thun", "be\u00b7flek\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "Die S\u00fcnde peinigte sein Herz,", "tokens": ["Die", "S\u00fcn\u00b7de", "pei\u00b7nig\u00b7te", "sein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Er kam dadurch zur Reu und Schmerz,", "tokens": ["Er", "kam", "da\u00b7durch", "zur", "Reu", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPRART", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "