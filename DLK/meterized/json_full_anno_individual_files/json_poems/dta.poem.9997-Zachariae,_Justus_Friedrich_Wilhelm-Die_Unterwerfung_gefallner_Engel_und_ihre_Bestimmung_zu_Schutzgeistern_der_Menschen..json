{"dta.poem.9997": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Unterwerfung gefallner Engel  \n und ihre Bestimmung  \n zu  \n Schutzgeistern der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thronen, F\u00fcrsten, und M\u00e4chte! der Reu und Be-", "tokens": ["Thro\u00b7nen", ",", "F\u00fcrs\u00b7ten", ",", "und", "M\u00e4ch\u00b7te", "!", "der", "Reu", "und", "Be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "$.", "ART", "NE", "KON", "TRUNC"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die zu Gott um Vergebung gefleht, sind vor ihn ge-", "tokens": ["Die", "zu", "Gott", "um", "Ver\u00b7ge\u00b7bung", "ge\u00b7fleht", ",", "sind", "vor", "ihn", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "APPR", "NN", "VVPP", "$,", "VAFIN", "APPR", "PPER", "TRUNC"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Haben Vergebung erlangt, und den Zorn des Richters", "tokens": ["Ha\u00b7ben", "Ver\u00b7ge\u00b7bung", "er\u00b7langt", ",", "und", "den", "Zorn", "des", "Rich\u00b7ters"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "VVPP", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Heil euch! da\u00df ihr im Staube gekniet, und bittere", "tokens": ["Heil", "euch", "!", "da\u00df", "ihr", "im", "Stau\u00b7be", "ge\u00b7kniet", ",", "und", "bit\u00b7te\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$.", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$,", "KON", "ADJA"], "meter": "+-++-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Zu dem H\u00f6chsten geweint, die euch Vergebung erlanget!", "tokens": ["Zu", "dem", "H\u00f6chs\u00b7ten", "ge\u00b7weint", ",", "die", "euch", "Ver\u00b7ge\u00b7bung", "er\u00b7lan\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Heil euch! Begnadigte! da\u00df f\u00fcr euch noch in Zeiten der", "tokens": ["Heil", "euch", "!", "Be\u00b7gna\u00b7dig\u00b7te", "!", "da\u00df", "f\u00fcr", "euch", "noch", "in", "Zei\u00b7ten", "der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "VVFIN", "$.", "KOUS", "APPR", "PPER", "ADV", "APPR", "NN", "ART"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.7": {"text": "Vom Satanischen Heer am Throne des Richters gezeuget,", "tokens": ["Vom", "Sa\u00b7ta\u00b7ni\u00b7schen", "Heer", "am", "Thro\u00b7ne", "des", "Rich\u00b7ters", "ge\u00b7zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Da\u00df ihr die Fahne des Aufruhrs verlie\u00dft, und in Zeiten", "tokens": ["Da\u00df", "ihr", "die", "Fah\u00b7ne", "des", "Auf\u00b7ruhrs", "ver\u00b7lie\u00dft", ",", "und", "in", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "NN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Bey dem Allm\u00e4chtgen gesucht, die jenen Rebellen ver-", "tokens": ["Bey", "dem", "All\u00b7m\u00e4cht\u00b7gen", "ge\u00b7sucht", ",", "die", "je\u00b7nen", "Re\u00b7bel\u00b7len", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PRELS", "PDAT", "NN", "TRUNC"], "meter": "+-++--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Heitert euch auf, wie Begnadigten ziemt! Doch for-", "tokens": ["Hei\u00b7tert", "euch", "auf", ",", "wie", "Be\u00b7gna\u00b7dig\u00b7ten", "ziemt", "!", "Doch", "for"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "NN", "VVFIN", "$.", "KON", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Euren Gehorsam nunmehr, nicht ohne Pr\u00fcfung. \u2014", "tokens": ["Eu\u00b7ren", "Ge\u00b7hor\u00b7sam", "nun\u00b7mehr", ",", "nicht", "oh\u00b7ne", "Pr\u00fc\u00b7fung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Da\u00df schon lang ein prophetisch Ger\u00fccht im Himmel ge-", "tokens": ["Da\u00df", "schon", "lang", "ein", "pro\u00b7phe\u00b7tisch", "Ge\u00b7r\u00fccht", "im", "Him\u00b7mel", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "ART", "ADJD", "NN", "APPRART", "NN", "TRUNC"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Von der Erschaffung unzehliger Welten, mit herrlichen", "tokens": ["Von", "der", "Er\u00b7schaf\u00b7fung", "un\u00b7zeh\u00b7li\u00b7ger", "Wel\u00b7ten", ",", "mit", "herr\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+--+-+---+--+--", "measure": "iambic.penta.invert"}, "line.14": {"text": "Und unsterblichen Seelen erf\u00fcllt; die hohe Bestimmung", "tokens": ["Und", "uns\u00b7terb\u00b7li\u00b7chen", "See\u00b7len", "er\u00b7f\u00fcllt", ";", "die", "ho\u00b7he", "Be\u00b7stim\u00b7mung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Von der geringern Erde, dem Schauplatz der g\u00f6ttlichen", "tokens": ["Von", "der", "ge\u00b7rin\u00b7gern", "Er\u00b7de", ",", "dem", "Schau\u00b7platz", "der", "g\u00f6tt\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und der Erbarmung des Sohns, ist euch nicht g\u00e4nzlich", "tokens": ["Und", "der", "Er\u00b7bar\u00b7mung", "des", "Sohns", ",", "ist", "euch", "nicht", "g\u00e4nz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Da wir so oft in heiligen Stunden, mit k\u00fchnen Ver-", "tokens": ["Da", "wir", "so", "oft", "in", "hei\u00b7li\u00b7gen", "Stun\u00b7den", ",", "mit", "k\u00fch\u00b7nen", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Uns von ihr unterredt. Jetzt sind die Tage gekommen.", "tokens": ["Uns", "von", "ihr", "un\u00b7ter\u00b7redt", ".", "Jetzt", "sind", "die", "Ta\u00b7ge", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVPP", "$.", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Gott steht noch in den Tiefen des Chaos, und winket", "tokens": ["Gott", "steht", "noch", "in", "den", "Tie\u00b7fen", "des", "Chaos", ",", "und", "win\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Aus dem Nichts und der Nacht; er hat auch der Erde", "tokens": ["Aus", "dem", "Nichts", "und", "der", "Nacht", ";", "er", "hat", "auch", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Sie bey ihrem Namen genannt, und mit m\u00e4chtiget", "tokens": ["Sie", "bey", "ih\u00b7rem", "Na\u00b7men", "ge\u00b7nannt", ",", "und", "mit", "m\u00e4ch\u00b7ti\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "KON", "APPR", "NE"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.22": {"text": "Um die stralende Sonne gef\u00fchrt; er gab ihr den Mond", "tokens": ["Um", "die", "stra\u00b7len\u00b7de", "Son\u00b7ne", "ge\u00b7f\u00fchrt", ";", "er", "gab", "ihr", "den", "Mond"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVPP", "$.", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.23": {"text": "Zum getreuen Gef\u00e4hrten der Nacht; der folgt ihr auf-", "tokens": ["Zum", "ge\u00b7treu\u00b7en", "Ge\u00b7f\u00e4hr\u00b7ten", "der", "Nacht", ";", "der", "folgt", "ihr", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$.", "ART", "VVFIN", "PPOSAT", "TRUNC"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Und entzieht ihr sein Angesicht nie. Doch fehlt noch", "tokens": ["Und", "ent\u00b7zieht", "ihr", "sein", "An\u00b7ge\u00b7sicht", "nie", ".", "Doch", "fehlt", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$.", "KON", "VVFIN", "ADV"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.25": {"text": "Was sie am herrlichsten macht, ein Gesch\u00f6pf mit dank-", "tokens": ["Was", "sie", "am", "herr\u00b7lichs\u00b7ten", "macht", ",", "ein", "Ge\u00b7sch\u00f6pf", "mit", "dank"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "ADJA", "VVFIN", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "W\u00fcrdig den Sch\u00f6pfer zu preisen, und zu den jauchzen-", "tokens": ["W\u00fcr\u00b7dig", "den", "Sch\u00f6p\u00b7fer", "zu", "prei\u00b7sen", ",", "und", "zu", "den", "jauch\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "PTKZU", "VVINF", "$,", "KON", "APPR", "ART", "TRUNC"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.27": {"text": "Von unzehligen Welten auch seine Ges\u00e4nge zu f\u00fcgen.", "tokens": ["Von", "un\u00b7zeh\u00b7li\u00b7gen", "Wel\u00b7ten", "auch", "sei\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge", "zu", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Doch Gott wird es erschaffen, so sprach er, er wird", "tokens": ["Doch", "Gott", "wird", "es", "er\u00b7schaf\u00b7fen", ",", "so", "sprach", "er", ",", "er", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.29": {"text": "Herrlich, unsterblich, nach seinem Bilde. Der Mensch,", "tokens": ["Herr\u00b7lich", ",", "uns\u00b7terb\u00b7lich", ",", "nach", "sei\u00b7nem", "Bil\u00b7de", ".", "Der", "Mensch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$.", "ART", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.30": {"text": "K\u00fcnftig ihn unser frohlockendes Chor,) der Mensch wird", "tokens": ["K\u00fcnf\u00b7tig", "ihn", "un\u00b7ser", "froh\u00b7lo\u00b7cken\u00b7des", "Chor", ",", ")", "der", "Mensch", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJD", "PPER", "PPOSAT", "ADJA", "NN", "$,", "$(", "ART", "NN", "VAFIN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.31": {"text": "Seines Sch\u00f6pfers vorz\u00fcglich genie\u00dfen, und seiner Er-", "tokens": ["Sei\u00b7nes", "Sch\u00f6p\u00b7fers", "vor\u00b7z\u00fcg\u00b7lich", "ge\u00b7nie\u00b7\u00dfen", ",", "und", "sei\u00b7ner", "Er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,", "KON", "PPOSAT", "TRUNC"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "Unbegreiflich den Engeln und Himmeln, gew\u00fcrdiget", "tokens": ["Un\u00b7be\u00b7greif\u00b7lich", "den", "En\u00b7geln", "und", "Him\u00b7meln", ",", "ge\u00b7w\u00fcr\u00b7di\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "ART", "NN", "KON", "NN", "$,", "VVFIN"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Diesem erw\u00e4hlten Geschlecht bestimmt des Ewigen", "tokens": ["Die\u00b7sem", "er\u00b7w\u00e4hl\u00b7ten", "Ge\u00b7schlecht", "be\u00b7stimmt", "des", "E\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.34": {"text": "Euch zu F\u00fchrern und W\u00e4chtern. Jhr sollt auf verwor-", "tokens": ["Euch", "zu", "F\u00fch\u00b7rern", "und", "W\u00e4ch\u00b7tern", ".", "Ihr", "sollt", "auf", "ver\u00b7wor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$.", "PPER", "VMFIN", "APPR", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Diese neuen Unsterblichen leiten; sollt ihre Gem\u00fcther", "tokens": ["Die\u00b7se", "neu\u00b7en", "U\u00b7nsterb\u00b7li\u00b7chen", "lei\u00b7ten", ";", "sollt", "ih\u00b7re", "Ge\u00b7m\u00fc\u00b7ther"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVINF", "$.", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+---+-+-+-+-", "measure": "unknown.measure.septa"}, "line.36": {"text": "Vor dem verf\u00fchrenden Laster verwahren, und hohe", "tokens": ["Vor", "dem", "ver\u00b7f\u00fch\u00b7ren\u00b7den", "Las\u00b7ter", "ver\u00b7wah\u00b7ren", ",", "und", "ho\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,", "KON", "ADJA"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "In den Seelen erschaffen, wenn unter den Fesseln des", "tokens": ["In", "den", "See\u00b7len", "er\u00b7schaf\u00b7fen", ",", "wenn", "un\u00b7ter", "den", "Fes\u00b7seln", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KOUS", "APPR", "ART", "NN", "ART"], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.38": {"text": "Unter der wilden Zerstreuung und unter der Eitelkeit", "tokens": ["Un\u00b7ter", "der", "wil\u00b7den", "Zer\u00b7streu\u00b7ung", "und", "un\u00b7ter", "der", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "APPR", "ART", "NN"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.39": {"text": "Jhr vom Himmel stammender Geist zum Laster ver-", "tokens": ["Ihr", "vom", "Him\u00b7mel", "stam\u00b7men\u00b7der", "Geist", "zum", "Las\u00b7ter", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ADJA", "NN", "APPRART", "NN", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.40": {"text": "Wenn dann des Weltgerichts letzte Posaune die Him-", "tokens": ["Wenn", "dann", "des", "Welt\u00b7ge\u00b7richts", "letz\u00b7te", "Po\u00b7sau\u00b7ne", "die", "Him"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN", "ART", "TRUNC"], "meter