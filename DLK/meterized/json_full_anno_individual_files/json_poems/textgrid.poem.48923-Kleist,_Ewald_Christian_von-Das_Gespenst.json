{"textgrid.poem.48923": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gespenst", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sank in Thetis Purpurschoo\u00df;", "tokens": ["Die", "Son\u00b7ne", "sank", "in", "The\u00b7tis", "Pur\u00b7pur\u00b7schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Wolken blitzten vom Rubine,", "tokens": ["Die", "Wol\u00b7ken", "blitz\u00b7ten", "vom", "Ru\u00b7bi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Bach, der wie geschmolzen Silber flo\u00df,", "tokens": ["Ein", "Bach", ",", "der", "wie", "ge\u00b7schmol\u00b7zen", "Sil\u00b7ber", "flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zog mich ins \u00fcberguldte Gr\u00fcne;", "tokens": ["Zog", "mich", "ins", "\u00fc\u00b7berg\u00b7uld\u00b7te", "Gr\u00fc\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich h\u00f6rte dort in stolzer Ruh", "tokens": ["Ich", "h\u00f6r\u00b7te", "dort", "in", "stol\u00b7zer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der B\u00fcsche hellen Kehlen zu.", "tokens": ["Der", "B\u00fc\u00b7sche", "hel\u00b7len", "Keh\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Abendlied der holden Nachtigall", "tokens": ["Das", "A\u00b7ben\u00b7dlied", "der", "hol\u00b7den", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ward durch der Wachtel Schlag begleitet,", "tokens": ["Ward", "durch", "der", "Wach\u00b7tel", "Schlag", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich gemach von diesem Freudenschall", "tokens": ["Und", "ich", "ge\u00b7mach", "von", "die\u00b7sem", "Freu\u00b7den\u00b7schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum s\u00fc\u00dfen Schlummer zubereitet.", "tokens": ["Zum", "s\u00fc\u00b7\u00dfen", "Schlum\u00b7mer", "zu\u00b7be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schlummerte und h\u00f6rte doch,", "tokens": ["Ich", "schlum\u00b7mer\u00b7te", "und", "h\u00f6r\u00b7te", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dreymal schlug die Wachtel noch.", "tokens": ["Und", "drey\u00b7mal", "schlug", "die", "Wach\u00b7tel", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf schlief ich ein. Es schreckte mich ein Traum,", "tokens": ["Drauf", "schlief", "ich", "ein", ".", "Es", "schreck\u00b7te", "mich", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir d\u00e4ucht, ich s\u00e4he durch die Str\u00e4uche", "tokens": ["Mir", "d\u00e4ucht", ",", "ich", "s\u00e4\u00b7he", "durch", "die", "Str\u00e4u\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein wei\u00df Gespenst am braun gewordnen Baum.", "tokens": ["Ein", "wei\u00df", "Ge\u00b7spenst", "am", "braun", "ge\u00b7word\u00b7nen", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Haar erhebt sich; ich entweiche.", "tokens": ["Das", "Haar", "er\u00b7hebt", "sich", ";", "ich", "ent\u00b7wei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Ast, der mich zu Boden rei\u00dft,", "tokens": ["Ein", "Ast", ",", "der", "mich", "zu", "Bo\u00b7den", "rei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erweckt mich, und ich seh den Geist.", "tokens": ["Er\u00b7weckt", "mich", ",", "und", "ich", "seh", "den", "Geist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Blick war wild; ich sprang vom Lager auf:", "tokens": ["Mein", "Blick", "war", "wild", ";", "ich", "sprang", "vom", "La\u00b7ger", "auf", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Allein der Geist rief mir geschwinde:", "tokens": ["Al\u00b7lein", "der", "Geist", "rief", "mir", "ge\u00b7schwin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Seladon! Schatz! hemme deinen Lauf,", "tokens": ["O", "Se\u00b7la\u00b7don", "!", "Schatz", "!", "hem\u00b7me", "dei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wen scheuest du? Ich bin Philinde.", "tokens": ["Wen", "scheu\u00b7est", "du", "?", "Ich", "bin", "Phi\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum hatt ich meinen Blick gewandt:", "tokens": ["Kaum", "hatt", "ich", "mei\u00b7nen", "Blick", "ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So f\u00fchlt ich ihre sanfte Hand.", "tokens": ["So", "f\u00fchlt", "ich", "ih\u00b7re", "sanf\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie, wem die Luft in wilder Fluth entgeht,", "tokens": ["Wie", ",", "wem", "die", "Luft", "in", "wil\u00b7der", "Fluth", "ent\u00b7geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die wallend ihm im Schlunde w\u00fchlet,", "tokens": ["Die", "wal\u00b7lend", "ihm", "im", "Schlun\u00b7de", "w\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn itzt sein Fu\u00df auf festem Grunde steht,", "tokens": ["Wenn", "itzt", "sein", "Fu\u00df", "auf", "fes\u00b7tem", "Grun\u00b7de", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich endlich tr\u00e4ufelnd wieder f\u00fchlet:", "tokens": ["Sich", "end\u00b7lich", "tr\u00e4u\u00b7felnd", "wie\u00b7der", "f\u00fch\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So ward ich meiner selbst bewu\u00dft:", "tokens": ["So", "ward", "ich", "mei\u00b7ner", "selbst", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Zentnerlast fiel von der Brust.", "tokens": ["Die", "Zent\u00b7ner\u00b7last", "fiel", "von", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Ku\u00df, den mir Philinde z\u00e4rtlich gab,", "tokens": ["Ein", "Ku\u00df", ",", "den", "mir", "Phi\u00b7lin\u00b7de", "z\u00e4rt\u00b7lich", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erhitzte bald die starren Glieder.", "tokens": ["Er\u00b7hitz\u00b7te", "bald", "die", "star\u00b7ren", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wischten wir oft Florens Thr\u00e4nen ab:", "tokens": ["Und", "wischten", "wir", "oft", "Flo\u00b7rens", "Thr\u00e4\u00b7nen", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So k\u00fchlte Zephirs Lispeln wieder,", "tokens": ["So", "k\u00fchl\u00b7te", "Ze\u00b7phirs", "Lis\u00b7peln", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das in des Geistes Locken stie\u00df,", "tokens": ["Das", "in", "des", "Geis\u00b7tes", "Lo\u00b7cken", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich so reizend greifen lie\u00df.", "tokens": ["Der", "sich", "so", "rei\u00b7zend", "grei\u00b7fen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Sternenheer! o Mond! o Silberlicht!", "tokens": ["O", "Ster\u00b7nen\u00b7heer", "!", "o", "Mond", "!", "o", "Sil\u00b7ber\u00b7licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "$.", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr hellen Zeugen meiner Freuden,", "tokens": ["Ihr", "hel\u00b7len", "Zeu\u00b7gen", "mei\u00b7ner", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Euch kommen oft Gespenster zu Gesicht,", "tokens": ["Euch", "kom\u00b7men", "oft", "Ge\u00b7spens\u00b7ter", "zu", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchrt sie zu mir; ich mag sie leiden.", "tokens": ["F\u00fchrt", "sie", "zu", "mir", ";", "ich", "mag", "sie", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, m\u00f6cht es heute noch geschehn,", "tokens": ["Ja", ",", "m\u00f6cht", "es", "heu\u00b7te", "noch", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie froh wollt ich sie kommen sehn.", "tokens": ["Wie", "froh", "wollt", "ich", "sie", "kom\u00b7men", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Sonne sank in Thetis Purpurschoo\u00df;", "tokens": ["Die", "Son\u00b7ne", "sank", "in", "The\u00b7tis", "Pur\u00b7pur\u00b7schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Wolken blitzten vom Rubine,", "tokens": ["Die", "Wol\u00b7ken", "blitz\u00b7ten", "vom", "Ru\u00b7bi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Bach, der wie geschmolzen Silber flo\u00df,", "tokens": ["Ein", "Bach", ",", "der", "wie", "ge\u00b7schmol\u00b7zen", "Sil\u00b7ber", "flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure"