{"dta.poem.9974": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kan andern dieses geben/ was mir selbsten doch gebricht/", "tokens": ["Ich", "kan", "an\u00b7dern", "die\u00b7ses", "ge\u00b7ben", "/", "was", "mir", "selbs\u00b7ten", "doch", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PDAT", "VVINF", "$(", "PWS", "PPER", "VVFIN", "ADV", "VVFIN", "$("], "meter": "+++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Alles mu\u00df mir seyn gehorsam/ was beschaut der sonnen licht.", "tokens": ["Al\u00b7les", "mu\u00df", "mir", "seyn", "ge\u00b7hor\u00b7sam", "/", "was", "be\u00b7schaut", "der", "son\u00b7nen", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PPOSAT", "NN", "$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Vor mir f\u00fcrcht sich alle welt/ ich zehl ab die zeit und stunden/", "tokens": ["Vor", "mir", "f\u00fcrcht", "sich", "al\u00b7le", "welt", "/", "ich", "zehl", "ab", "die", "zeit", "und", "stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "PIAT", "NN", "$(", "PPER", "ADV", "APPR", "ART", "NN", "KON", "ADJA", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Aller orten werd ich sonst/ nur im himmel nicht/ gefunden.", "tokens": ["Al\u00b7ler", "or\u00b7ten", "werd", "ich", "sonst", "/", "nur", "im", "him\u00b7mel", "nicht", "/", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADV", "$(", "ADV", "APPRART", "NN", "PTKNEG", "$(", "VVPP", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}