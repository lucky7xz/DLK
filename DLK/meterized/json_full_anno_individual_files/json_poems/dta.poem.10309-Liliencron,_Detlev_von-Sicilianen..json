{"dta.poem.10309": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Sicilianen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Drei gr&#252;ne Fleckchen hab' ich doch gefunden             ", "tokens": ["Drei", "gr", "&#252;", "ne", "Fleck\u00b7chen", "hab'", "ich", "doch", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "XY", "$(", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im d\u00fcrren Lebenssand, mich gern zu recken:", "tokens": ["Im", "d\u00fcr\u00b7ren", "Le\u00b7bens\u00b7sand", ",", "mich", "gern", "zu", "re\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf nassem Hengst in Qualm und Tod und Wunden", "tokens": ["Auf", "nas\u00b7sem", "Hengst", "in", "Qualm", "und", "Tod", "und", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Feindes Skalp am Sattel festzustecken;", "tokens": ["Des", "Fein\u00b7des", "Skalp", "am", "Sat\u00b7tel", "fest\u00b7zu\u00b7ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Behaglich nach der Jagd mich mit den Hunden", "tokens": ["Be\u00b7hag\u00b7lich", "nach", "der", "Jagd", "mich", "mit", "den", "Hun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Fr\u00fchst\u00fcck unterm Heidbusch auszustrecken;", "tokens": ["Zum", "Fr\u00fch\u00b7st\u00fcck", "un\u00b7term", "Heid\u00b7busch", "aus\u00b7zu\u00b7stre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Geheim mit meinem M\u00e4dchen kurze Stunden", "tokens": ["Ge\u00b7heim", "mit", "mei\u00b7nem", "M\u00e4d\u00b7chen", "kur\u00b7ze", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der s\u00fc\u00dfen S\u00fcnde Abgrund zu entdecken.", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "S\u00fcn\u00b7de", "Ab\u00b7grund", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du hast ein fl\u00fcchtig Gl\u00fcck.", "tokens": ["Du", "hast", "ein", "fl\u00fcch\u00b7tig", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verrat\u2019 es nicht und zeig\u2019 es keiner Seele.", "tokens": ["Ver\u00b7rat'", "es", "nicht", "und", "zeig'", "es", "kei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Neid, ein arger Dieb, hat scharfe Brillen,", "tokens": ["Der", "Neid", ",", "ein", "ar\u00b7ger", "Dieb", ",", "hat", "schar\u00b7fe", "Bril\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wei\u00df, es ist die kostbarste Juwele,", "tokens": ["Er", "wei\u00df", ",", "es", "ist", "die", "kost\u00b7bars\u00b7te", "Ju\u00b7we\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wird nicht eher seinen Hunger stillen,", "tokens": ["Und", "wird", "nicht", "e\u00b7her", "sei\u00b7nen", "Hun\u00b7ger", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bis er\u2019s geraubt dir hat mit hei\u00dfer Kehle.", "tokens": ["Bis", "er's", "ge\u00b7raubt", "dir", "hat", "mit", "hei\u00b7\u00dfer", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sag\u2019, wenn du willst, es brennten die Antillen,", "tokens": ["Sag'", ",", "wenn", "du", "willst", ",", "es", "brenn\u00b7ten", "die", "An\u00b7til\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du rittest hin auf einsamem Kamele.", "tokens": ["Du", "rit\u00b7test", "hin", "auf", "ein\u00b7sa\u00b7mem", "Ka\u00b7me\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}