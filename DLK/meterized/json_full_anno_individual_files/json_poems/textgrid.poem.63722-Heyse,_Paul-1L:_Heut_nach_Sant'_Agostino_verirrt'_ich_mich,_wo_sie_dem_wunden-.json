{"textgrid.poem.63722": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heut nach Sant' Agostino verirrt' ich mich, wo sie dem wunden-", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut nach Sant' Agostino verirrt' ich mich, wo sie dem wunden-", "tokens": ["Heut", "nach", "Sant'", "A\u00b7gos\u00b7ti\u00b7no", "ver\u00b7irrt'", "ich", "mich", ",", "wo", "sie", "dem", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NE", "VVFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "T\u00e4t'gen Madonnenbild k\u00fcssen den marmornen Fu\u00df.", "tokens": ["T\u00e4t'\u00b7gen", "Ma\u00b7don\u00b7nen\u00b7bild", "k\u00fcs\u00b7sen", "den", "mar\u00b7mor\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Und da lie\u00df mich das Gl\u00fcck der Wunder eines erleben,", "tokens": ["Und", "da", "lie\u00df", "mich", "das", "Gl\u00fcck", "der", "Wun\u00b7der", "ei\u00b7nes", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie sie an Fleisch und Blut wirkt der verg\u00f6tterte Stein.", "tokens": ["Wie", "sie", "an", "Fleisch", "und", "Blut", "wirkt", "der", "ver\u00b7g\u00f6t\u00b7ter\u00b7te", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Wenige Fraun und M\u00e4dchen \u2013 es l\u00e4utete grade zur Vesper \u2013", "tokens": ["We\u00b7ni\u00b7ge", "Fraun", "und", "M\u00e4d\u00b7chen", "\u2013", "es", "l\u00e4u\u00b7te\u00b7te", "gra\u00b7de", "zur", "Ves\u00b7per", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Knieeten dort im Gebet, z\u00fcchtig die Augen gesenkt,", "tokens": ["Knie\u00b7e\u00b7ten", "dort", "im", "Ge\u00b7bet", ",", "z\u00fcch\u00b7tig", "die", "Au\u00b7gen", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "T\u00fccher ums Haupt, darunter die silberne Nadel hervorsah", "tokens": ["T\u00fc\u00b7cher", "ums", "Haupt", ",", "da\u00b7run\u00b7ter", "die", "sil\u00b7ber\u00b7ne", "Na\u00b7del", "her\u00b7vor\u00b7sah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.8": {"text": "Oder der blinkende Reif an dem gebogenen Kamm.", "tokens": ["O\u00b7der", "der", "blin\u00b7ken\u00b7de", "Reif", "an", "dem", "ge\u00b7bo\u00b7ge\u00b7nen", "Kamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Nur ein finsterer Bursch stand fern am Pfeiler. Er schien nicht", "tokens": ["Nur", "ein", "fins\u00b7te\u00b7rer", "Bursch", "stand", "fern", "am", "Pfei\u00b7ler", ".", "Er", "schien", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$.", "PPER", "VVFIN", "PTKNEG"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Betens halber und nicht gl\u00e4ubigen Herzens genaht.", "tokens": ["Be\u00b7tens", "hal\u00b7ber", "und", "nicht", "gl\u00e4u\u00b7bi\u00b7gen", "Her\u00b7zens", "ge\u00b7naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPO", "KON", "PTKNEG", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Fest hinstarrten die Augen auf eins der knieenden M\u00e4gdlein,", "tokens": ["Fest", "hins\u00b7tarr\u00b7ten", "die", "Au\u00b7gen", "auf", "eins", "der", "kni\u00b7e\u00b7en\u00b7den", "M\u00e4gd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Und es gl\u00fcht' ihm das Herz bis zu den Wangen hinauf.", "tokens": ["Und", "es", "gl\u00fcht'", "ihm", "das", "Herz", "bis", "zu", "den", "Wan\u00b7gen", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Doch sie achtet' es nicht, sie lie\u00df nicht unter dem Schleier", "tokens": ["Doch", "sie", "ach\u00b7tet'", "es", "nicht", ",", "sie", "lie\u00df", "nicht", "un\u00b7ter", "dem", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Nach dem Pfeiler zu ihm wandern verstohlenen Blick.", "tokens": ["Nach", "dem", "Pfei\u00b7ler", "zu", "ihm", "wan\u00b7dern", "ver\u00b7stoh\u00b7le\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Freilich, der Bursch war d\u00fcrftig und unansehnlich; sie selber", "tokens": ["Frei\u00b7lich", ",", "der", "Bursch", "war", "d\u00fcrf\u00b7tig", "und", "un\u00b7an\u00b7sehn\u00b7lich", ";", "sie", "sel\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "PPER", "ADV"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Trug in dem Sch\u00f6nheitskampf sicher die Palme davon.", "tokens": ["Trug", "in", "dem", "Sch\u00f6n\u00b7heits\u00b7kampf", "si\u00b7cher", "die", "Pal\u00b7me", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Nun vom Knieen erhob sich eins ums andere. Sittsam", "tokens": ["Nun", "vom", "Kni\u00b7e\u00b7en", "er\u00b7hob", "sich", "eins", "ums", "an\u00b7de\u00b7re", ".", "Sitt\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PRF", "PIS", "APPRART", "ADJA", "$.", "NN"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Trat zu der Jungfrau Bild jedes der M\u00e4dchen heran,", "tokens": ["Trat", "zu", "der", "Jung\u00b7frau", "Bild", "je\u00b7des", "der", "M\u00e4d\u00b7chen", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PIAT", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Heftete Lippen und Stirn und wieder die Lippen in Andacht", "tokens": ["Hef\u00b7te\u00b7te", "Lip\u00b7pen", "und", "Stirn", "und", "wie\u00b7der", "die", "Lip\u00b7pen", "in", "An\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "KON", "ADV", "ART", "NN", "APPR", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Gegen den Marmorfu\u00df, kreuzte sich, knickst' und verschwand.", "tokens": ["Ge\u00b7gen", "den", "Mar\u00b7mor\u00b7fu\u00df", ",", "kreuz\u00b7te", "sich", ",", "knickst'", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "PRF", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.21": {"text": "Immer noch starrte der Bursch. Da kam die Sch\u00f6nste gegangen,", "tokens": ["Im\u00b7mer", "noch", "starr\u00b7te", "der", "Bursch", ".", "Da", "kam", "die", "Sch\u00f6ns\u00b7te", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Knickst' und k\u00fc\u00dfte den Stein. Jetzt in gewaltiger Hast,", "tokens": ["Knickst'", "und", "k\u00fc\u00df\u00b7te", "den", "Stein", ".", "Jetzt", "in", "ge\u00b7wal\u00b7ti\u00b7ger", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Gleich als lief' er Gefahr, sein ewiges Heil zu vers\u00e4umen,", "tokens": ["Gleich", "als", "lie\u00b7f'", "er", "Ge\u00b7fahr", ",", "sein", "e\u00b7wi\u00b7ges", "Heil", "zu", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "NN", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-++--+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Wild wie ein rei\u00dfender Wolf zwischen die Schafe sich st\u00fcrzt,", "tokens": ["Wild", "wie", "ein", "rei\u00b7\u00dfen\u00b7der", "Wolf", "zwi\u00b7schen", "die", "Scha\u00b7fe", "sich", "st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NE", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Dr\u00e4ngt' er die Weiber zur\u00fcck und k\u00fc\u00dfte die n\u00e4mliche Stelle,", "tokens": ["Dr\u00e4ngt'", "er", "die", "Wei\u00b7ber", "zu\u00b7r\u00fcck", "und", "k\u00fc\u00df\u00b7te", "die", "n\u00e4m\u00b7li\u00b7che", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Und des Kicherns umher achtet' der Feurige nicht.", "tokens": ["Und", "des", "Ki\u00b7cherns", "um\u00b7her", "ach\u00b7tet'", "der", "Feu\u00b7ri\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "--++-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.27": {"text": "Denn er sah nur die Eine, die purpurgl\u00fchend ihn anstaunt',", "tokens": ["Denn", "er", "sah", "nur", "die", "Ei\u00b7ne", ",", "die", "pur\u00b7pur\u00b7gl\u00fc\u00b7hend", "ihn", "an\u00b7staunt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Und, o Wunder! er schien pl\u00f6tzlich verwandelt, der Wuchs", "tokens": ["Und", ",", "o", "Wun\u00b7der", "!", "er", "schien", "pl\u00f6tz\u00b7lich", "ver\u00b7wan\u00b7delt", ",", "der", "Wuchs"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "ADJD", "VVPP", "$,", "ART", "NN"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "H\u00f6her und stolzer der Blick. Du aber schautest mit L\u00e4cheln", "tokens": ["H\u00f6\u00b7her", "und", "stol\u00b7zer", "der", "Blick", ".", "Du", "a\u00b7ber", "schau\u00b7test", "mit", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "ART", "NN", "$.", "PPER", "ADV", "VVFIN", "APPR", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Auf dein liebliches Werk, Mutter der Gnaden, herab!", "tokens": ["Auf", "dein", "lieb\u00b7li\u00b7ches", "Werk", ",", "Mut\u00b7ter", "der", "Gna\u00b7den", ",", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Heut nach Sant' Agostino verirrt' ich mich, wo sie dem wunden-", "tokens": ["Heut", "nach", "Sant'", "A\u00b7gos\u00b7ti\u00b7no", "ver\u00b7irrt'", "ich", "mich", ",", "wo", "sie", "dem", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NE", "VVFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "T\u00e4t'gen Madonnenbild k\u00fcssen den marmornen Fu\u00df.", "tokens": ["T\u00e4t'\u00b7gen", "Ma\u00b7don\u00b7nen\u00b7bild", "k\u00fcs\u00b7sen", "den", "mar\u00b7mor\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Und da lie\u00df mich das Gl\u00fcck der Wunder eines erleben,", "tokens": ["Und", "da", "lie\u00df", "mich", "das", "Gl\u00fcck", "der", "Wun\u00b7der", "ei\u00b7nes", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie sie an Fleisch und Blut wirkt der verg\u00f6tterte Stein.", "tokens": ["Wie", "sie", "an", "Fleisch", "und", "Blut", "wirkt", "der", "ver\u00b7g\u00f6t\u00b7ter\u00b7te", "Stein