{"dta.poem.8482": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Carpe diem?", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Pfl&#252;cke die Stunde, w&#228;r' sie noch so bla\u00df,", "tokens": ["Pfl", "&#252;", "cke", "die", "Stun\u00b7de", ",", "w", "&#228;", "r'", "sie", "noch", "so", "bla\u00df", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "punct", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "ART", "NN", "$,", "NE", "$(", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ein falbes Moos, vom Dunst des Moores na\u00df,", "tokens": ["Ein", "fal\u00b7bes", "Moos", ",", "vom", "Dunst", "des", "Moo\u00b7res", "na\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein farblos Bl\u00fcmchen, flatternd auf der Haide;", "tokens": ["Ein", "farb\u00b7los", "Bl\u00fcm\u00b7chen", ",", "flat\u00b7ternd", "auf", "der", "Hai\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, einst von Allem tr\u00e4umt die Seele s\u00fc\u00df,", "tokens": ["Ach", ",", "einst", "von", "Al\u00b7lem", "tr\u00e4umt", "die", "See\u00b7le", "s\u00fc\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "PIS", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Allem, was, ihr eigen, sie verlie\u00df,", "tokens": ["Von", "Al\u00b7lem", ",", "was", ",", "ihr", "ei\u00b7gen", ",", "sie", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "$,", "PPER", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und mancher Seufzer gilt entflohnem Leide.", "tokens": ["Und", "man\u00b7cher", "Seuf\u00b7zer", "gilt", "ent\u00b7floh\u00b7nem", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In Alles senkt sie Blutestropfen ein,", "tokens": ["In", "Al\u00b7les", "senkt", "sie", "Blu\u00b7tes\u00b7trop\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Legt Perlen aus dem heiligtiefsten Schrein,", "tokens": ["Legt", "Per\u00b7len", "aus", "dem", "hei\u00b7lig\u00b7tiefs\u00b7ten", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bewu\u00dftlos, selbst in grauverh\u00e4ngte Stunden;", "tokens": ["Be\u00b7wu\u00dft\u00b7los", ",", "selbst", "in", "grau\u00b7ver\u00b7h\u00e4ng\u00b7te", "Stun\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steigt oft ein unklar Sehnen dir empor,", "tokens": ["Steigt", "oft", "ein", "un\u00b7klar", "Seh\u00b7nen", "dir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du schaust vielleicht wie durch Gew\u00f6lkes Flor", "tokens": ["Du", "schaust", "viel\u00b7leicht", "wie", "durch", "Ge\u00b7w\u00f6l\u00b7kes", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach Tagen, l\u00e4ngst vergessen, doch empfunden.", "tokens": ["Nach", "Ta\u00b7gen", ",", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", ",", "doch", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer, der an seine Kinderzeit gedenkt,", "tokens": ["Wer", ",", "der", "an", "sei\u00b7ne", "Kin\u00b7der\u00b7zeit", "ge\u00b7denkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als die Vokabeln ihn in Noth versenkt,", "tokens": ["Als", "die", "Vo\u00b7ka\u00b7beln", "ihn", "in", "Noth", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer m\u00f6cht\u2019 nicht wieder Kind sein und sich grauen?", "tokens": ["Wer", "m\u00f6cht'", "nicht", "wie\u00b7der", "Kind", "sein", "und", "sich", "grau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ADV", "NN", "VAINF", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja, der Gefangne, der die Wand beschrieb,", "tokens": ["Ja", ",", "der", "Ge\u00b7fang\u00b7ne", ",", "der", "die", "Wand", "be\u00b7schrieb", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fchlt er nach Jahren Gl\u00fcckes nicht den Trieb,", "tokens": ["F\u00fchlt", "er", "nach", "Jah\u00b7ren", "Gl\u00fc\u00b7ckes", "nicht", "den", "Trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die alten Spr\u00fcche einmal noch zu schauen?", "tokens": ["Die", "al\u00b7ten", "Spr\u00fc\u00b7che", "ein\u00b7mal", "noch", "zu", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl giebt es Stunden, die so ganz verha\u00dft,", "tokens": ["Wohl", "giebt", "es", "Stun\u00b7den", ",", "die", "so", "ganz", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, dem Ged\u00e4chtni\u00df eine Centnerlast,", "tokens": ["Da\u00df", ",", "dem", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ei\u00b7ne", "Cent\u00b7ner\u00b7last", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir ihren Schatten abzuw\u00e4lzen sorgen;", "tokens": ["Wir", "ih\u00b7ren", "Schat\u00b7ten", "ab\u00b7zu\u00b7w\u00e4l\u00b7zen", "sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch selten schickt sie uns des Himmels Zorn,", "tokens": ["Doch", "sel\u00b7ten", "schickt", "sie", "uns", "des", "Him\u00b7mels", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und meistens ist darin ein gift\u2019ger Dorn,", "tokens": ["Und", "meis\u00b7tens", "ist", "da\u00b7rin", "ein", "gift'\u00b7ger", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Moderwurm geheimer Schuld verborgen.", "tokens": ["Der", "Mo\u00b7der\u00b7wurm", "ge\u00b7hei\u00b7mer", "Schuld", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Drum, wer noch eines Blicks nach oben werth,", "tokens": ["Drum", ",", "wer", "noch", "ei\u00b7nes", "Blicks", "nach", "o\u00b7ben", "werth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWS", "ADV", "ART", "NN", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der nehme, was an Lieben ihm bescheert,", "tokens": ["Der", "neh\u00b7me", ",", "was", "an", "Lie\u00b7ben", "ihm", "be\u00b7scheert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "APPR", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die stolze, wie die Stund\u2019 im schlichten Kleide", "tokens": ["Die", "stol\u00b7ze", ",", "wie", "die", "Stund'", "im", "schlich\u00b7ten", "Klei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der schl\u00fcrfe jeden stillen Tropfen Thau,", "tokens": ["Der", "schl\u00fcr\u00b7fe", "je\u00b7den", "stil\u00b7len", "Trop\u00b7fen", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und spiegelt drin sich nicht des Aethers Blau,", "tokens": ["Und", "spie\u00b7gelt", "drin", "sich", "nicht", "des", "A\u00b7e\u00b7thers", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "So lispelt dr\u00fcber wohl die fromme Weide.", "tokens": ["So", "lis\u00b7pelt", "dr\u00fc\u00b7ber", "wohl", "die", "from\u00b7me", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Freu\u2019 dich an deines S\u00e4uglings L\u00e4cheln, freu\u2019", "tokens": ["Freu'", "dich", "an", "dei\u00b7nes", "S\u00e4ug\u00b7lings", "L\u00e4\u00b7cheln", ",", "freu'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich an des Jauchzens ungewissem Schrei,", "tokens": ["Dich", "an", "des", "Jauch\u00b7zens", "un\u00b7ge\u00b7wis\u00b7sem", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit dem er streckt die lustbewegten Glieder;", "tokens": ["Mit", "dem", "er", "streckt", "die", "lust\u00b7be\u00b7weg\u00b7ten", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "W\u00e4r zehnmal stolzer auch, was dich durchweht,", "tokens": ["W\u00e4r", "zehn\u00b7mal", "stol\u00b7zer", "auch", ",", "was", "dich", "durch\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn er vor dir dereinst, ein J\u00fcngling, steht,", "tokens": ["Wenn", "er", "vor", "dir", "de\u00b7reinst", ",", "ein", "J\u00fcng\u00b7ling", ",", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein l\u00e4chelnd Kindlein gibt er dir nicht wieder.", "tokens": ["Dein", "l\u00e4\u00b7chelnd", "Kin\u00b7dlein", "gibt", "er", "dir", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Freu\u2019 dich des Freundes, eh zum Greis er reift,", "tokens": ["Freu'", "dich", "des", "Freun\u00b7des", ",", "eh", "zum", "Greis", "er", "reift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erfahrung ihm die k\u00fchne Stirn gestreift,", "tokens": ["Er\u00b7fah\u00b7rung", "ihm", "die", "k\u00fch\u00b7ne", "Stirn", "ge\u00b7streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von seinem Scheitel Grabesblumen wehen;", "tokens": ["Von", "sei\u00b7nem", "Schei\u00b7tel", "Gra\u00b7bes\u00b7blu\u00b7men", "we\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Freu\u2019 dich des Greises, schau\u2019 ihm lange nach,", "tokens": ["Freu'", "dich", "des", "Grei\u00b7ses", ",", "schau'", "ihm", "lan\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Kurzem g\u00e4bst vielleicht du manchen Tag,", "tokens": ["In", "Kur\u00b7zem", "g\u00e4bst", "viel\u00b7leicht", "du", "man\u00b7chen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um einmal noch dies graue Haupt zu sehen.", "tokens": ["Um", "ein\u00b7mal", "noch", "dies", "grau\u00b7e", "Haupt", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "PDS", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O wer nur ernst und fest die Stund\u2019 ergreift,", "tokens": ["O", "wer", "nur", "ernst", "und", "fest", "die", "Stund'", "er\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Kranz ihr auch von bleichen Locken streift,", "tokens": ["Den", "Kranz", "ihr", "auch", "von", "blei\u00b7chen", "Lo\u00b7cken", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem spendet willig sie die reichste Beute;", "tokens": ["Dem", "spen\u00b7det", "wil\u00b7lig", "sie", "die", "reichs\u00b7te", "Beu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wir, wir Thoren dr\u00e4ngen sie zur\u00fcck,", "tokens": ["Doch", "wir", ",", "wir", "Tho\u00b7ren", "dr\u00e4n\u00b7gen", "sie", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word"