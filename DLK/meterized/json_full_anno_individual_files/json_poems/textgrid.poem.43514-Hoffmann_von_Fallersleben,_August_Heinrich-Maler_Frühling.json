{"textgrid.poem.43514": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Maler Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling ist ein Maler,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ein", "Ma\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er malet Alles an,", "tokens": ["Er", "ma\u00b7let", "Al\u00b7les", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Berge mit den W\u00e4ldern,", "tokens": ["Die", "Ber\u00b7ge", "mit", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Th\u00e4ler mit den Feldern:", "tokens": ["Die", "Th\u00e4\u00b7ler", "mit", "den", "Fel\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was der doch malen kann!", "tokens": ["Was", "der", "doch", "ma\u00b7len", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch meine lieben Blumen", "tokens": ["Auch", "mei\u00b7ne", "lie\u00b7ben", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schm\u00fcckt er mit Farbenpracht:", "tokens": ["Schm\u00fcckt", "er", "mit", "Far\u00b7ben\u00b7pracht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wie sie so herrlich strahlen!", "tokens": ["Wie", "sie", "so", "herr\u00b7lich", "strah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sch\u00f6n kann Keiner malen,", "tokens": ["So", "sch\u00f6n", "kann", "Kei\u00b7ner", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So sch\u00f6n, wie er es macht.", "tokens": ["So", "sch\u00f6n", ",", "wie", "er", "es", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O k\u00f6nnt' ich doch so malen,", "tokens": ["O", "k\u00f6nnt'", "ich", "doch", "so", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich malt' ihm einen Strau\u00df", "tokens": ["Ich", "malt'", "ihm", "ei\u00b7nen", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spr\u00e4ch' in frohem Muthe", "tokens": ["Und", "spr\u00e4ch'", "in", "fro\u00b7hem", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr alles Lieb' und Gute", "tokens": ["F\u00fcr", "al\u00b7les", "Lieb'", "und", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So meinen Dank ihm aus!", "tokens": ["So", "mei\u00b7nen", "Dank", "ihm", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Fr\u00fchling ist ein Maler,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ein", "Ma\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er malet Alles an,", "tokens": ["Er", "ma\u00b7let", "Al\u00b7les", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Berge mit den W\u00e4ldern,", "tokens": ["Die", "Ber\u00b7ge", "mit", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Th\u00e4ler mit den Feldern:", "tokens": ["Die", "Th\u00e4\u00b7ler", "mit", "den", "Fel\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was der doch malen kann!", "tokens": ["Was", "der", "doch", "ma\u00b7len", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": 