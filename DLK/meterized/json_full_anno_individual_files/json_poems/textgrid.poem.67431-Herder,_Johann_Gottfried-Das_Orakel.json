{"textgrid.poem.67431": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Orakel", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Alexander einst zu Ammon's Sitze gelangt war", "tokens": ["Als", "A\u00b7lex\u00b7an\u00b7der", "einst", "zu", "Am\u00b7mon's", "Sit\u00b7ze", "ge\u00b7langt", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "APPR", "NE", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ihn Jupiter selbst nannte den g\u00f6ttlichen Sohn,", "tokens": ["Und", "ihn", "Ju\u00b7pi\u00b7ter", "selbst", "nann\u00b7te", "den", "g\u00f6tt\u00b7li\u00b7chen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fragt' er den Vater um nichts als um die Quelle des Nilstroms,", "tokens": ["Fragt'", "er", "den", "Va\u00b7ter", "um", "nichts", "als", "um", "die", "Quel\u00b7le", "des", "Nil\u00b7stroms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PIS", "KOKOM", "APPR", "ART", "NN", "ART", "NE", "$,"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "F\u00fchlete Schicksal und Gl\u00fcck ruhen in eigener Hand.", "tokens": ["F\u00fch\u00b7le\u00b7te", "Schick\u00b7sal", "und", "Gl\u00fcck", "ru\u00b7hen", "in", "ei\u00b7ge\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Auch wir wollen die G\u00f6tter nur um Geheimnisse fragen;", "tokens": ["Auch", "wir", "wol\u00b7len", "die", "G\u00f6t\u00b7ter", "nur", "um", "Ge\u00b7heim\u00b7nis\u00b7se", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Pflicht und Tugend und Gl\u00fcck schrieben sie uns in das Herz.", "tokens": ["Pflicht", "und", "Tu\u00b7gend", "und", "Gl\u00fcck", "schrie\u00b7ben", "sie", "uns", "in", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Als Alexander einst zu Ammon's Sitze gelangt war", "tokens": ["Als", "A\u00b7lex\u00b7an\u00b7der", "einst", "zu", "Am\u00b7mon's", "Sit\u00b7ze", "ge\u00b7langt", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "APPR", "NE", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ihn Jupiter selbst nannte den g\u00f6ttlichen Sohn,", "tokens": ["Und", "ihn", "Ju\u00b7pi\u00b7ter", "selbst", "nann\u00b7te", "den", "g\u00f6tt\u00b7li\u00b7chen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fragt' er den Vater um nichts als um die Quelle des Nilstroms,", "tokens": ["Fragt'", "er", "den", "Va\u00b7ter", "um", "nichts", "al