{"textgrid.poem.33981": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gr\u00fc\u00dft dich wohl ein Augenblick,", "tokens": ["Es", "gr\u00fc\u00dft", "dich", "wohl", "ein", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist so \u00fcberschwellend voll,", "tokens": ["Der", "ist", "so", "\u00fc\u00b7bersc\u00b7hwel\u00b7lend", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob er dich mit sel'gem Gl\u00fcck", "tokens": ["Als", "ob", "er", "dich", "mit", "sel'\u00b7gem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alle Zukunft tr\u00e4nken soll.", "tokens": ["F\u00fcr", "al\u00b7le", "Zu\u00b7kunft", "tr\u00e4n\u00b7ken", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du aber wehrst, eh' du's vermeinst,", "tokens": ["Du", "a\u00b7ber", "wehrst", ",", "eh'", "du's", "ver\u00b7meinst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn scheu und zitternd selber ab,", "tokens": ["I