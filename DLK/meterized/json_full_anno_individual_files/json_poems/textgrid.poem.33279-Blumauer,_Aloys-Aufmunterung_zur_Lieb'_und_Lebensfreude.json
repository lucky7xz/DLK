{"textgrid.poem.33279": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Aufmunterung zur Lieb' und Lebensfreude", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine bange Sorge, liebes M\u00e4dchen,", "tokens": ["Kei\u00b7ne", "ban\u00b7ge", "Sor\u00b7ge", ",", "lie\u00b7bes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kr\u00e4nke dein mich liebend Herz,", "tokens": ["Kr\u00e4n\u00b7ke", "dein", "mich", "lie\u00b7bend", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "PRF", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur am sanften, bunten Freudenf\u00e4d'chen", "tokens": ["Nur", "am", "sanf\u00b7ten", ",", "bun\u00b7ten", "Freu\u00b7den\u00b7f\u00e4d'\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "G\u00e4ngle dich der Liebe Scherz!", "tokens": ["G\u00e4ng\u00b7le", "dich", "der", "Lie\u00b7be", "Scherz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Zephyrl\u00fcftchen, sanft und leise,", "tokens": ["Wie", "ein", "Ze\u00b7phyr\u00b7l\u00fcft\u00b7chen", ",", "sanft", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weh' der Liebe Hauch aus dir;", "tokens": ["Weh'", "der", "Lie\u00b7be", "Hauch", "aus", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerchensang, nicht Nachtigallenweise,", "tokens": ["Ler\u00b7chen\u00b7sang", ",", "nicht", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "T\u00f6n' aus deiner Kehle mir!", "tokens": ["T\u00f6n'", "aus", "dei\u00b7ner", "Keh\u00b7le", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur mit leichtem, stillen Wonnebeben,", "tokens": ["Nur", "mit", "leich\u00b7tem", ",", "stil\u00b7len", "Won\u00b7ne\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Poche sanft dein Herz mir zu,", "tokens": ["Po\u00b7che", "sanft", "dein", "Herz", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Liebe Lustgef\u00fchle heben", "tokens": ["Nur", "der", "Lie\u00b7be", "Lust\u00b7ge\u00b7f\u00fch\u00b7le", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deinen Busen aus der Ruh!", "tokens": ["Dei\u00b7nen", "Bu\u00b7sen", "aus", "der", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus dem sanften Zauberauge blinke", "tokens": ["Aus", "dem", "sanf\u00b7ten", "Zau\u00b7be\u00b7rau\u00b7ge", "blin\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir die Lust der Liebe nur,", "tokens": ["Mir", "die", "Lust", "der", "Lie\u00b7be", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn d'raus ich deine Thr\u00e4nen trinke,", "tokens": ["Und", "wenn", "d'raus", "ich", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "trin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sey'n es Freudenthr\u00e4nen nur.", "tokens": ["Sey'n", "es", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deiner Tag- und Nachtgedanken Reihe", "tokens": ["Dei\u00b7ner", "Tag", "und", "Nacht\u00b7ge\u00b7dan\u00b7ken", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sei ein Rosenkettchen dir;", "tokens": ["Sei", "ein", "Ro\u00b7sen\u00b7kett\u00b7chen", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wachend oder tr\u00e4umend, immer freue,", "tokens": ["Wa\u00b7chend", "o\u00b7der", "tr\u00e4u\u00b7mend", ",", "im\u00b7mer", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Freue, M\u00e4dchen, dich mit mir!", "tokens": ["Freu\u00b7e", ",", "M\u00e4d\u00b7chen", ",", "dich", "mit", "mir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jede deiner Morgenstunden gl\u00e4nze", "tokens": ["Je\u00b7de", "dei\u00b7ner", "Mor\u00b7gen\u00b7stun\u00b7den", "gl\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rosig, wie dein Angesicht,", "tokens": ["Ro\u00b7sig", ",", "wie", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hehr und heiter sei des Tages Gr\u00e4nze,", "tokens": ["Hehr", "und", "hei\u00b7ter", "sei", "des", "Ta\u00b7ges", "Gr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie dein reines Angesicht.", "tokens": ["Wie", "dein", "rei\u00b7nes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und auf jedem deiner Tritte spriesse", "tokens": ["Und", "auf", "je\u00b7dem", "dei\u00b7ner", "Trit\u00b7te", "spries\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dir ein Freudenbl\u00fcmchen auf,", "tokens": ["Dir", "ein", "Freu\u00b7den\u00b7bl\u00fcm\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du, liebes holdes M\u00e4dchen, giesse", "tokens": ["Und", "du", ",", "lie\u00b7bes", "hol\u00b7des", "M\u00e4d\u00b7chen", ",", "gies\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ADJA", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nur des Dankes Thr\u00e4nen d'rauf.", "tokens": ["Nur", "des", "Dan\u00b7kes", "Thr\u00e4\u00b7nen", "d'\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Von den B\u00e4umen, Wiesen, Blumen, Fl\u00fcssen", "tokens": ["Von", "den", "B\u00e4u\u00b7men", ",", "Wie\u00b7sen", ",", "Blu\u00b7men", ",", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4chle dir Vergn\u00fcgen zu,", "tokens": ["L\u00e4ch\u00b7le", "dir", "Ver\u00b7gn\u00fc\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Wonnebecher der Natur, den s\u00fcssen", "tokens": ["Und", "den", "Won\u00b7ne\u00b7be\u00b7cher", "der", "Na\u00b7tur", ",", "den", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wonnebecher, leere du.", "tokens": ["Won\u00b7ne\u00b7be\u00b7cher", ",", "lee\u00b7re", "du", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wandle in der Sonne hellem Auge", "tokens": ["Wand\u00b7le", "in", "der", "Son\u00b7ne", "hel\u00b7lem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit verkl\u00e4rtem Angesicht,", "tokens": ["Mit", "ver\u00b7kl\u00e4r\u00b7tem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in stiller Abendd\u00e4mm'rung sauge", "tokens": ["Und", "in", "stil\u00b7ler", "A\u00b7bend\u00b7d\u00e4m\u00b7m'\u00b7rung", "sau\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wonne nur aus Lunens Licht.", "tokens": ["Won\u00b7ne", "nur", "aus", "Lu\u00b7nens", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Selten, M\u00e4dchen, girre mit dem T\u00e4ubchen,", "tokens": ["Sel\u00b7ten", ",", "M\u00e4d\u00b7chen", ",", "gir\u00b7re", "mit", "dem", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Klage mit der Nachtigall:", "tokens": ["Kla\u00b7ge", "mit", "der", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du hast ja, liebes Herzensweibchen,", "tokens": ["Denn", "du", "hast", "ja", ",", "lie\u00b7bes", "Her\u00b7zens\u00b7weib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich und deine Lieben all'.", "tokens": ["Mich", "und", "dei\u00b7ne", "Lie\u00b7ben", "all'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "ADJA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Diesen Kranz von Lebensfreuden winde", "tokens": ["Die\u00b7sen", "Kranz", "von", "Le\u00b7bens\u00b7freu\u00b7den", "win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stets dir Herz und Phantasie!", "tokens": ["Stets", "dir", "Herz", "und", "Phan\u00b7ta\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leiden \u2013 unser Wiegenangebinde \u2013", "tokens": ["Lei\u00b7den", "\u2013", "un\u00b7ser", "Wie\u00b7ge\u00b7nan\u00b7ge\u00b7bin\u00b7de", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Trage, Liebchen, such' es nie!", "tokens": ["Tra\u00b7ge", ",", "Lieb\u00b7chen", ",", "such'", "es", "nie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Keine bange Sorge, liebes M\u00e4dchen,", "tokens": ["Kei\u00b7ne", "ban\u00b7ge", "Sor\u00b7ge", ",", "lie\u00b7bes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kr\u00e4nke dein mich liebend Herz,", "tokens": ["Kr\u00e4n\u00b7ke", "dein", "mich", "lie\u00b7bend", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "PRF", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur am sanften, bunten Freudenf\u00e4d'chen", "tokens": ["Nur", "am", "sanf\u00b7ten", ",", "bun\u00b7ten", "Freu\u00b7den\u00b7f\u00e4d'\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "G\u00e4ngle dich der Liebe Scherz!", "tokens": ["G\u00e4ng\u00b7le", "dich", "der", "Lie\u00b7be", "Scherz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie ein Zephyrl\u00fcftchen, sanft und leise,", "tokens": ["Wie", "ein", "Ze\u00b7phyr\u00b7l\u00fcft\u00b7chen", ",", "sanft", "und", "lei\u00b7se", ","], "token_info"