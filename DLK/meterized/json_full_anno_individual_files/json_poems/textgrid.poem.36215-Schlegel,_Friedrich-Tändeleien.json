{"textgrid.poem.36215": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "T\u00e4ndeleien", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als der Witz ein Liebchen suchte,", "tokens": ["Als", "der", "Witz", "ein", "Lieb\u00b7chen", "such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neckt' er M\u00e4dchen fern und nah,", "tokens": ["Neckt'", "er", "M\u00e4d\u00b7chen", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich w\u00e4hlt' er doch die Freude", "tokens": ["End\u00b7lich", "w\u00e4hlt'", "er", "doch", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihm leis' entgegen kam.", "tokens": ["Die", "ihm", "leis'", "ent\u00b7ge\u00b7gen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwerer ward ihr zartes Seelchen,", "tokens": ["Schwe\u00b7rer", "ward", "ihr", "zar\u00b7tes", "Seel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wu\u00dfte nicht, wie ihr geschah,", "tokens": ["Wu\u00df\u00b7te", "nicht", ",", "wie", "ihr", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis nach einem leichten St\u00f6hnen,", "tokens": ["Bis", "nach", "ei\u00b7nem", "leich\u00b7ten", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem S\u00f6hnlein sie genas.", "tokens": ["Von", "dem", "S\u00f6hn\u00b7lein", "sie", "ge\u00b7nas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mutwill hie\u00df das Kind der beiden,", "tokens": ["Mut\u00b7will", "hie\u00df", "das", "Kind", "der", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der noch kaum ein Knabe war,", "tokens": ["Der", "noch", "kaum", "ein", "Kna\u00b7be", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Als er schon mit M\u00e4dchen spielet,", "tokens": ["Als", "er", "schon", "mit", "M\u00e4d\u00b7chen", "spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gleich erhascht' die fl\u00fccht'ge Scham.", "tokens": ["Gleich", "er\u00b7hascht'", "die", "fl\u00fccht'\u00b7ge", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "A