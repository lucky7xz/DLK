{"textgrid.poem.41802": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Christbescherung", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Christnacht heilig' Offenbaren,", "tokens": ["Der", "Christ\u00b7nacht", "hei\u00b7lig'", "Of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das einst an alles Volk erging,", "tokens": ["Das", "einst", "an", "al\u00b7les", "Volk", "er\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kunde, die durch Engelscharen", "tokens": ["Die", "Kun\u00b7de", ",", "die", "durch", "En\u00b7gel\u00b7scha\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuerst das arme Volk empfing:", "tokens": ["Zu\u00b7erst", "das", "ar\u00b7me", "Volk", "emp\u00b7fing", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdie Liebe ist zur Welt gekommen,", "tokens": ["\u00bb", "die", "Lie\u00b7be", "ist", "zur", "Welt", "ge\u00b7kom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um einen neuen Bund zu weihn,", "tokens": ["Um", "ei\u00b7nen", "neu\u00b7en", "Bund", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein reines Licht ist hell entglommen", "tokens": ["Ein", "rei\u00b7nes", "Licht", "ist", "hell", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Stern mit wunderreichem Schein!\u00ab \u2013", "tokens": ["Ein", "Stern", "mit", "wun\u00b7der\u00b7rei\u00b7chem", "Schein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Kunde klingt aufs neue wieder", "tokens": ["Die", "Kun\u00b7de", "klingt", "aufs", "neu\u00b7e", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu uns in jeder Weihnachtszeit", "tokens": ["Zu", "uns", "in", "je\u00b7der", "Weih\u00b7nachts\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie t\u00f6nt durch alle Festeslieder", "tokens": ["Sie", "t\u00f6nt", "durch", "al\u00b7le", "Fes\u00b7tes\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In jedem Gru\u00df von nah und weit.", "tokens": ["In", "je\u00b7dem", "Gru\u00df", "von", "nah", "und", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdie Liebe soll die Welt regieren!\u00ab", "tokens": ["\u00bb", "die", "Lie\u00b7be", "soll", "die", "Welt", "re\u00b7gie\u00b7ren", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die Losung allerw\u00e4rts,", "tokens": ["Das", "ist", "die", "Lo\u00b7sung", "al\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lichter, die den Christbaum zieren", "tokens": ["Die", "Lich\u00b7ter", ",", "die", "den", "Christ\u00b7baum", "zie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie strahlen sie in jedes Herz;", "tokens": ["Wie", "strah\u00b7len", "sie", "in", "je\u00b7des", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und all die Gaben, lichtumschwommen,", "tokens": ["Und", "all", "die", "Ga\u00b7ben", ",", "lich\u00b7tum\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr jung und alt, f\u00fcr gro\u00df und klein:", "tokens": ["F\u00fcr", "jung", "und", "alt", ",", "f\u00fcr", "gro\u00df", "und", "klein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Himmel scheinen sie gekommen", "tokens": ["Vom", "Him\u00b7mel", "schei\u00b7nen", "sie", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einer Wundernacht zu sein! \u2013", "tokens": ["In", "ei\u00b7ner", "Wun\u00b7der\u00b7nacht", "zu", "sein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch all das Wunder zu vollenden,", "tokens": ["Doch", "all", "das", "Wun\u00b7der", "zu", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Sorgen gab es Tag und Nacht.", "tokens": ["Viel", "Sor\u00b7gen", "gab", "es", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel M\u00fchen von gesch\u00e4ft'gen H\u00e4nden,", "tokens": ["Viel", "M\u00fc\u00b7hen", "von", "ge\u00b7sch\u00e4ft'\u00b7gen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel Opfer freudig dargebracht.", "tokens": ["Viel", "Op\u00b7fer", "freu\u00b7dig", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und Weihnacht zeigt, da\u00df sie's vermag,", "tokens": ["Und", "Weih\u00b7nacht", "zeigt", ",", "da\u00df", "sie's", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch h\u00f6hres Ziel mu\u00df sie sich k\u00fcren,", "tokens": ["Doch", "h\u00f6h\u00b7res", "Ziel", "mu\u00df", "sie", "sich", "k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als schaffen nur f\u00fcr ", "tokens": ["Als", "schaf\u00b7fen", "nur", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Solch M\u00fchn und Opfern wohl uns ziert,", "tokens": ["Solch", "M\u00fchn", "und", "Op\u00b7fern", "wohl", "uns", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wir das Wort der Weihnacht ehren:", "tokens": ["Die", "wir", "das", "Wort", "der", "Weih\u00b7nacht", "eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Liebe nur die Welt regiert \u2013", "tokens": ["Da\u00df", "Lie\u00b7be", "nur", "die", "Welt", "re\u00b7giert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Auch V\u00f6lkerw\u00fcnsche sich erf\u00fcllen", "tokens": ["Auch", "V\u00f6l\u00b7ker\u00b7w\u00fcn\u00b7sche", "sich", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht durch das Wunder einer Nacht,", "tokens": ["Nicht", "durch", "das", "Wun\u00b7der", "ei\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum m\u00fche jeder sich im stillen", "tokens": ["Drum", "m\u00fc\u00b7he", "je\u00b7der", "sich", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "PRF", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis einst das Liebeswerk vollbracht;", "tokens": ["Bis", "einst", "das", "Lie\u00b7bes\u00b7werk", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Bis da\u00df im ganzen Vaterlande", "tokens": ["Bis", "da\u00df", "im", "gan\u00b7zen", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freiheit Christbaum leuchtend gl\u00fcht \u2013", "tokens": ["Der", "Frei\u00b7