{"dta.poem.2159": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wissenssucht sch\u00e4dliche  \n Folgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es fragt sich, ob die Ungewi\u00dfheit, der Zwiespalt,", "tokens": ["Es", "fragt", "sich", ",", "ob", "die", "Un\u00b7ge\u00b7wi\u00df\u00b7heit", ",", "der", "Zwies\u00b7palt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jrrthum, Zank und Streit", "tokens": ["Jrr\u00b7thum", ",", "Zank", "und", "Streit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der Philosophen auf der Welt nicht die\u00df allein zum", "tokens": ["Der", "Phi\u00b7lo\u00b7so\u00b7phen", "auf", "der", "Welt", "nicht", "die\u00df", "al\u00b7lein", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKNEG", "PDS", "ADV", "APPRART"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Grunde habe,", "tokens": ["Grun\u00b7de", "ha\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df man vergessen, erst auf sich und seines Geists Be-", "tokens": ["Da\u00df", "man", "ver\u00b7ges\u00b7sen", ",", "erst", "auf", "sich", "und", "sei\u00b7nes", "Geists", "Be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVPP", "$,", "ADV", "APPR", "PRF", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "schaffenheit,", "tokens": ["schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Auf unsrer Seel umschr\u00e4nktes Wesen, wie weit derselben", "tokens": ["Auf", "uns\u00b7rer", "Seel", "um\u00b7schr\u00e4nk\u00b7tes", "We\u00b7sen", ",", "wie", "weit", "der\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,", "PWAV", "ADJD", "PDAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Kr\u00e4fte gehn,", "tokens": ["Kr\u00e4f\u00b7te", "gehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Mit mehrerer Aufmerksamkeit und schuld\u2019ger Achtsamkeit", "tokens": ["Mit", "meh\u00b7re\u00b7rer", "Auf\u00b7merk\u00b7sam\u00b7keit", "und", "schuld'\u00b7ger", "Acht\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "zu sehn,", "tokens": ["zu", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Eh man sich unterstanden, sich mit h\u00f6hern Dingen zu", "tokens": ["Eh", "man", "sich", "un\u00b7ter\u00b7stan\u00b7den", ",", "sich", "mit", "h\u00f6\u00b7hern", "Din\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "VVPP", "$,", "PRF", "APPR", "ADJA", "NN", "PTKZU"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.12": {"text": "befassen,", "tokens": ["be\u00b7fas\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Und eh man, in die Gottheit gar zu schauen, sich gel\u00fcsten", "tokens": ["Und", "eh", "man", ",", "in", "die", "Got\u00b7theit", "gar", "zu", "schau\u00b7en", ",", "sich", "ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PIS", "$,", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "PRF", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "lassen?", "tokens": ["las\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Anstatt von unsrer Selbsterkenntni\u00df vor allen Dingen", "tokens": ["An\u00b7statt", "von", "uns\u00b7rer", "Selbs\u00b7ter\u00b7kennt\u00b7ni\u00df", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "anzufangen,", "tokens": ["an\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Und von ihr auf den Weg zur Demuth geleitet, durch", "tokens": ["Und", "von", "ihr", "auf", "den", "Weg", "zur", "De\u00b7muth", "ge\u00b7lei\u00b7tet", ",", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PPOSAT", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,", "APPR"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "dieselbe nur", "tokens": ["die\u00b7sel\u00b7be", "nur"], "token_info": ["word", "word"], "pos": ["PDAT", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Zur ehrerbietigen Bewundrung des Sch\u00f6pfers, in der", "tokens": ["Zur", "ehr\u00b7er\u00b7bie\u00b7ti\u00b7gen", "Be\u00b7wund\u00b7rung", "des", "Sch\u00f6p\u00b7fers", ",", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Kreatur", "tokens": ["Kre\u00b7a\u00b7tur"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "So froh- als ernstlicher Betrachtung, nach einer Ord-", "tokens": ["So", "froh", "als", "ernst\u00b7li\u00b7cher", "Be\u00b7trach\u00b7tung", ",", "nach", "ei\u00b7ner", "Ord"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "TRUNC", "KON", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "nung, zu gelangen,", "tokens": ["nung", ",", "zu", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "So hat man, aus selbsteigner F\u00fclle des Geists, von", "tokens": ["So", "hat", "man", ",", "aus", "selbs\u00b7teig\u00b7ner", "F\u00fcl\u00b7le", "des", "Geists", ",", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PIS", "$,", "APPR", "ADJA", "NN", "ART", "NN", "$,", "APPR"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "allem auf der Welt", "tokens": ["al\u00b7lem", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.25": {"text": "Sich selbst als einen klugen Richter, von Hochmuth", "tokens": ["Sich", "selbst", "als", "ei\u00b7nen", "klu\u00b7gen", "Rich\u00b7ter", ",", "von", "Hoch\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADV", "KOUS", "ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "aufgebl\u00e4ht, gestellt.", "tokens": ["auf\u00b7ge\u00b7bl\u00e4ht", ",", "ge\u00b7stellt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Hiedurch nun sind wir, durch den Stolz verf\u00fchrt, aufs", "tokens": ["Hie\u00b7durch", "nun", "sind", "wir", ",", "durch", "den", "Stolz", "ver\u00b7f\u00fchrt", ",", "aufs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "ADV", "VAFIN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "irrige Begreifen", "tokens": ["ir\u00b7ri\u00b7ge", "Be\u00b7grei\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Wohl recht ungl\u00fccklich hingerathen, wodurch der Eigen-", "tokens": ["Wohl", "recht", "un\u00b7gl\u00fcck\u00b7lich", "hin\u00b7ge\u00b7ra\u00b7then", ",", "wo\u00b7durch", "der", "Ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "$,", "PWAV", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "liebe zwar", "tokens": ["lie\u00b7be", "zwar"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}}}}