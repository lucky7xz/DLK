{"dta.poem.21449": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fragment.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lehne tr\u00e4umend am Br\u00fcckenrand,", "tokens": ["Ich", "leh\u00b7ne", "tr\u00e4u\u00b7mend", "am", "Br\u00fc\u00b7cken\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Aug\u2019 zu des Stromes Tiefen gewandt.", "tokens": ["Das", "Aug'", "zu", "des", "Stro\u00b7mes", "Tie\u00b7fen", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Schatten huscht es an mir vorbei,", "tokens": ["Wie", "Schat\u00b7ten", "huscht", "es", "an", "mir", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nur halb noch h\u00f6r\u2019 ich verworr\u2019nes Geschrei.", "tokens": ["Nur", "halb", "noch", "h\u00f6r'", "ich", "ver\u00b7worr'\u00b7nes", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Abend d\u00e4mmert m\u00e4hlich herein \u2026", "tokens": ["Der", "A\u00b7bend", "d\u00e4m\u00b7mert", "m\u00e4h\u00b7lich", "her\u00b7ein", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Pl\u00f6tzlich ergie\u00dft sich tr\u00fcbfahler Schein:", "tokens": ["Pl\u00f6tz\u00b7lich", "er\u00b7gie\u00dft", "sich", "tr\u00fcb\u00b7fah\u00b7ler", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "J\u00e4h\u2019 trifft mein Blick die Menschen all\u2019,", "tokens": ["J\u00e4h'", "trifft", "mein", "Blick", "die", "Men\u00b7schen", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die vor\u00fcberfluthen in wirrem Schwall.", "tokens": ["Die", "vor\u00b7\u00fc\u00b7berf\u00b7lut\u00b7hen", "in", "wir\u00b7rem", "Schwall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Ich sehe Karossen stolz und reich,", "tokens": ["Ich", "se\u00b7he", "Ka\u00b7ros\u00b7sen", "stolz", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Daneben die Armuth kummerbleich.", "tokens": ["Da\u00b7ne\u00b7ben", "die", "Ar\u00b7muth", "kum\u00b7mer\u00b7bleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Zumeist grub tiefe Linien die Noth,", "tokens": ["Zu\u00b7meist", "grub", "tie\u00b7fe", "Li\u00b7ni\u00b7en", "die", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das Laster, die Sorge um Leben und Brot.", "tokens": ["Das", "Las\u00b7ter", ",", "die", "Sor\u00b7ge", "um", "Le\u00b7ben", "und", "Brot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Verrohung spiegelt gar mancher Zug,", "tokens": ["Ver\u00b7ro\u00b7hung", "spie\u00b7gelt", "gar", "man\u00b7cher", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Unselige Selbstsucht, Lug und Trug.", "tokens": ["Un\u00b7se\u00b7li\u00b7ge", "Selbst\u00b7sucht", ",", "Lug", "und", "Trug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Keinem Auge entspr\u00fch\u2019t des Daseins Lust \u2014", "tokens": ["Kei\u00b7nem", "Au\u00b7ge", "ent\u00b7spr\u00fch't", "des", "Da\u00b7seins", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Weltscheue Schwermuth f\u00fcllt meine Brust.", "tokens": ["Welt\u00b7scheu\u00b7e", "Schwer\u00b7muth", "f\u00fcllt", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "++-+-++-+", "measure": "unknown.measure.hexa"}, "line.17": {"text": "Unendliches Weh und unendlicher Groll:", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", "Weh", "und", "un\u00b7end\u00b7li\u00b7cher", "Groll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.18": {"text": "Was all\u2019 das tolle Treiben soll!", "tokens": ["Was", "all'", "das", "tol\u00b7le", "Trei\u00b7ben", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ART", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Meisten kommen zur Erde und geh\u2019n", "tokens": ["Die", "Meis\u00b7ten", "kom\u00b7men", "zur", "Er\u00b7de", "und", "geh'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und haben nie sich selber geseh\u2019n.", "tokens": ["Und", "ha\u00b7ben", "nie", "sich", "sel\u00b7ber", "ge\u00b7seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VAFIN", "ADV", "PRF", "ADV", "VVFIN", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Sie lebten dumpf in thierischem Triebe,", "tokens": ["Sie", "leb\u00b7ten", "dumpf", "in", "thie\u00b7ri\u00b7schem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Sie f\u00fchlten nie das Gl\u00fcck der Liebe.", "tokens": ["Sie", "f\u00fchl\u00b7ten", "nie", "das", "Gl\u00fcck", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Sie sahen nie der Gottheit Spur,", "tokens": ["Sie", "sa\u00b7hen", "nie", "der", "Got\u00b7theit", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Sie kannten dich nicht, Allmutter Natur.", "tokens": ["Sie", "kann\u00b7ten", "dich", "nicht", ",", "A\u00b7llmut\u00b7ter", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}