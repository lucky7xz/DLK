{"textgrid.poem.64892": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab f\u00fcr jede Kr\u00e4nkung,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab f\u00fcr jede Kr\u00e4nkung,", "tokens": ["Ich", "hab", "f\u00fcr", "je\u00b7de", "Kr\u00e4n\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die du mir angethan,", "tokens": ["Die", "du", "mir", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr dich ein Gebet entsendet", "tokens": ["F\u00fcr", "dich", "ein", "Ge\u00b7bet", "ent\u00b7sen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum Herrn der Welt hinan.", "tokens": ["Zum", "Herrn", "der", "Welt", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich hab' das Wort des Fluches,", "tokens": ["Ich", "hab'", "das", "Wort", "des", "Flu\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Womit du mich geh\u00f6hnt,", "tokens": ["Wo\u00b7mit", "du", "mich", "ge\u00b7h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Segen dir vergolten,", "tokens": ["Mit", "Se\u00b7gen", "dir", "ver\u00b7gol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Innersten vers\u00f6hnt.", "tokens": ["Im", "In\u00b7ners\u00b7ten", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich hab f\u00fcr deine H\u00e4rte", "tokens": ["Ich", "hab", "f\u00fcr", "dei\u00b7ne", "H\u00e4r\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein L\u00e4cheln stets gehabt,", "tokens": ["Ein", "L\u00e4\u00b7cheln", "stets", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer nur ganz heimlich", "tokens": ["Und", "im\u00b7mer", "nur", "ganz", "heim\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Thr\u00e4nen mich gelabt.", "tokens": ["An", "Thr\u00e4\u00b7nen", "mich", "ge\u00b7labt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und h\u00e4tt'st du nicht so schmerzlich,", "tokens": ["Und", "h\u00e4tt'st", "du", "nicht", "so", "schmerz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So t\u00f6dtlich mich betr\u00fcbt:", "tokens": ["So", "t\u00f6dt\u00b7lich", "mich", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich glaube fast, ich h\u00e4tte", "tokens": ["Ich", "glau\u00b7be", "fast", ",", "ich", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich nicht so hei\u00df geliebt.", "tokens": ["Dich", "nicht", "so", "hei\u00df", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So dienen wohl die St\u00fcrme", "tokens": ["So", "die\u00b7nen", "wohl", "die", "St\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ihrem wilden Graus,", "tokens": ["Mit", "ih\u00b7rem", "wil\u00b7den", "Graus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Perle zu erzeugen,", "tokens": ["Die", "Per\u00b7le", "zu", "er\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im dunkeln Muschelhaus.", "tokens": ["Im", "dun\u00b7keln", "Mu\u00b7schel\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich hab f\u00fcr jede Kr\u00e4nkung,", "tokens": ["Ich", "ha