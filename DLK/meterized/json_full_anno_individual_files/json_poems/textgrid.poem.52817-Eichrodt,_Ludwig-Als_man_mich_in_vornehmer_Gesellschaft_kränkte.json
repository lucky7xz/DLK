{"textgrid.poem.52817": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Als man mich in vornehmer Gesellschaft kr\u00e4nkte", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich jetzt kein Mensche w\u00e4r',", "tokens": ["O", "da\u00df", "ich", "jetzt", "kein", "Men\u00b7sche", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie fr\u00f6hlich wollt' ich sein,", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "wollt'", "ich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie gut hat es der Esel,", "tokens": ["Wie", "gut", "hat", "es", "der", "E\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wohl ist es dem Schwein!", "tokens": ["Wie", "wohl", "ist", "es", "dem", "Schwein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Den Menschen g\u00e4b' ich einen Tritt,", "tokens": ["Den", "Men\u00b7schen", "g\u00e4b'", "ich", "ei\u00b7nen", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich der Esel w\u00e4r',", "tokens": ["Wenn", "ich", "der", "E\u