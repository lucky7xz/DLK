{"textgrid.poem.60281": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lebe nicht so schnell und st\u00fcrmisch;", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebe nicht so schnell und st\u00fcrmisch;", "tokens": ["Le\u00b7be", "nicht", "so", "schnell", "und", "st\u00fcr\u00b7misch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh den holden Fr\u00fchling prangen,", "tokens": ["Sieh", "den", "hol\u00b7den", "Fr\u00fch\u00b7ling", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6re seine Wonnelieder;", "tokens": ["H\u00f6\u00b7re", "sei\u00b7ne", "Won\u00b7ne\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wie bleich sind deine Wangen!", "tokens": ["Ach", ",", "wie", "bleich", "sind", "dei\u00b7ne", "Wan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welkt die Rose, kehrt sie wieder;", "tokens": ["Welkt", "die", "Ro\u00b7se", ",", "kehrt", "sie", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den lauen Fr\u00fchlingswinden", "tokens": ["Mit", "den", "lau\u00b7en", "Fr\u00fch\u00b7lings\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehren auch die Nachtigallen;", "tokens": ["Keh\u00b7ren", "auch", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden sie dich wiederfinden? \u2013", "tokens": ["Wer\u00b7den", "sie", "dich", "wie\u00b7der\u00b7fin\u00b7den", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "\u00bbk\u00f6nnt ich leben also innig,", "tokens": ["\u00bb", "k\u00f6nnt", "ich", "le\u00b7ben", "al\u00b7so", "in\u00b7nig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feurig, rasch und ungebunden,", "tokens": ["Feu\u00b7rig", ",", "rasch", "und", "un\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das Leben jenes Blitzes,", "tokens": ["Wie", "das", "Le\u00b7ben", "je\u00b7nes", "Blit\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dort im Gebirg verschwunden!\u00ab", "tokens": ["Der", "dort", "im", "Ge\u00b7birg", "ver\u00b7schwun\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct",