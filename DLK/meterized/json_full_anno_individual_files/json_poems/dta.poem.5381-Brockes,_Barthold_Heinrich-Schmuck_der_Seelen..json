{"dta.poem.5381": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schmuck der Seelen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach m\u00f6gten unsre Seelen doch, nach ihren Pflichten,", "tokens": ["Ach", "m\u00f6g\u00b7ten", "uns\u00b7re", "See\u00b7len", "doch", ",", "nach", "ih\u00b7ren", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sich bestreben,", "tokens": ["sich", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und, durch die Werckzeug\u2019 ihrer Sinnen, auf GOttes", "tokens": ["Und", ",", "durch", "die", "Wer\u00b7ck\u00b7zeug'", "ih\u00b7rer", "Sin\u00b7nen", ",", "auf", "Got\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wunder achtung geben!", "tokens": ["Wun\u00b7der", "ach\u00b7tung", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ach m\u00f6gten sie, den Bienen gleich,", "tokens": ["Ach", "m\u00f6g\u00b7ten", "sie", ",", "den", "Bie\u00b7nen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aufmercksam um dieselben schweben,", "tokens": ["Auf\u00b7merck\u00b7sam", "um", "die\u00b7sel\u00b7ben", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie w\u00fcrden nicht allein f\u00fcr sich der Anmuth s\u00fcssen Honig", "tokens": ["Sie", "w\u00fcr\u00b7den", "nicht", "al\u00b7lein", "f\u00fcr", "sich", "der", "An\u00b7muth", "s\u00fcs\u00b7sen", "Ho\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPR", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "heben;", "tokens": ["he\u00b7ben", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Sie w\u00fcrden durch der Sch\u00f6nheit-Schein", "tokens": ["Sie", "w\u00fcr\u00b7den", "durch", "der", "Sch\u00f6n\u00b7heit\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Creatur, mit der sie sich verbunden,", "tokens": ["Der", "Crea\u00b7tur", ",", "mit", "der", "sie", "sich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Von GOtt selbst lieblicher geziert gefunden,", "tokens": ["Von", "Gott", "selbst", "lieb\u00b7li\u00b7cher", "ge\u00b7ziert", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Verhoffentlich ihm angenehmer, seyn.", "tokens": ["Ver\u00b7hof\u00b7fent\u00b7lich", "ihm", "an\u00b7ge\u00b7neh\u00b7mer", ",", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PPER", "ADJD", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da ihnen, wenn von GOttes Macht,", "tokens": ["Da", "ih\u00b7nen", ",", "wenn", "von", "Got\u00b7tes", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In seiner Creaturen Pracht,", "tokens": ["In", "sei\u00b7ner", "Crea\u00b7tu\u00b7ren", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Sie oft was liebliches geh\u00f6ret und erblicket,", "tokens": ["Sie", "oft", "was", "lieb\u00b7li\u00b7ches", "ge\u00b7h\u00f6\u00b7ret", "und", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PWS", "ADJA", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sich eine Sch\u00f6nheit selbst durch Dencken eingedr\u00fccket.", "tokens": ["Sich", "ei\u00b7ne", "Sch\u00f6n\u00b7heit", "selbst", "durch", "Den\u00b7cken", "ein\u00b7ge\u00b7dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mu\u00df eine Seele nicht, auf solche Art geschm\u00fccket,", "tokens": ["Mu\u00df", "ei\u00b7ne", "See\u00b7le", "nicht", ",", "auf", "sol\u00b7che", "Art", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zumahl wenn sie dadurch zu reinen Trieben,", "tokens": ["Zu\u00b7mahl", "wenn", "sie", "da\u00b7durch", "zu", "rei\u00b7nen", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Nach allen Kr\u00e4ften GOtt zu lieben,", "tokens": ["Nach", "al\u00b7len", "Kr\u00e4f\u00b7ten", "Gott", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Gef\u00fchret wird; der GOttheit nicht gefallen,", "tokens": ["Ge\u00b7f\u00fch\u00b7ret", "wird", ";", "der", "Got\u00b7theit", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und angenehmer seyn, als wenn wir uns auf Erden", "tokens": ["Und", "an\u00b7ge\u00b7neh\u00b7mer", "seyn", ",", "als", "wenn", "wir", "uns", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAINF", "$,", "KOKOM", "KOUS", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So \u00e4ngstiglich allein bestreben, reich zu werden,", "tokens": ["So", "\u00e4ngs\u00b7tig\u00b7lich", "al\u00b7lein", "be\u00b7stre\u00b7ben", ",", "reich", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVPP", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und wir auf nichts, als auf Metallen", "tokens": ["Und", "wir", "auf", "nichts", ",", "als", "auf", "Me\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIS", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Alhier gesehen?", "tokens": ["Al\u00b7hier", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Da solche Seelen fast allein", "tokens": ["Da", "sol\u00b7che", "See\u00b7len", "fast", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Aus blos Metallischen Jdeen,", "tokens": ["Aus", "blos", "Me\u00b7tal\u00b7li\u00b7schen", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Wodurch sie fast allein gen\u00e4hret seyn,", "tokens": ["Wo\u00b7durch", "sie", "fast", "al\u00b7lein", "ge\u00b7n\u00e4h\u00b7ret", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der Menschlichkeit zum Hohn, bestehen.", "tokens": ["Der", "Menschlich\u00b7keit", "zum", "Hohn", ",", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}