{"dta.poem.4377": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mittel l\u00e4nger und vergn\u00fcgt  \n zu leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Getheilet wiederholen kann;", "tokens": ["Ge\u00b7thei\u00b7let", "wie\u00b7der\u00b7ho\u00b7len", "kann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je \u00f6fter wird man seine Pflichten", "tokens": ["Je", "\u00f6f\u00b7ter", "wird", "man", "sei\u00b7ne", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht nur zu GOttes Ruhm verrichten,", "tokens": ["Nicht", "nur", "zu", "Got\u00b7tes", "Ruhm", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man trifft ein neu Vergn\u00fcgen an.", "tokens": ["Man", "trifft", "ein", "neu", "Ver\u00b7gn\u00fc\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch mehr, ich wei\u00df, da\u00df wir auf Erden,", "tokens": ["Noch", "mehr", ",", "ich", "wei\u00df", ",", "da\u00df", "wir", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf diese Weise, l\u00e4nger leben,", "tokens": ["Auf", "die\u00b7se", "Wei\u00b7se", ",", "l\u00e4n\u00b7ger", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Sch\u00f6pfers Allmacht mehr erheben,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "All\u00b7macht", "mehr", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Seine Liebe preisen werden.", "tokens": ["Und", "Sei\u00b7ne", "Lie\u00b7be", "prei\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}