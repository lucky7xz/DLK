{"textgrid.poem.54383": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erbrich, gelehrter Freund, erbrich dies schmale Blat", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erbrich, gelehrter Freund, erbrich dies schmale Blat", "tokens": ["Er\u00b7brich", ",", "ge\u00b7lehr\u00b7ter", "Freund", ",", "er\u00b7brich", "dies", "schma\u00b7le", "Blat"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,", "ADJD", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das blo\u00df Erkenntlichkeit zu seinem Inhalt hat.", "tokens": ["Das", "blo\u00df", "Er\u00b7kennt\u00b7lich\u00b7keit", "zu", "sei\u00b7nem", "In\u00b7halt", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein jedes Wort zeigt dir mein redliches Gem\u00fcthe;", "tokens": ["Ein", "je\u00b7des", "Wort", "zeigt", "dir", "mein", "red\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es brennt in meiner Brust der Dank vor deine G\u00fcte.", "tokens": ["Es", "brennt", "in", "mei\u00b7ner", "Brust", "der", "Dank", "vor", "dei\u00b7ne", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dieselbe zeigt der Flei\u00df, ich schreibe unerm\u00fcdt,", "tokens": ["Die\u00b7sel\u00b7be", "zeigt", "der", "Flei\u00df", ",", "ich", "schrei\u00b7be", "un\u00b7er\u00b7m\u00fcdt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil mich dein Beyfall itzt zu dem Gesch\u00e4fte zieht.", "tokens": ["Weil", "mich", "dein", "Bey\u00b7fall", "itzt", "zu", "dem", "Ge\u00b7sch\u00e4f\u00b7te", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich denke noch daran was dir mein Mund versprochen,", "tokens": ["Ich", "den\u00b7ke", "noch", "da\u00b7ran", "was", "dir", "mein", "Mund", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "PWS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als ich den Lorber sah den du vor mich gebrochen.", "tokens": ["Als", "ich", "den", "Lor\u00b7ber", "sah", "den", "du", "vor", "mich", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die werthe Leucoris liegt mir stets in dem Sinn,", "tokens": ["Die", "wert\u00b7he", "Leu\u00b7co\u00b7ris", "liegt", "mir", "stets", "in", "dem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So da\u00df ich nicht wie sonst in meiner Ruhe bin.", "tokens": ["So", "da\u00df", "ich", "nicht", "wie", "sonst", "in", "mei\u00b7ner", "Ru\u00b7he", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Geist ist aufgebracht, ich will mein Wort auch halten", "tokens": ["Mein", "Geist", "ist", "auf\u00b7ge\u00b7bracht", ",", "ich", "will", "mein", "Wort", "auch", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nach unsrer Deutschen Art, wie sonst die lieben Alten.", "tokens": ["Nach", "uns\u00b7rer", "Deut\u00b7schen", "Art", ",", "wie", "sonst", "die", "lie\u00b7ben", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Damit mein Wissen auch gesetzt und gr\u00fcndlich sey,", "tokens": ["Da\u00b7mit", "mein", "Wis\u00b7sen", "auch", "ge\u00b7setzt", "und", "gr\u00fcnd\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADV", "VVPP", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So unterfang ich mich im Lernen vielerley.", "tokens": ["So", "un\u00b7ter\u00b7fang", "ich", "mich", "im", "Ler\u00b7nen", "vie\u00b7ler\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der dir Bekannte \u2013 sucht mich zu unterrichten", "tokens": ["Der", "dir", "Be\u00b7kann\u00b7te", "\u2013", "sucht", "mich", "zu", "un\u00b7ter\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "$(", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "In der Philosophie; die bessert einst mein Dichten.", "tokens": ["In", "der", "Phi\u00b7lo\u00b7so\u00b7phie", ";", "die", "bes\u00b7sert", "einst", "mein", "Dich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ART", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mir geht ein neues Licht in dem Verstande auf,", "tokens": ["Mir", "geht", "ein", "neu\u00b7es", "Licht", "in", "dem", "Ver\u00b7stan\u00b7de", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich sp\u00fchr der Lehren Kraft, und gebe Achtung drauf.", "tokens": ["Ich", "sp\u00fchr", "der", "Leh\u00b7ren", "Kraft", ",", "und", "ge\u00b7be", "Ach\u00b7tung", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "O! sch\u00f6ne Wissenschaft, du bist ganz auserlesen.", "tokens": ["O", "!", "sch\u00f6\u00b7ne", "Wis\u00b7sen\u00b7schaft", ",", "du", "bist", "ganz", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dein reizender Begriff, dein Schlu\u00df, dein Satz, dein Wesen,", "tokens": ["Dein", "rei\u00b7zen\u00b7der", "Be\u00b7griff", ",", "dein", "Schlu\u00df", ",", "dein", "Satz", ",", "dein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zeigt uns der Weisheit bahn; hier scheut man nicht Gefahr.", "tokens": ["Zeigt", "uns", "der", "Weis\u00b7heit", "bahn", ";", "hier", "scheut", "man", "nicht", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das Ende stellet uns etwas Vollkommnes dar.", "tokens": ["Das", "En\u00b7de", "stel\u00b7let", "uns", "et\u00b7was", "Voll\u00b7komm\u00b7nes", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich z\u00fcrne da\u00df ich nicht vorl\u00e4ngst darauf verfallen;", "tokens": ["Ich", "z\u00fcr\u00b7ne", "da\u00df", "ich", "nicht", "vor\u00b7l\u00e4ngst", "da\u00b7rauf", "ver\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PTKNEG", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mir ekelt noch vor mir, ich kann nur davon lallen;", "tokens": ["Mir", "e\u00b7kelt", "noch", "vor", "mir", ",", "ich", "kann", "nur", "da\u00b7von", "lal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "PPER", "VMFIN", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zwar \u00e4rgert mein Geschlecht sich wohl nicht wenig dran,", "tokens": ["Zwar", "\u00e4r\u00b7gert", "mein", "Ge\u00b7schlecht", "sich", "wohl", "nicht", "we\u00b7nig", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ADV", "PTKNEG", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es sieht mich statt des Danks mit schelen Augen an;", "tokens": ["Es", "sieht", "mich", "statt", "des", "Danks", "mit", "sche\u00b7len", "Au\u00b7gen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch mein gelassner Geist erduldet alles Schm\u00e4hen.", "tokens": ["Doch", "mein", "ge\u00b7lass\u00b7ner", "Geist", "er\u00b7dul\u00b7det", "al\u00b7les", "Schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Gesetzt das M\u00e4nnervolk kanns auch nicht gerne sehen;", "tokens": ["Ge\u00b7setzt", "das", "M\u00e4n\u00b7ner\u00b7volk", "kanns", "auch", "nicht", "ger\u00b7ne", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Vorsatz ist gefa\u00dft, mich st\u00f6rt kein schwarzer Neid.", "tokens": ["Mein", "Vor\u00b7satz", "ist", "ge\u00b7fa\u00dft", ",", "mich", "st\u00f6rt", "kein", "schwar\u00b7zer", "Neid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich \u00fcberwind ihn doch einst durch Vernunft und Zeit.", "tokens": ["Ich", "\u00fc\u00b7berw\u00b7ind", "ihn", "doch", "einst", "durch", "Ver\u00b7nunft", "und", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was kann ich dir zum Schlu\u00df vor deine Freundschaft schenken?", "tokens": ["Was", "kann", "ich", "dir", "zum", "Schlu\u00df", "vor", "dei\u00b7ne", "Freund\u00b7schaft", "schen\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein Herz voll Redlichkeit, ein stetes Angedenken", "tokens": ["Ein", "Herz", "voll", "Red\u00b7lich\u00b7keit", ",", "ein", "ste\u00b7tes", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mehr fordre nicht von mir, gr\u00fc\u00df was mir g\u00fcnstig ist,", "tokens": ["Mehr", "ford\u00b7re", "nicht", "von", "mir", ",", "gr\u00fc\u00df", "was", "mir", "g\u00fcns\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "VVFIN", "PWS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Damit in Wittenberg man meiner nicht vergi\u00dft.", "tokens": ["Da\u00b7mit", "in", "Wit\u00b7ten\u00b7berg", "man", "mei\u00b7ner", "nicht", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NE", "PIS", "PPOSAT", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erbrich, gelehrter Freund, erbrich dies schmale Blat", "tokens": ["Er\u00b7brich", ",", "ge\u00b7lehr\u00b7ter", "Freund", ",", "er\u00b7brich", "dies", "schma\u00b7le", "Blat"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,", "ADJD", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das blo\u00df Erkenntlichkeit zu seinem Inhalt hat.", "tokens": ["Das", "blo\u00df", "Er\u00b7kennt\u00b7lich\u00b7keit", "zu", "sei\u00b7nem", "In\u00b7halt", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein jedes Wort zeigt dir mein redliches Gem\u00fcthe;", "tokens": ["Ein", "je\u00b7des", "Wort", "zeigt", "dir", "mein", "red\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es brennt in meiner Brust der Dank vor deine G\u00fcte.", "tokens": ["Es", "brennt", "in", "mei\u00b7ner", "Brust", "der", "Dank", "vor", "dei\u00b7ne", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dieselbe zeigt der Flei\u00df, ich schreibe unerm\u00fcdt,", "tokens": ["Die\u00b7sel\u00b7be", "zeigt", "der", "Flei\u00df", ",", "ich", "schrei\u00b7be", "un\u00b7er\u00b7m\u00fcdt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil mich dein Beyfall itzt zu dem Gesch\u00e4fte zieht.", "tokens": ["Weil", "mich", "dein", "Bey\u00b7fall", "itzt", "zu", "dem", "Ge\u00b7sch\u00e4f\u00b7te", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich denke noch daran was dir mein Mund versprochen,", "tokens": ["Ich", "den\u00b7ke", "noch", "da\u00b7ran", "was", "dir", "mein", "Mund", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "PWS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als ich den Lorber sah den du vor mich gebrochen.", "tokens": ["Als", "ich", "den", "Lor\u00b7ber", "sah", "den", "du", "vor", "mich", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die werthe Leucoris liegt mir stets in dem Sinn,", "tokens": ["Die", "wert\u00b7he", "Leu\u00b7co\u00b7ris", "liegt", "mir", "stets", "in", "dem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So da\u00df ich nicht wie sonst in meiner Ruhe bin.", "tokens": ["So", "da\u00df", "ich", "nicht", "wie", "sonst", "in", "mei\u00b7ner", "Ru\u00b7he", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Geist ist aufgebracht, ich will mein Wort auch halten", "tokens": ["Mein", "Geist", "ist", "auf\u00b7ge\u00b7bracht", "