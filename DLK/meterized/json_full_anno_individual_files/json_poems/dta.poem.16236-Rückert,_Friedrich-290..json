{"dta.poem.16236": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "290.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Gott in dir nur ist, so wird in H\u00f6hn und Gr\u00fcnden", "tokens": ["Wenn", "Gott", "in", "dir", "nur", "ist", ",", "so", "wird", "in", "H\u00f6hn", "und", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPER", "ADV", "VAFIN", "$,", "ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00f6pfung \u00fcberall sein Wirken dir sich k\u00fcnden.", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "\u00fc\u00b7be\u00b7rall", "sein", "Wir\u00b7ken", "dir", "sich", "k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dis ist, und dieses nur, die H\u00fclfe der Natur:", "tokens": ["Dis", "ist", ",", "und", "die\u00b7ses", "nur", ",", "die", "H\u00fcl\u00b7fe", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KON", "PDS", "ADV", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Sie lehret dich nicht Gott, doch zeigt dir seine Spur.", "tokens": ["Sie", "leh\u00b7ret", "dich", "nicht", "Gott", ",", "doch", "zeigt", "dir", "sei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das wesentliche Licht mu\u00df in dir seyn dein eigen,", "tokens": ["Das", "we\u00b7sent\u00b7li\u00b7che", "Licht", "mu\u00df", "in", "dir", "seyn", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PPER", "VAINF", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sich sein Abglanz soll in tausend Spiegeln zeigen.", "tokens": ["Wenn", "sich", "sein", "Ab\u00b7glanz", "soll", "in", "tau\u00b7send", "Spie\u00b7geln", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VMFIN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Schl\u00fcssel der Natur mu\u00df dir in H\u00e4nden ruhn,", "tokens": ["Der", "Schl\u00fcs\u00b7sel", "der", "Na\u00b7tur", "mu\u00df", "dir", "in", "H\u00e4n\u00b7den", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um ihre ewigen Schatzkammern aufzuthun.", "tokens": ["Um", "ih\u00b7re", "e\u00b7wi\u00b7gen", "Schatz\u00b7kam\u00b7mern", "auf\u00b7zu\u00b7thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie aber ist nun Gott in dich hineingekommen?", "tokens": ["Wie", "a\u00b7ber", "ist", "nun", "Gott", "in", "dich", "hin\u00b7ein\u00b7ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ADV", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du ihn auf- und an-? hat er dich eingenommen?", "tokens": ["Hast", "du", "ihn", "auf", "und", "an", "?", "hat", "er", "dich", "ein\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "TRUNC", "KON", "TRUNC", "$.", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du hast ihn nicht erdacht, noch selbst hervorgebracht;", "tokens": ["Du", "hast", "ihn", "nicht", "er\u00b7dacht", ",", "noch", "selbst", "her\u00b7vor\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schlief er villeicht in dir, und w\u00e4re nur erwacht?", "tokens": ["Schlief", "er", "vil\u00b7leicht", "in", "dir", ",", "und", "w\u00e4\u00b7re", "nur", "er\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du bist die Wiege, die er selber sich erkoren;", "tokens": ["Du", "bist", "die", "Wie\u00b7ge", ",", "die", "er", "sel\u00b7ber", "sich", "er\u00b7ko\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht du gebarest ihn, er hat sich dir geboren.", "tokens": ["Nicht", "du", "ge\u00b7ba\u00b7rest", "ihn", ",", "er", "hat", "sich", "dir", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Er hat, um einzuziehn, die Pforten dir verliehn,", "tokens": ["Er", "hat", ",", "um", "ein\u00b7zu\u00b7ziehn", ",", "die", "Pfor\u00b7ten", "dir", "ver\u00b7liehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUI", "VVIZU", "$,", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch dazu die Macht, selbst auszuschlie\u00dfen ihn.", "tokens": ["Und", "auch", "da\u00b7zu", "die", "Macht", ",", "selbst", "aus\u00b7zu\u00b7schlie\u00b7\u00dfen", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Er steht und klopfet an, und wenn du aufgethan,", "tokens": ["Er", "steht", "und", "klop\u00b7fet", "an", ",", "und", "wenn", "du", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,", "KON", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hast du auch dazu von ihm die Kraft empfahn.", "tokens": ["So", "hast", "du", "auch", "da\u00b7zu", "von", "ihm", "die", "Kraft", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}