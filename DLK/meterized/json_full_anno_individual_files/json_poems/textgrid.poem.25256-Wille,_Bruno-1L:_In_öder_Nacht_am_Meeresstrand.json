": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vor der w\u00fcsten Wolkenrotte.", "tokens": ["Vor", "der", "w\u00fcs\u00b7ten", "Wol\u00b7ken\u00b7rot\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun sp\u00fcre, mein Fels, vom Taumel umtost,", "tokens": ["Nun", "sp\u00fc\u00b7re", ",", "mein", "Fels", ",", "vom", "Tau\u00b7mel", "um\u00b7tost", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie ein frommes Lied dich durchschauert:", "tokens": ["Wie", "ein", "from\u00b7mes", "Lied", "dich", "durch\u00b7schau\u00b7ert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbhalt aus! Es keimt ein heimlicher Trost,", "tokens": ["\u00bb", "halt", "aus", "!", "Es", "keimt", "ein", "heim\u00b7li\u00b7cher", "Trost", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo Treue in Trennung trauert.", "tokens": ["Wo", "Treu\u00b7e", "in", "Tren\u00b7nung", "trau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Der heiligen Keuschheit bleibe geweiht", "tokens": ["Der", "hei\u00b7li\u00b7gen", "Keuschheit", "blei\u00b7be", "ge\u00b7weiht"], "t