{"dta.poem.10807": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken \u00fcber die Thr\u00e4nen  \n JEsu, welche er bei dem Anblik der  \n Stadt Jerusalem ver-  \n gossen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe thr\u00e4nt in dir, o! Heiland da du\nweinst,", "tokens": ["Die", "Lie\u00b7be", "thr\u00e4nt", "in", "dir", ",", "o", "!", "Hei\u00b7land", "da", "du", "weinst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "FM", "$.", "NN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deiner M\u00f6rderstadt, als wie ein Lamm", "tokens": ["In", "dei\u00b7ner", "M\u00f6r\u00b7der\u00b7stadt", ",", "als", "wie", "ein", "Lamm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das in der Unschuld wallt, und f\u00fcr die S\u00fcnder b\u00fcs-", "tokens": ["Das", "in", "der", "Un\u00b7schuld", "wallt", ",", "und", "f\u00fcr", "die", "S\u00fcn\u00b7der", "b\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu deren Reinigung dein heilig Blut zerfliesset.", "tokens": ["Zu", "de\u00b7ren", "Rei\u00b7ni\u00b7gung", "dein", "hei\u00b7lig", "Blut", "zer\u00b7flies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du siehst von ferne schon der Juden steinern Herz,", "tokens": ["Du", "siehst", "von", "fer\u00b7ne", "schon", "der", "Ju\u00b7den", "stei\u00b7nern", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Bosheit die dich w\u00fcrgt, dich qu\u00e4lt mit heissen", "tokens": ["Die", "Bos\u00b7heit", "die", "dich", "w\u00fcrgt", ",", "dich", "qu\u00e4lt", "mit", "heis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du siehst im Geist vorher, wie das Gericht zertr\u00fcm-", "tokens": ["Du", "siehst", "im", "Geist", "vor\u00b7her", ",", "wie", "das", "Ge\u00b7richt", "zer\u00b7tr\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$,", "PWAV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jm Rachfeur alles schmelzt, was noch im Glanze", "tokens": ["Jm", "Rach\u00b7feur", "al\u00b7les", "schmelzt", ",", "was", "noch", "im", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PIS", "VVFIN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das Ungl\u00fck r\u00fchrt dein Herz, es weint, durch die-", "tokens": ["Das", "Un\u00b7gl\u00fck", "r\u00fchrt", "dein", "Herz", ",", "es", "weint", ",", "durch", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Juden steinern Herz noch endlich zu erwei-", "tokens": ["Der", "Ju\u00b7den", "stei\u00b7nern", "Herz", "noch", "end\u00b7lich", "zu", "er\u00b7wei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr S\u00fcnder! sehet hier ein Bild der Gnade an,", "tokens": ["Ihr", "S\u00fcn\u00b7der", "!", "se\u00b7het", "hier", "ein", "Bild", "der", "Gna\u00b7de", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die wenn sie euch nicht mehr das Herze r\u00fchren", "tokens": ["Die", "wenn", "sie", "euch", "nicht", "mehr", "das", "Her\u00b7ze", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "PDS", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Euch solche Zeichen giebt, daran ihr k\u00f6nnt erkennen,", "tokens": ["Euch", "sol\u00b7che", "Zei\u00b7chen", "giebt", ",", "da\u00b7ran", "ihr", "k\u00f6nnt", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$,", "PAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es sey ", "tokens": ["Es", "sey"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Der Augen Z\u00e4hrensalz, das von den Wangen", "tokens": ["Der", "Au\u00b7gen", "Z\u00e4h\u00b7ren\u00b7salz", ",", "das", "von", "den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Des Mundes klagend Wort: ", "tokens": ["Des", "Mun\u00b7des", "kla\u00b7gend", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Bezeugen JEsus Treu, da\u00df er das Volk gesuchet,", "tokens": ["Be\u00b7zeu\u00b7gen", "Je\u00b7sus", "Treu", ",", "da\u00df", "er", "das", "Volk", "ge\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das aus gerechten Zorn des H\u00f6chsten Macht ver-", "tokens": ["Das", "aus", "ge\u00b7rech\u00b7ten", "Zorn", "des", "H\u00f6chs\u00b7ten", "Macht", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Jhr Menschen sehet doch, wie woll es JEsus meint,", "tokens": ["Ihr", "Men\u00b7schen", "se\u00b7het", "doch", ",", "wie", "woll", "es", "Je\u00b7sus", "meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PWAV", "VMFIN", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er sieht eur Ungl\u00fck an, daran ihr Schuld, und", "tokens": ["Er", "sieht", "eur", "Un\u00b7gl\u00fck", "an", ",", "da\u00b7ran", "ihr", "Schuld", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PAV", "PPOSAT", "NN", "$,", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Indem ihr ihn verwerfft, bewei\u00dft er Sanfftmuths-", "tokens": ["In\u00b7dem", "ihr", "ihn", "ver\u00b7werfft", ",", "be\u00b7wei\u00dft", "er", "Sanf\u00b7ft\u00b7muths"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und das beweiset euch, da\u00df JEsus sey die Lie-", "tokens": ["Und", "das", "be\u00b7wei\u00b7set", "euch", ",", "da\u00df", "Je\u00b7sus", "sey", "die", "Lie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$,", "KOUS", "NE", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}