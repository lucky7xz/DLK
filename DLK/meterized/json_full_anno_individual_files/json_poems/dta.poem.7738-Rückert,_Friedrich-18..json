{"dta.poem.7738": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen bl\u00fchn so sch\u00f6n noch wie vor tausend Jahren,", "tokens": ["Die", "Blu\u00b7men", "bl\u00fchn", "so", "sch\u00f6n", "noch", "wie", "vor", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "ADV", "KOKOM", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wir sind schlechter nicht als unsre V\u00e4ter waren.", "tokens": ["Und", "wir", "sind", "schlech\u00b7ter", "nicht", "als", "uns\u00b7re", "V\u00e4\u00b7ter", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "PTKNEG", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Blumen bl\u00fchen jetzt nicht sch\u00f6ner als vor Jahren,", "tokens": ["Die", "Blu\u00b7men", "bl\u00fc\u00b7hen", "jetzt", "nicht", "sch\u00f6\u00b7ner", "als", "vor", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "ADJD", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wir sind weiser nicht als unsre V\u00e4ter waren.", "tokens": ["Und", "wir", "sind", "wei\u00b7ser", "nicht", "als", "uns\u00b7re", "V\u00e4\u00b7ter", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKNEG", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn wo nur Himmelstrich und Jahrzeit es erlaubt,", "tokens": ["Denn", "wo", "nur", "Him\u00b7mel\u00b7strich", "und", "Jahr\u00b7zeit", "es", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bl\u00fcht Geist in Glanz getaucht, Gem\u00fcth von Duft bestaubt.", "tokens": ["Bl\u00fcht", "Geist", "in", "Glanz", "ge\u00b7taucht", ",", "Ge\u00b7m\u00fcth", "von", "Duft", "be\u00b7staubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "VVPP", "$,", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}