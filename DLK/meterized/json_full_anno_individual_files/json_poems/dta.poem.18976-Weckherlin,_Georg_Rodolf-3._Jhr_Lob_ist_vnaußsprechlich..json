{"dta.poem.18976": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "3.  \n  Jhr Lob ist vnau\u00dfsprechlich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Welchen der G\u00f6tter schatz/ der Natur beste kunst/", "tokens": ["Wel\u00b7chen", "der", "G\u00f6t\u00b7ter", "schatz", "/", "der", "Na\u00b7tur", "bes\u00b7te", "kunst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Des himmels k\u00f6stlichkeit allhie zu sehen s\u00f6hnet/", "tokens": ["Des", "him\u00b7mels", "k\u00f6st\u00b7lich\u00b7keit", "all\u00b7hie", "zu", "se\u00b7hen", "s\u00f6h\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der segne sein gesicht durch die Sch\u00f6n/ deren", "tokens": ["Der", "seg\u00b7ne", "sein", "ge\u00b7sicht", "durch", "die", "Sch\u00f6n", "/", "de\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "VVPP", "APPR", "ART", "NE", "$(", "PDS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "gunst", "tokens": ["gunst"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Erquicket meinen gaist/ vnd diese welt besch\u00f6net.", "tokens": ["Er\u00b7quic\u00b7ket", "mei\u00b7nen", "gaist", "/", "vnd", "die\u00b7se", "welt", "be\u00b7sch\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch kommet Er nicht bald/ so kom\u0303et Er vmbsunst/", "tokens": ["Doch", "kom\u00b7met", "Er", "nicht", "bald", "/", "so", "kom\u0303et", "Er", "vmbsunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$(", "ADV", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dan schon ein jeder Got begihrig nach jhr g\u00f6hnet/", "tokens": ["Dan", "schon", "ein", "je\u00b7der", "Got", "be\u00b7gih\u00b7rig", "nach", "jhr", "g\u00f6h\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIAT", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit in purer lieb vnd gantz hayliger brunst", "tokens": ["Da\u00b7mit", "in", "pu\u00b7rer", "lieb", "vnd", "gantz", "hay\u00b7li\u00b7ger", "brunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "ADJD", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Er werd mit ehr von jhr/ vnd sie von jhm gekr\u00f6net.", "tokens": ["Er", "werd", "mit", "ehr", "von", "jhr", "/", "vnd", "sie", "von", "jhm", "ge\u00b7kr\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "PPOSAT", "$(", "KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der tugent s\u00fcsse krafft/ der Lieb vn\u0304 Sch\u00f6nheit prob/", "tokens": ["Der", "tu\u00b7gent", "s\u00fcs\u00b7se", "krafft", "/", "der", "Lieb", "vn\u0304", "Sch\u00f6n\u00b7heit", "prob", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der holdseeligkeit vnd g\u00f6tlichen geberden", "tokens": ["Mit", "der", "hold\u00b7see\u00b7lig\u00b7keit", "vnd", "g\u00f6t\u00b7li\u00b7chen", "ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In eines Engels leib er sehen wird auff erden/", "tokens": ["In", "ei\u00b7nes", "En\u00b7gels", "leib", "er", "se\u00b7hen", "wird", "auff", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Bekennend/ da\u00df allein des s\u00fcssen Engels lob", "tokens": ["Be\u00b7ken\u00b7nend", "/", "da\u00df", "al\u00b7lein", "des", "s\u00fcs\u00b7sen", "En\u00b7gels", "lob"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "KOUS", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von eines Engels mund kan recht gesungen", "tokens": ["Von", "ei\u00b7nes", "En\u00b7gels", "mund", "kan", "recht", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VMFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "werden/", "tokens": ["wer\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VAINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vnd da\u00df ohn Sie die welt gantz ellend/ arm/ vnd", "tokens": ["Vnd", "da\u00df", "ohn", "Sie", "die", "welt", "gantz", "el\u00b7lend", "/", "arm", "/", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "KOUS", "APPR", "PPER", "ART", "NN", "ADV", "ADJD", "$(", "ADJD", "$(", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "grob.", "tokens": ["grob", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}}}}}