{"dta.poem.7444": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was schmerzlich oft die Seele mir durchw\u00fchlte", "tokens": ["Was", "schmerz\u00b7lich", "oft", "die", "See\u00b7le", "mir", "durch\u00b7w\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und drin in stillen N\u00e4chten sich bewegte,", "tokens": ["Und", "drin", "in", "stil\u00b7len", "N\u00e4ch\u00b7ten", "sich", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie meine Mutter mich, ", "tokens": ["Wie", "mei\u00b7ne", "Mut\u00b7ter", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was ich ", "tokens": ["Was", "ich"], "token_info": ["word", "word"], "pos": ["PWS", "PPER"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Hier ist es, wie ich's aus der Brust mir sp\u00fclte,", "tokens": ["Hier", "ist", "es", ",", "wie", "ich's", "aus", "der", "Brust", "mir", "sp\u00fcl\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "PIS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ich's in scharfgeschliff'ne Formen legte,", "tokens": ["Wie", "ich's", "in", "scha\u00b7rf\u00b7ge\u00b7schliff'\u00b7ne", "For\u00b7men", "leg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vor roher Hand mit einem Zaun umhegte,", "tokens": ["Vor", "ro\u00b7her", "Hand", "mit", "ei\u00b7nem", "Zaun", "um\u00b7heg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begl\u00fcckt, da\u00df ich das Herz mir endlich k\u00fchlte.", "tokens": ["Be\u00b7gl\u00fcckt", ",", "da\u00df", "ich", "das", "Herz", "mir", "end\u00b7lich", "k\u00fchl\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch schaudert mich, so wild sind meine Musen,", "tokens": ["Doch", "schau\u00b7dert", "mich", ",", "so", "wild", "sind", "mei\u00b7ne", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein toll Geschlecht, gleich jener Rotte Kora,", "tokens": ["Ein", "toll", "Ge\u00b7schlecht", ",", "gleich", "je\u00b7ner", "Rot\u00b7te", "Ko\u00b7ra", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADV", "PDAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Abscheuliche, versteinernde Medusen \u2014", "tokens": ["Ab\u00b7scheu\u00b7li\u00b7che", ",", "ver\u00b7stei\u00b7nern\u00b7de", "Me\u00b7du\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Allein nur zu \u2014 ", "tokens": ["Al\u00b7lein", "nur", "zu"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Fort mit den Ungeheuern aus dem Busen,", "tokens": ["Fort", "mit", "den", "Un\u00b7ge\u00b7heu\u00b7ern", "aus", "dem", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und aufgethan die B\u00fcchse der Pandora!", "tokens": ["Und", "auf\u00b7ge\u00b7than", "die", "B\u00fcch\u00b7se", "der", "Pan\u00b7do\u00b7ra", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}