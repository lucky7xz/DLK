{"dta.poem.11109": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gut macht gluth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man bot dem Livio zu einer heyrath dar", "tokens": ["Man", "bot", "dem", "Li\u00b7vio", "zu", "ei\u00b7ner", "hey\u00b7rath", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein m\u00e4gdgen, das zwar arm, doch guten standes war;", "tokens": ["Ein", "m\u00e4gd\u00b7gen", ",", "das", "zwar", "arm", ",", "doch", "gu\u00b7ten", "stan\u00b7des", "war", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "ADJD", "$,", "ADV", "ADJA", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur etwas fehlt, sprach er, da\u00df ich mich nicht erkl\u00e4re:", "tokens": ["Nur", "et\u00b7was", "fehlt", ",", "sprach", "er", ",", "da\u00df", "ich", "mich", "nicht", "er\u00b7kl\u00e4\u00b7re", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ankunfft w\u00e4r wohl gut, wenn nur die einkunfft w\u00e4re.", "tokens": ["Die", "an\u00b7kunfft", "w\u00e4r", "wohl", "gut", ",", "wenn", "nur", "die", "ein\u00b7kunfft", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"