{"textgrid.poem.66317": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Jerusalem", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wallet sanft, ihr T\u00f6ne!", "tokens": ["Wal\u00b7let", "sanft", ",", "ihr", "T\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Salem gl\u00e4nzt, die Sch\u00f6ne,", "tokens": ["Sa\u00b7lem", "gl\u00e4nzt", ",", "die", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mir ins Angesicht.", "tokens": ["Mir", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Festlich strahlen dorten", "tokens": ["Fest\u00b7lich", "strah\u00b7len", "dor\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Diamantne Pforten,", "tokens": ["Di\u00b7a\u00b7mant\u00b7ne", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Hell, wie Sonnenlicht.", "tokens": ["Hell", ",", "wie", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ist ein Stein,", "tokens": ["Ist", "ein", "Stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ein Glas so rein?", "tokens": ["Ein", "Glas", "so", "rein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Gl\u00e4nzt, wie Salem in der Ferne,", "tokens": ["Gl\u00e4nzt", ",", "wie", "Sa\u00b7lem", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "So die Pracht der Sterne?", "tokens": ["So", "die", "Pracht", "der", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Aller Glanz verdunkelt,", "tokens": ["Al\u00b7ler", "Glanz", "ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der auf Erden funkelt,", "tokens": ["Der", "auf", "Er\u00b7den", "fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sonn' und Sonnenstrahl,", "tokens": ["Sonn'", "und", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jaspis und Rubinen,", "tokens": ["Jas\u00b7pis", "und", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Spielend Feu'r in ihnen,", "tokens": ["Spie\u00b7lend", "Feu'r", "in", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Morgenthau im Thal.", "tokens": ["Mor\u00b7gent\u00b7hau", "im", "Thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Kronenpracht", "tokens": ["Kro\u00b7nen\u00b7pracht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Verl\u00f6scht, wie Nacht,", "tokens": ["Ver\u00b7l\u00f6scht", ",", "wie", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wenn der neuen Sonne Strahlen", "tokens": ["Wenn", "der", "neu\u00b7en", "Son\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Salems Pforten malen.", "tokens": ["Sa\u00b7lems", "Pfor\u00b7ten", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gott ist ihre Sonne,", "tokens": ["Gott", "ist", "ih\u00b7re", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist der Frommen Wonne,", "tokens": ["Ist", "der", "From\u00b7men", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und sie preisen ihn.", "tokens": ["Und", "sie", "prei\u00b7sen", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Lebensstr\u00f6m' ergie\u00dfen", "tokens": ["Le\u00b7bens\u00b7str\u00f6m'", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["VVIMP", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sich vom Stuhl und flie\u00dfen", "tokens": ["Sich", "vom", "Stuhl", "und", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ueber Goldsand hin.", "tokens": ["Ue\u00b7ber", "Gold\u00b7sand", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und vom Thron", "tokens": ["Und", "vom", "Thron"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Erschallt der Ton:", "tokens": ["Er\u00b7schallt", "der", "Ton", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Schauet, Gott in ihrer Mitte,", "tokens": ["Schau\u00b7et", ",", "Gott", "in", "ih\u00b7rer", "Mit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der Erl\u00f6sten H\u00fctte!", "tokens": ["Der", "Er\u00b7l\u00f6s\u00b7ten", "H\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Von dem neuen Lichte", "tokens": ["Von", "dem", "neu\u00b7en", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Trocknen im Gesichte", "tokens": ["Trock\u00b7nen", "im", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alle Thr\u00e4nen auf.", "tokens": ["Al\u00b7le", "Thr\u00e4\u00b7nen", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Laute Klagen schweigen;", "tokens": ["Lau\u00b7te", "Kla\u00b7gen", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Keine Seufzer steigen", "tokens": ["Kei\u00b7ne", "Seuf\u00b7zer", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Von der Erd' hinauf.", "tokens": ["Von", "der", "Erd'", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Gram und Noth,", "tokens": ["Gram", "und", "Noth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Geschrei und Tod,", "tokens": ["Ge\u00b7schrei", "und", "Tod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Blasse, ausgeweinte Wangen,", "tokens": ["Blas\u00b7se", ",", "aus\u00b7ge\u00b7wein\u00b7te", "Wan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alles ist vergangen.", "tokens": ["Al\u00b7les", "ist", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Alle Ueberwinder", "tokens": ["Al\u00b7le", "Ue\u00b7ber\u00b7win\u00b7der"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nenn' ich meine Kinder,", "tokens": ["Nenn'", "ich", "mei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich bin ihr Gott,", "tokens": ["Und", "ich", "bin", "ihr", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Alles, alles erben", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "er\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["PIS", "$,", "PIS", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Helden in dem Sterben,", "tokens": ["Hel\u00b7den", "in", "dem", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Christen in dem Tod.", "tokens": ["Chris\u00b7ten", "in", "dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Jedermann,", "tokens": ["Je\u00b7der\u00b7mann", ","], "token_info": ["word", "punct"], "pos": ["PIS", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wer k\u00e4mpfen kann,", "tokens": ["Wer", "k\u00e4mp\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Juden, K\u00f6nige und Heiden", "tokens": ["Ju\u00b7den", ",", "K\u00f6\u00b7ni\u00b7ge", "und", "Hei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gehn im Licht der Freuden!", "tokens": ["Gehn", "im", "Licht", "der", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Doch der Sklav der S\u00fcnden,", "tokens": ["Doch", "der", "Sklav", "der", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Feig zum Ueberwinden,", "tokens": ["Feig", "zum", "Ue\u00b7ber\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Erbt die Krone nicht.", "tokens": ["Erbt", "die", "Kro\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Finstere Verbrecher", "tokens": ["Fins\u00b7te\u00b7re", "Ver\u00b7bre\u00b7cher"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Fliehen vor dem R\u00e4cher,", "tokens": ["Flie\u00b7hen", "vor", "dem", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wandeln nicht im Licht.", "tokens": ["Wan\u00b7deln", "nicht", "im", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "S\u00fcnder, flieht!", "tokens": ["S\u00fcn\u00b7der", ",", "flieht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Die Rache gl\u00fcht,", "tokens": ["Die", "Ra\u00b7che", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und sie ruft mit ernstem Munde:", "tokens": ["Und", "sie", "ruft", "mit", "erns\u00b7tem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Erster! Letzter! komme!", "tokens": ["Ers\u00b7ter", "!", "Letz\u00b7ter", "!", "kom\u00b7me", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Siehe, wie die fromme", "tokens": ["Sie\u00b7he", ",", "wie", "die", "from\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ART", "ADJA"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.3": {"text": "Seele zu dir wallt!", "tokens": ["See\u00b7le", "zu", "dir", "wallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Doch die Ohren h\u00f6ren", "tokens": ["Doch", "die", "Oh\u00b7ren", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Harmonie der Sph\u00e4ren:", "tokens": ["Har\u00b7mo\u00b7nie", "der", "Sph\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schon erscheint", "tokens": ["Schon", "er\u00b7scheint"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Der Menschenfreund!", "tokens": ["Der", "Men\u00b7schen\u00b7freund", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Seele, nenne seinen Namen:", "tokens": ["See\u00b7le", ",", "nen\u00b7ne", "sei\u00b7nen", "Na\u00b7men", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Komm, ", "tokens": ["Komm", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Wallet sanft, ihr T\u00f6ne!", "tokens": ["Wal\u00b7let", "sanft", ",", "ihr", "T\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Salem gl\u00e4nzt, die Sch\u00f6ne,", "tokens": ["Sa\u00b7lem", "gl\u00e4nzt", ",", "die", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mir ins Angesicht.", "tokens": ["Mir", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Festlich strahlen dorten", "tokens": ["Fest\u00b7lich", "strah\u00b7len", "dor\u00b7ten"], "token_info": ["word", "wor