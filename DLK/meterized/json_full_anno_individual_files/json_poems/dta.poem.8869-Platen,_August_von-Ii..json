{"dta.poem.8869": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann, der willig trug der Liebe Kette,", "tokens": ["Ein", "Mann", ",", "der", "wil\u00b7lig", "trug", "der", "Lie\u00b7be", "Ket\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sang sie der verg\u00f6tterten Laurette,", "tokens": ["Er", "sang", "sie", "der", "ver\u00b7g\u00f6t\u00b7ter\u00b7ten", "Lau\u00b7ret\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Leben ihm und nach dem Leben theuer.", "tokens": ["Im", "Le\u00b7ben", "ihm", "und", "nach", "dem", "Le\u00b7ben", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und also sang auch manches Abenteuer,", "tokens": ["Und", "al\u00b7so", "sang", "auch", "man\u00b7ches", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In schmelzend musikalischem Sonette,", "tokens": ["In", "schmel\u00b7zend", "mu\u00b7si\u00b7ka\u00b7li\u00b7schem", "So\u00b7net\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Held, der einst durch wildes Wogenbette", "tokens": ["Ein", "Held", ",", "der", "einst", "durch", "wil\u00b7des", "Wo\u00b7gen\u00b7bet\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinem Liede schwamm, als seinem Steuer.", "tokens": ["Mit", "sei\u00b7nem", "Lie\u00b7de", "schwamm", ",", "als", "sei\u00b7nem", "Steu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Deutsche hat sich beygesellt, ein Dritter,", "tokens": ["Der", "Deut\u00b7sche", "hat", "sich", "bey\u00b7ge\u00b7sellt", ",", "ein", "Drit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Florentiner und dem Portugiesen,", "tokens": ["Dem", "Flo\u00b7ren\u00b7ti\u00b7ner", "und", "dem", "Por\u00b7tu\u00b7gie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sang geharnischte f\u00fcr k\u00fchne Ritter.", "tokens": ["Und", "sang", "ge\u00b7har\u00b7nischte", "f\u00fcr", "k\u00fch\u00b7ne", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf diese folg' ich, die sich gro\u00df erwiesen,", "tokens": ["Auf", "die\u00b7se", "fol\u00b7g'", "ich", ",", "die", "sich", "gro\u00df", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Nur wie ein Aehrenleser folgt dem Schnitter,", "tokens": ["Nur", "wie", "ein", "A\u00b7eh\u00b7ren\u00b7le\u00b7ser", "folgt", "dem", "Schnit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Denn nicht als Vierter wag' ich mich zu diesen.", "tokens": ["Denn", "nicht", "als", "Vier\u00b7ter", "wag'", "ich", "mich", "zu", "die\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOUS", "NN", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}