{"textgrid.poem.37830": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Buko von Halberstadt,", "tokens": ["Bu\u00b7ko", "von", "Hal\u00b7ber\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Bring doch meinem Kinde was,", "tokens": ["Bring", "doch", "mei\u00b7nem", "Kin\u00b7de", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PWS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was soll ich ihm bringen?", "tokens": ["Was", "soll", "ich", "ihm", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Rothe Schuh mit Ringen,", "tokens": ["Ro\u00b7the", "Schuh", "mit", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sch\u00f6ne Schuh mit Gold beschlagen,", "tokens": ["Sch\u00f6\u00b7ne", "Schuh", "mit", "Gold", "be\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die soll unser Kindchen tragen.", "tokens": ["Die", "soll", "un\u00b7ser", "Kind\u00b7chen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hurraso, Burra fort,", "tokens": ["Hur\u00b7ra\u00b7so", ",", "Bur\u00b7ra", "fort", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wagen und sch\u00f6n Schuh sind fort,", "tokens": ["Wa\u00b7gen", "und", "sch\u00f6n", "Schuh", "sind", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Stecken tief im Sumpfe,", "tokens": ["Ste\u00b7cken", "tief", "im", "Sump\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Pferde sind ertrunken,", "tokens": ["Pfer\u00b7de", "sind", "er\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hurra, schrei nicht Reitersknecht,", "tokens": ["Hur\u00b7ra", ",", "schrei", "nicht", "Rei\u00b7ters\u00b7knecht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Warum f\u00e4hrst du auch so schlecht!", "tokens": ["Wa\u00b7rum", "f\u00e4hrst", "du", "auch", "so", "schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Buko von Halberstadt,", "tokens": ["Bu\u00b7ko", "von", "Hal\u00b7ber\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "li