{"dta.poem.10518": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Habe auch, in jungen Jahren,", "tokens": ["Ha\u00b7be", "auch", ",", "in", "jun\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manches bitt're Leid erfahren", "tokens": ["Man\u00b7ches", "bitt'\u00b7re", "Leid", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Liebe Gluth.", "tokens": ["Von", "der", "Lie\u00b7be", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Doch das Holz ist gar zu theuer,", "tokens": ["Doch", "das", "Holz", "ist", "gar", "zu", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erl\u00f6schen will das Feuer,", "tokens": ["Und", "er\u00b7l\u00f6\u00b7schen", "will", "das", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das bedenke, junge Sch\u00f6ne,", "tokens": ["Das", "be\u00b7den\u00b7ke", ",", "jun\u00b7ge", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schicke fort die dumme Thr\u00e4ne,", "tokens": ["Schi\u00b7cke", "fort", "die", "dum\u00b7me", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den dummen Liebesharm.", "tokens": ["Und", "den", "dum\u00b7men", "Lie\u00b7bes\u00b7harm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das Leben dir geblieben,", "tokens": ["Ist", "das", "Le\u00b7ben", "dir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So vergi\u00df das alte Lieben,", "tokens": ["So", "ver\u00b7gi\u00df", "das", "al\u00b7te", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}