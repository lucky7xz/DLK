{"dta.poem.13505": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es geht wohl anders, als du meinst,", "tokens": ["Es", "geht", "wohl", "an\u00b7ders", ",", "als", "du", "meinst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Derweil du roth und fr\u00f6hlich scheinst", "tokens": ["Der\u00b7weil", "du", "roth", "und", "fr\u00f6h\u00b7lich", "scheinst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Lenz und Sonnenschein verflogen,", "tokens": ["Ist", "Lenz", "und", "Son\u00b7nen\u00b7schein", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die liebe Gegend schwarz umzogen;", "tokens": ["Die", "lie\u00b7be", "Ge\u00b7gend", "schwarz", "um\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und kaum hast du dich ausgeweint,", "tokens": ["Und", "kaum", "hast", "du", "dich", "aus\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lacht Alles wieder, die Sonne scheint \u2014", "tokens": ["Lacht", "Al\u00b7les", "wie\u00b7der", ",", "die", "Son\u00b7ne", "scheint"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Es geht wohl anders als man meint.", "tokens": ["Es", "geht", "wohl", "an\u00b7ders", "als", "man", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}