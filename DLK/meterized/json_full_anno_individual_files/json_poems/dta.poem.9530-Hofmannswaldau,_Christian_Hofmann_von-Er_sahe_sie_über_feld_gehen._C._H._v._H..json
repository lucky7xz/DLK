{"dta.poem.9530": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er sahe sie \u00fcber feld gehen.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es gieng die Lesbia in einem sch\u00e4fer-kleide", "tokens": ["Es", "gieng", "die", "Les\u00b7bia", "in", "ei\u00b7nem", "sch\u00e4\u00b7fer\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als Hirtin/ wie es schien/ der seelen/ \u00fcber feld/", "tokens": ["Als", "Hir\u00b7tin", "/", "wie", "es", "schien", "/", "der", "see\u00b7len", "/", "\u00fc\u00b7ber", "feld", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "ART", "NN", "$(", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es schaute sie mit lust das auge dieser welt/", "tokens": ["Es", "schau\u00b7te", "sie", "mit", "lust", "das", "au\u00b7ge", "die\u00b7ser", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "VVFIN", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es neigte sich vor ihr das tr\u00e4chtige getraide;", "tokens": ["Es", "neig\u00b7te", "sich", "vor", "ihr", "das", "tr\u00e4ch\u00b7ti\u00b7ge", "ge\u00b7trai\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es kriegte meine lust auch wieder neue weyde", "tokens": ["Es", "krieg\u00b7te", "mei\u00b7ne", "lust", "auch", "wie\u00b7der", "neu\u00b7e", "wey\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von wegen dieser brust/ da Venus wache h\u00e4lt;", "tokens": ["Von", "we\u00b7gen", "die\u00b7ser", "brust", "/", "da", "Ve\u00b7nus", "wa\u00b7che", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "$(", "KOUS", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der schultern/ wo sich zeigt der liebligkeiten feld", "tokens": ["Der", "schul\u00b7tern", "/", "wo", "sich", "zeigt", "der", "lieb\u00b7lig\u00b7kei\u00b7ten", "feld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVINF", "$(", "PWAV", "PRF", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dann der sch\u00f6nen schoos/ des hafens aller freude/", "tokens": ["Und", "dann", "der", "sch\u00f6\u00b7nen", "schoos", "/", "des", "ha\u00b7fens", "al\u00b7ler", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sprach: ach Lesbta! wie zierlich geht dein fu\u00df/", "tokens": ["Ich", "sprach", ":", "ach", "Lesb\u00b7ta", "!", "wie", "zier\u00b7lich", "geht", "dein", "fu\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "XY", "NE", "$.", "PWAV", "ADJD", "VVFIN", "PPOSAT", "PTKVZ", "$("], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Da\u00df Juno/ wie mich daucht/ sich selbst entf\u00e4rben mu\u00df/", "tokens": ["Da\u00df", "Ju\u00b7no", "/", "wie", "mich", "daucht", "/", "sich", "selbst", "ent\u00b7f\u00e4r\u00b7ben", "mu\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "PWAV", "PPER", "VVFIN", "$(", "PRF", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und Ph\u00f6bus dich zu sehn verj\u00fcngt die alte kertze;", "tokens": ["Und", "Ph\u00f6\u00b7bus", "dich", "zu", "sehn", "ver\u00b7j\u00fcngt", "die", "al\u00b7te", "kert\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "PTKZU", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Richt glaube Lesbia/ da\u00df du den boden r\u00fchrst/", "tokens": ["Richt", "glau\u00b7be", "Les\u00b7bia", "/", "da\u00df", "du", "den", "bo\u00b7den", "r\u00fchrst", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und den geschwinden fu\u00df auf gra\u00df und blumen f\u00fchrst/", "tokens": ["Und", "den", "ge\u00b7schwin\u00b7den", "fu\u00df", "auf", "gra\u00df", "und", "blu\u00b7men", "f\u00fchrst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PTKVZ", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es geht ein ieder tritt auf mein verwundtes hertze.", "tokens": ["Es", "geht", "ein", "ie\u00b7der", "tritt", "auf", "mein", "ver\u00b7wund\u00b7tes", "hert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}