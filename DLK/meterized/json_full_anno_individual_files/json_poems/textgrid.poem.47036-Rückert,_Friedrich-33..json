{"textgrid.poem.47036": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, ein seltsam Spiel hast du begonnen", "tokens": ["Mein", "Kind", ",", "ein", "selt\u00b7sam", "Spiel", "hast", "du", "be\u00b7gon\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJD", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier mit dem wehrlos ausgestreckten Linnen;", "tokens": ["Hier", "mit", "dem", "wehr\u00b7los", "aus\u00b7ge\u00b7streck\u00b7ten", "Lin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wahrlich, wenn es h\u00e4tte Menschensinnen,", "tokens": ["Und", "wahr\u00b7lich", ",", "wenn", "es", "h\u00e4t\u00b7te", "Men\u00b7schen\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "M\u00fc\u00dft's ihm ein Spiel sein recht zu Weh und Wonnen;", "tokens": ["M\u00fc\u00dft's", "ihm", "ein", "Spiel", "sein", "recht", "zu", "Weh", "und", "Won\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PPOSAT", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie du ihm bald gebietest, sich zu sonnen,", "tokens": ["Wie", "du", "ihm", "bald", "ge\u00b7bie\u00b7test", ",", "sich", "zu", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald kalte Fluten dr\u00fcber l\u00e4ssest rinnen,", "tokens": ["Bald", "kal\u00b7te", "Flu\u00b7ten", "dr\u00fc\u00b7ber", "l\u00e4s\u00b7sest", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PAV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald wieder sonnst das Flutenna\u00df von hinnen,", "tokens": ["Bald", "wie\u00b7der", "sonnst", "das", "Flu\u00b7ten\u00b7na\u00df", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald wieder tilgst die Glut mit neuen Bronnen.", "tokens": ["Bald", "wie\u00b7der", "tilgst", "die", "Glut", "mit", "neu\u00b7en", "Bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Kind, wenn Sonnen gleich sind deine Blicke,", "tokens": ["Mein", "Kind", ",", "wenn", "Son\u00b7nen", "gleich", "sind", "dei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und deines Mundes Gr\u00fc\u00dfe gleich den Fluten,", "tokens": ["Und", "dei\u00b7nes", "Mun\u00b7des", "Gr\u00fc\u00b7\u00dfe", "gleich", "den", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wei\u00df ich, da\u00df ich selbst dem Linnen gleiche;", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "ich", "selbst", "dem", "Lin\u00b7nen", "glei\u00b7che", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da du mich sonnend gl\u00fchst auf Augenblicke,", "tokens": ["Da", "du", "mich", "son\u00b7nend", "gl\u00fchst", "auf", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann ach, durch kaltes Wort mir k\u00fchlst die Gluten,", "tokens": ["Dann", "ach", ",", "durch", "kal\u00b7tes", "Wort", "mir", "k\u00fchlst", "die", "Glu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ADJA", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So da\u00df, wie jenes bleicht, ich selbst erbleiche.", "tokens": ["So", "da\u00df", ",", "wie", "je\u00b7nes", "bleicht", ",", "ich", "selbst", "er\u00b7blei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "PWAV", "PDS", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mein