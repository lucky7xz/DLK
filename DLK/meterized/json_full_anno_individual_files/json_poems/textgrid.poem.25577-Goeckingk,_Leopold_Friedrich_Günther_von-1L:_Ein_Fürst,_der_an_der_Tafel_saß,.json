{"textgrid.poem.25577": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein F\u00fcrst, der an der Tafel sa\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein F\u00fcrst, der an der Tafel sa\u00df,", "tokens": ["Ein", "F\u00fcrst", ",", "der", "an", "der", "Ta\u00b7fel", "sa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, w\u00e4hrend er von einer Creme a\u00df,", "tokens": ["Und", ",", "w\u00e4h\u00b7rend", "er", "von", "ei\u00b7ner", "Cre\u00b7me", "a\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In einem B\u00fcchelchen, das neben", "tokens": ["In", "ei\u00b7nem", "B\u00fc\u00b7chel\u00b7chen", ",", "das", "ne\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Teller lag, mitunter las,", "tokens": ["Dem", "Tel\u00b7ler", "lag", ",", "mi\u00b7tun\u00b7ter", "las", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Rief aus: Ein herrlich Epigramm!", "tokens": ["Rief", "aus", ":", "Ein", "herr\u00b7lich", "E\u00b7pig\u00b7ramm", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die\u00df fremde Wort merkt sich gar eben", "tokens": ["Die\u00df", "frem\u00b7de", "Wort", "merkt", "sich", "gar", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "PRF", "ADV", "ADV"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Ein Herr aus altem deutschen Stamm',", "tokens": ["Ein", "Herr", "aus", "al\u00b7tem", "deut\u00b7schen", "Stamm'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beschreibt, so gut er kann, zu Hause seinem Koche", "tokens": ["Be\u00b7schreibt", ",", "so", "gut", "er", "kann", ",", "zu", "Hau\u00b7se", "sei\u00b7nem", "Ko\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Crem', und spricht: Nun schaff' mir diese Woche", "tokens": ["Die", "Crem'", ",", "und", "spricht", ":", "Nun", "schaff'", "mir", "die\u00b7se", "Wo\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auch solch ein herrlich Epigramm.", "tokens": ["Auch", "solch", "ein", "herr\u00b7lich", "E\u00b7pig\u00b7ramm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein F\u00fcrst, der an der Tafel sa\u00