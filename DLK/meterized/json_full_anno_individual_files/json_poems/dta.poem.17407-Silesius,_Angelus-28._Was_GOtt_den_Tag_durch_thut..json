{"dta.poem.17407": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "28.  Was GOtt den Tag durch thut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Des Morgens geht GOtt au\u00df/ zu mittag schl\u00e4ffet er/", "tokens": ["Des", "Mor\u00b7gens", "geht", "Gott", "au\u00df", "/", "zu", "mit\u00b7tag", "schl\u00e4f\u00b7fet", "er", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "NN", "PTKVZ", "$(", "APPR", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "De\u00df Nachts ist er erwacht/ reist\u2019 Abends ohn beschwehr.", "tokens": ["De\u00df", "Nachts", "ist", "er", "er\u00b7wacht", "/", "reist'", "A\u00b7bends", "ohn", "be\u00b7schwehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "PPER", "VVPP", "$(", "VVFIN", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}