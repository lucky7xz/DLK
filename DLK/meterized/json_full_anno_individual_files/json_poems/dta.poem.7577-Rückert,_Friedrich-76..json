{"dta.poem.7577": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des ganzen Menschen und des einzelnen Geschichte,", "tokens": ["Des", "gan\u00b7zen", "Men\u00b7schen", "und", "des", "ein\u00b7zel\u00b7nen", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zusammenfassen kannst du sie in drei Berichte:", "tokens": ["Zu\u00b7sam\u00b7men\u00b7fas\u00b7sen", "kannst", "du", "sie", "in", "drei", "Be\u00b7rich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Mensch, mit der Natur im Frieden, war ein Kind;", "tokens": ["Der", "Mensch", ",", "mit", "der", "Na\u00b7tur", "im", "Frie\u00b7den", ",", "war", "ein", "Kind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sind die Gl\u00fccklichen, die es geblieben sind.", "tokens": ["Das", "sind", "die", "Gl\u00fcck\u00b7li\u00b7chen", ",", "die", "es", "ge\u00b7blie\u00b7ben", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Mensch, mit der Natur im Kampfe, ward ein Mann,", "tokens": ["Der", "Mensch", ",", "mit", "der", "Na\u00b7tur", "im", "Kamp\u00b7fe", ",", "ward", "ein", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewann, verlor, gewann, verlor, gewann, gewann.", "tokens": ["Ge\u00b7wann", ",", "ver\u00b7lor", ",", "ge\u00b7wann", ",", "ver\u00b7lor", ",", "ge\u00b7wann", ",", "ge\u00b7wann", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Mensch mit der Natur Besiegung wird ein Greis,", "tokens": ["Der", "Mensch", "mit", "der", "Na\u00b7tur", "Be\u00b7sie\u00b7gung", "wird", "ein", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des neuen Friedens Kind; so kreist in sich der Kreis.", "tokens": ["Des", "neu\u00b7en", "Frie\u00b7dens", "Kind", ";", "so", "kreist", "in", "sich", "der", "Kreis", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "ADV", "ADJD", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}