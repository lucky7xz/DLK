{"textgrid.poem.56371": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod von Otto von Pirch", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen birgt da unten tief die schwarze Truhe", "tokens": ["Wen", "birgt", "da", "un\u00b7ten", "tief", "die", "schwar\u00b7ze", "Tru\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die von dem Fall der Erde dumpf erschallt?", "tokens": ["Die", "von", "dem", "Fall", "der", "Er\u00b7de", "dumpf", "er\u00b7schallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sagt, welchen M\u00fcden legt ihr da zur Ruhe? \u2013", "tokens": ["Sagt", ",", "wel\u00b7chen", "M\u00fc\u00b7den", "legt", "ihr", "da", "zur", "Ru\u00b7he", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PWAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Pirch. \u2013 Ihr l\u00fcgt! gar lebensfreudig wallt,", "tokens": ["Von", "Pirch", ".", "\u2013", "Ihr", "l\u00fcgt", "!", "gar", "le\u00b7bens\u00b7freu\u00b7dig", "wallt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "$(", "PPER", "VVFIN", "$.", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich sah ihn gestern noch im Tagesscheine,", "tokens": ["Ich", "sah", "ihn", "ge\u00b7stern", "noch", "im", "Ta\u00b7ges\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die kr\u00e4ft'ge, jugendstrahlende Gestalt \u2013", "tokens": ["Die", "kr\u00e4ft'\u00b7ge", ",", "ju\u00b7gend\u00b7strah\u00b7len\u00b7de", "Ge\u00b7stalt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da liegt er bleich und kalt im engen Schreine. \u2013", "tokens": ["Da", "liegt", "er", "bleich", "und", "kalt", "im", "en\u00b7gen", "Schrei\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er sollt es sein?? \u2013 Er ist's, den wir begraben. \u2013", "tokens": ["Er", "sollt", "es", "sein", "??", "\u2013", "Er", "ist's", ",", "den", "wir", "be\u00b7gra\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VAINF", "$.", "$(", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Edle, Tapfre, Weise, Fromme, Reine!", "tokens": ["Der", "Ed\u00b7le", ",", "Tapf\u00b7re", ",", "Wei\u00b7se", ",", "From\u00b7me", ",", "Rei\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Er, welchen schm\u00fcckten alle h\u00f6hern Gaben,", "tokens": ["Er", ",", "wel\u00b7chen", "schm\u00fcck\u00b7ten", "al\u00b7le", "h\u00f6\u00b7hern", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAT", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Den wir ein Muster aller T\u00fcchtigkeit", "tokens": ["Den", "wir", "ein", "Mus\u00b7ter", "al\u00b7ler", "T\u00fcch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Geehrt vor allen und geliebet haben.", "tokens": ["Ge\u00b7ehrt", "vor", "al\u00b7len", "und", "ge\u00b7lie\u00b7bet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Er, den in dieser d\u00fcnkelhaften Zeit", "tokens": ["Er", ",", "den", "in", "die\u00b7ser", "d\u00fcn\u00b7kel\u00b7haf\u00b7ten", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der Reiz der Demut zierte wunderbar,", "tokens": ["Der", "Reiz", "der", "De\u00b7mut", "zier\u00b7te", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dem Bessern stets zu huldigen bereit.", "tokens": ["Dem", "Bes\u00b7sern", "stets", "zu", "hul\u00b7di\u00b7gen", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der wie ein Held, der wie ein Kind auch war,", "tokens": ["Der", "wie", "ein", "Held", ",", "der", "wie", "ein", "Kind", "auch", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Der.... O mein Pirch! du bist dahin gegangen,", "tokens": ["Der", "....", "O", "mein", "Pirch", "!", "du", "bist", "da\u00b7hin", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NE", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ich aber sch\u00fcttle noch mein greises Haar.", "tokens": ["Ich", "a\u00b7ber", "sch\u00fctt\u00b7le", "noch", "mein", "grei\u00b7ses", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dein klares Aug und deine frischen Wangen,", "tokens": ["Dein", "kla\u00b7res", "Aug", "und", "dei\u00b7ne", "fri\u00b7schen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Dein Bild wird, der Verg\u00e4nglichkeit entrafft,", "tokens": ["Dein", "Bild", "wird", ",", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "en\u00b7trafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Stets jugendhell vor meiner Seele prangen.", "tokens": ["Stets", "ju\u00b7gend\u00b7hell", "vor", "mei\u00b7ner", "See\u00b7le", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Das Alter aber zehrt an meiner Kraft,", "tokens": ["Das", "Al\u00b7ter", "a\u00b7ber", "zehrt", "an", "mei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der Lenz erweckt in mir den alten nicht,", "tokens": ["Der", "Lenz", "er\u00b7weckt", "in", "mir", "den", "al\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PPER", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Da pr\u00fcf ich mich, da f\u00fchl ich mich erschlafft.", "tokens": ["Da", "pr\u00fcf", "ich", "mich", ",", "da", "f\u00fchl", "ich", "mich", "er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Es zieht ein Nebelflor vor mein Gesicht,", "tokens": ["Es", "zieht", "ein", "Ne\u00b7bel\u00b7flor", "vor", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Von meinem Ohr entfernen sich die T\u00f6ne;", "tokens": ["Von", "mei\u00b7nem", "Ohr", "ent\u00b7fer\u00b7nen", "sich", "die", "T\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ich merke, wie der Bau zusammenbricht.", "tokens": ["Ich", "mer\u00b7ke", ",", "wie", "der", "Bau", "zu\u00b7sam\u00b7men\u00b7bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Dich nahm der Tod in deiner vollen Sch\u00f6ne,", "tokens": ["Dich", "nahm", "der", "Tod", "in", "dei\u00b7ner", "vol\u00b7len", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Du f\u00fchltest nicht dich sterben St\u00fcck f\u00fcr St\u00fcck,", "tokens": ["Du", "f\u00fchl\u00b7test", "nicht", "dich", "ster\u00b7ben", "St\u00fcck", "f\u00fcr", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Wie andre morsch gewordne Menschens\u00f6hne.", "tokens": ["Wie", "and\u00b7re", "morsch", "ge\u00b7word\u00b7ne", "Men\u00b7schen\u00b7s\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Dir war das Leben Hoffnung nur und Gl\u00fcck,", "tokens": ["Dir", "war", "das", "Le\u00b7ben", "Hoff\u00b7nung", "nur", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Entt\u00e4uschung hat es nimmer dir verg\u00e4llt;", "tokens": ["Ent\u00b7t\u00e4u\u00b7schung", "hat", "es", "nim\u00b7mer", "dir", "ver\u00b7g\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Wir aber rufen schmerzlich dich zur\u00fcck.", "tokens": ["Wir", "a\u00b7ber", "ru\u00b7fen", "schmerz\u00b7lich", "dich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Denn alt geworden ist um uns die Welt,", "tokens": ["Denn", "alt", "ge\u00b7wor\u00b7den", "ist", "um", "uns", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAPP", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Es gleicht, was noch besteht, dem letzten Traum", "tokens": ["Es", "gleicht", ",", "was", "noch", "be\u00b7steht", ",", "dem", "letz\u00b7ten", "Traum"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Zur Stunde, wo der Osten sich erhellt.", "tokens": ["Zur", "Stun\u00b7de", ",", "wo", "der", "Os\u00b7ten", "sich", "er\u00b7hellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Es tragen sich die morschen Pfeiler kaum,", "tokens": ["Es", "tra\u00b7gen", "sich", "die", "mor\u00b7schen", "Pfei\u00b7ler", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Der Boden wankt, der Glauben ist verloren,", "tokens": ["Der", "Bo\u00b7den", "wankt", ",", "der", "Glau\u00b7ben", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Tiar'- und Kronengold ist eitel Schaum.", "tokens": ["Ti\u00b7a\u00b7r'", "und", "Kro\u00b7nen\u00b7gold", "ist", "ei\u00b7tel", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NE", "VAFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.40": {"text": "Dem Alten ist der Untergang geschworen,", "tokens": ["Dem", "Al\u00b7ten", "ist", "der", "Un\u00b7ter\u00b7gang", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Verwesung greift um sich, die Stoffe g\u00e4ren,", "tokens": ["Ver\u00b7we\u00b7sung", "greift", "um", "sich", ",", "die", "Stof\u00b7fe", "g\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PRF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Im Schmerze wird die neue Zeit geboren;", "tokens": ["Im", "Schmer\u00b7ze", "wird", "die", "neu\u00b7e", "Zeit", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Sie wird nach M\u00e4nnern so wie du, begehren.", "tokens": ["Sie", "wird", "nach", "M\u00e4n\u00b7nern", "so", "wie", "du", ",", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "KOKOM", "PPER", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wen birgt da unten tief die schwarze Truhe", "tokens": ["Wen", "birgt", "da", "un\u00b7ten", "tief", "die", "schwar\u00b7ze", "Tru\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die von dem Fall der Erde dumpf erschallt?", "tokens": ["Die", "von", "dem", "Fall", "der", "Er\u00b7de", "dumpf", "er\u00b7schallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sagt, welchen M\u00fcden legt ihr da zur Ruhe? \u2013", "tokens": ["Sagt", ",", "wel\u00b7chen", "M\u00fc\u00b7den", "legt", "ihr", "da", "zur", "Ru\u00b7he", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PWAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+