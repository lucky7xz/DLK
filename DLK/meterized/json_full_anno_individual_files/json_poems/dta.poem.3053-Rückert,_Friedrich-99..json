{"dta.poem.3053": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Statt dich zu zanken mit den eigenen Gedanken,", "tokens": ["Statt", "dich", "zu", "zan\u00b7ken", "mit", "den", "ei\u00b7ge\u00b7nen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ist dirs zuweilen gut mit fremden dich zu zanken.", "tokens": ["Ist", "dirs", "zu\u00b7wei\u00b7len", "gut", "mit", "frem\u00b7den", "dich", "zu", "zan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "APPR", "ADJA", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar kommt so wenig auch bei diesem Zank heraus", "tokens": ["Zwar", "kommt", "so", "we\u00b7nig", "auch", "bei", "die\u00b7sem", "Zank", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im fremden, als bei dem in deinem eignen Haus.", "tokens": ["Im", "frem\u00b7den", ",", "als", "bei", "dem", "in", "dei\u00b7nem", "eig\u00b7nen", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KOUS", "APPR", "ART", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn mit fremden du dich recht herumgeschlagen,", "tokens": ["Doch", "wenn", "mit", "frem\u00b7den", "du", "dich", "recht", "her\u00b7um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du villeicht dich mit den eignen ehr vertragen.", "tokens": ["Wirst", "du", "vil\u00b7leicht", "dich", "mit", "den", "eig\u00b7nen", "ehr", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mit deinen eigenen Gedanken leb' in Frieden!", "tokens": ["Mit", "dei\u00b7nen", "ei\u00b7ge\u00b7nen", "Ge\u00b7dan\u00b7ken", "leb'", "in", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn, ist er nicht in dir, wo ist der Fried' hienieden?", "tokens": ["Denn", ",", "ist", "er", "nicht", "in", "dir", ",", "wo", "ist", "der", "Fried'", "hien\u00b7ie\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "PWAV", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}