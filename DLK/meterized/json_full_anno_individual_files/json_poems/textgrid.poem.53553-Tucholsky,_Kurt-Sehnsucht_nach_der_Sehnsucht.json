{"textgrid.poem.53553": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach der Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst wollte ich mich dir in Keuschheit nahn.", "tokens": ["Erst", "woll\u00b7te", "ich", "mich", "dir", "in", "Keuschheit", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Kette schmolz.", "tokens": ["Die", "Ket\u00b7te", "schmolz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich bin doch schlie\u00dflich, schlie\u00dflich auch ein Mann,", "tokens": ["Ich", "bin", "doch", "schlie\u00df\u00b7lich", ",", "schlie\u00df\u00b7lich", "auch", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und nicht von Holz.", "tokens": ["und", "nicht", "von", "Holz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Mai ist da. Der Vogel Pirol pfeift.", "tokens": ["Der", "Mai", "ist", "da", ".", "Der", "Vo\u00b7gel", "Pi\u00b7rol", "pfeift", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ART", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es geht 