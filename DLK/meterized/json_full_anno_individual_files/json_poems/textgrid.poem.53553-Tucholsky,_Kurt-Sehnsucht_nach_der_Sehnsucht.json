er": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Es geht nicht, wenn die linde Luft weht und", "tokens": ["Es", "geht", "nicht", ",", "wenn", "die", "lin\u00b7de", "Luft", "weht", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die Amsel singt \u2013", "tokens": ["die", "Am\u00b7sel", "singt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "wir brauchen alle einen roten Mund,", "tokens": ["wir", "brau\u00b7chen", "al\u00b7le", "ei\u00b7nen", "ro\u00b7ten", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der uns beschwingt.", "tokens": ["der", "uns", "be\u00b7schwingt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wir brauchen alle etwas, das das Blut", "tokens": ["Wir", "brau\u00b7chen", "al\u00b7le", "et\u00b7was", ",", "das", "das", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "rasch vorw\u00e4rtstreibt \u2013", "tokens": ["rasch", "vor\u00b7w\u00e4rt\u00b7streibt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "es dichtet sich doch noch einmal so gut,", "tokens": ["es", "dich\u00b7tet", "sich", "doch", "noch", "ein\u00b7mal", "so", "gut", ","], "token_in