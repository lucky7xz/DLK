{"dta.poem.6806": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will man ein St\u00e4ubchen, so vorhin nicht sichtbar, sehn:", "tokens": ["Will", "man", "ein", "St\u00e4ub\u00b7chen", ",", "so", "vor\u00b7hin", "nicht", "sicht\u00b7bar", ",", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man schau es durch ein Glas! es wird entstehn,", "tokens": ["Man", "schau", "es", "durch", "ein", "Glas", "!", "es", "wird", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja, es vergr\u00f6ssert sich, wird wie ein Berg gestallt,", "tokens": ["Ja", ",", "es", "ver\u00b7gr\u00f6s\u00b7sert", "sich", ",", "wird", "wie", "ein", "Berg", "ge\u00b7stallt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PRF", "$,", "VAFIN", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und scheint der Riesen Aufenthalt.", "tokens": ["Und", "scheint", "der", "Rie\u00b7sen", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit einer dicken Haut mit Schuppen auf den R\u00fccken", "tokens": ["Mit", "ei\u00b7ner", "di\u00b7cken", "Haut", "mit", "Schup\u00b7pen", "auf", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan man selbst eine M\u00fclb\u2019 und eine Miet\u2019 erblicken,", "tokens": ["Kan", "man", "selbst", "ei\u00b7ne", "M\u00fclb'", "und", "ei\u00b7ne", "Miet'", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Fast dem Rinoceros und Elephanten gleich.", "tokens": ["Fast", "dem", "Ri\u00b7no\u00b7ce\u00b7ros", "und", "E\u00b7le\u00b7phan\u00b7ten", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "In einem Tr\u00f6pffchen Na\u00df (als wie in Thetis Reich)", "tokens": ["In", "ei\u00b7nem", "Tr\u00f6pffc\u00b7hen", "Na\u00df", "(", "als", "wie", "in", "The\u00b7tis", "Reich", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "KOUS", "KOKOM", "APPR", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vermag ein Glas uns zwischen regen Wellen", "tokens": ["Ver\u00b7mag", "ein", "Glas", "uns", "zwi\u00b7schen", "re\u00b7gen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Recht grosse Fische vorzustellen.", "tokens": ["Recht", "gros\u00b7se", "Fi\u00b7sche", "vor\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}