{"dta.poem.18219": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Weltgeschichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Duftend im Vollmond schwanken die Gr\u00e4ser;", "tokens": ["Duf\u00b7tend", "im", "Voll\u00b7mond", "schwan\u00b7ken", "die", "Gr\u00e4\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Alles schl\u00e4ft! Nur ein steinalter Mann", "tokens": ["Al\u00b7les", "schl\u00e4ft", "!", "Nur", "ein", "stei\u00b7nal\u00b7ter", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Putzt sich gesch\u00e4ftig die Brillengl\u00e4ser.", "tokens": ["Putzt", "sich", "ge\u00b7sch\u00e4f\u00b7tig", "die", "Bril\u00b7len\u00b7gl\u00e4\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nimmt sich ein Prieschen und sagt: H\u00e4tschi!", "tokens": ["Nimmt", "sich", "ein", "Prie\u00b7schen", "und", "sagt", ":", "H\u00e4t\u00b7schi", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "$.", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ich bin der achte der sieben Weisen!", "tokens": ["Ich", "bin", "der", "ach\u00b7te", "der", "sie\u00b7ben", "Wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ach, und er merkt es nicht einmal, wie", "tokens": ["Ach", ",", "und", "er", "merkt", "es", "nicht", "ein\u00b7mal", ",", "wie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PWAV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Ueber ihm leuchtend die Sterne kreisen!", "tokens": ["Ue\u00b7ber", "ihm", "leuch\u00b7tend", "die", "Ster\u00b7ne", "krei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sehns\u00fcchtig harft durch die Zweige der Wind,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "harft", "durch", "die", "Zwei\u00b7ge", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Bl\u00fcthen erschlie\u00dfen sich, Knospen schwellen;", "tokens": ["Bl\u00fc\u00b7then", "er\u00b7schlie\u00b7\u00dfen", "sich", ",", "Knos\u00b7pen", "schwel\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "$,", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Alles still! Nur der Nachtthau rinnt", "tokens": ["Al\u00b7les", "still", "!", "Nur", "der", "Nachtt\u00b7hau", "rinnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "PTKVZ", "$.", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und von den Bergen her rauschen die Quellen.", "tokens": ["Und", "von", "den", "Ber\u00b7gen", "her", "rau\u00b7schen", "die", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APZR", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Raune nur traumhaft, du dunkle Natur,", "tokens": ["Rau\u00b7ne", "nur", "traum\u00b7haft", ",", "du", "dunk\u00b7le", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Raune das R\u00e4thsel der Elemente,", "tokens": ["Rau\u00b7ne", "das", "R\u00e4th\u00b7sel", "der", "E\u00b7le\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--++--+--", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Hat doch der alte Graukopf nur", "tokens": ["Hat", "doch", "der", "al\u00b7te", "Grau\u00b7kopf", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sinn f\u00fcr B\u00fccher und Pergamente!", "tokens": ["Sinn", "f\u00fcr", "B\u00fc\u00b7cher", "und", "Per\u00b7ga\u00b7men\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn er nur schn\u00fcffeln und b\u00fcffeln kann,", "tokens": ["Wenn", "er", "nur", "schn\u00fcf\u00b7feln", "und", "b\u00fcf\u00b7feln", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mag dreist dies Sonnensystem erkalten;", "tokens": ["Mag", "dreist", "dies", "Son\u00b7nen\u00b7sys\u00b7tem", "er\u00b7kal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihm ist's schon recht, denn was geht es ihn an,", "tokens": ["Ihm", "ist's", "schon", "recht", ",", "denn", "was", "geht", "es", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "PWS", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df sich die Welten wie Blumen entfalten?", "tokens": ["Da\u00df", "sich", "die", "Wel\u00b7ten", "wie", "Blu\u00b7men", "ent\u00b7fal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Festgeleimt an den Stuhl das Ges\u00e4\u00df,", "tokens": ["Fest\u00b7ge\u00b7leimt", "an", "den", "Stuhl", "das", "Ge\u00b7s\u00e4\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "F\u00e4ngt er sich Grillen und m\u00e4stet sich Motten,", "tokens": ["F\u00e4ngt", "er", "sich", "Gril\u00b7len", "und", "m\u00e4s\u00b7tet", "sich", "Mot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NN", "KON", "VVFIN", "PRF", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "H\u00fcstelt und schreibt gelehrte Essays", "tokens": ["H\u00fcs\u00b7telt", "und", "schreibt", "ge\u00b7lehr\u00b7te", "Es\u00b7says"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ueber Assyrer und Hottentotten.", "tokens": ["Ue\u00b7ber", "As\u00b7sy\u00b7rer", "und", "Hot\u00b7ten\u00b7tot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Tintenf\u00e4sser bilden Spalier,", "tokens": ["Tin\u00b7ten\u00b7f\u00e4s\u00b7ser", "bil\u00b7den", "Spa\u00b7lier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Goldstreusand und Radiermesser blinken,", "tokens": ["Gold\u00b7streu\u00b7sand", "und", "Ra\u00b7dier\u00b7mes\u00b7ser", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ganze Ballen von Schreibpapier", "tokens": ["Gan\u00b7ze", "Bal\u00b7len", "von", "Schreib\u00b7pa\u00b7pier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Liegen bekritzelt ihm schon zur Linken.", "tokens": ["Lie\u00b7gen", "be\u00b7krit\u00b7zelt", "ihm", "schon", "zur", "Lin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "S\u00e4uberlich hat er drin aufnotirt", "tokens": ["S\u00e4u\u00b7ber\u00b7lich", "hat", "er", "drin", "auf\u00b7no\u00b7tirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Jede Schlacht und jedes Gemetzel,", "tokens": ["Je\u00b7de", "Schlacht", "und", "je\u00b7des", "Ge\u00b7met\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Neben Napoleon figurirt", "tokens": ["Ne\u00b7ben", "Na\u00b7po\u00b7le\u00b7on", "fi\u00b7gu\u00b7rirt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Kaiser Tiber und der Hunnenchan Etzel.", "tokens": ["Kai\u00b7ser", "Ti\u00b7ber", "und", "der", "Hun\u00b7nen\u00b7chan", "Et\u00b7zel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Ekelerregend mit jedem Band", "tokens": ["E\u00b7ke\u00b7ler\u00b7re\u00b7gend", "mit", "je\u00b7dem", "Band"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PIAT", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schwillt das Gemengsel von Blut, Fleisch und Knochen;", "tokens": ["Schwillt", "das", "Ge\u00b7men\u00b7gsel", "von", "Blut", ",", "Fleisch", "und", "Kno\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Leute wie Sokrates, Shakesspeare und Kant", "tokens": ["Leu\u00b7te", "wie", "Sok\u00b7ra\u00b7tes", ",", "Sha\u00b7kes\u00b7spea\u00b7re", "und", "Kant"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "NE", "$,", "NE", "KON", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Werden nur so nebenbei besprochen.", "tokens": ["Wer\u00b7den", "nur", "so", "ne\u00b7ben\u00b7bei", "be\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Weltharmonie und Sph\u00e4renmusik", "tokens": ["Welt\u00b7har\u00b7mo\u00b7nie", "und", "Sph\u00e4\u00b7ren\u00b7mu\u00b7sik"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "K\u00f6nnen ihm vollends gestohlen bleiben;", "tokens": ["K\u00f6n\u00b7nen", "ihm", "vol\u00b7lends", "ge\u00b7stoh\u00b7len", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Interessanter ist schon die Rubrik,", "tokens": ["In\u00b7ter\u00b7es\u00b7san\u00b7ter", "ist", "schon", "die", "Rub\u00b7rik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wie sich die Kaiser von China entleiben!", "tokens": ["Wie", "sich", "die", "Kai\u00b7ser", "von", "Chi\u00b7na", "ent\u00b7lei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Also sitzt er und schmiert und schmiert", "tokens": ["Al\u00b7so", "sitzt", "er", "und", "schmiert", "und", "schmiert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Todte Zahlen und trockne Berichte,", "tokens": ["Tod\u00b7te", "Zah\u00b7len", "und", "trock\u00b7ne", "Be\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bis er dann endlich \u201eSchlu\u00df\u201c drunter kliert", "tokens": ["Bis", "er", "dann", "end\u00b7lich", "\u201e", "Schlu\u00df", "\u201c", "drun\u00b7ter", "kliert"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$(", "NN", "$(", "PAV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und auf das Titelblatt: \u201eWeltgeschichte\u201c.", "tokens": ["Und", "auf", "das", "Ti\u00b7tel\u00b7blatt", ":", "\u201e", "Welt\u00b7ge\u00b7schich\u00b7te", "\u201c", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$.", "$(", "NN", "$(", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Weltgeschichte! O blutiger Hohn!", "tokens": ["Welt\u00b7ge\u00b7schich\u00b7te", "!", "O", "blu\u00b7ti\u00b7ger", "Hohn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Uralter Hymnus auf die Bornirtheit!", "tokens": ["Ur\u00b7al\u00b7ter", "Hym\u00b7nus", "auf", "die", "Bor\u00b7nirt\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wann, o wann kommt des Menschen Sohn,", "tokens": ["Wann", ",", "o", "wann", "kommt", "des", "Men\u00b7schen", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "PWAV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der dich erl\u00f6st aus deiner Verthiertheit?", "tokens": ["Der", "dich", "er\u00b7l\u00f6st", "aus", "dei\u00b7ner", "Ver\u00b7thier\u00b7theit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Immer noch br\u00fctet die alte Nacht", "tokens": ["Im\u00b7mer", "noch", "br\u00fc\u00b7tet", "die", "al\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Grauenvoll \u00fcber den V\u00f6lkern der Erde,", "tokens": ["Grau\u00b7en\u00b7voll", "\u00fc\u00b7ber", "den", "V\u00f6l\u00b7kern", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Aber schon seh ich rothlodernd entfacht", "tokens": ["A\u00b7ber", "schon", "seh", "ich", "rot\u00b7hlo\u00b7dernd", "ent\u00b7facht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "VVPP"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Flammen des Geistes auf ewigem Herde.", "tokens": ["Flam\u00b7men", "des", "Geis\u00b7tes", "auf", "e\u00b7wi\u00b7gem", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA"