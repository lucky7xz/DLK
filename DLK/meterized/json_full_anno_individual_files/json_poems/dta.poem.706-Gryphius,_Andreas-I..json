{"dta.poem.706": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wje seelig ist der hohe Geist zu sch\u00e4tzen/ ", "tokens": ["Wie", "see\u00b7lig", "ist", "der", "ho\u00b7he", "Geist", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der de\u00df geschminckten Gl\u00fcckes falsche Pracht", "tokens": ["Der", "de\u00df", "ge\u00b7schminck\u00b7ten", "Gl\u00fc\u00b7ckes", "fal\u00b7sche", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vnd was beth\u00f6rte ", "tokens": ["Vnd", "was", "be\u00b7th\u00f6r\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit sorg vnd kummer freyem Mutt verlacht!", "tokens": ["Mit", "sorg", "vnd", "kum\u00b7mer", "frey\u00b7em", "Mutt", "ver\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem kein verzagen/", "tokens": ["Dem", "kein", "ver\u00b7za\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Das Hertz zubricht", "tokens": ["Das", "Hertz", "zu\u00b7bricht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den auch kein klagen ", "tokens": ["Den", "auch", "kein", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Noch hohn anficht/", "tokens": ["Noch", "hohn", "an\u00b7ficht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Noch Neydt ansticht.", "tokens": ["Noch", "Neydt", "an\u00b7sticht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Er tritt was alles tritt mit steiffen ", "tokens": ["Er", "tritt", "was", "al\u00b7les", "tritt", "mit", "steif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "PIS", "VVFIN", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herrscht \u00fcber sich vnd pocht der Menschen Noth", "tokens": ["Herrscht", "\u00fc\u00b7ber", "sich", "vnd", "pocht", "der", "Men\u00b7schen", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er trotzt was Fleisch vnd Jahre leiden m\u00fcssen/", "tokens": ["Er", "trotzt", "was", "Fleisch", "vnd", "Jah\u00b7re", "lei\u00b7den", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er z", "tokens": ["Er", "z"], "token_info": ["word", "word"], "pos": ["PPER", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Er findet in sich/", "tokens": ["Er", "fin\u00b7det", "in", "sich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Was jener sucht", "tokens": ["Was", "je\u00b7ner", "sucht"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der stets/ gleich al\u00df Ich", "tokens": ["Der", "stets", "/", "gleich", "al\u00df", "Ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "$(", "ADV", "KOUS", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In schneller flucht ", "tokens": ["In", "schnel\u00b7ler", "flucht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Jrr't ohne Frucht. ", "tokens": ["Jrr't", "oh\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Er h\u00f6rt mit lust/ wen mancher r\u00fchm\u2019t vnd leuget", "tokens": ["Er", "h\u00f6rt", "mit", "lust", "/", "wen", "man\u00b7cher", "r\u00fchm't", "vnd", "leu\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "PWS", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd h\u00f6hnt den Rauch der stoltzen ", "tokens": ["Vnd", "h\u00f6hnt", "den", "Rauch", "der", "stolt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schaw\u2019t/ wenn mich ein falscher ", "tokens": ["Er", "scha\u00b7w't", "/", "wenn", "mich", "ein", "fal\u00b7scher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ART", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich vmb/ nach trew/ der hochbegrei\u00dften zeit. ", "tokens": ["Sich", "vmb", "/", "nach", "trew", "/", "der", "hoch\u00b7be\u00b7grei\u00df\u00b7ten", "zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "$(", "APPR", "ADJD", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er lib\u2019t nicht Liebe", "tokens": ["Er", "lib't", "nicht", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Wind vnd dunst", "tokens": ["Die", "Wind", "vnd", "dunst"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Vnd Seelen hiebe", "tokens": ["Vnd", "See\u00b7len", "hie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Gibt vor die gunst", "tokens": ["Gibt", "vor", "die", "gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der keuschen Brunst.", "tokens": ["Der", "keu\u00b7schen", "Brunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Er schm\u00fcckt sein gantz mit Ehr geziert Gem\u00fctte", "tokens": ["Er", "schm\u00fcckt", "sein", "gantz", "mit", "Ehr", "ge\u00b7ziert", "Ge\u00b7m\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "APPR", "NN", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit nicht gemeinem glantz der ", "tokens": ["Mit", "nicht", "ge\u00b7mei\u00b7nem", "glantz", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PTKNEG", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er lern\u2019t ", "tokens": ["Er", "lern't"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Er kenn\u2019t die ", "tokens": ["Er", "kenn't", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Was in den L\u00fcfften ", "tokens": ["Was", "in", "den", "L\u00fcff\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was ob vns schweb;", "tokens": ["Was", "ob", "vns", "schweb", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was au\u00df den kl\u00fcfften", "tokens": ["Was", "au\u00df", "den", "kl\u00fcff\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der grufft/ erheb'/ ", "tokens": ["Der", "grufft", "/", "er\u00b7heb'", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Vnd ewig leb'. ", "tokens": ["Vnd", "e\u00b7wig", "leb'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Jhm steht was ", "tokens": ["Jhm", "steht", "was"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "Er denen nur/ die sein Verstand erwehlt.", "tokens": ["Er", "de\u00b7nen", "nur", "/", "die", "sein", "Ver\u00b7stand", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADV", "$(", "ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von denen gleiche Seel vnd gunst zu hoffen/", "tokens": ["Von", "de\u00b7nen", "glei\u00b7che", "Seel", "vnd", "gunst", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vnd Trew/ die Freund erkiest vnd selten zehlt/", "tokens": ["Vnd", "Trew", "/", "die", "Freund", "er\u00b7kiest", "vnd", "sel\u00b7ten", "zehlt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "ART", "NN", "VVFIN", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit den vrtheilet ", "tokens": ["Mit", "den", "vrthei\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Er lust vnd leidt", "tokens": ["Er", "lust", "vnd", "leidt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was schlegt vnd heilet", "tokens": ["Was", "schlegt", "vnd", "hei\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Was nah\u2019 vnd weit", "tokens": ["Was", "nah'", "vnd", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Vnd Todt vnd Zeit.", "tokens": ["Vnd", "Todt", "vnd", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ach! k\u00f6nt ich/ was ich jtzund r\u00fchm\u2019 erlangen/", "tokens": ["Ach", "!", "k\u00f6nt", "ich", "/", "was", "ich", "jt\u00b7zund", "r\u00fchm'", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "$(", "PWS", "PPER", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach mein verh\u00e4ngnis was h\u00e4lt mich zu r\u00fcck?", "tokens": ["Ach", "mein", "ver\u00b7h\u00e4ng\u00b7nis", "was", "h\u00e4lt", "mich", "zu", "r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "PWS", "VVFIN", "PPER", "PTKZU", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn wird mich doch die s\u00fcsse Ruh\u2019 vmbfangen?", "tokens": ["Wenn", "wird", "mich", "doch", "die", "s\u00fcs\u00b7se", "Ruh'", "vmb\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sch\u00f6ne Lust/ das allerh\u00f6chste Gl\u00fcck.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Lust", "/", "das", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mich w\u00fcrd ergetzen", "tokens": ["Mich", "w\u00fcrd", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ein lustig Feldt ", "tokens": ["Ein", "lus\u00b7tig", "Feldt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Vor reich\u00dften ", "tokens": ["Vor", "reich\u00df\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Der F\u00fcrsten Zelt/", "tokens": ["Der", "F\u00fcrs\u00b7ten", "Zelt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ja Ehr vnd Welt. ", "tokens": ["Ja", "Ehr", "vnd", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}