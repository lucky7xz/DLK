{"dta.poem.12927": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Waer ich die nachtigall, die Florabelle liebt,", "tokens": ["Waer", "ich", "die", "nach\u00b7ti\u00b7gall", ",", "die", "Flo\u00b7ra\u00b7bel\u00b7le", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der sie nichts als k\u00fc\u00df\u2019 und zucker giebt;", "tokens": ["Und", "der", "sie", "nichts", "als", "k\u00fc\u00df'", "und", "zu\u00b7cker", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PIS", "KOKOM", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00e4ren ihr auch meine liebes-lieder", "tokens": ["So", "w\u00e4\u00b7ren", "ihr", "auch", "mei\u00b7ne", "lie\u00b7bes\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleichte richt zuwider.", "tokens": ["Viel\u00b7leich\u00b7te", "richt", "zu\u00b7wi\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum, Amor! der du einst den Jupiter zum schwan", "tokens": ["Drum", ",", "A\u00b7mor", "!", "der", "du", "einst", "den", "Ju\u00b7pi\u00b7ter", "zum", "schwan"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "NE", "$.", "ART", "PPER", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und wei\u00df nicht, was gemacht, schau meine sehnsucht an,", "tokens": ["Und", "wei\u00df", "nicht", ",", "was", "ge\u00b7macht", ",", "schau", "mei\u00b7ne", "sehn\u00b7sucht", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRELS", "VVPP", "$,", "ADJD", "PPOSAT", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die sich so gerne m\u00f6cht\u2019 in Florabellen spiegeln.", "tokens": ["Die", "sich", "so", "ger\u00b7ne", "m\u00f6cht'", "in", "Flo\u00b7ra\u00b7bel\u00b7len", "spie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verwandle mich demnach fein bald", "tokens": ["Ver\u00b7wand\u00b7le", "mich", "dem\u00b7nach", "fein", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PAV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In diese zart\u2019 und artige gestalt;", "tokens": ["In", "die\u00b7se", "zart'", "und", "ar\u00b7ti\u00b7ge", "ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "KON", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mach alle regungen zu fl\u00fcgeln;", "tokens": ["Mach", "al\u00b7le", "re\u00b7gun\u00b7gen", "zu", "fl\u00fc\u00b7geln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Mach\u2019 aus den seuffzern reine lufft,", "tokens": ["Mach'", "aus", "den", "seuff\u00b7zern", "rei\u00b7ne", "lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Damit ich nach dem ziele streiche,", "tokens": ["Da\u00b7mit", "ich", "nach", "dem", "zie\u00b7le", "strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Dahin mich mein verlangen rufft,", "tokens": ["Da\u00b7hin", "mich", "mein", "ver\u00b7lan\u00b7gen", "rufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und in ihren sch\u00f6nen augen mein gew\u00fcnschtes nest erreiche!", "tokens": ["Und", "in", "ih\u00b7ren", "sch\u00f6\u00b7nen", "au\u00b7gen", "mein", "ge\u00b7w\u00fcnschtes", "nest", "er\u00b7rei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Doch was vor furcht hemmt meinen hei\u00dfen schlu\u00df?", "tokens": ["Doch", "was", "vor", "furcht", "hemmt", "mei\u00b7nen", "hei\u00b7\u00dfen", "schlu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mu\u00df ich die fl\u00fcgel sincken lassen?", "tokens": ["Mu\u00df", "ich", "die", "fl\u00fc\u00b7gel", "sin\u00b7cken", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Darff denn mein hertze nicht die s\u00fc\u00dfe k\u00fchnheit fassen?", "tokens": ["Darff", "denn", "mein", "hert\u00b7ze", "nicht", "die", "s\u00fc\u00b7\u00dfe", "k\u00fchn\u00b7heit", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nein, nein! weil es so netz\u2019 als pfeile f\u00fcrchten mu\u00df.", "tokens": ["Nein", ",", "nein", "!", "weil", "es", "so", "netz'", "als", "pfei\u00b7le", "f\u00fcrch\u00b7ten", "mu\u00df", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "KOUS", "PPER", "ADV", "ADJD", "KOKOM", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}