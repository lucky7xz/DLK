{"dta.poem.4429": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein neblichtes und schlackriges  \n Wetter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach st\u00e4rke, grosser GOtt, doch meiner Selen Augen,", "tokens": ["Ach", "st\u00e4r\u00b7ke", ",", "gros\u00b7ser", "Gott", ",", "doch", "mei\u00b7ner", "Se\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie Dich, auch wenn Tr\u00fcbsals D\u00fcft\u2019 entstehn,", "tokens": ["Da\u00df", "sie", "Dich", ",", "auch", "wenn", "Tr\u00fcb\u00b7sals", "D\u00fcft'", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "ADV", "KOUS", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit froher Zuversicht zu sehn,", "tokens": ["Mit", "fro\u00b7her", "Zu\u00b7ver\u00b7sicht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kindlich zu verehren, taugen!", "tokens": ["Und", "kind\u00b7lich", "zu", "ver\u00b7eh\u00b7ren", ",", "tau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wird gewi\u00df zu rechter Zeit", "tokens": ["So", "wird", "ge\u00b7wi\u00df", "zu", "rech\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Nebel aller Widrigkeit,", "tokens": ["Der", "Ne\u00b7bel", "al\u00b7ler", "Wid\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Recht wie ein Nebel, schnell verschwinden,", "tokens": ["Recht", "wie", "ein", "Ne\u00b7bel", ",", "schnell", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und werden wir, wo nicht an diesem Ort,", "tokens": ["Und", "wer\u00b7den", "wir", ",", "wo", "nicht", "an", "die\u00b7sem", "Ort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PWAV", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch unausbleiblich dort,", "tokens": ["Doch", "un\u00b7aus\u00b7bleib\u00b7lich", "dort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der Freuden lichten Glanz und heitern Himmel finden.", "tokens": ["Der", "Freu\u00b7den", "lich\u00b7ten", "Glanz", "und", "hei\u00b7tern", "Him\u00b7mel", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}