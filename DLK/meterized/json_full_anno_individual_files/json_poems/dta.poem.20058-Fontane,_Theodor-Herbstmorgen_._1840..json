{"dta.poem.20058": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Herbstmorgen .  \n 1840.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken ziehn wie Trauerg\u00e4ste", "tokens": ["Die", "Wol\u00b7ken", "ziehn", "wie", "Trau\u00b7er\u00b7g\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mond zu Grabe zu geleiten;", "tokens": ["Den", "Mond", "zu", "Gra\u00b7be", "zu", "ge\u00b7lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wind durchfegt die starren Aeste,", "tokens": ["Der", "Wind", "durch\u00b7fegt", "die", "star\u00b7ren", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sucht ein Blatt aus bessren Zeiten.", "tokens": ["Und", "sucht", "ein", "Blatt", "aus", "bess\u00b7ren", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die gr\u00fcnen Tannen schaun so d\u00fcster", "tokens": ["Die", "gr\u00fc\u00b7nen", "Tan\u00b7nen", "schaun", "so", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf eine jung-geknickte Eiche,", "tokens": ["Auf", "ei\u00b7ne", "jung\u00b7ge\u00b7knick\u00b7te", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als blickten trauernde Geschwister", "tokens": ["Als", "blick\u00b7ten", "trau\u00b7ern\u00b7de", "Ge\u00b7schwis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Auf der geliebten Schwester Leiche.", "tokens": ["Auf", "der", "ge\u00b7lieb\u00b7ten", "Schwes\u00b7ter", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon flattern in der Luft die Raben,", "tokens": ["Schon", "flat\u00b7tern", "in", "der", "Luft", "die", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Winters unheilvolle Boten;", "tokens": ["Des", "Win\u00b7ters", "un\u00b7heil\u00b7vol\u00b7le", "Bo\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wird er tief in Schnee begraben", "tokens": ["Bald", "wird", "er", "tief", "in", "Schnee", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Erde \u2014 seinen gro\u00dfen Todten.", "tokens": ["Die", "Er\u00b7de", "sei\u00b7nen", "gro\u00b7\u00dfen", "Tod\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Bach l\u00e4uft hastig mir zur Seite;", "tokens": ["Ein", "Bach", "l\u00e4uft", "has\u00b7tig", "mir", "zur", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er ahnt des Winters Eisesketten,", "tokens": ["Er", "ahnt", "des", "Win\u00b7ters", "Ei\u00b7ses\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und st\u00fcrzt sich fort und sucht das Weite", "tokens": ["Und", "st\u00fcrzt", "sich", "fort", "und", "sucht", "das", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als k\u00f6nnt\u2019 ihm Flucht das Leben retten.", "tokens": ["Als", "k\u00f6nnt'", "ihm", "Flucht", "das", "Le\u00b7ben", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da mocht\u2019 ich l\u00e4nger nicht inmitten", "tokens": ["Da", "mocht'", "ich", "l\u00e4n\u00b7ger", "nicht", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So todesnaher Oede weilen;", "tokens": ["So", "to\u00b7des\u00b7na\u00b7her", "O\u00b7e\u00b7de", "wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es trieb mich fort, mit hast\u2019gen Schritten", "tokens": ["Es", "trieb", "mich", "fort", ",", "mit", "hast'\u00b7gen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem fl\u00fccht\u2019gen Bache nachzueilen.", "tokens": ["Dem", "fl\u00fccht'\u00b7gen", "Ba\u00b7che", "nach\u00b7zu\u00b7ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}