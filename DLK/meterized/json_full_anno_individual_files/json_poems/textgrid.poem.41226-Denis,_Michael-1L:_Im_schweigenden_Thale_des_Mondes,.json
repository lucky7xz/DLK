{"textgrid.poem.41226": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im schweigenden Thale des Mondes,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im schweigenden Thale des Mondes,", "tokens": ["Im", "schwei\u00b7gen\u00b7den", "Tha\u00b7le", "des", "Mon\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Umkr\u00e4nzet von heiligen Eichen,", "tokens": ["Um\u00b7kr\u00e4n\u00b7zet", "von", "hei\u00b7li\u00b7gen", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da walten die Geister der Barden,", "tokens": ["Da", "wal\u00b7ten", "die", "Geis\u00b7ter", "der", "Bar\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wenn Schlummer unr\u00fchmliche Menschen begr\u00e4bt.", "tokens": ["Wenn", "Schlum\u00b7mer", "un\u00b7r\u00fchm\u00b7li\u00b7che", "Men\u00b7schen", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Sie schweben auf Silbergew\u00f6lken", "tokens": ["Sie", "schwe\u00b7ben", "auf", "Sil\u00b7ber\u00b7ge\u00b7w\u00f6l\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den thauigen Abhang herunter,", "tokens": ["Den", "thau\u00b7i\u00b7gen", "Ab\u00b7hang", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und wandeln am Rande der Quelle,", "tokens": ["Und", "wan\u00b7deln", "am", "Ran\u00b7de", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die mitten im Thale durch Blumen sich schleicht.", "tokens": ["Die", "mit\u00b7ten", "im", "Tha\u00b7le", "durch", "Blu\u00b7men", "sich", "schleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Dann heben sich Lieder der Vorzeit,", "tokens": ["Dann", "he\u00b7ben", "sich", "Lie\u00b7der", "der", "Vor\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und Harfen begleiten die Lieder,", "tokens": ["Und", "Har\u00b7fen", "be\u00b7glei\u00b7ten", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und sanftester Nachhall entz\u00fccket", "tokens": ["Und", "sanf\u00b7tes\u00b7ter", "Nach\u00b7hall", "ent\u00b7z\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die lauschenden W\u00e4lder und Fluren umher.", "tokens": ["Die", "lau\u00b7schen\u00b7den", "W\u00e4l\u00b7der", "und", "Flu\u00b7ren", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Da war es, Erzeugter von Fingal!", "tokens": ["Da", "war", "es", ",", "Er\u00b7zeug\u00b7ter", "von", "Fin\u00b7gal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "APPR", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df Sined in Mitte der Barden", "tokens": ["Da\u00df", "Si\u00b7ned", "in", "Mit\u00b7te", "der", "Bar\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "NN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von ferne dein Antlitz entdeckte,", "tokens": ["Von", "fer\u00b7ne", "dein", "Ant\u00b7litz", "ent\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dein Silbergelocke vom Monde bestralt.", "tokens": ["Dein", "Sil\u00b7ber\u00b7ge\u00b7lo\u00b7cke", "vom", "Mon\u00b7de", "be\u00b7stralt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NE", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Du sangest in Saiten von Selma", "tokens": ["Du", "san\u00b7gest", "in", "Sai\u00b7ten", "von", "Sel\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Thaten des gro\u00dfen Erzeugers,", "tokens": ["Die", "Tha\u00b7ten", "des", "gro\u00b7\u00dfen", "Er\u00b7zeu\u00b7gers", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den bl\u00fchenden Oscar, den Kummer", "tokens": ["Den", "bl\u00fc\u00b7hen\u00b7den", "Os\u00b7car", ",", "den", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der treuen Bragela des Gatten beraubt;", "tokens": ["Der", "treu\u00b7en", "Bra\u00b7ge\u00b7la", "des", "Gat\u00b7ten", "be\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Den Jammer Temoras, die Preise", "tokens": ["Den", "Jam\u00b7mer", "Te\u00b7mo\u00b7ras", ",", "die", "Prei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der z\u00e4rtlichen Everallina,", "tokens": ["Der", "z\u00e4rt\u00b7li\u00b7chen", "E\u00b7ver\u00b7al\u00b7li\u00b7na", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den besseren Bruder Cairbars,", "tokens": ["Den", "bes\u00b7se\u00b7ren", "Bru\u00b7der", "Cair\u00b7bars", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Wunde Darthulen, die St\u00e4rke von Gaul.", "tokens": ["Die", "Wun\u00b7de", "Dar\u00b7thu\u00b7len", ",", "die", "St\u00e4r\u00b7ke", "von", "Gaul", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Wie war mir! Von welchen Gef\u00fchlen", "tokens": ["Wie", "war", "mir", "!", "Von", "wel\u00b7chen", "Ge\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "APPR", "PWAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Erbebte mein Busen! Wie brannte", "tokens": ["Er\u00b7beb\u00b7te", "mein", "Bu\u00b7sen", "!", "Wie", "brann\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Wange! Wie schwellten die Zehren", "tokens": ["Die", "Wan\u00b7ge", "!", "Wie", "schwell\u00b7ten", "die", "Zeh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der s\u00fc\u00dfesten Wehmuth mein starrendes Aug'!", "tokens": ["Der", "s\u00fc\u00b7\u00dfes\u00b7ten", "Weh\u00b7muth", "mein", "star\u00b7ren\u00b7des", "Aug'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Noch Knabe verga\u00df ich des Spieles", "tokens": ["Noch", "Kna\u00b7be", "ver\u00b7ga\u00df", "ich", "des", "Spie\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bey F\u00fc\u00dfen der Barden, und horchte;", "tokens": ["Bey", "F\u00fc\u00b7\u00dfen", "der", "Bar\u00b7den", ",", "und", "horch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch niemal, o Kehle von Morven!", "tokens": ["Doch", "nie\u00b7mal", ",", "o", "Keh\u00b7le", "von", "Mor\u00b7ven", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Empfand ich so feurig, wie die\u00dfmal bey dir.", "tokens": ["Emp\u00b7fand", "ich", "so", "feu\u00b7rig", ",", "wie", "die\u00df\u00b7mal", "bey", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "ADV", "APPR", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Da schwur ich (das schweigende Mondthal,", "tokens": ["Da", "schwur", "ich", "(", "das", "schwei\u00b7gen\u00b7de", "Mond\u00b7thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Wipfel der heiligen Eichen,", "tokens": ["Die", "Wip\u00b7fel", "der", "hei\u00b7li\u00b7gen", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die moosigen Tr\u00fcmmer, auf welche", "tokens": ["Die", "moo\u00b7si\u00b7gen", "Tr\u00fcm\u00b7mer", ",", "auf", "wel\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Linke sich st\u00fctzte, vernahmen den Schwur)", "tokens": ["Die", "Lin\u00b7ke", "sich", "st\u00fctz\u00b7te", ",", "ver\u00b7nah\u00b7men", "den", "Schwur", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Da schwur ich, dich Lehrer zu nennen,", "tokens": ["Da", "schwur", "ich", ",", "dich", "Leh\u00b7rer", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Saiten der Donau nach deinem", "tokens": ["Die", "Sai\u00b7ten", "der", "Do\u00b7nau", "nach", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NE", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gesange zu stimmen, zum Herzen,", "tokens": ["Ge\u00b7san\u00b7ge", "zu", "stim\u00b7men", ",", "zum", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zum Herzen die Wege zu suchen, wie du;", "tokens": ["Zum", "Her\u00b7zen", "die", "We\u00b7ge", "zu", "su\u00b7chen", ",", "wie", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Die Zeiten der Ahnen, die Zeiten", "tokens": ["Die", "Zei\u00b7ten", "der", "Ah\u00b7nen", ",", "die", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Vaterlandsliebe, der Tugend,", "tokens": ["Der", "Va\u00b7ter\u00b7lands\u00b7lie\u00b7be", ",", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Des Muthes, der Ruhmgier und Einfalt", "tokens": ["Des", "Mu\u00b7thes", ",", "der", "Ruhm\u00b7gier", "und", "Ein\u00b7falt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Im Liede zur\u00fccke zu f\u00fchren, wie du.", "tokens": ["Im", "Lie\u00b7de", "zu\u00b7r\u00fc\u00b7cke", "zu", "f\u00fch\u00b7ren", ",", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Du h\u00f6rtest mich schw\u00f6ren, und blicktest", "tokens": ["Du", "h\u00f6r\u00b7test", "mich", "schw\u00f6\u00b7ren", ",", "und", "blick\u00b7test"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit L\u00e4cheln auf deinen Geschwor'nen,", "tokens": ["Mit", "L\u00e4\u00b7cheln", "auf", "dei\u00b7nen", "Ge\u00b7schwor'\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und schienst mir die Harfe zu reichen,", "tokens": ["Und", "schienst", "mir", "die", "Har\u00b7fe", "zu", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und leise zu sagen: Versuche den Griff!", "tokens": ["Und", "lei\u00b7se", "zu", "sa\u00b7gen", ":", "Ver\u00b7su\u00b7che", "den", "Griff", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$.", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.13": {"line.1": {"text": "Seit diesem Gesichte bewohn' ich", "tokens": ["Seit", "die\u00b7sem", "Ge\u00b7sich\u00b7te", "be\u00b7wohn'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Vorwelt, und lerne die Weisen", "tokens": ["Die", "Vor\u00b7welt", ",", "und", "ler\u00b7ne", "die", "Wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Barden, und rette der T\u00f6ne", "tokens": ["Der", "Bar\u00b7den", ",", "und", "ret\u00b7te", "der", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zur\u00fcck' in mein Alter, so viel ich vermag.", "tokens": ["Zu\u00b7r\u00fcck'", "in", "mein", "Al\u00b7ter", ",", "so", "viel", "ich", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "PPER", "VV