{"dta.poem.815": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  Auf den  II.  Sontag des wider erscheinenden  \n Richters. Oder den  II.  der Zukunft Christi.  \n Luc. 21.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Schaw't schaw't jhr V\u00f6lcker schaw't/ die schweren wun- ", "tokens": ["Scha\u00b7w't", "scha\u00b7w't", "jhr", "V\u00f6l\u00b7cker", "scha\u00b7w't", "/", "die", "schwe\u00b7ren", "wun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "VVFIN", "$(", "ART", "ADJA", "TRUNC"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "derzeichen! ", "tokens": ["der\u00b7zei\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Das grosse Firmament/ der Himmel krafft zubricht.", "tokens": ["Das", "gros\u00b7se", "Fir\u00b7ma\u00b7ment", "/", "der", "Him\u00b7mel", "krafft", "zu\u00b7bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Monden steht in Blue! es schwind\u2019t der Sternen Licht", "tokens": ["Der", "Mon\u00b7den", "steht", "in", "Blue", "!", "es", "schwind't", "der", "Ster\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man siht die klare Sonn in hellem Tag erbleichen.", "tokens": ["Man", "siht", "die", "kla\u00b7re", "Sonn", "in", "hel\u00b7lem", "Tag", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die aufgeschwelte See wil \u00fcber Berge reichen/", "tokens": ["Die", "auf\u00b7ge\u00b7schwel\u00b7te", "See", "wil", "\u00fc\u00b7ber", "Ber\u00b7ge", "rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer h\u00f6rt der Winde grim/ der L\u00fcffte Rasen nicht?", "tokens": ["Wer", "h\u00f6rt", "der", "Win\u00b7de", "grim", "/", "der", "L\u00fcff\u00b7te", "Ra\u00b7sen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NE", "$(", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein jeder Mensch verschmacht/ v\u00f1 wei\u00df nicht was er spricht", "tokens": ["Ein", "je\u00b7der", "Mensch", "ver\u00b7schmacht", "/", "v\u00f1", "wei\u00df", "nicht", "was", "er", "spricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVPP", "$(", "NE", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor grosser Hertzens angst. Die rawen Felsen weichen.", "tokens": ["Vor", "gros\u00b7ser", "Hert\u00b7zens", "angst", ".", "Die", "ra\u00b7wen", "Fel\u00b7sen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch zittert Berg\u2019 vnd Thal O HERR der Herrligkeit", "tokens": ["Auch", "zit\u00b7tert", "Ber\u00b7g'", "vnd", "Thal", "O", "HeRR", "der", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "NE", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der du in Feur\u2019 die Welt zu richten dich bereit.", "tokens": ["Der", "du", "in", "Feur'", "die", "Welt", "zu", "rich\u00b7ten", "dich", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN", "PTKZU", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hilff da\u00df ich ja mein Hertz mit S\u00fcnden nicht beschwere!", "tokens": ["Hilff", "da\u00df", "ich", "ja", "mein", "Hertz", "mit", "S\u00fcn\u00b7den", "nicht", "be\u00b7schwe\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weck auff HERR/ wenn mich sorg vnd sicherheit anf\u00e4lt:", "tokens": ["Weck", "auff", "HeRR", "/", "wenn", "mich", "sorg", "vnd", "si\u00b7cher\u00b7heit", "an\u00b7f\u00e4lt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "KOUS", "PPER", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df wenn dein hartter Zorn einbricht die grosse Welt/", "tokens": ["Da\u00df", "wenn", "dein", "hart\u00b7ter", "Zorn", "ein\u00b7bricht", "die", "gros\u00b7se", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich nicht der Donnerstral de\u00df letzten Tag\u2019s verzehre.", "tokens": ["Mich", "nicht", "der", "Don\u00b7ner\u00b7stral", "de\u00df", "letz\u00b7ten", "Tag's", "ver\u00b7zeh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}