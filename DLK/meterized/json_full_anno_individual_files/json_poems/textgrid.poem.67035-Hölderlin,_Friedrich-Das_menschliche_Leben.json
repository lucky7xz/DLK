, "nur", "noch", "et\u00b7lich", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So entfliehst du, und dann g\u00f6ttlich sch\u00f6n", "tokens": ["So", "ent\u00b7fliehst", "du", ",", "und", "dann", "g\u00f6tt\u00b7lich", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "ADJD", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wird der Geist verkl\u00e4rt, ein be\u00dfres Gl\u00fccke", "tokens": ["Wird", "der", "Geist", "ver\u00b7kl\u00e4rt", ",", "ein", "be\u00df\u00b7res", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wird dann gl\u00e4nzender mein Auge sehn;", "tokens": ["Wird", "dann", "gl\u00e4n\u00b7zen\u00b7der", "mein", "Au\u00b7ge", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Bald umgibt dich, unvollkommne H\u00fclle,", "tokens": ["Bald", "um\u00b7gibt", "dich", ",", "un\u00b7voll\u00b7komm\u00b7ne", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dunkle Nacht, des Grabes Stille.", "tokens": ["Dunk\u00b7le", "Nacht", ",", "des", "Gra\u00b7bes", "Stil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}