{"textgrid.poem.67035": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das menschliche Leben", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschen, Menschen! was ist euer Leben,", "tokens": ["Men\u00b7schen", ",", "Men\u00b7schen", "!", "was", "ist", "eu\u00b7er", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eure Welt, die tr\u00e4nenvolle Welt,", "tokens": ["Eu\u00b7re", "Welt", ",", "die", "tr\u00e4\u00b7nen\u00b7vol\u00b7le", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieser Schauplatz, kann er Freuden geben,", "tokens": ["Die\u00b7ser", "Schau\u00b7platz", ",", "kann", "er", "Freu\u00b7den", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo sich Trauern nicht dazu gesellt?", "tokens": ["Wo", "sich", "Trau\u00b7ern", "nicht", "da\u00b7zu", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "PTKNEG", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O! die Schatten, welche euch umschweben,", "tokens": ["O", "!", "die", "Schat\u00b7ten", ",", "wel\u00b7che", "euch", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die sind euer Freudenleben.", "tokens": ["Die", "sind", "eu\u00b7er", "Freu\u00b7den\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tr\u00e4nen, flie\u00dft! o flie\u00dfet, Mitleidstr\u00e4nen,", "tokens": ["Tr\u00e4\u00b7nen", ",", "flie\u00dft", "!", "o", "flie\u00b7\u00dfet", ",", "Mit\u00b7leids\u00b7tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "FM", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Taumel, Reue, Tugend, Spott der Welt,", "tokens": ["Tau\u00b7mel", ",", "Reu\u00b7e", ",", "Tu\u00b7gend", ",", "Spott", "der", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wiederkehr zu ihr, ein neues Sehnen,", "tokens": ["Wie\u00b7der\u00b7kehr", "zu", "ihr", ",", "ein", "neu\u00b7es", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Banges Seufzen, das die Leiden z\u00e4hlt,", "tokens": ["Ban\u00b7ges", "Seuf\u00b7zen", ",", "das", "die", "Lei\u00b7den", "z\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sind der armen Sterblichen Begleiter,", "tokens": ["Sind", "der", "ar\u00b7men", "Sterb\u00b7li\u00b7chen", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O, nur allzu wenig heiter!", "tokens": ["O", ",", "nur", "all\u00b7zu", "we\u00b7nig", "hei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKA", "PIS", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Banger Schauer fa\u00dft die tr\u00fcbe Seele,", "tokens": ["Ban\u00b7ger", "Schau\u00b7er", "fa\u00dft", "die", "tr\u00fc\u00b7be", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn sie jene Torenfreuden sieht,", "tokens": ["Wenn", "sie", "je\u00b7ne", "To\u00b7ren\u00b7freu\u00b7den", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Welt, Verf\u00fchrung, manches Guten H\u00f6lle,", "tokens": ["Welt", ",", "Ver\u00b7f\u00fch\u00b7rung", ",", "man\u00b7ches", "Gu\u00b7ten", "H\u00f6l\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Flieht von mir, auf ewig immer flieht!", "tokens": ["Flieht", "von", "mir", ",", "auf", "e\u00b7wig", "im\u00b7mer", "flieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "APPR", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ja gewi\u00df, schon manche gute Seele hat, betrogen,", "tokens": ["Ja", "ge\u00b7wi\u00df", ",", "schon", "man\u00b7che", "gu\u00b7te", "See\u00b7le", "hat", ",", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ADV", "PIAT", "ADJA", "NN", "VAFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Euer t\u00f6tend Gift gesogen.", "tokens": ["Eu\u00b7er", "t\u00f6\u00b7tend", "Gift", "ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann der S\u00fcnde dann ihr Urteil t\u00f6net,", "tokens": ["Wann", "der", "S\u00fcn\u00b7de", "dann", "ihr", "Ur\u00b7teil", "t\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Des Gewissens Schreckensreu sie lehrt,", "tokens": ["Des", "Ge\u00b7wis\u00b7sens", "Schre\u00b7ckens\u00b7reu", "sie", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie die Lasterbahn ihr Ende kr\u00f6net,", "tokens": ["Wie", "die", "Las\u00b7ter\u00b7bahn", "ihr", "En\u00b7de", "kr\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schmerz, der ihr Gebein versehrt!", "tokens": ["Schmerz", ",", "der", "ihr", "Ge\u00b7bein", "ver\u00b7sehrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann sieht das verirrte Herz zur\u00fccke;", "tokens": ["Dann", "sieht", "das", "ver\u00b7irr\u00b7te", "Herz", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Reue schluchzen seine Blicke.", "tokens": ["Reu\u00b7e", "schluch\u00b7zen", "sei\u00b7ne", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Tugend bietet ihre Freuden", "tokens": ["Und", "die", "Tu\u00b7gend", "bie\u00b7tet", "ih\u00b7re", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gerne Mitleid l\u00e4chelnd an,", "tokens": ["Ger\u00b7ne", "Mit\u00b7leid", "l\u00e4\u00b7chelnd", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Welt \u2013 bald streut sie ihre Leiden", "tokens": ["Doch", "die", "Welt", "\u2013", "bald", "streut", "sie", "ih\u00b7re", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auch auf die zufrieden heitre Bahn:", "tokens": ["Auch", "auf", "die", "zu\u00b7frie\u00b7den", "heit\u00b7re", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Weil sie dem, der Tugendfreuden kennet,", "tokens": ["Weil", "sie", "dem", ",", "der", "Tu\u00b7gend\u00b7freu\u00b7den", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sein zufrieden Herz nicht g\u00f6nnet.", "tokens": ["Sein", "zu\u00b7frie\u00b7den", "Herz", "nicht", "g\u00f6n\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tausend mi\u00dfgunstvolle L\u00e4sterungen", "tokens": ["Tau\u00b7send", "mi\u00df\u00b7gunst\u00b7vol\u00b7le", "L\u00e4s\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sucht sie dann, da\u00df ihr die Tugend gleicht;", "tokens": ["Sucht", "sie", "dann", ",", "da\u00df", "ihr", "die", "Tu\u00b7gend", "gleicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bei\u00dfend spotten dann des Neides Zungen,", "tokens": ["Bei\u00b7\u00dfend", "spot\u00b7ten", "dann", "des", "Nei\u00b7des", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis die arme Unschuld ihnen weicht;", "tokens": ["Bis", "die", "ar\u00b7me", "Un\u00b7schuld", "ih\u00b7nen", "weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kaum verflossen etlich Freudentage,", "tokens": ["Kaum", "ver\u00b7flos\u00b7sen", "et\u00b7lich", "Freu\u00b7den\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sieh, so sinkt der Tugend Waage.", "tokens": ["Sieh", ",", "so", "sinkt", "der", "Tu\u00b7gend", "Waa\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Etlich K\u00e4mpfe \u2013 Tugend und Gewissen \u2013", "tokens": ["Et\u00b7lich", "K\u00e4mp\u00b7fe", "\u2013", "Tu\u00b7gend", "und", "Ge\u00b7wis\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nur noch schwach bewegen sie das Herz,", "tokens": ["Nur", "noch", "schwach", "be\u00b7we\u00b7gen", "sie", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wieder umgefallen! \u2013 und es flie\u00dfen", "tokens": ["Wie\u00b7der", "um\u00b7ge\u00b7fal\u00b7len", "!", "\u2013", "und", "es", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "$.", "$(", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Neue Tr\u00e4nen, neuer Schmerz!", "tokens": ["Neu\u00b7e", "Tr\u00e4\u00b7nen", ",", "neu\u00b7er", "Schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O du S\u00fcnde, Dolch der edlen Seelen,", "tokens": ["O", "du", "S\u00fcn\u00b7de", ",", "Dolch", "der", "ed\u00b7len", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mu\u00df denn jede dich erw\u00e4hlen?", "tokens": ["Mu\u00df", "denn", "je\u00b7de", "dich", "er\u00b7w\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schwachheit, nur noch etlich Augenblicke,", "tokens": ["Schwach\u00b7heit", ",", "nur", "noch", "et\u00b7lich", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So entfliehst du, und dann g\u00f6ttlich sch\u00f6n", "tokens": ["So", "ent\u00b7fliehst", "du", ",", "und", "dann", "g\u00f6tt\u00b7lich", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "ADJD", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wird der Geist verkl\u00e4rt, ein be\u00dfres Gl\u00fccke", "tokens": ["Wird", "der", "Geist", "ver\u00b7kl\u00e4rt", ",", "ein", "be\u00df\u00b7res", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wird dann gl\u00e4nzender mein Auge sehn;", "tokens": ["Wird", "dann", "gl\u00e4n\u00b7zen\u00b7der", "mein", "Au\u00b7ge", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Bald umgibt dich, unvollkommne H\u00fclle,", "tokens": ["Bald", "um\u00b7gibt", "dich", ",", "un\u00b7voll\u00b7komm\u00b7ne", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dunkle Nacht, des Grabes Stille.", "tokens": ["Dunk\u00b7le", "Nacht", ",", "des", "Gra\u00b7bes", "Stil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Menschen, Menschen! was ist euer Leben,", "tokens": ["Men\u00b7schen", ",", "Men\u00b7schen", "!", "was", "ist", "eu\u00b7er", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eure Welt, die tr\u00e4nenvolle Welt,", "tokens": ["Eu\u00b7re", "Welt", ",", "die", "tr\u00e4\u00b7nen\u00b7vol\u00b7le", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieser Schauplatz, kann er Freuden geben,", "tokens": ["Die\u00b7ser", "Schau\u00b7platz", ",", "kann", "er", "Freu\u00b7den", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo sich Trauern nicht dazu gesellt?", "tokens": ["Wo", "sich", "Trau\u00b7ern", "nicht", "da\u00b7zu", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "PTKNEG", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O! die Schatten, welche euch umschweben,", "tokens": ["O", "!", "die", "Schat\u00b7ten", ",", "wel\u00b7che", "euch", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die sind euer Freudenleben.", "tokens": ["Die", "sind", "eu\u00b7er", "Freu\u00b7den\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Tr\u00e4nen, flie\u00dft! o flie\u00dfet, Mitleidstr\u00e4nen,", "tokens": ["Tr\u00e4\u00b7nen", ",", "flie\u00dft", "!", "o", "flie\u00b7\u00dfet", ",", "Mit\u00b7leids\u00b7tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "FM", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Taumel, Reue, Tugend, Spott der Welt,", "tokens": ["Tau\u00b7mel", ",", "Reu\u00b7e", ",", "Tu\u00b7gend", ",", "Spott", "der", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wiederkehr zu ihr, ein neues Sehnen,", "tokens": ["Wie\u00b7der\u00b7kehr", "zu", "ihr", ",", "ein", "neu\u00b7es", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Banges Seufzen, das die Leiden z\u00e4hlt,", "tokens": ["Ban\u00b7ges", "Seuf\u00b7zen", ",", "das", "die", "Lei\u00b7den", "z\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sind der armen Sterblichen Begleiter,", "tokens": ["Sind", "der", "ar\u00b7men", "Sterb\u00b7li\u00b7chen", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O, nur allzu wenig heiter!", "tokens": ["O", ",", "nur", "all\u00b7zu", "we\u00b7nig", "hei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKA", "PIS", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Banger Schauer fa\u00dft die tr\u00fcbe Seele,", "tokens": ["Ban\u00b7ger", "Schau\u00b7er", "fa\u00dft", "die", "tr\u00fc\u00b7be", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn sie jene Torenfreuden sieht,", "tokens": ["Wenn", "sie", "je\u00b7ne", "To\u00b7ren\u00b7freu\u00b7den", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Welt, Verf\u00fchrung, manches Guten H\u00f6lle,", "tokens": ["Welt", ",", "Ver\u00b7f\u00fch\u00b7rung", ",", "man\u00b7ches", "Gu\u00b7ten", "H\u00f6l\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Flieht von mir, auf ewig immer flieht!", "tokens": ["Flieht", "von", "mir", ",", "auf", "e\u00b7wig", "im\u00b7mer", "flieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "APPR", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ja gewi\u00df, schon manche gute Seele hat, betrogen,", "tokens": ["Ja", "ge\u00b7wi\u00df", ",", "schon", "man\u00b7che", "gu\u00b7te", "See\u00b7le", "hat", ",", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ADV", "PIAT", "ADJA", "NN", "VAFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Euer t\u00f6tend Gift gesogen.", "tokens": ["Eu\u00b7er", "t\u00f6\u00b7tend", "Gift", "ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wann der S\u00fcnde dann ihr Urteil t\u00f6net,", "tokens": ["Wann", "der", "S\u00fcn\u00b7de", "dann", "ihr", "Ur\u00b7teil", "t\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Des Gewissens Schreckensreu sie lehrt,", "tokens": ["Des", "Ge\u00b7wis\u00b7sens", "Schre\u00b7ckens\u00b7reu", "sie", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie die Lasterbahn ihr Ende kr\u00f6net,", "tokens": ["Wie", "die", "Las\u00b7ter\u00b7bahn", "ihr", "En\u00b7de", "kr\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schmerz, der ihr Gebein versehrt!", "tokens": ["Schmerz", ",", "der", "ihr", "Ge\u00b7bein", "ver\u00b7sehrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann sieht das verirrte Herz zur\u00fccke;", "tokens": ["Dann", "sieht", "das", "ver\u00b7irr\u00b7te", "Herz", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Reue schluchzen seine Blicke.", "tokens": ["Reu\u00b7e", "schluch\u00b7zen", "sei\u00b7ne", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und die Tugend bietet ihre Freuden", "tokens": ["Und", "die", "Tu\u00b7gend", "bie\u00b7tet", "ih\u00b7re", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gerne Mitleid l\u00e4chelnd an,", "tokens": ["Ger\u00b7ne", "Mit\u00b7leid", "l\u00e4\u00b7chelnd", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Welt \u2013 bald streut sie ihre Leiden", "tokens": ["Doch", "die", "Welt", "\u2013", "bald", "streut", "sie", "ih\u00b7re", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auch auf die zufrieden heitre Bahn:", "tokens": ["Auch", "auf", "die", "zu\u00b7frie\u00b7den", "heit\u00b7re", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Weil sie dem, der Tugendfreuden kennet,", "tokens": ["Weil", "sie", "dem", ",", "der", "Tu\u00b7gend\u00b7freu\u00b7den", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sein zufrieden Herz nicht g\u00f6nnet.", "tokens": ["Sein", "zu\u00b7frie\u00b7den", "Herz", "nicht", "g\u00f6n\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Tausend mi\u00dfgunstvolle L\u00e4sterungen", "tokens": ["Tau\u00b7send", "mi\u00df\u00b7gunst\u00b7vol\u00b7le", "L\u00e4s\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sucht sie dann, da\u00df ihr die Tugend gleicht;", "tokens": ["Sucht", "sie", "dann", ",", "da\u00df", "ihr", "die", "Tu\u00b7gend", "gleicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bei\u00dfend spotten dann des Neides Zungen,", "tokens": ["Bei\u00b7\u00dfend", "spot\u00b7ten", "dann", "des", "Nei\u00b7des", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis die arme Unschuld ihnen weicht;", "tokens": ["Bis", "die", "ar\u00b7me", "Un\u00b7schuld", "ih\u00b7nen", "weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kaum verflossen etlich Freudentage,", "tokens": ["Kaum", "ver\u00b7flos\u00b7sen", "et\u00b7lich", "Freu\u00b7den\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sieh, so sinkt der Tugend Waage.", "tokens": ["Sieh", ",", "so", "sinkt", "der", "Tu\u00b7gend", "Waa\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Etlich K\u00e4mpfe \u2013 Tugend und Gewissen \u2013", "tokens": ["Et\u00b7lich", "K\u00e4mp\u00b7fe", "\u2013", "Tu\u00b7gend", "und", "Ge\u00b7wis\u00b7sen", "\u2013"], "toke