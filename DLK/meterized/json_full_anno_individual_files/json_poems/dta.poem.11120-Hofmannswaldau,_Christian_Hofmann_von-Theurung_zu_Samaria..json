{"dta.poem.11120": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Theurung zu Samaria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es galt ein esels-kopff hier drey\u00dfig silberlinge;", "tokens": ["Es", "galt", "ein", "e\u00b7sels\u00b7kopff", "hier", "drey\u00b7\u00dfig", "sil\u00b7ber\u00b7lin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df las ein Spanier. O! sprach er, wunder-dinge!", "tokens": ["Di\u00df", "las", "ein", "Spa\u00b7nier", ".", "O", "!", "sprach", "er", ",", "wun\u00b7der\u00b7din\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "NE", "$.", "VVFIN", "PPER", "$,", "ADJA", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die esels-k\u00f6pffe sind bey uns ja so gemein,", "tokens": ["Die", "e\u00b7sels\u00b7k\u00f6pf\u00b7fe", "sind", "bey", "uns", "ja", "so", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df ihrer drey\u00dfig kaum f\u00fcr einen groschen seyn.", "tokens": ["Da\u00df", "ih\u00b7rer", "drey\u00b7\u00dfig", "kaum", "f\u00fcr", "ei\u00b7nen", "gro\u00b7schen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "CARD", "ADV", "APPR", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}