{"textgrid.poem.60275": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose der Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als treulos ich das teure Land verlie\u00df,", "tokens": ["Als", "treu\u00b7los", "ich", "das", "teu\u00b7re", "Land", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo mir, wie nirgend sonst, die Freude bl\u00fchte,", "tokens": ["Wo", "mir", ",", "wie", "nir\u00b7gend", "sonst", ",", "die", "Freu\u00b7de", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PWAV", "ADV", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich selbst versto\u00dfend aus dem Paradies", "tokens": ["Mich", "selbst", "ver\u00b7sto\u00b7\u00dfend", "aus", "dem", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Voll Freundesliebe, holder Fraueng\u00fcte;", "tokens": ["Voll", "Freun\u00b7des\u00b7lie\u00b7be", ",", "hol\u00b7der", "Frau\u00b7en\u00b7g\u00fc\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und als ich stand zum ernsten Scheidegru\u00df", "tokens": ["Und", "als", "ich", "stand", "zum", "erns\u00b7ten", "Schei\u00b7de\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An meiner Freuden maiengr\u00fcnem Saume,", "tokens": ["An", "mei\u00b7ner", "Freu\u00b7den", "mai\u00b7en\u00b7gr\u00fc\u00b7nem", "Sau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als mir im Auge quoll der Tr\u00e4nengu\u00df", "tokens": ["Als", "mir", "im", "Au\u00b7ge", "quoll", "der", "Tr\u00e4\u00b7nen\u00b7gu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie warmer Regen nach dem Fr\u00fchlingstraume:", "tokens": ["Wie", "war\u00b7mer", "Re\u00b7gen", "nach", "dem", "Fr\u00fch\u00b7lings\u00b7trau\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da bog sich mir zum Lebewohl herab", "tokens": ["Da", "bog", "sich", "mir", "zum", "Le\u00b7be\u00b7wohl", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der reichsten einer von den Bl\u00fctenzweigen,", "tokens": ["Der", "reichs\u00b7ten", "ei\u00b7ner", "von", "den", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der freundlich mir noch eine Rose gab;", "tokens": ["Der", "freund\u00b7lich", "mir", "noch", "ei\u00b7ne", "Ro\u00b7se", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz verstand sein liebevolles Schweigen.", "tokens": ["Mein", "Herz", "ver\u00b7stand", "sein", "lie\u00b7be\u00b7vol\u00b7les", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u203anicht in den Staub, o Freund, hier weine hin,", "tokens": ["\u203a", "nicht", "in", "den", "Staub", ",", "o", "Freund", ",", "hier", "wei\u00b7ne", "hin", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "ART", "NN", "$,", "FM", "NN", "$,", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier auf die weichen Bl\u00e4tter dieser Rose!\u2039", "tokens": ["Hier", "auf", "die", "wei\u00b7chen", "Bl\u00e4t\u00b7ter", "die\u00b7ser", "Ro\u00b7se", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das war der stummen Gabe milder Sinn;", "tokens": ["Das", "war", "der", "stum\u00b7men", "Ga\u00b7be", "mil\u00b7der", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schmerzlich rasch folgt ich dem Wanderlose", "tokens": ["Und", "schmerz\u00b7lich", "rasch", "folgt", "ich", "dem", "Wan\u00b7der\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In fremde Welten fuhr mich der Pilot,", "tokens": ["In", "frem\u00b7de", "Wel\u00b7ten", "fuhr", "mich", "der", "Pi\u00b7lot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom teuren Lande trennen mich nun Meere;", "tokens": ["Vom", "teu\u00b7ren", "Lan\u00b7de", "tren\u00b7nen", "mich", "nun", "Mee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie mir einst das Lebewohl gebot,", "tokens": ["Und", "wie", "mir", "einst", "das", "Le\u00b7be\u00b7wohl", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Netz ich die Blume mit getreuer Z\u00e4hre.", "tokens": ["Netz", "ich", "die", "Blu\u00b7me", "mit", "ge\u00b7treu\u00b7er", "Z\u00e4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Rose inniglicher Duft entschwand,", "tokens": ["Der", "Ro\u00b7se", "in\u00b7nig\u00b7li\u00b7cher", "Duft", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es ging die frische Farbenglut verbleichen;", "tokens": ["Es", "ging", "die", "fri\u00b7sche", "Far\u00b7beng\u00b7lut", "ver\u00b7blei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie ruht so bla\u00df und starr in meiner Hand,", "tokens": ["Sie", "ruht", "so", "bla\u00df", "und", "starr", "in", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Unverwelklichen ein welkes Zeichen.", "tokens": ["Des", "Un\u00b7ver\u00b7wel\u00b7kli\u00b7chen", "ein", "wel\u00b7kes", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Des Unverwelklichen? \u2013 sie rauscht so bang,", "tokens": ["Des", "Un\u00b7ver\u00b7wel\u00b7kli\u00b7chen", "?", "\u2013", "sie", "rauscht", "so", "bang", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Will meine Hand die Rose wieder wecken;", "tokens": ["Will", "mei\u00b7ne", "Hand", "die", "Ro\u00b7se", "wie\u00b7der", "we\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als w\u00e4r es ein prophetisch tr\u00fcber Klang,", "tokens": ["Als", "w\u00e4r", "es", "ein", "pro\u00b7phe\u00b7tisch", "tr\u00fc\u00b7ber", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6r ich den Laut mit heimlichem Erschrecken.", "tokens": ["H\u00f6r", "ich", "den", "Laut", "mit", "heim\u00b7li\u00b7chem", "Er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O Rose der Erinnerung geweiht!", "tokens": ["O", "Ro\u00b7se", "der", "E\u00b7rin\u00b7ne\u00b7rung", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir d\u00fcnket deiner welken Bl\u00e4tter Rauschen", "tokens": ["Mir", "d\u00fcn\u00b7ket", "dei\u00b7ner", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein leises Schreiten der Verg\u00e4nglichkeit,", "tokens": ["Ein", "lei\u00b7ses", "Schrei\u00b7ten", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6rbar geworden pl\u00f6tzlich meinem Lauschen!", "tokens": ["H\u00f6r\u00b7bar", "ge\u00b7wor\u00b7den", "pl\u00f6tz\u00b7lich", "mei\u00b7nem", "Lau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["A