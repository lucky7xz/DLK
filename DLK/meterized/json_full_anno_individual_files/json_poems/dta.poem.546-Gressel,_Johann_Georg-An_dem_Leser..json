{"dta.poem.546": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An dem Leser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kan zwar werther Freund mein Werck nicht kostbahr", "tokens": ["Ich", "kan", "zwar", "wert\u00b7her", "Freund", "mein", "Werck", "nicht", "kost\u00b7bahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "PPOSAT", "NN", "PTKNEG", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "preisen:", "tokens": ["prei\u00b7sen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich wei\u00df nicht ob mein Buch dir was ", "tokens": ["Ich", "wei\u00df", "nicht", "ob", "mein", "Buch", "dir", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPOSAT", "NN", "PPER", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ob die Lieblichkeit sich in den Zeilen zeigt/", "tokens": ["Und", "ob", "die", "Lieb\u00b7lich\u00b7keit", "sich", "in", "den", "Zei\u00b7len", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Der Geist des ", "tokens": ["Der", "Geist", "des"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Jedoch mir ist alsdann das beste Gl\u00fcck beschehrt", "tokens": ["Je\u00b7doch", "mir", "ist", "als\u00b7dann", "das", "bes\u00b7te", "Gl\u00fcck", "be\u00b7schehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn es dein Auge lie\u00dft. Ja sch\u00e4tzest du es werth/", "tokens": ["Wenn", "es", "dein", "Au\u00b7ge", "lie\u00dft", ".", "Ja", "sch\u00e4t\u00b7zest", "du", "es", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "PTKANT", "VVFIN", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wird es selbst von mir den Sternen zugef\u00fchret.", "tokens": ["So", "wird", "es", "selbst", "von", "mir", "den", "Ster\u00b7nen", "zu\u00b7ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}