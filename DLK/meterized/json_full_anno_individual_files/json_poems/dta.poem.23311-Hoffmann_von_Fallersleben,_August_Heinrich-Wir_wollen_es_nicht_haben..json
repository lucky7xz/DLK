{"dta.poem.23311": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wir wollen es nicht haben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sollen h\u00fcbsch im Paradiese bleiben", "tokens": ["Wir", "sol\u00b7len", "h\u00fcbsch", "im", "Pa\u00b7ra\u00b7die\u00b7se", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und uns wie's Adam that die Zeit vertreiben,", "tokens": ["Und", "uns", "wie's", "A\u00b7dam", "that", "die", "Zeit", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und keine B\u00fccher lesen, keine schreiben \u2014", "tokens": ["Und", "kei\u00b7ne", "B\u00fc\u00b7cher", "le\u00b7sen", ",", "kei\u00b7ne", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$,", "PIAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir sollen h\u00fcbsch im Paradiese bleiben.", "tokens": ["Wir", "sol\u00b7len", "h\u00fcbsch", "im", "Pa\u00b7ra\u00b7die\u00b7se", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir sollen vom Erkenntni\u00dfbaum nicht essen,", "tokens": ["Wir", "sol\u00b7len", "vom", "Er\u00b7kenn\u00b7tni\u00df\u00b7baum", "nicht", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns freu'n an Allem was uns zugemessen,", "tokens": ["Uns", "freu'n", "an", "Al\u00b7lem", "was", "uns", "zu\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und des Gebotes nimmermehr vergessen:", "tokens": ["Und", "des", "Ge\u00b7bo\u00b7tes", "nim\u00b7mer\u00b7mehr", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir sollen vom Erkenntni\u00dfbaum nicht essen.", "tokens": ["Wir", "sol\u00b7len", "vom", "Er\u00b7kenn\u00b7tni\u00df\u00b7baum", "nicht", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Paradies hat uns nur stets verdrossen,", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", "hat", "uns", "nur", "stets", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie gerne sind wir ", "tokens": ["Wie", "ger\u00b7ne", "sind", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Drum haben wir von diesem Baum genossen \u2014", "tokens": ["Drum", "ha\u00b7ben", "wir", "von", "die\u00b7sem", "Baum", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Paradies hat uns nur stets verdrossen.", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", "hat", "uns", "nur", "stets", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du Paradies der Diener und Soldaten,", "tokens": ["Du", "Pa\u00b7ra\u00b7dies", "der", "Die\u00b7ner", "und", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lebwohl, du Jagdrevier der Potentaten,", "tokens": ["Leb\u00b7wohl", ",", "du", "Jagd\u00b7re\u00b7vier", "der", "Po\u00b7ten\u00b7ta\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir wollen dein auf ewig nun entrathen,", "tokens": ["Wir", "wol\u00b7len", "dein", "auf", "e\u00b7wig", "nun", "ent\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "APPR", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Paradies der Diener und Soldaten!", "tokens": ["Du", "Pa\u00b7ra\u00b7dies", "der", "Die\u00b7ner", "und", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}