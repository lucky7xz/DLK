{"textgrid.poem.44280": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Vergn\u00fcgen geht zu Grabe,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Vergn\u00fcgen geht zu Grabe,", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "geht", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine s\u00fc\u00dfe Hofnung stirbt,", "tokens": ["Mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Hof\u00b7nung", "stirbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich was verbrochen habe,", "tokens": ["Weil", "ich", "was", "ver\u00b7bro\u00b7chen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mir Fall und Tod erwirbt.", "tokens": ["Das", "mir", "Fall", "und", "Tod", "er\u00b7wirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6nster Engel, ach Louise,", "tokens": ["Sch\u00f6ns\u00b7ter", "En\u00b7gel", ",", "ach", "Lou\u00b7i\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "XY", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine sonst beliebte Hand", "tokens": ["Dei\u00b7ne", "sonst", "be\u00b7lieb\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "St\u00f6\u00dft mich aus dem Paradiese", "tokens": ["St\u00f6\u00dft", "mich", "aus", "dem", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In ein d\u00fcrres Hungerland.", "tokens": ["In", "ein", "d\u00fcr\u00b7res", "Hun\u00b7ger\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach Louise, deine K\u00fc\u00dfe,", "tokens": ["Ach", "Lou\u00b7i\u00b7se", ",", "dei\u00b7ne", "K\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mein Mund zuvor geschmeckt,", "tokens": ["Die", "mein", "Mund", "zu\u00b7vor", "ge\u00b7schmeckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waren mir wie Manna s\u00fc\u00dfe", "tokens": ["Wa\u00b7ren", "mir", "wie", "Man\u00b7na", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KOKOM", "NE", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Edens Thau bedeckt;", "tokens": ["Und", "mit", "E\u00b7dens", "Thau", "be\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja zu diesem Freygerichte", "tokens": ["Ja", "zu", "die\u00b7sem", "Frey\u00b7ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ludestu mich selber ein,", "tokens": ["Lu\u00b7des\u00b7tu", "mich", "sel\u00b7ber", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner Sch\u00f6nheit Rosenfr\u00fcchte", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Ro\u00b7sen\u00b7fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sollten mir ein Garthen seyn.", "tokens": ["Soll\u00b7ten", "mir", "ein", "Gar\u00b7then", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieser Garthen wird zur W\u00fcsten", "tokens": ["Die\u00b7ser", "Gar\u00b7then", "wird", "zur", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dein Auge mir zur Nacht,", "tokens": ["Und", "dein", "Au\u00b7ge", "mir", "zur", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ein Grif nach deinen B \u2013 \u2013", "tokens": ["Denn", "ein", "Grif", "nach", "dei\u00b7nen", "B", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat dich so erz\u00fcrnt gemacht.", "tokens": ["Hat", "dich", "so", "er\u00b7z\u00fcrnt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solcher \u00c4pfel Milchcorallen", "tokens": ["Sol\u00b7cher", "\u00c4p\u00b7fel", "Milch\u00b7co\u00b7ral\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hei\u00dfen die verbothne Frucht,", "tokens": ["Hei\u00b7\u00dfen", "die", "ver\u00b7both\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ich auf den Marmorballen", "tokens": ["Die", "ich", "auf", "den", "Mar\u00b7mor\u00b7bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir zum Tode selbst gesucht.", "tokens": ["Mir", "zum", "To\u00b7de", "selbst", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach Louise, soll ich sterben?", "tokens": ["Ach", "Lou\u00b7i\u00b7se", ",", "soll", "ich", "ster\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist ein Grif des Todes werth?", "tokens": ["Ist", "ein", "Grif", "des", "To\u00b7des", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so sieh, da\u00df mein Verderben", "tokens": ["Ach", ",", "so", "sieh", ",", "da\u00df", "mein", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch nicht deine Lust verzehrt.", "tokens": ["Auch", "nicht", "dei\u00b7ne", "Lust", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach der ersten Eltern Falle", "tokens": ["Nach", "der", "ers\u00b7ten", "El\u00b7tern", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fiel auch Edens Anmuth ein,", "tokens": ["Fiel", "auch", "E\u00b7dens", "An\u00b7muth", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Sch\u00f6nheit kan dem Balle", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "kan", "dem", "Bal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Ver\u00e4ndrung \u00e4hnlich seyn.", "tokens": ["Der", "Ver\u00b7\u00e4n\u00b7drung", "\u00e4hn\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwar du hei\u00dfest mich zum Scherze", "tokens": ["Zwar", "du", "hei\u00b7\u00dfest", "mich", "zum", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur des Todes Ebenbild;", "tokens": ["Nur", "des", "To\u00b7des", "E\u00b7ben\u00b7bild", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so t\u00f6dte doch mein Herze,", "tokens": ["Ach", ",", "so", "t\u00f6d\u00b7te", "doch", "mein", "Her\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nster Engel, wenn du wilt.", "tokens": ["Sch\u00f6ns\u00b7ter", "En\u00b7gel", ",", "wenn", "du", "wilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Las mich nur die Gunst erwerben,", "tokens": ["Las", "mich", "nur", "die", "Gunst", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "G\u00f6nne mir den guten Tag,", "tokens": ["G\u00f6n\u00b7ne", "mir", "den", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich noch bey meinem Sterben", "tokens": ["Da\u00df", "ich", "noch", "bey", "mei\u00b7nem", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dein Getreuer hei\u00dfen mag.", "tokens": ["Dein", "Ge\u00b7treu\u00b7er", "hei\u00b7\u00dfen", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sencke meine Schuld der L\u00fcste", "tokens": ["Sen\u00b7cke", "mei\u00b7ne", "Schuld", "der", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dein tief Erbarmen ein;", "tokens": ["In", "dein", "tief", "Er\u00b7bar\u00b7men", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Las den Schnee gew\u00f6lbter B \u2013 \u2013", "tokens": ["Las", "den", "Schnee", "ge\u00b7w\u00f6lb\u00b7ter", "B", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "XY", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Todtenbaare seyn.", "tokens": ["Mei\u00b7ne", "Tod\u00b7ten\u00b7baa\u00b7re", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines Leibes runde E \u2013 \u2013", "tokens": ["Dei\u00b7nes", "Lei\u00b7bes", "run\u00b7de", "E", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeige mir mein Grabmahl an,", "tokens": ["Zei\u00b7ge", "mir", "mein", "Grab\u00b7mahl", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich nach beliebter L\u00e4nge", "tokens": ["Da\u00df", "ich", "nach", "be\u00b7lieb\u00b7ter", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wieder auferstehen kan.", "tokens": ["Wie\u00b7der", "auf\u00b7er\u00b7ste\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mein Vergn\u00fcgen geht zu Grabe,", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "geht", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine s\u00fc\u00dfe Hofnung stirbt,", "tokens": ["Mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Hof\u00b7nung", "stirbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich was verbrochen habe,", "tokens": ["Weil", "ich", "was", "ver\u00b7bro\u00b7chen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mir Fall und Tod erwirbt.", "tokens": ["Das", "mir", "Fall", "und", "Tod", "er\u00b7wirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6nster Engel, ach Louise,", "tokens": ["Sch\u00f6ns\u00b7ter", "En\u00b7gel", ",", "ach", "Lou\u00b7i\u00b7se", ","], "token_info": ["word", "word", "punct