{"textgrid.poem.57463": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcrstinn! deren Geist und Pracht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrstinn! deren Geist und Pracht", "tokens": ["F\u00fcrs\u00b7tinn", "!", "de\u00b7ren", "Geist", "und", "Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PRELAT", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auch G\u00f6ttinnen schamroth macht;", "tokens": ["Auch", "G\u00f6t\u00b7tin\u00b7nen", "scham\u00b7roth", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die du itzt die Schmerzensstunden", "tokens": ["Die", "du", "itzt", "die", "Schmer\u00b7zenss\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Kindbetts \u00fcberwunden;", "tokens": ["Dei\u00b7nes", "Kind\u00b7betts", "\u00fc\u00b7berw\u00b7un\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcrstinn! dich und deinen Sohn", "tokens": ["F\u00fcrs\u00b7tinn", "!", "dich", "und", "dei\u00b7nen", "Sohn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heute w\u00fcrdig zu besingen,", "tokens": ["Heu\u00b7te", "w\u00fcr\u00b7dig", "zu", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fc\u00dfte mir der sch\u00f6nste Ton,", "tokens": ["M\u00fc\u00df\u00b7te", "mir", "der", "sch\u00f6ns\u00b7te", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4chst den Musen selbst, gelingen.", "tokens": ["N\u00e4chst", "den", "Mu\u00b7sen", "selbst", ",", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gleichwohl wag ichs, da mirs gl\u00fcckt,", "tokens": ["Gleich\u00b7wohl", "wag", "ichs", ",", "da", "mirs", "gl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich dich gesund erblickt,", "tokens": ["Da\u00df", "ich", "dich", "ge\u00b7sund", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was \u00f6ffentlich geschehen,", "tokens": ["Und", "was", "\u00f6f\u00b7fent\u00b7lich", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Vergn\u00fcgen angesehen;", "tokens": ["Mit", "Ver\u00b7gn\u00fc\u00b7gen", "an\u00b7ge\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleichwohl wag ichs, durch die\u00df Lied,", "tokens": ["Gleich\u00b7wohl", "wag", "ichs", ",", "durch", "die\u00df", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "APPR", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich und deinen Sohn zu ehren:", "tokens": ["Dich", "und", "dei\u00b7nen", "Sohn", "zu", "eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn nur das, was andre zieht,", "tokens": ["Wenn", "nur", "das", ",", "was", "and\u00b7re", "zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir nicht wird die Reime st\u00f6ren.", "tokens": ["Mir", "nicht", "wird", "die", "Rei\u00b7me", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deine Sch\u00f6nheit, dein Verstand", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "dein", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind der halben Welt bekannt,", "tokens": ["Sind", "der", "hal\u00b7ben", "Welt", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein m\u00e4nnlich muntres Wesen", "tokens": ["Und", "dein", "m\u00e4nn\u00b7lich", "mun\u00b7tres", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nennt der ganze Hof erlesen.", "tokens": ["Nennt", "der", "gan\u00b7ze", "Hof", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine reizende Gestalt,", "tokens": ["Dei\u00b7ne", "rei\u00b7zen\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Sammt der schlanken Glieder L\u00e4nge,", "tokens": ["Sammt", "der", "schlan\u00b7ken", "Glie\u00b7der", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hemmen jede Zunge bald,", "tokens": ["Hem\u00b7men", "je\u00b7de", "Zun\u00b7ge", "bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie noch so fertig s\u00e4nge.", "tokens": ["Wenn", "sie", "noch", "so", "fer\u00b7tig", "s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zarter Prinz, wo bist denn du?", "tokens": ["Zar\u00b7ter", "Prinz", ",", "wo", "bist", "denn", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VAFIN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deckt dich Gold und Sammet zu?", "tokens": ["Deckt", "dich", "Gold", "und", "Sam\u00b7met", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, ich seh dich, statt der Wiegen,", "tokens": ["Nein", ",", "ich", "seh", "dich", ",", "statt", "der", "Wie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den sch\u00f6nsten Armen liegen.", "tokens": ["In", "den", "sch\u00f6ns\u00b7ten", "Ar\u00b7men", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Anblick st\u00f6rt mich auch:", "tokens": ["Die\u00b7ser", "An\u00b7blick", "st\u00f6rt", "mich", "auch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier darf niemand Lieder bringen,", "tokens": ["Hier", "darf", "nie\u00b7mand", "Lie\u00b7der", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich, nach andrer Kinder Brauch,", "tokens": ["Dich", ",", "nach", "an\u00b7drer", "Kin\u00b7der", "Brauch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Recht zur Unzeit einzusingen.", "tokens": ["Recht", "zur", "Un\u00b7zeit", "ein\u00b7zu\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schlafe nicht, und sieh sie an,", "tokens": ["Schla\u00b7fe", "nicht", ",", "und", "sieh", "sie", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KON", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so z\u00e4rtlich k\u00fcssen kann;", "tokens": ["Die", "so", "z\u00e4rt\u00b7lich", "k\u00fcs\u00b7sen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie dich mit Mutterblicken", "tokens": ["Wenn", "sie", "dich", "mit", "Mut\u00b7ter\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weis an Mund und Brust zu dr\u00fccken.", "tokens": ["Weis", "an", "Mund", "und", "Brust", "zu", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh sie an, und l\u00e4chle nur,", "tokens": ["Sieh", "sie", "an", ",", "und", "l\u00e4ch\u00b7le", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn der F\u00fcrst mit dir will scherzen;", "tokens": ["Wenn", "der", "F\u00fcrst", "mit", "dir", "will", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn der Vaterliebe Spur", "tokens": ["Denn", "der", "Va\u00b7ter\u00b7lie\u00b7be", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Regt sich schon in seinem Herzen.", "tokens": ["Regt", "sich", "schon", "in", "sei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Carl hat dir den tapfern Muth,", "tokens": ["Carl", "hat", "dir", "den", "tap\u00b7fern", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So, wie Hollsteins Heldenblut,", "tokens": ["So", ",", "wie", "Holl\u00b7steins", "Hel\u00b7den\u00b7blut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das so rein in ihn geflossen,", "tokens": ["Das", "so", "rein", "in", "ihn", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Adern eingegossen.", "tokens": ["In", "die", "A\u00b7dern", "ein\u00b7ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Folge seinem Beyspiel nach,", "tokens": ["Fol\u00b7ge", "sei\u00b7nem", "Bey\u00b7spiel", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lerne bald den Degen f\u00fchren;", "tokens": ["Ler\u00b7ne", "bald", "den", "De\u00b7gen", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch, nach manchem Ungemach,", "tokens": ["Doch", ",", "nach", "man\u00b7chem", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch im Lieben triumphiren.", "tokens": ["Auch", "im", "Lie\u00b7ben", "tri\u00b7um\u00b7phi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schlinge dich um seinen Hals;", "tokens": ["Schlin\u00b7ge", "dich", "um", "sei\u00b7nen", "Hals", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die Mutter ebenfalls", "tokens": ["La\u00df", "die", "Mut\u00b7ter", "e\u00b7ben\u00b7falls"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner kleinen Arme Spielen", "tokens": ["Dei\u00b7ner", "klei\u00b7nen", "Ar\u00b7me", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die wei\u00dfen Schultern f\u00fchlen.", "tokens": ["Um", "die", "wei\u00b7\u00dfen", "Schul\u00b7tern", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcrstinn! sieh, wie buhlt er schon!", "tokens": ["F\u00fcrs\u00b7tinn", "!", "sieh", ",", "wie", "buhlt", "er", "schon", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sollte dich ein Heyde kennen,", "tokens": ["Soll\u00b7te", "dich", "ein", "Hey\u00b7de", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrd er dich und deinen Sohn", "tokens": ["W\u00fcrd", "er", "dich", "und", "dei\u00b7nen", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Venus und Cupido nennen.", "tokens": ["Ve\u00b7nus", "und", "Cu\u00b7pi\u00b7do", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Henkt ihm Amors K\u00f6cher um!", "tokens": ["Henkt", "ihm", "A\u00b7mors", "K\u00f6\u00b7cher", "um", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ey! ich gebe was darum,", "tokens": ["Ey", "!", "ich", "ge\u00b7be", "was", "da\u00b7rum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PIS", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er so mit Pfeil und Bogen", "tokens": ["Wenn", "er", "so", "mit", "Pfeil", "und", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Zur Redoute k\u00f6mmt gezogen.", "tokens": ["Zur", "Re\u00b7dou\u00b7te", "k\u00f6mmt", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da wird die\u00df vermummte Kind", "tokens": ["Da", "wird", "die\u00df", "ver\u00b7mumm\u00b7te", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDS", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Tausend Herzen an sich ziehen,", "tokens": ["Tau\u00b7send", "Her\u00b7zen", "an", "sich", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und was nicht der Sohn entz\u00fcndt,", "tokens": ["Und", "was", "nicht", "der", "Sohn", "ent\u00b7z\u00fcndt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch der Mutter nicht entfliehen.", "tokens": ["Doch", "der", "Mut\u00b7ter", "nicht", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "O wie wohl bist du verm\u00e4hlt!", "tokens": ["O", "wie", "wohl", "bist", "du", "ver\u00b7m\u00e4hlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses hat dir noch gefehlt.", "tokens": ["Die\u00b7ses", "hat", "dir", "noch", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nste F\u00fcrstinn! dein Vergn\u00fcgen", "tokens": ["Sch\u00f6ns\u00b7te", "F\u00fcrs\u00b7tinn", "!", "dein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist bisher noch stets gestiegen:", "tokens": ["Ist", "bis\u00b7her", "noch", "stets", "ge\u00b7stie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da du nun schon Mutter bist,", "tokens": ["Da", "du", "nun", "schon", "Mut\u00b7ter", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00dft du, nebst dem muntern Knaben,", "tokens": ["Mu\u00dft", "du", ",", "nebst", "dem", "mun\u00b7tern", "Kna\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der dem Vater \u00e4hnlich ist,", "tokens": ["Der", "dem", "Va\u00b7ter", "\u00e4hn\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Etwas, das dir gleichet, haben.", "tokens": ["Et\u00b7was", ",", "das", "dir", "glei\u00b7chet", ",", "ha\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "F\u00fcrstinn! deren Geist und Pracht", "tokens": ["F\u00fcrs\u00b7tinn", "!", "de\u00b7ren", "Geist", "und", "Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PRELAT", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auch G\u00f6ttinnen schamroth macht;", "tokens": ["Auch", "G\u00f6t\u00b7tin\u00b7nen", "scham\u00b7roth", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die du itzt die Schmerzensstunden", "tokens": ["Die", "du", "itzt", "die", "Schmer\u00b7zenss\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Kindbetts \u00fcberwunden;", "tokens": ["Dei\u00b7nes", "Kind\u00b7betts", "\u00fc\u00b7berw\u00b7un\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcrstinn! dich und deinen Sohn", "tokens": ["F\u00fcrs\u00b7tinn", "!", "dich", "und", "dei\u00b7nen", "Sohn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heute w\u00fcrdig zu besingen,", "tokens": ["Heu\u00b7te", "w\u00fcr\u00b7dig", "zu", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fc\u00dfte mir der sch\u00f6nste Ton,", "tokens": ["M\u00fc\u00df\u00b7te", "mir", "der", "sch\u00f6ns\u00b7te", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4chst den Musen selbst, gelingen.", "tokens": ["N\u00e4chst", "den", "Mu\u00b7sen", "selbst", ",", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Gleichwohl wag ichs, da mirs gl\u00fcckt,", "tokens": ["Gleich\u00b7wohl", "wag", "ichs", ",", "da", "mirs", "gl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich dich gesund erblickt,", "tokens": ["Da\u00df", "ich", "dich", "ge\u00b7sund", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was \u00f6ffentlich geschehen,", "tokens": ["Und", "was", "\u00f6f\u00b7fent\u00b7lich", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Vergn\u00fcgen anges