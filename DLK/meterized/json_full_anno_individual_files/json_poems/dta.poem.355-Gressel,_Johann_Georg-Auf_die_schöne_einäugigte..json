{"dta.poem.355": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf die sch\u00f6ne ein\u00e4ugigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hat dir ein Unfall gleich ein Auge weggenommen/", "tokens": ["Hat", "dir", "ein", "Un\u00b7fall", "gleich", "ein", "Au\u00b7ge", "weg\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit gedoppelt sonst die rechte Sch\u00f6nheit prahlt:", "tokens": ["Wo\u00b7mit", "ge\u00b7dop\u00b7pelt", "sonst", "die", "rech\u00b7te", "Sch\u00f6n\u00b7heit", "prahlt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So weine darum nicht/ du bist und bleibest sch\u00f6n/", "tokens": ["So", "wei\u00b7ne", "da\u00b7rum", "nicht", "/", "du", "bist", "und", "blei\u00b7best", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PTKNEG", "$(", "PPER", "VAFIN", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es kan die Lieb\u2019 ins Hertz\u2019 auch durch ein Auge kommen/", "tokens": ["Es", "kan", "die", "Lieb'", "ins", "Hertz'", "auch", "durch", "ein", "Au\u00b7ge", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist nur eine Sonn\u2019/ so diese Welt bestrahlt:", "tokens": ["Es", "ist", "nur", "ei\u00b7ne", "Sonn'", "/", "so", "die\u00b7se", "Welt", "be\u00b7strahlt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$(", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es l\u00e4st sich nur ein Mond am blassen Himmel sehn/", "tokens": ["Es", "l\u00e4st", "sich", "nur", "ein", "Mond", "am", "blas\u00b7sen", "Him\u00b7mel", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie Sonn\u2019 und Mond die Welt genug erleuchten k\u00f6nnen", "tokens": ["Wie", "Sonn'", "und", "Mond", "die", "Welt", "ge\u00b7nug", "er\u00b7leuch\u00b7ten", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "ADV", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hat dein eines Aug\u2019 auch Feuer satt zum brennen.", "tokens": ["So", "hat", "dein", "ei\u00b7nes", "Aug'", "auch", "Feu\u00b7er", "satt", "zum", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ART", "NN", "ADV", "NN", "ADJD", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}