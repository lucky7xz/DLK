{"dta.poem.23833": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Henricus Sanctus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Die Feinde m\u00fcssen sich vor meiner Macht verkriechen;", "tokens": ["Die", "Fein\u00b7de", "m\u00fcs\u00b7sen", "sich", "vor", "mei\u00b7ner", "Macht", "ver\u00b7krie\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Welschland trieb ich weg den gantzen Schwarm der", "tokens": ["Aus", "Wel\u00b7schland", "trieb", "ich", "weg", "den", "gant\u00b7zen", "Schwarm", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dieweil mein Ehgemahl stets Jungfrau bey mir bleibt/", "tokens": ["Die\u00b7weil", "mein", "Eh\u00b7ge\u00b7mahl", "stets", "Jung\u00b7frau", "bey", "mir", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Werd ich der Heiligen Register einverleibt.", "tokens": ["Werd", "ich", "der", "Hei\u00b7li\u00b7gen", "Re\u00b7gis\u00b7ter", "ein\u00b7ver\u00b7leibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}