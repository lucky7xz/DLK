{"textgrid.poem.42268": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Verlust", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wonnelohn getreuer Huldigungen,", "tokens": ["Won\u00b7ne\u00b7lohn", "ge\u00b7treu\u00b7er", "Hul\u00b7di\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dem ich mehr als hundert Monden lang,", "tokens": ["Dem", "ich", "mehr", "als", "hun\u00b7dert", "Mon\u00b7den", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "KOKOM", "CARD", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tag und Nacht, wie gegen Sturm und Drang", "tokens": ["Tag", "und", "Nacht", ",", "wie", "ge\u00b7gen", "Sturm", "und", "Drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der Pilot dem Hafen, nachgerungen!", "tokens": ["Der", "Pi\u00b7lot", "dem", "Ha\u00b7fen", ",", "nach\u00b7ge\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVIZU", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Becher, allgenug f\u00fcr G\u00f6tterzungen,", "tokens": ["Be\u00b7cher", ",", "all\u00b7ge\u00b7nug", "f\u00fcr", "G\u00f6t\u00b7ter\u00b7zun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "lin