 "ist", "die", "Welt", "so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Weg geh\u00fcllt in Schnee.", "tokens": ["Der", "Weg", "ge\u00b7h\u00fcllt", "in", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kann zu meiner Reisen", "tokens": ["Ich", "kann", "zu", "mei\u00b7ner", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht w\u00e4hlen mit der Zeit:", "tokens": ["Nicht", "w\u00e4h\u00b7len", "mit", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df selbst den Weg mir weisen", "tokens": ["Mu\u00df", "selbst", "den", "Weg", "mir", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser Dunkelheit.", "tokens": ["In", "die\u00b7ser", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es zieht ein Mondenschatten", "tokens": ["Es", "zieht", "ein", "Mon\u00b7den\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als mein Gef\u00e4hrte mit,", "tokens": ["Als", "mein", "Ge\u00b7f\u00e4hr\u00b7te", "mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auf den wei\u00dfen Matten", "tokens": ["Und", "auf", "den", "wei\u00b7\u00dfen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Such' ich des Wildes Tritt.", "tokens": ["Such'", "ich", "des", "Wil\u00b7des", "Tritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was soll ich l\u00e4nger weilen,", "tokens": ["Was", "soll", "ich", "l\u00e4n\u00b7ger", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis man mich trieb' hinaus?", "tokens": ["Bis", "man", "mich", "trieb'", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df irre Hunde heulen", "tokens": ["La\u00df", "ir\u00b7re", "Hun\u00b7de", "heu\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor ihres Herren Haus!", "tokens": ["Vor", "ih\u00b7res", "Her\u00b7ren", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Liebe liebt das Wandern, \u2013", "tokens": ["Die", "Lie\u00b7be", "liebt", "das", "Wan\u00b7dern", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "p