{"textgrid.poem.64416": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "34. Das Gl\u00fcck der witzigen K\u00f6pfe in ihrem Vaterlande", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Land, das mich gezeugt, du warest mich nicht werth;", "tokens": ["O", "Land", ",", "das", "mich", "ge\u00b7zeugt", ",", "du", "wa\u00b7rest", "mich", "nicht", "werth", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Witz war dir verha\u00dft, so schrecke dich mein Schwert.", "tokens": ["Mein", "Witz", "war", "dir", "ver\u00b7ha\u00dft", ",", "so", "schre\u00b7cke", "dich", "mein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gesetzt, ich h\u00e4tte nun auch meinen Witz gezwungen,", "tokens": ["Ge\u00b7setzt", ",", "ich", "h\u00e4t\u00b7te", "nun", "auch", "mei\u00b7nen", "Witz", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Thoren nicht verlacht, die M\u00e4dchen nicht besungen,", "tokens": ["Die", "Tho\u00b7ren", "nicht", "ver\u00b7lacht", ",", "die", "M\u00e4d\u00b7chen", "nicht", "be\u00b7sun\u00b7gen", ",