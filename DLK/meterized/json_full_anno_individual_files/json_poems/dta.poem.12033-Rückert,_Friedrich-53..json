{"dta.poem.12033": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer leer im Innern ist, sei au\u00dfen doch gef\u00e4llig;", "tokens": ["Wer", "leer", "im", "In\u00b7nern", "ist", ",", "sei", "au\u00b7\u00dfen", "doch", "ge\u00b7f\u00e4l\u00b7lig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPRART", "NN", "VAFIN", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer einsam m\u00fc\u00dfig geht, thu lieber es gesellig.", "tokens": ["Wer", "ein\u00b7sam", "m\u00fc\u00b7\u00dfig", "geht", ",", "thu", "lie\u00b7ber", "es", "ge\u00b7sel\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch dem erlassen wir die Weltgef\u00e4lligkeit,", "tokens": ["Doch", "dem", "er\u00b7las\u00b7sen", "wir", "die", "Welt\u00b7ge\u00b7f\u00e4l\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer f\u00fcr ein Gotteswerk braucht alle Kraft und Zeit.", "tokens": ["Wer", "f\u00fcr", "ein", "Got\u00b7tes\u00b7werk", "braucht", "al\u00b7le", "Kraft", "und", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der ist in menschlicher Gestalt ein Gott erschienen;", "tokens": ["Der", "ist", "in", "menschli\u00b7cher", "Ge\u00b7stalt", "ein", "Gott", "er\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer kann in gleichem Ma\u00df Gott und den Menschen dienen?", "tokens": ["Wer", "kann", "in", "glei\u00b7chem", "Ma\u00df", "Gott", "und", "den", "Men\u00b7schen", "die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ADJA", "NN", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}