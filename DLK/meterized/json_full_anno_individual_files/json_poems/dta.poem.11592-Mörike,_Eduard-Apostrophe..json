{"dta.poem.11592": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Apostrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr mehr als tausendj\u00e4hrigen,", "tokens": ["Ihr", "mehr", "als", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eichb\u00e4um', ihr rauh-moos-h\u00e4rigen!", "tokens": ["Eich\u00b7b\u00e4um'", ",", "ihr", "rauh\u00b7moos\u00b7h\u00e4\u00b7ri\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.3": {"text": "Ihr, fr\u00f6hlichen, spitz\u00f6hrigen", "tokens": ["Ihr", ",", "fr\u00f6h\u00b7li\u00b7chen", ",", "spit\u00b7z\u00f6h\u00b7ri\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["PPER", "$,", "ADJA", "$,", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Waldteufeln angeh\u00f6rigen!", "tokens": ["Wald\u00b7teu\u00b7feln", "an\u00b7ge\u00b7h\u00f6\u00b7ri\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr lang von wuthbeflissenen", "tokens": ["Ihr", "lang", "von", "wuth\u00b7be\u00b7flis\u00b7se\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Nordst\u00fcrmen wild zerrissenen!", "tokens": ["Nord\u00b7st\u00fcr\u00b7men", "wild", "zer\u00b7ris\u00b7se\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "++-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Nun angeweht von weichlichen", "tokens": ["Nun", "an\u00b7ge\u00b7weht", "von", "weich\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mail\u00fcftchen, unvergleichlichen;", "tokens": ["Mai\u00b7l\u00fcft\u00b7chen", ",", "un\u00b7ver\u00b7gleich\u00b7li\u00b7chen", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$."], "meter": "++-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Und euer Fu\u00df, der t\u00fcchtige,", "tokens": ["Und", "eu\u00b7er", "Fu\u00df", ",", "der", "t\u00fcch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den grimmig der bergschl\u00fcchtige,", "tokens": ["Den", "grim\u00b7mig", "der", "berg\u00b7schl\u00fcch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von Felsen \u00fcberpurzelte", "tokens": ["Von", "Fel\u00b7sen", "\u00fc\u00b7ber\u00b7pur\u00b7zel\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Waldstrom so gern entwurzelte,", "tokens": ["Wald\u00b7strom", "so", "gern", "ent\u00b7wur\u00b7zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Begl\u00e4nzt von B\u00e4chleins Schimmer nun,", "tokens": ["Be\u00b7gl\u00e4nzt", "von", "B\u00e4ch\u00b7leins", "Schim\u00b7mer", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dessen Gespr\u00e4chlein nimmer ruhn:", "tokens": ["Des\u00b7sen", "Ge\u00b7spr\u00e4ch\u00b7lein", "nim\u00b7mer", "ruhn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Von Grund des Herzens preis' ich euch,", "tokens": ["Von", "Grund", "des", "Her\u00b7zens", "preis'", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und \u00fcbergl\u00fccklich hei\u00df' ich euch,", "tokens": ["Und", "\u00fc\u00b7berg\u00b7l\u00fcck\u00b7lich", "hei\u00df'", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df ihr so hoch euch beide streckt", "tokens": ["Da\u00df", "ihr", "so", "hoch", "euch", "bei\u00b7de", "streckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und in so dicken H\u00e4uten steckt,", "tokens": ["Und", "in", "so", "di\u00b7cken", "H\u00e4u\u00b7ten", "steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df, was ich euch in k\u00fcnstlichen,", "tokens": ["Da\u00df", ",", "was", "ich", "euch", "in", "k\u00fcnst\u00b7li\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So \u00e4u\u00dferst sprachverdienstlichen", "tokens": ["So", "\u00e4u\u00b7\u00dferst", "sprach\u00b7ver\u00b7dienst\u00b7li\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Reimweisen eben vorgesungen,", "tokens": ["Reim\u00b7wei\u00b7sen", "e\u00b7ben", "vor\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Euch gar nicht an das Ohr gedrungen.", "tokens": ["Euch", "gar", "nicht", "an", "das", "Ohr", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}