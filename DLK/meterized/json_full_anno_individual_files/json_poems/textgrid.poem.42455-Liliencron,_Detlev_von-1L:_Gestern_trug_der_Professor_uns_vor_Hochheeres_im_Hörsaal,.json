{"textgrid.poem.42455": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gestern trug der Professor uns vor Hochheeres im H\u00f6rsaal,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern trug der Professor uns vor Hochheeres im H\u00f6rsaal,", "tokens": ["Ge\u00b7stern", "trug", "der", "Pro\u00b7fes\u00b7sor", "uns", "vor", "Hoch\u00b7hee\u00b7res", "im", "H\u00f6r\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sprach von Platon, Homer, k\u00fcndet Apelles' Verdienst,", "tokens": ["Sprach", "von", "Pla\u00b7ton", ",", "Ho\u00b7mer", ",", "k\u00fcn\u00b7det", "A\u00b7pel\u00b7les'", "Ver\u00b7dienst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "NE", "$,", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Und dick troff ihm die Stirn von heiliger Weihe wie Angstschwei\u00df,", "tokens": ["Und", "dick", "troff", "ihm", "die", "Stirn", "von", "hei\u00b7li\u00b7ger", "Wei\u00b7he", "wie", "A\u00b7ngstschwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Uns auch tropfte die Stirn, wehe, der Juni war schw\u00fcl.", "tokens": ["Uns", "auch", "tropf\u00b7te", "die", "Stirn", ",", "we\u00b7he", ",", "der", "Ju\u00b7ni", "war", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u00bbseht,\u00ab so rief er erhaben, \u00bbdie Griechen, die nenn' ich ein Volk noch,", "tokens": ["\u00bb", "seht", ",", "\u00ab", "so", "rief", "er", "er\u00b7ha\u00b7ben", ",", "\u00bb", "die", "Grie\u00b7chen", ",", "die", "nenn'", "ich", "ein", "Volk", "noch", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "$(", "ADV", "VVFIN", "PPER", "ADJD", "$,", "$(", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Herrliche Strenge der Form, g\u00f6ttliches Nasenger\u00fcst.", "tokens": ["Herr\u00b7li\u00b7che", "Stren\u00b7ge", "der", "Form", ",", "g\u00f6tt\u00b7li\u00b7ches", "Na\u00b7sen\u00b7ge\u00b7r\u00fcst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nichts war ihnen bekannt von des Nordens barbarischem Rohtrotz,", "tokens": ["Nichts", "war", "ih\u00b7nen", "be\u00b7kannt", "von", "des", "Nor\u00b7dens", "bar\u00b7ba\u00b7ri\u00b7schem", "Roh\u00b7trotz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Zeus, ah, regierte die Welt, flammte vom hohen Olymp.\u00ab", "tokens": ["Zeus", ",", "ah", ",", "re\u00b7gier\u00b7te", "die", "Welt", ",", "flamm\u00b7te", "vom", "ho\u00b7hen", "O\u00b7lymp", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ach, mir schwitzte das Hirn, ich befand mich im Brodel des W\u00fcstseins,", "tokens": ["Ach", ",", "mir", "schwitz\u00b7te", "das", "Hirn", ",", "ich", "be\u00b7fand", "mich", "im", "Bro\u00b7del", "des", "W\u00fcst\u00b7seins", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Drau\u00dfen der Sommer so sch\u00f6n, sa\u00dfen wir drinnen im Pferch.", "tokens": ["Drau\u00b7\u00dfen", "der", "Som\u00b7mer", "so", "sch\u00f6n", ",", "sa\u00b7\u00dfen", "wir", "drin\u00b7nen", "im", "Pferch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Endlich ert\u00f6nte das Zeichen, wir st\u00fcrmten hinaus in die Freiheit,", "tokens": ["End\u00b7lich", "er\u00b7t\u00f6n\u00b7te", "das", "Zei\u00b7chen", ",", "wir", "st\u00fcrm\u00b7ten", "hin\u00b7aus", "in", "die", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Mit der Mapp' im Arm, schleppt' ich mein Wissen nach Haus.", "tokens": ["Mit", "der", "Mapp'", "im", "Arm", ",", "schleppt'", "ich", "mein", "Wis\u00b7sen", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Dort auf dem Tische der Zettel: \u00bbGewartet hab' ich vergebens,\u00ab", "tokens": ["Dort", "auf", "dem", "Ti\u00b7sche", "der", "Zet\u00b7tel", ":", "\u00bb", "Ge\u00b7war\u00b7tet", "hab'", "ich", "ver\u00b7ge\u00b7bens", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$.", "$(", "VVPP", "VAFIN", "PPER", "ADV", "$,", "$("], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Sagte mir deutlich und klar: Griechisches fand sich nicht ein;", "tokens": ["Sag\u00b7te", "mir", "deut\u00b7lich", "und", "klar", ":", "Grie\u00b7chi\u00b7sches", "fand", "sich", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "NE", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Aber Poppinga war da, mit dem h\u00f6chst unklassischen Nasbein;", "tokens": ["A\u00b7ber", "Pop\u00b7pin\u00b7ga", "war", "da", ",", "mit", "dem", "h\u00f6chst", "un\u00b7klas\u00b7si\u00b7schen", "Nas\u00b7be\u00b7in", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.16": {"text": "Und nun ist es zu sp\u00e4t, hol' dich der Satanas, Zeus.", "tokens": ["Und", "nun", "ist", "es", "zu", "sp\u00e4t", ",", "hol'", "dich", "der", "Sa\u00b7ta\u00b7nas", ",", "Zeus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "NE", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Gestern trug der Professor uns vor Hochheeres im H\u00f6rsaal,", "tokens": ["Ge\u00b7stern", "trug", "der", "Pro\u00b7fes\u00b7sor", "uns", "vor", "Hoch\u00b7hee\u00b7res", "im", "H\u00f6r\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sprach von Platon, Homer, k\u00fcndet Apelles' Verdienst,", "tokens": ["Sprach", "von", "Pla\u00b7ton", ",", "Ho\u00b7mer", ",", "k\u00fcn\u00b7det", "A\u00b7pel\u00b7les'", "Ver\u00b7dienst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "NE", "$,", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Und dick troff ihm die Stirn von heiliger Weihe wie Angstschwei\u00df,", "tokens": ["Und", "dick", "troff", "ihm", "die", "Stirn", "von", "hei\u00b7li\u00b7ger", "Wei\u00b7he", "wie", "A\u00b7ngstschwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa