 ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Besieht mit finstern Blick,", "tokens": ["Be\u00b7sieht", "mit", "fins\u00b7tern", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und den, der Geld bewacht,", "tokens": ["Und", "den", ",", "der", "Geld", "be\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Den qu\u00e4l er Tag und Nacht!", "tokens": ["Den", "qu\u00e4l", "er", "Tag", "und", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Furcht, die Traurigkeit,", "tokens": ["Die", "Furcht", ",", "die", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Kummer um die Zeit,", "tokens": ["Den", "Kum\u00b7mer", "um", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die morgen kommen soll.", "tokens": ["Die", "mor\u00b7gen", "kom\u00b7men", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vertreib du mir, Apoll!", "tokens": ["Ver\u00b7treib", "du", "mir", ",", "A\u00b7poll", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "NE", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Mir gieb dein Saitenspiel", "tokens": ["Mir", "gieb", "dein", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den Freunden gieb Gef\u00fchl", "tokens": ["Den", "Freun\u00b7den", "gieb", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVIMP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der klugen Welt Geh\u00f6r;", "tokens": ["Der", "klu\u00b7gen", "Welt", "Ge\u00b7h\u00f6r", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dann heisch ich mir nichts mehr", "tokens": ["Dann", "heisch", "ich", "mir", "nichts", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PPER", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Als n\u00e4chtlich sanfte Ruh", "tokens": ["Als", "n\u00e4cht\u00b7lich", "sanf\u00b7te", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Vom Vater Zeus dazu.", "tokens": ["Vom", "Va\u00b7ter", "Zeus", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein ist kein Winkel Land", "tokens": ["Mein", "ist", "kein", "Win\u00b7kel", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und keine Traubenwand;", "tokens": ["Und", "kei\u00b7ne", "Trau\u0