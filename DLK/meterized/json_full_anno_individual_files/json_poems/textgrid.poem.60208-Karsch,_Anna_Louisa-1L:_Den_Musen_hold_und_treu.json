"Kein", "fal\u00b7scher", "Freund", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du Bruder von dem May", "tokens": ["Du", "Bru\u00b7der", "von", "dem", "May"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Becr\u00e4nzter Monath sey", "tokens": ["Be\u00b7cr\u00b7\u00e4nz\u00b7ter", "Mo\u00b7nath", "sey"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit deinen Rosen mein", "tokens": ["Mit", "dei\u00b7nen", "Ro\u00b7sen", "mein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Streu sie um unsern Wein!", "tokens": ["Streu", "sie", "um", "un\u00b7sern", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die j\u00fcngsten, die du hast", "tokens": ["Die", "j\u00fcng\u00b7sten", ",", "die", "du", "hast"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gieb mir f\u00fcr Wirth", "tokens": ["Gieb", "mir", "f\u00fcr", "Wirth"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Becr\u00e4nzet sey ihr Haupt", "tokens": ["Be\u00b7cr\u00e4n\u00b7zet", "sey", "ihr", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"t