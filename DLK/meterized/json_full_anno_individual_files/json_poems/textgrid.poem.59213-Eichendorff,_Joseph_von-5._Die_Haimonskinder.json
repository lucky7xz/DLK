{"textgrid.poem.59213": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "5. Die Haimonskinder", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf feur'gem Rosse kommt Bacchus daher,", "tokens": ["Auf", "feur'\u00b7gem", "Ros\u00b7se", "kommt", "Bac\u00b7chus", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NE", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Becher hoch in der Hand,", "tokens": ["Den", "Be\u00b7cher", "hoch", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sein R\u00f6\u00dflein wird wild, sein Kopf ist ihm schwer,", "tokens": ["Sein", "R\u00f6\u00df\u00b7lein", "wird", "wild", ",", "sein", "Kopf", "ist", "ihm", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er versch\u00fcttet den Wein auf das Land.", "tokens": ["Er", "ver\u00b7sch\u00fct\u00b7tet", "den", "Wein", "auf", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Den Dichter erbarmet der Rebensaft,", "tokens": ["Den", "Dich\u00b7ter", "er\u00b7bar\u00b7met", "der", "Re\u00b7ben\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In den B\u00fcgel er k\u00fchn sich stellt", "tokens": ["In", "den", "B\u00fc\u00b7gel", "er", "k\u00fchn", "sich", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "PRF", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und trinkt mit dem Gotte Br\u00fcderschaft \u2013", "tokens": ["Und", "trinkt", "mit", "dem", "Got\u00b7te", "Br\u00fc\u00b7der\u00b7schaft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nun geht's erst, als ging's aus der Welt!", "tokens": ["Nun", "geht's", "erst", ",", "als", "ging's", "aus", "der", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "\u00bbei, sieh da, so einsam, Herr Komponist!", "tokens": ["\u00bb", "ei", ",", "sieh", "da", ",", "so", "ein\u00b7sam", ",", "Herr", "Kom\u00b7po\u00b7nist", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VVIMP", "ADV", "$,", "ADV", "ADJD", "$,", "NN", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Steig auf mit, 's ist schad um die Schuh,", "tokens": ["Steig", "auf", "mit", ",", "'s", "ist", "schad", "um", "die", "Schuh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PTKVZ", "$,", "PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Du l\u00f6st erst die Schwinge \u2013 und wo keine ist,", "tokens": ["Du", "l\u00f6st", "erst", "die", "Schwin\u00b7ge", "\u2013", "und", "wo", "kei\u00b7ne", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "KON", "PWAV", "PIAT", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da mach uns die Fl\u00fcgel dazu!\u00ab", "tokens": ["Da", "mach", "uns", "die", "Fl\u00fc\u00b7gel", "da\u00b7zu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und was sie ersonnen nun, singen die drei.", "tokens": ["Und", "was", "sie", "er\u00b7son\u00b7nen", "nun", ",", "sin\u00b7gen", "die", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "ADV", "$,", "VVFIN", "ART", "CARD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u00bbo weh!\u00ab ruft ein S\u00e4nger herauf,", "tokens": ["\u00bb", "o", "weh", "!", "\u00ab", "ruft", "ein", "S\u00e4n\u00b7ger", "her\u00b7auf", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$.", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbihr schreit ja die k\u00f6stlichsten Noten entzwei!\u00ab", "tokens": ["\u00bb", "ihr", "schreit", "ja", "die", "k\u00f6st\u00b7lichs\u00b7ten", "No\u00b7ten", "ent\u00b7zwei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": 