{"textgrid.poem.57109": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Reinheit ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du, was Reinheit ist? So sieh den Abendstern", "tokens": ["Wei\u00dft", "du", ",", "was", "Rein\u00b7heit", "ist", "?", "So", "sieh", "den", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWS", "NN", "VAFIN", "$.", "ADV", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "hoch \u00fcber blassen Wolken gl\u00e4nzen, still und klar.", "tokens": ["hoch", "\u00fc\u00b7ber", "blas\u00b7sen", "Wol\u00b7ken", "gl\u00e4n\u00b7zen", ",", "still", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So gl\u00e4nzt auch unser armer Erdenball vielleicht", "tokens": ["So", "gl\u00e4nzt", "auch", "un\u00b7ser", "ar\u00b7mer", "Er\u00b7den\u00b7ball", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nach einem fernen Stern, und dort erhebt ein Mensch,", "tokens": ["nach", "ei\u00b7nem", "fer\u00b7nen", "Stern", ",", "und", "dort", "er\u00b7hebt", "ein", "Mensch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wie hier, den Blick und fragt: Wei\u00dft du, was Reinheit ist?", "tokens": ["wie", "hier", ",", "den", "Blick", "und", "fragt", ":", "Wei\u00dft", "du", ",", "was", "Rein\u00b7heit", "ist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ART", "NN", "KON", "VVFIN", "$.", "VVFIN", "PPER", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und eine Frauenstimme sagt vielleicht zu ihm:", "tokens": ["Und", "ei\u00b7ne", "Frau\u00b7en\u00b7stim\u00b7me", "sagt", "viel\u00b7leicht", "zu", "ihm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du meinst den Abendstern da droben, still und klar?", "tokens": ["Du", "meinst", "den", "A\u00b7bends\u00b7tern", "da", "dro\u00b7ben", ",", "still", "und", "klar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADV", "ADV", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-----+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Dort mu\u00df es wohnen, dort, das Gl\u00fcck, das wahre Gl\u00fcck!", "tokens": ["Dort", "mu\u00df", "es", "woh\u00b7nen", ",", "dort", ",", "das", "Gl\u00fcck", ",", "das", "wah\u00b7re", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "ADV", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und feucht wird Beider Aug. \u2013 W