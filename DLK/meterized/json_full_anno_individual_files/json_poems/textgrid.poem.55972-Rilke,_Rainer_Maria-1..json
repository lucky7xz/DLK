{"textgrid.poem.55972": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hohe Tor scheint keine einzuhalten,", "tokens": ["Das", "ho\u00b7he", "Tor", "scheint", "kei\u00b7ne", "ein\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Br\u00fccke geht gleich gerne hin und her,", "tokens": ["die", "Br\u00fc\u00b7cke", "geht", "gleich", "ger\u00b7ne", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und doch sind sicher alle in dem alten", "tokens": ["und", "doch", "sind", "si\u00b7cher", "al\u00b7le", "in", "dem", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "PIS", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "offenen Ulmenhof und gehn nicht mehr", "tokens": ["of\u00b7fe\u00b7nen", "Ul\u00b7men\u00b7hof", "und", "gehn", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "VVFIN", "PTKNEG", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "aus ihren H\u00e4usern, als auf jenem Streifen", "tokens": ["aus", "ih\u00b7ren", "H\u00e4u\u00b7sern", ",", "als", "auf", "je\u00b7nem", "Strei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "zur Kirche hin, um besser zu begreifen", "tokens": ["zur", "Kir\u00b7che", "hin", ",", "um", "bes\u00b7ser", "zu", "be\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KOUI", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "warum in ihnen so viel Liebe war.", "tokens": ["wa\u00b7rum", "in", "ih\u00b7nen", "so", "viel", "Lie\u00b7be", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort knieen sie, verdeckt mit reinem Leinen,", "tokens": ["Dort", "kni\u00b7e\u00b7en", "sie", ",", "ver\u00b7deckt", "mit", "rei\u00b7nem", "Lei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "so gleich, als w\u00e4re nur das Bild der einen", "tokens": ["so", "gleich", ",", "als", "w\u00e4\u00b7re", "nur", "das", "Bild", "der", "ei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOKOM", "VAFIN", "ADV", "ART", "NN", "ART", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "tausendmal im Choral, der tief und klar", "tokens": ["tau\u00b7send\u00b7mal", "im", "Cho\u00b7ral", ",", "der", "tief", "und", "klar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "zu Spiegeln wird an den verteilten Pfeilern;", "tokens": ["zu", "Spie\u00b7geln", "wird", "an", "den", "ver\u00b7teil\u00b7ten", "Pfei\u00b7lern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "und ihre Stimmen gehn den immer steilern", "tokens": ["und", "ih\u00b7re", "Stim\u00b7men", "gehn", "den", "im\u00b7mer", "stei\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gesang hinan und werfen sich von dort,", "tokens": ["Ge\u00b7sang", "hi\u00b7nan", "und", "wer\u00b7fen", "sich", "von", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "wo es nicht weitergeht, vom letzten Wort,", "tokens": ["wo", "es", "nicht", "wei\u00b7ter\u00b7geht", ",", "vom", "letz\u00b7ten", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "den Engeln zu, die sie nicht wiedergeben.", "tokens": ["den", "En\u00b7geln", "zu", ",", "die", "sie", "nicht", "wie\u00b7der\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum sind die unten, wenn sie sich erheben", "tokens": ["Drum", "sind", "die", "un\u00b7ten", ",", "wenn", "sie", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADV", "$,", "KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und wenden, still. Drum reichen sie sich schweigend", "tokens": ["und", "wen\u00b7den", ",", "still", ".", "Drum", "rei\u00b7chen", "sie", "sich", "schwei\u00b7gend"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "ADJD", "$.", "PAV", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit einem Neigen, Zeigende zu zeigend", "tokens": ["mit", "ei\u00b7nem", "Nei\u00b7gen", ",", "Zei\u00b7gen\u00b7de", "zu", "zei\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empfangenden, geweihtes Wasser, das", "tokens": ["Emp\u00b7fan\u00b7gen\u00b7den", ",", "ge\u00b7weih\u00b7tes", "Was\u00b7ser", ",", "das"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "die Stirnen k\u00fchl macht und die Munde bla\u00df.", "tokens": ["die", "Stir\u00b7nen", "k\u00fchl", "macht", "und", "die", "Mun\u00b7de", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und gehen dann, verhangen und verhalten,", "tokens": ["Und", "ge\u00b7hen", "dann", ",", "ver\u00b7han\u00b7gen", "und", "ver\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word",