{"textgrid.poem.56710": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Pro domo", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr wollt mich nicht? Ihr werdet mich schon wollen", "tokens": ["Ihr", "wollt", "mich", "nicht", "?", "Ihr", "wer\u00b7det", "mich", "schon", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "PPER", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und euch nach meiner Zauberpfeife tollen", "tokens": ["und", "euch", "nach", "mei\u00b7ner", "Zau\u00b7ber\u00b7pfei\u00b7fe", "tol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so leicht, als ob ihr ihren Klang verst\u00fcndet,", "tokens": ["so", "leicht", ",", "als", "ob", "ihr", "ih\u00b7ren", "Klang", "ver\u00b7st\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ihr werdet schrein, ihr h\u00e4ttet sie ergr\u00fcndet,", "tokens": ["ihr", "wer\u00b7det", "schrein", ",", "ihr", "h\u00e4t\u00b7tet", "sie", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "bis ihr sie n\u00e4chstens selbst zu blasen prahlt;", "tokens": ["bis", "ihr", "sie", "n\u00e4chs\u00b7tens", "selbst", "zu", "bla\u00b7sen", "prahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ihr affenelendes Geschlecht, es malt", "tokens": ["ihr", "af\u00b7fe\u00b7nel\u00b7en\u00b7des", "Ge\u00b7schlecht", ",", "es", "malt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "auf eure niedre Stirne tintendick", "tokens": ["auf", "eu\u00b7re", "nied\u00b7re", "Stir\u00b7ne", "tin\u00b7ten\u00b7dick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und g\u00e4nzlich unverkennbar die Natur", "tokens": ["und", "g\u00e4nz\u00b7lich", "un\u00b7ver\u00b7kenn\u00b7bar", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "der unheilbaren Dummheit Signatur \u2013", "tokens": ["der", "un\u00b7heil\u00b7ba\u00b7ren", "Dumm\u00b7heit", "Sig\u00b7na\u00b7tur", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr wollt mich nicht? Ihr werdet mich schon wollen", "tokens": ["Ihr", "wollt", "mich", "nicht", "?", "Ihr", "wer\u00b7det", "mich", "schon", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "PPER", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und euch na