{"textgrid.poem.59720": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Klage um Auerswald und Lichnowsky", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du noch Lebensodem,", "tokens": ["Hast", "du", "noch", "Le\u00b7ben\u00b7so\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Erde gr\u00fcn und sch\u00f6n,", "tokens": ["O", "Er\u00b7de", "gr\u00fcn", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um die aus schwarzem Brodem", "tokens": ["Um", "die", "aus", "schwar\u00b7zem", "Bro\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur finstre Nebel wehn,", "tokens": ["Nur", "finst\u00b7re", "Ne\u00b7bel", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf der blutwilde Horden", "tokens": ["Auf", "der", "blut\u00b7wil\u00b7de", "Hor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Brand, Mord und Zeter schrein", "tokens": ["Brand", ",", "Mord", "und", "Ze\u00b7ter", "schrein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und frech in Meuchelmorden", "tokens": ["Und", "frech", "in", "Meu\u0