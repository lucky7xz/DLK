 "pos": ["PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr Wolf und f\u00fcr Hy\u00e4ne,", "tokens": ["F\u00fcr", "Wolf", "und", "f\u00fcr", "Hy\u00b7\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch nicht f\u00fcr Deutsche gut?", "tokens": ["Doch", "nicht", "f\u00fcr", "Deut\u00b7sche", "gut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sind dies der Freiheit Gaben?", "tokens": ["Sind", "dies", "der", "Frei\u00b7heit", "Ga\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist dies der Freiheit Klang,", "tokens": ["Ist", "dies", "der", "Frei\u00b7heit", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von schwarzen Galgenraben", "tokens": ["Von", "schwar\u00b7zen", "Gal\u00b7gen\u00b7ra\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Mitternachtgesang?", "tokens": ["Der", "Mit\u00b7ter\u00b7nacht\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nein! Nein! Von Freiheitst\u00f6tern", "tokens": ["Nein", "!", "Nein", "!", "Von", "Frei\u00b7heits\u00b7t\u00f6\u00b7tern"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "PTKANT", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Blindschleichs Schlangenlist,", "tokens": ["Des", "Blind\u00b7schleichs", "Schlan\u00b7gen\u00b7list", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo unter grausen Zetern", "tokens": ["Wo", "un\u00b7ter", "grau\u00b7sen", "Ze\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Laut der Freiheit ist.", "tokens": ["Kein", "Laut", "der", "Frei\u00b7heit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ist dies die deutsche Treue?", "tokens": ["Ist", "dies", "die", "deut\u00b7sche", "Treu\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trifft so das deutsche Schwert?", "tokens": ["Trifft", "so", "das", "deut\u00b7sche", "Schwert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Springt so der deutsche Leue,", "tokens": ["Springt", "so", "der", "deut\u00b7sche", "Leu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der grad' aufs Eisen f\u00e4hrt?", "tokens": ["Der", "grad'", "aufs", "Ei\u00b7sen", "f\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch sieht man solche Tat an,", "tokens": ["Doch", "sieht", "man", "sol\u00b7che", "Tat", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "So bricht das Herz inzwei.", "tokens": ["So", "bricht", "das", "Herz", "in\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwei Helden sind gefallen,", "tokens": ["Zwei", "Hel\u00b7den", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht, wie der Tapfre f\u00e4llt", "tokens": ["Nicht", ",", "wie", "der", "Tapf\u00b7re", "f\u00e4llt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei hellem Trommelschallen", "tokens": ["Bei", "hel\u00b7lem", "Trom\u00b7mel\u00b7schal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf blut'gem Schlachtenfeld;", "tokens": ["Auf", "blut'\u00b7gem", "Schlach\u00b7ten\u00b7feld", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben andre Rosen", "tokens": ["Sie", "ha\u00b7ben", "and\u00b7re", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weiland gepfl\u00fcckt im Streit:", "tokens": ["Wei\u00b7land", "ge\u00b7pfl\u00fcckt", "im", "Streit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Was war den Waffenlosen", "tokens": ["Was", "war", "den", "Waf\u00b7fen\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hier f\u00fcr ein Kampf bereit?", "tokens": ["Hier", "f\u00fcr", "ein", "Kampf", "be\u00b7reit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mein Deutschland, Land der Treue!", "tokens": ["Mein", "Deutschland", ",", "Land", "der", "Treu\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mein Deutschland, Land des Muts!", "tokens": ["Mein", "Deutschland", ",", "Land", "des", "Muts", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wann l\u00f6schet lange Reue", "tokens": ["Wann", "l\u00f6\u00b7schet", "lan\u00b7ge", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Flecken solchen Bluts?", "tokens": ["Die", "Fle\u00b7cken", "sol\u00b7chen", "Bluts", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Mord, womit der Feige", "tokens": ["Den", "Mord", ",", "wo\u00b7mit", "der", "Fei\u00b7ge"], "token_info": ["word", "word", "punct", "word",