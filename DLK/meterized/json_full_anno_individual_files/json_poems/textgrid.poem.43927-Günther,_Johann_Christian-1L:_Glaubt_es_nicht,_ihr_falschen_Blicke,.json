{"textgrid.poem.43927": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Glaubt es nicht, ihr falschen Blicke,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaubt es nicht, ihr falschen Blicke,", "tokens": ["Glaubt", "es", "nicht", ",", "ihr", "fal\u00b7schen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ihr mich ins Neze zieht,", "tokens": ["Da\u00df", "ihr", "mich", "ins", "Ne\u00b7ze", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil mein Herz auch goldne Stricke", "tokens": ["Weil", "mein", "Herz", "auch", "gold\u00b7ne", "Stri\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gepuzte Br\u00fccken flieht.", "tokens": ["Und", "ge\u00b7puz\u00b7te", "Br\u00fc\u00b7cken", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Farbe kan den Geist wohl st\u00e4rcken,", "tokens": ["Far\u00b7be", "kan", "den", "Geist", "wohl", "st\u00e4r\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Mienen Schmeicheley", "tokens": ["Und", "der", "Mie\u00b7nen", "Schmei\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dient wohl oft zu Satans Wercken,", "tokens": ["Dient", "wohl", "oft", "zu", "Sa\u00b7tans", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber nicht zu wahrer Treu.", "tokens": ["A\u00b7ber", "nicht", "zu", "wah\u00b7rer", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie manchem k\u00f6mmt der Glaube", "tokens": ["O", "wie", "man\u00b7chem", "k\u00f6mmt", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Nachreu in die Hand,", "tokens": ["Mit", "der", "Nach\u00b7reu", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er bey verbuhltem Raube", "tokens": ["Wenn", "er", "bey", "ver\u00b7buhl\u00b7tem", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kraft und Kosten aufgewand.", "tokens": ["Kraft", "und", "Kos\u00b7ten", "auf\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie das Morgenroth dem Tage", "tokens": ["Wie", "das", "Mor\u00b7gen\u00b7roth", "dem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wind und Regen prophezeit,", "tokens": ["Wind", "und", "Re\u00b7gen", "pro\u00b7phe\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also kommt ein Haus voll Plage", "tokens": ["Al\u00b7so", "kommt", "ein", "Haus", "voll", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch ein Kind der Eitelkeit.", "tokens": ["Durch", "ein", "Kind", "der", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blumen stehn in ihrem Kleide", "tokens": ["Blu\u00b7men", "stehn", "in", "ih\u00b7rem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Feldern noch so sch\u00f6n", "tokens": ["Auf", "den", "Fel\u00b7dern", "noch", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als auf Leinwand oder Seide,", "tokens": ["Als", "auf", "Lein\u00b7wand", "o\u00b7der", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie Strich und Kunst erh\u00f6hn;", "tokens": ["Wo", "sie", "Strich", "und", "Kunst", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir gef\u00e4llt bey netten Sachen", "tokens": ["Mir", "ge\u00b7f\u00e4llt", "bey", "net\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stets die Einfalt der Natur,", "tokens": ["Stets", "die", "Ein\u00b7falt", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und wo fremde Wangen lachen,", "tokens": ["Und", "wo", "frem\u00b7de", "Wan\u00b7gen", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht mein Eckel gleich die Spur.", "tokens": ["Sieht", "mein", "E\u00b7ckel", "gleich", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00dcberhaupt bl\u00fcht mein Vergn\u00fcgen", "tokens": ["\u00dc\u00b7ber\u00b7haupt", "bl\u00fcht", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch bis jezo ganz allein;", "tokens": ["Noch", "bis", "je\u00b7zo", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll was S\u00fc\u00dfes bey mir liegen,", "tokens": ["Soll", "was", "S\u00fc\u00b7\u00dfes", "bey", "mir", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df es nur die Freyheit seyn,", "tokens": ["Mu\u00df", "es", "nur", "die", "Frey\u00b7heit", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil mein Geist an ihrer Seite", "tokens": ["Weil", "mein", "Geist", "an", "ih\u00b7rer", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lauter Himmelstr\u00e4ume sp\u00fcrt,", "tokens": ["Lau\u00b7ter", "Him\u00b7mel\u00b7str\u00e4u\u00b7me", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob gleich Belgrads reiche Beute", "tokens": ["Ob", "gleich", "Bel\u00b7grads", "rei\u00b7che", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eben nicht mein Lager ziert.", "tokens": ["E\u00b7ben", "nicht", "mein", "La\u00b7ger", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwar ich will es nicht verschw\u00f6ren,", "tokens": ["Zwar", "ich", "will", "es", "nicht", "ver\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Liebe, wie man sagt,", "tokens": ["Weil", "die", "Lie\u00b7be", ",", "wie", "man", "sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, so ihr den R\u00fccken kehren,", "tokens": ["Die", ",", "so", "ihr", "den", "R\u00fc\u00b7cken", "keh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00d6fters unverhoft erjagt;", "tokens": ["\u00d6f\u00b7ters", "un\u00b7ver\u00b7hoft", "er\u00b7jagt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich befind auch mir im Herzen", "tokens": ["Ich", "be\u00b7find", "auch", "mir", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen Zunder, der leicht f\u00e4ngt,", "tokens": ["Ei\u00b7nen", "Zun\u00b7der", ",", "der", "leicht", "f\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der sch\u00f6nen Kinder Scherzen", "tokens": ["Wenn", "der", "sch\u00f6\u00b7nen", "Kin\u00b7der", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lust und Glut ins Auge senckt.", "tokens": ["Lust", "und", "Glut", "ins", "Au\u00b7ge", "senckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So weit kann ich mich verme\u00dfen,", "tokens": ["So", "weit", "kann", "ich", "mich", "ver\u00b7me\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df mich wohl kein Kind ber\u00fcckt,", "tokens": ["Da\u00df", "mich", "wohl", "kein", "Kind", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "De\u00dfen Anmuth und Caressen", "tokens": ["De\u00b7\u00dfen", "An\u00b7muth", "und", "Ca\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Nicht der Tugend Wohlstand schm\u00fcckt;", "tokens": ["Nicht", "der", "Tu\u00b7gend", "Wohl\u00b7stand", "schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Find ich Wiz und Treu beysammen", "tokens": ["Find", "ich", "Wiz", "und", "Treu", "bey\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Vernunft und Zucht vermehlt,", "tokens": ["Und", "Ver\u00b7nunft", "und", "Zucht", "ver\u00b7mehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O so will ich gern die Flammen,", "tokens": ["O", "so", "will", "ich", "gern", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deren Reizung z\u00e4rtlich qu\u00e4lt.", "tokens": ["De\u00b7ren", "Rei\u00b7zung", "z\u00e4rt\u00b7lich", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Glaubt es nicht, ihr falschen Blicke,", "tokens": ["Glaubt", "es", "nicht", ",", "ihr", "fal\u00b7schen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ihr mich ins Neze zieht,", "tokens": ["Da\u00df", "ihr", "mich", "ins", "Ne\u00b7ze", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil mein Herz auch goldne Stricke", "tokens": ["Weil", "mein", "Herz", "auch", "gold\u00b7ne", "Stri\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gepuzte Br\u00fccken flieht.", "tokens": ["Und", "ge\u00b7puz\u00b7te", "Br\u00fc\u00b7cken", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Farbe kan den Geist wohl st\u00e4rcken,", "tokens": ["Far\u00b7be", "kan", "den", "Geist", "wohl", "st\u00e4r\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Mienen Schmeicheley", "tokens": ["Und", "der", "Mie\u00b7nen", "Schmei\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dient wohl oft zu Satans Wercken,", "tokens": ["Dient", "wohl", "oft", "zu", "Sa\u00b7tans", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber nicht zu wahrer Treu.", "tokens": ["A\u00b7ber", "nicht", "zu", "wah\u00b7rer", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O wie manchem k\u00f6mmt der Glaube", "tokens": ["O", "wie", "man\u00b7chem", "k\u00f6mmt", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Nachreu in die Hand,", "tokens": ["Mit", "der", "Nach\u00b7reu", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er bey v