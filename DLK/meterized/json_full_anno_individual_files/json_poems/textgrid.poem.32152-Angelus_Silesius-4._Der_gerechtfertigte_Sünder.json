{"textgrid.poem.32152": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "4. Der gerechtfertigte S\u00fcnder", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war des Teufels Sklav und ging in seinen Banden,", "tokens": ["Ich", "war", "des", "Teu\u00b7fels", "Sklav", "und", "ging", "in", "sei\u00b7nen", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich war mit S\u00fcndenwust verstellt und blutig rot,", "tokens": ["Ich", "war", "mit", "S\u00fcn\u00b7den\u00b7wust", "ver\u00b7stellt", "und", "blu\u00b7tig", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Wollust w\u00e4lzt ich mich wie eine Sau im Kot,", "tokens": ["In", "Wol\u00b7lust", "w\u00e4lzt", "ich", "mich", "wie", "ei\u00b7ne", "Sau", "im", "Kot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich stank vor Eitelkeit, die h\u00e4ufig war vorhanden,", "tokens": ["Ich", "stank", "vor", "Ei\u00b7tel\u00b7keit", ",", "die", "h\u00e4u\u00b7fig", "war", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich war dem Abgrund nah und fing schon an zu stranden,", "tokens": ["Ich", "war", "dem", "Ab\u00b7grund", "nah", "und", "fing", "schon", "an", "zu", "stran\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich lebte wie ein Vieh und fragte nicht nach Gott,", "tokens": ["Ich", "leb\u00b7te", "wie", "ein", "Vieh", "und", "frag\u00b7te", "nicht", "nach", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "KON", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich war ein Schattenmensch und noch lebendig tot.", "tokens": ["Ich", "war", "ein", "Schat\u00b7ten\u00b7mensch", "und", "noch", "le\u00b7ben\u00b7dig", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nun bin ich wiederum in Christo auferstanden", "tokens": ["Nun", "bin", "ich", "wie\u00b7de\u00b7rum", "in", "Chris\u00b7to", "auf\u00b7er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und lebendig gemacht; die Ketten sind entzwei,", "tokens": ["Und", "le\u00b7ben\u00b7dig", "ge\u00b7macht", ";", "die", "Ket\u00b7ten", "sind", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Teufel ist verjagt und ich bin los und frei.", "tokens": ["Der", "Teu\u00b7fel", "ist", "ver\u00b7jagt", "und", "ich", "bin", "los", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "PPER", "VAFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich suche Gott allein mit eifrigem Gem\u00fcte", "tokens": ["Ich", "su\u00b7che", "Gott", "al\u00b7lein", "mit", "eif\u00b7ri\u00b7gem", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und gebe mich ihm auf. Was er mir immer tut", "tokens": ["Und", "ge\u00b7be", "mich", "ihm", "auf", ".", "Was", "er", "mir", "im\u00b7mer", "tut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "PWS", "PPER", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In Zeit und Ewigkeit, das sprech ich alles gut.", "tokens": ["In", "Zeit", "und", "E\u00b7wig\u00b7keit", ",", "das", "sprech", "ich", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PDS", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ach! da\u00df er mich doch nur vor mehrerm Fall beh\u00fcte! "