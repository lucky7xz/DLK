{"textgrid.poem.25457": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier bringet ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier bringet ", "tokens": ["Hier", "brin\u00b7get"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Den Rappen; f\u00fcttre du ihn todt!", "tokens": ["Den", "Rap\u00b7pen", ";", "f\u00fctt\u00b7re", "du", "ihn", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwar gingen mir die Augen \u00fcber,", "tokens": ["Zwar", "gin\u00b7gen", "mir", "die", "Au\u00b7gen", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als er das letzte St\u00fcckchen Brod", "tokens": ["Als", "er", "das", "letz\u00b7te", "St\u00fcck\u00b7chen", "Brod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir heute Morgen aus den H\u00e4nden", "tokens": ["Mir", "heu\u00b7te", "Mor\u00b7gen", "aus", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Stalle fra\u00df; doch, da er mir", "tokens": ["Im", "Stal\u00b7le", "fra\u00df", ";", "doch", ",", "da", "er", "mir"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$.", "ADV", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nichts nutz mehr ist, mag er bei dir", "tokens": ["Nichts", "nutz", "mehr", "ist", ",", "mag", "er", "bei", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADV", "VAFIN", "$,", "VMFIN", "PPER", "APPR", "PPER"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Sein Leben nach Gefallen enden.", "tokens": ["Sein", "Le\u00b7ben", "nach", "Ge\u00b7fal\u00b7len", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Soll ich das Ro\u00df, das gegen ", "tokens": ["Soll", "ich", "das", "Ro\u00df", ",", "das", "ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die Preu\u00dfen sonst ins Treffen f\u00fchrte", "tokens": ["Die", "Preu\u00b7\u00dfen", "sonst", "ins", "Tref\u00b7fen", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Verdammen, nun den Pflug zu ziehn?", "tokens": ["Ver\u00b7dam\u00b7men", ",", "nun", "den", "Pflug", "zu", "ziehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das Ro\u00df, das kaum den Sand ber\u00fchrte,", "tokens": ["Das", "Ro\u00df", ",", "das", "kaum", "den", "Sand", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Durch Treibeis wie ein Wallfisch schwamm,", "tokens": ["Durch", "Trei\u00b7beis", "wie", "ein", "Wall\u00b7fisch", "schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn mich's zu meinem M\u00e4dchen f\u00fchrte,", "tokens": ["Wenn", "mich's", "zu", "mei\u00b7nem", "M\u00e4d\u00b7chen", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und dennoch, fromm als wie ein Lamm,", "tokens": ["Und", "den\u00b7noch", ",", "fromm", "als", "wie", "ein", "Lamm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "KOKOM", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bei ihrem Streicheln sich nicht r\u00fchrte;", "tokens": ["Bei", "ih\u00b7rem", "Strei\u00b7cheln", "sich", "nicht", "r\u00fchr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die\u00df alte, brave, treue Pferd,", "tokens": ["Die\u00df", "al\u00b7te", ",", "bra\u00b7ve", ",", "treu\u00b7e", "Pferd", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sollt' in der Karre k\u00fcnftig gehen?", "tokens": ["Sollt'", "in", "der", "Kar\u00b7re", "k\u00fcnf\u00b7tig", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Nein, Freund, eh' sollt' an meinem Herd'", "tokens": ["Nein", ",", "Freund", ",", "eh'", "sollt'", "an", "mei\u00b7nem", "Herd'"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "KOUS", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Kein Topf am Feuer wieder stehen,", "tokens": ["Kein", "Topf", "am", "Feu\u00b7er", "wie\u00b7der", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Bis ich das Thier, nach seinem Werth',", "tokens": ["Bis", "ich", "das", "Thier", ",", "nach", "sei\u00b7nem", "Wert\u00b7h'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Auf Lebenszeit versorgt gesehen.", "tokens": ["Auf", "Le\u00b7bens\u00b7zeit", "ver\u00b7sorgt", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Zum Gl\u00fcck' f\u00fcr mich und f\u00fcr den Blessen,", "tokens": ["Zum", "Gl\u00fcck", "f\u00fcr", "mich", "und", "f\u00fcr", "den", "Bles\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Brauchst du ihn n\u00f6thiger, als ich.", "tokens": ["Brauchst", "du", "ihn", "n\u00f6\u00b7thi\u00b7ger", ",", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Wir m\u00f6gen beide nach dem Essen", "tokens": ["Wir", "m\u00f6\u00b7gen", "bei\u00b7de", "nach", "dem", "Es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Gern m\u00fc\u00dfig seyn, allein, indessen", "tokens": ["Gern", "m\u00fc\u00b7\u00dfig", "seyn", ",", "al\u00b7lein", ",", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VAINF", "$,", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Dein Freund verdaut, erwartet dich", "tokens": ["Dein", "Freund", "ver\u00b7daut", ",", "er\u00b7war\u00b7tet", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Dein Filial, ja lie\u00dfe sich", "tokens": ["Dein", "Fi\u00b7li\u00b7al", ",", "ja", "lie\u00b7\u00dfe", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die Mitternacht von deinem Kleide", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "von", "dei\u00b7nem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Nicht unterscheiden, l\u00e4ge schier", "tokens": ["Nicht", "un\u00b7ter\u00b7schei\u00b7den", ",", "l\u00e4\u00b7ge", "schier"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVINF", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Der Schnee zwei Schuh' hoch auf der Heide,", "tokens": ["Der", "Schnee", "zwei", "Schuh'", "hoch", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Du gingest doch zu Fu\u00df mit Freude,", "tokens": ["Du", "gin\u00b7gest", "doch", "zu", "Fu\u00df", "mit", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Verlangt' ein Sterbender nach dir.", "tokens": ["Ver\u00b7langt'", "ein", "Ster\u00b7ben\u00b7der", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ich aber habe nichts zu gehen,", "tokens": ["Ich", "a\u00b7ber", "ha\u00b7be", "nichts", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Als etwa, mich vom Finkenherd'", "tokens": ["Als", "et\u00b7wa", ",", "mich", "vom", "Fin\u00b7ken\u00b7herd'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Bei heiterm Wetter umzusehen:", "tokens": ["Bei", "hei\u00b7term", "Wet\u00b7ter", "um\u00b7zu\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Und wozu soll mir nun das Pferd?", "tokens": ["Und", "wo\u00b7zu", "soll", "mir", "nun", "das", "Pferd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Dich zu besuchen? Darum sey's!", "tokens": ["Dich", "zu", "be\u00b7su\u00b7chen", "?", "Da\u00b7rum", "sey's", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "PAV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "So oft ich k\u00fcnftig vom Kalm\u00e4usern", "tokens": ["So", "oft", "ich", "k\u00fcnf\u00b7tig", "vom", "Kal\u00b7m\u00e4u\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Pausire, schick' du mir den Greis,", "tokens": ["Pau\u00b7si\u00b7re", ",", "schick'", "du", "mir", "den", "Greis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Der Weg und Steg im Harz itzt wei\u00df,", "tokens": ["Der", "Weg", "und", "Steg", "im", "Harz", "itzt", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Wie vormals auf den ", "tokens": ["Wie", "vor\u00b7mals", "au