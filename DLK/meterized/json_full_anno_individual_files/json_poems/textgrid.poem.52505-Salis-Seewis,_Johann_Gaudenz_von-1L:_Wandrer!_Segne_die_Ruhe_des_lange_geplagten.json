{"textgrid.poem.52505": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wandrer! Segne die Ruhe des lange geplagten", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandrer! Segne die Ruhe des lange geplagten", "tokens": ["Wand\u00b7rer", "!", "Seg\u00b7ne", "die", "Ru\u00b7he", "des", "lan\u00b7ge", "ge\u00b7plag\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die er, vom Vaterland fern, Fl\u00fcchtling und Pilger hier fand.", "tokens": ["Die", "er", ",", "vom", "Va\u00b7ter\u00b7land", "fern", ",", "Fl\u00fccht\u00b7ling", "und", "Pil\u00b7ger", "hier", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPRART", "NN", "PTKVZ", "$,", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Umgetrieben in St\u00fcrmen, von Klippe zu Klippe geworfen,", "tokens": ["Um\u00b7ge\u00b7trie\u00b7ben", "in", "St\u00fcr\u00b7men", ",", "von", "Klip\u00b7pe", "zu", "Klip\u00b7pe", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Litt er unendliche Pein von der ", "tokens": ["Litt", "er", "un\u00b7end\u00b7li\u00b7che", "Pein", "von", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "APPR", "ART"], "meter": "+--+--+--", "measure": "