word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df noch so lau mein Herz Dir ist;", "tokens": ["Da\u00df", "noch", "so", "lau", "mein", "Herz", "Dir", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb Liebe, Liebe mir!", "tokens": ["Gieb", "Lie\u00b7be", ",", "Lie\u00b7be", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Gieb Liebe, Liebe bis zum Tod,", "tokens": ["Gieb", "Lie\u00b7be", ",", "Lie\u00b7be", "bis", "zum", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb Eifer mir und Muth!", "tokens": ["Gieb", "Ei\u00b7fer", "mir", "und", "Muth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Dir geh' ich durch Schmerz und Noth,", "tokens": ["Mit", "Dir", "geh'", "ich", "durch", "Schmerz", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Dir durch Flamm' und Fluth.", "tokens": ["Mit", "Dir", "durch", "Flamm'", "und", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Nun Zagen fort! Nun Zweifel fort:", "tokens": ["Nun", "Za\u00b7gen", "fort", "!", "Nun", "Zwei\u00b7fel", "fort", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKVZ", "$.", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir hilft der Herr im Streit.", "tokens": ["Mir", "hilft", "der", "Herr", "im", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}