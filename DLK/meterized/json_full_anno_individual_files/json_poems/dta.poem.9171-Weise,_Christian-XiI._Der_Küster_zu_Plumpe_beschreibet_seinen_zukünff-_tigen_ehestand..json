{"dta.poem.9171": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n Der K\u00fcster zu Plumpe beschreibet seinen zuk\u00fcnff-  \n tigen ehestand.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nunmehr beschlossen", "tokens": ["Es", "ist", "nun\u00b7mehr", "be\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ich will unverdrossen/", "tokens": ["Und", "ich", "will", "un\u00b7ver\u00b7dros\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinaus auff Plumpe ziehn/", "tokens": ["Hin\u00b7aus", "auff", "Plum\u00b7pe", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da will ich probe singen/", "tokens": ["Da", "will", "ich", "pro\u00b7be", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und mich vor allen dingen", "tokens": ["Und", "mich", "vor", "al\u00b7len", "din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Umb ein h\u00fcbsch lied bem\u00fchn.", "tokens": ["Umb", "ein", "h\u00fcbsch", "lied", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Nach diesem will ich sorgen/", "tokens": ["Nach", "die\u00b7sem", "will", "ich", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob ich heut oder morgen/", "tokens": ["Ob", "ich", "heut", "o\u00b7der", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein sch\u00e4tzgen haben kan/", "tokens": ["Ein", "sch\u00e4tz\u00b7gen", "ha\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich will ein weib erkiesen/", "tokens": ["Ich", "will", "ein", "weib", "er\u00b7kie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn hat es doch vor diesen", "tokens": ["Denn", "hat", "es", "doch", "vor", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PDAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein vater auch gethan.", "tokens": ["Mein", "va\u00b7ter", "auch", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "So mu\u00df sie auch in allen", "tokens": ["So", "mu\u00df", "sie", "auch", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich lassen Herre seyn/", "tokens": ["Mich", "las\u00b7sen", "Her\u00b7re", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie mu\u00df zu allen schweigen", "tokens": ["Sie", "mu\u00df", "zu", "al\u00b7len", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und mir respect erzeigen/", "tokens": ["Und", "mir", "res\u00b7pect", "er\u00b7zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sonst th\u00e4t ich zehnmahl drein.", "tokens": ["Sonst", "th\u00e4t", "ich", "zehn\u00b7mahl", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Sie mu\u00df sich lassen schelten/", "tokens": ["Sie", "mu\u00df", "sich", "las\u00b7sen", "schel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mu\u00df auch di\u00df entgelten", "tokens": ["Und", "mu\u00df", "auch", "di\u00df", "ent\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was sie nicht schuldig ist:", "tokens": ["Was", "sie", "nicht", "schul\u00b7dig", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wann ich sie werde schlagen/", "tokens": ["Wann", "ich", "sie", "wer\u00b7de", "schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df sie gedultig sagen:", "tokens": ["Mu\u00df", "sie", "ge\u00b7dul\u00b7tig", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schatz/ da\u00df du b\u00f6se bist.", "tokens": ["Schatz", "/", "da\u00df", "du", "b\u00f6\u00b7se", "bist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "5. Ich mu\u00df im hause schmehlen/", "tokens": ["Ich", "mu\u00df", "im", "hau\u00b7se", "schmeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gantz allein befehlen", "tokens": ["Und", "gantz", "al\u00b7lein", "be\u00b7feh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Umb kleider/ spei\u00df und tranck:", "tokens": ["Umb", "klei\u00b7der", "/", "spei\u00df", "und", "tranck", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "$(", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den hals wolt ich ihr brechen/", "tokens": ["Den", "hals", "wolt", "ich", "ihr", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn sie nicht wolte sprechen:", "tokens": ["Wenn", "sie", "nicht", "wol\u00b7te", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun GOtt sey lob und danck.", "tokens": ["Nun", "Gott", "sey", "lob", "und", "danck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "6. Trotz/ wann ichs haben wolte/", "tokens": ["Trotz", "/", "wann", "ichs", "ha\u00b7ben", "wol\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "PWAV", "PIS", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df sie nicht sprechen solte", "tokens": ["Da\u00df", "sie", "nicht", "spre\u00b7chen", "sol\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die weisse milch sey schwartz.", "tokens": ["Die", "weis\u00b7se", "milch", "sey", "schwartz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie mu\u00df gehorsam bleiben", "tokens": ["Sie", "mu\u00df", "ge\u00b7hor\u00b7sam", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und mir zu ehren gl\u00e4uben/", "tokens": ["Und", "mir", "zu", "eh\u00b7ren", "gl\u00e4u\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dreck w\u00e4re fiedel-hartz.", "tokens": ["Dreck", "w\u00e4\u00b7re", "fie\u00b7del\u00b7hartz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "7. Sollt ich gleich alls versauffen", "tokens": ["Sollt", "ich", "gleich", "alls", "ver\u00b7sauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und in die schencke lauffen/", "tokens": ["Und", "in", "die", "schen\u00b7cke", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So geht es sie nichts an/", "tokens": ["So", "geht", "es", "sie", "nichts", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIS", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gnug/ da\u00df sie ihre sachen", "tokens": ["Gnug", "/", "da\u00df", "sie", "ih\u00b7re", "sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In ruh und friede machen", "tokens": ["In", "ruh", "und", "frie\u00b7de", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und essen kochen kan.", "tokens": ["Und", "es\u00b7sen", "ko\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "8. Ich folge meinem kopffe/", "tokens": ["Ich", "fol\u00b7ge", "mei\u00b7nem", "kopf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und werffe mit dem topffe", "tokens": ["Und", "werf\u00b7fe", "mit", "dem", "topf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach frauen/ kind und magd/", "tokens": ["Nach", "frau\u00b7en", "/", "kind", "und", "magd", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo jemand in dem hause/", "tokens": ["Wo", "je\u00b7mand", "in", "dem", "hau\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ich zu offte schmause/", "tokens": ["Wenn", "ich", "zu", "off\u00b7te", "schmau\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir was zu wider sagt.", "tokens": ["Mir", "was", "zu", "wi\u00b7der", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKZU", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "9. Wann ich will lerchen fressen/", "tokens": ["Wann", "ich", "will", "ler\u00b7chen", "fres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "VVINF", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "So mag sie unterdessen", "tokens": ["So", "mag", "sie", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jm k\u00e4se lustig seyn/", "tokens": ["Jm", "k\u00e4\u00b7se", "lus\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Geh ich zu wein und biere/", "tokens": ["Geh", "ich", "zu", "wein", "und", "bie\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Alsdann so jubilire", "tokens": ["Als\u00b7dann", "so", "ju\u00b7bi\u00b7li\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie \u00fcbern g\u00e4nse-wein.", "tokens": ["Sie", "\u00fc\u00b7bern", "g\u00e4n\u00b7se\u00b7wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "10. Spiel ich wo in der karten", "tokens": ["Spiel", "ich", "wo", "in", "der", "kar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PWAV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So mag sie immer warten", "tokens": ["So", "mag", "sie", "im\u00b7mer", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auff wucher und gewinn/", "tokens": ["Auff", "wu\u00b7cher", "und", "ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn werd ich viel verspielen", "tokens": ["Denn", "werd", "ich", "viel", "ver\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So sol sie redlich f\u00fchlen", "tokens": ["So", "sol", "sie", "red\u00b7lich", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie ich so b\u00f6se bin.", "tokens": ["Wie", "ich", "so", "b\u00f6\u00b7se", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "11. Und wann ich zw\u00f6lffmahl h\u00e4tte", "tokens": ["Und", "wann", "ich", "zw\u00f6lff\u00b7mahl", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zusammen in das bette", "tokens": ["Zu\u00b7sam\u00b7men", "in", "das", "bet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PDS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja an ihr bein gethan/", "tokens": ["Ja", "an", "ihr", "bein", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So mu\u00df sie dennoch kommen", "tokens": ["So", "