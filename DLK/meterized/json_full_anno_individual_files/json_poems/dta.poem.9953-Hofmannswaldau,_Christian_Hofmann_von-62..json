{"dta.poem.9953": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich thu nichts wen\u0303 mie nicht hilffet zu der arbeit lufft un\u0303 wind/", "tokens": ["Ich", "thu", "nichts", "we\u00f1", "mie", "nicht", "hilf\u00b7fet", "zu", "der", "ar\u00b7beit", "lufft", "u\u00f1", "wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VVFIN", "PPER", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "NN", "KON", "NE", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Fliege niemahls von der stelle/ da doch fl\u00fcgel an mir sind:", "tokens": ["Flie\u00b7ge", "nie\u00b7mahls", "von", "der", "stel\u00b7le", "/", "da", "doch", "fl\u00fc\u00b7gel", "an", "mir", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "$(", "ADV", "ADV", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch geschiehets nicht aus furcht/ sondern ich mu\u00df also bleibe\u0303/", "tokens": ["Doch", "ge\u00b7schie\u00b7hets", "nicht", "aus", "furcht", "/", "son\u00b7dern", "ich", "mu\u00df", "al\u00b7so", "bleib\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "NN", "$(", "KON", "PPER", "VMFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Bi\u00df dem menschen es gef\u00e4llt/ da\u00df er mich herum wil treiben.", "tokens": ["Bi\u00df", "dem", "men\u00b7schen", "es", "ge\u00b7f\u00e4llt", "/", "da\u00df", "er", "mich", "he\u00b7rum", "wil", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PPER", "VVPP", "$(", "KOUS", "PPER", "PRF", "APZR", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}