{"textgrid.poem.25402": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Uhren schlagen sieben.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Uhren schlagen sieben.", "tokens": ["Die", "Uh\u00b7ren", "schla\u00b7gen", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun gehen \u00fcberall in der Stadt die Gesch\u00e4fte aus.", "tokens": ["Nun", "ge\u00b7hen", "\u00fc\u00b7be\u00b7rall", "in", "der", "Stadt", "die", "Ge\u00b7sch\u00e4f\u00b7te", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aus schon umdunkelten Hausfluren,", "tokens": ["Aus", "schon", "um\u00b7dun\u00b7kel\u00b7ten", "Haus\u00b7flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "durch enge Winkelh\u00f6fe aus protzigen Hallen dr\u00e4ngen", "tokens": ["durch", "en\u00b7ge", "Win\u00b7kel\u00b7h\u00f6\u00b7fe", "aus", "prot\u00b7zi\u00b7gen", "Hal\u00b7len", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "sich die Verk\u00e4uferinnen heraus.", "tokens": ["sich", "die", "Ver\u00b7k\u00e4u\u00b7fe\u00b7rin\u00b7nen", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Noch ein wenig blind und wie bet\u00e4ubt", "tokens": ["Noch", "ein", "we\u00b7nig", "blind", "und", "wie", "be\u00b7t\u00e4ubt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PIS", "ADJD", "KON", "PWAV", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "vom langen Eingeschlossensein", "tokens": ["vom", "lan\u00b7gen", "Ein\u00b7ge\u00b7schlos\u00b7sen\u00b7sein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Treten sie, leise erregt, in die woll\u00fcstige Helle", "tokens": ["Tre\u00b7ten", "sie", ",", "lei\u00b7se", "er\u00b7regt", ",", "in", "die", "wol\u00b7l\u00fcs\u00b7ti\u00b7ge", "Hel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "und die sanfte Offenheit des Sommerabends ein.", "tokens": ["und", "die", "sanf\u00b7te", "Of\u00b7fen\u00b7heit", "des", "Som\u00b7mer\u00b7a\u00b7bends", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Griesgr\u00e4mige Stra\u00dfenz\u00fcge leuchten auf und schlagen", "tokens": ["Gries\u00b7gr\u00e4\u00b7mi\u00b7ge", "Stra\u00b7\u00dfen\u00b7z\u00fc\u00b7ge", "leuch\u00b7ten", "auf", "und", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "KON", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "mit einem Male helleren Takt,", "tokens": ["mit", "ei\u00b7nem", "Ma\u00b7le", "hel\u00b7le\u00b7ren", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Alle Trottoirs sind eng mit bunten Blusen", "tokens": ["Al\u00b7le", "Trot\u00b7to\u00b7irs", "sind", "eng", "mit", "bun\u00b7ten", "Blu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "und M\u00e4dchengel\u00e4chter vollgepackt.", "tokens": ["und", "M\u00e4d\u00b7chen\u00b7ge\u00b7l\u00e4ch\u00b7ter", "voll\u00b7ge\u00b7packt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wie ein See, durch den das starke Treiben", "tokens": ["Wie", "ein", "See", ",", "durch", "den", "das", "star\u00b7ke", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "eines jungen Flusses w\u00fchlt,", "tokens": ["ei\u00b7nes", "jun\u00b7gen", "Flus\u00b7ses", "w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Ist die ganze Stadt von Jugend", "tokens": ["Ist", "die", "gan\u00b7ze", "Stadt", "von", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "und Heimkehr \u00fcbersp\u00fclt.", "tokens": ["und", "Heim\u00b7kehr", "\u00fc\u00b7bers\u00b7p\u00fclt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Zwischen die gleichgiltigen Gesichter", "tokens": ["Zwi\u00b7schen", "die", "gleich\u00b7gil\u00b7ti\u00b7gen", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "der Vor\u00fcbergehenden", "tokens": ["der", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+-+--", "measure": "anapaest.init"}, "line.20": {"text": "ist ein vielf\u00e4ltiges Schicksal gestellt \u2013", "tokens": ["ist", "ein", "viel\u00b7f\u00e4l\u00b7ti\u00b7ges", "Schick\u00b7sal", "ge\u00b7stellt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die Erregung jungen Lebens,", "tokens": ["Die", "Er\u00b7re\u00b7gung", "jun\u00b7gen", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "vom Feuer dieser Abendstunde \u00fcberhellt,", "tokens": ["vom", "Feu\u00b7er", "die\u00b7ser", "A\u00b7bends\u00b7tun\u00b7de", "\u00fc\u00b7berh\u00b7ellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In deren S\u00fc\u00dfe alles Dunkle sich verkl\u00e4rt", "tokens": ["In", "de\u00b7ren", "S\u00fc\u00b7\u00dfe", "al\u00b7les", "Dunk\u00b7le", "sich", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PIS", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "und alles Schwere schmilzt, als w\u00e4r es leicht und frei,", "tokens": ["und", "al\u00b7les", "Schwe\u00b7re", "schmilzt", ",", "als", "w\u00e4r", "es", "leicht", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und als warte nicht schon,", "tokens": ["Und", "als", "war\u00b7te", "nicht", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.26": {"text": "durch wenig Stunden getrennt, das triste Einerlei", "tokens": ["durch", "we\u00b7nig", "Stun\u00b7den", "ge\u00b7trennt", ",", "das", "tris\u00b7te", "Ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Der t\u00e4glichen Frohn \u2013 als warte nicht Heimkehr,", "tokens": ["Der", "t\u00e4g\u00b7li\u00b7chen", "Frohn", "\u2013", "als", "war\u00b7te", "nicht", "Heim\u00b7kehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOKOM", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Gewinkel schmutziger Vorstadth\u00e4user,", "tokens": ["Ge\u00b7win\u00b7kel", "schmut\u00b7zi\u00b7ger", "Vor\u00b7stadt\u00b7h\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "zwischen nackte Mietskasernen gekeilt,", "tokens": ["zwi\u00b7schen", "nack\u00b7te", "Miets\u00b7ka\u00b7ser\u00b7nen", "ge\u00b7keilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "Karges Mahl, Beklommenheit der Familienstube", "tokens": ["Kar\u00b7ges", "Mahl", ",", "Be\u00b7klom\u00b7men\u00b7heit", "der", "Fa\u00b7mi\u00b7li\u00b7en\u00b7stu\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.31": {"text": "und die enge Nachtkammer,", "tokens": ["und", "die", "en\u00b7ge", "Nacht\u00b7kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "mit den kleinen Geschwistern geteilt,", "tokens": ["mit", "den", "klei\u00b7nen", "Ge\u00b7schwis\u00b7tern", "ge\u00b7teilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.33": {"text": "Und kurzer Schlaf, den schon die erste Fr\u00fche", "tokens": ["Und", "kur\u00b7zer", "Schlaf", ",", "den", "schon", "die", "ers\u00b7te", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "aus dem Goldland der Tr\u00e4ume hetzt \u2013", "tokens": ["aus", "dem", "Gold\u00b7land", "der", "Tr\u00e4u\u00b7me", "hetzt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.35": {"text": "All das ist jetzt ganz weit \u2013 von Abend zugedeckt \u2013", "tokens": ["All", "das", "ist", "jetzt", "ganz", "weit", "\u2013", "von", "A\u00b7bend", "zu\u00b7ge\u00b7deckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "VAFIN", "ADV", "ADV", "ADJD", "$(", "APPR", "NN", "VVPP", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.36": {"text": "und doch schon da, und wartend wie ein b\u00f6ses Tier,", "tokens": ["und", "doch", "schon", "da", ",", "und", "war\u00b7tend", "wie", "ein", "b\u00f6\u00b7ses", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "KON", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "das sich zur Beute niedersetzt,", "tokens": ["das", "sich", "zur", "Beu\u00b7te", "nie\u00b7der\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und selbst die Gl\u00fccklichsten,", "tokens": ["Und", "selbst", "die", "Gl\u00fcck\u00b7lichs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.39": {"text": "die leicht mit schlankem Schritt", "tokens": ["die", "leicht", "mit", "schlan\u00b7kem", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Am Arm des Liebsten t\u00e4nzeln,", "tokens": ["Am", "Arm", "des", "Liebs\u00b7ten", "t\u00e4n\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "tragen in der Einsamkeit der Augen", "tokens": ["tra\u00b7gen", "in", "der", "Ein\u00b7sam\u00b7keit", "der", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "einen fernen Schatten mit.", "tokens": ["ei\u00b7nen", "fer\u00b7nen", "Schat\u00b7ten", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Und manchmal, wenn von ungef\u00e4hr der Blick", "tokens": ["Und", "manch\u00b7mal", ",", "wenn", "von", "un\u00b7ge\u00b7f\u00e4hr", "der", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "APPR", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "der M\u00e4dchen im Gespr\u00e4ch zu Boden f\u00e4llt,", "tokens": ["der", "M\u00e4d\u00b7chen", "im", "Ge\u00b7spr\u00e4ch", "zu", "Bo\u00b7den", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Geschieht es, da\u00df ein Schreckgesicht mit h\u00f6hnischer", "tokens": ["Ge\u00b7schieht", "es", ",", "da\u00df", "ein", "Schreck\u00b7ge\u00b7sicht", "mit", "h\u00f6h\u00b7ni\u00b7scher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Grimasse ihrer Fr\u00f6hlichkeit den Weg verstellt.", "tokens": ["Gri\u00b7mas\u00b7se", "ih\u00b7rer", "Fr\u00f6h\u00b7lich\u00b7keit", "den", "Weg", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Dann schmiegen sie sich enger, und die Hand erzittert,", "tokens": ["Dann", "schmie\u00b7gen", "sie", "sich", "en\u00b7ger", ",", "und", "die", "Hand", "er\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "die den Arm des Freundes greift,", "tokens": ["die", "den", "Arm", "des", "Freun\u00b7des", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Als st\u00e4nde schon das Alter hinter ihnen,", "tokens": ["Als", "st\u00e4n\u00b7de", "schon", "das", "Al\u00b7ter", "hin\u00b7ter", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "das ihr Leben dem Verl\u00f6schen in der Dunkelheit", "tokens": ["das", "ihr", "Le\u00b7ben", "dem", "Ver\u00b7l\u00f6\u00b7schen", "in", "der", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.51": {"text": "entgegenschleift.", "tokens": ["ent\u00b7ge\u00b7gen\u00b7schleift", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stan