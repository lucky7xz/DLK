{"dta.poem.2362": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "War\u00fcm soll Ich mein Hertz mit Gr\u00e4men t\u00e4glich fressen/", "tokens": ["Wa\u00b7r\u00fcm", "soll", "Ich", "mein", "Hertz", "mit", "Gr\u00e4\u00b7men", "t\u00e4g\u00b7lich", "fres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df Ich Menschlich sey/ so liederlich vergessen/", "tokens": ["Und", "da\u00df", "Ich", "Menschlich", "sey", "/", "so", "lie\u00b7der\u00b7lich", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VAFIN", "$(", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ob schon die Feuersbrunst de\u00df Meingen mich beraube", "tokens": ["Ob", "schon", "die", "Feu\u00b7ers\u00b7brunst", "de\u00df", "Mein\u00b7gen", "mich", "be\u00b7rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was Gottes Gunst und Gl\u00fckk Mir reichlich hatt\u2019", "tokens": ["Was", "Got\u00b7tes", "Gunst", "und", "Gl\u00fckk", "Mir", "reich\u00b7lich", "hatt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "NN", "KON", "NN", "NE", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "erlaubt.", "tokens": ["er\u00b7laubt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Es war ja nichts als Gut/ welchs nie best\u00e4ndig bleibet/", "tokens": ["Es", "war", "ja", "nichts", "als", "Gut", "/", "welchs", "nie", "be\u00b7st\u00e4n\u00b7dig", "blei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "KOKOM", "ADJD", "$(", "PWS", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welchs oftmals unversehns wie Heu und Spreu zersteubet/", "tokens": ["Welchs", "oft\u00b7mals", "un\u00b7ver\u00b7sehns", "wie", "Heu", "und", "Spreu", "zer\u00b7steu\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott theilte Mir es mit aus seiner Gnadenhand/", "tokens": ["Gott", "theil\u00b7te", "Mir", "es", "mit", "aus", "sei\u00b7ner", "Gna\u00b7den\u00b7hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der hat es wieder\u00fcm auch von Mir abgewande.", "tokens": ["Der", "hat", "es", "wie\u00b7de\u00b7r\u00fcm", "auch", "von", "Mir", "ab\u00b7ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "ADV", "APPR", "NE", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich bin ein Mensch/ war\u00fcm soll Ich dem Gl\u00fckke fluchen/", "tokens": ["Ich", "bin", "ein", "Mensch", "/", "wa\u00b7r\u00fcm", "soll", "Ich", "dem", "Gl\u00fck\u00b7ke", "flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist doch mein Gott getreu/ der Mich nicht lest versuchen/", "tokens": ["Ist", "doch", "mein", "Gott", "ge\u00b7treu", "/", "der", "Mich", "nicht", "lest", "ver\u00b7su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$(", "PRELS", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Ich was tragen soll in dieser S\u00fcndenwelt", "tokens": ["Da\u00df", "Ich", "was", "tra\u00b7gen", "soll", "in", "die\u00b7ser", "S\u00fcn\u00b7den\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was \u00fcber meine Macht/ und Mit unm\u00f6glich f\u00e4llt.", "tokens": ["Was", "\u00fc\u00b7ber", "mei\u00b7ne", "Macht", "/", "und", "Mit", "un\u00b7m\u00f6g\u00b7lich", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "$(", "KON", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gott legt uns eine Last zwar auf den matten R\u00fckken/", "tokens": ["Gott", "legt", "uns", "ei\u00b7ne", "Last", "zwar", "auf", "den", "mat\u00b7ten", "R\u00fck\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df wir gantz Athemlo\u00df darunter fast erstikken/", "tokens": ["Da\u00df", "wir", "gantz", "A\u00b7them\u00b7lo\u00df", "da\u00b7run\u00b7ter", "fast", "ers\u00b7tik\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NE", "PAV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch tr\u00e4gt Er selber mit/ und schenkt uns seine Huld/", "tokens": ["Doch", "tr\u00e4gt", "Er", "sel\u00b7ber", "mit", "/", "und", "schenkt", "uns", "sei\u00b7ne", "Huld", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "$(", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn Er nur an uns merkt die hertzliche Geduld.", "tokens": ["Wenn", "Er", "nur", "an", "uns", "merkt", "die", "hertz\u00b7li\u00b7che", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er wil mich die\u00dfmal auch ins n\u00fctzlich\u2019 Ungt\u00fck f\u00fchren/", "tokens": ["Er", "wil", "mich", "die\u00df\u00b7mal", "auch", "ins", "n\u00fctz\u00b7lich'", "Ung\u00b7t\u00fck", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Er mein kindlichs Hertz m\u00f6g als ein Vater sp\u00fchren/", "tokens": ["Da\u00df", "Er", "mein", "kind\u00b7lichs", "Hertz", "m\u00f6g", "als", "ein", "Va\u00b7ter", "sp\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VMFIN", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie es geberden wil in solchem harten Stand\u2019/", "tokens": ["Wie", "es", "ge\u00b7ber\u00b7den", "wil", "in", "sol\u00b7chem", "har\u00b7ten", "Stand'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Obs auch zu frieden sey mit seiner Z\u00fcchtgungshand.", "tokens": ["Obs", "auch", "zu", "frie\u00b7den", "sey", "mit", "sei\u00b7ner", "Z\u00fccht\u00b7gungs\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dar\u00fcm ist auch mein Glaub\u2019 als Mauerfest gegr\u00fcndet/", "tokens": ["Da\u00b7r\u00fcm", "ist", "auch", "mein", "Glaub'", "als", "Mau\u00b7er\u00b7fest", "ge\u00b7gr\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPOSAT", "NN", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df/ wie nach grossem Sturm\u2019 ein Son\u0303enschein sich findet/", "tokens": ["Da\u00df", "/", "wie", "nach", "gros\u00b7sem", "Sturm'", "ein", "So\u00f1en\u00b7schein", "sich", "fin\u00b7det", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOKOM", "APPR", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mir wieder scheinen wird auf dieses Traurig-seyn/", "tokens": ["Mir", "wie\u00b7der", "schei\u00b7nen", "wird", "auf", "die\u00b7ses", "Trau\u00b7rig\u00b7seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VAFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Gl\u00fckk das zwiefach ist/ und Gottes Gnadenschein.", "tokens": ["Ein", "Gl\u00fckk", "das", "zwie\u00b7fach", "ist", "/", "und", "Got\u00b7tes", "Gna\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "VAFIN", "$(", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}