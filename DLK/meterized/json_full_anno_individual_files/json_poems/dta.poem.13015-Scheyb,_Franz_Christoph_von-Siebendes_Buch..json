{"dta.poem.13015": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indessen war der Saal stets mehr erregt, gest\u00f6rt,", "tokens": ["In\u00b7des\u00b7sen", "war", "der", "Saal", "stets", "mehr", "er\u00b7regt", ",", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mithin ihr Reden nur verwirret angeh\u00f6rt.", "tokens": ["Mi\u00b7thin", "ihr", "Re\u00b7den", "nur", "ver\u00b7wir\u00b7ret", "an\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wollten andere so wohl, als diese, sprechen,", "tokens": ["Es", "woll\u00b7ten", "an\u00b7de\u00b7re", "so", "wohl", ",", "als", "die\u00b7se", ",", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "ADV", "$,", "KOUS", "PDS", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "60So mu\u00dften folglich sich die Reden unterbrechen.", "tokens": ["mu\u00df\u00b7ten", "folg\u00b7lich", "sich", "die", "Re\u00b7den", "un\u00b7ter\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Kaum h\u00f6rte man ein Wort, so wiedersprach man es.", "tokens": ["Kaum", "h\u00f6r\u00b7te", "man", "ein", "Wort", ",", "so", "wie\u00b7der\u00b7sprach", "man", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "ADV", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den ganzen Saal durchschlich ein streitendes Get\u00f6\u00df.", "tokens": ["Den", "gan\u00b7zen", "Saal", "durch\u00b7schlich", "ein", "strei\u00b7ten\u00b7des", "Ge\u00b7t\u00f6\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}