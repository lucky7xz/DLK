{"textgrid.poem.49386": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "An lichtgewobener Kette", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An lichtgewobener Kette mu\u00df ich h\u00e4ngen", "tokens": ["An", "licht\u00b7ge\u00b7wo\u00b7be\u00b7ner", "Ket\u00b7te", "mu\u00df", "ich", "h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus hohen Himmeln in das tr\u00fcbe Leben,", "tokens": ["Aus", "ho\u00b7hen", "Him\u00b7meln", "in", "das", "tr\u00fc\u00b7be", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gen\u00f6tigt hin und her zu schweben,", "tokens": ["Ge\u00b7n\u00f6\u00b7tigt", "hin", "und", "her", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKVZ", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sanfte \u00c4therwellen mich bedr\u00e4ngen.", "tokens": ["Weil", "sanf\u00b7te", "\u00c4t\u00b7her\u00b7wel\u00b7len", "mich", "be\u00b7dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man haucht mich an mit Worten und mit Kl\u00e4ngen,", "tokens": ["Man", "haucht", "mich", "an", "mit", "Wor\u00b7ten", "und", "mit", "Kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word