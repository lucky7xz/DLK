{"textgrid.poem.43347": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XiV. An den Mond.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O lieblichklarer Mond, ich denke dran,", "tokens": ["O", "lieb\u00b7lich\u00b7kla\u00b7rer", "Mond", ",", "ich", "den\u00b7ke", "dran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie ich, nun wird's ein Jahr, von diesem H\u00fcgel,", "tokens": ["Wie", "ich", ",", "nun", "wird's", "ein", "Jahr", ",", "von", "die\u00b7sem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "VAFIN", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Herz voll Schwermuth, zu dir aufgeblickt.", "tokens": ["Das", "Herz", "voll", "Schwer\u00b7muth", ",", "zu", "dir", "auf\u00b7ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du schwebtest damals \u00fcber jenem Walde", "tokens": ["Du", "schweb\u00b7test", "da\u00b7mals", "\u00fc\u00b7ber", "je\u00b7nem", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ganz so wie heut, wo du ihn voll verkl\u00e4rst;", "tokens": ["Ganz", "so", "wie", "heut", ",", "wo", "du", "ihn", "voll", "ver\u00b7kl\u00e4rst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ADV", "$,", "PWAV", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch nebelhaft und zitternd, da von Thr\u00e4nen", "tokens": ["Doch", "ne\u00b7bel\u00b7haft", "und", "zit\u00b7ternd", ",", "da", "von", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "VVPP", "$,", "KOUS", "APPR", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Die Wimper \u00fcberquoll, erschien dein Bild", "tokens": ["Die", "Wim\u00b7per", "\u00fc\u00b7berq\u00b7uoll", ",", "er\u00b7schien", "dein", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Damals vor meinem Blick; denn leidvoll war", "tokens": ["Da\u00b7mals", "vor", "mei\u00b7nem", "Blick", ";", "denn", "leid\u00b7voll", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mein Leben, wie noch heut und alle Zeit,", "tokens": ["Mein", "Le\u00b7ben", ",", "wie", "noch", "heut", "und", "al\u00b7le", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADV", "ADV", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O mein geliebter Mond. Und doch erfreut mich", "tokens": ["O", "mein", "ge\u00b7lieb\u00b7ter", "Mond", ".", "Und", "doch", "er\u00b7freut", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$.", "KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Erinnrung; denn ich z\u00e4hle gern, wie alt", "tokens": ["E\u00b7rinn\u00b7rung", ";", "denn", "ich", "z\u00e4h\u00b7le", "gern", ",", "wie", "alt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "KON", "PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mein Kummer wird. O wie so reizend ist's,", "tokens": ["Mein", "Kum\u00b7mer", "wird", ".", "O", "wie", "so", "rei\u00b7zend", "ist's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "NE", "KOKOM", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In jungen Jahren, wo die Bahn der Hoffnung", "tokens": ["In", "jun\u00b7gen", "Jah\u00b7ren", ",", "wo", "die", "Bahn", "der", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Noch lang und kurz nur des Erinnerns Pfad,", "tokens": ["Noch", "lang", "und", "kurz", "nur", "des", "E\u00b7rin\u00b7nerns", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Zur\u00fcckzudenken an vergangne Dinge,", "tokens": ["Zu\u00b7r\u00fcck\u00b7zu\u00b7den\u00b7ken", "an", "ver\u00b7gang\u00b7ne", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic