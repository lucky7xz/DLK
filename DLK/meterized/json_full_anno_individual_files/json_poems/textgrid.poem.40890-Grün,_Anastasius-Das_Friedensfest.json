{"textgrid.poem.40890": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Das Friedensfest", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu K\u00f6ln, da bot der Pfalzgraf Albrechten friedlich die Hand,", "tokens": ["Zu", "K\u00f6ln", ",", "da", "bot", "der", "Pfalz\u00b7graf", "Al\u00b7brech\u00b7ten", "fried\u00b7lich", "die", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und K\u00f6nig Max als Mittler vereint das Friedensband;", "tokens": ["Und", "K\u00f6\u00b7nig", "Max", "als", "Mitt\u00b7ler", "ver\u00b7eint", "das", "Frie\u00b7dens\u00b7band", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "KOUS", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gen\u00fcgen will's nun Jedem, was fr\u00fcher ihm zu schlecht,", "tokens": ["Ge\u00b7n\u00fc\u00b7gen", "will's", "nun", "Je\u00b7dem", ",", "was", "fr\u00fc\u00b7her", "ihm", "zu", "schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PIAT", "$,", "PRELS", "ADJD", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Burghausen nimmt der Pfalzgraf, den F\u00fcrstenhut Albrecht.", "tokens": ["Burg\u00b7hau\u00b7sen", "nimmt", "der", "Pfalz\u00b7graf", ",", "den", "F\u00fcrs\u00b7ten\u00b7hut", "Al\u00b7brecht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Des Abends gab der K\u00f6nig ein Lustbankett den Herrn,", "tokens": ["Des", "A\u00b7bends", "gab", "der", "K\u00f6\u00b7nig", "ein", "Lust\u00b7ban\u00b7kett", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn er verm\u00e4hlt dem Ernste die heitre Freude gern,", "tokens": ["Denn", "er", "ver\u00b7m\u00e4hlt", "dem", "Erns\u00b7te", "die", "heit\u00b7re", "Freu\u00b7de", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gleichwie man Trauermale mit Rosen gern umheckt", "tokens": ["Gleich\u00b7wie", "man", "Trau\u00b7er\u00b7ma\u00b7le", "mit", "Ro\u00b7sen", "gern", "um\u00b7heckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "NN", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und auf den ernsten Altar viel lust'ge Ampeln steckt.", "tokens": ["Und", "auf", "den", "erns\u00b7ten", "Al\u00b7tar", "viel", "lust'\u00b7ge", "Am\u00b7peln", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Da gab es Tanz und Lieder und schalkisch Mummenspiel", "tokens": ["Da", "gab", "es", "Tanz", "und", "Lie\u00b7der", "und", "schal\u00b7kisch", "Mum\u00b7men\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Possen sonder Ende und Jubeln sonder Ziel.", "tokens": ["Und", "Pos\u00b7sen", "son\u00b7der", "En\u00b7de", "und", "Ju\u00b7beln", "son\u00b7der", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die zwei vers\u00f6hnten F\u00fcrsten, verschlungen Arm in Arm,", "tokens": ["Die", "zwei", "ver\u00b7s\u00f6hn\u00b7ten", "F\u00fcrs\u00b7ten", ",", "ver\u00b7schlun\u00b7gen", "Arm", "in", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "$,", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Durchwallten, fr\u00f6hlich scherzend, den buntbewegten Schwarm.", "tokens": ["Durch\u00b7wall\u00b7ten", ",", "fr\u00f6h\u00b7lich", "scher\u00b7zend", ",", "den", "bunt\u00b7be\u00b7weg\u00b7ten", "Schwarm", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Hervor nun zu den Beiden trat aus dem Mummenzug", "tokens": ["Her\u00b7vor", "nun", "zu", "den", "Bei\u00b7den", "trat", "aus", "dem", "Mum\u00b7men\u00b7zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein flinker Ganymedes, der zwei Pokale trug;", "tokens": ["Ein", "flin\u00b7ker", "Ga\u00b7ny\u00b7me\u00b7des", ",", "der", "zwei", "Po\u00b7ka\u00b7le", "trug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "CARD", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es war die eine Schale von Golde, rein und klar,", "tokens": ["Es", "war", "die", "ei\u00b7ne", "Scha\u00b7le", "von", "Gol\u00b7de", ",", "rein", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ART", "NN", "APPR", "NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein hohler Todtensch\u00e4del jedoch die andre war:", "tokens": ["Ein", "hoh\u00b7ler", "Tod\u00b7ten\u00b7sch\u00e4\u00b7del", "je\u00b7doch", "die", "and\u00b7re", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbihr Herrn, mag euch ein Becher vielleicht nach Wunsche sein?", "tokens": ["\u00bb", "ihr", "Herrn", ",", "mag", "euch", "ein", "Be\u00b7cher", "viel\u00b7leicht", "nach", "Wun\u00b7sche", "sein", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur das Geh\u00e4us ist ungleich, doch euer ist die Wahl!\u00ab", "tokens": ["Nur", "das", "Ge\u00b7h\u00e4us", "ist", "un\u00b7gleich", ",", "doch", "eu\u00b7er", "ist", "die", "Wahl", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "PPOSAT", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Da fa\u00dften beide F\u00fcrsten zugleich den Goldpokal.", "tokens": ["Da", "fa\u00df\u00b7ten", "bei\u00b7de", "F\u00fcrs\u00b7ten", "zu\u00b7gleich", "den", "Gold\u00b7po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbei, h\u00e4tt' ich fast gewettet, ihr w\u00e4hlt den Sch\u00e4del euch!", "tokens": ["\u00bb", "ei", ",", "h\u00e4tt'", "ich", "fast", "ge\u00b7wet\u00b7tet", ",", "ihr", "w\u00e4hlt", "den", "Sch\u00e4\u00b7del", "euch", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sonst ist's doch eure Art so!\u00ab Er sprachs und verschwand sogleich.", "tokens": ["Sonst", "ist's", "doch", "eu\u00b7re", "Art", "so", "!", "\u00ab", "Er", "sprachs", "und", "ver\u00b7schwand", "sog\u00b7leich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "$.", "$(", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wollt ihr's nicht verrathen, sei's im Vertraun gesagt:", "tokens": ["Und", "wollt", "ih\u00b7r's", "nicht", "ver\u00b7ra\u00b7then", ",", "sei's", "im", "Ver\u00b7traun", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kunz war es, der vor F\u00fcrsten solch k\u00fchnes Wort gewagt.", "tokens": ["Kunz", "war", "es", ",", "der", "vor", "F\u00fcrs\u00b7ten", "solch", "k\u00fch\u00b7nes", "Wort", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PRELS", "APPR", "NN", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Darauf im Heroldsschmucke zu ihnen trat ein Mann,", "tokens": ["Da\u00b7rauf", "im", "He\u00b7rolds\u00b7schmu\u00b7cke", "zu", "ih\u00b7nen", "trat", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der K\u00f6nig war es selber, wohl sah man's bald ihm an,", "tokens": ["Der", "K\u00f6\u00b7nig", "war", "es", "sel\u00b7ber", ",", "wohl", "sah", "man's", "bald", "ihm", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PIS", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit einer farb'gen Sch\u00e4rpe schm\u00fcckt er die F\u00fcrsten beide,", "tokens": ["Mit", "ei\u00b7ner", "fa\u00b7rb'\u00b7gen", "Sch\u00e4r\u00b7pe", "schm\u00fcckt", "er", "die", "F\u00fcrs\u00b7ten", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PIS", "$,"], "meter": "-+----+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Drauf stand ein Doppeladler und solcher Spruch in Seide:", "tokens": ["Drauf", "stand", "ein", "Dop\u00b7pe\u00b7lad\u00b7ler", "und", "sol\u00b7cher", "Spruch", "in", "Sei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "KON", "PIAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbnicht ist mit zweien H\u00e4uptern begabt der deutsche Aar,", "tokens": ["\u00bb", "nicht", "ist", "mit", "zwei\u00b7en", "H\u00e4up\u00b7tern", "be\u00b7gabt", "der", "deut\u00b7sche", "Aar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VAFIN", "APPR", "CARD", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf da\u00df ein Haupt das andre zerfleisch' und morde gar!", "tokens": ["Auf", "da\u00df", "ein", "Haupt", "das", "and\u00b7re", "zer\u00b7fleisch'", "und", "mor\u00b7de", "gar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ART", "PIS", "VVFIN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df er schon fern erschaue die nahende Gefahr,", "tokens": ["Da\u00df", "er", "schon", "fern", "er\u00b7schau\u00b7e", "die", "na\u00b7hen\u00b7de", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+---+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dazu hat Deutschlands Adler sein Doppelaugenpaar!\u00ab", "tokens": ["Da\u00b7zu", "hat", "Deutschlands", "Ad\u00b7ler", "sein", "Dop\u00b7pe\u00b7lau\u00b7gen\u00b7paar", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VAFIN", "NE", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Zu K\u00f6ln, da bot der Pfalzgraf Albrechten friedlich die Hand,", "tokens": ["Zu", "K\u00f6ln", ",", "da", "bot", "der", "Pfalz\u00b7graf", "Al\u00b7brech\u00b7ten", "fried\u00b7lich", "die", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und K\u00f6nig Max als Mittler vereint das Friedensband;", "tokens": ["Und", "K\u00f6\u00b7nig", "Max", "als", "Mitt\u00b7ler", "ver\u00b7eint", "das", "Frie\u00b7dens\u00b7band", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "KOUS", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gen\u00fcgen will's nun Jedem, was fr\u00fcher ihm zu schlecht,", "tokens": ["Ge\u00b7n\u00fc\u00b7gen", "will's", "nun", "Je\u00b7dem", ",", "was", "fr\u00fc\u00b7her", "ihm", "zu", "schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PIAT", "$,", "PRELS", "ADJD", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Burghausen nimmt der Pfalzgraf, den F\u00fcrstenhut Albrecht.", "