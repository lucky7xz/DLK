{"textgrid.poem.60986": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Rothenburg", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter kommt mit leichtem Mut gezogen", "tokens": ["Der", "Dich\u00b7ter", "kommt", "mit", "leich\u00b7tem", "Mut", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch gr\u00fcne Triften und durch Korneswogen;", "tokens": ["Durch", "gr\u00fc\u00b7ne", "Trif\u00b7ten", "und", "durch", "Kor\u00b7nes\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da steigt vor ihm auf wald'gem Bergeskranze", "tokens": ["Da", "steigt", "vor", "ihm", "auf", "wald'\u00b7gem", "Ber\u00b7ges\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schlo\u00df empor im Abendsonnenglanze.", "tokens": ["Ein", "Schlo\u00df", "em\u00b7por", "im", "A\u00b7bend\u00b7son\u00b7nen\u00b7glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald ist der steile Gipfel k\u00fchn erklommen;", "tokens": ["Bald", "ist", "der", "stei\u00b7le", "Gip\u00b7fel", "k\u00fchn", "er\u00b7klom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald hat den Gast der Burghof aufgenommen;", "tokens": ["Bald", "hat", "den", "Gast", "der", "Burg\u00b7hof", "auf\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort stehn als W\u00e4chter, eingelullt in Tr\u00e4ume,", "tokens": ["Dort", "stehn", "als", "W\u00e4ch\u00b7ter", ",", "ein\u00b7ge\u00b7lullt", "in", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NE", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die alten bl\u00fctenduft'gen Lindenb\u00e4ume.", "tokens": ["Die", "al\u00b7ten", "bl\u00fc\u00b7ten\u00b7duft'\u00b7gen", "Lin\u00b7den\u00b7b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Tores W\u00f6lbung ist in Schutt zerfallen", "tokens": ["Des", "To\u00b7res", "W\u00f6l\u00b7bung", "ist", "in", "Schutt", "zer\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ungehindert tritt er in die Hallen,", "tokens": ["Und", "un\u00b7ge\u00b7hin\u00b7dert", "tritt", "er", "in", "die", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In die mit goldnem Strahl die Sonne schauet,", "tokens": ["In", "die", "mit", "gold\u00b7nem", "Strahl", "die", "Son\u00b7ne", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In die von oben klar der Himmel blauet.", "tokens": ["In", "die", "von", "o\u00b7ben", "klar", "der", "Him\u00b7mel", "blau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf einen moos'gen Stein setzt er sich schweigend,", "tokens": ["Auf", "ei\u00b7nen", "moos'\u00b7gen", "Stein", "setzt", "er", "sich", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er st\u00fctzt das Haupt, es in die Rechte neigend,", "tokens": ["Er", "st\u00fctzt", "das", "Haupt", ",", "es", "in", "die", "Rech\u00b7te", "nei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4\u00dft in freiem Spiele die Gedanken", "tokens": ["Und", "l\u00e4\u00dft", "in", "frei\u00b7em", "Spie\u00b7le", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich mit dem Efeu um die Tr\u00fcmmer ranken:", "tokens": ["Sich", "mit", "dem", "E\u00b7feu", "um", "die", "Tr\u00fcm\u00b7mer", "ran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "\u00bbdu altes Schlo\u00df, wie bist du still geworden,", "tokens": ["\u00bb", "du", "al\u00b7tes", "Schlo\u00df", ",", "wie", "bist", "du", "still", "ge\u00b7wor\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schollst so laut einst von der Lust Akkorden!", "tokens": ["Und", "schollst", "so", "laut", "einst", "von", "der", "Lust", "Ak\u00b7kor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie ist der helle Schmuck dir abgefallen,", "tokens": ["Wie", "ist", "der", "hel\u00b7le", "Schmuck", "dir", "ab\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gl\u00e4nztest einst das herrlichste von allen!", "tokens": ["Und", "gl\u00e4nz\u00b7test", "einst", "das", "herr\u00b7lichs\u00b7te", "von", "al\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Hier fanden sonst zu Spiel und lust'gem Feste", "tokens": ["Hier", "fan\u00b7den", "sonst", "zu", "Spiel", "und", "lust'\u00b7gem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In buntem Schwarm sich hundert edle G\u00e4ste;", "tokens": ["In", "bun\u00b7tem", "Schwarm", "sich", "hun\u00b7dert", "ed\u00b7le", "G\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein hoher Wandrer zog vorbei der St\u00e4tte,", "tokens": ["Kein", "ho\u00b7her", "Wand\u00b7rer", "zog", "vor\u00b7bei", "der", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der unter deinem Dach geruht nicht h\u00e4tte.", "tokens": ["Der", "un\u00b7ter", "dei\u00b7nem", "Dach", "ge\u00b7ruht", "nicht", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nun spielen in des Windes leisem Kosen", "tokens": ["Nun", "spie\u00b7len", "in", "des", "Win\u00b7des", "lei\u00b7sem", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Holunderstr\u00e4uche nur und wilde Rosen,", "tokens": ["Ho\u00b7lun\u00b7der\u00b7str\u00e4u\u00b7che", "nur", "und", "wil\u00b7de", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nur der Sonne, nur des Mondes Schimmer,", "tokens": ["Und", "nur", "der", "Son\u00b7ne", ",", "nur", "des", "Mon\u00b7des", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deinen Hallen rasten sie noch immer.", "tokens": ["In", "dei\u00b7nen", "Hal\u00b7len", "ras\u00b7ten", "sie", "noch", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Hier st\u00fcrzte sich in raschen Melodien", "tokens": ["Hier", "st\u00fcrz\u00b7te", "sich", "in", "ra\u00b7schen", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Trompetenjubel von den Galerien;", "tokens": ["Trom\u00b7pe\u00b7ten\u00b7ju\u00b7bel", "von", "den", "Ga\u00b7le\u00b7ri\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schleppen rauschten, und die Sporen klangen,", "tokens": ["Die", "Schlep\u00b7pen", "rauschten", ",", "und", "die", "Spo\u00b7ren", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn sich im Fackeltanz die Paare schwangen.", "tokens": ["Wenn", "sich", "im", "Fa\u00b7ckel\u00b7tanz", "die", "Paa\u00b7re", "schwan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Jetzt h\u00f6rst du nur das Lied der Nachtigallen", "tokens": ["Jetzt", "h\u00f6rst", "du", "nur", "das", "Lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus den umb\u00fcschten Mauerblenden schallen;", "tokens": ["Aus", "den", "um\u00b7b\u00fcschten", "Mau\u00b7er\u00b7blen\u00b7den", "schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Leuchtk\u00e4fer lassen m\u00e4rchenhaft im Dunkeln", "tokens": ["Leucht\u00b7k\u00e4\u00b7fer", "las\u00b7sen", "m\u00e4r\u00b7chen\u00b7haft", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dazu den lichten Reigen n\u00e4chtlich funkeln.", "tokens": ["Da\u00b7zu", "den", "lich\u00b7ten", "Rei\u00b7gen", "n\u00e4cht\u00b7lich", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Einst schm\u00fcckten Scharlachdecken diese W\u00e4nde,", "tokens": ["Einst", "schm\u00fcck\u00b7ten", "Schar\u00b7lach\u00b7de\u00b7cken", "die\u00b7se", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchwirkt mit lautern Goldes reicher Spende;", "tokens": ["Durch\u00b7wirkt", "mit", "lau\u00b7tern", "Gol\u00b7des", "rei\u00b7cher", "Spen\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom grauen Turme wehten bunte Fahnen,", "tokens": ["Vom", "grau\u00b7en", "Tur\u00b7me", "weh\u00b7ten", "bun\u00b7te", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die stolzen Zeichen der erlauchten Ahnen.", "tokens": ["Die", "stol\u00b7zen", "Zei\u00b7chen", "der", "er\u00b7lauch\u00b7ten", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Nun l\u00e4\u00dft der Himmel seine Purpurgluten", "tokens": ["Nun", "l\u00e4\u00dft", "der", "Him\u00b7mel", "sei\u00b7ne", "Pur\u00b7pur\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In vollen Str\u00f6men um die Tr\u00fcmmer fluten,", "tokens": ["In", "vol\u00b7len", "Str\u00f6\u00b7men", "um", "die", "Tr\u00fcm\u00b7mer", "flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von den Zinnen seh' ich Efeuranken,", "tokens": ["Und", "von", "den", "Zin\u00b7nen", "seh'", "ich", "E\u00b7feu\u00b7ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verg\u00e4nglichkeit, dein gr\u00fcnes Wappen, schwanken.", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ",", "dein", "gr\u00fc\u00b7nes", "Wap\u00b7pen", ",", "schwan\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Dort vom Altane sah im Abendstrahle", "tokens": ["Dort", "vom", "Al\u00b7ta\u00b7ne", "sah", "im", "A\u00b7bend\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Burgherrn ros'ge Tochter wohl zu Tale", "tokens": ["Des", "Burg\u00b7herrn", "ros'\u00b7ge", "Toch\u00b7ter", "wohl", "zu", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und barg geheimnisvoll im reinen Sinne", "tokens": ["Und", "barg", "ge\u00b7heim\u00b7nis\u00b7voll", "im", "rei\u00b7nen", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ersten s\u00fc\u00dfen Bl\u00fctentraum der Minne.", "tokens": ["Den", "ers\u00b7ten", "s\u00fc\u00b7\u00dfen", "Bl\u00fc\u00b7ten\u00b7traum", "der", "Min\u00b7ne", "."]