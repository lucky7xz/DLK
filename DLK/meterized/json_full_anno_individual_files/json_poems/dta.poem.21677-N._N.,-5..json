{"dta.poem.21677": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihm der Suma milchig Gift kredenzt,", "tokens": ["Wenn", "ihm", "der", "Su\u00b7ma", "mil\u00b7chig", "Gift", "kre\u00b7denzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So schl\u00fcrft das Kind wohl arglos diesen Saft.", "tokens": ["So", "schl\u00fcrft", "das", "Kind", "wohl", "arg\u00b7los", "die\u00b7sen", "Saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Schierling statt Magnolien sich\u2019s bekr\u00e4nzt.", "tokens": ["Mit", "Schier\u00b7ling", "statt", "Mag\u00b7no\u00b7li\u00b7en", "sich's", "be\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "NE", "VVPP", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Doch Kinder bleiben wir. Denn ewig gl\u00e4nzt", "tokens": ["Doch", "Kin\u00b7der", "blei\u00b7ben", "wir", ".", "Denn", "e\u00b7wig", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "$.", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Sch\u00f6nheit Tand vor\u2019m Blick der Leidenschaft \u2014", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Tand", "vor'm", "Blick", "der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gleich wie ein Splitter Glas im Mittagsschein", "tokens": ["Gleich", "wie", "ein", "Split\u00b7ter", "Glas", "im", "Mit\u00b7tags\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem Kinde strahlt als bunter Edelstein.", "tokens": ["Dem", "Kin\u00b7de", "strahlt", "als", "bun\u00b7ter", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}