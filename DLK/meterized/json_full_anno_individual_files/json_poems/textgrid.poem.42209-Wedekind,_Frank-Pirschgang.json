{"textgrid.poem.42209": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Pirschgang", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft mich schnobern, la\u00dft mich schn\u00fcffeln", "tokens": ["La\u00dft", "mich", "schno\u00b7bern", ",", "la\u00dft", "mich", "schn\u00fcf\u00b7feln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Stille der W\u00e4lder fort.", "tokens": ["Durch", "die", "Stil\u00b7le", "der", "W\u00e4l\u00b7der", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Schon wittre ich das schwellende Fleisch der Tr\u00fcffeln,", "tokens": ["Schon", "witt\u00b7re", "ich", "das", "schwel\u00b7len\u00b7de", "Fleisch", "der", "Tr\u00fcf\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der saftigen Br\u00fcnetten von Perigord.", "tokens": ["Der", "saf\u00b7ti\u00b7gen", "Br\u00fc\u00b7net\u00b7ten", "von", "Pe\u00b7ri\u00b7gord", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Hier ist der Ort. Ich wetze die Hauer,", "tokens": ["Hier", "ist", "der", "Ort", ".", "Ich", "wet\u00b7ze", "die", "Hau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bohre den R\u00fcssel wohl in den Grund \u2013", "tokens": ["Ich", "boh\u00b7re", "den", "R\u00fcs\u00b7sel", "wohl", "in", "den", "Grund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie macht doch Arbeit das Leben sauer,", "tokens": ["Wie", "macht", "doch", "Ar\u00b7beit", "das", "Le\u00b7ben", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Seele krank und die Glieder wund!", "tokens": ["Die", "See\u00b7le", "krank", "und", "die", "Glie\u00b7der", "wund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Gierig verschling ich die prickelnden Fr\u00fcchte,", "tokens": ["Gie\u00b7rig", "ver\u00b7schling", "ich", "die", "pri\u00b7ckeln\u00b7den", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Bis mich der Satan im R\u00fccken kneipt \u2013", "tokens": ["Bis", "mich", "der", "Sa\u00b7tan", "im", "R\u00fc\u00b7cken", "kneipt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "----+-+-