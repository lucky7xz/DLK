{"textgrid.poem.24132": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Seele ist krank, ich wei\u00df nicht, nach wem,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Seele ist krank, ich wei\u00df nicht, nach wem,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "krank", ",", "ich", "wei\u00df", "nicht", ",", "nach", "wem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "APPR", "PIS", "$,"], "meter": "+-+-++--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Meiner Tr\u00e4ume Gestalten sind Schleier und Nebel,", "tokens": ["Mei\u00b7ner", "Tr\u00e4u\u00b7me", "Ge\u00b7stal\u00b7ten", "sind", "Schlei\u00b7er", "und", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Stimmen, die ich h\u00f6re, sind fern und verweht.", "tokens": ["Die", "Stim\u00b7men", ",", "die", "ich", "h\u00f6\u00b7re", ",", "sind", "fern", "und", "ver\u00b7weht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, schweifende Sehnsucht ohne Ziel!", "tokens": ["Ach", ",", "schwei\u00b7fen\u00b7de", "Sehn\u00b7sucht", "oh\u00b7ne", "Ziel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Irrflug der Seele!", "tokens": ["Irr\u00b7flug", "der", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich stehe einsam, seelenverlassen, arm", "tokens": ["Ich", "ste\u00b7he", "ein\u00b7sam", ",", "see\u00b7len\u00b7ver\u00b7las\u00b7sen", ",", "arm"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In weiter W\u00fcste, starre Wolken nach,", "tokens": ["In", "wei\u00b7ter", "W\u00fcs\u00b7te", ",", "star\u00b7re", "Wol\u00b7ken", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Leeren Gebilden der Winde, die ich liebe,", "tokens": ["Lee\u00b7ren", "Ge\u00b7bil\u00b7den", "der", "Win\u00b7de", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Weil sie wie meine Seele ziellos sind,", "tokens": ["Weil", "sie", "wie", "mei\u00b7ne", "See\u00b7le", "ziel\u00b7los", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wechselgestaltige, sonnenangegl\u00fchte,", "tokens": ["Wech\u00b7sel\u00b7ge\u00b7stal\u00b7ti\u00b7ge", ",", "son\u00b7nen\u00b7an\u00b7ge\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Hochfliegende, die immer wieder", "tokens": ["Hoch\u00b7flie\u00b7gen\u00b7de", ",", "die", "im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zur Erde m\u00fcssen.", "tokens": ["Zur", "Er\u00b7de", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und ist doch um mich rings das Leben voll Gestalten,", "tokens": ["Und", "ist", "doch", "um", "mich", "rings", "das", "Le\u00b7ben", "voll", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPER", "ADV", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das blutgetriebene, bl\u00fchende, voll von Fr\u00fcchten,", "tokens": ["Das", "blut\u00b7ge\u00b7trie\u00b7be\u00b7ne", ",", "bl\u00fc\u00b7hen\u00b7de", ",", "voll", "von", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und manchmal klingen Laute an mein Ohr,", "tokens": ["Und", "manch\u00b7mal", "klin\u00b7gen", "Lau\u00b7te", "an", "mein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und im Vor\u00fcbergehen streift mich manche Hand,", "tokens": ["Und", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "streift", "mich", "man\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und hei\u00dfe Augen seh ich, rote Lippen, leuchtendes Haar,", "tokens": ["Und", "hei\u00b7\u00dfe", "Au\u00b7gen", "seh", "ich", ",", "ro\u00b7te", "Lip\u00b7pen", ",", "leuch\u00b7ten\u00b7des", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "Gew\u00e4nder, die von sch\u00f6nen Gliedern hold bewegt sind, \u2013:", "tokens": ["Ge\u00b7w\u00e4n\u00b7der", ",", "die", "von", "sch\u00f6\u00b7nen", "Glie\u00b7dern", "hold", "be\u00b7wegt", "sind", ",", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN", "ADJD", "VVPP", "VAFIN", "$,", "$(", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mu\u00df ich denn einsam sein?", "tokens": ["Mu\u00df", "ich", "denn", "ein\u00b7sam", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich habe Freunde, die ich neidlos liebe;", "tokens": ["Ich", "ha\u00b7be", "Freun\u00b7de", ",", "die", "ich", "neid\u00b7los", "lie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die kahle Not entfloh aus meinem Hause;", "tokens": ["Die", "kah\u00b7le", "Not", "ent\u00b7floh", "aus", "mei\u00b7nem", "Hau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Gro\u00dfe bildeten, darf ich genie\u00dfen,", "tokens": ["Was", "Gro\u00b7\u00dfe", "bil\u00b7de\u00b7ten", ",", "darf", "ich", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dankbar, nicht m\u00e4kelnd, hingegeben, ruhig;", "tokens": ["Dank\u00b7bar", ",", "nicht", "m\u00e4\u00b7kelnd", ",", "hin\u00b7ge\u00b7ge\u00b7ben", ",", "ru\u00b7hig", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PTKNEG", "VVPP", "$,", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und, wohl bewu\u00dft der Kraft, die mir geworden,", "tokens": ["Und", ",", "wohl", "be\u00b7wu\u00dft", "der", "Kraft", ",", "die", "mir", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht hastig frech ins Ueberm\u00e4\u00dfige schweifend,", "tokens": ["Nicht", "has\u00b7tig", "frech", "ins", "Ue\u00b7ber\u00b7m\u00e4\u00b7\u00dfi\u00b7ge", "schwei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJD", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Selbstsicher im Bereiche meiner Kunst,", "tokens": ["Selbst\u00b7si\u00b7cher", "im", "Be\u00b7rei\u00b7che", "mei\u00b7ner", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcg ich fast m\u00fchelos mir zum Genu\u00df", "tokens": ["F\u00fcg", "ich", "fast", "m\u00fc\u00b7he\u00b7los", "mir", "zum", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gebilde an Gebild.", "tokens": ["Ge\u00b7bil\u00b7de", "an", "Ge\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was seh ich Wolken nach? Was schweifst du irr", "tokens": ["Was", "seh", "ich", "Wol\u00b7ken", "nach", "?", "Was", "schweifst", "du", "irr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins ziellos Weite, sehnsuchtkrank, oh Seele?", "tokens": ["Ins", "ziel\u00b7los", "Wei\u00b7te", ",", "sehn\u00b7sucht\u00b7krank", ",", "oh", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich wei\u00df es wohl, was mich so einsam macht.", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "was", "mich", "so", "ein\u00b7sam", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dies alles, das ich habe, ist ein Tand,", "tokens": ["Dies", "al\u00b7les", ",", "das", "ich", "ha\u00b7be", ",", "ist", "ein", "Tand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "PRELS", "PPER", "VAFIN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht wert, daf\u00fcr des Morgens aufzustehn.", "tokens": ["Nicht", "wert", ",", "da\u00b7f\u00fcr", "des", "Mor\u00b7gens", "auf\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PAV", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du hast die Liebe nicht. Das Wort trifft mich.", "tokens": ["Du", "hast", "die", "Lie\u00b7be", "nicht", ".", "Das", "Wort", "trifft", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Drum bin ich in des Lebens F\u00fclle fremd,", "tokens": ["Drum", "bin", "ich", "in", "des", "Le\u00b7bens", "F\u00fcl\u00b7le", "fremd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Starr, wurzellos und blicke Wolken nach,", "tokens": ["Starr", ",", "wur\u00b7zel\u00b7los", "und", "bli\u00b7