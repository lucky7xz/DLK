{"dta.poem.11932": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die besten Fechter sind im Kampf gefallen immer,", "tokens": ["Die", "bes\u00b7ten", "Fech\u00b7ter", "sind", "im", "Kampf", "ge\u00b7fal\u00b7len", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.2": {"text": "So wie ertrunken meist im Strom die besten Schwimmer.", "tokens": ["So", "wie", "er\u00b7trun\u00b7ken", "meist", "im", "Strom", "die", "bes\u00b7ten", "Schwim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Warum? weil in den Strom sich nur ein Schwimmer wagt,", "tokens": ["Wa\u00b7rum", "?", "weil", "in", "den", "Strom", "sich", "nur", "ein", "Schwim\u00b7mer", "wagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "APPR", "ART", "NN", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur ein Fechter nicht vorm Spiel der Waffen zagt.", "tokens": ["Und", "nur", "ein", "Fech\u00b7ter", "nicht", "vorm", "Spiel", "der", "Waf\u00b7fen", "zagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So reizend ist Gefahr, da\u00df, wer nur halb sie kennt,", "tokens": ["So", "rei\u00b7zend", "ist", "Ge\u00b7fahr", ",", "da\u00df", ",", "wer", "nur", "halb", "sie", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "NN", "$,", "KOUS", "$,", "PWS", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich gleich in sie verliebt, und zu mit Lust ihr rennt;", "tokens": ["Sich", "gleich", "in", "sie", "ver\u00b7liebt", ",", "und", "zu", "mit", "Lust", "ihr", "rennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPER", "VVPP", "$,", "KON", "APPR", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer aber nicht sie kennt, und nie sie hat versucht,", "tokens": ["Wer", "a\u00b7ber", "nicht", "sie", "kennt", ",", "und", "nie", "sie", "hat", "ver\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "PPER", "VVFIN", "$,", "KON", "ADV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": " sie scheuet und sich ihr entzieht mit feiger Flucht;", "tokens": ["sie", "scheu\u00b7et", "und", "sich", "ihr", "ent\u00b7zieht", "mit", "fei\u00b7ger", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PRF", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und nur die Weisesten die rechte Mitt' erzielen,", "tokens": ["Und", "nur", "die", "Wei\u00b7ses\u00b7ten", "die", "rech\u00b7te", "Mitt'", "er\u00b7zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weder Gefahr zu scheun, noch mit Gefahr zu spielen.", "tokens": ["We\u00b7der", "Ge\u00b7fahr", "zu", "scheun", ",", "noch", "mit", "Ge\u00b7fahr", "zu", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$,", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}