{"dta.poem.20080": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.71", "af:0.28"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Zerstoben sind die Wolkenmassen,", "tokens": ["Zer\u00b7sto\u00b7ben", "sind", "die", "Wol\u00b7ken\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Morgensonn\u2019 in\u2019s Fenster scheint:", "tokens": ["Die", "Mor\u00b7gen\u00b7sonn'", "in's", "Fens\u00b7ter", "scheint", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun kann ich wieder mal nicht fassen,", "tokens": ["Nun", "kann", "ich", "wie\u00b7der", "mal", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich die Nacht hindurch geweint.", "tokens": ["Da\u00df", "ich", "die", "Nacht", "hin\u00b7durch", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dahin ist alles was mich dr\u00fcckte,", "tokens": ["Da\u00b7hin", "ist", "al\u00b7les", "was", "mich", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Aug\u2019 ist klar, der Sinn ist frei,", "tokens": ["Das", "Aug'", "ist", "klar", ",", "der", "Sinn", "ist", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was nur je mein Herz entz\u00fcckte,", "tokens": ["Und", "was", "nur", "je", "mein", "Herz", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tanzt wieder, lachend, mir vorbei.", "tokens": ["Tanzt", "wie\u00b7der", ",", "la\u00b7chend", ",", "mir", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJD", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es gr\u00fc\u00dft, es nickt; \u2014 ich steh betroffen,", "tokens": ["Es", "gr\u00fc\u00dft", ",", "es", "nickt", ";", "ich", "steh", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geblendet schier von all dem Licht:", "tokens": ["Ge\u00b7blen\u00b7det", "schier", "von", "all", "dem", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte, liebe, b\u00f6se Hoffen \u2014", "tokens": ["Das", "al\u00b7te", ",", "lie\u00b7be", ",", "b\u00f6\u00b7se", "Hof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele l\u00e4\u00dft es einmal nicht.", "tokens": ["Die", "See\u00b7le", "l\u00e4\u00dft", "es", "ein\u00b7mal", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}