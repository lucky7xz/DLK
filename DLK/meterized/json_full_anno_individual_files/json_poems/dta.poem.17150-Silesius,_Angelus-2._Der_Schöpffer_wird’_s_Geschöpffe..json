{"dta.poem.17150": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "2.  Der Sch\u00f6pffer wird\u2019 s Gesch\u00f6pffe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Das Unerschaffene Licht wird ein erschaffnes Wesen:", "tokens": ["Das", "Un\u00b7er\u00b7schaf\u00b7fe\u00b7ne", "Licht", "wird", "ein", "er\u00b7schaff\u00b7nes", "We\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df sein Gesch\u00f6pffe nur durch selbes-kan genesen.", "tokens": ["Da\u00df", "sein", "Ge\u00b7sch\u00f6pf\u00b7fe", "nur", "durch", "sel\u00b7bes\u00b7kan", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}