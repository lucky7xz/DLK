{"textgrid.poem.55107": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollt ich mich denn so ganz an ", "tokens": ["Sollt", "ich", "mich", "denn", "so", "ganz", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das w\u00e4re mir zuletzt doch reine Plage.", "tokens": ["Das", "w\u00e4\u00b7re", "mir", "zu\u00b7letzt", "doch", "rei\u00b7ne", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Darum versuch ich's gleich am heut'gen Tage", "tokens": ["Da\u00b7rum", "ver\u00b7such", "ich's", "gleich", "am", "heut'\u00b7gen", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nahe nicht dem vielgewohnten Sch\u00f6nen.", "tokens": ["Und", "na\u00b7he", "nicht", "dem", "viel\u00b7ge\u00b7wohn\u00b7ten", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie aber mag ich dich, mein Herz, vers\u00f6hnen,", "tokens": ["Wie", "a\u00b7ber", "mag", "ich", "dich", ",", "mein", "Herz", ",", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich im wicht'gen Fall dich nicht befrage?", "tokens": ["Da\u00df", "ich", "im", "wicht'\u00b7gen", "Fall", "dich", "nicht", "be\u00b7fra\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohlan! Komm her! Wir \u00e4u\u00dfern unsre Klage", "tokens": ["Wo\u00b7hlan", "!", "Komm", "her", "!", "Wir", "\u00e4u\u00b7\u00dfern", "uns\u00b7re", "Kla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In liebevollen, traurig heitern T\u00f6nen.", "tokens": ["In", "lie\u00b7be\u00b7vol\u00b7len", ",", "trau\u00b7rig", "hei\u00b7tern", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Siehst du, es geht! Des Dichters Wink gew\u00e4rtig,", "tokens": ["Siehst", "du", ",", "es", "geht", "!", "Des", "Dich\u00b7ters", "Wink", "ge\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "$.", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Melodisch klingt die durchgespielte Leier,", "tokens": ["Me\u00b7lo\u00b7disch", "klingt", "die", "durch\u00b7ge\u00b7spiel\u00b7te", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Liebesopfer traulich darzubringen.", "tokens": ["Ein", "Lie\u00b7be\u00b7sop\u00b7fer", "trau\u00b7lich", "dar\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du denkst es kaum, und sieh! das Lied ist fertig;", "tokens": ["Du", "denkst", "es", "kaum", ",", "und", "sieh", "!", "das", "Lied", "ist", "fer\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VVIMP", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein was nun? \u2013 Ich d\u00e4cht, im ersten Feuer", "tokens": ["Al\u00b7lein", "was", "nun", "?", "\u2013", "Ich", "d\u00e4cht", ",", "im", "ers\u00b7ten", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PWS", "ADV", "$.", "$(", "PPER", "VVFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir eilten hin, es vor ihr selbst zu singen.", "tokens": ["Wir", "eil\u00b7ten", "hin", ",", "es", "vor", "ihr", "selbst", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sollt ich mich denn so ganz an ", "tokens": ["Sollt", "ich", "mich", "denn", "so", "ganz", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN