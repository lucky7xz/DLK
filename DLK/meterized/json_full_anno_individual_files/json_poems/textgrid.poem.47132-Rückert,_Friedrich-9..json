{"textgrid.poem.47132": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der Guten wollt' ich schmollen,", "tokens": ["Mit", "der", "Gu\u00b7ten", "wollt'", "ich", "schmol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich den Banden zu entziehn,", "tokens": ["Mich", "den", "Ban\u00b7den", "zu", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mich so umstricken wollen,", "tokens": ["Die", "mich", "so", "um\u00b7stri\u00b7cken", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es mir bedenklich schien.", "tokens": ["Da\u00df", "es", "mir", "be\u00b7denk\u00b7lich", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als ich r\u00fcttelt' an den Banden,", "tokens": ["Als", "ich", "r\u00fct\u00b7telt'", "an", "den", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Merkt' ich erst, wie fest sie sind.", "tokens": ["Merkt'", "ich", "erst", ",", "wie", "fest", "sie", "sind", "."], "token_inf