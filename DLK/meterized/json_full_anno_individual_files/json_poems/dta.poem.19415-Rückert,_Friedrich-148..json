{"dta.poem.19415": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "148.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein vielerfahrner Fuchs, der alle Weg' und Stege", "tokens": ["Ein", "vie\u00b7ler\u00b7fahr\u00b7ner", "Fuchs", ",", "der", "al\u00b7le", "Weg'", "und", "Ste\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "$,", "PRELS", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohl ausgegangen hatt' in seinem Waldgehege,", "tokens": ["Wohl", "aus\u00b7ge\u00b7gan\u00b7gen", "hatt'", "in", "sei\u00b7nem", "Wald\u00b7ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat den von J\u00e4gerkunst gestellten Trug getroffen,", "tokens": ["Hat", "den", "von", "J\u00e4\u00b7ger\u00b7kunst", "ge\u00b7stell\u00b7ten", "Trug", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPR", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Falle zugedeckt, und nur die Lockspeis' offen.", "tokens": ["Die", "Fal\u00b7le", "zu\u00b7ge\u00b7deckt", ",", "und", "nur", "die", "Lock\u00b7speis'", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Lockung kannt' er wohl, ihn konnte sie nicht locken;", "tokens": ["Die", "Lo\u00b7ckung", "kannt'", "er", "wohl", ",", "ihn", "konn\u00b7te", "sie", "nicht", "lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum denn gieng er nicht vor\u00fcber ohne Stocken?", "tokens": ["Wa\u00b7rum", "denn", "gieng", "er", "nicht", "vor\u00b7\u00fc\u00b7ber", "oh\u00b7ne", "Sto\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er dachte: w\u00fcrde doch der unsichtbare Grund", "tokens": ["Er", "dach\u00b7te", ":", "w\u00fcr\u00b7de", "doch", "der", "un\u00b7sicht\u00b7ba\u00b7re", "Grund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Sichtbarn auch durch mich dem Unerfahrnen kund!", "tokens": ["Des", "Sicht\u00b7barn", "auch", "durch", "mich", "dem", "Un\u00b7er\u00b7fahr\u00b7nen", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vorsichtig zog er von der Falle weg die Reiser,", "tokens": ["Vor\u00b7sich\u00b7tig", "zog", "er", "von", "der", "Fal\u00b7le", "weg", "die", "Rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Bis sie am Tage lag; dann gieng er wie ein Weiser,", "tokens": ["Bis", "sie", "am", "Ta\u00b7ge", "lag", ";", "dann", "gieng", "er", "wie", "ein", "Wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Zufrieden nicht allein dem Schaden zu entweichen,", "tokens": ["Zu\u00b7frie\u00b7den", "nicht", "al\u00b7lein", "dem", "Scha\u00b7den", "zu", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Davor beh\u00fctet auch zu haben seinesgleichen.", "tokens": ["Da\u00b7vor", "be\u00b7h\u00fc\u00b7tet", "auch", "zu", "ha\u00b7ben", "sei\u00b7nes\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKZU", "VAINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nie soll ein weiser Mann auf seinem Weg vers\u00e4umen,", "tokens": ["Nie", "soll", "ein", "wei\u00b7ser", "Mann", "auf", "sei\u00b7nem", "Weg", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versuchung, wo er kann, f\u00fcr Thoren wegzur\u00e4umen.", "tokens": ["Ver\u00b7su\u00b7chung", ",", "wo", "er", "kann", ",", "f\u00fcr", "Tho\u00b7ren", "weg\u00b7zu\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}