{"dta.poem.11567": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Grab von Schillers Mutter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Nach der Seite des Dorf's, wo jener alternde Zaun dort", "tokens": ["Nach", "der", "Sei\u00b7te", "des", "Dor\u00b7f's", ",", "wo", "je\u00b7ner", "al\u00b7tern\u00b7de", "Zaun", "dort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PWAV", "PDAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "L\u00e4ndliche Gr\u00e4ber umschlie\u00df't, wall' ich in Einsamkeit", "tokens": ["L\u00e4nd\u00b7li\u00b7che", "Gr\u00e4\u00b7ber", "um\u00b7schlie\u00df't", ",", "wall'", "ich", "in", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sieh' den gesunkenen H\u00fcgel! es kennen wenige Greise", "tokens": ["Sieh'", "den", "ge\u00b7sun\u00b7ke\u00b7nen", "H\u00fc\u00b7gel", "!", "es", "ken\u00b7nen", "we\u00b7ni\u00b7ge", "Grei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Kaum ihn noch und es ahnt Niemand ein Heiligthum", "tokens": ["Kaum", "ihn", "noch", "und", "es", "ahnt", "Nie\u00b7mand", "ein", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "KON", "PPER", "VVFIN", "PIS", "ART", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Jegliche Zierde fehlt und jedes deutende Zeichen;", "tokens": ["Jeg\u00b7li\u00b7che", "Zier\u00b7de", "fehlt", "und", "je\u00b7des", "deu\u00b7ten\u00b7de", "Zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "D\u00fcrftig breitet ein Baum sch\u00fctzende Arme umher.", "tokens": ["D\u00fcrf\u00b7tig", "brei\u00b7tet", "ein", "Baum", "sch\u00fct\u00b7zen\u00b7de", "Ar\u00b7me", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Wilde Rose! dich find ich allein statt anderer Blumen;", "tokens": ["Wil\u00b7de", "Ro\u00b7se", "!", "dich", "find", "ich", "al\u00b7lein", "statt", "an\u00b7de\u00b7rer", "Blu\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Ja, besch\u00e4me sie nur, brich als ein Wunder hervor!", "tokens": ["Ja", ",", "be\u00b7sch\u00e4\u00b7me", "sie", "nur", ",", "brich", "als", "ein", "Wun\u00b7der", "her\u00b7vor", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "$,", "ADJD", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Tausendbl\u00e4ttrig er\u00f6ffne dein Herz! entz\u00fcnde dich herrlich", "tokens": ["Tau\u00b7send\u00b7bl\u00e4tt\u00b7rig", "er\u00b7\u00f6ff\u00b7ne", "dein", "Herz", "!", "ent\u00b7z\u00fcn\u00b7de", "dich", "herr\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADJD"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Am begeisternden Duft, den aus der Tiefe du ziehst!", "tokens": ["Am", "be\u00b7geis\u00b7tern\u00b7den", "Duft", ",", "den", "aus", "der", "Tie\u00b7fe", "du", "ziehst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "\u2014 Eines Unsterblichen Mutter liegt hier bestattet; es", "tokens": ["Ei\u00b7nes", "U\u00b7nsterb\u00b7li\u00b7chen", "Mut\u00b7ter", "liegt", "hier", "be\u00b7stat\u00b7tet", ";", "es"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$.", "PPER"], "meter": "+---+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Deutschlands M\u00e4nner und Frau'n eben den Marmor", "tokens": ["Deutschlands", "M\u00e4n\u00b7ner", "und", "Frau'n", "e\u00b7ben", "den", "Mar\u00b7mor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "ADV", "ART", "NN"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}}}}}