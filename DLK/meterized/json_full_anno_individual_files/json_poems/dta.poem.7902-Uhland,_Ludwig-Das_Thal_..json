{"dta.poem.7902": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Thal .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wie willst du dich mir offenbaren,", "tokens": ["Wie", "willst", "du", "dich", "mir", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ungewohnt, geliebtes Thal?", "tokens": ["Wie", "un\u00b7ge\u00b7wohnt", ",", "ge\u00b7lieb\u00b7tes", "Thal", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur in den fr\u00fchsten Jugendjahren", "tokens": ["Nur", "in", "den", "fr\u00fchs\u00b7ten", "Ju\u00b7gend\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erschienst du so mir manchesmal.", "tokens": ["Er\u00b7schienst", "du", "so", "mir", "man\u00b7ches\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne schon hinabgegangen,", "tokens": ["Die", "Son\u00b7ne", "schon", "hin\u00b7ab\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch aus den B\u00e4chen klarer Schein!", "tokens": ["Doch", "aus", "den", "B\u00e4\u00b7chen", "kla\u00b7rer", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein L\u00fcftchen spielt mir um die Wangen,", "tokens": ["Kein", "L\u00fcft\u00b7chen", "spielt", "mir", "um", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch sanftes Rauschen in dem Hain!", "tokens": ["Doch", "sanf\u00b7tes", "Rau\u00b7schen", "in", "dem", "Hain", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es duftet wieder alte Liebe,", "tokens": ["Es", "duf\u00b7tet", "wie\u00b7der", "al\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es gr\u00fcnet wieder alte Lust;", "tokens": ["Es", "gr\u00fc\u00b7net", "wie\u00b7der", "al\u00b7te", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja selbst die alten Liedertriebe", "tokens": ["Ja", "selbst", "die", "al\u00b7ten", "Lie\u00b7der\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beleben diese kalte Brust.", "tokens": ["Be\u00b7le\u00b7ben", "die\u00b7se", "kal\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Natur! wohl braucht es solcher Stunden,", "tokens": ["Na\u00b7tur", "!", "wohl", "braucht", "es", "sol\u00b7cher", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So innig und so liebevoll,", "tokens": ["So", "in\u00b7nig", "und", "so", "lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn dieses arme Herz gesunden,", "tokens": ["Wenn", "die\u00b7ses", "ar\u00b7me", "Herz", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das welkende genesen soll!", "tokens": ["Das", "wel\u00b7ken\u00b7de", "ge\u00b7ne\u00b7sen", "soll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Bedr\u00e4ngt mich einst die Welt noch b\u00e4nger,", "tokens": ["Be\u00b7dr\u00e4ngt", "mich", "einst", "die", "Welt", "noch", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So such\u2019 ich wieder dich, mein Thal!", "tokens": ["So", "such'", "ich", "wie\u00b7der", "dich", ",", "mein", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Empfange dann den kranken S\u00e4nger", "tokens": ["Emp\u00b7fan\u00b7ge", "dann", "den", "kran\u00b7ken", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit solcher Milde noch einmal!", "tokens": ["Mit", "sol\u00b7cher", "Mil\u00b7de", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sink\u2019 ich dann ermattet nieder,", "tokens": ["Und", "sink'", "ich", "dann", "er\u00b7mat\u00b7tet", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So \u00f6ffne leise deinen Grund,", "tokens": ["So", "\u00f6ff\u00b7ne", "lei\u00b7se", "dei\u00b7nen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nimm mich auf, und schlie\u00df ihn wieder,", "tokens": ["Und", "nimm", "mich", "auf", ",", "und", "schlie\u00df", "ihn", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und gr\u00fcne fr\u00f6hlich und gesund!", "tokens": ["Und", "gr\u00fc\u00b7ne", "fr\u00f6h\u00b7lich", "und", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}