{"textgrid.poem.58650": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich Gott in Leiden setzt und dich zu verlassen scheinet,", "tokens": ["Wenn", "dich", "Gott", "in", "Lei\u00b7den", "setzt", "und", "dich", "zu", "ver\u00b7las\u00b7sen", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "VVFIN", "KON", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Glaub's, er hat was Gutes vor, es ist ja nicht b\u00f6s gemeinet,", "tokens": ["Glaub's", ",", "er", "hat", "was", "Gu\u00b7tes", "vor", ",", "es", "ist", "ja", "nicht", "b\u00f6s", "ge\u00b7mei\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PIAT", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Er sucht nur dein ganzes Herz, er will's machen klein und rein,", "tokens": ["Er", "sucht", "nur", "dein", "gan\u00b7zes", "Herz", ",", "er", "will's", "ma\u00b7chen", "klein", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "VVINF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df es frei von S\u00fcnd' und Not ewig m\u00f6g' im Frieden sein.", "tokens": ["Da\u00df", "es", "frei", "von", "S\u00fcnd'", "und", "Not", "e\u00b7wig", "m\u00f6g'", "im", "Frie\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "ADJD", "VMFIN", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "H\u00f6re, Zion, bist du schon wie versto\u00dfen und gefangen,", "tokens": ["H\u00f6\u00b7re", ",", "Zi\u00b7on", ",", "bist", "du", "schon", "wie", "ver\u00b7sto\u00b7\u00dfen", "und", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VAFIN", "PPER", "ADV", "KOKOM", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Gott will wieder sammeln dich, du sollst wieder heim gelangen!", "tokens": ["Gott", "will", "wie\u00b7der", "sam\u00b7meln", "dich", ",", "du", "sollst", "wie\u00b7der", "heim", "ge\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "ADV", "PTKVZ", "VVINF", "$."], "meter": "+---+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Dann wirst du Gott suchen recht und im Geist ihn finden nah,", "tokens": ["Dann", "wirst", "du", "Gott", "su\u00b7chen", "recht", "und", "im", "Geist", "ihn", "fin\u00b7den", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVFIN", "ADJD", "KON", "APPRART", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Dieses ist das Ende doch, drauf du lang gewartet ja.", "to