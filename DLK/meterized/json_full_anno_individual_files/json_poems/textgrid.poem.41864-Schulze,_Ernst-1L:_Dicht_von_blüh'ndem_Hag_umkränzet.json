{"textgrid.poem.41864": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dicht von bl\u00fch'ndem Hag umkr\u00e4nzet", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dicht von bl\u00fch'ndem Hag umkr\u00e4nzet", "tokens": ["Dicht", "von", "bl\u00fch'n\u00b7dem", "Hag", "um\u00b7kr\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich einen Zauberhain;", "tokens": ["Wei\u00df", "ich", "ei\u00b7nen", "Zau\u00b7ber\u00b7hain", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Sch\u00f6ne sprie\u00dft und gl\u00e4nzet", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7ne", "sprie\u00dft", "und", "gl\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort im lieblichen Verein.", "tokens": ["Dort", "im", "lieb\u00b7li\u00b7chen", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was auf freyer Flur entkeimte,", "tokens": ["Was", "auf", "frey\u00b7er", "Flur", "ent\u00b7keim\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Was das Gartenbeet erzieht,", "tokens": ["Was", "das", "Gar\u00b7ten\u00b7beet", "er\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ich sah und was ich tr\u00e4umte,", "tokens": ["Was", "ich", "sah", "und", "was", "ich", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist verbunden dort entbl\u00fcht.", "tokens": ["Ist", "ver\u00b7bun\u00b7den", "dort", "ent\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wunderbare Pfade winden", "tokens": ["Wun\u00b7der\u00b7ba\u00b7re", "Pfa\u00b7de", "win\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Hain sich hin und her,", "tokens": ["Durch", "den", "Hain", "sich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich kann das Ziel nicht finden,", "tokens": ["Und", "ich", "kann", "das", "Ziel", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df den Anfang nimmermehr;", "tokens": ["Wei\u00df", "den", "An\u00b7fang", "nim\u00b7mer\u00b7mehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und doch scheint, so sehr ich staune,", "tokens": ["Und", "doch", "scheint", ",", "so", "sehr", "ich", "stau\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bey des Pfades Neckereyn,", "tokens": ["Bey", "des", "Pfa\u00b7des", "Ne\u00b7cke\u00b7reyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles ohne Kunst und Laune,", "tokens": ["Al\u00b7les", "oh\u00b7ne", "Kunst", "und", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles schlicht und recht zu seyn.", "tokens": ["Al\u00b7les", "schlicht", "und", "recht", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00dfe Lieder h\u00f6r' ich klingen", "tokens": ["S\u00fc\u00b7\u00dfe", "Lie\u00b7der", "h\u00f6r'", "ich", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem gr\u00fcnen Labyrinth,", "tokens": ["Aus", "dem", "gr\u00fc\u00b7nen", "La\u00b7by\u00b7rinth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehe frische Quellen springen,", "tokens": ["Se\u00b7he", "fri\u00b7sche", "Quel\u00b7len", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Athme leichten Fr\u00fchlingswind;", "tokens": ["Ath\u00b7me", "leich\u00b7ten", "Fr\u00fch\u00b7lings\u00b7wind", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald ergl\u00e4nzt im Lichtgefunkel", "tokens": ["Bald", "er\u00b7gl\u00e4nzt", "im", "Licht\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bunt und fr\u00f6hlich Wies' und Flur,", "tokens": ["Bunt", "und", "fr\u00f6h\u00b7lich", "Wies'", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bald empf\u00e4ngt im Hainesdunkel", "tokens": ["Bald", "emp\u00b7f\u00e4ngt", "im", "Hai\u00b7nes\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich der Tr\u00e4ume leise Spur.", "tokens": ["Dich", "der", "Tr\u00e4u\u00b7me", "lei\u00b7se", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freundlich geht und sinnig waltet", "tokens": ["Freund\u00b7lich", "geht", "und", "sin\u00b7nig", "wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Hain die Zauberin;", "tokens": ["In", "dem", "Hain", "die", "Zau\u00b7be\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zierlich, zart und sch\u00f6n gestaltet", "tokens": ["Zier\u00b7lich", ",", "zart", "und", "sch\u00f6n", "ge\u00b7stal\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles sich nach ihrem Sinn.", "tokens": ["Al\u00b7les", "sich", "nach", "ih\u00b7rem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Blumen zu erfrischen,", "tokens": ["Ih\u00b7re", "Blu\u00b7men", "zu", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weilt sie bald am klaren Bach,", "tokens": ["Weilt", "sie", "bald", "am", "kla\u00b7ren", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und bald jagt sie in den B\u00fcschen", "tokens": ["Und", "bald", "jagt", "sie", "in", "den", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bunten Schmetterlingen nach.", "tokens": ["Bun\u00b7ten", "Schmet\u00b7ter\u00b7lin\u00b7gen", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sp\u00e4hend irr' ich hin und wieder,", "tokens": ["Sp\u00e4\u00b7hend", "irr'", "ich", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harre lauschend an der Th\u00fcr,", "tokens": ["Har\u00b7re", "lau\u00b7schend", "an", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe manche leise Lieder,", "tokens": ["Sin\u00b7ge", "man\u00b7che", "lei\u00b7se", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dicht' und tr\u00e4ume nur von ihr.", "tokens": ["Dicht'", "und", "tr\u00e4u\u00b7me", "nur", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es ist gar hell und fr\u00f6hlich", "tokens": ["Denn", "es", "ist", "gar", "hell", "und", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem duft'gen Zauberhain,", "tokens": ["In", "dem", "duft'\u00b7gen", "Zau\u00b7ber\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich w\u00fcrde wunderseelig", "tokens": ["Und", "ich", "w\u00fcr\u00b7de", "wun\u00b7der\u00b7see\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bey der sch\u00f6nen Huldin seyn.", "tokens": ["Bey", "der", "sch\u00f6\u00b7nen", "Hul\u00b7din", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sie \u00f6ffnet wohl die Pforte,", "tokens": ["Und", "sie", "\u00f6ff\u00b7net", "wohl", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut heraus mit milder Ruh,", "tokens": ["Schaut", "he\u00b7raus", "mit", "mil\u00b7der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sendet manche holde Worte,", "tokens": ["Sen\u00b7det", "man\u00b7che", "hol\u00b7de", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen lieben Blick mir zu;", "tokens": ["Man\u00b7chen", "lie\u00b7ben", "Blick", "mir", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch den Wunsch, wonach ich trachte,", "tokens": ["Doch", "den", "Wunsch", ",", "wo\u00b7nach", "ich", "trach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6rt die Strenge nimmer an,", "tokens": ["H\u00f6rt", "die", "Stren\u00b7ge", "nim\u00b7mer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und je s\u00fc\u00dfer sie mir lachte,", "tokens": ["Und", "je", "s\u00fc\u00b7\u00dfer", "sie", "mir", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Desto bittrer wein' ich dann.", "tokens": ["Des\u00b7to", "bit\u00b7trer", "wein'", "ich", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "L\u00e4\u00dft sie mich auch ewig leiden", "tokens": ["L\u00e4\u00dft", "sie", "mich", "auch", "e\u00b7wig", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich weinen sp\u00e4t und fr\u00fch,", "tokens": ["Und", "mich", "wei\u00b7nen", "sp\u00e4t", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch kann ich nimmer scheiden,", "tokens": ["Den\u00b7noch", "kann", "ich", "nim\u00b7mer", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und z\u00fcrnen kann ich nie!", "tokens": ["Ach", ",", "und", "z\u00fcr\u00b7nen", "kann", "ich", "nie", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es wohnt in ihrer Sch\u00f6ne", "tokens": ["Denn", "es", "wohnt", "in", "ih\u00b7rer", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine wunderbare Kraft,", "tokens": ["Ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die zum L\u00e4cheln selbst die Thr\u00e4ne,", "tokens": ["Die", "zum", "L\u00e4\u00b7cheln", "selbst", "die", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und den Schmerz zur Hoffnung schafft.", "tokens": ["Und", "den", "Schmerz", "zur", "Hoff\u00b7nung", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dicht von bl\u00fch'ndem Hag umkr\u00e4nzet", "tokens": ["Dicht", "von", "bl\u00fch'n\u00b7dem", "Hag", "um\u00b7kr\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich einen Zauberhain;", "tokens": ["Wei\u00df", "ich", "ei\u00b7nen", "Zau\u00b7ber\u00b7hain", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Sch\u00f6ne sprie\u00dft und gl\u00e4nzet