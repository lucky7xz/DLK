", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es ist gar hell und fr\u00f6hlich", "tokens": ["Denn", "es", "ist", "gar", "hell", "und", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem duft'gen Zauberhain,", "tokens": ["In", "dem", "duft'\u00b7gen", "Zau\u00b7ber\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich w\u00fcrde wunderseelig", "tokens": ["Und", "ich", "w\u00fcr\u00b7de", "wun\u00b7der\u00b7see\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bey der sch\u00f6nen Huldin seyn.", "tokens": ["Bey", "der", "sch\u00f6\u00b7nen", "Hul\u00b7din", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und sie \u00f6ffnet wohl die Pforte,", "tokens": ["Und", "sie", "\u00f6ff\u00b7net", "wohl", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut heraus mit milder Ruh,", "tokens": ["Schaut", "he\u00b7raus", "mit", "mil\u00b7der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sende