, "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich weinen sp\u00e4t und fr\u00fch,", "tokens": ["Und", "mich", "wei\u00b7nen", "sp\u00e4t", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch kann ich nimmer scheiden,", "tokens": ["Den\u00b7noch", "kann", "ich", "nim\u00b7mer", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und z\u00fcrnen kann ich nie!", "tokens": ["Ach", ",", "und", "z\u00fcr\u00b7nen", "kann", "ich", "nie", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es wohnt in ihrer Sch\u00f6ne", "tokens": ["Denn", "es", "wohnt", "in", "ih\u00b7rer", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine wunderbare Kraft,", "tokens": ["Ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die zum L\u00e4cheln selbst die Thr\u00e4ne,", "tokens": ["Die", "zum", "L\u00e4\u00b7cheln", "selbst", "die", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und den Schmerz zur Hoffnung schafft.", "tokens": ["Und", "den", "Schmerz", "zur", "Hoff\u00b7nung", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}