{"textgrid.poem.55329": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Allerdings", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du Philister! \u2013", "tokens": ["O", "du", "Phi\u00b7lis\u00b7ter", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "NE", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mich und Geschwister", "tokens": ["Mich", "und", "Ge\u00b7schwis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPER", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "M\u00f6gt ihr an solches Wort", "tokens": ["M\u00f6gt", "ihr", "an", "sol\u00b7ches", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nur nicht erinnern: Wir denken:", "tokens": ["Nur", "nicht", "e\u00b7rin\u00b7nern", ":", "Wir", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ort f\u00fcr Ort Sind wir im Innern.", "tokens": ["Ort", "f\u00fcr", "Ort", "Sind", "wir", "im", "In\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das h\u00f6r ich sechzig Jahre wiederholen,", "tokens": ["Das", "h\u00f6r", "ich", "sech\u00b7zig", "Jah\u00b7re", "wie\u00b7der\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "CARD",