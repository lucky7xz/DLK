{"dta.poem.588": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Sch\u00f6ne in der Einbildung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kern ist auffgezehrt/ zieh nur die Schaalen ein/", "tokens": ["Der", "Kern", "ist", "auff\u00b7ge\u00b7zehrt", "/", "zieh", "nur", "die", "Schaa\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "VVFIN", "ADV", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergrabe nicht den Schatz/ den man nicht mahl begehrt/", "tokens": ["Ver\u00b7gra\u00b7be", "nicht", "den", "Schatz", "/", "den", "man", "nicht", "mahl", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$(", "ART", "PIS", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die leeren Tr\u00e4bern mu\u00df man vor die Schweine streun/", "tokens": ["Die", "lee\u00b7ren", "Tr\u00e4\u00b7bern", "mu\u00df", "man", "vor", "die", "Schwei\u00b7ne", "streun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es mu\u00df was feiners seyn/ was solch ein Gast verehrt/", "tokens": ["Es", "mu\u00df", "was", "fei\u00b7ners", "seyn", "/", "was", "solch", "ein", "Gast", "ver\u00b7ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PWS", "ADJA", "VAINF", "$(", "PWS", "PIAT", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als deine abgen\u00fctzte Brust", "tokens": ["Als", "dei\u00b7ne", "ab\u00b7ge\u00b7n\u00fctz\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Maden Lust.", "tokens": ["Der", "Ma\u00b7den", "Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sie sehn nat\u00fcrlich aus wie ausgeleerte Taschen", "tokens": ["Sie", "sehn", "na\u00b7t\u00fcr\u00b7lich", "aus", "wie", "aus\u00b7ge\u00b7leer\u00b7te", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Darinnen nichts zu naschen", "tokens": ["Da\u00b7rin\u00b7nen", "nichts", "zu", "na\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Vor frische Buhlers ist/", "tokens": ["Vor", "fri\u00b7sche", "Buh\u00b7lers", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der Wurm und Krebs sie frist.", "tokens": ["Der", "Wurm", "und", "Krebs", "sie", "frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}