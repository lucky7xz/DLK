 "measure": "iambic.tetra"}, "line.2": {"text": "Ganz offen stand er gar,", "tokens": ["Ganz", "of\u00b7fen", "stand", "er", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie's zu Boden tropfend rann,", "tokens": ["Und", "wie's", "zu", "Bo\u00b7den", "trop\u00b7fend", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da t\u00f6nt' es warnend klar.", "tokens": ["Da", "t\u00f6nt'", "es", "war\u00b7nend", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "\u00bbdem guten Kellergeist sei Dank,", "tokens": ["\u00bb", "dem", "gu\u00b7ten", "Kel\u00b7ler\u00b7geist", "sei", "Dank", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich am Bett gewahrt,", "tokens": ["Den", "ich", "am", "Bett", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat den allerbesten Trank", "tokens": ["Er", "hat", "den", "al\u00b7ler\u00b7bes\u00b7ten", "Trank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir gn\u00e4diglich bewahrt!\u00ab", "tokens": ["Mir", "gn\u00e4\u00b7di\u00b7glich", "be\u00b7wahrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Wohl manchem sitzt er in dem Kopf,", "tokens": ["Wohl", "man\u00b7chem", "sitzt", "er", "in", "dem", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den warnt er nimmermehr,", "tokens": ["Den", "warnt", "er", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er qu\u00e4lt mit Durst den armen Tropf", "tokens": ["Er", "qu\u00e4lt", "mit", "Durst", "den", "ar\u00b7men", "Tropf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis seine F\u00e4sser leer.", "tokens": ["Bis", "sei\u00b7ne", "F\u00e4s\u00b7ser", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "Doch wen er lieb hat, tr\u00e4nkt er gern,", "tokens": ["Doch", "wen", "er", "lieb", "hat", ",", "tr\u00e4nkt", "er", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4lt doch sichre Wacht,", "tokens": ["Und", "h\u00e4lt", "doch", "sich\u00b7re", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So that er