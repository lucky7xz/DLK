{"textgrid.poem.39913": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwas tritt da vor mein Bett zu Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas tritt da vor mein Bett zu Nacht", "tokens": ["\u00bb", "was", "tritt", "da", "vor", "mein", "Bett", "zu", "Nacht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Duftneblige Gestalt?", "tokens": ["Duft\u00b7neb\u00b7li\u00b7ge", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin doch wahrlich ganz erwacht,", "tokens": ["Ich", "bin", "doch", "wahr\u00b7lich", "ganz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist das noch Traums Gewalt?", "tokens": ["Ist", "das", "noch", "Traums", "Ge\u00b7walt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was dr\u00e4ngst du dich so w\u00fcst hervor", "tokens": ["Was", "dr\u00e4ngst", "du", "dich", "so", "w\u00fcst", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meiner \u00f6den Stirn,", "tokens": ["Aus", "mei\u00b7ner", "\u00f6\u00b7den", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du ungef\u00fcges Tr\u00e4umechor,", "tokens": ["Du", "un\u00b7ge\u00b7f\u00fc\u00b7ges", "Tr\u00e4u\u00b7me\u00b7chor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bleib drinne mir im Hirn!\u00ab", "tokens": ["Bleib", "drin\u00b7ne", "mir", "im", "Hirn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch nimmer weicht das dunkle Bild", "tokens": ["Doch", "nim\u00b7mer", "weicht", "das", "dunk\u00b7le", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Scheint's gleich nur Duft und Schaum,", "tokens": ["Scheint's", "gleich", "nur", "Duft", "und", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es winkt so hastig, blickt so wild:", "tokens": ["Es", "winkt", "so", "has\u00b7tig", ",", "blickt", "so", "wild", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O nein, das ist kein Traum!", "tokens": ["O", "nein", ",", "das", "ist", "kein", "Traum", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Hausherr springt vom Lager auf,", "tokens": ["Der", "Haus\u00b7herr", "springt", "vom", "La\u00b7ger", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerstoben ist's, wie Spreu?", "tokens": ["Zer\u00b7sto\u00b7ben", "ist's", ",", "wie", "Spreu", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wirft sich murrend wieder drauf,", "tokens": ["Er", "wirft", "sich", "mur\u00b7rend", "wie\u00b7der", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da gleich erscheint es neu.", "tokens": ["Da", "gleich", "er\u00b7scheint", "es", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie es kommt zum drittenmal,", "tokens": ["Und", "wie", "es", "kommt", "zum", "drit\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirft er sich in sein Kleid,", "tokens": ["Wirft", "er", "sich", "in", "sein", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er stellt sich mitten in den Saal", "tokens": ["Er", "stellt", "sich", "mit\u00b7ten", "in", "den", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Schutz und Trutz bereit.", "tokens": ["Zu", "Schutz", "und", "Trutz", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auf Kettenklirren, Geisterschritt", "tokens": ["Auf", "Ket\u00b7ten\u00b7klir\u00b7ren", ",", "Geis\u00b7ter\u00b7schritt"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spitzt er sein horchend Ohr,", "tokens": ["Spitzt", "er", "sein", "hor\u00b7chend", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch aus der tiefen Stille tritt", "tokens": ["Doch", "aus", "der", "tie\u00b7fen", "Stil\u00b7le", "tritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur sachtes Pochen vor.", "tokens": ["Nur", "sach\u00b7tes", "Po\u00b7chen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit wunderlicher Gegenwart", "tokens": ["Mit", "wun\u00b7der\u00b7li\u00b7cher", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treibt's ihn durch Saal und Flur,", "tokens": ["Treibt's", "ihn", "durch", "Saal", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es t\u00f6nt so leis, es t\u00f6nt so zart,", "tokens": ["Es", "t\u00f6nt", "so", "leis", ",", "es", "t\u00f6nt", "so", "zart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer kommt ihm auf die Spur?", "tokens": ["Wer", "kommt", "ihm", "auf", "die", "Spur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Im Hause wird nun Alles wach,", "tokens": ["Im", "Hau\u00b7se", "wird", "nun", "Al\u00b7les", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Alles h\u00f6rt den Laut;", "tokens": ["Und", "Al\u00b7les", "h\u00f6rt", "den", "Laut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie gehn dem stillen Geiste nach,", "tokens": ["Sie", "gehn", "dem", "stil\u00b7len", "Geis\u00b7te", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So arg es ihnen graut.", "tokens": ["So", "arg", "es", "ih\u00b7nen", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zur Treppe f\u00fchret sie der Lauf,", "tokens": ["Zur", "Trep\u00b7pe", "f\u00fch\u00b7ret", "sie", "der", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drunten sind sie schon,", "tokens": ["Und", "drun\u00b7ten", "sind", "sie", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da steiget von dem Keller auf", "tokens": ["Da", "stei\u00b7get", "von", "dem", "Kel\u00b7ler", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vernehmlich ganz der Ton.", "tokens": ["Ver\u00b7nehm\u00b7lich", "ganz", "der", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbdie Weine sind mir gar zu lieb,", "tokens": ["\u00bb", "die", "Wei\u00b7ne", "sind", "mir", "gar", "zu", "lieb", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es soll mir keiner dran,", "tokens": ["Es", "soll", "mir", "kei\u00b7ner", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geist oder Teufel sei der Dieb,", "tokens": ["Geist", "o\u00b7der", "Teu\u00b7fel", "sei", "der", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ich will ihn dennoch fahn!\u00ab", "tokens": ["Ich", "will", "ihn", "den\u00b7noch", "fahn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und mit der Leuchte durch das Thor", "tokens": ["Und", "mit", "der", "Leuch\u00b7te", "durch", "das", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tritt keck der Hausherr ein,", "tokens": ["Tritt", "keck", "der", "Haus\u00b7herr", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stellt sich laut bei seinem Ohr", "tokens": ["Da", "stellt", "sich", "laut", "bei", "sei\u00b7nem", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Musiciren ein.", "tokens": ["Das", "Mu\u00b7si\u00b7ci\u00b7ren", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Das war das allergr\u00f6\u00dfte Fa\u00df \u2013", "tokens": ["Das", "war", "das", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "Fa\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stand der Geist? o nein!", "tokens": ["Da", "stand", "der", "Geist", "?", "o", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "FM", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur war der Boden k\u00fchl und na\u00df,", "tokens": ["Nur", "war", "der", "Bo\u00b7den", "k\u00fchl", "und", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur pl\u00e4tschernd rann der Wein.", "tokens": ["Nur", "pl\u00e4t\u00b7schernd", "rann", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ein schlimmer W\u00e4chter war der Hahn,", "tokens": ["Ein", "schlim\u00b7mer", "W\u00e4ch\u00b7ter", "war", "der", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz offen stand er gar,", "tokens": ["Ganz", "of\u00b7fen", "stand", "er", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie's zu Boden tropfend rann,", "tokens": ["Und", "wie's", "zu", "Bo\u00b7den", "trop\u00b7fend", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da t\u00f6nt' es warnend klar.", "tokens": ["Da", "t\u00f6nt'", "es", "war\u00b7nend", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "\u00bbdem guten Kellergeist sei Dank,", "tokens": ["\u00bb", "dem", "gu\u00b7ten", "Kel\u00b7ler\u00b7geist", "sei", "Dank", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich am Bett gewahrt,", "tokens": ["Den", "ich", "am", "Bett", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat den allerbesten Trank", "tokens": ["Er", "hat", "den", "al\u00b7ler\u00b7bes\u00b7ten", "Trank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir gn\u00e4diglich bewahrt!\u00ab", "tokens": ["Mir", "gn\u00e4\u00b7di\u00b7glich", "be\u00b7wahrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wohl manchem sitzt er in dem Kopf,", "tokens": ["Wohl", "man\u00b7chem", "sitzt", "er", "in", "dem", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den warnt er nimmermehr,", "tokens": ["Den", "warnt", "er", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er qu\u00e4lt mit Durst den armen Tropf", "tokens": ["Er", "qu