{"dta.poem.2871": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man schl\u00e4gt die Kinder nicht mit schon gebrauchten Besen,", "tokens": ["Man", "schl\u00e4gt", "die", "Kin\u00b7der", "nicht", "mit", "schon", "ge\u00b7brauch\u00b7ten", "Be\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus frischen Zweigen mu\u00df man dazu Ruthen lesen.", "tokens": ["Aus", "fri\u00b7schen", "Zwei\u00b7gen", "mu\u00df", "man", "da\u00b7zu", "Ru\u00b7then", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PIS", "PAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn nicht aufs Ohngef\u00e4hr ge\u00fcbt wird Kinderzucht,", "tokens": ["Denn", "nicht", "aufs", "Ohn\u00b7ge\u00b7f\u00e4hr", "ge\u00b7\u00fcbt", "wird", "Kin\u00b7der\u00b7zucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "VVPP", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Werkzeug sei dazu mit Sorgfalt ausgesucht.", "tokens": ["Das", "Werk\u00b7zeug", "sei", "da\u00b7zu", "mit", "Sorg\u00b7falt", "aus\u00b7ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vom Kinde, das sie schlug, soll sie den Namen tragen,", "tokens": ["Vom", "Kin\u00b7de", ",", "das", "sie", "schlug", ",", "soll", "sie", "den", "Na\u00b7men", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit der Ruthe sollst du dann kein Thier mehr schlagen.", "tokens": ["Und", "mit", "der", "Ru\u00b7the", "sollst", "du", "dann", "kein", "Thier", "mehr", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}