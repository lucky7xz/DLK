, "line.4": {"text": "Der Willk\u00fcr unterwerfen.", "tokens": ["Der", "Will\u00b7k\u00fcr", "un\u00b7ter\u00b7wer\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum singt's in seine Burg hinein,", "tokens": ["Drum", "singt's", "in", "sei\u00b7ne", "Burg", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df zittern alle Scheiben:", "tokens": ["Da\u00df", "zit\u00b7tern", "al\u00b7le", "Schei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir wollen keine D\u00e4nen sein,", "tokens": ["Wir", "wol\u00b7len", "kei\u00b7ne", "D\u00e4\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir wollen Deutsche bleiben!", "tokens": ["Wir", "wol\u00b7len", "Deut\u00b7sche", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nicht s\u00fchnt uns fremder Herrschaft Putz", "tokens": ["Nicht", "s\u00fchnt", "uns", "frem\u00b7der", "Herr\u00b7schaft", "Putz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die eingebornen Schmerzen;", "tokens": ["Die", "ein\u00b7ge\u00b7bor\u00b7nen", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."],