": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Und auch davon den Armen", "tokens": ["Und", "auch", "da\u00b7von", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mittheilen aus Erbarmen,", "tokens": ["Mit\u00b7thei\u00b7len", "aus", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Kirchen, dir zu Ehren", "tokens": ["Den", "Kir\u00b7chen", ",", "dir", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deinen Dienst zu mehren.", "tokens": ["Und", "dei\u00b7nen", "Dienst", "zu", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Gieb unsern Hertzen Freude,", "tokens": ["Gieb", "un\u00b7sern", "Hert\u00b7zen", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Vieh gesinde Weyde", "tokens": ["Dem", "Vieh", "ge\u00b7sin\u00b7de", "Wey\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df den milden Seegen", "tokens": ["Und", "la\u00df", "den", "mil\u00b7den", "See\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um uns sich kr\u00e4ftig legen.", "tokens": ["Um", "uns", "sich", "kr\u00e4f\u00b7tig", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Voraus lab unsre Seele", "tokens": ["Vo\u00b7raus", "lab", "uns\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch deines Wortes Oehle,", "tokens": ["Durch", "dei\u00b7nes", "Wor\u00b7tes", "O\u00b7eh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das Christus hat erworben,", "tokens": ["Das", "Chris\u00b7tus", "hat", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da er f\u00fcr uns gestorben.", "tokens": ["Da", "er", "f\u00fcr", "uns", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Er ist das Brodt des Lebens,", "tokens": ["Er", "ist", "das", "Brodt", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn ihn ist mir vergebens", "tokens": ["Ohn", "ihn", "ist", "mir", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VAFIN", "PPER", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Das alles was wir haben,", "tokens": ["Das", "al\u00b7les", "was", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er schenkt uns Himmels Gaben.", "tokens": ["Er", "schenkt", "uns", "Him\u00b7mels", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Ach, la\u00df uns den gewinnen,", "tokens": ["Ach", ",", "la\u00df", "uns", "den", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er wird uns Seel und Sinnen", "tokens": ["Er", "wird", "uns", "Seel", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus seinen Wunden speisen,", "tokens": ["Aus", "sei\u00b7nen", "Wun\u00b7den", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn wir von hinnen reisen.", "tokens": ["Wenn", "wir", "von", "hin\u00b7nen", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}