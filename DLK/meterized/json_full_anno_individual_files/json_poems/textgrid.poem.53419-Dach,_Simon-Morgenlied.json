{"textgrid.poem.53419": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des hohen Himmels Zinnen,", "tokens": ["Des", "ho\u00b7hen", "Him\u00b7mels", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Erd und was darinnen", "tokens": ["Die", "Erd", "und", "was", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PWS", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist, reicher Gott, dir eigen", "tokens": ["Ist", ",", "rei\u00b7cher", "Gott", ",", "dir", "ei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "ADJD", "NN", "$,", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mu\u00df dir Pflicht erzeigen.", "tokens": ["Und", "mu\u00df", "dir", "Pflicht", "er\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du hei\u00dft das Feld und Auen", "tokens": ["Du", "hei\u00dft", "das", "Feld", "und", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und unser Saat ihm trauen,", "tokens": ["Und", "un\u00b7ser", "Saat", "ihm", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gehn wir auf deinen Wegen,", "tokens": ["Gehn", "wir", "auf", "dei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So bringt es reichen Segen.", "tokens": ["So", "bringt", "es", "rei\u00b7chen", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du hast zu meinem Leben", "tokens": ["Du", "hast", "zu", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Acker mir gegeben,", "tokens": ["Den", "A\u00b7cker", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich trau in deinem Nahmen", "tokens": ["Ich", "trau", "in", "dei\u00b7nem", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm meinen armen Saamen", "tokens": ["Ihm", "mei\u00b7nen", "ar\u00b7men", "Saa\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wei\u00df dabey mit nichten", "tokens": ["Und", "wei\u00df", "da\u00b7bey", "mit", "nich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein mehrers zu verrichten,", "tokens": ["Ein", "meh\u00b7rers", "zu", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ohn da\u00df ich komme treten", "tokens": ["Ohn", "da\u00df", "ich", "kom\u00b7me", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr deinen Thron mit Bethen.", "tokens": ["F\u00fcr", "dei\u00b7nen", "Thron", "mit", "Be\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sieh gn\u00e4dig an mich Armen", "tokens": ["Sieh", "gn\u00e4\u00b7dig", "an", "mich", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hab, o Gott, Erbarmen,", "tokens": ["Und", "hab", ",", "o", "Gott", ",", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "FM", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich f\u00fcr deinen Augen", "tokens": ["La\u00df", "mich", "f\u00fcr", "dei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit meinem Opffer taugen,", "tokens": ["Mit", "mei\u00b7nem", "Opf\u00b7fer", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df mich mein Feld nicht tr\u00fcge", "tokens": ["Da\u00df", "mich", "mein", "Feld", "nicht", "tr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch mein Geschrey erliege,", "tokens": ["Noch", "mein", "Ge\u00b7schrey", "er\u00b7lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm gn\u00e4dig meinen Saaten", "tokens": ["Komm", "gn\u00e4\u00b7dig", "mei\u00b7nen", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit deiner Hutt zu statten.", "tokens": ["Mit", "dei\u00b7ner", "Hutt", "zu", "stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df sie nebst sanften Winden", "tokens": ["La\u00df", "sie", "nebst", "sanf\u00b7ten", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stets Sonnenschein empfinden,", "tokens": ["Stets", "Son\u00b7nen\u00b7schein", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt regnen, da\u00df f\u00fcr Hitze", "tokens": ["Jetzt", "reg\u00b7nen", ",", "da\u00df", "f\u00fcr", "Hit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Hoffnung nicht versitze.", "tokens": ["Die", "Hoff\u00b7nung", "nicht", "ver\u00b7sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dein Ungeziefer steuer", "tokens": ["Dein", "Un\u00b7ge\u00b7zie\u00b7fer", "steu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und allem Ungeheuer,", "tokens": ["Und", "al\u00b7lem", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es treffe sie kein Schade", "tokens": ["Es", "tref\u00b7fe", "sie", "kein", "Scha\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch irgend eine Rade.", "tokens": ["Durch", "ir\u00b7gend", "ei\u00b7ne", "Ra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "La\u00df keinen Meehlthau fallen,", "tokens": ["La\u00df", "kei\u00b7nen", "Meehlt\u00b7hau", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Hagelschlag vor allen", "tokens": ["Den", "Ha\u00b7gel\u00b7schlag", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollst du zur\u00fccke halten", "tokens": ["Wollst", "du", "zu\u00b7r\u00fc\u00b7cke", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Gnade lassen walten,", "tokens": ["Und", "Gna\u00b7de", "las\u00b7sen", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Damit die Frucht der Erden", "tokens": ["Da\u00b7mit", "die", "Frucht", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl eingebracht mag werden", "tokens": ["Wohl", "ein\u00b7ge\u00b7bracht", "mag", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wir sie wohl gen\u00fcssen,", "tokens": ["Und", "wir", "sie", "wohl", "ge\u00b7n\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von keiner Noth gebissen,", "tokens": ["Von", "kei\u00b7ner", "Noth", "ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und auch davon den Armen", "tokens": ["Und", "auch", "da\u00b7von", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mittheilen aus Erbarmen,", "tokens": ["Mit\u00b7thei\u00b7len", "aus", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Kirchen, dir zu Ehren", "tokens": ["Den", "Kir\u00b7chen", ",", "dir", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deinen Dienst zu mehren.", "tokens": ["Und", "dei\u00b7nen", "Dienst", "zu", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Gieb unsern Hertzen Freude,", "tokens": ["Gieb", "un\u00b7sern", "Hert\u00b7zen", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Vieh gesinde Weyde", "tokens": ["Dem", "Vieh", "ge\u00b7sin\u00b7de", "Wey\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df den milden Seegen", "tokens": ["Und", "la\u00df", "den", "mil\u00b7den", "See\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um uns sich kr\u00e4ftig legen.", "tokens": ["Um", "uns", "sich", "kr\u00e4f\u00b7tig", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Voraus lab unsre Seele", "tokens": ["Vo\u00b7raus", "lab", "uns\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch deines Wortes Oehle,", "tokens": ["Durch", "dei\u00b7nes", "Wor\u00b7tes", "O\u00b7eh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das Christus hat erworben,", "tokens": ["Das", "Chris\u00b7tus", "hat", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da er f\u00fcr uns gestorben.", "tokens": ["Da", "er", "f\u00fcr", "uns", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Er ist das Brodt des Lebens,", "tokens": ["Er", "ist", "das", "Brodt", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn ihn ist mir vergebens", "tokens": ["Ohn", "ihn", "ist", "mir", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VAFIN", "PPER", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Das alles was wir haben,", "tokens": ["Das", "al\u00b7les", "was", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er schenkt uns Himmels Gaben.", "tokens": ["Er", "schenkt", "uns", "Him\u00b7mels", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ach, la\u00df uns den gewinnen,", "tokens": ["Ach", ",", "la\u00df", "uns", "den", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er wird uns Seel und Sinnen", "tokens": ["Er", "wird", "uns", "Seel", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus seinen Wunden speisen,", "tokens": ["Aus", "sei\u00b7nen", "Wun\u00b7den", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn wir von hinnen reisen.", "tokens": ["Wenn", "wir", "von", "hin\u00b7nen", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Des hohen Himmels Zinnen,", "tokens": ["Des", "ho\u00b7hen", "Him\u00b7mels", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Erd und was darinnen", "tokens": ["Die", "Erd", "und", "was", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PWS", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist, reicher Gott, dir eigen", "tokens": ["Ist", ",", "rei\u00b7cher", "Gott", ",", "dir", "ei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "ADJD", "NN", "$,", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mu\u00df dir Pflicht erzeigen.", "tokens": ["Und", "mu\u00df", "dir", "Pflicht", "er\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word