quem", "tokens": ["Vnd", "den", "Tod", "be\u00b7quem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In die Arme nehm'.", "tokens": ["In", "die", "Ar\u00b7me", "nehm'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "O wie werd' ich mich", "tokens": ["O", "wie", "werd'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Dort an dir erquicken,", "tokens": ["Dort", "an", "dir", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du wirst mich, und ich", "tokens": ["Du", "wirst", "mich", ",", "und", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KON", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Werde dich anblicken,", "tokens": ["Wer\u00b7de", "dich", "an\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ewig, herrlich, reich,", "tokens": ["E\u00b7wig", ",", "herr\u00b7lich", ",", "reich", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd den Engeln gleich.", "tokens": ["Vnd", "den", "En\u00b7geln", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.18"