.2": {"text": "Dich such ich f\u00fcr allen,", "tokens": ["Dich", "such", "ich", "f\u00fcr", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Weil ich \u00f6d' und w\u00fcst", "tokens": ["Weil", "ich", "\u00f6d'", "und", "w\u00fcst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "In der Welt mu\u00df wallen,", "tokens": ["In", "der", "Welt", "mu\u00df", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd von Creutz und Pein", "tokens": ["Vnd", "von", "Creutz", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nie befreyt kan seyn.", "tokens": ["Nie", "be\u00b7freyt", "kan", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Deinet wegen blo\u00df", "tokens": ["Dei\u00b7net", "we\u00b7gen", "blo\u00df"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Trag' ich die\u00df mein Leiden,", "tokens": ["Trag'", "ich", "die\u00df", "mein", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Diesen Hertzenssto\u00df", "tokens": ["Die\u00b7sen", "Hert\u00b7zens\u00b7sto\u00df"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Willig und mit Frewden,", "tokens": ["Wil\u00b7lig", "und", "mit", "Frew\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Du vers\u00fcssest mir", "tokens": ["Du", "ver\u00b7s\u00fcs\u00b7sest", "mir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Alle Gall allhier.", "tokens": ["Al\u00b7le", "Gall", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Tr\u00fcg' ich durch den Tod", "tokens": ["Tr\u00fcg'", "ich", "durch", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nicht nach dir verlangen,", "tokens": ["Nicht", "nach", "dir", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "O in meiner Noht", "tokens": ["O", "in", "mei\u00b7ner", "Noht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "W\u00e4r ich l\u00e4ngst vergangen,", "tokens": ["W\u00e4r", "ich", "l\u00e4ngst", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Du bist einig Du,", "tokens": ["Du", "bist", "ei\u00b7nig", "Du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nichts sonst meine Ruh.", "tokens": ["Nichts", "sonst", "mei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Gott du kennst vorhin", "tokens": ["Gott", "du", "kennst", "vor\u00b7hin"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Alles was mich kr\u00e4ncket,", "tokens": ["Al\u00b7les", "was", "mich", "kr\u00e4n\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd woran mein Sinn", "tokens": ["Vnd", "wo\u00b7ran", "mein", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Tag und Nacht gedencket,", "tokens": ["Tag", "und", "Nacht", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Niemand weis umb mich", "tokens": ["Nie\u00b7mand", "weis", "umb", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "PTKVZ", "APPR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Als nur Du und ich.", "tokens": ["Als", "nur", "Du", "und", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "KON", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Hab' ich noch nicht sehr", "tokens": ["Hab'", "ich", "noch", "nicht", "sehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Vrsach mich zu klagen,", "tokens": ["Vr\u00b7sach", "mich", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ey so thu noch mehr", "tokens": ["Ey", "so", "thu", "noch", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Plage zu den Plagen,", "tokens": ["Pla\u00b7ge", "zu", "den", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn du tr\u00e4gst, mein Heyl,", "tokens": ["Denn", "du", "tr\u00e4gst", ",", "mein", "Heyl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Doch das meiste Theil.", "tokens": ["Doch", "das", "meis\u00b7te", "Theil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df die\u00df Leben mir", "tokens": ["La\u00df", "die\u00df", "Le\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PDS", "NN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wol versaltzet werden,", "tokens": ["Wol", "ver\u00b7salt\u00b7zet", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ich mich nach dir", "tokens": ["Da\u00df", "ich", "mich", "nach", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sehne von der Erden,", "tokens": ["Seh\u00b7ne", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd den Tod bequem", "tokens": ["Vnd", "den", "Tod", "be\u00b7quem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In die Arme nehm'.", "tokens": ["In", "die", "Ar\u00b7me", "nehm'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "O wie werd' ich mich", "tokens": ["O", "wie", "werd'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Dort an dir erquicken,", "tokens": ["Dort", "an", "dir", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du wirst mich, und ich", "tokens": ["Du", "wirst", "mich", ",", "und", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KON", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Werde dich anblicken,", "tokens": ["Wer\u00b7de", "dich", "an\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ewig, herrlich, reich,", "tokens": ["E\u00b7wig", ",", "herr\u00b7lich", ",", "reich", ","], "token_info": ["word", "punc