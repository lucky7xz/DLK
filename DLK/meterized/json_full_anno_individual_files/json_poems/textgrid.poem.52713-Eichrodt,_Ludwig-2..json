{"textgrid.poem.52713": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuf\u00e4llig in der weiten Stadt,", "tokens": ["Zu\u00b7f\u00e4l\u00b7lig", "in", "der", "wei\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir einander begegnet,", "tokens": ["Sind", "wir", "ein\u00b7an\u00b7der", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oft wenn die Sonne geschienen hat,", "tokens": ["Oft", "wenn", "die", "Son\u00b7ne", "ge\u00b7schie\u00b7nen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein andermal wenns geregnet.", "tokens": ["Ein", "an\u00b7der\u00b7mal", "wenns", "ge\u00b7reg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich habe dich erst nur angestaunt,", "tokens": ["Ich", "ha\u00b7be", "dich", "erst", "nur", "an\u00b7ge\u00b7staunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von deiner Sch\u00f6nheit betroffen,", "tokens": ["Von", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Allm\u00e4lig ward ich so gut gelaunt,", "tokens": ["A\u00b7llm\u00e4\u00b7lig", "ward", "ich", "so", "gut", "ge\u00b7launt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf Gegengru\u00df zu hoffen.", "tokens": ["Auf", "Ge\u00b7gen\u00b7gru\u00df", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein L\u00e4cheln flog um deinen Mund", "tokens": ["Ein", "L\u00e4\u00b7cheln", "flog", "um", "dei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wir uns ferner sahen,", "tokens": ["Als", "wir", "uns", "fer\u00b7ner", "sa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem Innersten ward mirs kund,", "tokens": ["Und", "in", "dem", "In\u00b7ners\u00b7ten", "ward", "mirs", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dir, Sch\u00f6nste, d\u00fcrf ich nahen.", "tokens": ["Dir", ",", "Sch\u00f6ns\u00b7te", ",", "d\u00fcrf", "ich", "na\u00b7hen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O r\u00fchrender Wuchs, o Prachtgestalt,", "tokens": ["O", "r\u00fch\u00b7ren\u00b7der", "Wuchs", ",", "o", "Pracht\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Melosg\u00f6ttin vergleichbar,", "tokens": ["Der", "Me\u00b7los\u00b7g\u00f6t\u00b7tin", "ver\u00b7gleich\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Antlitz voll Nibelungengehalt,", "tokens": ["Ant\u00b7litz", "voll", "Ni\u00b7be\u00b7lun\u00b7gen\u00b7ge\u00b7halt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dein Schwung ist nimmer erreichbar!", "tokens": ["Dein", "Schwung", "ist", "nim\u00b7mer", "er\u00b7reich\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der liebliche Mund, ach wenn er lacht,", "tokens": ["Der", "lieb\u00b7li\u00b7che", "Mund", ",", "ach", "wenn", "er", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ach! deine strahlenden Z\u00fcge,", "tokens": ["Ach", "!", "dei\u00b7ne", "strah\u00b7len\u00b7den", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie rei\u00dfen zu dir mit Wahnsinnsmacht", "tokens": ["Sie", "rei\u00b7\u00dfen", "zu", "dir", "mit", "Wahn\u00b7sinns\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "All meine Gedankenfl\u00fcge!", "tokens": ["All", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken\u00b7fl\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Von Sehnsucht aber umhergejagt", "tokens": ["Von", "Sehn\u00b7sucht", "a\u00b7ber", "um\u00b7her\u00b7ge\u00b7jagt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Werd ich seit jener Stunde,", "tokens": ["Werd", "ich", "seit", "je\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ich den fl\u00fcchtigen Ku\u00df gewagt,", "tokens": ["Da", "ich", "den", "fl\u00fcch\u00b7ti\u00b7gen", "Ku\u00df", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da du mir hingst am Munde.", "tokens": ["Da", "du", "mir", "hingst", "am", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Kein Feuer, keine Kohle kann gl\u00fchn so hei\u00df", "tokens": ["Kein", "Feu\u00b7er", ",", "kei\u00b7ne", "Koh\u00b7le", "kann", "gl\u00fchn", "so", "hei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "ADJD", "ADV", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als heimliche Liebe, so m\u00f6gen", "tokens": ["Als", "heim\u00b7li\u00b7che", "Lie\u00b7be", ",", "so", "m\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$,", "ADV", "VMFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wirs halten, ein traut Geheimni\u00df seis,", "tokens": ["Wirs", "hal\u00b7ten", ",", "ein", "traut", "Ge\u00b7heim\u00b7ni\u00df", "seis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ART", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir wollen es w\u00fcrdig pflegen!", "tokens": ["Wir", "wol\u00b7len", "es", "w\u00fcr\u00b7dig", "pfle\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Wir wollen ach! einen sch\u00f6nen Traum", "tokens": ["Wir", "wol\u00b7len", "ach", "!", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Traum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ITJ", "$.", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zusammen tr\u00e4umen, wir wollen", "tokens": ["Zu\u00b7sam\u00b7men", "tr\u00e4u\u00b7men", ",", "wir", "wol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "PPER", "VMFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ausschl\u00fcrfen mit seinem s\u00fc\u00dfesten Schaum", "tokens": ["Aus\u00b7schl\u00fcr\u00b7fen", "mit", "sei\u00b7nem", "s\u00fc\u00b7\u00dfes\u00b7ten", "Schaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Lebenskelch, den vollen!", "tokens": ["Den", "Le\u00b7bens\u00b7kelch", ",", "den", "vol\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dies Werktagsleben es ist so schal,", "tokens": ["Dies", "Werk\u00b7tags\u00b7le\u00b7ben", "es", "ist", "so", "schal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tr\u00fcbgrau wie Regenwetter,", "tokens": ["Tr\u00fcb\u00b7grau", "wie", "Re\u00b7gen\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur hei\u00dfe Liebe ist Sonnenstrahl,", "tokens": ["Nur", "hei\u00b7\u00dfe", "Lie\u00b7be", "ist", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weckt Bl\u00fcthen auf und Bl\u00e4tter.", "tokens": ["Weckt", "Bl\u00fc\u00b7then", "auf", "und", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O k\u00f6nnt ich volle Stunden einmal", "tokens": ["O", "k\u00f6nnt", "ich", "vol\u00b7le", "Stun\u00b7den", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir ruhig am Busen s\u00e4umen,", "tokens": ["Dir", "ru\u00b7hig", "am", "Bu\u00b7sen", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vergessen die Erde, das Jammerthal,", "tokens": ["Ver\u00b7ges\u00b7sen", "die", "Er\u00b7de", ",", "das", "Jam\u00b7mer\u00b7thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vergessen und selig tr\u00e4umen!", "tokens": ["Ver\u00b7ges\u00b7sen", "und", "se\u00b7lig", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Beherrscherin meiner Phantasie,", "tokens": ["Be\u00b7herr\u00b7sche\u00b7rin", "mei\u00b7ner", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf! nenne mir Ort und Stunde!", "tokens": ["Auf", "!", "nen\u00b7ne", "mir", "Ort", "und", "Stun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Gib meinem Dasein Poesie \u2013", "tokens": ["Gib", "mei\u00b7nem", "Da\u00b7sein", "Poe\u00b7sie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann geh ich ja gern zu Grunde!", "tokens": ["Dann", "geh", "ich", "ja", "gern", "zu", "Grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Noch sind wir stark, noch sind wir jung,", "tokens": ["Noch", "sind", "wir", "stark", ",", "noch", "sind", "wir", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum habe du Muth und wache,", "tokens": ["Drum", "ha\u00b7be", "du", "Muth", "und", "wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "KON", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df selige Lenzerinnerung", "tokens": ["Da\u00df", "se\u00b7li\u00b7ge", "Len\u00b7ze\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns sp\u00e4t noch gl\u00fccklich mache!", "tokens": ["Uns", "sp\u00e4t", "noch", "gl\u00fcck\u00b7lich", "ma\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Zuf\u00e4llig in der weiten Stadt,", "tokens": ["Zu\u00b7f\u00e4l\u00b7lig", "in", "der", "wei\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir einander begegnet,", "tokens": ["Sind", "wir", "ein\u00b7an\u00b7der", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oft wenn die Sonne geschienen hat,", "tokens": ["Oft", "wenn", "die", "Son\u00b7ne", "ge\u00b7schie\u00b7nen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein andermal wenns geregnet.", "tokens": ["Ein", "an\u00b7der\u00b7mal", "wenns", "ge\u00b7reg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Ich habe dich erst nur angestaunt,", "tokens": ["Ich", "ha\u00b7be", "dich", "erst", "nur", "an\u00b7ge\u00b7staunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von deiner Sch\u00f6nheit betroffen,", "tokens": ["Von", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Allm\u00e4lig ward ich so gut gelaunt,", "tokens": ["A\u00b7llm\u00e4\u00b7lig", "ward", "ich", "so", "gut", "ge\u00b7launt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf Gegengru\u00df zu hoffen.", "tokens": ["Auf", "Ge\u00b7gen\u00b7gru\u00df", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ein L\u00e4cheln flog um deinen Mund", "tokens": ["Ein", "L\u00e4\u00b7cheln", "flog", "um", "dei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wir uns ferner sahen,", "tokens": ["Als", "wir", "uns", "fer\u00b7ner", "sa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem Innersten ward mirs kund,", "tokens": ["Und", "in", "dem", "In\u00b7ners\u00b7ten", "ward", "mirs", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dir, Sch\u00f6nste, d\u00fcrf ich nahen.", "tokens": ["Dir", ",", "Sch\u00f6ns\u00b7te", ",", "d\u00fcrf", "ich", "na\u00b7hen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "O r\u00fchrender Wuchs, o Prachtgestalt,", "tokens": ["O", "r\u00fch\u00b7ren\u00b7der", "Wuchs", ",", "o", "Pracht\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Melosg\u00f6ttin vergleichbar,", "tokens": ["Der", "Me\u00b7los\u00b7g\u00f6t\u00b7tin", "ver\u00b7glei