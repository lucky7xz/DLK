{"dta.poem.8784": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Das spr\u00fczt der Wallfisch nicht so rein empor;", "tokens": ["Das", "spr\u00fczt", "der", "Wall\u00b7fisch", "nicht", "so", "rein", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die feinsten Perlen, deine Thr\u00e4nen sind's,", "tokens": ["Die", "feins\u00b7ten", "Per\u00b7len", ",", "dei\u00b7ne", "Thr\u00e4\u00b7nen", "sin\u00b7d's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Kein Taucher fischt sie dir so fein empor;", "tokens": ["Kein", "Tau\u00b7cher", "fischt", "sie", "dir", "so", "fein", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du mu\u00dft die Nelke binden an den Stab,", "tokens": ["Du", "mu\u00dft", "die", "Nel\u00b7ke", "bin\u00b7den", "an", "den", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es rankt der Eppich sich allein empor;", "tokens": ["Es", "rankt", "der", "Ep\u00b7pich", "sich", "al\u00b7lein", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Trunk der Quelle f\u00fchrst du still zum Mund,", "tokens": ["Den", "Trunk", "der", "Quel\u00b7le", "f\u00fchrst", "du", "still", "zum", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch hebst du hoch den Becher Wein empor!", "tokens": ["Doch", "hebst", "du", "hoch", "den", "Be\u00b7cher", "Wein", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}