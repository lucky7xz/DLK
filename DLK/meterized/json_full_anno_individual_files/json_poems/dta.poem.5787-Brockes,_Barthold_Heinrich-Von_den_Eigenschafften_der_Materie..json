{"dta.poem.5787": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weshalben seine Folger sprechen:", "tokens": ["We\u00b7shal\u00b7ben", "sei\u00b7ne", "Fol\u00b7ger", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ohnm\u00f6glich k\u00f6nn\u2019 ein C\u00f6rper, ohn\u2019 ein Leer,", "tokens": ["Ohn\u00b7m\u00f6g\u00b7lich", "k\u00f6nn'", "ein", "C\u00f6r\u00b7per", ",", "ohn'", "ein", "Leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich r\u00fchren und durch C\u00f6rper brechen;", "tokens": ["Sich", "r\u00fch\u00b7ren", "und", "durch", "C\u00f6r\u00b7per", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil durch den ersteren, der einst ger\u00fchret w\u00e4r,", "tokens": ["Weil", "durch", "den", "ers\u00b7te\u00b7ren", ",", "der", "einst", "ge\u00b7r\u00fch\u00b7ret", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bis ins unendliche, sich andre r\u00fchren m\u00fcssen;", "tokens": ["Bis", "ins", "un\u00b7end\u00b7li\u00b7che", ",", "sich", "and\u00b7re", "r\u00fch\u00b7ren", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "$,", "PRF", "PIS", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und die\u00df, wie wir es alle wissen,", "tokens": ["Und", "die\u00df", ",", "wie", "wir", "es", "al\u00b7le", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PWAV", "PPER", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Geschehe ja wol nimmermehr.", "tokens": ["Ge\u00b7sche\u00b7he", "ja", "wol", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}