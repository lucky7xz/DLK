{"textgrid.poem.44265": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[das Leben gleichet einer Schule]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben gleichet einer Schule,", "tokens": ["Das", "Le\u00b7ben", "glei\u00b7chet", "ei\u00b7ner", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo die Gefahr ihr Amt besch\u00fczt", "tokens": ["Wo", "die", "Ge\u00b7fahr", "ihr", "Amt", "be\u00b7sch\u00fczt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und t\u00e4glich auf dem Lehrerstuhle", "tokens": ["Und", "t\u00e4g\u00b7lich", "auf", "dem", "Leh\u00b7rer\u00b7stuh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Unterweisung m\u00fchsam sizt.", "tokens": ["Zur", "Un\u00b7ter\u00b7wei\u00b7sung", "m\u00fch\u00b7sam", "sizt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man f\u00fchlet die Verfolgungsruthen", "tokens": ["Man", "f\u00fch\u00b7let", "die", "Ver\u00b7fol\u00b7gungs\u00b7ru\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wird durch eignen Schaden klug;", "tokens": ["Und", "wird", "durch", "eig\u00b7nen", "Scha\u00b7den", "klug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, wenn schon Aug und Herzen bluten,", "tokens": ["Ja", ",", "wenn", "schon", "Aug", "und", "Her\u00b7zen", "blu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Spricht doch das Ungl\u00fcck nicht: Genug!", "tokens": ["Spricht", "doch", "das", "Un\u00b7gl\u00fcck", "nicht", ":", "Ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "$.", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man l\u00e4st sich durch die Classen f\u00fchren,", "tokens": ["Man", "l\u00e4st", "sich", "durch", "die", "Clas\u00b7sen", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man lernt das Elends-A. B. C.", "tokens": ["Man", "lernt", "das", "E\u00b7lends", "A.", "B.", "C."], "token_info": ["word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PIS", "VVFIN", "ART", "TRUNC", "APPRART", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Wiege lehrt uns buchstabiren,", "tokens": ["Die", "Wie\u00b7ge", "lehrt", "uns", "buch\u00b7sta\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier aber sezt es lauter W.", "tokens": ["Hier", "a\u00b7ber", "sezt", "es", "lau\u00b7ter", "W."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn lesen wir die Klagelieder,", "tokens": ["Denn", "le\u00b7sen", "wir", "die", "Kla\u00b7ge\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eh noch die Zunge lesen kan,", "tokens": ["Eh", "noch", "die", "Zun\u00b7ge", "le\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und deuten durch die schwachen Glieder", "tokens": ["Und", "deu\u00b7ten", "durch", "die", "schwa\u00b7chen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gewi\u00dfer F\u00e4lle Zukunft an.", "tokens": ["Ge\u00b7wi\u00b7\u00dfer", "F\u00e4l\u00b7le", "Zu\u00b7kunft", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die andre Classe bringt die Jugend,", "tokens": ["Die", "and\u00b7re", "Clas\u00b7se", "bringt", "die", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier schreibet uns der Ehrgeiz vor,", "tokens": ["Hier", "schrei\u00b7bet", "uns", "der", "Ehr\u00b7geiz", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wollust zieht uns von der Tugend", "tokens": ["Die", "Wol\u00b7lust", "zieht", "uns", "von", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gar oft in ihr Syrenenchor.", "tokens": ["Gar", "oft", "in", "ihr", "Sy\u00b7re\u00b7nen\u00b7chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Man lernt des alten Adams Sprache", "tokens": ["Man", "lernt", "des", "al\u00b7ten", "A\u00b7dams", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus angebohrner F\u00e4higkeit,", "tokens": ["Aus", "an\u00b7ge\u00b7bohr\u00b7ner", "F\u00e4\u00b7hig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zumahl wenn man sich im Gelache", "tokens": ["Zu\u00b7mahl", "wenn", "man", "sich", "im", "Ge\u00b7la\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der unerkandten S\u00fcnden freut.", "tokens": ["Der", "un\u00b7er\u00b7kand\u00b7ten", "S\u00fcn\u00b7den", "freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drauf r\u00fcckt man auf der Creuzbanck h\u00f6her,", "tokens": ["Drauf", "r\u00fcckt", "man", "auf", "der", "Creuz\u00b7banck", "h\u00f6\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich die Lust des Ehstands zeigt;", "tokens": ["Wenn", "sich", "die", "Lust", "des", "Eh\u00b7stands", "zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wird man ein Pythagoraeer,", "tokens": ["Da", "wird", "man", "ein", "Py\u00b7tha\u00b7go\u00b7ra\u00b7eer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der die geheime Noth verschweigt.", "tokens": ["Der", "die", "ge\u00b7hei\u00b7me", "Noth", "ver\u00b7schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kummer weckt uns alle Morgen;", "tokens": ["Der", "Kum\u00b7mer", "weckt", "uns", "al\u00b7le", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann giebt uns die Erfahrung ein,", "tokens": ["Dann", "giebt", "uns", "die", "Er\u00b7fah\u00b7rung", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df B\u00fccher den vermehlten Sorgen", "tokens": ["Da\u00df", "B\u00fc\u00b7cher", "den", "ver\u00b7mehl\u00b7ten", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll lauter schwarzer Creuze seyn.", "tokens": ["Voll", "lau\u00b7ter", "schwar\u00b7zer", "Creu\u00b7ze", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zulezt kommt uns im Kranckenbette", "tokens": ["Zu\u00b7lezt", "kommt", "uns", "im", "Kran\u00b7cken\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kunst zu sterben schwerer f\u00fcr,", "tokens": ["Die", "Kunst", "zu", "ster\u00b7ben", "schwe\u00b7rer", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als man es je gemeinet h\u00e4tte:", "tokens": ["Als", "man", "es", "je", "ge\u00b7mei\u00b7net", "h\u00e4t\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da liegen, flehn und \u00e4chzen wir,", "tokens": ["Da", "lie\u00b7gen", ",", "flehn", "und", "\u00e4ch\u00b7zen", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVINF", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da fa\u00dfen wir am Lebensrande", "tokens": ["Da", "fa\u00b7\u00dfen", "wir", "am", "Le\u00b7bens\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Predger von der Eitelkeit,", "tokens": ["Den", "Pred\u00b7ger", "von", "der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis uns in dem bedr\u00e4ngten Stande", "tokens": ["Bis", "uns", "in", "dem", "be\u00b7dr\u00e4ng\u00b7ten", "Stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Buch der Gnade Trost verleiht.", "tokens": ["Das", "Buch", "der", "Gna\u00b7de", "Trost", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hierinnen weist sich dein Gem\u00fcthe,", "tokens": ["Hie\u00b7rin\u00b7nen", "weist", "sich", "dein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlseeligster, mehr als ge\u00fcbt,", "tokens": ["Wohl\u00b7see\u00b7ligs\u00b7ter", ",", "mehr", "als", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da dein erkaltendes Gebl\u00fcte", "tokens": ["Da", "dein", "er\u00b7kal\u00b7ten\u00b7des", "Ge\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Seelen willig Abschied giebt.", "tokens": ["Der", "See\u00b7len", "wil\u00b7lig", "Ab\u00b7schied", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sprichst das schwere Wort: Ich sterbe,", "tokens": ["Du", "sprichst", "das", "schwe\u00b7re", "Wort", ":", "Ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ungezwungner Gro\u00dfmuth aus,", "tokens": ["Mit", "un\u00b7ge\u00b7zwung\u00b7ner", "Gro\u00df\u00b7muth", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du l\u00e4st der Welt ihr armes Erbe", "tokens": ["Du", "l\u00e4st", "der", "Welt", "ihr", "ar\u00b7mes", "Er\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ziehst ins rechte Wei\u00dfheitshaus.", "tokens": ["Und", "ziehst", "ins", "rech\u00b7te", "Wei\u00df\u00b7heits\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Schulstaub machte deinem R\u00fccken", "tokens": ["Der", "Schul\u00b7staub", "mach\u00b7te", "dei\u00b7nem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht selten eine schwere Last,", "tokens": ["Nicht", "sel\u00b7ten", "ei\u00b7ne", "schwe\u00b7re", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die du mit unverwandten Blicken", "tokens": ["Die", "du", "mit", "un\u00b7ver\u00b7wand\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Flei\u00df und M\u00fch getragen hast.", "tokens": ["Bey", "Flei\u00df", "und", "M\u00fch", "ge\u00b7tra\u00b7gen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jezt kanstu sie in Sarg verschlie\u00dfen", "tokens": ["Jezt", "kans\u00b7tu", "sie", "in", "Sarg", "ver\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und nach gethaner Arbeit ruhn,", "tokens": ["Und", "nach", "ge\u00b7tha\u00b7ner", "Ar\u00b7beit", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da, wo dich Gl\u00fcck und Wollust k\u00fc\u00dfen", "tokens": ["Da", ",", "wo", "dich", "Gl\u00fcck", "und", "Wol\u00b7lust", "k\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und stets nach deinem Willen thun.", "tokens": ["Und", "stets", "nach", "dei\u00b7nem", "Wil\u00b7len", "thun", "."], "token_info": ["word", "word", "wo