{"textgrid.poem.32321": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "7. Der 24ste J\u00e4nner in Berlin", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch leichter Morgentraum lie\u00df, auf den heil'gen H\u00f6hen,", "tokens": ["Welch", "leich\u00b7ter", "Mor\u00b7gen\u00b7traum", "lie\u00df", ",", "auf", "den", "heil'\u00b7gen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Musen Fest um ", "tokens": ["Der", "Mu\u00b7sen", "Fest", "um"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mich bei Aurorens Glanz mit frommem Schauer sehen,", "tokens": ["Mich", "bei", "Au\u00b7ro\u00b7rens", "Glanz", "mit", "from\u00b7mem", "Schau\u00b7er", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der noch, der noch die Seele f\u00fcllt.", "tokens": ["Der", "noch", ",", "der", "noch", "die", "See\u00b7le", "f\u00fcllt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Traum? nein, nein, kein Traum. Ich sah mit wachem Sinne,", "tokens": ["Ein", "Traum", "?", "nein", ",", "nein", ",", "kein", "Traum", ".", "Ich", "sah", "mit", "wa\u00b7chem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PTKANT", "$,", "PTKANT", "$,", "PIAT", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Musen tanzten darum her.", "tokens": ["Die", "Mu\u00b7sen", "tanz\u00b7ten", "da\u00b7rum", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wach ward ich nah dabei C\u00e4sars und Solons inne,", "tokens": ["Wach", "ward", "ich", "nah", "da\u00b7bei", "C\u00e4\u00b7sars", "und", "So\u00b7lons", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "PAV", "NE", "KON", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch keinen, da\u00df er neidisch w\u00e4r'.", "tokens": ["Doch", "kei\u00b7nen", ",", "da\u00df", "er", "nei\u00b7disch", "w\u00e4r'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein s\u00fc\u00dfer Silberton durchzitterte die L\u00fcfte,", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Sil\u00b7ber\u00b7ton", "durch\u00b7zit\u00b7ter\u00b7te", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis in des Ohres krummen Gang;", "tokens": ["Bis", "in", "des", "Oh\u00b7res", "krum\u00b7men", "Gang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blumen brachen auf, und streuten Balsamd\u00fcfte;", "tokens": ["Die", "Blu\u00b7men", "bra\u00b7chen", "auf", ",", "und", "streu\u00b7ten", "Bal\u00b7sam\u00b7d\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Berg lag lauschend; Klio sang:", "tokens": ["Der", "Berg", "lag", "lau\u00b7schend", ";", "Klio", "sang", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbheil dir! festlicher Tag, der unsern Freund geboren.", "tokens": ["\u00bb", "heil", "dir", "!", "fest\u00b7li\u00b7cher", "Tag", ",", "der", "un\u00b7sern", "Freund", "ge\u00b7bo\u00b7ren", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "$.", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ein K\u00f6nig, Schwestern, unser Freund!", "tokens": ["Ein", "K\u00f6\u00b7nig", ",", "Schwes\u00b7tern", ",", "un\u00b7ser", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heil dir! uns neues Reich, zum Schauplatz ihm erkoren,", "tokens": ["Heil", "dir", "!", "uns", "neu\u00b7es", "Reich", ",", "zum", "Schau\u00b7platz", "ihm", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "ADJA", "NN", "$,", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem frommen Krieger, niemands Feind.", "tokens": ["Dem", "from\u00b7men", "Krie\u00b7ger", ",", "nie\u00b7mands", "Feind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00dft freudig um sein Bild, voll Majest\u00e4t in Blicken,", "tokens": ["La\u00dft", "freu\u00b7dig", "um", "sein", "Bild", ",", "voll", "Ma\u00b7jes\u00b7t\u00e4t", "in", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "APPR", "PPOSAT", "NN", "$,", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der T\u00e4nze Hieroglyphen ziehn!", "tokens": ["Der", "T\u00e4n\u00b7ze", "Hie\u00b7ro\u00b7gly\u00b7phen", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einst, Schwestern, tanzen wir, mit trunkenerm Entz\u00fccken,", "tokens": ["Einst", ",", "Schwes\u00b7tern", ",", "tan\u00b7zen", "wir", ",", "mit", "trun\u00b7ke\u00b7nerm", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Einst, freut euch, tanzen wir um ihn!\u00ab", "tokens"