{"dta.poem.20634": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An die Leda .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von dem Olympus zogest du ihn nieder,                 ", "tokens": ["Von", "dem", "O\u00b7lym\u00b7pus", "zo\u00b7gest", "du", "ihn", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Leda! deinetwegen tr\u00e4gt", "tokens": ["O", "Le\u00b7da", "!", "dei\u00b7net\u00b7we\u00b7gen", "tr\u00e4gt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Donnergott ein lilienwei\u00df Gefieder,", "tokens": ["Der", "Don\u00b7ner\u00b7gott", "ein", "li\u00b7li\u00b7en\u00b7wei\u00df", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der sonst mit Keulen um sich schl\u00e4gt.", "tokens": ["Der", "sonst", "mit", "Keu\u00b7len", "um", "sich", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er theilt die Wolken, seine Fl\u00fcgel trennen", "tokens": ["Er", "theilt", "die", "Wol\u00b7ken", ",", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Aether und den Sonnenstrahl,", "tokens": ["Den", "A\u00b7e\u00b7ther", "und", "den", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Er kommt, und deines Auges Blicke brennen,", "tokens": ["Er", "kommt", ",", "und", "dei\u00b7nes", "Au\u00b7ges", "Bli\u00b7cke", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Antlitz bl\u00fchet wie das Thal.", "tokens": ["Dein", "Ant\u00b7litz", "bl\u00fc\u00b7het", "wie", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Busen schwillt, wie kleine Flocken H\u00fcgel,", "tokens": ["Dein", "Bu\u00b7sen", "schwillt", ",", "wie", "klei\u00b7ne", "Flo\u00b7cken", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Boreas durch Fluren bl\u00e4st", "tokens": ["Wenn", "Bo\u00b7re\u00b7as", "durch", "Flu\u00b7ren", "bl\u00e4st"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder Bach verwandelt wird zum Spiegel,", "tokens": ["Und", "je\u00b7der", "Bach", "ver\u00b7wan\u00b7delt", "wird", "zum", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und das gestorbne Laub verwest.", "tokens": ["Und", "das", "ge\u00b7storb\u00b7ne", "Laub", "ver\u00b7west", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du l\u00e4chelst mit der fein geschnitzten Lippe", "tokens": ["Du", "l\u00e4\u00b7chelst", "mit", "der", "fein", "ge\u00b7schnitz\u00b7ten", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Schwane, der den Hals erhebt", "tokens": ["Dem", "Schwa\u00b7ne", ",", "der", "den", "Hals", "er\u00b7hebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nach der wei\u00dfen Alabaster-Klippe", "tokens": ["Und", "nach", "der", "wei\u00b7\u00dfen", "A\u00b7la\u00b7bas\u00b7ter\u00b7Klip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Woll\u00fcstig mit dem Schnabel strebt.", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "mit", "dem", "Schna\u00b7bel", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Sein maulbeerfarbnes Auge redet Liebe,", "tokens": ["Sein", "maul\u00b7beer\u00b7farb\u00b7nes", "Au\u00b7ge", "re\u00b7det", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ganze Macht der Buhlerei,", "tokens": ["Die", "gan\u00b7ze", "Macht", "der", "Buh\u00b7le\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den innern Aufruhr schlau versteckter Triebe", "tokens": ["Den", "in\u00b7nern", "Auf\u00b7ruhr", "schlau", "ver\u00b7steck\u00b7ter", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verr\u00e4th der Schwan durch Schmeichelei.", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "der", "Schwan", "durch", "Schmei\u00b7che\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Er will dich k\u00fcssen, sterbliche Begl\u00fcckte!", "tokens": ["Er", "will", "dich", "k\u00fcs\u00b7sen", ",", "sterb\u00b7li\u00b7che", "Be\u00b7gl\u00fcck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beneidenswerthe Leda! dich", "tokens": ["Be\u00b7nei\u00b7dens\u00b7wert\u00b7he", "Le\u00b7da", "!", "dich"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$.", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umfa\u00dft mit beiden Fl\u00fcgeln der entz\u00fcckte,", "tokens": ["Um\u00b7fa\u00dft", "mit", "bei\u00b7den", "Fl\u00fc\u00b7geln", "der", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beflammte Gott, und w\u00fcnschet sich", "tokens": ["Be\u00b7flamm\u00b7te", "Gott", ",", "und", "w\u00fcn\u00b7schet", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Den s\u00fc\u00dfen Rausch der K\u00fcssenden auf Erden,", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Rausch", "der", "K\u00fcs\u00b7sen\u00b7den", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fchlet Amors st\u00e4rksten Pfeil,", "tokens": ["Und", "f\u00fch\u00b7let", "A\u00b7mors", "st\u00e4rks\u00b7ten", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und trinket mit s\u00fc\u00dflachenden Geberden", "tokens": ["Und", "trin\u00b7ket", "mit", "s\u00fc\u00df\u00b7la\u00b7chen\u00b7den", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Liebes-Nektars lezten Theil.", "tokens": ["Des", "Lie\u00b7bes\u00b7Nek\u00b7tars", "lez\u00b7ten", "Theil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}