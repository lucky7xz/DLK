{"dta.poem.13284": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun hast du\u201e, fuhr sie auf, das jenige gesehen,", "tokens": ["\u201e", "nun", "hast", "du", "\u201e", ",", "fuhr", "sie", "auf", ",", "das", "je\u00b7ni\u00b7ge", "ge\u00b7se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$(", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewarum die schwere Reis und unser Flug geschehen.", "tokens": ["\u201e", "wa\u00b7rum", "die", "schwe\u00b7re", "Reis", "und", "un\u00b7ser", "Flug", "ge\u00b7sche\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201enicht wahr? du wirst ja mir darum verbunden seyn?", "tokens": ["\u201e", "nicht", "wahr", "?", "du", "wirst", "ja", "mir", "da\u00b7rum", "ver\u00b7bun\u00b7den", "seyn", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "PPER", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und du, war meine Red entgegen, mir verzeihn,", "tokens": ["Und", "du", ",", "war", "mei\u00b7ne", "Red", "ent\u00b7ge\u00b7gen", ",", "mir", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df ich aus Ungeduld dir bin zur Last gewesen?", "tokens": ["Da\u00df", "ich", "aus", "Un\u00b7ge\u00b7duld", "dir", "bin", "zur", "Last", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VAFIN", "APPRART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "20Du konntst auf meiner Stirn oft meine Schwachheit lesen.", "tokens": ["konntst", "auf", "mei\u00b7ner", "Stirn", "oft", "mei\u00b7ne", "Schwach\u00b7heit", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "O klebte diese Nacht mir allzeit in dem Sinn!", "tokens": ["O", "kleb\u00b7te", "die\u00b7se", "Nacht", "mir", "all\u00b7zeit", "in", "dem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDAT", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "O da\u00df ich nicht so starck und so begeistert bin", "tokens": ["O", "da\u00df", "ich", "nicht", "so", "starck", "und", "so", "be\u00b7geis\u00b7tert", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "KON", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den Innhalt dieses Streits, die Reden aufzuschreiben,", "tokens": ["Den", "Inn\u00b7halt", "die\u00b7ses", "Streits", ",", "die", "Re\u00b7den", "auf\u00b7zu\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df sie der sp\u00e4tsten Welt zum Beyspiel m\u00f6chten bleiben!", "tokens": ["Da\u00df", "sie", "der", "sp\u00e4ts\u00b7ten", "Welt", "zum", "Bey\u00b7spiel", "m\u00f6ch\u00b7ten", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "25Hierauf versezte sie: \u201dNimm Geist und Wiz zu Rath!", "tokens": ["ver\u00b7sez\u00b7te", "sie", ":", "\"", "Nimm", "Geist", "und", "Wiz", "zu", "Rath", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "NE", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u201eermuntre das Gem\u00fcth und gib der M\u00fche Statt;", "tokens": ["\u201e", "er\u00b7munt\u00b7re", "das", "Ge\u00b7m\u00fcth", "und", "gib", "der", "M\u00fc\u00b7he", "Statt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "KON", "VVIMP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eso wirst du jedes Spruchs und Umstands dich entsinnen,", "tokens": ["\u201e", "so", "wirst", "du", "je\u00b7des", "Spruchs", "und", "Um\u00b7stands", "dich", "ent\u00b7sin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PIAT", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201emithin ganz leicht den Lauf zu deinem Ziel beginnen.", "tokens": ["\u201e", "mi\u00b7thin", "ganz", "leicht", "den", "Lauf", "zu", "dei\u00b7nem", "Ziel", "be\u00b7gin\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}