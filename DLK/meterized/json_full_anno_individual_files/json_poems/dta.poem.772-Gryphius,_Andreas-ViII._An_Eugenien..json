{"dta.poem.772": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  An  Eugenien.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn meine Seel in Euch/ mein Licht? wie kan ich leben? ", "tokens": ["Wenn", "mei\u00b7ne", "Seel", "in", "Euch", "/", "mein", "Licht", "?", "wie", "kan", "ich", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "$(", "PPOSAT", "NN", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun das verh\u00e4ngn\u00fc\u00df mich so ferne von euch rei\u00dft.", "tokens": ["Nun", "das", "ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "mich", "so", "fer\u00b7ne", "von", "euch", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie kan ich fr\u00f6lich seyn/ wenn jhr mir Ewren geist", "tokens": ["Wie", "kan", "ich", "fr\u00f6\u00b7lich", "seyn", "/", "wenn", "jhr", "mir", "Ew\u00b7ren", "geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VAINF", "$(", "KOUS", "PPER", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht f\u00fcr den meinen woll\u2019t/ (den jhr gefangen) geben?", "tokens": ["Nicht", "f\u00fcr", "den", "mei\u00b7nen", "woll't", "/", "(", "den", "jhr", "ge\u00b7fan\u00b7gen", ")", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "PPOSAT", "NN", "$(", "$(", "ART", "PPOSAT", "ADJA", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man siht mich hier/ doch nur al\u00df ein Gespenste schweben.", "tokens": ["Man", "siht", "mich", "hier", "/", "doch", "nur", "al\u00df", "ein", "Ge\u00b7spens\u00b7te", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$(", "ADV", "ADV", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Al\u00df ein verzaubert Bild/ das sich beweglich wei\u00dft", "tokens": ["Al\u00df", "ein", "ver\u00b7zau\u00b7bert", "Bild", "/", "das", "sich", "be\u00b7weg\u00b7lich", "wei\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "VVFIN", "NN", "$(", "PRELS", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch frembder k\u00fcnste macht/ di\u00df was man sterben hei\u00dft", "tokens": ["Durch", "fremb\u00b7der", "k\u00fcns\u00b7te", "macht", "/", "di\u00df", "was", "man", "ster\u00b7ben", "hei\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "VVFIN", "$(", "PDS", "PRELS", "PIS", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan meine Sch\u1ebdrtzen wol/ nicht meine flamm\u2019 aufheben.", "tokens": ["Kan", "mei\u00b7ne", "Sch\u1ebdrt\u00b7zen", "wol", "/", "nicht", "mei\u00b7ne", "flamm'", "auf\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "$(", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Klagt euch das Hertze nicht das jhr in bande legt", "tokens": ["Klagt", "euch", "das", "Hert\u00b7ze", "nicht", "das", "jhr", "in", "ban\u00b7de", "legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "VVFIN", "PTKNEG", "ART", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie scharff die Geissel sey die meine glieder schl\u00e4gt/", "tokens": ["Wie", "scharff", "die", "Geis\u00b7sel", "sey", "die", "mei\u00b7ne", "glie\u00b7der", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "VAFIN", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch nein es ist zu schwach/ sein Elend au\u00df zusprechen.", "tokens": ["Doch", "nein", "es", "ist", "zu", "schwach", "/", "sein", "E\u00b7lend", "au\u00df", "zu\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "PPER", "VAFIN", "PTKA", "ADJD", "$(", "PPOSAT", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es wei\u00df nichts mehr von mir/ Es kennt euch nur allein/", "tokens": ["Es", "wei\u00df", "nichts", "mehr", "von", "mir", "/", "Es", "kennt", "euch", "nur", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "APPR", "PPER", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es frew\u2019t sich seiner Angst/ vnd w\u00fcndschet diese pein", "tokens": ["Es", "fre\u00b7w't", "sich", "sei\u00b7ner", "Angst", "/", "vnd", "w\u00fcnd\u00b7schet", "die\u00b7se", "pein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PDAT", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Bande/ durch ein band/ das ewig sey/ zu brechen.", "tokens": ["Der", "Ban\u00b7de", "/", "durch", "ein", "band", "/", "das", "e\u00b7wig", "sey", "/", "zu", "bre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ART", "NN", "$(", "ART", "ADJD", "VAFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}