{"dta.poem.21531": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gloria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist der Ruhm? Ein luftig Traumgebild,", "tokens": ["Was", "ist", "der", "Ruhm", "?", "Ein", "luf\u00b7tig", "Traum\u00b7ge\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das wesenlos vor trunk\u2019nen Blicken schaukelt", "tokens": ["Das", "we\u00b7sen\u00b7los", "vor", "trunk'\u00b7nen", "Bli\u00b7cken", "schau\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gold\u2019ne Tr\u00e4ume vor die Seele gaukelt", "tokens": ["Und", "gold'\u00b7ne", "Tr\u00e4u\u00b7me", "vor", "die", "See\u00b7le", "gau\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nimmer euer gl\u00fchend Sehnen stillt.", "tokens": ["Und", "nim\u00b7mer", "eu\u00b7er", "gl\u00fc\u00b7hend", "Seh\u00b7nen", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Greift ihr danach, es schwindet, es zerrinnt", "tokens": ["Greift", "ihr", "da\u00b7nach", ",", "es", "schwin\u00b7det", ",", "es", "zer\u00b7rinnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und flieht in unerme\u00dflich ferne Weiten,", "tokens": ["Und", "flieht", "in", "un\u00b7er\u00b7me\u00df\u00b7lich", "fer\u00b7ne", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Weg kann keines Ird\u2019schen Fu\u00df beschreiten,", "tokens": ["Den", "Weg", "kann", "kei\u00b7nes", "Ird'\u00b7schen", "Fu\u00df", "be\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was auch das Herz erhofft, das Hirn ersinnt.", "tokens": ["Was", "auch", "das", "Herz", "er\u00b7hofft", ",", "das", "Hirn", "er\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was m\u00fcht ihr euch, ihr Durstigen, und ringt", "tokens": ["Was", "m\u00fcht", "ihr", "euch", ",", "ihr", "Durs\u00b7ti\u00b7gen", ",", "und", "ringt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Danach und traut der Hoffnung ros\u2019gen Schimmern,", "tokens": ["Da\u00b7nach", "und", "traut", "der", "Hoff\u00b7nung", "ros'\u00b7gen", "Schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bald, wie der zerborst\u2019nen Glocke Wimmern", "tokens": ["So", "bald", ",", "wie", "der", "zer\u00b7bor\u00b7st'\u00b7nen", "Glo\u00b7cke", "Wim\u00b7mern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Sturm zerflattert, euer Ruhm verklingt.", "tokens": ["Im", "Sturm", "zer\u00b7flat\u00b7tert", ",", "eu\u00b7er", "Ruhm", "ver\u00b7klingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nein, la\u00dft davon in eurem wilden Drang,", "tokens": ["Nein", ",", "la\u00dft", "da\u00b7von", "in", "eu\u00b7rem", "wil\u00b7den", "Drang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PAV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Genie\u00dft der Fr\u00fcchte, die am Lebensbaume", "tokens": ["Ge\u00b7nie\u00dft", "der", "Fr\u00fcch\u00b7te", ",", "die", "am", "Le\u00b7bens\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im \u00fcpp\u2019gen Licht gereift, la\u00dft ab vom Traume,", "tokens": ["Im", "\u00fcpp'\u00b7gen", "Licht", "ge\u00b7reift", ",", "la\u00dft", "ab", "vom", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,", "VVIMP", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann einst verweht des Namens letzter Klang.", "tokens": ["Wann", "einst", "ver\u00b7weht", "des", "Na\u00b7mens", "letz\u00b7ter", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich folge meines Herzens warmem Schlag", "tokens": ["Ich", "fol\u00b7ge", "mei\u00b7nes", "Her\u00b7zens", "war\u00b7mem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und such\u2019 in mir und meiner Kraft Gen\u00fcge,", "tokens": ["Und", "such'", "in", "mir", "und", "mei\u00b7ner", "Kraft", "Ge\u00b7n\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Berauscht mich auch der Duft der s\u00fc\u00dfen L\u00fcge,", "tokens": ["Be\u00b7rauscht", "mich", "auch", "der", "Duft", "der", "s\u00fc\u00b7\u00dfen", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sink\u2019 nicht m\u00fcde hin, ich bleibe wach.", "tokens": ["Ich", "sink'", "nicht", "m\u00fc\u00b7de", "hin", ",", "ich", "blei\u00b7be", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich nutze alles, was das Leben beut,", "tokens": ["Ich", "nut\u00b7ze", "al\u00b7les", ",", "was", "das", "Le\u00b7ben", "beut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Schmerzen ringen, mit den Freuden kosen", "tokens": ["Mit", "Schmer\u00b7zen", "rin\u00b7gen", ",", "mit", "den", "Freu\u00b7den", "ko\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und flechten in den Dornenkranz die Rosen,", "tokens": ["Und", "flech\u00b7ten", "in", "den", "Dor\u00b7nen\u00b7kranz", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das hei\u00dft ein Leben, das sich selbst erneut.", "tokens": ["Das", "hei\u00dft", "ein", "Le\u00b7ben", ",", "das", "sich", "selbst", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da\u00df nicht, wenn ihr mich einstens fragt", "tokens": ["Da\u00df", "nicht", ",", "wenn", "ihr", "mich", "eins\u00b7tens", "fragt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Sterbebett: Was war das Leben, rede!", "tokens": ["Am", "Ster\u00b7be\u00b7bett", ":", "Was", "war", "das", "Le\u00b7ben", ",", "re\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PWS", "VAFIN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lippen zucken und die Wangen bl\u00f6de", "tokens": ["Die", "Lip\u00b7pen", "zu\u00b7cken", "und", "die", "Wan\u00b7gen", "bl\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kaum l\u00e4cheln k\u00f6nnen still verzagt.", "tokens": ["Kaum", "l\u00e4\u00b7cheln", "k\u00f6n\u00b7nen", "still", "ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn meines Lebens Sonne untergeht,", "tokens": ["Wenn", "mei\u00b7nes", "Le\u00b7bens", "Son\u00b7ne", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "La\u00dft sie in reinem Purpur niedertauchen,", "tokens": ["La\u00dft", "sie", "in", "rei\u00b7nem", "Pur\u00b7pur", "nie\u00b7der\u00b7tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df meine Lippen nicht zu stammeln brauchen:", "tokens": ["Da\u00df", "mei\u00b7ne", "Lip\u00b7pen", "nicht", "zu", "stam\u00b7meln", "brau\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du gro\u00dfer Tag, du kamst f\u00fcr mich zu sp\u00e4t.", "tokens": ["Du", "gro\u00b7\u00dfer", "Tag", ",", "du", "kamst", "f\u00fcr", "mich", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}