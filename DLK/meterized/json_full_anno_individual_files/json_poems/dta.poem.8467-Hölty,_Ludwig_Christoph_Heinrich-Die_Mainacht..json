{"dta.poem.8467": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Mainacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und sein schlummerndes Licht \u00fcber den Rasen streut,", "tokens": ["Und", "sein", "schlum\u00b7mern\u00b7des", "Licht", "\u00fc\u00b7ber", "den", "Ra\u00b7sen", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und die Nachtigall fl\u00f6tet,", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gall", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wandl' ich traurig von Busch zu Busch.", "tokens": ["Wandl'", "ich", "trau\u00b7rig", "von", "Busch", "zu", "Busch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Selig preis' ich dich dann, fl\u00f6tende Nachtigall,", "tokens": ["Se\u00b7lig", "preis'", "ich", "dich", "dann", ",", "fl\u00f6\u00b7ten\u00b7de", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PRF", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Weil dein Weibchen mit dir wohnet in Einem Nest,", "tokens": ["Weil", "dein", "Weib\u00b7chen", "mit", "dir", "woh\u00b7net", "in", "Ei\u00b7nem", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ihrem singenden Gatten", "tokens": ["Ih\u00b7rem", "sin\u00b7gen\u00b7den", "Gat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Tausend trauliche K\u00fcsse giebt.", "tokens": ["Tau\u00b7send", "trau\u00b7li\u00b7che", "K\u00fcs\u00b7se", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ueberh\u00fcllet von Laub, girret ein Taubenpaar", "tokens": ["Ue\u00b7ber\u00b7h\u00fcl\u00b7let", "von", "Laub", ",", "gir\u00b7ret", "ein", "Tau\u00b7ben\u00b7paar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sein ", "tokens": ["Sein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Suche dunklere Schatten,", "tokens": ["Su\u00b7che", "dunk\u00b7le\u00b7re", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und die einsame Thr\u00e4ne rinnt.", "tokens": ["Und", "die", "ein\u00b7sa\u00b7me", "Thr\u00e4\u00b7ne", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann, o l\u00e4chelndes Bild, welches wie Morgenroth", "tokens": ["Wann", ",", "o", "l\u00e4\u00b7cheln\u00b7des", "Bild", ",", "wel\u00b7ches", "wie", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "FM", "ADJA", "NN", "$,", "PRELS", "KOKOM", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Durch die Seele mir stralt, find' ich auf Erden dich?", "tokens": ["Durch", "die", "See\u00b7le", "mir", "stralt", ",", "find'", "ich", "auf", "Er\u00b7den", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und die einsame Thr\u00e4ne", "tokens": ["Und", "die", "ein\u00b7sa\u00b7me", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bebt mir heisser die Wang' herab.", "tokens": ["Bebt", "mir", "heis\u00b7ser", "die", "Wang'", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}