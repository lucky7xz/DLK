{"dta.poem.18398": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff schwartze augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwartze augen sind der zunder/", "tokens": ["Schwart\u00b7ze", "au\u00b7gen", "sind", "der", "zun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mich noch zu asche macht.", "tokens": ["Der", "mich", "noch", "zu", "asc\u00b7he", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses sind die st\u00e4rckste blitzen/", "tokens": ["Die\u00b7ses", "sind", "die", "st\u00e4rcks\u00b7te", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Die aus schwartzen wolcken gehn.", "tokens": ["Die", "aus", "schwart\u00b7zen", "wol\u00b7cken", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sie kan noch mehr erh\u00f6hn/", "tokens": ["Was", "sie", "kan", "noch", "mehr", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist/ da\u00df sie/ gleich einem wunder/", "tokens": ["Ist", "/", "da\u00df", "sie", "/", "gleich", "ei\u00b7nem", "wun\u00b7der", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "$(", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sonnen sind und doch auch nacht.", "tokens": ["Son\u00b7nen", "sind", "und", "doch", "auch", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwartze augen sind der zunder/", "tokens": ["Schwart\u00b7ze", "au\u00b7gen", "sind", "der", "zun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der mich noch zu asche macht.", "tokens": ["Der", "mich", "noch", "zu", "asc\u00b7he", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}