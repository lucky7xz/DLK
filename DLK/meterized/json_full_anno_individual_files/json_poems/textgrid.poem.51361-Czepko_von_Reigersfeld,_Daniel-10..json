{"textgrid.poem.51361": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Apollo seinen Strahl", "tokens": ["Wie", "A\u00b7pol\u00b7lo", "sei\u00b7nen", "Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4st zu uns hernieder gehen,", "tokens": ["L\u00e4st", "zu", "uns", "her\u00b7nie\u00b7der", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch in des Himmels H\u00f6hen", "tokens": ["Und", "doch", "in", "des", "Him\u00b7mels", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bleibt Apollo \u00fcberall;", "tokens": ["Bleibt", "A\u00b7pol\u00b7lo", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wie Arethusa Fu\u00df", "tokens": ["Und", "wie", "A\u00b7ret\u00b7hu\u00b7sa", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die See mit seinen Fischen", "tokens": ["Durch", "die", "See", "mit", "sei\u00b7nen", "Fi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kommt gegang