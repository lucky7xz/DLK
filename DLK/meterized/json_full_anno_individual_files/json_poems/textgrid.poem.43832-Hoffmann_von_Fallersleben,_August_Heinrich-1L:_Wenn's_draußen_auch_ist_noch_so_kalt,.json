{"textgrid.poem.43832": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn's drau\u00dfen auch ist noch so kalt,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn's drau\u00dfen auch ist noch so kalt,", "tokens": ["Wenn's", "drau\u00b7\u00dfen", "auch", "ist", "noch", "so", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fuchs geht spazieren im Wald.", "tokens": ["Der", "Fuchs", "geht", "spa\u00b7zie\u00b7ren", "im", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Lauf, lauf, F\u00fcchslein, lauf!", "tokens": ["Lauf", ",", "lauf", ",", "F\u00fcchs\u00b7lein", ",", "lauf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nur keine Zeit verloren!", "tokens": ["Nur", "kei\u00b7ne", "Zeit", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sonst, sonst fassen dich", "tokens": ["Sonst", ",", "sonst", "fas\u00b7sen", "dich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Die Hunde bei den Ohren.", "tokens": ["Die", "Hun\u00b7de", "bei", "den", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Fuchs sieht sich um und ist schlau,", "tokens": ["Der", "Fuchs", "sieht", "sich", "um", "und", "ist", "schlau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "APPR", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Er schl\u00fcpfet geschwind in den Bau.", "tokens": ["Er", "schl\u00fcp\u00b7fet", "ge\u00b7schwind", "in", "den", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Lauft, lauft, Hunde, lauft!", "tokens": ["Lauft", ",", "lauft", ",", "Hun\u00b7de", ",", "lauft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nur keine Zeit verloren!", "tokens": ["Nur", "kei\u00b7ne", "Zeit", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sonst, sonst werden euch", "tokens": ["Sonst", ",", "sonst", "wer\u00b7den", "euch"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Erfrieren Nas' und Ohren.", "tokens": ["Er\u00b7frie\u00b7ren", "Nas'", "und", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem J\u00e4ger dem w