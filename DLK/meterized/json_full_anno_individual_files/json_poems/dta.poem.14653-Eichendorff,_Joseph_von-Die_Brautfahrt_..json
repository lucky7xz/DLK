{"dta.poem.14653": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst Du, hei\u00dfer Sehnsucht Fl\u00fcgel,", "tokens": ["Siehst", "Du", ",", "hei\u00b7\u00dfer", "Sehn\u00b7sucht", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfe Seegel dort gespannt?", "tokens": ["Wei\u00b7\u00dfe", "See\u00b7gel", "dort", "ge\u00b7spannt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst Du tief die feuchten H\u00fcgel", "tokens": ["H\u00f6rst", "Du", "tief", "die", "feuch\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlagen an die Felsenwand?", "tokens": ["Schla\u00b7gen", "an", "die", "Fel\u00b7sen\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das ist Sang zum Hochzeitsreigen \u2014", "tokens": ["Das", "ist", "Sang", "zum", "Hoch\u00b7zeits\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Willst Du mit mir niedersteigen?", "tokens": ["Willst", "Du", "mit", "mir", "nie\u00b7ders\u00b7tei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}