e.3": {"text": "Vnd gibt dabey im Hertzen", "tokens": ["Vnd", "gibt", "da\u00b7bey", "im", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vns einen Stich", "tokens": ["Vns", "ei\u00b7nen", "Stich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der ewig sich", "tokens": ["Der", "e\u00b7wig", "sich"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Enth\u00e4lt in tausent Schmertzen.", "tokens": ["Ent\u00b7h\u00e4lt", "in", "tau\u00b7sent", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Erinnre dich der letzten Noht,", "tokens": ["E\u00b7rinn\u00b7re", "dich", "der", "letz\u00b7ten", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedenck den abgefleischten Tod,", "tokens": ["Be\u00b7denck", "den", "ab\u00b7ge\u00b7fleischten", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Hellen weiten Rachen,", "tokens": ["Der", "Hel\u00b7len", "wei\u00b7ten", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ewig speyht", "tokens": ["Der", "e\u00b7wig", "speyht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {