{"textgrid.poem.53438": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Da der HochEdle etc. Herr Wolff von Kreytzen, K\u00f6nigl. Mayt. in Polen vnd Schw. auch Churfl. Durchl. zu Brandenb. Kriegs- und Landes-Oberste diese Welt gesegnet", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm nichts zu thun in deinen Sinn,", "tokens": ["Nimm", "nichts", "zu", "thun", "in", "dei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schaw alzeit auff das End erst hin,", "tokens": ["Schaw", "al\u00b7zeit", "auff", "das", "End", "erst", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wirstu heilig leben,", "tokens": ["So", "wirs\u00b7tu", "hei\u00b7lig", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast hinfort", "tokens": ["Du", "hast", "hin\u00b7fort"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von jedem Wort", "tokens": ["Von", "je\u00b7dem", "Wort"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auch Rechenschafft zu geben.", "tokens": ["Auch", "Re\u00b7chen\u00b7schafft", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-