{"textgrid.poem.64172": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Zaubermantel", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch thronte K\u00f6nig Arthus im goldnen K\u00f6nigssaal,", "tokens": ["Hoch", "thron\u00b7te", "K\u00f6\u00b7nig", "Ar\u00b7thus", "im", "gold\u00b7nen", "K\u00f6\u00b7nigs\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ginevra ihm zur Seite, sein \u00fcppiges Gemahl.", "tokens": ["Gi\u00b7nev\u00b7ra", "ihm", "zur", "Sei\u00b7te", ",", "sein", "\u00fcp\u00b7pi\u00b7ges", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie trug versteckt im Busen ein feuerfarben Band,", "tokens": ["Sie", "trug", "ver\u00b7steckt", "im", "Bu\u00b7sen", "ein", "feu\u00b7er\u00b7far\u00b7ben", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit feuerfarbner Sch\u00e4rpe Herr Lancelott bei ihr stand.", "tokens": ["Mit", "feu\u00b7er\u00b7farb\u00b7ner", "Sch\u00e4r\u00b7pe", "Herr", "Lan\u00b7ce\u00b7lott", "bei", "ihr", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Ritter der Tafelrunde mit ihren stolzen Frau'n,", "tokens": ["Die", "Rit\u00b7ter", "der", "Ta\u00b7fel\u00b7run\u00b7de", "mit", "ih\u00b7ren", "stol\u00b7zen", "Frau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die sa\u00dfen auf goldnen St\u00fchlen: \u2013 viel Pracht war da zu schaun.", "tokens": ["Die", "sa\u00b7\u00dfen", "auf", "gold\u00b7nen", "St\u00fch\u00b7len", ":", "\u2013", "viel", "Pracht", "war", "da", "zu", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$.", "$(", "PIAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Pfau prangt auf der Tafel, der Schenk f\u00fcllt den Pokal:", "tokens": ["Der", "Pfau", "prangt", "auf", "der", "Ta\u00b7fel", ",", "der", "Schenk", "f\u00fcllt", "den", "Po\u00b7kal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So oft ihn leert der K\u00f6nig, klingen die H\u00f6rner im Saal.", "tokens": ["So", "oft", "ihn", "leert", "der", "K\u00f6\u00b7nig", ",", "klin\u00b7gen", "die", "H\u00f6r\u00b7ner", "im", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Da tritt mit rotem Mantel ein Knabe vor sie hin: \u2013", "tokens": ["Da", "tritt", "mit", "ro\u00b7tem", "Man\u00b7tel", "ein", "Kna\u00b7be", "vor", "sie", "hin", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbgegr\u00fc\u00dft, du edler K\u00f6nig, gegr\u00fc\u00dft, Frau K\u00f6nigin.", "tokens": ["\u00bb", "ge\u00b7gr\u00fc\u00dft", ",", "du", "ed\u00b7ler", "K\u00f6\u00b7nig", ",", "ge\u00b7gr\u00fc\u00dft", ",", "Frau", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "PPER", "ADJA", "NN", "$,", "VVPP", "$,", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Jetzt mag sich freun und r\u00fchmen, wem treu sein sittig Weib:", "tokens": ["Jetzt", "mag", "sich", "freun", "und", "r\u00fch\u00b7men", ",", "wem", "treu", "sein", "sit\u00b7tig", "Weib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$,", "PWS", "ADJD", "VAINF", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Zaubermantel kleidet keinen schn\u00f6den Leib.\u00ab", "tokens": ["Der", "Zau\u00b7ber\u00b7man\u00b7tel", "klei\u00b7det", "kei\u00b7nen", "schn\u00f6\u00b7den", "Leib", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der K\u00f6nig winkt: \u2013 die K\u00f6nigin, sie steht vom Thronstuhl auf:", "tokens": ["Der", "K\u00f6\u00b7nig", "winkt", ":", "\u2013", "die", "K\u00f6\u00b7ni\u00b7gin", ",", "sie", "steht", "vom", "Thron\u00b7stuhl", "auf", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Es ballt die Faust Herr Lancelot an seinem Schwertesknauf.", "tokens": ["Es", "ballt", "die", "Faust", "Herr", "Lan\u00b7ce\u00b7lot", "an", "sei\u00b7nem", "Schwer\u00b7tes\u00b7knauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Den Mantel wirft die K\u00f6nigin um ihre Schultern leicht,", "tokens": ["Den", "Man\u00b7tel", "wirft", "die", "K\u00f6\u00b7ni\u00b7gin", "um", "ih\u00b7re", "Schul\u00b7tern", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Da wirft er b\u00f6se Falten, der sch\u00f6ne Purpur bleicht.", "tokens": ["Da", "wirft", "er", "b\u00f6\u00b7se", "Fal\u00b7ten", ",", "der", "sch\u00f6\u00b7ne", "Pur\u00b7pur", "bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Wie welkes Laub im Herbste schrumpft er zusammen fahl: \u2013", "tokens": ["Wie", "wel\u00b7kes", "Laub", "im", "Herbs\u00b7te", "schrumpft", "er", "zu\u00b7sam\u00b7men", "fahl", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PWAT", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie schleudert ihn zur Erde und st\u00fcrmet aus dem Saal.", "tokens": ["Sie", "schleu\u00b7dert", "ihn", "zur", "Er\u00b7de", "und", "st\u00fcr\u00b7met", "aus", "dem", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Der K\u00f6nig furcht gewaltig die d\u00fcstern Augenbrauen:", "tokens": ["Der", "K\u00f6\u00b7nig", "furcht", "ge\u00b7wal\u00b7tig", "die", "d\u00fcs\u00b7tern", "Au\u00b7gen\u00b7brau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbwohlan! Wer ist die zweite von diesen edlen Frauen?\u00ab", "tokens": ["\u00bb", "wo\u00b7hlan", "!", "Wer", "ist", "die", "zwei\u00b7te", "von", "die\u00b7sen", "ed\u00b7len", "Frau\u00b7en", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "PWS", "VAFIN", "ART", "ADJA", "APPR", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Er ruft's: \u2013 sie schweigen alle: \u2013 sie blicken in den Scho\u00df \u2013", "tokens": ["Er", "ruft's", ":", "\u2013", "sie", "schwei\u00b7gen", "al\u00b7le", ":", "\u2013", "sie", "bli\u00b7cken", "in", "den", "Scho\u00df", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PIS", "$.", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbwie? Keine will es wagen? Die Schande, traun! ist gro\u00df.\u00ab", "tokens": ["\u00bb", "wie", "?", "Kei\u00b7ne", "will", "es", "wa\u00b7gen", "?", "Die", "Schan\u00b7de", ",", "traun", "!", "ist", "gro\u00df", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "$.", "PIS", "VMFIN", "PPER", "VVINF", "$.", "ART", "NN", "$,", "VVINF", "$.", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Da tritt Herrn Lanvals Gattin hervor, Frau Floribell:", "tokens": ["Da", "tritt", "Herrn", "Lan\u00b7vals", "Gat\u00b7tin", "her\u00b7vor", ",", "Frau", "Flo\u00b7ri\u00b7bell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "NN", "PTKVZ", "$,", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es gl\u00fchen ihre Wangen wie zwei junge Rosen hell.", "tokens": ["Es", "gl\u00fc\u00b7hen", "ih\u00b7re", "Wan\u00b7gen", "wie", "zwei", "jun\u00b7ge", "Ro\u00b7sen", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOKOM", "CARD", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.13": {"line.1": {"text": "Sie steigt gesenkten Auges den goldnen Thron hinan,", "tokens": ["Sie", "steigt", "ge\u00b7senk\u00b7ten", "Au\u00b7ges", "den", "gold\u00b7nen", "Thron", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sonder Zittern legt sie den Zaubermantel an.", "tokens": ["Und", "son\u00b7der", "Zit\u00b7tern", "legt", "sie", "den", "Zau\u00b7ber\u00b7man\u00b7tel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Da gl\u00e4ttet sich und schmiegt sich und dehnt sich das Gewand: \u2013", "tokens": ["Da", "gl\u00e4t\u00b7tet", "sich", "und", "schmiegt", "sich", "und", "dehnt", "sich", "das", "Ge\u00b7wand", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nur eine leise Falte sich an der Schulter spannt.", "tokens": ["Nur", "ei\u00b7ne", "lei\u00b7se", "Fal\u00b7te", "sich", "an", "der", "Schul\u00b7ter", "spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "\u00bbo schmiege dich, mein Mantel! Willst du mir nicht verzeihn,", "tokens": ["\u00bb", "o", "schmie\u00b7ge", "dich", ",", "mein", "Man\u00b7tel", "!", "Willst", "du", "mir", "nicht", "ver\u00b7zeihn", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df ich als M\u00e4dchen k\u00fc\u00dfte Lanval, den Gatten mein?\u00ab", "tokens": ["Da\u00df", "ich", "als", "M\u00e4d\u00b7chen", "k\u00fc\u00df\u00b7te", "Lan\u00b7val", ",", "den", "Gat\u00b7ten", "mein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VVFIN", "NE", "$,", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Da f\u00e4llt die letzte Falte, der Mantel flie\u00dft und wallt,", "tokens": ["Da", "f\u00e4llt", "die", "letz\u00b7te", "Fal\u00b7te", ",", "der", "Man\u00b7tel", "flie\u00dft", "und", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und herrlich ist zu schauen die liebliche Gestalt.", "tokens": ["Und", "herr\u00b7lich", "ist", "zu", "schau\u00b7en", "die", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "\u00bbherr Lanval,\u00ab \u2013 rief der K\u00f6nig \u2013 \u00bbIhr seid der Erste hier:", "tokens": ["\u00bb", "herr", "Lan\u00b7val", ",", "\u00ab", "\u2013", "rief", "der", "K\u00f6\u00b7nig", "\u2013", "\u00bb", "Ihr", "seid", "der", "Ers\u00b7te", "hier", ":"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "$(", "$(", "VVFIN", "ART", "NN", "$(", "$(", "PPER", "VAFIN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich trage nur die Krone, der Gl\u00fccklichste seid Ihr.\u00ab", "tokens": ["Ich", "tra\u00b7ge", "nur", "die", "Kro\u00b7ne", ",", "der", "Gl\u00fcck\u00b7lichs\u00b7te", "seid", "Ihr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "$.", "$("], "meter": "-+-+-+---+-++", "measure": "unknown.measure.hexa"}}, "stanza.18": {"line.1": {"text": "Hoch thronte K\u00f6nig Arthus im goldnen K\u00f6nigssaal,", "tokens": ["Hoch", "thron\u00b7te", "K\u00f6\u00b7nig", "Ar\u00b7thus", "im", "gold\u00b7nen", "K\u00f6\u00b7nigs\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ginevra ihm zur Seite, sein \u00fcppiges Gemahl.", "tokens": ["Gi\u00b7nev\u00b7ra", "ihm", "zur", "Sei\u00b7te", ",", "sein", "\u00fcp\u00b7pi\u00b7ges", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "Sie trug versteckt im Busen ein feuerfarben Band,", "tokens": ["Sie", "trug", "ver\u00b7steckt", "im", "Bu\u00b7sen", "ein", "feu\u00b7er\u00b7far\u00b7ben", "Ban