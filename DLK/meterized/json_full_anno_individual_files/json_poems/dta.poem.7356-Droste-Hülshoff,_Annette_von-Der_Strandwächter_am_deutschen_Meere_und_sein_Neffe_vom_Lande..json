{"dta.poem.7356": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Strandw\u00e4chter am deutschen Meere  \n und sein Neffe vom Lande.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "\u201esieben N\u00e4chte stand ich am Riff", "tokens": ["\u201e", "sie\u00b7ben", "N\u00e4ch\u00b7te", "stand", "ich", "am", "Riff"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "CARD", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und h\u00f6rte die Woge zerschellen,", "tokens": ["Und", "h\u00f6r\u00b7te", "die", "Wo\u00b7ge", "zer\u00b7schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Taucht kein Segel, kein irres Schiff?", "tokens": ["Taucht", "kein", "Se\u00b7gel", ",", "kein", "ir\u00b7res", "Schiff", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Schon dunkelt's \u00fcber den Wellen.", "tokens": ["Schon", "dun\u00b7kelt's", "\u00fc\u00b7ber", "den", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nimm das Nachtrohr, Neffe vom Land'!", "tokens": ["Nimm", "das", "Nach\u00b7trohr", ",", "Nef\u00b7fe", "vom", "Land'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ich will in die Matte mich strecken,", "tokens": ["Ich", "will", "in", "die", "Mat\u00b7te", "mich", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dr\u00f6hnt ein Schu\u00df oder flackert ein Brand,", "tokens": ["Dr\u00f6hnt", "ein", "Schu\u00df", "o\u00b7der", "fla\u00b7ckert", "ein", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Dann zieh' an der Schnur, mich zu wecken.\u201c \u2014", "tokens": ["Dann", "zieh'", "an", "der", "Schnur", ",", "mich", "zu", "we\u00b7cken", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\u201esch\u00f6ner Platz, an der Lucke hier,", "tokens": ["\u201e", "sch\u00f6\u00b7ner", "Platz", ",", "an", "der", "Lu\u00b7cke", "hier", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "F\u00fcr einen unschuld'gen Privaten!", "tokens": ["F\u00fcr", "ei\u00b7nen", "un\u00b7schuld'\u00b7gen", "Pri\u00b7va\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Drunten die See, das w\u00fcste Gethier,", "tokens": ["Drun\u00b7ten", "die", "See", ",", "das", "w\u00fcs\u00b7te", "Ge\u00b7thier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das Haye speit und Piraten.", "tokens": ["Das", "Ha\u00b7ye", "speit", "und", "Pi\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Von der Seeschlang' w\u00fcthigem Kampf", "tokens": ["Von", "der", "See\u00b7schlang'", "w\u00fct\u00b7hi\u00b7gem", "Kampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Auch hat man Neues vernommen,", "tokens": ["Auch", "hat", "man", "Neu\u00b7es", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wei\u00df der Himmel, ob nicht per Dampf", "tokens": ["Wei\u00df", "der", "Him\u00b7mel", ",", "ob", "nicht", "per", "Dampf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PTKNEG", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "In's deutsche Meer sie gekommen?\u201c", "tokens": ["In's", "deut\u00b7sche", "Meer", "sie", "ge\u00b7kom\u00b7men", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201eist's doch jetzt eine Wunderzeit,", "tokens": ["\u201e", "ist's", "doch", "jetzt", "ei\u00b7ne", "Wun\u00b7der\u00b7zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Gletscher brennen wie Essen,", "tokens": ["Wo", "Glet\u00b7scher", "bren\u00b7nen", "wie", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVINF", "KOKOM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weiber turnieren im M\u00e4nnerkleid,", "tokens": ["Wei\u00b7ber", "tur\u00b7nie\u00b7ren", "im", "M\u00e4n\u00b7ner\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und Knaben die Ruthe vergessen.", "tokens": ["Und", "Kna\u00b7ben", "die", "Ru\u00b7the", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Jeder Wurm entfaltet sein Licht,", "tokens": ["Je\u00b7der", "Wurm", "ent\u00b7fal\u00b7tet", "sein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und jeder Narr seine Kappe,", "tokens": ["Und", "je\u00b7der", "Narr", "sei\u00b7ne", "Kap\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Also, Seele, wundre dich nicht,", "tokens": ["Al\u00b7so", ",", "See\u00b7le", ",", "wund\u00b7re", "dich", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wenn heute du stehst an der Klappe.\u201c", "tokens": ["Wenn", "heu\u00b7te", "du", "stehst", "an", "der", "Klap\u00b7pe", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201evetter! ein Segel, ein Segel f\u00fcrwahr,", "tokens": ["\u201e", "vet\u00b7ter", "!", "ein", "Se\u00b7gel", ",", "ein", "Se\u00b7gel", "f\u00fcr\u00b7wahr", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Ein Boot mit flatternden Streifen,", "tokens": ["Ein", "Boot", "mit", "flat\u00b7tern\u00b7den", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lichterchen dann, eine schwimmende Schaar,", "tokens": ["Lich\u00b7ter\u00b7chen", "dann", ",", "ei\u00b7ne", "schwim\u00b7men\u00b7de", "Schaar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die unter den Flanken ihm schweifen!", "tokens": ["Die", "un\u00b7ter", "den", "Flan\u00b7ken", "ihm", "schwei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Schau, nun schleichen sie alle seitab,", "tokens": ["Schau", ",", "nun", "schlei\u00b7chen", "sie", "al\u00b7le", "seit\u00b7ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Nun wechseln sie h\u00fcben und dr\u00fcben \u2014\u201c", "tokens": ["Nun", "wech\u00b7seln", "sie", "h\u00fc\u00b7ben", "und", "dr\u00fc\u00b7ben", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "KON", "ADV", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "\u201e'S ist eine Fischerflotte, mein Knab',", "tokens": ["\u201e", "'", "S", "ist", "ei\u00b7ne", "Fi\u00b7scher\u00b7flot\u00b7te", ",", "mein", "Knab", "'", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "NE", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$(", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Sind nur Leute die fischen im Tr\u00fcben.\u201c \u2014", "tokens": ["Sind", "nur", "Leu\u00b7te", "die", "fi\u00b7schen", "im", "Tr\u00fc\u00b7ben", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "ADV", "NN", "ART", "ADJA", "APPRART", "NN", "$.", "$(", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "\u201ewie das Wasser kr\u00e4uselt und rennt,", "tokens": ["\u201e", "wie", "das", "Was\u00b7ser", "kr\u00e4u\u00b7selt", "und", "rennt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und wie die K\u00e4mme ihm flittern!", "tokens": ["Und", "wie", "die", "K\u00e4m\u00b7me", "ihm", "flit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vetter, ob wohl die D\u00fcne brennt?", "tokens": ["Vet\u00b7ter", ",", "ob", "wohl", "die", "D\u00fc\u00b7ne", "brennt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ich h\u00f6re das Seegras knittern.\u201c \u2014", "tokens": ["Ich", "h\u00f6\u00b7re", "das", "See\u00b7gras", "knit\u00b7tern", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u201ed\u00fcnste, mein Junge, nur Phosphorlicht,", "tokens": ["\u201e", "d\u00fcns\u00b7te", ",", "mein", "Jun\u00b7ge", ",", "nur", "Phos\u00b7phor\u00b7licht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Vermoderte Quallen und Schnecken,", "tokens": ["Ver\u00b7mo\u00b7der\u00b7te", "Qual\u00b7len", "und", "Schne\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "La\u00df sie leuchten, sie z\u00fcnden nicht,", "tokens": ["La\u00df", "sie", "leuch\u00b7ten", ",", "sie", "z\u00fcn\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Und morgen sind's gr\u00fcnliche Flecken.\u201c \u2014", "tokens": ["Und", "mor\u00b7gen", "sin\u00b7d's", "gr\u00fcn\u00b7li\u00b7che", "Fle\u00b7cken", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201edort kein R\u00e4uber? kein Feuer hier?", "tokens": ["\u201e", "dort", "kein", "R\u00e4u\u00b7ber", "?", "kein", "Feu\u00b7er", "hier", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "$.", "PIAT", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ich h\u00e4tt' es f\u00fcr Beides genommen.", "tokens": ["Ich", "h\u00e4tt'", "es", "f\u00fcr", "Bei\u00b7des", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wetter! ist doch die Welle mir", "tokens": ["Wet\u00b7ter", "!", "ist", "doch", "die", "Wel\u00b7le", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VAFIN", "ADV", "ART", "NN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Schier \u00fcber den Tubus geschwommen.", "tokens": ["Schier", "\u00fc\u00b7ber", "den", "Tu\u00b7bus", "ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Welch' ein Leben, so angerannt", "tokens": ["Welch'", "ein", "Le\u00b7ben", ",", "so", "an\u00b7ge\u00b7rannt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "ADV", "VVPP"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Auf nackter D\u00fcne zu wohnen!", "tokens": ["Auf", "nack\u00b7ter", "D\u00fc\u00b7ne", "zu", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und die schnarchenden Robben am Strand, \u2014", "tokens": ["Und", "die", "schnar\u00b7chen\u00b7den", "Rob\u00b7ben", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Man meint es seyen Kanonen!\u201c", "tokens": ["Man", "meint", "es", "se\u00b7yen", "Ka\u00b7no\u00b7nen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201eschl\u00e4ft der Alte in gutem Muth,", "tokens": ["\u201e", "schl\u00e4ft", "der", "Al\u00b7te", "in", "gu\u00b7tem", "Muth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und l\u00e4\u00dft mich allein mit dem Spucke,", "tokens": ["Und", "l\u00e4\u00dft", "mich", "al\u00b7lein", "mit", "dem", "Spu\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir ist als steige die Fluth,", "tokens": ["Und", "mir", "ist", "als", "stei\u00b7ge", "die", "Fluth", ","],