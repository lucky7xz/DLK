{"textgrid.poem.33994": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Rechtfertigung", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst ward das Gold, das edle, hart gescholten,", "tokens": ["J\u00fcngst", "ward", "das", "Gold", ",", "das", "ed\u00b7le", ",", "hart", "ge\u00b7schol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die andern Erden schm\u00e4hten es, und riefen:", "tokens": ["Die", "an\u00b7dern", "Er\u00b7den", "schm\u00e4h\u00b7ten", "es", ",", "und", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sind's, in denen Baum und Blume schliefen,", "tokens": ["Wir", "sin\u00b7d's", ",", "in", "de\u00b7nen", "Baum", "und", "Blu\u00b7me", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PRELS", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die jegliches Gesch\u00f6pf erquicken sollten;", "tokens": ["Die", "jeg\u00b7li\u00b7ches", "Ge\u00b7sch\u00f6pf", "er\u00b7qui\u00b7cken", "soll\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn dich auch alle Sonnen k\u00fcssen wollten,", "tokens": ["Wenn", "dich", "auch", "al\u00b7le", "Son\u00b7nen", "k\u00fcs\u00b7sen", "woll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die jemals um das ew'ge Centrum liefen,", "tokens": ["Die", "je\u00b7mals", "um", "das", "ew'\u00b7ge", "Cen\u00b7trum", "lie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie weckten Nichts in deines Schoo\u00dfes Tiefen,", "tokens": ["Sie", "weck\u00b7ten", "Nichts", "in", "dei\u00b7nes", "Schoo\u00b7\u00dfes", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum hast du uns auch stets f\u00fcr Nichts gegolten!", "tokens": ["Drum", "hast", "du", "uns", "auch", "stets", "f\u00fcr", "Nichts", "ge\u00b7gol\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun sprach das Gold: ich bin das l\u00e4ngst gewesen,", "tokens": ["Nun", "sprach", "das", "Gold", ":", "ich", "bin", "das", "l\u00e4ngst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "PDS", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ihr jetzt seid, und wenn euch so viel Lenze,", "tokens": ["Was", "ihr", "jetzt", "seid", ",", "und", "wenn", "euch", "so", "viel", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$,", "KON", "KOUS", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie mir, entkeimten, werdet ihr mir gleichen;", "tokens": ["Wie", "mir", ",", "ent\u00b7keim\u00b7ten", ",", "wer\u00b7det", "ihr", "mir", "glei\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "$,", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von mir sind keine Fr\u00fcchte mehr zu lesen,", "tokens": ["Von", "mir", "sind", "kei\u00b7ne", "Fr\u00fcch\u00b7te", "mehr", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil ich schon frei im eig'nen Dasein gl\u00e4nze,", "tokens": ["Weil", "ich", "schon", "frei", "im", "eig'\u00b7nen", "Da\u00b7sein", "gl\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum bl\u00fcht und duftet fort, mich zu erreichen!", "tokens": ["Drum", "bl\u00fcht", "und", "duf\u00b7tet", "fort", ",", "mich", "zu", "er\u00b7rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measu