{"textgrid.poem.42020": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Myrte, bescheidener Strauch, geh, gr\u00fc\u00df' an dem Feste die Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Myrte, bescheidener Strauch, geh, gr\u00fc\u00df' an dem Feste die Freundin", "tokens": ["Myr\u00b7te", ",", "be\u00b7schei\u00b7de\u00b7ner", "Strauch", ",", "geh", ",", "gr\u00fc\u00df'", "an", "dem", "Fes\u00b7te", "die", "Freun\u00b7din"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-++-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Freundlich von mir und sprich, wie ich es lehre, das Wort:", "tokens": ["Freund\u00b7lich", "von", "mir", "und", "sprich", ",", "wie", "ich", "es", "leh\u00b7re", ",", "das", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KON", "ADJD", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Sch\u00fcchternes Laub h\u00fcllt z\u00fcchtig mich ein, und ich bl\u00fche verborgen:", "tokens": ["Sch\u00fcch\u00b7ter\u00b7nes", "Laub", "h\u00fcllt", "z\u00fcch\u00b7tig", "mich", "ein", ",", "und", "ich", "bl\u00fc\u00b7he", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "VVPP", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber um jegliches Blatt s\u00e4uselt der geistige Duft;", "tokens": ["A\u00b7ber", "um", "jeg\u00b7li\u00b7ches", "Blatt", "s\u00e4u\u00b7selt", "der", "geis\u00b7ti\u00b7ge", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "K\u00fchlung hauch' ich umher und erquicke mit heimlicher Anmuth", "tokens": ["K\u00fch\u00b7lung", "hauch'", "ich", "um\u00b7her", "und", "er\u00b7qui\u00b7cke", "mit", "heim\u00b7li\u00b7cher", "An\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Aug' und Gef\u00fchl, und es liebt Jeder das sinnige Gr\u00fcn.", "tokens": ["Aug'", "und", "Ge\u00b7f\u00fchl", ",", "und", "es", "liebt", "Je\u00b7der", "das", "sin\u00b7ni\u00b7ge", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "PPER", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Stets ist's heiter im Schatten bei mir und still und gem\u00fcthlich:", "tokens": ["Stets", "ist's", "hei\u00b7ter", "im", "Schat\u00b7ten", "bei", "mir", "und", "still", "und", "ge\u00b7m\u00fcth\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "APPRART", "NN", "APPR", "PPER", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Nie mit bet\u00e4ubendem Duft tr\u00fcb' ich den schwindelnden Geist.", "tokens": ["Nie", "mit", "be\u00b7t\u00e4u\u00b7ben\u00b7dem", "Duft", "tr\u00fcb'", "ich", "den", "schwin\u00b7deln\u00b7den", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Gern umkr\u00e4nz' ich die zartere Lust mit dem bl\u00fchenden Zweige,", "tokens": ["Gern", "um\u00b7kr\u00e4nz'", "ich", "die", "zar\u00b7te\u00b7re", "Lust", "mit", "dem", "bl\u00fc\u00b7hen\u00b7den", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Und doch werd' ich so oft stolz und verschlossen genannt;", "tokens": ["Und", "doch", "werd'", "ich", "so", "oft", "stolz", "und", "ver\u00b7schlos\u00b7sen", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "VVPP", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Denn nicht beug' ich mich feig, wie die anderen Blumen, des Westes", "tokens": ["Denn", "nicht", "beug'", "ich", "mich", "feig", ",", "wie", "die", "an\u00b7de\u00b7ren", "Blu\u00b7men", ",", "des", "Wes\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "PRF", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Leisestem Hauch, und frei heb' ich zur Sonne das Blatt.", "tokens": ["Lei\u00b7ses\u00b7tem", "Hauch", ",", "und", "frei", "heb'", "i