{"textgrid.poem.49346": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Sch\u00f6pfer aller Dinge,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Sch\u00f6pfer aller Dinge,", "tokens": ["O", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du v\u00e4terliches Herz,", "tokens": ["Du", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Merk auf, wie hart ich ringe,", "tokens": ["Merk", "auf", ",", "wie", "hart", "ich", "rin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was f\u00fcr ein schwerer Schmerz", "tokens": ["Was", "f\u00fcr", "ein", "schwe\u00b7rer", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich Armen hat \u00fcmfangen", "tokens": ["Mich", "Ar\u00b7men", "hat", "\u00fcm\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In dieser letzten Not!", "tokens": ["In", "die\u00b7ser", "letz\u00b7ten", "Not", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo sol ich H\u00fclf' erlangen?", "tokens": ["Wo", "sol", "ich", "H\u00fcl\u00b7f'", "er\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sehr nah' ist mir der Tod.", "tokens": ["Sehr", "nah'", "ist", "mir", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich habe nun vollendet,", "tokens": ["Ich", "ha\u00b7be", "nun", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Herr, meines Lebens Lauf", "tokens": ["Herr", ",", "mei\u00b7nes", "Le\u00b7bens", "Lauf"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und mich zu dir gewendet;", "tokens": ["Und", "mich", "zu", "dir", "ge\u00b7wen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ach, nim mich gn\u00e4dig auf!", "tokens": ["Ach", ",", "nim", "mich", "gn\u00e4\u00b7dig", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Bin ich doch schon geschm\u00fccket", "tokens": ["Bin", "ich", "doch", "schon", "ge\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Mit deines Sohnes Blut", "tokens": ["Mit", "dei\u00b7nes", "Soh\u00b7nes", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und trefflich wol erquicket", "tokens": ["Und", "treff\u00b7lich", "wol", "er\u00b7quic\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Durch ihn, das h\u00f6chste Gut.", "tokens": ["Durch", "ihn", ",", "das", "h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Dein Wort hab' ich gh\u00f6ret", "tokens": ["Dein", "Wort", "hab'", "ich", "gh\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Mit rechter Herzenslust;", "tokens": ["Mit", "rech\u00b7ter", "Her\u00b7zens\u00b7lust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Was selbigs mich gelehret,", "tokens": ["Was", "sel\u00b7bigs", "mich", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ist mir noch wol bewust;", "tokens": ["Ist", "mir", "noch", "wol", "be\u00b7wust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Dr\u00fcm glaub' ich ohne Wanken,", "tokens": ["Dr\u00fcm", "glaub'", "ich", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da\u00df du mein Helfer bist,", "tokens": ["Da\u00df", "du", "mein", "Hel\u00b7fer", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Wil dir auch sterbend danken,", "tokens": ["Wil", "dir", "auch", "ster\u00b7bend", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.24": {"text": "O mein Herr Jesu Christ.", "tokens": ["O", "mein", "Herr", "Je\u00b7su", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Zu deinen treuen H\u00e4nden", "tokens": ["Zu", "dei\u00b7nen", "treu\u00b7en", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Stell' ich itz meinen Geist,", "tokens": ["Stell'", "ich", "itz", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Du wirst mir H\u00fclfe senden,", "tokens": ["Du", "wirst", "mir", "H\u00fcl\u00b7fe", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Wie du mir n\u00f6tig wei\u00dfst;", "tokens": ["Wie", "du", "mir", "n\u00f6\u00b7tig", "wei\u00dfst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Du hast zum Freudenleben,", "tokens": ["Du", "hast", "zum", "Freu\u00b7den\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Mein Gott, berufen mich,", "tokens": ["Mein", "Gott", ",", "be\u00b7ru\u00b7fen", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Du wirst es mir auch geben,", "tokens": ["Du", "wirst", "es", "mir", "auch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Das glaub' ich sicherlich.", "tokens": ["Das", "glaub'", "ich", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "In meinen letsten N\u00f6ten", "tokens": ["In", "mei\u00b7nen", "lets\u00b7ten", "N\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Hilf mir, du starker Held;", "tokens": ["Hilf", "mir", ",", "du", "star\u00b7ker", "Held", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Wenn mich der Tod wil t\u00f6ten", "tokens": ["Wenn", "mich", "der", "Tod", "wil", "t\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "In dieser schn\u00f6den Welt,", "tokens": ["In", "die\u00b7ser", "schn\u00f6\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "So rei\u00df' aus seinen Banden", "tokens": ["So", "rei\u00df'", "aus", "sei\u00b7nen", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Mich freudig hin zu dir,", "tokens": ["Mich", "freu\u00b7dig", "hin", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Da werd' ich nicht zu Schanden:", "tokens": ["Da", "werd'", "ich", "nicht", "zu", "Schan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Erf\u00fcll, Herr, mein' Begier.", "tokens": ["Er\u00b7f\u00fcll", ",", "Herr", ",", "mein'", "Be\u00b7gier", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Drauf wil ich ruhig schlafen", "tokens": ["Drauf", "wil", "ich", "ru\u00b7hig", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "In meinem K\u00e4mmerlein;", "tokens": ["In", "mei\u00b7nem", "K\u00e4m\u00b7mer\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Gott, der du mich erschaffen,", "tokens": ["Gott", ",", "der", "du", "mich", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.44": {"text": "Wirst mein Erwecker sein", "tokens": ["Wirst", "mein", "Er\u00b7we\u00b7cker", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Und mein verborgnes Leben", "tokens": ["Und", "mein", "ver\u00b7borg\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Bald machen offenbar,", "tokens": ["Bald", "ma\u00b7chen", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Da\u00df ich m\u00fcg' ewig schweben", "tokens": ["Da\u00df", "ich", "m\u00fcg'", "e\u00b7wig", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Bei deiner Engel Schar.", "tokens": ["Bei", "dei\u00b7ner", "En\u00b7gel", "Schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Sch\u00f6pfer aller Dinge,", "tokens": ["O", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du v\u00e4terliches Herz,", "tokens": ["Du", "v\u00e4\u00b7ter\u00b7li\u00b7ches", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Merk auf, wie hart ich ringe,", "tokens": ["Merk", "auf", ",", "wie", "hart", "ich", "rin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was f\u00fcr ein schwerer Schmerz", "tokens": ["Was", "f\u00fcr", "ein", "schwe\u00b7rer", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich Armen hat \u00fcmfangen", "tokens": ["Mich", "Ar\u00b7men", "hat", "\u00fcm\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In dieser letzten Not!", "tokens": ["In", "die\u00b7ser", "letz\u00b7ten", "Not", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo sol ich H\u00fclf' erlangen?", "tokens": ["Wo", "sol", "ich", "H\u00fcl\u00b7f'", "er\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sehr nah' ist mir der Tod.", "tokens": ["Sehr", "nah'", "ist", "mir", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich habe nun vollendet,", "tokens": ["Ich", "ha\u00b7be", "nun", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Herr, meines Lebens Lauf", "tokens": ["Herr", ",", "mei\u00b7nes", "Le\u00b7bens", "Lauf"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und mich zu dir gewendet;", "tokens": ["Und", "mich", "zu", "dir", "ge\u00b7wen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ach, nim mich gn\u00e4dig auf!", "tokens": ["Ach", ",", "nim", "mich", "gn\u00e4\u00b7dig", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Bin ich doch schon geschm\u00fccket", "tokens": ["Bin", "ich", "doch", "schon", "ge\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Mit deines Sohnes Blut", "tokens": ["Mit", "dei\u00b7nes", "Soh\u00b7nes", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und trefflich wol erquicket", "tokens": ["Und", "treff\u00b7lich", "wol", "er\u00b7quic\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Durch ihn, das h\u00f6chste Gut.", "tokens": ["Durch", "ihn", ",", "das", "h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Dein Wort hab' ich gh\u00f6ret", "tokens": ["Dein", "Wort", "hab'", "ich", "gh\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Mit rechter Herzenslust;", "tokens": ["Mit", "rech\u00b7ter", "Her\u00b7zens\u00b7lust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Was selbigs mich gelehret,", "tokens": ["Was", "sel\u00b7bigs", "mich", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ist mir noch wol bewust;", "tokens": ["Ist", "mir", "noch", "wol", "be\u00b7wust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Dr\u00fcm glaub' ich ohne Wanken,", "tokens": ["Dr\u00fcm", "glaub'", "ich", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da\u00df du mein Helfer bist,", "tokens": ["Da\u00df", "du", "mein", "Hel\u00b7fer", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Wil dir auch sterbend danken,", "tokens": ["Wil", "dir", "auch", "ster\u00b7bend", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.24": {"text": "O mein Herr Jesu Christ.", "tokens": ["O", "mein", "Herr", "Je\u00b7su", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Zu deinen treuen H\u00e4nden", "tokens": ["Zu", "dei\u00b7nen", "treu\u00b7en", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Stell' ich itz meinen Geist,", "tokens": ["Stell'", "ich", "itz", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Du wirst mir H\u00fclfe senden,", "tokens": ["Du", "wirst", "mir", "H\u00fcl\u00b7fe", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Wie du mir n\u00f6tig wei\u00dfst;", "tokens": ["Wie", "du", "mir", "n\u00f6\u00b7tig", "wei\u00dfst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Du hast zum Freudenleben,", "tokens": ["Du", "hast", "zum", "Freu\u00b7den\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Mein Gott, berufen mich,", "tokens": ["Mein", "Gott", ",", "be\u00b7ru\u00b7fen", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Du wirst es mir auch geben,", "tokens": ["Du", "wirst", "es", "mir", "auch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Das glaub' ich sicherlich.", "tokens": ["Das", "glaub'", "ich", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "In meinen letsten N\u00f6ten", "tokens": ["In", "mei\u00b7nen", "lets\u00b7ten", "N\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Hilf mir, du starker Held;", "tokens": ["Hilf", "mir", ",", "du", "star\u00b7ker", "Held", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Wenn mich der Tod wil t\u00f6ten", "tokens": ["Wenn", "mich", "der", "Tod", "wil", "t\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "In dieser schn\u00f6den Welt,", "tokens": ["In", "die\u00b7ser", "schn\u00f6\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "So rei\u00df' aus seinen Banden", "tokens": ["So", "rei\u00df'", "aus", "sei\u00b7nen", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Mich freudig hin zu dir,", "tokens": ["Mich", "freu\u00b7dig", "hin", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Da werd' ich nicht zu Schanden:", "tokens": ["Da", "werd'", "ich", "nicht", "zu", "Schan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Erf\u00fcll, Herr, mein' Begier.", "tokens": ["Er\u00b7f\u00fcll", ",", "Herr", ",", "mein'", "Be\u00b7gier", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Drauf wil ich ruhig schlafen", "tokens": ["Drauf", "wil", "ich", "ru\u00b7hig", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "In meinem K\u00e4mmerlein;", "tokens": ["In", "mei\u00b7nem", "K\u00e4m\u00b7mer\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Gott, der du mich erschaffen,", "tokens": ["Gott", ",", "der", "du", "mich", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.44": {"text": "Wirst mein Erwecker sein", "tokens": ["Wirst", "mein", "Er\u00b7we\u00b7cker", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Und mein verborgnes Leben", "tokens": ["Und", "mein", "ver\u00b7borg\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Bald machen offenbar,", "tokens": ["Bald", "ma\u00b7chen", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Da\u00df ich m\u00fcg' ewig schweben", "tokens": ["Da\u00df", "ich", "m\u00fcg'", "e\u00b7wig", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Bei deiner Engel Schar.", "tokens": ["Bei", "dei\u00b7ner", "En\u00b7gel", "Schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}