{"dta.poem.23578": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Champagnerlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist f\u00fcr mich die beste Schlacht,", "tokens": ["Das", "ist", "f\u00fcr", "mich", "die", "bes\u00b7te", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn die Flaschen knallen,", "tokens": ["Wenn", "die", "Fla\u00b7schen", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn die St\u00f6psel fallen,", "tokens": ["Wenn", "die", "St\u00f6p\u00b7sel", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn es schnell wie Blut sich ergie\u00dfet,", "tokens": ["Wenn", "es", "schnell", "wie", "Blut", "sich", "er\u00b7gie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KOKOM", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn es hell wie Feuer flie\u00dfet,", "tokens": ["Wenn", "es", "hell", "wie", "Feu\u00b7er", "flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sich alle H\u00e4nd' erheben,", "tokens": ["Wenn", "sich", "al\u00b7le", "H\u00e4nd'", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jedem Herz und Hand zu geben \u2014", "tokens": ["Je\u00b7dem", "Herz", "und", "Hand", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sto\u00dfet an! sto\u00dfet an!", "tokens": ["Sto\u00b7\u00dfet", "an", "!", "sto\u00b7\u00dfet", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Das ist die sch\u00f6nste Schlacht der Welt,", "tokens": ["Das", "ist", "die", "sch\u00f6ns\u00b7te", "Schlacht", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die einzige die mir gef\u00e4llt!", "tokens": ["Die", "ein\u00b7zi\u00b7ge", "die", "mir", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRELS", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Unsre Feinde, ha, wir kennen sie nicht;", "tokens": ["Uns\u00b7re", "Fein\u00b7de", ",", "ha", ",", "wir", "ken\u00b7nen", "sie", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Gott geb' ihnen hier mehr W\u00e4rm' und Licht,", "tokens": ["Gott", "geb'", "ih\u00b7nen", "hier", "mehr", "W\u00e4rm'", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Da\u00df es ihnen dort nicht dran gebricht.", "tokens": ["Da\u00df", "es", "ih\u00b7nen", "dort", "nicht", "dran", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Sto\u00dfet an! sto\u00dfet an!", "tokens": ["Sto\u00b7\u00dfet", "an", "!", "sto\u00b7\u00dfet", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Und wird von uns ja einer erschlagen,", "tokens": ["Und", "wird", "von", "uns", "ja", "ei\u00b7ner", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Wir wollen ihn zu Grabe tragen", "tokens": ["Wir", "wol\u00b7len", "ihn", "zu", "Gra\u00b7be", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Mit einem Labetrank,", "tokens": ["Mit", "ei\u00b7nem", "La\u00b7be\u00b7trank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Mit einem Habedank:", "tokens": ["Mit", "ei\u00b7nem", "Ha\u00b7be\u00b7dank", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Er hat es satt!", "tokens": ["Er", "hat", "es", "satt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Und w\u00fcnschen, da\u00df der m\u00fcde Streiter", "tokens": ["Und", "w\u00fcn\u00b7schen", ",", "da\u00df", "der", "m\u00fc\u00b7de", "Strei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Erwache morgen frisch und heiter.", "tokens": ["Er\u00b7wa\u00b7che", "mor\u00b7gen", "frisch", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wir aber k\u00e4mpfen immer weiter,", "tokens": ["Wir", "a\u00b7ber", "k\u00e4mp\u00b7fen", "im\u00b7mer", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Bis die letzten Flaschen knallen,", "tokens": ["Bis", "die", "letz\u00b7ten", "Fla\u00b7schen", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Bis die letzten St\u00f6psel fallen. \u2014", "tokens": ["Bis", "die", "letz\u00b7ten", "St\u00f6p\u00b7sel", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}