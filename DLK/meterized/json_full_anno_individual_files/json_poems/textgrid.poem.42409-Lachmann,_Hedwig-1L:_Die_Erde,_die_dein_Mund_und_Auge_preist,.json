{"textgrid.poem.42409": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Erde, die dein Mund und Auge preist,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde, die dein Mund und Auge preist,", "tokens": ["Die", "Er\u00b7de", ",", "die", "dein", "Mund", "und", "Au\u00b7ge", "preist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hallt kr\u00e4ftig unter deinen Wanderschritten;", "tokens": ["Hallt", "kr\u00e4f\u00b7tig", "un\u00b7ter", "dei\u00b7nen", "Wan\u00b7der\u00b7schrit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf freier H\u00f6he stehst du nun inmitten", "tokens": ["Auf", "frei\u00b7er", "H\u00f6\u00b7he", "stehst", "du", "nun", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der weiten Landschaft, die das Leben heisst.", "tokens": ["Der", "wei\u00b7ten", "Land\u00b7schaft", ",", "die", "das", "Le\u00b7ben", "heisst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Eng angeschlossen an den Himmelssaum,", "tokens": ["Eng", "an\u00b7ge\u00b7schlos\u00b7sen", "an", "den", "Him\u00b7mels\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So dass er es zerteilt mit seiner Schneide,", "tokens": ["So", "dass", "er", "es", "zer\u00b7teilt", "mit", "sei\u00b7ner", "Schnei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und immer wieder hinter jener Scheide", "tokens": ["Und", "im\u00b7mer", "wie\u00b7der", "hin\u00b7ter", "je\u00b7ner", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "W\u00e4lzt sich das Meer im uferlosen Raum.", "tokens": ["W\u00e4lzt", "sich", "das", "Meer", "im", "u\u00b7fer\u00b7lo\u00b7sen", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weltgl\u00e4ubig fromm, von Andacht \u00fcbermannt", "tokens": ["Welt\u00b7gl\u00e4u\u00b7big", "fromm", ",", "von", "An\u00b7dacht", "\u00fc\u00b7berm\u00b7annt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ganz von deinem Gotte voll und trunken", "tokens": ["Und", "ganz", "von", "dei\u00b7nem", "Got\u00b7te", "voll", "und", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bist du am Ufer in die Knie gesunken,", "tokens": ["Bist", "du", "am", "U\u00b7fer", "in", "die", "Knie", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein seliger, verz\u00fcckter Korybant.", "tokens": ["Ein", "se\u00b7li\u00b7ger", ",", "ver\u00b7z\u00fcck\u00b7ter", "Ko\u00b7ry\u00b7bant", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Im Sturm frohlockend liessest du dich schwank", "tokens": ["Im", "Sturm", "froh\u00b7lo\u00b7ckend", "lies\u00b7sest", "du", "dich", "schwank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und st\u00e4hlern von den Elementen tragen;", "tokens": ["Und", "st\u00e4h\u00b7lern", "von", "den", "E\u00b7le\u00b7men\u00b7ten", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und noch den Wettern, die zerst\u00f6rend schlagen,", "tokens": ["Und", "noch", "den", "Wet\u00b7tern", ",", "die", "zer\u00b7st\u00f6\u00b7rend", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gabst du dich preis und nahmst sie hin mit Dank.", "tokens": ["Gabst", "du", "dich", "preis", "und", "nahmst", "sie", "hin", "mit", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Versenkt ins Rauschen deines Blutes, tief", "tokens": ["Ver\u00b7senkt", "ins", "Rau\u00b7schen", "dei\u00b7nes", "Blu\u00b7tes", ",", "tief"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tiefer auf den Wesensgrund zu sch\u00fcrfen,", "tokens": ["Und", "tie\u00b7fer", "auf", "den", "We\u00b7sens\u00b7grund", "zu", "sch\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernahmst du, wie im innigsten Bed\u00fcrfen", "tokens": ["Ver\u00b7nahmst", "du", ",", "wie", "im", "in\u00b7nigs\u00b7ten", "Be\u00b7d\u00fcr\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der eignen Brust die Menschennot dich rief.", "tokens": ["Der", "eig\u00b7nen", "Brust", "die", "Men\u00b7schen\u00b7not", "dich", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Welle staut sich, wo am Rand der Bucht", "tokens": ["Die", "Wel\u00b7le", "staut", "sich", ",", "wo", "am", "Rand", "der", "Bucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unendlichkeit und Leben sich ber\u00fchren", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", "und", "Le\u00b7ben", "sich", "be\u00b7r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihren ungemessnen Raub entf\u00fchren", "tokens": ["Und", "ih\u00b7ren", "un\u00b7ge\u00b7mess\u00b7nen", "Raub", "ent\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die schwarzen Fluten stumm in j\u00e4her Flucht.", "tokens": ["Die", "schwar\u00b7zen", "Flu\u00b7ten", "stumm", "in", "j\u00e4\u00b7her", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Jahre zwingen dich in ihr Gesetz", "tokens": ["Die", "Jah\u00b7re", "zwin\u00b7gen", "dich", "in", "ihr", "Ge\u00b7setz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und furchen dir die Stirne im Entweichen;", "tokens": ["Und", "fur\u00b7chen", "dir", "die", "Stir\u00b7ne", "im", "Ent\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch l\u00f6st mit einer Schwungkraft sondergleichen", "tokens": ["Doch", "l\u00f6st", "mit", "ei\u00b7ner", "Schwung\u00b7kraft", "son\u00b7derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Seele sich aus ihrem dunklen Netz.", "tokens": ["Die", "See\u00b7le", "sich", "aus", "ih\u00b7rem", "dunk\u00b7len", "Netz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Getrost und unvertraut mit allem Sein", "tokens": ["Ge\u00b7trost", "und", "un\u00b7ver\u00b7traut", "mit", "al\u00b7lem", "Sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "PIS", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4sst sich der Dichter in die Zukunft gleiten;", "tokens": ["L\u00e4sst", "sich", "der", "Dich\u00b7ter", "in", "die", "Zu\u00b7kunft", "glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Erden schon enthoben in die Weiten", "tokens": ["Auf", "Er\u00b7den", "schon", "ent\u00b7ho\u00b7ben", "in", "die", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "W\u00e4chst er in Zeit und Ewigkeit hinein.", "tokens": ["W\u00e4chst", "er", "in", "Zeit", "und", "E\u00b7wig\u00b7keit", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "i