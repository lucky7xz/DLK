{"textgrid.poem.64784": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "6. An Hrn. Joh. El. Schlegel, Prof. in Soroe", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, nur ein Geist, regiert von edlen Trieben,", "tokens": ["Freund", ",", "nur", "ein", "Geist", ",", "re\u00b7giert", "von", "ed\u00b7len", "Trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kennt und verdient das Gl\u00fcck vergn\u00fcgt zu lieben.", "tokens": ["Kennt", "und", "ver\u00b7dient", "das", "Gl\u00fcck", "ver\u00b7gn\u00fcgt", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das f\u00fchlt kein Sinn, von wilder Lust entbrannt,", "tokens": ["Das", "f\u00fchlt", "kein", "Sinn", ",", "von", "wil\u00b7der", "Lust", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht, dessen Herz um Gold zu Kaufe stand,", "tokens": ["Nicht", ",", "des\u00b7sen", "Herz", "um", "Gold", "zu", "Kau\u00b7fe", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELAT", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht, der die Treu aus Tr\u00e4gheit nie gebrochen,", "tokens": ["Nicht", ",", "der", "die", "Treu", "aus", "Tr\u00e4g\u00b7heit", "nie", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Frau nur nahm zum Waschen und zum Kochen:", "tokens": ["Die", "Frau", "nur", "nahm", "zum", "Wa\u00b7schen", "und", "zum", "Ko\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur der genau den Werth des Reizes sch\u00e4tzt,", "tokens": ["Nur", "der", "ge\u00b7nau", "den", "Werth", "des", "Rei\u00b7zes", "sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und z\u00e4rtlich f\u00fchlt, wenn ihn der Reiz erg\u00f6tzt,", "tokens": ["Und", "z\u00e4rt\u00b7lich", "f\u00fchlt", ",", "wenn", "ihn", "der", "Reiz", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wei\u00df, welche Lust ein menschlich Herz entz\u00fccket,", "tokens": ["Wei\u00df", ",", "wel\u00b7che", "Lust", "ein", "menschlich", "Herz", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "me