{"dta.poem.4424": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V.  Das Gef\u00fcl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hiemit stellen wir dem Denken", "tokens": ["Hie\u00b7mit", "stel\u00b7len", "wir", "dem", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf das Schmecken auch ein Ziel,", "tokens": ["Auf", "das", "Schme\u00b7cken", "auch", "ein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns\u2019re Geister hinzulenken", "tokens": ["Un\u00b7s'\u00b7re", "Geis\u00b7ter", "hin\u00b7zu\u00b7len\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs empfindliche Gef\u00fcl,", "tokens": ["Aufs", "emp\u00b7find\u00b7li\u00b7che", "Ge\u00b7f\u00fcl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen Kr\u00e4fte den Gedanken", "tokens": ["Des\u00b7sen", "Kr\u00e4f\u00b7te", "den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne Mass\u2019 und ohne Schranken", "tokens": ["Oh\u00b7ne", "Mass'", "und", "oh\u00b7ne", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Allenthalben, allgemein,", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", ",", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und im ganzen C\u00f6rper seyn.", "tokens": ["Und", "im", "gan\u00b7zen", "C\u00f6r\u00b7per", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}