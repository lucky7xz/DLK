{"dta.poem.21798": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Liebe/ die K\u00f6niginn der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Kind/ das G\u00f6tt- und V\u00e4ter zwinget/", "tokens": ["Kind", "/", "das", "G\u00f6t\u00b7t", "und", "V\u00e4\u00b7ter", "zwin\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "TRUNC", "KON", "NN", "VVFIN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Kind/ de\u00df hoher Zepter dringet", "tokens": ["Kind", "/", "de\u00df", "ho\u00b7her", "Zep\u00b7ter", "drin\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "durch die Macht der ganzen Welt/", "tokens": ["durch", "die", "Macht", "der", "gan\u00b7zen", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr der Erden/ Zwang der Sterne/", "tokens": ["Herr", "der", "Er\u00b7den", "/", "Zwang", "der", "Ster\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herrscher \u00fcber Nah und ferne/", "tokens": ["Herr\u00b7scher", "\u00fc\u00b7ber", "Nah", "und", "fer\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "dehm/ was lebt/ zu Fusse f\u00e4llt.", "tokens": ["dehm", "/", "was", "lebt", "/", "zu", "Fus\u00b7se", "f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PWS", "VVFIN", "$(", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Amor/ weil ich leb\u2019 in L\u00fcfften/", "tokens": ["A\u00b7mor", "/", "weil", "ich", "leb'", "in", "L\u00fcff\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "werd\u2019 ich deinen hohen Prei\u00df", "tokens": ["werd'", "ich", "dei\u00b7nen", "ho\u00b7hen", "Prei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "des Tierh\u00fcters hinzutragen", "tokens": ["des", "Tier\u00b7h\u00fc\u00b7ters", "hin\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Keinen Lorbeer werd\u2019 ich finden/", "tokens": ["Kei\u00b7nen", "Lor\u00b7beer", "werd'", "ich", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "den ich dir nicht umzubinden", "tokens": ["den", "ich", "dir", "nicht", "um\u00b7zu\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "PTKNEG", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "b\u00fckkend werde sein bedacht.", "tokens": ["b\u00fck\u00b7kend", "wer\u00b7de", "sein", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hundert-tausend Keyser Krohnen", "tokens": ["Hun\u00b7der\u00b7ttau\u00b7send", "Key\u00b7ser", "Kroh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O/ wie wol wird der begn\u00fcget/", "tokens": ["O", "/", "wie", "wol", "wird", "der", "be\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOKOM", "ADV", "VAFIN", "ART", "VVFIN", "$("], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "der f\u00fcr dir auff Knien lieget", "tokens": ["der", "f\u00fcr", "dir", "auff", "Kni\u00b7en", "lie\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und dich eyffrig betet an!", "tokens": ["und", "dich", "eyf\u00b7frig", "be\u00b7tet", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ist Gedult nur bey dem Schreyen:", "tokens": ["Ist", "Ge\u00b7dult", "nur", "bey", "dem", "Schre\u00b7yen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ihme werden kund getahn.", "tokens": ["ih\u00b7me", "wer\u00b7den", "kund", "ge\u00b7tahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df sich nu mein Leiden endet/", "tokens": ["Da\u00df", "sich", "nu", "mein", "Lei\u00b7den", "en\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df sich Freude zu mir wendet/", "tokens": ["da\u00df", "sich", "Freu\u00b7de", "zu", "mir", "wen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df mein Liebchen freundlich sicht:", "tokens": ["da\u00df", "mein", "Lieb\u00b7chen", "freund\u00b7lich", "sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df die zarten Purpur-wangen", "tokens": ["da\u00df", "die", "zar\u00b7ten", "Pur\u00b7pur\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "an den meinen lieblich hangen:", "tokens": ["an", "den", "mei\u00b7nen", "lieb\u00b7lich", "han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ist das deine Gnade nicht?", "tokens": ["ist", "das", "dei\u00b7ne", "Gna\u00b7de", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja. Eh\u2019 ich dich/ Allguht/ ehrte/", "tokens": ["Ja", ".", "Eh'", "ich", "dich", "/", "All\u00b7guht", "/", "ehr\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "PPER", "PRF", "$(", "NN", "$(", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O! wie mancher Seuffzer st\u00f6rte", "tokens": ["O", "!", "wie", "man\u00b7cher", "Seuff\u00b7zer", "st\u00f6r\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "meiner N\u00e4chte sanffte Ruh\u2019", "tokens": ["mei\u00b7ner", "N\u00e4ch\u00b7te", "sanff\u00b7te", "Ruh'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach/ mit was f\u00fcr herber Klage/", "tokens": ["Ach", "/", "mit", "was", "f\u00fcr", "her\u00b7ber", "Kla\u00b7ge", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "APPR", "PRELS", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "bracht\u2019 ich meine Fr\u00fchlings-Tage", "tokens": ["bracht'", "ich", "mei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nu beginnt mein Gl\u00fckk zu bl\u00fchen", "tokens": ["Nu", "be\u00b7ginnt", "mein", "Gl\u00fckk", "zu", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und der Winter weg zu ziehen/", "tokens": ["und", "der", "Win\u00b7ter", "weg", "zu", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der mein Leben machte grau.", "tokens": ["der", "mein", "Le\u00b7ben", "mach\u00b7te", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nu besprengt bey hellem Wetter", "tokens": ["Nu", "be\u00b7sprengt", "bey", "hel\u00b7lem", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "meines Lebens gr\u00fcne Bl\u00e4tter", "tokens": ["mei\u00b7nes", "Le\u00b7bens", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Rosillen Lippen-tau.", "tokens": ["Der", "Ro\u00b7sil\u00b7len", "Lip\u00b7pen\u00b7tau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Das/ was mich vorhin betr\u00fcbte/", "tokens": ["Das", "/", "was", "mich", "vor\u00b7hin", "be\u00b7tr\u00fcb\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was ich sonder Nuzzen liebte", "tokens": ["was", "ich", "son\u00b7der", "Nuz\u00b7zen", "lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "bringestu mir redlich ein.", "tokens": ["brin\u00b7ge\u00b7stu", "mir", "red\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer nu dich wil grausam nennen/", "tokens": ["Wer", "nu", "dich", "wil", "grau\u00b7sam", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "mu\u00df ganz keine G\u00fcte kennen", "tokens": ["mu\u00df", "ganz", "kei\u00b7ne", "G\u00fc\u00b7te", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und ohn all\u2019 Erk\u00e4ntn\u00fcs sein.", "tokens": ["und", "ohn", "all'", "Er\u00b7k\u00e4nt\u00b7n\u00fcs", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Du bists/ der du mir das Leben/", "tokens": ["Du", "bists", "/", "der", "du", "mir", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "PRELS", "PPER", "PPER", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und des Lebens Lust gegeben/", "tokens": ["und", "des", "Le\u00b7bens", "Lust", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ohne dich stirbt alle Freud\u2019", "tokens": ["oh\u00b7ne", "dich", "stirbt", "al\u00b7le", "Freud'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "alle Wollust wird zu Schmerzen", "tokens": ["al\u00b7le", "Wol\u00b7lust", "wird", "zu", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "gibstu nicht dem kranken Herzen", "tokens": ["gibs\u00b7tu", "nicht", "dem", "kran\u00b7ken", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Labsal und Ergezligkeit.", "tokens": ["Lab\u00b7sal", "und", "Er\u00b7gez\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Darum/ wer sich in dem Lieben", "tokens": ["Da\u00b7rum", "/", "wer", "sich", "in", "dem", "Lie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$(", "PWS", "PRF", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unbetr\u00fcbt gedenkt zu \u00fcben/", "tokens": ["un\u00b7be\u00b7tr\u00fcbt", "ge\u00b7denkt", "zu", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ehre deiner Hoheit Pracht.", "tokens": ["eh\u00b7re", "dei\u00b7ner", "Ho\u00b7heit", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich/ so lang\u2019 ich werde bleiben/", "tokens": ["Ich", "/", "so", "lang'", "ich", "wer\u00b7de", "blei\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ADV", "ADV", "PPER", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wil von deiner G\u00fcte schreiben", "tokens": ["wil", "von", "dei\u00b7ner", "G\u00fc\u00b7te", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und erheben deine Macht.", "tokens": ["und", "er\u00b7he\u00b7ben", "dei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}