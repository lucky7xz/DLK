{"dta.poem.8365": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n th\u00f6rigte Religion der eingebil-  \n deten Klugen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die da freie Geister sind, wollen sich mit\nMacht befleissen,", "tokens": ["Die", "da", "frei\u00b7e", "Geis\u00b7ter", "sind", ",", "wol\u00b7len", "sich", "mit", "Macht", "be\u00b7fleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "$,", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Durch den albern Aberwiz klug, scharf-", "tokens": ["Durch", "den", "al\u00b7bern", "A\u00b7ber\u00b7wiz", "klug", ",", "scha\u00b7rf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Darum sind auch jezt bekandt die nun die Religi-", "tokens": ["Da\u00b7rum", "sind", "auch", "jezt", "be\u00b7kandt", "die", "nun", "die", "Re\u00b7li\u00b7gi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "ADJD", "ART", "ADV", "ART", "TRUNC"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Durch die Klugheit ganz verstelln und ihr bringen", "tokens": ["Durch", "die", "Klug\u00b7heit", "ganz", "ver\u00b7stelln", "und", "ihr", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "KON", "PPER", "VVINF"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Diese baun des Glaubensgrund auf so leicht und", "tokens": ["Die\u00b7se", "baun", "des", "Glau\u00b7bens\u00b7grund", "auf", "so", "leicht", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJD", "KON"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Da\u00df sie ihn vielmehr zerst\u00f6hrn, wenn sie scheinen ihn", "tokens": ["Da\u00df", "sie", "ihn", "viel\u00b7mehr", "zer\u00b7st\u00f6hrn", ",", "wenn", "sie", "schei\u00b7nen", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "PPER"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Eine wahre Gottesfurcht wird von ihnen nicht er-", "tokens": ["Ei\u00b7ne", "wah\u00b7re", "Got\u00b7tes\u00b7furcht", "wird", "von", "ih\u00b7nen", "nicht", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "PTKNEG", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Sondern unter heilgen Schein gar aus aller Welt", "tokens": ["Son\u00b7dern", "un\u00b7ter", "heil\u00b7gen", "Schein", "gar", "aus", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Was dem Fleische nicht gef\u00e4llt und die wilde Nei-", "tokens": ["Was", "dem", "Flei\u00b7sche", "nicht", "ge\u00b7f\u00e4llt", "und", "die", "wil\u00b7de", "Nei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVPP", "KON", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Heisset heilge Einfalt nur die der Aberglaube tr\u00e4u-", "tokens": ["Heis\u00b7set", "heil\u00b7ge", "Ein\u00b7falt", "nur", "die", "der", "A\u00b7berg\u00b7lau\u00b7be", "tr\u00e4u"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ADV", "ART", "ART", "NN", "TRUNC"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Diese Klugen glauben nichts, nehmen nichts als", "tokens": ["Die\u00b7se", "Klu\u00b7gen", "glau\u00b7ben", "nichts", ",", "neh\u00b7men", "nichts", "als"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PIS", "$,", "VVFIN", "PIS", "KOKOM"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Als was ihnen wollgef\u00e4lt, und das jeder glauben", "tokens": ["Als", "was", "ih\u00b7nen", "woll\u00b7ge\u00b7f\u00e4lt", ",", "und", "das", "je\u00b7der", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PWS", "PPER", "VMFIN", "$,", "KON", "ART", "PIS", "VVINF"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Der nur die Religion als ein Sp\u00f6tter will ver-", "tokens": ["Der", "nur", "die", "Re\u00b7li\u00b7gi\u00b7on", "als", "ein", "Sp\u00f6t\u00b7ter", "will", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der den Nahmen nur beh\u00e4lt, um sich nicht zum", "tokens": ["Der", "den", "Nah\u00b7men", "nur", "be\u00b7h\u00e4lt", ",", "um", "sich", "nicht", "zum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,", "KOUI", "PRF", "PTKNEG", "APPRART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Dieses soll vern\u00fcnfftig seyn; und der ist ein star-", "tokens": ["Die\u00b7ses", "soll", "ver\u00b7n\u00fcnff\u00b7tig", "seyn", ";", "und", "der", "ist", "ein", "sta\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADJD", "VAINF", "$.", "KON", "ART", "VAFIN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.16": {"text": "Wer sich, wie ihr Ausdruk heist einen blinden Wahn", "tokens": ["Wer", "sich", ",", "wie", "ihr", "Aus\u00b7druk", "heist", "ei\u00b7nen", "blin\u00b7den", "Wahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Tolle Klugheit dieser Welt! die auch die Vernunft", "tokens": ["Tol\u00b7le", "Klug\u00b7heit", "die\u00b7ser", "Welt", "!", "die", "auch", "die", "Ver\u00b7nunft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PDAT", "NN", "$.", "ART", "ADV", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Wenn man die Religion zu der Bosheit Dekkel", "tokens": ["Wenn", "man", "die", "Re\u00b7li\u00b7gi\u00b7on", "zu", "der", "Bos\u00b7heit", "Dek\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer das nicht annehmen will, was der Sch\u00f6pfer", "tokens": ["Wer", "das", "nicht", "an\u00b7neh\u00b7men", "will", ",", "was", "der", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PDS", "PTKNEG", "VVINF", "VMFIN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Weil die Einfalt solches thut, der hat sich genug", "tokens": ["Weil", "die", "Ein\u00b7falt", "sol\u00b7ches", "thut", ",", "der", "hat", "sich", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIS", "VVFIN", "$,", "PRELS", "VAFIN", "PRF", "ADV"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.21": {"text": "Der wird ein verkehrter Thor um nur wizzig, klug", "tokens": ["Der", "wird", "ein", "ver\u00b7kehr\u00b7ter", "Thor", "um", "nur", "wiz\u00b7zig", ",", "klug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADV", "ADJD", "$,", "ADJD"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Der will seine Wollfahrt baun, doch die Gr\u00fcnde", "tokens": ["Der", "will", "sei\u00b7ne", "Woll\u00b7fahrt", "baun", ",", "doch", "die", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,", "ADV", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Der prahlt von Religion, die er als warhafftig,", "tokens": ["Der", "prahlt", "von", "Re\u00b7li\u00b7gi\u00b7on", ",", "die", "er", "als", "war\u00b7haff\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,", "PRELS", "PPER", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da er sich den Grund davon, ein recht wahr Erkennt-", "tokens": ["Da", "er", "sich", "den", "Grund", "da\u00b7von", ",", "ein", "recht", "wahr", "Er\u00b7kennt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "PAV", "$,", "ART", "ADJD", "ADJD", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}