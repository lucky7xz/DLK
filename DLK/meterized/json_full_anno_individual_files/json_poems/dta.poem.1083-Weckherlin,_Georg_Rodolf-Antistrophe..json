{"dta.poem.1083": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wan die mi\u00dfgunst/ das arge thier/", "tokens": ["Wan", "die", "mi\u00df\u00b7gunst", "/", "das", "ar\u00b7ge", "thier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Welches jedermans ehr verl\u00f6tzet/", "tokens": ["Wel\u00b7ches", "je\u00b7der\u00b7mans", "ehr", "ver\u00b7l\u00f6t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sein blaiches gesicht auf dich setzet/", "tokens": ["Sein", "blai\u00b7ches", "ge\u00b7sicht", "auf", "dich", "set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVPP", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd dich erfindet so voll zier/", "tokens": ["Vnd", "dich", "er\u00b7fin\u00b7det", "so", "voll", "zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df es mit zitterender sehl", "tokens": ["Mu\u00df", "es", "mit", "zit\u00b7te\u00b7ren\u00b7der", "sehl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wider seinen willen gestehen/", "tokens": ["Wi\u00b7der", "sei\u00b7nen", "wil\u00b7len", "ge\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Das es so pur von allem fehl", "tokens": ["Das", "es", "so", "pur", "von", "al\u00b7lem", "fehl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "ADJD", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Keinen andern als dich gesehen:", "tokens": ["Kei\u00b7nen", "an\u00b7dern", "als", "dich", "ge\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "KOKOM", "PPER", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Ja/ sein giftig vnd weitter rach", "tokens": ["Ja", "/", "sein", "gif\u00b7tig", "vnd", "weit\u00b7ter", "rach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "PPOSAT", "ADJD", "KON", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Wirt schier mit ges\u00e4uberter zungen/", "tokens": ["Wirt", "schier", "mit", "ge\u00b7s\u00e4u\u00b7ber\u00b7ter", "zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Vnd mit vngewohnlicher sprach", "tokens": ["Vnd", "mit", "vn\u00b7ge\u00b7wohn\u00b7li\u00b7cher", "sprach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Deinleben zu loben gezwungen.", "tokens": ["Dein\u00b7le\u00b7ben", "zu", "lo\u00b7ben", "ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}