{"textgrid.poem.33883": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie man das Heilige ber\u00fchrt:", "tokens": ["Wie", "man", "das", "Hei\u00b7li\u00b7ge", "be\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man will ihm selbst nicht geben,", "tokens": ["Man", "will", "ihm", "selbst", "nicht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist genug, da\u00df man es sp\u00fcrt,", "tokens": ["Es", "ist", "ge\u00b7nug", ",", "da\u00df", "man", "es", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So k\u00fc\u00dft' ich sie mit Beben,", "tokens": ["So", "k\u00fc\u00dft'", "ich", "sie", "mit", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und that der Mund", "tokens": ["Und", "that", "der", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nicht Alles kund,", "tokens": ["Nicht", "Al\u00b7les", "kund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So brachte sie's zu Ende", "tokens": ["So", "brach\u00b7te", "sie's", "zu", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In frommen Sinn", "tokens": ["In", "from\u00b7men", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Zum Vollgewinn", "tokens": ["Zum", "Voll\u00b7ge\u00b7winn"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Durch einen Druck der H\u00e4nde!", "tokens": ["Durch", "ei\u00b7nen", "Druck", "der", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie man das Heilige ber\u00fchrt:", "tokens": ["Wie", "man", "das", "Hei\u00b7li\u00b7ge", "be\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man will ihm selbst nicht geben,", "tokens": ["Man", "will", "ihm", "selbst", "nicht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist genug, da\u00df man es sp\u00fcrt,", "tokens": ["Es", "ist", "ge\u00b7nug", ",", "da\u00df", "man", "es", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So k\u00fc\u00dft' ich