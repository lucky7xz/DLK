{"dta.poem.10502": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' mir lang den Kopf zerbrochen", "tokens": ["Ich", "hab'", "mir", "lang", "den", "Kopf", "zer\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Denken und Sinnen, Tag und Nacht,", "tokens": ["Mit", "Den\u00b7ken", "und", "Sin\u00b7nen", ",", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch deine liebensw\u00fcrdigen Augen", "tokens": ["Doch", "dei\u00b7ne", "lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7gen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie haben mich zum Entschlu\u00df gebracht.", "tokens": ["Sie", "ha\u00b7ben", "mich", "zum", "Ent\u00b7schlu\u00df", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Jetzt bleib' ich, wo deine Augen leuchten,", "tokens": ["Jetzt", "bleib'", "ich", ",", "wo", "dei\u00b7ne", "Au\u00b7gen", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In ihrer s\u00fc\u00dfen, klugen Pracht \u2014", "tokens": ["In", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", ",", "klu\u00b7gen", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich noch einmal w\u00fcrde lieben,", "tokens": ["Da\u00df", "ich", "noch", "ein\u00b7mal", "w\u00fcr\u00b7de", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00e4tt' es nimmermehr gedacht.", "tokens": ["Ich", "h\u00e4tt'", "es", "nim\u00b7mer\u00b7mehr", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}