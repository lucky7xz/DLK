{"dta.poem.6205": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns den stoltzen Stamm verwegner Eichen,", "tokens": ["Lasst", "uns", "den", "stolt\u00b7zen", "Stamm", "ver\u00b7weg\u00b7ner", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mit den Aesten fast bis an die Wolcken reichen,", "tokens": ["Die", "mit", "den", "A\u00b7es\u00b7ten", "fast", "bis", "an", "die", "Wol\u00b7cken", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wodurch nicht einst der Sonnen Strahlen gehn,", "tokens": ["Wo\u00b7durch", "nicht", "einst", "der", "Son\u00b7nen", "Strah\u00b7len", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sammt ihren dichten Bl\u00e4ttern sehn.", "tokens": ["Sammt", "ih\u00b7ren", "dich\u00b7ten", "Bl\u00e4t\u00b7tern", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man wirfft ein\u2019Eichel in das Land;", "tokens": ["Man", "wirfft", "ein'\u00b7Ei\u00b7chel", "in", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daselbst wird sie erweicht,", "tokens": ["Da\u00b7selbst", "wird", "sie", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er\u00f6ffnet sich, und da sie feucht,", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "sich", ",", "und", "da", "sie", "feucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KON", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Entwickelt sie sich bald, und spannt", "tokens": ["Ent\u00b7wi\u00b7ckelt", "sie", "sich", "bald", ",", "und", "spannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die kleinen Wurtzeln aus; worauf der Erden Safft,", "tokens": ["Die", "klei\u00b7nen", "Wurt\u00b7zeln", "aus", ";", "wo\u00b7rauf", "der", "Er\u00b7den", "Safft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Beweget durch der Hitze Krafft,", "tokens": ["Be\u00b7we\u00b7get", "durch", "der", "Hit\u00b7ze", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In ihre L\u00f6cher dringt, gem\u00e4chlich aufwerts steiget", "tokens": ["In", "ih\u00b7re", "L\u00f6\u00b7cher", "dringt", ",", "ge\u00b7m\u00e4ch\u00b7lich", "auf\u00b7werts", "stei\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und anfangs kleine Zweiglein zeiget.", "tokens": ["Und", "an\u00b7fangs", "klei\u00b7ne", "Zwei\u00b7glein", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Man beuget einen Baum, der jung ist, mit der Hand,", "tokens": ["Man", "beu\u00b7get", "ei\u00b7nen", "Baum", ",", "der", "jung", "ist", ",", "mit", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Allein sein Stamm, der zart und schwach,", "tokens": ["Al\u00b7lein", "sein", "Stamm", ",", "der", "zart", "und", "schwach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Vermehrt sich nach und nach.", "tokens": ["Ver\u00b7mehrt", "sich", "nach", "und", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Durch seine Rahrungs-Safft, und durch den Lauff der Zeit", "tokens": ["Durch", "sei\u00b7ne", "Rah\u00b7rungs\u00b7\u00b7S\u00b7afft", ",", "und", "durch", "den", "Lauff", "der", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Stets mehr befestiget, nimmt er so festen Stand,", "tokens": ["Stets", "mehr", "be\u00b7fes\u00b7ti\u00b7get", ",", "nimmt", "er", "so", "fes\u00b7ten", "Stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.18": {"text": "Und hat zuletzt ein\u2019 Unbeweglichkeit.", "tokens": ["Und", "hat", "zu\u00b7letzt", "ein'", "Un\u00b7be\u00b7weg\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Da er nun t\u00e4glich w\u00e4chst, wiewol gantz unvermerckt;", "tokens": ["Da", "er", "nun", "t\u00e4g\u00b7lich", "w\u00e4chst", ",", "wie\u00b7wol", "gantz", "un\u00b7ver\u00b7merckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "KOUS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So wird, was anfangs nur ein schwaches Pfl\u00e4ntzgen wiese,", "tokens": ["So", "wird", ",", "was", "an\u00b7fangs", "nur", "ein", "schwa\u00b7ches", "Pfl\u00e4ntz\u00b7gen", "wie\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein ungeheurer Baum, ein rechter Riese.", "tokens": ["Ein", "un\u00b7ge\u00b7heu\u00b7rer", "Baum", ",", "ein", "rech\u00b7ter", "Rie\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}