{"textgrid.poem.35495": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Oft schweif' ich ...", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft schweif' ich durch der Menschen Reihen hin", "tokens": ["Oft", "schweif'", "ich", "durch", "der", "Men\u00b7schen", "Rei\u00b7hen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sp\u00fcre keinen ...", "tokens": ["Und", "sp\u00fc\u00b7re", "kei\u00b7nen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ahne traumumflirrt nur,", "tokens": ["Ah\u00b7ne", "trau\u00b7mum\u00b7flirrt", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wie ein gewaltig Chaos mich umbraust,", "tokens": ["Wie", "ein", "ge\u00b7wal\u00b7tig", "Chaos", "mich", "um\u00b7braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wie ein verwirrtes T\u00f6nen zu mir ruft,", "tokens": ["Wie", "ein", "ver\u00b7wirr\u00b7tes", "T\u00f6\u00b7nen", "zu", "mir", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gleich Stimmen, die von fernen Inseln kommen ...", "tokens": ["Gleich", "Stim\u00b7men", ",", "die", "von", "fer\u00b7nen", "In\u00b7seln", "kom\u00b7men", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sp\u00fcre keinen \u2013", "tokens": ["Ich", "sp\u00fc\u00b7re", "kei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und ob Freund, ob Feind", "tokens": ["Und", "ob", "Freund", ",", "ob", "Feind"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "NN", "$,", "KOUS", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "An mir vor\u00fcbertreibt:", "tokens": ["An", "mir", "vor\u00b7\u00fc\u00b7bert\u00b7reibt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich wei\u00df es nicht ...", "tokens": ["Ich", "wei\u00df", "es", "nicht", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wie Schatten hastet's hin \u2013", "tokens": ["Wie", "Schat\u00b7ten", "has\u00b7tet's", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Verh\u00e4ngt ist mein Gesicht,", "tokens": ["Ver\u00b7h\u00e4ngt", "ist", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Entr\u00fcckt mein Sinn ...", "tokens": ["Ent\u00b7r\u00fcckt", "mein", "Sinn", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dann sing' ich leise Lieder f\u00fcr mich hin,", "tokens": ["Dann", "sing'", "ich", "lei\u00b7se", "Lie\u00b7der", "f\u00fcr", "mich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die niemand sang bis heute ...", "tokens": ["Die", "nie\u00b7mand", "sang", "bis", "heu\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was dieser schweren Weisen Gang", "tokens": ["Was", "die\u00b7ser", "schwe\u00b7ren", "Wei\u00b7sen", "Gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedeute \u2013", "tokens": ["Be\u00b7deu\u00b7te", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Kaum kl\u00e4rt sich's mir ...", "tokens": ["Kaum", "kl\u00e4rt", "sich's", "mir", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.4": {"line.1": {"text": "Mit Wesen sprech' ich,", "tokens": ["Mit", "We\u00b7sen", "sprech'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die noch ungeboren \u2013", "tokens": ["Die", "noch", "un\u00b7ge\u00b7bo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich noch verloren", "tokens": ["Sich", "noch", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ADV", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In Ewigkeiten, schrankenlosen,", "tokens": ["In", "E\u00b7wig\u00b7kei\u00b7ten", ",", "schran\u00b7ken\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Unvergleichbar", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7bar"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Irdischen Losen,", "tokens": ["Ir\u00b7di\u00b7schen", "Lo\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Unerreichbar", "tokens": ["Un\u00b7er\u00b7reich\u00b7bar"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "F\u00fcr Menschenma\u00df ...", "tokens": ["F\u00fcr", "Men\u00b7schen\u00b7ma\u00df", "..."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Was ich besa\u00df \u2013", "tokens": ["Was", "ich", "be\u00b7sa\u00df", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was ich besitze,", "tokens": ["Was", "ich", "be\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mir zugeeignet", "tokens": ["Mir", "zu\u00b7ge\u00b7eig\u00b7net"], "token_info": ["word", "word"], "pos": ["PPER", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wachend bewu\u00dft:", "tokens": ["Wa\u00b7chend", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Liegt \u00fcberflutet", "tokens": ["Liegt", "\u00fc\u00b7berf\u00b7lu\u00b7tet"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wie vom Vergessen,", "tokens": ["Wie", "vom", "Ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "M\u00e4rchenversunken", "tokens": ["M\u00e4r\u00b7chen\u00b7ver\u00b7sun\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Tief in der Brust ...", "tokens": ["Tief", "in", "der", "Brust", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Aber die Stimmen, die geheimnisvollen,", "tokens": ["A\u00b7ber", "die", "Stim\u00b7men", ",", "die", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die verschollen", "tokens": ["Die", "ver\u00b7schol\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Im Wachsen und Reifen,", "tokens": ["Im", "Wach\u00b7sen", "und", "Rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "T\u00f6nen herauf,", "tokens": ["T\u00f6\u00b7nen", "her\u00b7auf", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und sie begreifen", "tokens": ["Und", "sie", "be\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nach eigener Satzung", "tokens": ["Nach", "ei\u00b7ge\u00b7ner", "Sat\u00b7zung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Eine eigene Welt ...", "tokens": ["Ei\u00b7ne", "ei\u00b7ge\u00b7ne", "Welt", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Die sich entwirkend", "tokens": ["Die", "sich", "ent\u00b7wir\u00b7kend"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Mich dem Scho\u00dfe", "tokens": ["Mich", "dem", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Alles Entstehens", "tokens": ["Al\u00b7les", "Ent\u00b7ste\u00b7hens"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ahnend gesellt ...", "tokens": ["Ah\u00b7nend", "ge\u00b7sellt", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Urworte denk' ich,", "tokens": ["Ur\u00b7wor\u00b7te", "denk'", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Und ich versenk' mich", "tokens": ["Und", "ich", "ver\u00b7senk'", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In den Strudel der Kraft,", "tokens": ["In", "den", "Stru\u00b7del", "der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die sich entfaltend", "tokens": ["Die", "sich", "ent\u00b7fal\u00b7tend"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Alles gestaltend", "tokens": ["Al\u00b7les", "ge\u00b7stal\u00b7tend"], "token_info": ["word", "word"], "pos": ["PIS", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zum Wandel schafft ...", "tokens": ["Zum", "Wan\u00b7del", "schafft", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Selbst ich entrolle", "tokens": ["Selbst", "ich", "ent\u00b7rol\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bemessener Scholle", "tokens": ["Be\u00b7mes\u00b7se\u00b7ner", "Schol\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zum Urborn des Seins,", "tokens": ["Zum", "Ur\u00b7born", "des", "Seins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und es verliert sich,", "tokens": ["Und", "es", "ver\u00b7liert", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Was nur gebiert sich,", "tokens": ["Was", "nur", "ge\u00b7biert", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PRF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Als Schatten des Scheins ...", "tokens": ["Als", "Schat\u00b7ten", "des", "Scheins", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Oft schweif' ich durch der Menschen Reihen hin", "tokens": ["Oft", "schweif'", "ich", "durch", "der", "Men\u00b7schen", "Rei\u00b7hen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sp\u00fcre keinen ...", "tokens": ["Und", "sp\u00fc\u00b7re", "kei\u00b7nen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ahne traumumflirrt nur,", "tokens": ["Ah\u00b7ne", "trau\u00b7mum\u00b7flirrt", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wie ein gewaltig Chaos mich umbraust,", "tokens": ["Wie", "ein", "ge\u00b7wal\u00b7tig", "Chaos", "mich", "um\u00b7braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN"