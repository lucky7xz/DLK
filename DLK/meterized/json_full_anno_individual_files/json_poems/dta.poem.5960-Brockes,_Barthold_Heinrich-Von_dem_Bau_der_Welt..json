{"dta.poem.5960": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von diesen Wirbeln nun wird sich die Zahl nicht fassen,", "tokens": ["Von", "die\u00b7sen", "Wir\u00b7beln", "nun", "wird", "sich", "die", "Zahl", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VAFIN", "PRF", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und von der Menschen Geist sich je begreiffen lassen.", "tokens": ["Und", "von", "der", "Men\u00b7schen", "Geist", "sich", "je", "be\u00b7greif\u00b7fen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PRF", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein, ob wir gleich in Gedancken sehn,", "tokens": ["Al\u00b7lein", ",", "ob", "wir", "gleich", "in", "Ge\u00b7dan\u00b7cken", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ihre Schrancken all\u2019 ins Unbegr\u00e4ntzte gehn;", "tokens": ["Da\u00df", "ih\u00b7re", "Schran\u00b7cken", "all'", "ins", "Un\u00b7be\u00b7gr\u00e4ntz\u00b7te", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So lasset uns, uns einzuschr\u00e4ncken, streben,", "tokens": ["So", "las\u00b7set", "uns", ",", "uns", "ein\u00b7zu\u00b7schr\u00e4n\u00b7cken", ",", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In diesem Raum, den das gestirnte Blau", "tokens": ["In", "die\u00b7sem", "Raum", ",", "den", "das", "ge\u00b7stirn\u00b7te", "Blau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als ein Gew\u00f6lb umschlossen und umgeben,", "tokens": ["Als", "ein", "Ge\u00b7w\u00f6lb", "um\u00b7schlos\u00b7sen", "und", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und, da\u00df dies unsre Welt sey, blo\u00df gedencken;", "tokens": ["Und", ",", "da\u00df", "dies", "uns\u00b7re", "Welt", "sey", ",", "blo\u00df", "ge\u00b7den\u00b7cken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PDS", "PPOSAT", "NN", "VAFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ohn in ein andre Welt uns irrend zu versencken.", "tokens": ["Ohn", "in", "ein", "and\u00b7re", "Welt", "uns", "ir\u00b7rend", "zu", "ver\u00b7sen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}