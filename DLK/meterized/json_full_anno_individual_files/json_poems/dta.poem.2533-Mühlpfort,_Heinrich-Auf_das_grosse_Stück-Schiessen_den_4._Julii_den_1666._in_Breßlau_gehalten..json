{"dta.poem.2533": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf das grosse St\u00fcck-Schiessen den 4. Julii  \n den 1666. in Bre\u00dflau gehalten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df die Gerechtigkeit und heilsame Gesetze/", "tokens": ["Da\u00df", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "und", "heil\u00b7sa\u00b7me", "Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein unverg\u00e4nglich Lob/ du sch\u00f6ne Stadt erh\u00f6hn/", "tokens": ["Dein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Lob", "/", "du", "sch\u00f6\u00b7ne", "Stadt", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$(", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Rath und That zugleich dir an der Seiten", "tokens": ["Da\u00df", "Rath", "und", "That", "zu\u00b7gleich", "dir", "an", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Damit kein gisstig Bi\u00df de\u00df Feindes dich verletze/", "tokens": ["Da\u00b7mit", "kein", "giss\u00b7tig", "Bi\u00df", "de\u00df", "Fein\u00b7des", "dich", "ver\u00b7let\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kr\u00f6hnt Bre\u00dflau deinen Ruhm/ und mehrt die grosse Ziehr", "tokens": ["Kr\u00b7\u00f6hnt", "Bre\u00df\u00b7lau", "dei\u00b7nen", "Ruhm", "/", "und", "mehrt", "die", "gros\u00b7se", "Ziehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "In welcher du vorl\u00e4ngst/ gehst andern St\u00e4dten f\u00fcr.", "tokens": ["In", "wel\u00b7cher", "du", "vor\u00b7l\u00e4ngst", "/", "gehst", "an\u00b7dern", "St\u00e4d\u00b7ten", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch ist di\u00df nicht allein. Auch mitten in dem Frieden/", "tokens": ["Doch", "ist", "di\u00df", "nicht", "al\u00b7lein", ".", "Auch", "mit\u00b7ten", "in", "dem", "Frie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "PTKNEG", "ADV", "$.", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da ungekr\u00e4nckte Ruh die g\u00fcldnen Zeiten weist/", "tokens": ["Da", "un\u00b7ge\u00b7kr\u00e4nck\u00b7te", "Ruh", "die", "g\u00fcld\u00b7nen", "Zei\u00b7ten", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schl\u00e4ft doch dein B\u00fcrger nicht/ der embsig sich befleist", "tokens": ["Schl\u00e4ft", "doch", "dein", "B\u00fcr\u00b7ger", "nicht", "/", "der", "emb\u00b7sig", "sich", "be\u00b7fleist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$(", "ART", "ADJD", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie durch der Waffen Macht/ wird aller Fall vermieden/", "tokens": ["Wie", "durch", "der", "Waf\u00b7fen", "Macht", "/", "wird", "al\u00b7ler", "Fall", "ver\u00b7mie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$(", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und als jetzt hundert Jahr gleich ihren Krei\u00df vollziehn/", "tokens": ["Und", "als", "jetzt", "hun\u00b7dert", "Jahr", "gleich", "ih\u00b7ren", "Krei\u00df", "voll\u00b7ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "CARD", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sieht man de\u00df Werders Platz an St\u00fcck und Zelten bl\u00fchn.", "tokens": ["Sieht", "man", "de\u00df", "Wer\u00b7ders", "Platz", "an", "St\u00fcck", "und", "Zel\u00b7ten", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es wolte Jupiter nechst seinen Blitz ablegen/", "tokens": ["Es", "wol\u00b7te", "Ju\u00b7pi\u00b7ter", "nechst", "sei\u00b7nen", "Blitz", "ab\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.14": {"text": "Als er den Donner-Knall auf Erden h\u00f6rte gehn/", "tokens": ["Als", "er", "den", "Don\u00b7ner\u00b7Knall", "auf", "Er\u00b7den", "h\u00f6r\u00b7te", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Mars blieb nicht wie vor auf seinen H\u00e4mus stehn.", "tokens": ["Und", "Mars", "blieb", "nicht", "wie", "vor", "auf", "sei\u00b7nen", "H\u00e4\u00b7mus", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "KOKOM", "APPR", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er dachte gleiche Lust mit solchem Volck zu h\u00e4gen/", "tokens": ["Er", "dach\u00b7te", "glei\u00b7che", "Lust", "mit", "sol\u00b7chem", "Volck", "zu", "h\u00e4\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das in dem Hertzen Feur/ Krafft in den Armen hat/", "tokens": ["Das", "in", "dem", "Hert\u00b7zen", "Feur", "/", "Krafft", "in", "den", "Ar\u00b7men", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "$(", "NN", "APPR", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.18": {"text": "Und das nicht ohn Verdienst/ erlangt ein Lorbeer-Blat.", "tokens": ["Und", "das", "nicht", "ohn", "Ver\u00b7dienst", "/", "er\u00b7langt", "ein", "Lor\u00b7beer\u00b7Blat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PTKNEG", "APPR", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Hiermit so wuchs der Muth als Kleinod und Geschencke/", "tokens": ["Hier\u00b7mit", "so", "wuchs", "der", "Muth", "als", "Klei\u00b7nod", "und", "Ge\u00b7schen\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "KOUS", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von G\u00f6ttern dieser Stadt den B\u00fcrgern au\u00dfgetheilt/", "tokens": ["Von", "G\u00f6t\u00b7tern", "die\u00b7ser", "Stadt", "den", "B\u00fcr\u00b7gern", "au\u00df\u00b7ge\u00b7theilt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein jedes Auge hat der Kugel gleich geeilt/", "tokens": ["Ein", "je\u00b7des", "Au\u00b7ge", "hat", "der", "Ku\u00b7gel", "gleich", "ge\u00b7eilt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Zu sehen ob der Schu\u00df tieff in dem Schirme hencke/", "tokens": ["Zu", "se\u00b7hen", "ob", "der", "Schu\u00df", "tieff", "in", "dem", "Schir\u00b7me", "hen\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOUS", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie weit daran ger\u00fcckt/ wie nah es m\u00fcsse seyn/", "tokens": ["Wie", "weit", "da\u00b7ran", "ge\u00b7r\u00fcckt", "/", "wie", "nah", "es", "m\u00fcs\u00b7se", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PAV", "VVPP", "$(", "PWAV", "ADJD", "PPER", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und was da eigendlich zum Vortheil leitet ein.", "tokens": ["Und", "was", "da", "ei\u00b7gend\u00b7lich", "zum", "Vor\u00b7theil", "lei\u00b7tet", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Den Uberwinder hat die Stimme der Trompeten", "tokens": ["Den", "U\u00b7ber\u00b7win\u00b7der", "hat", "die", "Stim\u00b7me", "der", "Trom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein freudiges Gl\u00fcck zu/ ein silberner Pocal", "tokens": ["Ein", "freu\u00b7di\u00b7ges", "Gl\u00fcck", "zu", "/", "ein", "sil\u00b7ber\u00b7ner", "Po\u00b7cal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "$(", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Siegprangend aufgef\u00fchrt/ und denn ein k\u00f6stlich Mahl", "tokens": ["Sieg\u00b7pran\u00b7gend", "auf\u00b7ge\u00b7f\u00fchrt", "/", "und", "denn", "ein", "k\u00f6st\u00b7lich", "Mahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$(", "KON", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Geheissen alle Noth und bittre Sorgen t\u00f6dten/", "tokens": ["Ge\u00b7heis\u00b7sen", "al\u00b7le", "Noth", "und", "bitt\u00b7re", "Sor\u00b7gen", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer lobt im Frieden nicht Bereitung zu dem Streit/", "tokens": ["Wer", "lobt", "im", "Frie\u00b7den", "nicht", "Be\u00b7rei\u00b7tung", "zu", "dem", "Streit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "PTKNEG", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und da\u00df man di\u00df erlernt/ was man braucht mit der Zeit.", "tokens": ["Und", "da\u00df", "man", "di\u00df", "er\u00b7lernt", "/", "was", "man", "braucht", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PDS", "VVPP", "$(", "PWS", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Zwar ewiges Gel\u00fcck/ und Frieden-reicher Segen", "tokens": ["Zwar", "e\u00b7wi\u00b7ges", "Ge\u00b7l\u00fcck", "/", "und", "Frie\u00b7den\u00b7rei\u00b7cher", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Schweb\u2019 umb dein bl\u00fchend Haupt/ du unbefleckte Stadt/", "tokens": ["Schweb'", "umb", "dein", "bl\u00fc\u00b7hend", "Haupt", "/", "du", "un\u00b7be\u00b7fleck\u00b7te", "Stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die nie kein Feind geschw\u00e4cht/ kein Sturm gesch\u00e4ndet hat.", "tokens": ["Die", "nie", "kein", "Feind", "ge\u00b7schw\u00e4cht", "/", "kein", "Sturm", "ge\u00b7sch\u00e4n\u00b7det", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$(", "PIAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es m\u00fcsse stetes Heil sich umb die Mauren legen/", "tokens": ["Es", "m\u00fcs\u00b7se", "ste\u00b7tes", "Heil", "sich", "umb", "die", "Mau\u00b7ren", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Oelbaum s\u00fcsser Ruh ein Paradi\u00df dir baun/", "tokens": ["Der", "O\u00b7el\u00b7baum", "s\u00fcs\u00b7ser", "Ruh", "ein", "Pa\u00b7ra\u00b7di\u00df", "dir", "baun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.36": {"text": "In dem die Einigkeit ist wesendlich zu schaun.", "tokens": ["In", "dem", "die", "Ei\u00b7nig\u00b7keit", "ist", "we\u00b7send\u00b7lich", "zu", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}