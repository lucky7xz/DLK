{"textgrid.poem.63743": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies junge braune Schelmenangesicht", "tokens": ["Dies", "jun\u00b7ge", "brau\u00b7ne", "Schel\u00b7men\u00b7an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Feuerblick und lachend wei\u00dfen Z\u00e4hnen \u2013", "tokens": ["Mit", "Feu\u00b7er\u00b7blick", "und", "la\u00b7chend", "wei\u00b7\u00dfen", "Z\u00e4h\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie reizend hexenhaft der wirren Str\u00e4hnen", "tokens": ["Wie", "rei\u00b7zend", "he\u00b7xen\u00b7haft", "der", "wir\u00b7ren", "Str\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tiefschwarzer Kranz die niedre Stirn umflicht.", "tokens": ["Tief\u00b7schwar\u00b7zer", "Kranz", "die", "nied\u00b7re", "Stirn", "um\u00b7flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie kennt nichts H\u00f6h'res, als am Sonnenlicht", "tokens": ["Sie", "kennt", "nichts", "H\u00f6h'\u00b7res", ",", "als", "am", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "NN", "$,", "KOUS", "APPRART", "NN"], "meter