{"dta.poem.20069": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Nun kann ich wieder wie die L\u00fcfte schweifen,", "tokens": ["Nun", "kann", "ich", "wie\u00b7der", "wie", "die", "L\u00fcf\u00b7te", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Strom, im Wald auf\u2019s Neue bei den alten", "tokens": ["Am", "Strom", ",", "im", "Wald", "auf's", "Neu\u00b7e", "bei", "den", "al\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "APPRART", "ADJA", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geliebten Pl\u00e4tzen Rast und Andacht halten,", "tokens": ["Ge\u00b7lieb\u00b7ten", "Pl\u00e4t\u00b7zen", "Rast", "und", "An\u00b7dacht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4chelnd nach der Abendr\u00f6the greifen.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "nach", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Markte fern, dem Feilschen und dem Keifen", "tokens": ["Dem", "Mark\u00b7te", "fern", ",", "dem", "Feil\u00b7schen", "und", "dem", "Kei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl ich der Seele Schwingen sich entfalten,", "tokens": ["F\u00fchl", "ich", "der", "See\u00b7le", "Schwin\u00b7gen", "sich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir kehrt die Kraft mein Denken zu gestalten,", "tokens": ["Mir", "kehrt", "die", "Kraft", "mein", "Den\u00b7ken", "zu", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Keim wird stark zur Frucht heranzureifen.", "tokens": ["Der", "Keim", "wird", "stark", "zur", "Frucht", "her\u00b7an\u00b7zu\u00b7rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bald werd ich neu zu Freud und Frohsinn taugen;", "tokens": ["Bald", "werd", "ich", "neu", "zu", "Freud", "und", "Froh\u00b7sinn", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon lern ich aus des Fr\u00fchlings heitren Kl\u00e4ngen,", "tokens": ["Schon", "lern", "ich", "aus", "des", "Fr\u00fch\u00b7lings", "hei\u00b7tren", "Kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie s\u00fc\u00dfen Nektar, Lust am Leben saugen;", "tokens": ["Wie", "s\u00fc\u00b7\u00dfen", "Nek\u00b7tar", ",", "Lust", "am", "Le\u00b7ben", "sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schon l\u00e4chl\u2019 ich wieder, statt den Kopf zu h\u00e4ngen,", "tokens": ["Schon", "l\u00e4chl'", "ich", "wie\u00b7der", ",", "statt", "den", "Kopf", "zu", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zwischen mich und Deine lieben Augen,", "tokens": ["Und", "zwi\u00b7schen", "mich", "und", "Dei\u00b7ne", "lie\u00b7ben", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh ich sich f\u00fcrder keine Wolke dr\u00e4ngen.", "tokens": ["Seh", "ich", "sich", "f\u00fcr\u00b7der", "kei\u00b7ne", "Wol\u00b7ke", "dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PIAT", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}}}}