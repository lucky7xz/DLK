{"textgrid.poem.49593": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sommeridylle", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Berge und T\u00e4ler sind jetzt voll von Menschen,", "tokens": ["Ber\u00b7ge", "und", "T\u00e4\u00b7ler", "sind", "jetzt", "voll", "von", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Welche sich Urlaub genommen haben", "tokens": ["Wel\u00b7che", "sich", "Ur\u00b7laub", "ge\u00b7nom\u00b7men", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "NN", "VVPP", "VAINF"], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und an der reinen Luft der Kurorte", "tokens": ["Und", "an", "der", "rei\u00b7nen", "Luft", "der", "Kur\u00b7or\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sowohl sich als ihre Angeh\u00f6rigen laben.", "tokens": ["So\u00b7wohl", "sich", "als", "ih\u00b7re", "An\u00b7ge\u00b7h\u00f6\u00b7ri\u00b7gen", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Viele h\u00f6rt man mit Neugierde fragen,", "tokens": ["Vie\u00b7le", "h\u00f6rt", "man", "mit", "Neu\u00b7gier\u00b7de", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ob hier noch echte Wilderer wachsen,", "tokens": ["Ob", "hier", "noch", "ech\u00b7te", "Wil\u00b7de\u00b7rer", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Welche die wirklichen Gemsen t\u00f6ten.", "tokens": ["Wel\u00b7che", "die", "wirk\u00b7li\u00b7chen", "Gem\u00b7sen", "t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Meistens sind diese Leute aus Sachsen.", "tokens": ["Meis\u00b7tens", "sind", "die\u00b7se", "Leu\u00b7te", "aus", "Sach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "APPR", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Manche baden in dem klaren Gew\u00e4sser,", "tokens": ["Man\u00b7che", "ba\u00b7den", "in", "dem", "kla\u00b7ren", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wobei erwachsene T\u00f6chter nicht geizen", "tokens": ["Wo\u00b7bei", "er\u00b7wach\u00b7se\u00b7ne", "T\u00f6ch\u00b7ter", "nicht", "gei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit ihren Formen, von denen man f\u00fcglich", "tokens": ["Mit", "ih\u00b7ren", "For\u00b7men", ",", "von", "de\u00b7nen", "man", "f\u00fcg\u00b7lich"], "token_inf