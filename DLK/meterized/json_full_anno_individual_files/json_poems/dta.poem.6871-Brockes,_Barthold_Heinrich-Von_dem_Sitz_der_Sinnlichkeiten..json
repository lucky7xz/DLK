{"dta.poem.6871": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "tr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir k\u00f6nnen ihre Krafft nunmehr", "tokens": ["Wir", "k\u00f6n\u00b7nen", "ih\u00b7re", "Krafft", "nun\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In fremden W\u00fcrckungen verschiedner Gl\u00e4ser sp\u00fcren,", "tokens": ["In", "frem\u00b7den", "W\u00fcr\u00b7ckun\u00b7gen", "ver\u00b7schied\u00b7ner", "Gl\u00e4\u00b7ser", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Worin die Gegenbr\u00fcch erzeugen und formiren", "tokens": ["Wo\u00b7rin", "die", "Ge\u00b7gen\u00b7br\u00fcch", "er\u00b7zeu\u00b7gen", "und", "for\u00b7mi\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gef\u00e4rbte Gegenw\u00fcrff, bald gro\u00df, bald klein,", "tokens": ["Ge\u00b7f\u00e4rb\u00b7te", "Ge\u00b7gen\u00b7w\u00fcrff", ",", "bald", "gro\u00df", ",", "bald", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die blos nur nach dem Schein,", "tokens": ["Die", "blos", "nur", "nach", "dem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch Striche, die nicht da, zu sehen seyn.", "tokens": ["Durch", "Stri\u00b7che", ",", "die", "nicht", "da", ",", "zu", "se\u00b7hen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "ADV", "$,", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}