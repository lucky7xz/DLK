{"textgrid.poem.42073": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die D\u00e4monen fassen Robinson", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum hab ich dies Eiland so erfahren,", "tokens": ["Wa\u00b7rum", "hab", "ich", "dies", "Ei\u00b7land", "so", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDS", "NN", "ADV", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df nichts mir fremd blieb? Ach, ein Baumgesicht,", "tokens": ["Da\u00df", "nichts", "mir", "fremd", "blieb", "?", "Ach", ",", "ein", "Baum\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$.", "ITJ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verh\u00e4ngt von feuchten, windzerfetzten Haaren,", "tokens": ["Ver\u00b7h\u00e4ngt", "von", "feuch\u00b7ten", ",", "wind\u00b7zer\u00b7fetz\u00b7ten", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich traf es einmal \u2013 fault es nun mir nicht", "tokens": ["Ich", "traf", "es", "ein\u00b7mal", "\u2013", "fault", "es", "nun", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "VVFIN", "PPER", "ADV", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zwischen den Schultern? Eine Kr\u00f6te war,", "tokens": ["Zwi\u00b7schen", "den", "Schul\u00b7tern", "?", "Ei\u00b7ne", "Kr\u00f6\u00b7te", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Fett und gefleckt, die Schlange schl\u00fcrfte sie.", "tokens": ["Fett", "und", "ge\u00b7fleckt", ",", "die", "Schlan\u00b7ge", "schl\u00fcrf\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auch lag ein Glied, schamlos und offenbar,", "tokens": ["Auch", "lag", "ein", "Glied", ",", "scham\u00b7los", "und", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An einem Sumpf. Ein Stein vielleicht, doch wie", "tokens": ["An", "ei\u00b7nem", "Sumpf", ".", "Ein", "Stein", "viel\u00b7leicht", ",", "doch", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "ADV", "$,", "ADV", "KOKOM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aus mir gerissen. Und ein Flu\u00df, gespalten", "tokens": ["Aus", "mir", "ge\u00b7ris\u00b7sen", ".", "Und", "ein", "Flu\u00df", ",", "ge\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "VVPP", "$.", "KON", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie durch mein Herz so schmerzlich. Ach, es steht", "tokens": ["Wie", "durch", "mein", "Herz", "so", "schmerz\u00b7lich", ".", "