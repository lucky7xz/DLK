{"textgrid.poem.32812": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Scherzhafte Muse, meine Freude,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scherzhafte Muse, meine Freude,", "tokens": ["Scherz\u00b7haf\u00b7te", "Mu\u00b7se", ",", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die in zufriedner Einsamkeit", "tokens": ["Die", "in", "zu\u00b7fried\u00b7ner", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich oft, entfernt vom Stolz und Neide,", "tokens": ["Mich", "oft", ",", "ent\u00b7fernt", "vom", "Stolz", "und", "Nei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr, als ein lautes Gl\u00fcck erfreut:", "tokens": ["Mehr", ",", "als", "ein", "lau\u00b7tes", "Gl\u00fcck", "er\u00b7freut", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df dich in Auen, B\u00fcschen, Gr\u00fcnden,", "tokens": ["La\u00df", "dich", "in", "Au\u00b7en", ",", "B\u00fc\u00b7schen", ",", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo ich dich suche, liebreich finden,", "tokens": ["Wo", "ich", "dich", "su\u00b7che", ",", "lieb\u00b7reich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4chle Heiterkeit und Ruh", "tokens": ["Und", "l\u00e4ch\u00b7le", "Hei\u00b7ter\u00b7keit", "und", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den freyen Nebenstunden zu.", "tokens": ["Den", "frey\u00b7en", "Ne\u00b7bens\u00b7tun\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich will nicht Helden ewig singen,", "tokens": ["Ich", "will", "nicht", "Hel\u00b7den", "e\u00b7wig", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch mich durch sie zur Ewigkeit:", "tokens": ["Noch", "mich", "durch", "sie", "zur", "E\u00b7wig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Lied soll nicht von Waffen klingen;", "tokens": ["Mein", "Lied", "soll", "nicht", "von", "Waf\u00b7fen", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Muse bebt vor Blut und Streit.", "tokens": ["Die", "Mu\u00b7se", "bebt", "vor", "Blut", "und", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier, unter Oel\u2013und Lorberb\u00e4umen", "tokens": ["Hier", ",", "un\u00b7ter", "O\u00b7el", "\u2013", "und", "Lor\u00b7ber\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "APPR", "NN", "$(", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Soll sie von Fried und Freuden tr\u00e4umen:", "tokens": ["Soll", "sie", "von", "Fried", "und", "Freu\u00b7den", "tr\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den sch\u00f6nsten M\u00e4dchen, besten Wein", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "M\u00e4d\u00b7chen", ",", "bes\u00b7ten", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und liebsten Freunden heilig seyn.", "tokens": ["Und", "liebs\u00b7ten", "Freun\u00b7den", "hei\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Scherze sollen sie begleiten,", "tokens": ["Die", "Scher\u00b7ze", "sol\u00b7len", "sie", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von s\u00fc\u00dfer Unschuld sanft regiert:", "tokens": ["Von", "s\u00fc\u00b7\u00dfer", "Un\u00b7schuld", "sanft", "re\u00b7giert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lasse keinen von der Seiten,", "tokens": ["Sie", "las\u00b7se", "kei\u00b7nen", "von", "der", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihn der Jugend Witz verf\u00fchrt!", "tokens": ["Wenn", "ihn", "der", "Ju\u00b7gend", "Witz", "ver\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn, solt ihr einer ja entfliehen,", "tokens": ["Ihn", ",", "solt", "ihr", "ei\u00b7ner", "ja", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Soll gleich der Ernst zur\u00fccke ziehen;", "tokens": ["Soll", "gleich", "der", "Ernst", "zu\u00b7r\u00fc\u00b7cke", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er leg ihm Blumenfesseln an,", "tokens": ["Er", "leg", "ihm", "Blu\u00b7men\u00b7fes\u00b7seln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df er nicht mehr entwischen kann.", "tokens": ["Da\u00df", "er", "nicht", "mehr", "ent\u00b7wi\u00b7schen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohlan! so sing in s\u00fc\u00dfen T\u00f6nen", "tokens": ["Wo\u00b7hlan", "!", "so", "sing", "in", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein junges anmuthsvolles Lied!", "tokens": ["Dein", "jun\u00b7ges", "an\u00b7muths\u00b7vol\u00b7les", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wird dich gleich kein Lorbeer kr\u00f6nen,", "tokens": ["Und", "wird", "dich", "gleich", "kein", "Lor\u00b7beer", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der f\u00fcr die Heldendichter bl\u00fcht:", "tokens": ["Der", "f\u00fcr", "die", "Hel\u00b7den\u00b7dich\u00b7ter", "bl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Beyfall, den dir Freunde senden,", "tokens": ["Der", "Bey\u00b7fall", ",", "den", "dir", "Freun\u00b7de", "sen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Veilchen