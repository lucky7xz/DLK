{"dta.poem.10849": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken \u00fcber die Eintheilung  \n der Zeit in Wochen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man findet \u00fcberhaupt da\u00df in der\nganzen Welt,", "tokens": ["Man", "fin\u00b7det", "\u00fc\u00b7ber\u00b7haupt", "da\u00df", "in", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df alle V\u00f6lker stets die Rech-", "tokens": ["Da\u00df", "al\u00b7le", "V\u00f6l\u00b7ker", "stets", "die", "Rech"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie der Zeiten Lauf nach", "tokens": ["Da\u00df", "sie", "der", "Zei\u00b7ten", "Lauf", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alle sieben Tag zu solchen", "tokens": ["Und", "al\u00b7le", "sie\u00b7ben", "Tag", "zu", "sol\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "CARD", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es scheint, da\u00df ", "tokens": ["Es", "scheint", ",", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und durch die Fortpflanzung die Nachwelt es ge-", "tokens": ["Und", "durch", "die", "Fort\u00b7pflan\u00b7zung", "die", "Nach\u00b7welt", "es", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "PPER", "TRUNC"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wie er in einer Zeit von solchen Wochen-Tagen,", "tokens": ["Wie", "er", "in", "ei\u00b7ner", "Zeit", "von", "sol\u00b7chen", "Wo\u00b7chen\u00b7Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Dinge die da sind aus Nichts hervorgetragen,", "tokens": ["Die", "Din\u00b7ge", "die", "da", "sind", "aus", "Nichts", "her\u00b7vor\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "VAFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Am siebten Tag geruht. Die Weisheit hat da-", "tokens": ["Am", "sieb\u00b7ten", "Tag", "ge\u00b7ruht", ".", "Die", "Weis\u00b7heit", "hat", "da"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$.", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Die Rechnung so gemacht, und nicht ein Ohnge-", "tokens": ["Die", "Rech\u00b7nung", "so", "ge\u00b7macht", ",", "und", "nicht", "ein", "Ohn\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "KON", "PTKNEG", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Fr\u00e4gt man warum es ", "tokens": ["Fr\u00e4gt", "man", "wa\u00b7rum", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PWAV", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "So wird die Antwort drauf nicht ganz unrichtig", "tokens": ["So", "wird", "die", "Ant\u00b7wort", "drauf", "nicht", "ganz", "un\u00b7rich\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn man drauf sagt: GOtt wil wir sollen alle-", "tokens": ["Wenn", "man", "drauf", "sagt", ":", "Gott", "wil", "wir", "sol\u00b7len", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PAV", "VVFIN", "$.", "NN", "VMFIN", "PPER", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Am siebten Tage ruhn. Das ist die heilge Zahl", "tokens": ["Am", "sieb\u00b7ten", "Ta\u00b7ge", "ruhn", ".", "Das", "ist", "die", "heil\u00b7ge", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er hat auf diesen Tag f\u00fcr uns besondren Segen", "tokens": ["Er", "hat", "auf", "die\u00b7sen", "Tag", "f\u00fcr", "uns", "be\u00b7sond\u00b7ren", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gelegt, und will da\u00df wir denselbigen erwegen,", "tokens": ["Ge\u00b7legt", ",", "und", "will", "da\u00df", "wir", "den\u00b7sel\u00b7bi\u00b7gen", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "VMFIN", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Erlangen was uns gut. Jhr Menschen denkt dem", "tokens": ["Er\u00b7lan\u00b7gen", "was", "uns", "gut", ".", "Ihr", "Men\u00b7schen", "denkt", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PWS", "PPER", "ADJD", "$.", "PPOSAT", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und feirt in stiller Ruh doch diesen Segens-Tag;", "tokens": ["Und", "feirt", "in", "stil\u00b7ler", "Ruh", "doch", "die\u00b7sen", "Se\u00b7gens\u00b7Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dient euren ", "tokens": ["Dient", "eu\u00b7ren"], "token_info": ["word", "word"], "pos": ["NN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Wie euch durch Woch und Jahr erfreut eur Woll-", "tokens": ["Wie", "euch", "durch", "Woch", "und", "Jahr", "er\u00b7freut", "eur", "Woll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}