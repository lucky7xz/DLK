{"textgrid.poem.32267": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die Beredsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freunde, Wasser machet stumm:", "tokens": ["Freun\u00b7de", ",", "Was\u00b7ser", "ma\u00b7chet", "stumm", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernet dieses an den Fischen.", "tokens": ["Ler\u00b7net", "die\u00b7ses", "an", "den", "Fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch beim Weine kehrt sichs um:", "tokens": ["Doch", "beim", "Wei\u00b7ne", "kehrt", "sichs", "um", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses lernt an unsern Tischen.", "tokens": ["Die\u00b7ses", "lernt", "an", "un\u00b7sern", "Ti\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was f\u00fcr Redner sind wir nicht,", "tokens": ["Was", "f\u00fcr", "Red\u00b7ner", "sind", "wir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn der Rheinwein aus uns spricht!", "tokens": ["Wenn", "der", "Rhein\u00b7wein", "aus", "uns", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wir ermahnen, streiten, lehren;", "tokens": ["Wir", "er\u00b7mah\u00b7nen", ",", "strei\u00b7ten", ",", "leh\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Keiner will den andern h\u00f6ren.", "tokens": ["Kei\u00b7ner", "will", "den", "an\u00b7dern", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freunde, Wasser machet stumm:", "tokens": ["Freun\u00b7de", ",", "Was\u00b7ser", "ma\u00b7chet", "stumm", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernet dieses an den Fischen.", "tokens": ["Ler\u00b7net", "die\u00b7ses", "an", "den", "Fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch beim Weine kehrt sichs um:", "tokens": ["Doch", "beim", "Wei\u00b7ne", "kehrt", "sichs", "um", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses lernt an unsern Tischen.", "tokens": ["Die\u00b7ses", "lernt", "an", "un\u00b7sern", "Ti\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+