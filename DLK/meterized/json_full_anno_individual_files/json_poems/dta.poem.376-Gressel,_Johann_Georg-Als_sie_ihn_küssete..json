{"dta.poem.376": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn k\u00fcssete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Halt ein! mein Kind halt ein!", "tokens": ["Halt", "ein", "!", "mein", "Kind", "halt", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Geist wird sonsten truncken/", "tokens": ["Mein", "Geist", "wird", "sons\u00b7ten", "trun\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Lippen s\u00fcsser Wein", "tokens": ["Der", "Lip\u00b7pen", "s\u00fcs\u00b7ser", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist gar zu starck/", "tokens": ["Ist", "gar", "zu", "starck", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es dringt durch Bein und Marck", "tokens": ["Es", "dringt", "durch", "Bein", "und", "Marck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Den ich geschmeckt.", "tokens": ["Den", "ich", "ge\u00b7schmeckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Doch Nein! halt noch nicht ein", "tokens": ["Doch", "Nein", "!", "halt", "noch", "nicht", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "VVFIN", "ADV", "PTKNEG", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ich will gern truncken seyn.", "tokens": ["Ich", "will", "gern", "trun\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wer sich also berauscht ist \u00fcberaus begl\u00fccket", "tokens": ["Wer", "sich", "al\u00b7so", "be\u00b7rauscht", "ist", "\u00fc\u00b7be\u00b7raus", "be\u00b7gl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ADV", "VVPP", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil ihm der Himmel selbst den Rausch hat zugeschicket.", "tokens": ["Weil", "ihm", "der", "Him\u00b7mel", "selbst", "den", "Rausch", "hat", "zu\u00b7ge\u00b7schi\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}