{"textgrid.poem.48560": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9. Auf Herren Johan Behrs Leichbestattung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df doch der unversch\u00e4mte Tod", "tokens": ["Da\u00df", "doch", "der", "un\u00b7ver\u00b7sch\u00e4m\u00b7te", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nichts l\u00e4sset lange sein,", "tokens": ["nichts", "l\u00e4s\u00b7set", "lan\u00b7ge", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "was uns vor andern lieb und not", "tokens": ["was", "uns", "vor", "an\u00b7dern", "lieb", "und", "not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PIS", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und sonst nicht ist gemein'!", "tokens": ["und", "sonst", "nicht", "ist", "ge\u00b7mein'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist bei ihm kein Unterscheid,", "tokens": ["Es", "ist", "bei", "ihm", "kein", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "er fordert Alles rau\u00df", "tokens": ["er", "for\u00b7dert", "Al\u00b7les", "rau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "und l\u00e4\u00dft vor andern seinen Neid", "tokens": ["und", "l\u00e4\u00dft", "vor", "an\u00b7dern", "sei\u00b7nen", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "an werten Sachen aus.", "tokens": ["an", "wer\u00b7ten", "Sa\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Welt, die ist ein Gl\u00fcckestopf,", "tokens": ["Die", "Welt", ",", "die", "ist", "ein", "Gl\u00fc\u00b7ckes\u00b7topf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der stets herumher l\u00e4uft;", "tokens": ["der", "stets", "her\u00b7um\u00b7her", "l\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da gilt es einen ieden Kopf,", "tokens": ["da", "gilt", "es", "ei\u00b7nen", "ie\u00b7den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wen das Verh\u00e4ngn\u00fc\u00df greift.", "tokens": ["wen", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "greift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Es geht blind zu, es gilt kein Wort;", "tokens": ["Es", "geht", "blind", "zu", ",", "es", "gilt", "kein", "Wort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "nicht richtet, Niemand schreibt.", "tokens": ["nicht", "rich\u00b7tet", ",", "Nie\u00b7mand", "schreibt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gel\u00fcckt's, so mu\u00df der K\u00f6nig fort,", "tokens": ["Ge\u00b7l\u00fcckt's", ",", "so", "mu\u00df", "der", "K\u00f6\u00b7nig", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VMFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der Eseltreiber bleibt.", "tokens": ["der", "E\u00b7sel\u00b7trei\u00b7ber", "bleibt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer sagt nicht, da\u00df es Schade sei", "tokens": ["Wer", "sagt", "nicht", ",", "da\u00df", "es", "Scha\u00b7de", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "um diesen tapfern Man,", "tokens": ["um", "die\u00b7sen", "tap\u00b7fern", "Man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der neulich solchen Nutz und Treu'", "tokens": ["der", "neu\u00b7lich", "sol\u00b7chen", "Nutz", "und", "Treu'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "an unsrer Stadt getan?", "tokens": ["an", "uns\u00b7rer", "Stadt", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer sich des Vaterlandes wehrt", "tokens": ["Wer", "sich", "des", "Va\u00b7ter\u00b7lan\u00b7des", "wehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und f\u00fcrchtet keinen Sto\u00df,", "tokens": ["und", "f\u00fcrch\u00b7tet", "kei\u00b7nen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "der ist im Leben wolgeehrt", "tokens": ["der", "ist", "im", "Le\u00b7ben", "wol\u00b7ge\u00b7ehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und nach dem Tode gro\u00df.", "tokens": ["und", "nach", "dem", "To\u00b7de", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der tolle Mars erboste sich", "tokens": ["Der", "tol\u00b7le", "Mars", "er\u00b7bos\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und strengt' uns grimmig an;", "tokens": ["und", "strengt'", "uns", "grim\u00b7mig", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "du stundest steif und wagtest dich,", "tokens": ["du", "stun\u00b7dest", "steif", "und", "wag\u00b7test", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du teurer B\u00fcrgersman!", "tokens": ["du", "teu\u00b7rer", "B\u00fcr\u00b7gers\u00b7man", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df diese Stadt noch steht im Heil", "tokens": ["Da\u00df", "die\u00b7se", "Stadt", "noch", "steht", "im", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und fiel nicht allzusehr,", "tokens": ["und", "fiel", "nicht", "all\u00b7zu\u00b7sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "des dankt sie billich dir ein Teil", "tokens": ["des", "dankt", "sie", "bil\u00b7lich", "dir", "ein", "Teil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PPER", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und klagt dich desto mehr.", "tokens": ["und", "klagt", "dich", "des\u00b7to", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verla\u00dfne Jungfrau, sch\u00fctzet di\u00df", "tokens": ["Ver\u00b7la\u00df\u00b7ne", "Jung\u00b7frau", ",", "sch\u00fct\u00b7zet", "di\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PDS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "den starken Tr\u00e4nen f\u00fcr,", "tokens": ["den", "star\u00b7ken", "Tr\u00e4\u00b7nen", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df euer Vater ist gewi\u00df,", "tokens": ["da\u00df", "eu\u00b7er", "Va\u00b7ter", "ist", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wo ihr solt hin und wir!", "tokens": ["wo", "ihr", "solt", "hin", "und", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "ADV", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Ander', das euch mehr betr\u00fcbt,", "tokens": ["Das", "An\u00b7der'", ",", "das", "euch", "mehr", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das euch zur Waisen macht,", "tokens": ["das", "euch", "zur", "Wai\u00b7sen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "befehlt dem, der uns Eltern gibt", "tokens": ["be\u00b7fehlt", "dem", ",", "der", "uns", "El\u00b7tern", "gibt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "$,", "PRELS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und nimt nach seiner Acht!", "tokens": ["und", "nimt", "nach", "sei\u00b7ner", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was klagt ihr aber viel um euch?", "tokens": ["Was", "klagt", "ihr", "a\u00b7ber", "viel", "um", "euch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Christenheit", "tokens": ["Die", "gan\u00b7ze", "Chris\u00b7ten\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ist itzund einer Waisen gleich", "tokens": ["ist", "it\u00b7zund", "ei\u00b7ner", "Wai\u00b7sen", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und \u00e4chzet um ihr Leid.", "tokens": ["und", "\u00e4ch\u00b7zet", "um", "ihr", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Vater, Heiland, Schirm und Held,", "tokens": ["Ihr", "Va\u00b7ter", ",", "Hei\u00b7land", ",", "Schirm", "und", "Held", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ihr bester Trost nach Gott,", "tokens": ["ihr", "bes\u00b7ter", "Trost", "nach", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "der strengste Ritter von der Welt,", "t