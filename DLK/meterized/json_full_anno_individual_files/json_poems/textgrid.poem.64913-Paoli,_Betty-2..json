{"textgrid.poem.64913": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen Bl\u00e4ttern hier magst du es deutlich lesen,", "tokens": ["In", "die\u00b7sen", "Bl\u00e4t\u00b7tern", "hier", "magst", "du", "es", "deut\u00b7lich", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was meiner Seele du, der strebenden, gewesen.", "tokens": ["Was", "mei\u00b7ner", "See\u00b7le", "du", ",", "der", "stre\u00b7ben\u00b7den", ",", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "$,", "ART", "ADJA", "$,", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht nur, was du mir warst, auch was du mir noch bist,", "tokens": ["Nicht", "nur", ",", "was", "du", "mir", "warst", ",", "auch", "was", "du", "mir", "noch", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWS", "PPER", "PPER", "VAFIN", "$,", "ADV", "PWS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stets mir bleiben wirst zu jeder Lebensfrist.", "tokens": ["Und", "stets", "mir", "blei\u00b7ben", "wirst", "zu", "je\u00b7der", "Le\u00b7bens\u00b7frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "P