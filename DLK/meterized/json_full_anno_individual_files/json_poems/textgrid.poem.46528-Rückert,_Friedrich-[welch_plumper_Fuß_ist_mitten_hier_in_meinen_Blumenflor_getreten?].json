{"textgrid.poem.46528": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[welch plumper Fu\u00df ist mitten hier in meinen Blumenflor getreten?]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch plumper Fu\u00df ist mitten hier in meinen Blumenflor getreten?", "tokens": ["Welch", "plum\u00b7per", "Fu\u00df", "ist", "mit\u00b7ten", "hier", "in", "mei\u00b7nen", "Blu\u00b7men\u00b7flor", "ge\u00b7tre\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Welch ein vermummter Schauder ist in meinen Freudenchor getreten?", "tokens": ["Welch", "ein", "ver\u00b7mumm\u00b7ter", "Schau\u00b7der", "ist", "in", "mei\u00b7nen", "Freu\u00b7denc\u00b7hor", "ge\u00b7tre\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "In meinen stillen W\u00e4nden war ein Fest der Lieb' und des Gesanges;", "tokens": ["In", "mei\u00b7nen", "stil\u00b7len", "W\u00e4n\u00b7den", "war", "ein", "Fest", "der", "Lieb'", "und", "des", "Ge\u00b7san\u00b7ges", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Unangemeldet ist ein Gast, ein schweigender, ins Thor getreten,", "tokens": ["Un\u00b7an\u00b7ge\u00b7mel\u00b7det", "ist", "ein", "Gast", ",", "ein", "schwei\u00b7gen\u00b7der", ",", "ins", "Thor", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Hat finster um sich her geblickt, da\u00df alle Kerzen d\u00fcster brannten,", "tokens": ["Hat", "fins\u00b7ter", "um", "sich", "her", "ge\u00b7blickt", ",", "da\u00df", "al\u00b7le", "Ker\u00b7zen", "d\u00fcs\u00b7ter", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PRF", "APZR", "VVPP", "$,", "KOUS", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und ist mit Furchteinfl\u00fcsterungen mir zum entsetzten Ohr getreten.", "tokens": ["Und", "ist", "mit", "Furcht\u00b7ein\u00b7fl\u00fcs\u00b7te\u00b7run\u00b7gen", "mir", "zum", "ent\u00b7setz\u00b7ten", "Ohr", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf jedem Weg ins Herz zur\u00fcck ist das erschrockne Blut geflohen,", "tokens": ["Auf", "je\u00b7dem", "Weg", "ins", "Herz", "zu\u00b7r\u00fcck", "ist", "das", "er\u00b7schrock\u00b7ne", "Blut", "ge\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "PTKVZ", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und aus den Thr\u00e4nenschleu\u00dfen ist die Fluth ins Aug' empor getreten.", "tokens": ["Und", "aus", "den", "Thr\u00e4\u00b7nen\u00b7schleu\u00b7\u00dfen", "ist", "die", "Fluth", "ins", "Aug'", "em\u00b7por", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Wie ernst hat eine schwere Hand ins leichte Saitenspiel gegriffen!", "tokens": ["Wie", "ernst", "hat", "ei\u00b7ne", "schwe\u00b7re", "Hand", "ins", "leich\u00b7te", "Sai\u00b7ten\u00b7spiel", "ge\u00b7grif\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wie fremd ist geisterhafter Hauch ins lustgestimmte Rohr getreten!", "tokens": ["Wie", "fremd", "ist", "geis\u00b7ter\u00b7haf\u00b7ter", "Hauch", "ins", "lust\u00b7ge\u00b7stimm\u00b7te", "Rohr", "ge\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "So ungeahnet pl\u00f6tzlich ist der Tod aus vollen Lebens Mitte,", "tokens": ["So", "un\u00b7ge\u00b7ah\u00b7net", "pl\u00f6tz\u00b7lich", "ist", "der", "Tod", "aus", "vol\u00b7len", "Le\u00b7bens", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wie aus der Bl\u00fcthe Bl\u00e4tterschmuck die nackte Frucht, hervorgetreten.", "tokens": ["Wie", "aus", "der", "Bl\u00fc\u00b7the", "Bl\u00e4t\u00b7ter\u00b7schmuck", "die", "nack\u00b7te", "Frucht", ",", "her\u00b7vor\u00b7ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Es ist der Tod des Lebens Kern, als wie die Frucht der Kern der Bl\u00fcthe;", "tokens": ["Es", "ist", "der", "Tod", "des", "Le\u00b7bens", "Kern", ",", "als", "wie", "die", "Frucht", "der", "Kern", "der", "Bl\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Er war von Anfang drin verh\u00fcllt und ist nun aus dem Flor getreten.", "tokens": ["Er", "war", "von", "An\u00b7fang", "drin", "ver\u00b7h\u00fcllt", "und", "ist", "nun", "aus", "dem", "Flor", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "KON", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Welch plumper Fu\u00df ist mitten hier in meinen Blumenflor getreten?", "tokens": ["Welch", "plum\u00b7per", "Fu\u00df", "ist", "mit\u00b7ten", "hier", "in", "mei\u00b7nen", "