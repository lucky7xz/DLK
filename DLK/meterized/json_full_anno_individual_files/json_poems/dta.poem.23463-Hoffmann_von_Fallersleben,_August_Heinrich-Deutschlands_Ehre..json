{"dta.poem.23463": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Deutschlands Ehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hei\u00dft mich froh willkommen sein,", "tokens": ["Hei\u00dft", "mich", "froh", "will\u00b7kom\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der euch Neues bringet, das bin ich;", "tokens": ["Der", "euch", "Neu\u00b7es", "brin\u00b7get", ",", "das", "bin", "ich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eitle Worte sind's allein,", "tokens": ["Eit\u00b7le", "Wor\u00b7te", "sin\u00b7d's", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die ihr noch vernahmt: jetzt fraget mich.", "tokens": ["Die", "ihr", "noch", "ver\u00b7nahmt", ":", "jetzt", "fra\u00b7get", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn ihr Lohn gew\u00e4hret", "tokens": ["Wenn", "ihr", "Lohn", "ge\u00b7w\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und den Sold nicht scheut,", "tokens": ["Und", "den", "Sold", "nicht", "scheut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Will ich Manches sagen, was die Herzen freut:", "tokens": ["Will", "ich", "Man\u00b7ches", "sa\u00b7gen", ",", "was", "die", "Her\u00b7zen", "freut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Seht, wie ihr mich w\u00fcrdig ehret.", "tokens": ["Seht", ",", "wie", "ihr", "mich", "w\u00fcr\u00b7dig", "eh\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich verk\u00fcnde deutschen Frau'n", "tokens": ["Ich", "ver\u00b7k\u00fcn\u00b7de", "deut\u00b7schen", "Frau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche Dinge, das sie alle Welt", "tokens": ["Sol\u00b7che", "Din\u00b7ge", ",", "das", "sie", "al\u00b7le", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Noch begier'ger wird zu schau'n:", "tokens": ["Noch", "be\u00b7gier'\u00b7ger", "wird", "zu", "schau'n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Daf\u00fcr nehm' ich weder Gut noch Geld.", "tokens": ["Da\u00b7f\u00fcr", "nehm'", "ich", "we\u00b7der", "Gut", "noch", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "ADJD", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Was wollt' ich von den S\u00fc\u00dfen?", "tokens": ["Was", "wollt'", "ich", "von", "den", "S\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie sind mir zu hehr:", "tokens": ["Sie", "sind", "mir", "zu", "hehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Drum bescheid' ich mich und bitte sie nichts mehr,", "tokens": ["Drum", "be\u00b7scheid'", "ich", "mich", "und", "bit\u00b7te", "sie", "nichts", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Als da\u00df sie mich freundlich gr\u00fc\u00dfen.", "tokens": ["Als", "da\u00df", "sie", "mich", "freund\u00b7lich", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}