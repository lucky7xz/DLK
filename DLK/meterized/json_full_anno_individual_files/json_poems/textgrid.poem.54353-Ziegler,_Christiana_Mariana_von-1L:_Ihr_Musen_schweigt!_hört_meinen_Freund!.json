{"textgrid.poem.54353": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Musen schweigt! h\u00f6rt meinen Freund!", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Musen schweigt! h\u00f6rt meinen Freund!", "tokens": ["Ihr", "Mu\u00b7sen", "schweigt", "!", "h\u00f6rt", "mei\u00b7nen", "Freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Freund, der es recht redlich meynt,", "tokens": ["Den", "Freund", ",", "der", "es", "recht", "red\u00b7lich", "meynt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der singt aus einem andern Thone.", "tokens": ["Der", "singt", "aus", "ei\u00b7nem", "an\u00b7dern", "Tho\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er flieht die Sp\u00f6tter dieser Zeit,", "tokens": ["Er", "flieht", "die", "Sp\u00f6t\u00b7ter", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Kiel macht mir ein Lob bereit;", "tokens": ["Sein", "Kiel", "macht", "mir", "ein", "Lob", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er g\u00f6nnet mir die Dichterkrone.", "tokens": ["Er", "g\u00f6n\u00b7net", "mir", "die", "Dich\u00b7ter\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Itzt h\u00f6r ich nicht auf Orpheus Klang,", "tokens": ["Itzt", "h\u00f6r", "ich", "nicht", "auf", "Or\u00b7pheus", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn dein so lieblicher Gesang,", "tokens": ["Denn", "dein", "so", "lieb\u00b7li\u00b7cher", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erg\u00f6tzt die aufmerksamen Ohren.", "tokens": ["Er\u00b7g\u00f6tzt", "die", "auf\u00b7merk\u00b7sa\u00b7men", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du bist ein Feind der Schmeicheley,", "tokens": ["Du", "bist", "ein", "Feind", "der", "Schmei\u00b7che\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist von Neid und Misgunst frey,", "tokens": ["Du", "bist", "von", "Neid", "und", "Mis\u00b7gunst", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist vor tausend auserkohren.", "tokens": ["Du", "bist", "vor", "tau\u00b7send", "au\u00b7ser\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "CARD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Br\u00fcllt Feinde, rast, flucht immerhin,", "tokens": ["Br\u00fcllt", "Fein\u00b7de", ",", "rast", ",", "flucht", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein froher und gela\u00dfner Sinn", "tokens": ["Mein", "fro\u00b7her", "und", "ge\u00b7la\u00df\u00b7ner", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erschrickt nicht vor dergleichen Wettern.", "tokens": ["Er\u00b7schrickt", "nicht", "vor", "derg\u00b7lei\u00b7chen", "Wet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Unschuld heiliges Gewand", "tokens": ["Der", "Un\u00b7schuld", "hei\u00b7li\u00b7ges", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat mir der Himmel zugesandt,", "tokens": ["Hat", "mir", "der", "Him\u00b7mel", "zu\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum f\u00fcrcht ich mich auch nicht f\u00fcr Sp\u00f6ttern.", "tokens": ["Drum", "f\u00fcrcht", "ich", "mich", "auch", "nicht", "f\u00fcr", "Sp\u00f6t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Besitz ich eines Freundes Herz,", "tokens": ["Be\u00b7sitz", "ich", "ei\u00b7nes", "Freun\u00b7des", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Theilt der mit mir Verdru\u00df und Schmerz,", "tokens": ["Theilt", "der", "mit", "mir", "Ver\u00b7dru\u00df", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So hab ich mir genug erworben.", "tokens": ["So", "hab", "ich", "mir", "ge\u00b7nug", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Neiden h\u00f6ret doch nicht auf,", "tokens": ["Das", "Nei\u00b7den", "h\u00f6\u00b7ret", "doch", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lange daurt mein Lebenslauf", "tokens": ["So", "lan\u00b7ge", "daurt", "mein", "Le\u00b7bens\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer weis geschiehts, bin ich gestorben?", "tokens": ["Wer", "weis", "ge\u00b7schiehts", ",", "bin", "ich", "ge\u00b7stor\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Komm, Freund, und h\u00f6re mir nur zu,", "tokens": ["Komm", ",", "Freund", ",", "und", "h\u00f6\u00b7re", "mir", "nur", "zu", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich oft sitz in stiller Ruh", "tokens": ["Wenn", "ich", "oft", "sitz", "in", "stil\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spiele die gestimmte Cyther;", "tokens": ["Und", "spie\u00b7le", "die", "ge\u00b7stimm\u00b7te", "Cy\u00b7ther", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! da bin ich wie Cr\u00f6sus reich.", "tokens": ["Ach", "!", "da", "bin", "ich", "wie", "Cr\u00f6\u00b7sus", "reich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "KOKOM", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nichts ist dem Vergn\u00fcgen gleich;", "tokens": ["Und", "nichts", "ist", "dem", "Ver\u00b7gn\u00fc\u00b7gen", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Es daurt auch bey dem Ungewitter.", "tokens": ["Es", "daurt", "auch", "bey", "dem", "Un\u00b7ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier schlie\u00dft sich schon der matte Reim", "tokens": ["Hier", "schlie\u00dft", "sich", "schon", "der", "mat\u00b7te", "Reim"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und saget dir noch in geheim:", "tokens": ["Und", "sa\u00b7get", "dir", "noch", "in", "ge\u00b7heim", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Besuch doch einstens unsre Linden;", "tokens": ["Be\u00b7such", "doch", "eins\u00b7tens", "uns\u00b7re", "Lin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schwere bey dem Deutschen Sinn,", "tokens": ["Ich", "schwe\u00b7re", "bey", "dem", "Deut\u00b7schen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch den ich unge\u00e4ndert bin:", "tokens": ["Durch", "den", "ich", "un\u00b7ge\u00b7\u00e4n\u00b7dert", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du sollst mich allzeit redlich finden.", "tokens": ["Du", "sollst", "mich", "all\u00b7zeit", "red\u00b7lich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr Musen schweigt! h\u00f6rt meinen Freund!", "tokens": ["Ihr", "Mu\u00b7sen", "schweigt", "!", "h\u00f6rt", "mei\u00b7nen", "Freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Freund, der es recht redlich meynt,", "tokens": ["Den", "Freund", ",", "der", "es", "recht", "red\u00b7lich", "meynt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der singt aus einem andern Thone.", "tokens": ["Der", "singt", "aus", "ei\u00b7nem", "an\u00b7dern", "Tho\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er flieht die Sp\u00f6tter dieser Zeit,", "tokens": ["Er", "flieht", "die", "Sp\u00f6t\u00b7ter", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Kiel macht mir ein Lob bereit;", "tokens": ["Sein", "Kiel", "macht", "mir", "ein", "Lob", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er g\u00f6