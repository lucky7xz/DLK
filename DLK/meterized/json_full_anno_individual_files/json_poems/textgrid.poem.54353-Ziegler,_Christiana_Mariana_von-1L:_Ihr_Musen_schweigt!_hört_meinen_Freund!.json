 ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Besitz ich eines Freundes Herz,", "tokens": ["Be\u00b7sitz", "ich", "ei\u00b7nes", "Freun\u00b7des", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Theilt der mit mir Verdru\u00df und Schmerz,", "tokens": ["Theilt", "der", "mit", "mir", "Ver\u00b7dru\u00df", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So hab ich mir genug erworben.", "tokens": ["So", "hab", "ich", "mir", "ge\u00b7nug", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Neiden h\u00f6ret doch nicht auf,", "tokens": ["Das", "Nei\u00b7den", "h\u00f6\u00b7ret", "doch", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lange daurt mein Lebenslauf", "tokens": ["So", "lan\u00b7ge", "daurt", "mein", "Le\u00b7bens\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer weis geschiehts, bin ich gestorben?", "tokens": ["Wer", "weis", "ge\u00b7schiehts", ",", "bin", "ich", "ge\u00b7stor\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Komm, Freund, und h\u00f6re mir nur zu,", "tokens": ["Komm", ",", "Freund", ",", "und", "h\u00f6\u00b7re", "mir", "nur", "zu", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich oft sitz in stiller Ruh", "tokens": ["Wenn", "ich", "oft", "sitz", "in", "stil\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spiele die gestimmte Cyther;", "tokens": ["Und", "spie\u00b7le", "die", "ge\u00b7stimm\u00b7te", "Cy\u00b7ther", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! da bin ich wie Cr\u00f6sus reich.", "tokens": ["Ach", "!", "da", "bin", "ich", "wie", "Cr\u00f6\u00b7sus", "reich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "KOKOM", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nichts ist dem Vergn\u00fcgen gleich;", "tokens": ["Und", "nichts", "ist", "dem", "Ver\u00b7gn\u00fc\u00b7gen", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Es daurt auch bey dem Ungewitter.", "tokens": ["Es", "daurt", "auch", "bey", "dem", "Un\u00b7ge\