{"textgrid.poem.62475": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Wanderung in Worms", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bl\u00fcht der Rosengarten?", "tokens": ["Wo", "bl\u00fcht", "der", "Ro\u00b7sen\u00b7gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo weilt die s\u00fc\u00dfe Maid?", "tokens": ["Wo", "weilt", "die", "s\u00fc\u00b7\u00dfe", "Maid", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin ihr aufzuwarten,", "tokens": ["Ich", "bin", "ihr", "auf\u00b7zu\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Ehren hier bereit.", "tokens": ["In", "Eh\u00b7ren", "hier", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Rosen sind gebrochen", "tokens": ["Die", "Ro\u00b7sen", "sind", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor einem rauhen Wind;", "tokens": ["Vor", "ei\u00b7nem", "rau\u00b7hen", "Wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Hagen hat erstochen", "tokens": ["Der", "Ha\u00b7gen", "hat", "