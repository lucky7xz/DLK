"measure": "iambic.tri"}, "line.3": {"text": "Dein Schatz von rothem Golde,", "tokens": ["Dein", "Schatz", "von", "ro\u00b7them", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von s\u00fc\u00dfer Huld versank.", "tokens": ["Von", "s\u00fc\u00b7\u00dfer", "Huld", "ver\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gen Worms will ich mich wenden.", "tokens": ["Gen", "Worms", "will", "ich", "mich", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Stadt am gr\u00fcnen Strom,", "tokens": ["Zur", "Stadt", "am", "gr\u00fc\u00b7nen", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da prangt von Meisterh\u00e4nden", "tokens": ["Da", "prangt", "von", "Meis\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der alte heil'ge Dom.", "tokens": ["Der", "al\u00b7te", "heil'\u00b7ge", "Dom", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich h\u00f6rte viel vom alten", "tokens": ["Ich", "h\u00f6r\u00b7te", "viel", "vom", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Untadligen Geschlecht;", "tokens": ["Un\u00b7tad\u00b7li\u00b7gen", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die K\u00e4mmerer verwalten", "tokens": ["Die", "K\u00e4m\u00b7me\u00b7rer", "ver\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier wol das deutsche Recht.", "tokens": ["Hier", "wol", "das", "deut\u00b7sche", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Habt ihr es nicht vernommen,", "tokens": ["Habt", "ihr", "es", "nicht", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kaiser ist euch nah'?", "tokens": ["Der", "Kai\u00b7ser", "ist", "euch", "nah'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kaiser Franz wird kommen,", "tokens": ["Der", "Kai\u00b7ser", "Franz", "wird", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ist kein Dalberg da?", "tokens": ["Und", "ist", "kein", "Dal\u00b7berg", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie fern hast du ges\u00e4umet,", "tokens": ["Wie", "fern", "hast", "du", "ge\u00b7s\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Wandersmann, wie weit?", "tokens": ["O", "Wan\u00b7ders\u00b7mann", ",", "wie", "weit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beim Heldenlied vertr\u00e4umet", "tokens": ["Beim", "Hel\u00b7den\u00b7lied", "ver\u00b7tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hast du das j\u00fcngste Leid.", "tokens": ["Hast", "du", "das", "j\u00fcngs\u00b7te", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Des Hagens b\u00f6se Thaten", "tokens": ["Des", "Ha\u00b7gens", "b\u00f6\u00b7se", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erlebten wir aufs Neu',", "tokens": ["Er\u00b7leb\u00b7ten", "wir", "aufs", "Neu'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Dalberg ward verrathen", "tokens": ["Vom", "Dal\u00b7berg", "ward", "ver\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Stammes Ruhm und Treu.", "tokens": ["Des", "Stam\u00b7mes", "Ruhm", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und was noch ist geblieben", "tokens": ["Und", "was", "noch", "ist", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von deutscher Heldenlust?", "tokens": ["Von", "deut\u00b7scher", "Hel\u00b7den\u00b7lust", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Dienen und zum Lieben,", "tokens": ["Zum", "Die\u00b7nen", "und", "zum", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was blieb der deutschen Brust?", "tokens": ["Was", "blieb", "der", "deut\u00b7schen", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Geister und die Sagen,", "tokens": ["Die", "Geis\u00b7ter", "und", "die", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der alten Tage Zier,", "tokens": ["Der", "al\u00b7ten", "Ta\u00b7ge", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die kann kein Feind erschlagen,", "tokens": ["Die", "kann", "kein", "Feind", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie weilen ewig hier.", "tokens": ["Sie", "wei\u00b7len", "e\u00b7wig", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Auch flie\u00dfet noch zur Stunde", "tokens": ["Auch", "flie\u00b7\u00dfet", "noch", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der alte Rhein vorbei,", "tokens": ["Der", "al\u00b7te", "Rhein", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der blieb dem Heldenbunde,", "tokens": ["Der", "blieb", "dem", "Hel\u00b7den\u00b7bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Heldenzeiten treu.", "tokens": ["Den", "Hel\u00b7den\u00b7zei\u00b7ten", "treu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "O sammelt euch, ihr Br\u00fcder,", "tokens": ["O", "sam\u00b7melt", "euch", ",", "ihr", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um diesen heil'gen Ort,", "tokens": ["Um", "die\u00b7sen", "heil'\u00b7gen", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erklingen soll hier wieder", "tokens": ["Er\u00b7klin\u00b7gen", "soll", 