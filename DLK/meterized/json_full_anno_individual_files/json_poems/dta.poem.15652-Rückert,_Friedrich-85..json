{"dta.poem.15652": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie um die Sonne rund Planeten gehn im Kreise;", "tokens": ["Wie", "um", "die", "Son\u00b7ne", "rund", "Pla\u00b7ne\u00b7ten", "gehn", "im", "Krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was r\u00fcndet auf der Welt sich nichts in gleicher Weise?", "tokens": ["Was", "r\u00fcn\u00b7det", "auf", "der", "Welt", "sich", "nichts", "in", "glei\u00b7cher", "Wei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "PRF", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6ne Ganzheit scheint dem Ganzen vorbehalten,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ganz\u00b7heit", "scheint", "dem", "Gan\u00b7zen", "vor\u00b7be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Einzlen \u00fcberall Zersplitterung zu walten.", "tokens": ["Im", "Einz\u00b7len", "\u00fc\u00b7be\u00b7rall", "Zer\u00b7split\u00b7te\u00b7rung", "zu", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und nur ein Eiland gibts, ich wei\u00df nicht wo auf Erden,", "tokens": ["Und", "nur", "ein", "Ei\u00b7land", "gibts", ",", "ich", "wei\u00df", "nicht", "wo", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem die Ordnungen des Himmels sichtbar werden.", "tokens": ["An", "dem", "die", "Ord\u00b7nun\u00b7gen", "des", "Him\u00b7mels", "sicht\u00b7bar", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Mittelpunkte steht die K\u00f6nigsburg, im Bogen", "tokens": ["Im", "Mit\u00b7tel\u00b7punk\u00b7te", "steht", "die", "K\u00f6\u00b7nigs\u00b7burg", ",", "im", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind Kreise siebenfach des Lebens hergezogen.", "tokens": ["Sind", "Krei\u00b7se", "sie\u00b7ben\u00b7fach", "des", "Le\u00b7bens", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der erste Kreis die Stadt, der K\u00f6nigsburg zu F\u00fc\u00dfen,", "tokens": ["Der", "ers\u00b7te", "Kreis", "die", "Stadt", ",", "der", "K\u00f6\u00b7nigs\u00b7burg", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In stolzer Dienstbarkeit, gesch\u00e4ftig in Gen\u00fcssen.", "tokens": ["In", "stol\u00b7zer", "Dienst\u00b7bar\u00b7keit", ",", "ge\u00b7sch\u00e4f\u00b7tig", "in", "Ge\u00b7n\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der zweite Kreis umgibt die Stadt, ein Gartensaum,", "tokens": ["Der", "zwei\u00b7te", "Kreis", "um\u00b7gibt", "die", "Stadt", ",", "ein", "Gar\u00b7ten\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo gr\u00fcn des Lebens w\u00e4chst und der Erkenntnis Baum.", "tokens": ["Wo", "gr\u00fcn", "des", "Le\u00b7bens", "w\u00e4chst", "und", "der", "Er\u00b7kennt\u00b7nis", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "KON", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der dritte Kreis umf\u00e4ngt die G\u00e4rten, ein Gefilde,", "tokens": ["Der", "drit\u00b7te", "Kreis", "um\u00b7f\u00e4ngt", "die", "G\u00e4r\u00b7ten", ",", "ein", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Pflug und Sichel geht der arbeitfrohen Gilde.", "tokens": ["Wo", "Pflug", "und", "Si\u00b7chel", "geht", "der", "ar\u00b7beit\u00b7fro\u00b7hen", "Gil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der vierte Kreis ums Feld ein Waldrevier gereiht,", "tokens": ["Der", "vier\u00b7te", "Kreis", "ums", "Feld", "ein", "Wald\u00b7re\u00b7vier", "ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo freie Thiere gehn, der freien Jagd geweiht.", "tokens": ["Wo", "frei\u00b7e", "Thie\u00b7re", "gehn", ",", "der", "frei\u00b7en", "Jagd", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der f\u00fcnfte Kreis ums Waldgeheg' ein Klippenrand,", "tokens": ["Der", "f\u00fcnf\u00b7te", "Kreis", "ums", "Wald\u00b7ge\u00b7heg'", "ein", "Klip\u00b7pen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Edelstein im Schoo\u00df und Perlensaat im Sand.", "tokens": ["Mit", "E\u00b7del\u00b7stein", "im", "Schoo\u00df", "und", "Per\u00b7len\u00b7saat", "im", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Der sechste Kreis umspielt den Strand, des Meeres Flut,", "tokens": ["Der", "sechs\u00b7te", "Kreis", "um\u00b7spielt", "den", "Strand", ",", "des", "Mee\u00b7res", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sicher sich zu Schiff begibt des Landes Gut.", "tokens": ["Wo", "si\u00b7cher", "sich", "zu", "Schiff", "be\u00b7gibt", "des", "Lan\u00b7des", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Der Kreis der siebente zuletzt ums Meer gezogen,", "tokens": ["Der", "Kreis", "der", "sie\u00b7ben\u00b7te", "zu\u00b7letzt", "ums", "Meer", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist, mit Sonn' und Mond geschm\u00fcckt, der Himmelsbogen.", "tokens": ["Das", "ist", ",", "mit", "Sonn'", "und", "Mond", "ge\u00b7schm\u00fcckt", ",", "der", "Him\u00b7mels\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Begl\u00fcckt der K\u00f6nig, der den Mittelthron besitzt,", "tokens": ["Be\u00b7gl\u00fcckt", "der", "K\u00f6\u00b7nig", ",", "der", "den", "Mit\u00b7tel\u00b7thron", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von wo mit Blicken er durch sieben Kreise blitzt.", "tokens": ["Von", "wo", "mit", "Bli\u00b7cken", "er", "durch", "sie\u00b7ben", "Krei\u00b7se", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "APPR", "NN", "PPER", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Von Kreis zu Kreise geht sein Herrscherwort hinaus,", "tokens": ["Von", "Kreis", "zu", "Krei\u00b7se", "geht", "sein", "Herr\u00b7scher\u00b7wort", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wird nicht \u00fcbert\u00f6nt von Wald- und Wogenbraus.", "tokens": ["Und", "wird", "nicht", "\u00fc\u00b7ber\u00b7t\u00f6nt", "von", "Wald", "und", "Wo\u00b7gen\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "VVPP", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Und kommt zum \u00e4u\u00dfersten das Wort zum Himmelsbogen,", "tokens": ["Und", "kommt", "zum", "\u00e4u\u00b7\u00dfers\u00b7ten", "das", "Wort", "zum", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Verneigen schweigend auch sich Sonn' und Mond gewogen.", "tokens": ["Ver\u00b7nei\u00b7gen", "schwei\u00b7gend", "auch", "sich", "Sonn'", "und", "Mond", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PRF", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Der Herrscher m\u00f6cht' ich seyn, und dieser nur allein;", "tokens": ["Der", "Herr\u00b7scher", "m\u00f6cht'", "ich", "seyn", ",", "und", "die\u00b7ser", "nur", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,", "KON", "PDS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn jeder andre scheint mir gar beschr\u00e4nkt und klein.", "tokens": ["Denn", "je\u00b7der", "and\u00b7re", "scheint", "mir", "gar", "be\u00b7schr\u00e4nkt", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "VVFIN", "PPER", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}