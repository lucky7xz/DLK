{"textgrid.poem.46507": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[meine Klagen sollen lieblich wallen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Klagen sollen lieblich wallen,", "tokens": ["Mei\u00b7ne", "Kla\u00b7gen", "sol\u00b7len", "lieb\u00b7lich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den Krystallen gleich im Fr\u00fchlingsbache,", "tokens": ["Den", "Krys\u00b7tal\u00b7len", "gleich", "im", "Fr\u00fch\u00b7lings\u00b7ba\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die mit Ache h\u00fcpfen auf am Strande,", "tokens": ["Die", "mit", "A\u00b7che", "h\u00fcp\u00b7fen", "auf", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo vom Rande sich zwei Blumen neigen", "tokens": ["Wo", "vom", "Ran\u00b7de", "sich", "zwei", "Blu\u00b7men", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "PRF", "CARD", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mit Schweigen sich im heiterblauen", "tokens": ["Und", "mit", "Schwei\u00b7gen", "sich", "im", "hei\u00b7ter\u00b7blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PRF", "APPRART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Spiegel schauen, aber, eingeladen", "tokens": ["Spie\u00b7gel", "schau\u00b7en", ",", "a\u00b7ber", ",", "ein\u00b7ge\u00b7la\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VVINF", "$,", "ADV", "$,", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sich zu baden, scheu zur\u00fcck sich biegen", "tokens": ["Sich", "zu", "ba\u00b7den", ",", "scheu", "zu\u00b7r\u00fcck", "sich", "bie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "ADJD", "PTKVZ", "PRF", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und sich schmiegen, als ob sie sich sch\u00e4men;", "tokens": ["Und", "sich", "schmie\u00b7gen", ",", "als", "ob", "sie", "sich", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "$,", "KOKOM", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Doch mit Gr\u00e4men tr\u00fcben ihren hellen", "tokens": ["Doch", "mit", "Gr\u00e4\u00b7men", "tr\u00fc\u00b7ben", "ih\u00b7ren", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Blick die Wellen, die vor\u00fcber m\u00fcssen,", "tokens": ["Blick", "die", "Wel\u00b7len", ",", "die", "vor\u00b7\u00fc\u00b7ber", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Schmerzlich gr\u00fc\u00dfen sie im Weitereilen,", "tokens": ["Schmerz\u00b7lich", "gr\u00fc\u00b7\u00dfen", "sie", "im", "Wei\u00b7te\u00b7rei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "M\u00f6chten weilen, m\u00fcssen doch entjagen.", "tokens": ["M\u00f6ch\u00b7ten", "wei\u00b7len", ",", "m\u00fcs\u00b7sen", "doch", "ent\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Meine Klagen sollen lieblich wanken,", "tokens": ["Mei\u00b7ne", "Kla\u00b7gen", "sol\u00b7len", "lieb\u00b7lich", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie die Ranken sich am Boden dehnen,", "tokens": ["Wie", "die", "Ran\u00b7ken", "sich", "am", "Bo\u00b7den", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf sich sehnen nach der Lebensflamme,", "tokens": ["Auf", "sich", "seh\u00b7nen", "nach", "der", "Le\u00b7bens\u00b7flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nach dem Stamme, der zum Himmel steiget,", "tokens": ["Nach", "dem", "Stam\u00b7me", ",", "der", "zum", "Him\u00b7mel", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der sich neiget, wenn ihn r\u00fchren linde", "tokens": ["Der", "sich", "nei\u00b7get", ",", "wenn", "ihn", "r\u00fch\u00b7ren", "lin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PRF", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Fr\u00fchlingswinde, doch die stolzen Glieder", "tokens": ["Fr\u00fch\u00b7lings\u00b7win\u00b7de", ",", "doch", "die", "stol\u00b7zen", "Glie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Hebt er wieder, ohne sich der armen", "tokens": ["Hebt", "er", "wie\u00b7der", ",", "oh\u00b7ne", "sich", "der", "ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUI", "PRF", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zu erbarmen, die umsonst sich m\u00fchen", "tokens": ["Zu", "er\u00b7bar\u00b7men", ",", "die", "um\u00b7sonst", "sich", "m\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Aufzubl\u00fchen, jede Luft ben\u00fctzen,", "tokens": ["Auf\u00b7zu\u00b7bl\u00fc\u00b7hen", ",", "je\u00b7de", "Luft", "be\u00b7n\u00fct\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Falsche St\u00fctzen, die sie nur erheben,", "tokens": ["Fal\u00b7sche", "St\u00fct\u00b7zen", ",", "die", "sie", "nur", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Um mit Beben fallen sie zu lassen", "tokens": ["Um", "mit", "Be\u00b7ben", "fal\u00b7len", "sie", "zu", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "NN", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Auf den nassen Grund, wo sie verzagen.", "tokens": ["Auf", "den", "nas\u00b7sen", "Grund", ",", "wo", "sie", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Meine Klagen sollen lieblich st\u00f6hnen", "tokens": ["Mei\u00b7ne", "Kla\u00b7gen", "sol\u00b7len", "lieb\u00b7lich", "st\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gleich den T\u00f6nen holder Nachtigallen,", "tokens": ["Gleich", "den", "T\u00f6\u00b7nen", "hol\u00b7der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die vor allen, Rose, dich zu lieben", "tokens": ["Die", "vor", "al\u00b7len", ",", "Ro\u00b7se", ",", "dich", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "$,", "NE", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sind getrieben, und die Blumenschaaren", "tokens": ["Sind", "ge\u00b7trie\u00b7ben", ",", "und", "die", "Blu\u00b7men\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nicht gewahren, die zu den Ges\u00e4ngen", "tokens": ["Nicht", "ge\u00b7wah\u00b7ren", ",", "die", "zu", "den", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Rings sich dr\u00e4ngen, doch nur dir zum Preise", "tokens": ["Rings", "sich", "dr\u00e4n\u00b7gen", ",", "doch", "nur", "dir", "zum", "Prei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "VVFIN", "$,", "ADV", "ADV", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "T\u00f6nt die Weise: Ros', im Brautgemache", "tokens": ["T\u00f6nt", "die", "Wei\u00b7se", ":", "Ros'", ",", "im", "Braut\u00b7ge\u00b7ma\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "NE", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wach', erwache! Tritt vom Duft der Tr\u00e4ume", "tokens": ["Wach'", ",", "er\u00b7wa\u00b7che", "!", "Tritt", "vom", "Duft", "der", "Tr\u00e4u\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "In die R\u00e4ume, da\u00df die rauhe Erde", "tokens": ["In", "die", "R\u00e4u\u00b7me", ",", "da\u00df", "die", "rau\u00b7he", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Lieblich werde, da\u00df des Todes Bleiche", "tokens": ["Lieb\u00b7lich", "wer\u00b7de", ",", "da\u00df", "des", "To\u00b7des", "Blei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Schamroth weiche, wenn mit Brautgesange", "tokens": ["Scham\u00b7roth", "wei\u00b7che", ",", "wenn", "mit", "Braut\u00b7ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "$,", "KOUS", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Dir die Wange r\u00f6thet unser Schlagen.", "tokens": ["Dir", "die", "Wan\u00b7ge", "r\u00f6\u00b7thet", "un\u00b7ser", "Schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Meine Klagen sollen lieblich wallen,", "tokens": ["Mei\u00b7ne", "Kla\u00b7gen", "sol\u00b7len", "lieb\u00b7lich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den Krystallen gleich im Fr\u00fchlingsbache,", "tokens": ["Den", "Krys\u00b7tal\u00b7len", "gleich", "im", "Fr\u00fch\u00b7lings\u00b7ba\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die mit Ache h\u00fcpfen auf am Strande,", "tokens": ["Die", "mit", "A\u00b7che", "h\u00fcp\u00b7fen", "auf", "am", "Stran\u00b7de", ","