{"dta.poem.2806": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am gr\u00f6\u00dften ist alsdann des Flei\u00dfigen Behagen,", "tokens": ["Am", "gr\u00f6\u00df\u00b7ten", "ist", "als\u00b7dann", "des", "Flei\u00b7\u00dfi\u00b7gen", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er des Tags zuvor hat doppelt eingetragen.", "tokens": ["Wenn", "er", "des", "Tags", "zu\u00b7vor", "hat", "dop\u00b7pelt", "ein\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er freut sich da\u00df er heut nun d\u00fcrfte m\u00fc\u00dfig seyn,", "tokens": ["Er", "freut", "sich", "da\u00df", "er", "heut", "nun", "d\u00fcrf\u00b7te", "m\u00fc\u00b7\u00dfig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in der Freude tr\u00e4gt er wieder doppelt ein.", "tokens": ["Und", "in", "der", "Freu\u00b7de", "tr\u00e4gt", "er", "wie\u00b7der", "dop\u00b7pelt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ"