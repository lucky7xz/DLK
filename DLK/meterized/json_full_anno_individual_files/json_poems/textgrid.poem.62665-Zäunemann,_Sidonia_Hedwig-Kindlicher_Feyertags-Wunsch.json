{"textgrid.poem.62665": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Kindlicher Feyertags-Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes ", "tokens": ["Ge\u00b7lieb\u00b7tes"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die jenen traurig macht, und diesen hoch erfreut?", "tokens": ["Die", "je\u00b7nen", "trau\u00b7rig", "macht", ",", "und", "die\u00b7sen", "hoch", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADJD", "VVFIN", "$,", "KON", "PDAT", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Nebel, Rauch und Wind und Schatten kan auf Erden", "tokens": ["Den", "Ne\u00b7bel", ",", "Rauch", "und", "Wind", "und", "Schat\u00b7ten", "kan", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Zeit an Nichtigkeit und Lauf verglichen werden.", "tokens": ["Die", "Zeit", "an", "Nich\u00b7tig\u00b7keit", "und", "Lauf", "ver\u00b7gli\u00b7chen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df ", "tokens": ["Ich", "wei\u00df"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Mit mir: wie fliegt die Zeit so schnell, so bald dahin!", "tokens": ["Mit", "mir", ":", "wie", "fliegt", "die", "Zeit", "so", "schnell", ",", "so", "bald", "da\u00b7hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Jahr ist wiederum nun bald zu End gegangen,", "tokens": ["Ein", "Jahr", "ist", "wie\u00b7de\u00b7rum", "nun", "bald", "zu", "End", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gott sey gelobt vor das, was wir darin empfangen.", "tokens": ["Gott", "sey", "ge\u00b7lobt", "vor", "das", ",", "was", "wir", "da\u00b7rin", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "APPR", "PDS", "$,", "PRELS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Jahr des Elends wird zugleich zur\u00fcck gelegt.", "tokens": ["Ein", "Jahr", "des", "E\u00b7lends", "wird", "zu\u00b7gleich", "zu\u00b7r\u00fcck", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Christ-Lust, die der Herr in euer Hertz gepr\u00e4gt,", "tokens": ["Die", "Christ\u00b7Lust", ",", "die", "der", "Herr", "in", "eu\u00b7er", "Hertz", "ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die wird Geliebteste! Euch st\u00e4rcken, tr\u00f6sten, gr\u00fcnden,", "tokens": ["Die", "wird", "Ge\u00b7lieb\u00b7tes\u00b7te", "!", "Euch", "st\u00e4r\u00b7cken", ",", "tr\u00f6s\u00b7ten", ",", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$.", "PPER", "VVINF", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df Ihr in dieser Zeit werdt s\u00fcsse Freude finden.", "tokens": ["Da\u00df", "Ihr", "in", "die\u00b7ser", "Zeit", "werdt", "s\u00fcs\u00b7se", "Freu\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So feyret denn vergn\u00fcgt in Gott die\u00df heilge Fest,", "tokens": ["So", "fey\u00b7ret", "denn", "ver\u00b7gn\u00fcgt", "in", "Gott", "die\u00df", "heil\u00b7ge", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVPP", "APPR", "NN", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das uns der Herr zum Heil und Gl\u00fcck erscheinen l\u00e4\u00dft,", "tokens": ["Das", "uns", "der", "Herr", "zum", "Heil", "und", "Gl\u00fcck", "er\u00b7schei\u00b7nen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "APPRART", "NN", "KON", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Legt es gesund zur\u00fcck, und tretet mit Vergn\u00fcgen", "tokens": ["Legt", "es", "ge\u00b7sund", "zu\u00b7r\u00fcck", ",", "und", "tre\u00b7tet", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das Neue Jahr auch an. Der H\u00f6chste wolle f\u00fcgen,", "tokens": ["Das", "Neu\u00b7e", "Jahr", "auch", "an", ".", "Der", "H\u00f6chs\u00b7te", "wol\u00b7le", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$.", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df, ", "tokens": ["Da\u00df", ","], "token_info": ["word", "punct"], "pos": ["KOUS", "$,"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Und seine Lieb und Gnad stets um, und mit Euch geh.", "tokens": ["Und", "sei\u00b7ne", "Lieb", "und", "Gnad", "stets", "um", ",", "und", "mit", "Euch", "geh."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "ADV", "PTKVZ", "$,", "KON", "APPR", "PPER", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Es woll die Allmachtshand Euch sch\u00fctzen und begleiten,", "tokens": ["Es", "woll", "die", "All\u00b7machts\u00b7hand", "Euch", "sch\u00fct\u00b7zen", "und", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und wieder eure Feind und alles Ungl\u00fcck streiten.", "tokens": ["Und", "wie\u00b7der", "eu\u00b7re", "Feind", "und", "al\u00b7les", "Un\u00b7gl\u00fcck", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geliebtes ", "tokens": ["Ge\u00b7lieb\u00b7tes"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die jenen traurig macht, und diesen hoch erfreut?", "tokens": ["Die", "je\u00b7nen", "trau\u00b7rig", "macht", ",", "und", "die\u00b7sen", "hoch", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADJD", "VVFIN", "$,", "KON", "PDAT", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Nebel, Rauch und Wind und Schatten kan auf Erden", "tokens": ["Den", "Ne\u00b7bel", ",", "Rauch", "und", "Wind", "und", "Schat\u00b7ten", "kan", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Zeit an Nichtigkeit und Lauf verglichen werden.", "tokens": ["Die", "Zeit", "an", "Nich\u00b7tig\u00b7keit", "und", "Lauf", "ver\u00b7gli\u00b7chen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text