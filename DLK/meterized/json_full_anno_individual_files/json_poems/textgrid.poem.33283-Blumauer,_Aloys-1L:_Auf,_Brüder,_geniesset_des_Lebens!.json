{"textgrid.poem.33283": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf, Br\u00fcder, geniesset des Lebens!", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, Br\u00fcder, geniesset des Lebens!", "tokens": ["Auf", ",", "Br\u00fc\u00b7der", ",", "ge\u00b7nies\u00b7set", "des", "Le\u00b7bens", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nie winke die Lust euch vergebens;", "tokens": ["Nie", "win\u00b7ke", "die", "Lust", "euch", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Denn wisset, die Freud' ist ein Weib.", "tokens": ["Denn", "wis\u00b7set", ",", "die", "Freud'", "ist", "ein", "Weib", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Sobald wir den Blick von ihr wenden,", "tokens": ["So\u00b7bald", "wir", "den", "Blick", "von", "ihr", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Entschl\u00fcpft sie aus unseren H\u00e4nden;", "tokens": ["Ent\u00b7schl\u00fcpft", "sie", "aus", "un\u00b7se\u00b7ren", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Denn schl\u00fcpfrig wie Aal ist ihr Leib.", "tokens": ["Denn", "schl\u00fcpf\u00b7rig", "wie", "Aal", "ist", "ihr", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "O sey'd, wenn sie winket, nicht bl\u00f6de;", "tokens": ["O", "sey'd", ",", "wenn", "sie", "win\u00b7ket", ",", "nicht", "bl\u00f6\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKNEG", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Denn morgen gew\u00e4hrt euch die Spr\u00f6de", "tokens": ["Denn", "mor\u00b7gen", "ge\u00b7w\u00e4hrt", "euch", "die", "Spr\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nicht mehr, was sie heute verspricht:", "tokens": ["Nicht", "mehr", ",", "was", "sie", "heu\u00b7te", "ver\u00b7spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Doch auch die Gewalt m\u00fc\u00dft ihr meiden;", "tokens": ["Doch", "auch", "die", "Ge\u00b7walt", "m\u00fc\u00dft", "ihr", "mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie kann das Gebieten nicht leiden,", "tokens": ["Sie", "kann", "das", "Ge\u00b7bie\u00b7ten", "nicht", "lei\u00