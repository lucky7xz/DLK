{"textgrid.poem.51293": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weg schn\u00f6de Pracht! ein Sinn, drum du wilt weben,", "tokens": ["Weg", "schn\u00f6\u00b7de", "Pracht", "!", "ein", "Sinn", ",", "drum", "du", "wilt", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "ART", "NN", "$,", "PAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ist nicht frey, mu\u00df stets in F\u00fcrchten schweben,", "tokens": ["Der", "ist", "nicht", "frey", ",", "mu\u00df", "stets", "in", "F\u00fcrch\u00b7ten", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$,", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn wer so hofft, nihmt nicht die Zeit in acht,", "tokens": ["Denn", "wer", "so", "hofft", ",", "nihmt", "nicht", "die", "Zeit", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die ihn bestimmt, und fl\u00fcchtig sich hin macht,", "tokens": ["Die", "ihn", "be\u00b7stimmt", ",", "und", "fl\u00fcch\u00b7tig", "sich", "hin", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "KON", "ADJD", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weg schn\u00f6de Pracht!", "tokens": ["Weg", "schn\u00f6\u00b7de", "Pracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Er wil zwar sehr, doch kan er kein mahl leben,", "tokens": ["Er", "wil", "zwar", "sehr", ",", "doch", "kan", "er", "kein", "mahl", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$,", "ADV", "VMFIN", "PPER", "PIAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wer da wacht, traut fest auff Gott daneben,", "tokens": ["Doch", "wer", "da", "wacht", ",", "traut", "fest", "auff", "Gott", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VVFIN", "ADJD", "APPR", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wei\u00dflich wil nach seinem besten streben,", "tokens": ["Und", "wei\u00df\u00b7lich", "wil", "nach", "sei\u00b7nem", "bes\u00b7ten", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem steht Gott bey, der hat es wol gemacht.", "tokens": ["Dem", "steht", "Gott", "bey", ",", "der", "hat", "es", "wol", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "PTKVZ", "$,", "PRELS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weg schn\u00f6de Pracht!", "tokens": ["Weg", "schn\u00f6\u00b7de", "Pracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Auff dem und dem mit seiner Hoffnung kleben,", "tokens": ["Auff", "dem", "und", "dem", "mit", "sei\u00b7ner", "Hoff\u00b7nung", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "KON", "ART", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Qu\u00e4lt Hertz und Sinn kein' Hoffnung ist zu heben,", "tokens": ["Qu\u00e4lt", "Hertz", "und", "Sinn", "kein'", "Hoff\u00b7nung", "ist", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die auff der Welt nach Eitelkeiten tracht,", "tokens": ["Die", "auff", "der", "Welt", "nach", "Ei\u00b7tel\u00b7kei\u00b7ten", "tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Hoffnung ist, die nur auff Gott bedacht,", "tokens": ["Ein", "Hoff\u00b7nung", "ist", ",", "die", "nur", "auff", "Gott", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weg schn\u00f6de Pracht!", "tokens": ["Weg", "schn\u00f6\u00b7de", "Pracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+", "measure": "i