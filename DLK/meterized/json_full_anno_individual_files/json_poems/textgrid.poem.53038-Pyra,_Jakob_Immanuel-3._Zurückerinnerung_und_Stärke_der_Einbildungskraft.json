{"textgrid.poem.53038": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "3. Zur\u00fcckerinnerung und St\u00e4rke der Einbildungskraft", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein edler Geist der seiner Sinnen H\u00fclfe", "tokens": ["Ein", "ed\u00b7ler", "Geist", "der", "sei\u00b7ner", "Sin\u00b7nen", "H\u00fcl\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht immer n\u00f6thig hat, und der auch ohne sie", "tokens": ["Nicht", "im\u00b7mer", "n\u00f6\u00b7thig", "hat", ",", "und", "der", "auch", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "$,", "KON", "ART", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohl zu empfinden wei\u00df, kan was er will vollbringen.", "tokens": ["Wohl", "zu", "emp\u00b7fin\u00b7den", "wei\u00df", ",", "kan", "was", "er", "will", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$,", "VMFIN", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der zaubernden Gewalt der feuerreichen Kraft", "tokens": ["Der", "zau\u00b7bern\u00b7den", "Ge\u00b7walt", "der", "feu\u00b7er\u00b7rei\u00b7chen", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die tausend Bilder selbst, in unsrer Seele wirket,", "tokens": ["Die", "tau\u00b7send", "Bil\u00b7der", "selbst", ",", "in", "uns\u00b7rer", "See\u00b7le", "wir\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df alles in der Welt so gleich gehorsam seyn.", "tokens": ["Mu\u00df", "al\u00b7les", "in", "der", "Welt", "so", "gleich", "ge\u00b7hor\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie \u00fcberholet selbst durch ihre schnellen Fl\u00fcgel", "tokens": ["Sie", "\u00fc\u00b7berh\u00b7o\u00b7let", "selbst", "durch", "ih\u00b7re", "schnel\u00b7len", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die r\u00e4uberische Flucht der schon entwichnen Zeit,", "tokens": ["Die", "r\u00e4u\u00b7be\u00b7ri\u00b7sche", "Flucht", "der", "schon", "ent\u00b7wich\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Entreisst ihr wiederum, was sie schon in die Tiefe", "tokens": ["E\u00b7ntreisst", "ihr", "wie\u00b7de\u00b7rum", ",", "was", "sie", "schon", "in", "die", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zum Untergang versenkt, wohin sie alles das,", "tokens": ["Zum", "Un\u00b7ter\u00b7gang", "ver\u00b7senkt", ",", "wo\u00b7hin", "sie", "al\u00b7les", "das", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "PPER", "PIS", "PDS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was diese Welt umschliesst, mit schnellem Sturme reisset,", "tokens": ["Was", "die\u00b7se", "Welt", "um\u00b7schliesst", ",", "mit", "schnel\u00b7lem", "Stur\u00b7me", "reis\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und zwingt die Jahre selbst, die ihren Lauf vollbracht,", "tokens": ["Und", "zwingt", "die", "Jah\u00b7re", "selbst", ",", "die", "ih\u00b7ren", "Lauf", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In ihrer Gegenwart die Bahn zu wiederholen.", "tokens": ["In", "ih\u00b7rer", "Ge\u00b7gen\u00b7wart", "die", "Bahn", "zu", "wie\u00b7der\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie ruft die Todten oft aus ihrer Gruft hervor,", "tokens": ["Sie", "ruft", "die", "Tod\u00b7ten", "oft", "aus", "ih\u00b7rer", "Gruft", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Um noch einmal vor ihr den Schauplatz zu betreten.", "tokens": ["Um", "noch", "ein\u00b7mal", "vor", "ihr", "den", "Schau\u00b7platz", "zu", "be\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-++-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Sie geht mit ihnen um, kein Schrecken h\u00e4lt sie ab,", "tokens": ["Sie", "geht", "mit", "ih\u00b7nen", "um", ",", "kein", "Schre\u00b7cken", "h\u00e4lt", "sie", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In der Gesellschaft sich aufs neue zu erg\u00f6tzen.", "tokens": ["In", "der", "Ge\u00b7sell\u00b7schaft", "sich", "aufs", "neu\u00b7e", "zu", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein solcher weiser Geist geniesset bald sein Gl\u00fcck,", "tokens": ["Ein", "sol\u00b7cher", "wei\u00b7ser", "Geist", "ge\u00b7nies\u00b7set", "bald", "sein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "ale