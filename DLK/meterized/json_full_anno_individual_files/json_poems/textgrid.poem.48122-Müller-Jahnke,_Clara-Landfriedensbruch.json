{"textgrid.poem.48122": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Landfriedensbruch", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit ist hart und schwer die Not:", "tokens": ["Die", "Zeit", "ist", "hart", "und", "schwer", "die", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sie k\u00e4mpften um den Bissen Brot,", "tokens": ["sie", "k\u00e4mpf\u00b7ten", "um", "den", "Bis\u00b7sen", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie hielten treu zusammen.", "tokens": ["sie", "hiel\u00b7ten", "treu", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Krone schm\u00fcckt das neue Haus,", "tokens": ["Die", "Kro\u00b7ne", "schm\u00fcckt", "das", "neu\u00b7e", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "doch ihnen blies der Nordwind aus", "tokens": ["doch", "ih\u00b7nen", "blies", "der", "Nord\u00b7wind", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "im Herd die letzten Flammen.", "tokens": ["im", "Herd", "die", "letz\u00b7ten", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hohl heult der Sturm, bang braust das Meer.", "tokens": ["Hohl", "heult", "der", "Sturm", ",", "bang", "braust", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von S\u00fcden trieb der Satan her", "tokens": ["Von", "S\u00fc\u00b7den", "trieb", "der", "Sa\u00b7tan", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "schwarzhaarige Kollegen . . .", "tokens": ["schwarz\u00b7haa\u00b7ri\u00b7ge", "Kol\u00b7le\u00b7gen", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gell scholl der Pfiff der Eisenbahn,", "tokens": ["Gell", "scholl", "der", "Pfiff", "der", "Ei\u00b7sen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "die andern standen, knirschten, sahn", "tokens": ["die", "an\u00b7dern", "stan\u00b7den", ",", "knirschten", ",", "sahn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "dem fremden Tro\u00df entgegen.", "tokens": ["dem", "frem\u00b7den", "Tro\u00df", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der blonde F\u00fchrer strich den Bart.", "tokens": ["Der", "blon\u00b7de", "F\u00fch\u00b7rer", "strich", "den", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbkollege,\u00ab sprach er hei\u00df und hart,", "tokens": ["\u00bb", "kol\u00b7le\u00b7ge", ",", "\u00ab", "sprach", "er", "hei\u00df", "und", "hart", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "$(", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbhier ruhen Stab und Kelle.", "tokens": ["\u00bb", "hier", "ru\u00b7hen", "Stab", "und", "Kel\u00b7le", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Arbeit ruht. In Stundenfrist", "tokens": ["Die", "Ar\u00b7beit", "ruht", ".", "In", "Stun\u00b7den\u00b7frist"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "fahr heim, daher du kommen bist.\u00ab", "tokens": ["fahr", "heim", ",", "da\u00b7her", "du", "kom\u00b7men", "bist", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PAV", "PPER", "VVINF", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da lachte der Geselle.", "tokens": ["Da", "lach\u00b7te", "der", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er h\u00f6hnte: \u00bbKerls, ihr seid wohl toll!\u00ab", "tokens": ["Er", "h\u00f6hn\u00b7te", ":", "\u00bb", "Kerls", ",", "ihr", "seid", "wohl", "toll", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie da das Blut zum Hirne schwoll", "tokens": ["Wie", "da", "das", "Blut", "zum", "Hir\u00b7ne", "schwoll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in j\u00e4h aufflackerndem Brande!", "tokens": ["in", "j\u00e4h", "auf\u00b7fla\u00b7ckern\u00b7dem", "Bran\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbzur\u00fcck!\u00ab \u2013 \u00bbWir m\u00fc\u00dften Narren sein!\u00ab", "tokens": ["\u00bb", "zu\u00b7r\u00fcck", "!", "\u00ab", "\u2013", "\u00bb", "Wir", "m\u00fc\u00df\u00b7ten", "Nar\u00b7ren", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "$(", "$(", "PPER", "VMFIN", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sauste durch die Luft der Stein", "tokens": ["Da", "saus\u00b7te", "durch", "die", "Luft", "der", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "vom deutschen Klippenstrande.", "tokens": ["vom", "deut\u00b7schen", "Klip\u00b7pen\u00b7stran\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und br\u00fcllend wie die Meerflut drang", "tokens": ["Und", "br\u00fcl\u00b7lend", "wie", "die", "Meer\u00b7flut", "drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein Aufschrei rings. Schwer ging und bang", "tokens": ["ein", "Auf\u00b7schrei", "rings", ".", "Schwer", "ging", "und", "bang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "das Atmen der Sekunde . . .", "tokens": ["das", "At\u00b7men", "der", "Se\u00b7kun\u00b7de", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Hunger schrie in schrillem Ton:", "tokens": ["Der", "Hun\u00b7ger", "schrie", "in", "schril\u00b7lem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbum Weib und Kind! Um Brot und Lohn!", "tokens": ["\u00bb", "um", "Weib", "und", "Kind", "!", "Um", "Brot", "und", "Lohn", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "KON", "NN", "$.", "KOUI", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Peitsche f\u00fcr die Hunde!\u00ab", "tokens": ["Die", "Peit\u00b7sche", "f\u00fcr", "die", "Hun\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Schu\u00df aus ", "tokens": ["Ein", "Schu\u00df", "aus"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "zu blutigem Ernste ward das Spiel,", "tokens": ["zu", "blu\u00b7ti\u00b7gem", "Erns\u00b7te", "ward", "das", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Nun h\u00f6hnen sie von First und Schlot,", "tokens": ["Nun", "h\u00f6h\u00b7nen", "sie", "von", "First", "und", "Schlot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die letzte Mark, das letzte Brot,", "tokens": ["die", "letz\u00b7te", "Mark", ",", "das", "letz\u00b7te", "Brot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "sie haben es uns genommen!", "tokens": ["sie", "ha\u00b7ben", "es", "uns", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und als die Luft voll Christnachtsruch,", "tokens": ["Und", "als", "die", "Luft", "voll", "Christ\u00b7nachts\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da standen wegen Friedensbruch", "tokens": ["da", "stan\u00b7den", "we\u00b7gen", "Frie\u00b7dens\u00b7bruch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Treuen vor den Schranken.", "tokens": ["die", "Treu\u00b7en", "vor", "den", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da traf sie hart der Richterspruch,", "tokens": ["Da", "traf", "sie", "hart", "der", "Rich\u00b7ter\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da trugen sie in Nacht und Fluch", "tokens": ["da", "tru\u00b7gen", "sie", "in", "Nacht", "und", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die tobenden Gedanken . . .", "tokens": ["die", "to\u00b7ben\u00b7den", "Ge\u00b7dan\u00b7ken", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbum Friedensbruch?! O, Weib und Kind,", "tokens": ["\u00bb", "um", "Frie\u00b7dens\u00b7bruch", "?!", "O", ",", "Weib", "und", "Kind", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "wor