{"dta.poem.18553": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A uff ihren  G eburths-  \n Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Mein schlechter Kiel sol dich/ Annehmlichste! bedienen/", "tokens": ["Mein", "schlech\u00b7ter", "Kiel", "sol", "dich", "/", "An\u00b7nehm\u00b7lichs\u00b7te", "!", "be\u00b7die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "$(", "NE", "$.", "VVINF", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da dein beliebtes Licht in vollen Glantze strahlt.", "tokens": ["Da", "dein", "be\u00b7lieb\u00b7tes", "Licht", "in", "vol\u00b7len", "Glant\u00b7ze", "strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein gn\u00e4diger Befehl gebiehret des Erk\u00fchnen/", "tokens": ["Dein", "gn\u00e4\u00b7di\u00b7ger", "Be\u00b7fehl", "ge\u00b7bieh\u00b7ret", "des", "Er\u00b7k\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und mein G", "tokens": ["Und", "mein", "G"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Mein Wollen wolte nun wohl was geschicktes senden/", "tokens": ["Mein", "Wol\u00b7len", "wol\u00b7te", "nun", "wohl", "was", "ge\u00b7schick\u00b7tes", "sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "PWS", "ADJA", "NN", "$("], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Die weil dein kluger Geist die Poesie verehrt;", "tokens": ["Die", "weil", "dein", "klu\u00b7ger", "Geist", "die", "Poe\u00b7sie", "ver\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Allein nimm dieses auch mit hochgeneigten H\u00e4nden", "tokens": ["Al\u00b7lein", "nimm", "die\u00b7ses", "auch", "mit", "hoch\u00b7ge\u00b7neig\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PDAT", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was hier mein Sinnen-Schiff von Unverstande f\u00e4hrt.", "tokens": ["Was", "hier", "mein", "Sin\u00b7nen\u00b7Schiff", "von", "Un\u00b7ver\u00b7stan\u00b7de", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So bald ich diesen Tag in deinen Brieff erblicket/", "tokens": ["So", "bald", "ich", "die\u00b7sen", "Tag", "in", "dei\u00b7nen", "Brieff", "er\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An welchen die Natur ein Meister St\u00fcck gemacht/", "tokens": ["An", "wel\u00b7chen", "die", "Na\u00b7tur", "ein", "Meis\u00b7ter", "St\u00fcck", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "War die gequ\u00e4lte Brust auch wiederum erquicket/", "tokens": ["War", "die", "ge\u00b7qu\u00e4l\u00b7te", "Brust", "auch", "wie\u00b7de\u00b7rum", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die ihre Zeit bi\u00dfher in Thr\u00e4nen zugebracht.", "tokens": ["Die", "ih\u00b7re", "Zeit", "bi\u00df\u00b7her", "in", "Thr\u00e4\u00b7nen", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der h\u00f6chstegl\u00fcckte Tag verbannet alle Plagen/", "tokens": ["Der", "h\u00f6chs\u00b7te\u00b7gl\u00fcck\u00b7te", "Tag", "ver\u00b7ban\u00b7net", "al\u00b7le", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Sonne machet mich durch ihren Strahl vergn\u00fcgt.", "tokens": ["Die", "Son\u00b7ne", "ma\u00b7chet", "mich", "durch", "ih\u00b7ren", "Strahl", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich kan nun wiederum von tausend Freuden sagen/", "tokens": ["Ich", "kan", "nun", "wie\u00b7de\u00b7rum", "von", "tau\u00b7send", "Freu\u00b7den", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil mein Ergetzen blo\u00df in deiner Wollust liegt.", "tokens": ["Weil", "mein", "Er\u00b7get\u00b7zen", "blo\u00df", "in", "dei\u00b7ner", "Wol\u00b7lust", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Sinnen m\u00fcssen sich mit deiner Sonne gatten/", "tokens": ["Die", "Sin\u00b7nen", "m\u00fcs\u00b7sen", "sich", "mit", "dei\u00b7ner", "Son\u00b7ne", "gat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die mir bey klarer Lufft ein Traum im wachen weist.", "tokens": ["Die", "mir", "bey", "kla\u00b7rer", "Lufft", "ein", "Traum", "im", "wa\u00b7chen", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Augen sehen zwar die Mohren-Farbe Schatten/", "tokens": ["Die", "Au\u00b7gen", "se\u00b7hen", "zwar", "die", "Moh\u00b7ren\u00b7Fa\u00b7rbe", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch dein Schnee weisser Schmuck umarmet meinen Geist.", "tokens": ["Doch", "dein", "Schnee", "weis\u00b7ser", "Schmuck", "um\u00b7ar\u00b7met", "mei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.21": {"text": "So spielte Phantasie und Lust bey mir zusammen/", "tokens": ["So", "spiel\u00b7te", "Phan\u00b7ta\u00b7sie", "und", "Lust", "bey", "mir", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Gleich/ als der Augenblick so s\u00fcsse Post gebracht.", "tokens": ["Gleich", "/", "als", "der", "Au\u00b7gen\u00b7blick", "so", "s\u00fcs\u00b7se", "Post", "ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Aurora blitzte schon mit ihren g\u00fcldnen Flammen/", "tokens": ["Au\u00b7ro\u00b7ra", "blitz\u00b7te", "schon", "mit", "ih\u00b7ren", "g\u00fcld\u00b7nen", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und mein vergn\u00fcgter Sinn war noch auf dich bedacht.", "tokens": ["Und", "mein", "ver\u00b7gn\u00fcg\u00b7ter", "Sinn", "war", "noch", "auf", "dich", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Hier sprach mein treuer Geist: der Morgen-R\u00f6hte prangen", "tokens": ["Hier", "sprach", "mein", "treu\u00b7er", "Geist", ":", "der", "Mor\u00b7gen\u00b7R\u00f6h\u00b7te", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ist nur ein Schatten-Werck vor Dulcimenens Glantz/", "tokens": ["Ist", "nur", "ein", "Schat\u00b7ten\u00b7\u00b7Werck", "vor", "Dul\u00b7ci\u00b7me\u00b7nens", "Glantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Scharlach steht besch\u00e4mt bey ihren Purpur-Wangen/", "tokens": ["Der", "Schar\u00b7lach", "steht", "be\u00b7sch\u00e4mt", "bey", "ih\u00b7ren", "Pur\u00b7pur\u00b7Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vor ihren Wunder-Strahl erblast der Sternen-Krantz.", "tokens": ["Vor", "ih\u00b7ren", "Wun\u00b7der\u00b7Strahl", "er\u00b7blast", "der", "Ster\u00b7nen\u00b7Krantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Anmuht blitzetihr weit sch\u00f6ner aus den Augen/", "tokens": ["Die", "An\u00b7muht", "blit\u00b7ze\u00b7tihr", "weit", "sch\u00f6\u00b7ner", "aus", "den", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als aller Erden-Aug' ein sterblicher erblickt.", "tokens": ["Als", "al\u00b7ler", "Er\u00b7den\u00b7Aug'", "ein", "sterb\u00b7li\u00b7cher", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Sonne mu\u00df aus ihr die Krafft/ wie Luna saugen/", "tokens": ["Die", "Son\u00b7ne", "mu\u00df", "aus", "ihr", "die", "Krafft", "/", "wie", "Lu\u00b7na", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "ART", "NN", "$(", "KOKOM", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn sich ihr blasser Schein mit fremden Lichte schm\u00fcckt.", "tokens": ["Wenn", "sich", "ihr", "blas\u00b7ser", "Schein", "mit", "frem\u00b7den", "Lich\u00b7te", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So wie des Ph\u00f6bus Blitz durch dunckle Wolcken dringet/", "tokens": ["So", "wie", "des", "Ph\u00f6\u00b7bus", "Blitz", "durch", "dunck\u00b7le", "Wol\u00b7cken", "drin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So bricht ihr Sch\u00f6nheits-Strahl aus schwartzer Augen-", "tokens": ["So", "bricht", "ihr", "Sch\u00f6n\u00b7heits\u00b7Strahl", "aus", "schwart\u00b7zer", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Pracht:", "tokens": ["Pracht", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.36": {"text": "Denn da\u00df er alle Welt \u00fcm ihre Freyheit bringet/", "tokens": ["Denn", "da\u00df", "er", "al\u00b7le", "Welt", "\u00fcm", "ih\u00b7re", "Frey\u00b7heit", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "APPRART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So kommt der schlaue Dieb in Kohl-gef\u00e4rbter Nacht.", "tokens": ["So", "kommt", "der", "schlau\u00b7e", "Dieb", "in", "Kohl\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als meine Sinnen nun an deinen Sch\u00e4tzen hingen/", "tokens": ["Als", "mei\u00b7ne", "Sin\u00b7nen", "nun", "an", "dei\u00b7nen", "Sch\u00e4t\u00b7zen", "hin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wie Apelles Aug' ein Wunder-Bild besahn/", "tokens": ["Und", "wie", "A\u00b7pel\u00b7les", "Aug'", "ein", "Wun\u00b7der\u00b7Bild", "be\u00b7sahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn seines Pinsels-Kunst soll was zu Marckte bringen/", "tokens": ["Wenn", "sei\u00b7nes", "Pin\u00b7sels\u00b7Kunst", "soll", "was", "zu", "Marck\u00b7te", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "PIS", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das als ein Meisterst\u00fcck ein jeder mu\u00df bejahn:", "tokens": ["Das", "als", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "ein", "je\u00b7der", "mu\u00df", "be\u00b7jahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOUS", "ART", "NN", "ART", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So wolte Zephier mir in die Begierden wehen/", "tokens": ["So", "wol\u00b7te", "Ze\u00b7phier", "mir", "in", "die", "Be\u00b7gier\u00b7den", "we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und lockte mich noch Fr\u00fch durch seinen k\u00fchlen Wind/", "tokens": ["Und", "lock\u00b7te", "mich", "noch", "Fr\u00fch", "durch", "sei\u00b7nen", "k\u00fch\u00b7len", "Wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", 