{"dta.poem.13611": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckblick.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt' im Walde dichten", "tokens": ["Ich", "wollt'", "im", "Wal\u00b7de", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Heldenlied voll Pracht,", "tokens": ["Ein", "Hel\u00b7den\u00b7lied", "voll", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verwickelte Geschichten", "tokens": ["Ver\u00b7wi\u00b7ckel\u00b7te", "Ge\u00b7schich\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Recht sinnreich ausgedacht.", "tokens": ["Recht", "sinn\u00b7reich", "aus\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da rauschten B\u00e4ume, sprangen", "tokens": ["Da", "rauschten", "B\u00e4u\u00b7me", ",", "spran\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "VVFIN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Vom Fels die B\u00e4che drein,", "tokens": ["Vom", "Fels", "die", "B\u00e4\u00b7che", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und tausend Stimmen klangen", "tokens": ["Und", "tau\u00b7send", "Stim\u00b7men", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verwirrend aus und ein.", "tokens": ["Ver\u00b7wir\u00b7rend", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und manches Jauchzen schallen", "tokens": ["Und", "man\u00b7ches", "Jauch\u00b7zen", "schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Lie\u00df ich aus frischer Brust,", "tokens": ["Lie\u00df", "ich", "aus", "fri\u00b7scher", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Doch aus den Helden allen", "tokens": ["Doch", "aus", "den", "Hel\u00b7den", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PIAT"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "Ward nichts vor tiefer Lust.", "tokens": ["Ward", "nichts", "vor", "tie\u00b7fer", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kehr ich zur Stadt erst wieder", "tokens": ["Kehr", "ich", "zur", "Stadt", "erst", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus Feld und W\u00e4ldern k\u00fchl,", "tokens": ["Aus", "Feld", "und", "W\u00e4l\u00b7dern", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kommen all' die Lieder", "tokens": ["Da", "kom\u00b7men", "all'", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von fern durchs Weltgew\u00fchl,", "tokens": ["Von", "fern", "durchs", "Welt\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hallen Lust und Schmerzen", "tokens": ["Es", "hal\u00b7len", "Lust", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch einmal leise nach,", "tokens": ["Noch", "ein\u00b7mal", "lei\u00b7se", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bildend wird im Herzen", "tokens": ["Und", "bil\u00b7dend", "wird", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die alte Wehmuth wach,", "tokens": ["Die", "al\u00b7te", "Weh\u00b7muth", "wach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der Winter auch derweile", "tokens": ["Der", "Win\u00b7ter", "auch", "der\u00b7wei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Im Feld die Blumen bricht \u2014", "tokens": ["Im", "Feld", "die", "Blu\u00b7men", "bricht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dann giebt's vor Langerweile", "tokens": ["Dann", "giebt's", "vor", "Lan\u00b7ger\u00b7wei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ein \u00fcberlang Gedicht!", "tokens": ["Ein", "\u00fc\u00b7berl\u00b7ang", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}