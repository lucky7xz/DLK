{"textgrid.poem.46242": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das gold des Morenlands, wie pur es auch kan sein,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das gold des Morenlands, wie pur es auch kan sein,", "tokens": ["Das", "gold", "des", "Mo\u00b7ren\u00b7lands", ",", "wie", "pur", "es", "auch", "kan", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mu\u00df ihres krausen haars k\u00f6stlichem schimmer weichen:", "tokens": ["mu\u00df", "ih\u00b7res", "krau\u00b7sen", "haars", "k\u00f6st\u00b7li\u00b7chem", "schim\u00b7mer", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADJA", "ADJD", "VVINF", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "der roteste koral, des sch\u00f6nsten rubins schein", "tokens": ["der", "ro\u00b7tes\u00b7te", "ko\u00b7ral", ",", "des", "sch\u00f6ns\u00b7ten", "ru\u00b7bins", "schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ist ihres rosenmunds reichtum nicht zu vergleichen;", "tokens": ["ist", "ih\u00b7res", "ro\u00b7sen\u00b7munds", "reich\u00b7tum", "nicht", "zu", "ver\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "FM", "FM", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und keine perlein seind so wei\u00df, so gleich, so rein", "tokens": ["Und", "kei\u00b7ne", "per\u00b7lein", "seind", "so", "wei\u00df", ",", "so", "gleich", ",", "so", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "VVFIN", "$,", "ADV", "ADV", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "als die, die ihres munds red und geschm\u00f6ll bereichen:", "tokens": ["als", "die", ",", "die", "ih\u00b7res", "munds", "red", "und", "ge\u00b7schm\u00f6ll", "be\u00b7rei\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "so kan auch die natur und kunst kein helfenbein", "tokens": ["so", "kan", "auch", "die", "na\u00b7tur", "und", "kunst", "kein", "hel\u00b7fen\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "KON", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "das so zart, glat und wei\u00df wie ihr leib, herausstreichen.", "tokens": ["das", "so", "zart", ",", "glat", "und", "wei\u00df", "wie", "ihr", "leib", ",", "her\u00b7aus\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "KOKOM", "PPOSAT", "NN", "$,", "VVIZU", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Kurz, meine Nymf, Myrt, ist ein kunststuck der natur,", "tokens": ["Kurz", ",", "mei\u00b7ne", "Nymf", ",", "Myrt", ",", "ist", "ein", "kunst\u00b7stuck", "der", "na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "der herzen brunst und wunsch, die herscherin der seelen,", "tokens": ["der", "her\u00b7zen", "brunst", "und", "wunsch", ",", "die", "her\u00b7sche\u00b7rin", "der", "see\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJD", "$,", "PRELS", "ADV", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der holdseligkeit quell, der lieblichkeit figur,", "tokens": ["der", "hold\u00b7se\u00b7lig\u00b7keit", "quell", ",", "der", "lieb\u00b7lich\u00b7keit", "fi\u00b7gur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der augen s\u00fc\u00dfe weid, die todte zu beseelen,", "tokens": ["Der", "au\u00b7gen", "s\u00fc\u00b7\u00dfe", "weid", ",", "die", "tod\u00b7te", "zu", "be\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "der sch\u00f6nheit ganze sum, der tugenden richtschnur,", "tokens": ["der", "sch\u00f6n\u00b7heit", "gan\u00b7ze", "sum", ",", "der", "tu\u00b7gen\u00b7den", "richt\u00b7schnur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "