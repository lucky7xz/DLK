{"dta.poem.14716": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Unbekannte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und wie sich k\u00fchler nun die Schatten breiten,", "tokens": ["Und", "wie", "sich", "k\u00fch\u00b7ler", "nun", "die", "Schat\u00b7ten", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ADJD", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Berg Vesuv, der \u00fcber Tr\u00fcmmern raucht,", "tokens": ["Vom", "Berg", "Ve\u00b7suv", ",", "der", "\u00fc\u00b7ber", "Tr\u00fcm\u00b7mern", "raucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom blauen Meer, wo Schw\u00e4ne singend gleiten,", "tokens": ["Vom", "blau\u00b7en", "Meer", ",", "wo", "Schw\u00e4\u00b7ne", "sin\u00b7gend", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Krystall'nen Inseln, bl\u00fchend draus getaucht,", "tokens": ["Kry\u00b7stall'\u00b7nen", "In\u00b7seln", ",", "bl\u00fc\u00b7hend", "draus", "ge\u00b7taucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Glocken, die im Meeresgrunde schlagen,", "tokens": ["Und", "Glo\u00b7cken", ",", "die", "im", "Mee\u00b7res\u00b7grun\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wu\u00dft' wunderbar der sch\u00f6ne Gast zu sagen.", "tokens": ["Wu\u00dft'", "wun\u00b7der\u00b7bar", "der", "sch\u00f6\u00b7ne", "Gast", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}