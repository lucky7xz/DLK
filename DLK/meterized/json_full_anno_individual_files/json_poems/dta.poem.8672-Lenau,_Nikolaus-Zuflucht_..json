{"dta.poem.8672": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Zuflucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.85", "nl:0.14"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Thut man Kindern was zu Leide,", "tokens": ["Thut", "man", "Kin\u00b7dern", "was", "zu", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "NN", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flieh'n zur Mutter sie voll Schrecken,", "tokens": ["Flieh'n", "zur", "Mut\u00b7ter", "sie", "voll", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich in ihrem Faltenkleide", "tokens": ["Sich", "in", "ih\u00b7rem", "Fal\u00b7ten\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Qu\u00e4ler zu verstecken.", "tokens": ["Vor", "dem", "Qu\u00e4\u00b7ler", "zu", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weiche Herzen bleiben Kinder", "tokens": ["Wei\u00b7che", "Her\u00b7zen", "blei\u00b7ben", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All' ihr Leben lang, d'rum falle", "tokens": ["All'", "ihr", "Le\u00b7ben", "lang", ",", "d'\u00b7rum", "fal\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "ADJD", "$,", "PAV", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihnen auch das Loos gelinder,", "tokens": ["Ih\u00b7nen", "auch", "das", "Loos", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als den Herzen von Metalle.", "tokens": ["Als", "den", "Her\u00b7zen", "von", "Me\u00b7tal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jagt sie Ungl\u00fcck, wie zum Fluche,", "tokens": ["Jagt", "sie", "Un\u00b7gl\u00fcck", ",", "wie", "zum", "Flu\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flieh'n sie bang und immer b\u00e4nger,", "tokens": ["Flieh'n", "sie", "bang", "und", "im\u00b7mer", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis sie hinter'm Leichentuche", "tokens": ["Bis", "sie", "hin\u00b7ter'm", "Lei\u00b7chen\u00b7tu\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich verbergen ihrem Dr\u00e4nger.", "tokens": ["Sich", "ver\u00b7ber\u00b7gen", "ih\u00b7rem", "Dr\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}