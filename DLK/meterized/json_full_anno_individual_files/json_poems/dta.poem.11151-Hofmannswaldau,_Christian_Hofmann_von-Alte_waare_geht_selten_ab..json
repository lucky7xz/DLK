{"dta.poem.11151": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Alte waare geht selten ab.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mops hat ein sch\u00f6nes weib; er mu\u00df sie selbsten loben,", "tokens": ["Mops", "hat", "ein", "sch\u00f6\u00b7nes", "weib", ";", "er", "mu\u00df", "sie", "selbs\u00b7ten", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von hinten und von forn, von unten an bi\u00df oben,", "tokens": ["Von", "hin\u00b7ten", "und", "von", "forn", ",", "von", "un\u00b7ten", "an", "bi\u00df", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "APPR", "NE", "$,", "APPR", "ADV", "APPR", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df mans glauben soll, so schw\u00f6rt er stein und bein,", "tokens": ["Und", "da\u00df", "mans", "glau\u00b7ben", "soll", ",", "so", "schw\u00f6rt", "er", "stein", "und", "bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVINF", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn es ans tauschen geht, woll\u2019 er der erste seyn.", "tokens": ["Wenn", "es", "ans", "tau\u00b7schen", "geht", ",", "woll'", "er", "der", "ers\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VVFIN", "$,", "VMFIN", "PPER", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}