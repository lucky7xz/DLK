{"textgrid.poem.45847": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df ein Kind ganz still zu meinen F\u00fc\u00dfen,", "tokens": ["Es", "sa\u00df", "ein", "Kind", "ganz", "still", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und spielte froh mit freundlichen Gedanken,", "tokens": ["Und", "spiel\u00b7te", "froh", "mit", "freund\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es blickt mich an, bis ihm die Blicke sanken,", "tokens": ["Es", "blickt", "mich", "an", ",", "bis", "ihm", "die", "Bli\u00b7cke", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und goldne ferne Lande sich erschlie\u00dfen,", "tokens": ["Und", "gold\u00b7ne", "fer\u00b7ne", "Lan\u00b7de", "sich", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von allen Seiten dringt ein s\u00fc\u00dfes Gr\u00fc\u00dfen,", "tokens": ["Von", "al\u00b7len", "Sei\u00b7