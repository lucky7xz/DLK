{"textgrid.poem.36039": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Weihe.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Leipzig in Auerbachs Keller sass einer still und trank,", "tokens": ["Zu", "Leip\u00b7zig", "in", "Au\u00b7er\u00b7bachs", "Kel\u00b7ler", "sass", "ei\u00b7ner", "still", "und", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "NE", "VVFIN", "ART", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+----+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und in der Vorzeit Tage sein sinnender Blick versank.", "tokens": ["Und", "in", "der", "Vor\u00b7zeit", "Ta\u00b7ge", "sein", "sin\u00b7nen\u00b7der", "Blick", "ver\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zu Leipzig in Auerbachs Keller ist traun, ein rechter Ort,", "tokens": ["Zu", "Leip\u00b7zig", "in", "Au\u00b7er\u00b7bachs", "Kel\u00b7ler", "ist", "traun", ",", "ein", "rech\u00b7ter", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "NE", "VAFIN", "VVINF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Der alten Sagen zu denken, der Wunder verklungnem Wort.", "tokens": ["Der", "al\u00b7ten", "Sa\u00b7gen", "zu", "den\u00b7ken", ",", "der", "Wun\u00b7der", "ver\u00b7klung\u00b7nem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Er sass im kleinen Gew\u00f6lbe, war drin der einzige Gast;", "tokens": ["Er", "sass", "im", "klei\u00b7nen", "Ge\u00b7w\u00f6l\u00b7be", ",", "war", "drin", "der", "ein\u00b7zi\u00b7ge", "Gast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihm fiel kein leeres Geplauder, ja, keine Seele zur Last.", "tokens": ["Ihm", "fiel", "kein", "lee\u00b7res", "Ge\u00b7plau\u00b7der", ",", "ja", ",", "kei\u00b7ne", "See\u00b7le", "zur", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,", "PTKANT", "$,", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bald sah er in den R\u00f6mer, voll \u00e4chten Burgunder klar;", "tokens": ["Bald", "sah", "er", "in", "den", "R\u00f6\u00b7mer", ",", "voll", "\u00e4ch\u00b7ten", "Bur\u00b7gun\u00b7der", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bald auf uralte Bilder, gedunkelt und wunderbar.", "tokens": ["Bald", "auf", "ur\u00b7al\u00b7te", "Bil\u00b7der", ",", "ge\u00b7dun\u00b7kelt", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort stehn dreihundert Jahre zwei Bogen, gerundet stolz;", "tokens": ["Dort", "stehn", "drei\u00b7hun\u00b7dert", "Jah\u00b7re", "zwei", "Bo\u00b7gen", ",", "ge\u00b7run\u00b7det", "stolz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "CARD", "NN", "$,", "VVPP", "ADJD", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von Meisterhand gemalet sind Bilder auf das Holz,", "tokens": ["Von", "Meis\u00b7ter\u00b7hand", "ge\u00b7ma\u00b7let", "sind", "Bil\u00b7der", "auf", "das", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wenn Du sie recht lange mit stillem Geist beschaut,", "tokens": ["Und", "wenn", "Du", "sie", "recht", "lan\u00b7ge", "mit", "stil\u00b7lem", "Geist", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Spricht wohl lebendges Leben aus den zwei Bildern laut.", "tokens": ["Spricht", "wohl", "le\u00b7bend\u00b7ges", "Le\u00b7ben", "aus", "den", "zwei", "Bil\u00b7dern", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "APPR", "ART", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Dort sitzt der wackre Doktor, umher die Sch\u00fcler sein;", "tokens": ["Dort", "sitzt", "der", "wack\u00b7re", "Dok\u00b7tor", ",", "um\u00b7her", "die", "Sch\u00fc\u00b7ler", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUI", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erst las er ein Kollegium wohl \u00fcber den edlen Wein.", "tokens": ["Erst", "las", "er", "ein", "Kol\u00b7le\u00b7gi\u00b7um", "wohl", "\u00fc\u00b7ber", "den", "ed\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+---+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann nahm er seine Hefte, warf jedes an die Wand,", "tokens": ["Dann", "nahm", "er", "sei\u00b7ne", "Hef\u00b7te", ",", "warf", "je\u00b7des", "an", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und alle Studenten nahmen die vollen R\u00f6mer zur Hand.", "tokens": ["Und", "al\u00b7le", "Stu\u00b7den\u00b7ten", "nah\u00b7men", "die", "vol\u00b7len", "R\u00f6\u00b7mer", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Dann klang's aus ihrem Munde: ", "tokens": ["Dann", "klang's", "aus", "ih\u00b7rem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So sangen die lustgen Studenten, hell klang der Pokale Rand;", "tokens": ["So", "san\u00b7gen", "die", "lust\u00b7gen", "Stu\u00b7den\u00b7ten", ",", "hell", "klang", "der", "Po\u00b7ka\u00b7le", "Rand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+--++--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Es waren auch fahrende Spielleut' im Keller, aus B\u00f6hmenland.", "tokens": ["Es", "wa\u00b7ren", "auch", "fah\u00b7ren\u00b7de", "Spiel\u00b7leut'", "im", "Kel\u00b7ler", ",", "aus", "B\u00f6h\u00b7men\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "APPRART", "NN", "$,", "APPR", "NE", "$."], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Die r\u00fchrten gar wacker die Saiten, dass es den Keller durchdrang;", "tokens": ["Die", "r\u00fchr\u00b7ten", "gar", "wa\u00b7cker", "die", "Sai\u00b7ten", ",", "dass", "es", "den", "Kel\u00b7ler", "durch\u00b7drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Man h\u00f6rte sie fr\u00f6hlich begleiten den schallenden hallenden Sang. \u2013", "tokens": ["Man", "h\u00f6r\u00b7te", "sie", "fr\u00f6h\u00b7lich", "be\u00b7glei\u00b7ten", "den", "schal\u00b7len\u00b7den", "hal\u00b7len\u00b7den", "Sang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}}, "stanza.7": {"line.1": {"text": "Und auf dem zweiten der Bilder da f\u00e4hrt der k\u00fchne Patron,", "tokens": ["Und", "auf", "dem", "zwei\u00b7ten", "der", "Bil\u00b7der", "da", "f\u00e4hrt", "der", "k\u00fch\u00b7ne", "Pat\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der zauberm\u00e4chtige Faustus auf einem Fass davon.", "tokens": ["Der", "zau\u00b7ber\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Faus\u00b7tus", "auf", "ei\u00b7nem", "Fass", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Auf einem Fass voll Weines; wie seltsam klingt doch das?", "tokens": ["Auf", "ei\u00b7nem", "Fass", "voll", "Wei\u00b7nes", ";", "wie", "selt\u00b7sam", "klingt", "doch", "das", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$.", "PWAV", "ADJD", "VVFIN", "ADV", "PDS", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein schwarzes H\u00fcndlein, ein kleines, zieht Faustum sammt dem Fass. \u2013", "tokens": ["Ein", "schwar\u00b7zes", "H\u00fcnd\u00b7lein", ",", "ein", "klei\u00b7nes", ",", "zieht", "Faus\u00b7tum", "sammt", "dem", "Fass", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "NE", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Je mehr Du die Bilder betrachtet, je herrlicher scheinen sie Dir,", "tokens": ["Je", "mehr", "Du", "die", "Bil\u00b7der", "be\u00b7trach\u00b7tet", ",", "je", "herr\u00b7li\u00b7cher", "schei\u00b7nen", "sie", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "VVPP", "$,", "ADV", "ADJA", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "In farbenprangender Sch\u00f6ne des Auerbachkellers Zier.", "tokens": ["In", "far\u00b7ben\u00b7pran\u00b7gen\u00b7der", "Sch\u00f6\u00b7ne", "des", "Au\u00b7er\u00b7bach\u00b7kel\u00b7lers", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zuletzt gar werden lebendig die alten Gesichter dort,", "tokens": ["Zu\u00b7letzt", "gar", "wer\u00b7den", "le\u00b7ben\u00b7dig", "die", "al\u00b7ten", "Ge\u00b7sich\u00b7ter", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADJD", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+---+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da kommt Dir mancher Gedanke, dass ", "tokens": ["Da", "kommt", "Dir", "man\u00b7cher", "Ge\u00b7dan\u00b7ke", ",", "dass"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "KOUS"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein redekundger Professor in Auerbachs Keller haust,", "tokens": ["Ein", "re\u00b7de\u00b7kund\u00b7ger", "Pro\u00b7fes\u00b7sor", "in", "Au\u00b7er\u00b7bachs", "Kel\u00b7ler", "haust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NE", "VVFIN", "$,"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.3": {"text": "Der stets zur guten Stunde geheime Weisheit erschleusst,", "tokens": ["Der", "stets", "zur", "gu\u00b7ten", "Stun\u00b7de", "ge\u00b7hei\u00b7me", "Weis\u00b7heit", "er\u00b7schleusst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und dass im Schoos der F\u00e4sser ein ", "tokens": ["Und", "dass", "im", "Schoos", "der", "F\u00e4s\u00b7ser", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sei Goldbach mir gepriesen, Du Schl\u00fcssel zum Geisterreich,", "tokens": ["Sei", "Gold\u00b7bach", "mir", "ge\u00b7prie\u00b7sen", ",", "Du", "Schl\u00fcs\u00b7sel", "zum", "Geis\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$,", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+---+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Pforten des Lichtes aufthut, dem Salomons-Schl\u00fcssel gleich.", "tokens": ["Der", "Pfor\u00b7ten", "des", "Lich\u00b7tes", "auf\u00b7thut", ",", "dem", "Sa\u00b7lo\u00b7mons\u00b7Sch\u00b7l\u00fcs\u00b7sel", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+--+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du bringst auf goldner Welle, was grave Vorzeit sah,", "tokens": ["Du", "bringst", "auf", "gold\u00b7ner", "Wel\u00b7le", ",", "was", "gra\u00b7ve", "Vor\u00b7zeit", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In magischholder Helle dem Seher wieder nah! \u2013", "tokens": ["In", "ma\u00b7gischhol\u00b7der", "Hel\u00b7le", "dem", "Se\u00b7her", "wie\u00b7der", "nah", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Zu Leipzig in Auerbachs Keller sass einer still und trank,", "tokens": ["Zu", "Leip\u00b7zig", "in", "Au\u00b7er\u00b7bachs", "Kel\u00b7ler", "sass", "ei\u00b7ner", "still", "und", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "NE", "VVFIN", "ART", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+----+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und in der Vorzeit Tage sein sinnender Blick versank.", "tokens": ["Und", "in", "der", "Vor\u00b7zeit", "Ta\u00b7ge", "sein", "sin\u00b7nen\u00b7der", "Blick", "ver\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zu Leipzig in Auerbachs Keller ist traun, ein rechter Ort,", "tokens": ["Zu", "Leip\u00b7zig", "in", "Au\u00b7er\u00b7bachs"