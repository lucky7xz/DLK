{"dta.poem.3806": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungs-Schreiben,  \n An einen Freund, der 1742 auf die Universit\u00e4t gieng.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ansch\u00e4tzbar grosser Freund! nimm mit geneigter", "tokens": ["An\u00b7sch\u00e4tz\u00b7bar", "gros\u00b7ser", "Freund", "!", "nimm", "mit", "ge\u00b7neig\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$.", "VVIMP", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hand", "tokens": ["Hand"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ein schlecht doch redlich Blatt, der Ehrfurcht Un-", "tokens": ["Ein", "schlecht", "doch", "red\u00b7lich", "Blatt", ",", "der", "Ehr\u00b7furcht", "Un"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ADJD", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "terpfand.", "tokens": ["ter\u00b7pfand", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Doch, liebt etwa dein Blick nur Anmuths-volle", "tokens": ["Doch", ",", "liebt", "et\u00b7wa", "dein", "Blick", "nur", "An\u00b7muths\u00b7vol\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lieder,", "tokens": ["Lie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "So leg ich meinen Kiel besch\u00e4mt und traurig nieder.", "tokens": ["So", "leg", "ich", "mei\u00b7nen", "Kiel", "be\u00b7sch\u00e4mt", "und", "trau\u00b7rig", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "VVFIN", "NE", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du willst nun mit Gewalt von unsern Bergen gehn,", "tokens": ["Du", "willst", "nun", "mit", "Ge\u00b7walt", "von", "un\u00b7sern", "Ber\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du eilest ungemein in jenes Plei\u00df-Athen,", "tokens": ["Du", "ei\u00b7lest", "un\u00b7ge\u00b7mein", "in", "je\u00b7nes", "Plei\u00df\u00b7A\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wo Wei\u00dfheit, Witz und Kunst in ihren Mau-", "tokens": ["Wo", "Wei\u00df\u00b7heit", ",", "Witz", "und", "Kunst", "in", "ih\u00b7ren", "Mau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "ren wohnet,", "tokens": ["ren", "woh\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Da, wo Apollo selbst in Pracht und Anmuth thronet,", "tokens": ["Da", ",", "wo", "A\u00b7pol\u00b7lo", "selbst", "in", "Pracht", "und", "An\u00b7muth", "thro\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NE", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nach Leipzig, das gewi\u00df ein irrdisch Paradies,", "tokens": ["Nach", "Leip\u00b7zig", ",", "das", "ge\u00b7wi\u00df", "ein", "irr\u00b7disch", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Vlies,", "tokens": ["Vlies", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Das Jason, wen\u0303 er will, kan leicht und balde finden,", "tokens": ["Das", "Ja\u00b7son", ",", "we\u00f1", "er", "will", ",", "kan", "leicht", "und", "bal\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "$,", "VMFIN", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In diesen G\u00f6tter-Hayn der dickbelaubten Linden.", "tokens": ["In", "die\u00b7sen", "G\u00f6t\u00b7ter\u00b7Hayn", "der", "dick\u00b7be\u00b7laub\u00b7ten", "Lin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und also eilst auch du, nach deinem hohen Geist,", "tokens": ["Und", "al\u00b7so", "eilst", "auch", "du", ",", "nach", "dei\u00b7nem", "ho\u00b7hen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da dir der Vorsicht Macht so Weg als Mittel weist.", "tokens": ["Da", "dir", "der", "Vor\u00b7sicht", "Macht", "so", "Weg", "als", "Mit\u00b7tel", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADV", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du willst je mehr und mehr den Sinn zur Wei\u00df-", "tokens": ["Du", "willst", "je", "mehr", "und", "mehr", "den", "Sinn", "zur", "Wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "KON", "ADV", "ART", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "heit lencken,", "tokens": ["heit", "len\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "---", "measure": "unknown.measure.zero"}, "line.22": {"text": "Die Pleisse wird dich nun mit Hypocrene tr\u00e4ncken.", "tokens": ["Die", "Pleis\u00b7se", "wird", "dich", "nun", "mit", "Hy\u00b7po\u00b7cre\u00b7ne", "tr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wohlan, so mache dich zu ihren ", "tokens": ["Wo\u00b7hlan", ",", "so", "ma\u00b7che", "dich", "zu", "ih\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PRF", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sey Pursche, sey Student, und schw\u00f6hre ihrem", "tokens": ["Sey", "Pur\u00b7sche", ",", "sey", "Stu\u00b7dent", ",", "und", "schw\u00f6h\u00b7re", "ih\u00b7rem"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "NN", "$,", "KON", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Thron.", "tokens": ["Thron", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Von nun an sey nunmehr, o Freund! dein gantzes", "tokens": ["Von", "nun", "an", "sey", "nun\u00b7mehr", ",", "o", "Freund", "!", "dein", "gant\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "APZR", "VAFIN", "ADV", "$,", "FM", "NN", "$.", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Dichten,", "tokens": ["Dich\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Jhn durch den fr\u00f6msten Flei\u00df dein Opffer zu ent-", "tokens": ["Jhn", "durch", "den", "fr\u00f6ms\u00b7ten", "Flei\u00df", "dein", "Opf\u00b7fer", "zu", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "richten.", "tokens": ["rich\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Die Wei\u00dfheit sey nunmehr dein auserwehlter", "tokens": ["Die", "Wei\u00df\u00b7heit", "sey", "nun\u00b7mehr", "dein", "au\u00b7ser\u00b7wehl\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Schatz,", "tokens": ["Schatz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Gieb nur dem tr\u00e4gen Muth auf keine Weise Platz.", "tokens": ["Gieb", "nur", "dem", "tr\u00e4\u00b7gen", "Muth", "auf", "kei\u00b7ne", "Wei\u00b7se", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Recht \u00e4ngstlich mustu dich nach ihrem Nectar sehnen.", "tokens": ["Recht", "\u00e4ngst\u00b7lich", "mus\u00b7tu", "dich", "nach", "ih\u00b7rem", "Nec\u00b7tar", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verbanne jede Lust der schmeichelnden Syrenen,", "tokens": ["Ver\u00b7ban\u00b7ne", "je\u00b7de", "Lust", "der", "schmei\u00b7cheln\u00b7den", "Sy\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Verliebe dich nur nicht; denn glaube gantz gewi\u00df,", "tokens": ["Ver\u00b7lie\u00b7be", "dich", "nur", "nicht", ";", "denn", "glau\u00b7be", "gantz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$.", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Cleander w\u00e4r gelehrt, wo ihm nicht Dorilis", "tokens": ["Clean\u00b7der", "w\u00e4r", "ge\u00b7lehrt", ",", "wo", "ihm", "nicht", "Do\u00b7ri\u00b7lis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "VVPP", "$,", "PWAV", "PPER", "PTKNEG", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.37": {"text": "Durch ihre Reitzungen so Zeit als Flei\u00df benommen.", "tokens": ["Durch", "ih\u00b7re", "Reit\u00b7zun\u00b7gen", "so", "Zeit", "als", "Flei\u00df", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.38": {"text": "Nun ist er wie Hans Dum, ins Vaterland gekom\u0303en.", "tokens": ["Nun", "ist", "er", "wie", "Hans", "Dum", ",", "ins", "Va\u00b7ter\u00b7land", "ge\u00b7kom\u0303en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "NE", "NE", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Musen lieben nicht, wie andere, so fr\u00fch.", "tokens": ["Die", "Mu\u00b7sen", "lie\u00b7ben", "nicht", ",", "wie", "an\u00b7de\u00b7re", ",", "so", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Freund! alle unser Thun, Freund! unser Schwei\u00df", "tokens": ["Freund", "!", "al\u00b7le", "un\u00b7ser", "Thun", ",", "Freund", "!", "un\u00b7ser", "Schwei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PIAT", "PPOSAT", "NN", "$,", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "und M\u00fch", "tokens": ["und", "M\u00fch"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.42": {"text": "Mu\u00df, wenn er anders \u00e4cht, die Lockungen verachten,", "tokens": ["Mu\u00df", ",", "wenn", "er", "an\u00b7ders", "\u00e4cht", ",", "die", "Lo\u00b7ckun\u00b7gen", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.43": {"text": "Und nach dem Lorber-Crantz des hohen Pindus", "tokens": ["Und", "nach", "dem", "Lor\u00b7ber\u00b7Crantz", "des", "ho\u00b7hen", "Pin\u00b7dus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "trachten.", "tokens": ["trach\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Nur Kunst und Wissenschafft bringt Ehre, Ruhm", "tokens": ["Nur", "Kunst", "und", "Wis\u00b7sen\u00b7schafft", "bringt", "Eh\u00b7re", ",", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "und Prei\u00df,", "tokens": ["und", "Prei\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.47": {"text": "Drum suche selbige durch angewandten Flei\u00df.", "tokens": ["Drum", "su\u00b7che", "sel\u00b7bi\u00b7ge", "durch", "an\u00b7ge\u00b7wand\u00b7ten", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Hast du nun Eid und Treu zu den ", "tokens": ["Hast", "du", "nun", "Eid", "und", "Treu", "zu", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "NN", "APPR", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.49": {"text": "schwohren,", "tokens": ["schwoh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.50": {"text": "So flieh den schlechten Weg der lasterhafften Thore\u0303.", "tokens": ["So", "flieh", "den", "schlech\u00b7ten", "Weg", "der", "las\u00b7ter\u00b7haff\u00b7ten", "Thor\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Scheint offt die Tugend selbst dir allzu bang und", "tokens": ["Scheint", "offt", "die", "Tu\u00b7gend", "selbst", "dir", "all\u00b7zu", "bang", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "PPER", "PTKA", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "rauh,", "tokens": ["rauh", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.53": {"text": "O! glaub es nimmermehr; denn deines Gl\u00fcckes", "tokens": ["O", "!", "glaub", "es", "nim\u00b7mer\u00b7mehr", ";", "denn", "dei\u00b7nes