{"dta.poem.12043": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leb' in der Gegenwart! Zu leer ist und zu weit", "tokens": ["Leb'", "in", "der", "Ge\u00b7gen\u00b7wart", "!", "Zu", "leer", "ist", "und", "zu", "weit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "NN", "$.", "PTKA", "ADJD", "VAFIN", "KON", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zukunft Haus, zu gro\u00df das der Vergangenheit.", "tokens": ["Der", "Zu\u00b7kunft", "Haus", ",", "zu", "gro\u00df", "das", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PTKA", "ADJD", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In beiden wei\u00dft du nicht den Hausrath einzurichten", "tokens": ["In", "bei\u00b7den", "wei\u00dft", "du", "nicht", "den", "Haus\u00b7rath", "ein\u00b7zu\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ungeschehnen und geschehenen Geschichten.", "tokens": ["Der", "un\u00b7ge\u00b7scheh\u00b7nen", "und", "ge\u00b7sche\u00b7he\u00b7nen", "Ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Doch da\u00df die Gegenwart nicht eng dir sei und klein,", "tokens": ["Doch", "da\u00df", "die", "Ge\u00b7gen\u00b7wart", "nicht", "eng", "dir", "sei", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "PPER", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zieh die Vergangenheit und Zukunft mit herein.", "tokens": ["Zieh", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "und", "Zu\u00b7kunft", "mit", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die beiden m\u00f6gen dir erf\u00fcllen und erweitern", "tokens": ["Die", "bei\u00b7den", "m\u00f6\u00b7gen", "dir", "er\u00b7f\u00fcl\u00b7len", "und", "er\u00b7wei\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wohnung, und mit Glanz die dunkle sch\u00f6n erheitern.", "tokens": ["Die", "Woh\u00b7nung", ",", "und", "mit", "Glanz", "die", "dunk\u00b7le", "sch\u00f6n", "er\u00b7hei\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "APPR", "NN", "ART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}