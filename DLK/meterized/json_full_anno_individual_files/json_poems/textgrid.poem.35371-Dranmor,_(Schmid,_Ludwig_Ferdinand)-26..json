{"textgrid.poem.35371": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anendlichkeit, vor dir erbeben nur", "tokens": ["A\u00b7nend\u00b7lich\u00b7keit", ",", "vor", "dir", "er\u00b7be\u00b7ben", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPER", "VVFIN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kann mein Gehirn; doch dich ersehnen? Nein!", "tokens": ["Kann", "mein", "Ge\u00b7hirn", ";", "doch", "dich", "er\u00b7seh\u00b7nen", "?", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$.", "KON", "PPER", "VVINF", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die schwache, gramerf\u00fcllte Kreatur", "tokens": ["Die", "schwa\u00b7che", ",", "gra\u00b7mer\u00b7f\u00fcll\u00b7te", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kann nicht unendlich, nicht unsterblich sein.", "tokens": ["Kann", "nicht", "un\u00b7end\u00b7lich", ",", "nicht", "uns\u00b7terb\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir sind ein armes, winziges Geschlecht,", "tokens": ["Wir", "sind", "ein", "ar\u00b7mes", ",", "win\u00b7zi\u00b7ges", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das nach Minuten rechnet; wir verlangen,", "tokens": ["Das", "nach", "Mi\u00b7nu\u00b7ten", "rech\u00b7net", ";", "wir", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bevor des Todes Schauer uns umfangen,", "tokens": ["Be\u00b7vor", "des", "To\u00b7des", "Schau\u00b7er", "uns", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nichts als des Atmens, als des Daseins Recht.", "tokens": ["Nichts", "als", "des", "At\u00b7mens", ",", "als", "des", "Da\u00b7seins", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "$,", "KOUS", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Der Lebensfunke, mit dem Stoff verm\u00e4hlt,", "tokens": ["Der", "Le\u00b7bens\u00b7fun\u00b7ke", ",", "mit", "dem", "Stoff", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kann nicht selbst\u00e4ndig lodern; ausgezittert", "tokens": ["Kann", "nicht", "selb\u00b7st\u00e4n\u00b7dig", "lo\u00b7dern", ";", "aus\u00b7ge\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VVINF", "$.", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Hat der Gedanke, wenn die Form zersplittert,", "tokens": ["Hat", "der", "Ge\u00b7dan\u00b7ke", ",", "wenn", "die", "Form", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die ihm des Zufalls Laune auserw\u00e4hlt.", "tokens": ["Die", "ihm", "des", "Zu\u00b7falls", "Lau\u00b7ne", "au\u00b7ser\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Geist, der du in den undenkbaren Sph\u00e4ren", "tokens": ["Geist", ",", "der", "du", "in", "den", "un\u00b7denk\u00b7ba\u00b7ren", "Sph\u00e4\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Des Weltalls waltest, schaffend und erg\u00e4nzend,", "tokens": ["Des", "Welt\u00b7alls", "wal\u00b7test", ",", "schaf\u00b7fend", "und", "er\u00b7g\u00e4n\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Milliarden Wesen ihre Bahn begrenzend,", "tokens": ["Mil\u00b7li\u00b7ar\u00b7den", "We\u00b7sen", "ih\u00b7re", "Bahn", "be\u00b7gren\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Nicht um Milliarden Leichen zu verkl\u00e4ren,", "tokens": ["Nicht", "um", "Mil\u00b7li\u00b7ar\u00b7den", "Lei\u00b7chen", "zu", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Nicht um sie weiter, weiter zu geleiten;", "tokens": ["Nicht", "um", "sie", "wei\u00b7ter", ",", "wei\u00b7ter", "zu", "ge\u00b7lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "ADV", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Denn sie bed\u00fcrfen keines Lohns, und strafen", "tokens": ["Denn", "sie", "be\u00b7d\u00fcr\u00b7fen", "kei\u00b7nes", "Lohns", ",", "und", "stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Willst du sie nicht \u2013 wenn wir im Tod entschlafen,", "tokens": ["Willst", "du", "sie", "nicht", "\u2013", "wenn", "wir", "im", "Tod", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "$(", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Was ist Vergeltung, was sind Seligkeiten?", "tokens": ["Was", "ist", "Ver\u00b7gel\u00b7tung", ",", "was", "sind", "Se\u00b7lig\u00b7kei\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$,", "PWS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Des Lebens Schule hat uns so gestaltet,", "tokens": ["Des", "Le\u00b7bens", "Schu\u00b7le", "hat", "uns", "so", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Da\u00df uns nur fl\u00fcchtiger Genu\u00df entz\u00fcckt;", "tokens": ["Da\u00df", "uns", "nur", "fl\u00fcch\u00b7ti\u00b7ger", "Ge\u00b7nu\u00df", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und die Entbehrung hat uns so bedr\u00fcckt,", "tokens": ["Und", "die", "Ent\u00b7beh\u00b7rung", "hat", "uns", "so", "be\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Da\u00df nur aus ihr die Freude sich entfaltet.", "tokens": ["Da\u00df", "nur", "aus", "ihr", "die", "Freu\u00b7de", "sich", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Kurz mu\u00df die Wonne sein, die uns belohnt;", "tokens": ["Kurz", "mu\u00df", "die", "Won\u00b7ne", "sein", ",", "die", "uns", "be\u00b7lohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "VAINF", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Vervielfacht wird die Lust durch die Kasteiung;", "tokens": ["Ver\u00b7viel\u00b7facht", "wird", "die", "Lust", "durch", "die", "Kas\u00b7te\u00b7i\u00b7ung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Liebe ist Schmerz und Wollust nur Befreiung", "tokens": ["Lie\u00b7be", "ist", "Schmerz", "und", "Wol\u00b7lust", "nur", "Be\u00b7frei\u00b7ung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "ADV", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.28": {"text": "Aus Ketten, die zu tragen wir gewohnt.", "tokens": ["Aus", "Ket\u00b7ten", ",", "die", "zu", "tra\u00b7gen", "wir", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKZU", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und wie wir sind, zu sinnlich und gebrechlich,", "tokens": ["Und", "wie", "wir", "sind", ",", "zu", "sinn\u00b7lich", "und", "ge\u00b7brech\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "$,", "PTKA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Um auf der Erde Wonnen zu verzichten,", "tokens": ["Um", "auf", "der", "Er\u00b7de", "Won\u00b7nen", "zu", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Wir finden Augenblicke, unaussprechlich,", "tokens": ["Wir", "fin\u00b7den", "Au\u00b7gen\u00b7bli\u00b7cke", ",", "un\u00b7aus\u00b7sprech\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Die jeden Schmerz verscheuchen und vernichten.", "tokens": ["Die", "je\u00b7den", "Schmerz", "ver\u00b7scheu\u00b7chen", "und", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "O, voller Zauber ist der Liebe Zeit!", "tokens": ["O", ",", "vol\u00b7ler", "Zau\u00b7ber", "ist", "der", "Lie\u00b7be", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "F\u00fcr Herzen, die sich aneinanderpressen,", "tokens": ["F\u00fcr", "Her\u00b7zen", ",", "die", "sich", "an\u00b7ein\u00b7an\u00b7der\u00b7pres\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Berauschte Herzen, die sich selbst vergessen,", "tokens": ["Be\u00b7rauschte", "Her\u00b7zen", ",", "die", "sich", "selbst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Ist schon erf\u00fcllt der Traum der Seligkeit.", "tokens": ["Ist", "schon", "er\u00b7f\u00fcllt", "der", "Traum", "der", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Wenn ich mich t\u00e4usche, Weltgeist, wenn du lenkend", "tokens": ["Wenn", "ich", "mich", "t\u00e4u\u00b7sche", ",", "Welt\u00b7geist", ",", "wenn", "du", "len\u00b7kend"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VAFIN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Wenn du bewu\u00dftvoll schaffest und zerst\u00f6rst,", "tokens": ["Wenn", "du", "be\u00b7wu\u00dft\u00b7voll", "schaf\u00b7fest", "und", "zer\u00b7st\u00f6rst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Wenn du mein Ringen siehst, wenn du mich h\u00f6rst", "tokens": ["Wenn", "du", "mein", "Rin\u00b7gen", "siehst", ",", "wenn", "du", "mich", "h\u00f6rst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "So sei du dennoch meiner nicht gedenkend;", "tokens": ["So", "sei", "du", "den\u00b7noch", "mei\u00b7ner", "nicht", "ge\u00b7den\u00b7kend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "La\u00df dieses Herz, das einst so st\u00fcrmisch schlug,", "tokens": ["La\u00df", "die\u00b7ses", "Herz", ",", "das", "einst", "so", "st\u00fcr\u00b7misch", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und sei es auch das einzigste von allen,", "tokens": ["Und", "sei", "es", "auch", "das", "ein\u00b7zigs\u00b7te", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "APPR", "PIAT", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "La\u00df es in Staub zerfallen,", "tokens": ["La\u00df", "es", "in", "Staub", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Ich habe hei\u00df geliebt \u2013 das ist genug!", "tokens": ["Ich", "ha\u00b7be", "hei\u00df", "ge\u00b7liebt", "\u2013", "das", "ist", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$(", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Aetherisch durch den Sternenraum zu schweifen,", "tokens": ["A\u00b7e\u00b7the\u00b7risch", "durch", "den", "Ster\u00b7nen\u00b7raum", "zu", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.46": {"text": "Ich kann es nicht begreifen;", "tokens": ["Ich", "kann", "es", "nicht", "be\u00b7grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Der Sch\u00f6pfung Wunder, die mit Allgewalt", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Wun\u00b7der", ",", "die", "mit", "All\u00b7ge\u00b7walt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Ein stetes Dankgebet in mir