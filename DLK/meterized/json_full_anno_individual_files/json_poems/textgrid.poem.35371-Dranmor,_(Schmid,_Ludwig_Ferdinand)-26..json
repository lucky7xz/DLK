, "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das nach Minuten rechnet; wir verlangen,", "tokens": ["Das", "nach", "Mi\u00b7nu\u00b7ten", "rech\u00b7net", ";", "wir", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bevor des Todes Schauer uns umfangen,", "tokens": ["Be\u00b7vor", "des", "To\u00b7des", "Schau\u00b7er", "uns", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nichts als des Atmens, als des Daseins Recht.", "tokens": ["Nichts", "als", "des", "At\u00b7mens", ",", "als", "des", "Da\u00b7seins", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "$,", "KOUS", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Der Lebensfunke, mit dem Stoff verm\u00e4hlt,", "tokens": ["Der", "Le\u00b7bens\u00b7fun\u00b7ke", ",", "mit", "dem", "Stoff", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kann nicht selbst\u00e4ndig lodern; ausgezittert", "tokens": ["Kann", "nicht", "selb\u00b7st\u00e4n\u00b7dig", "lo\u00b7dern", ";", "aus\u00b7ge\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VVINF", "$.", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Hat der Gedanke, wenn die Form zersplittert,", "tokens": ["Hat", "der", "Ge\u00b7dan\u00b7ke", ",", "wenn", "die", "Form", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die ihm des Zufalls Laune auserw\u00e4hlt.", "tokens": ["Die", "ihm", "des", "Zu\u00b7falls", "Lau\u00b7ne", "au\u00b7ser\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Geist, der du in den undenkbaren Sph\u00e4ren", "tokens": ["Geist", ",", "der", "du", "in", "den", "un\u00b7denk\u00b7ba\u00b7ren", "Sph\u00e4\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Des Weltalls waltest, schaffend und erg\u00e4nzend,", "tokens": ["Des", "Welt\u00b7alls", "wal\u00b7test", ",", "schaf\u00b7fend", "und", "er\u00b7g\u00e4n\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Milliarden Wesen ihre Bahn begrenzend,", "tokens": ["Mil\u00b7li\u00b7ar\u00b7den", "We\u00b7sen", "ih\u00b7re", "Bahn", "be\u00b7gren\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Nicht um Milliarden Leichen zu verkl\u00e4ren,", "tokens": ["Nicht", "um", "Mil\u00b7li\u00b7ar\u00b7den", "Lei\u00b7chen", "zu", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Nicht um sie weiter, weiter zu geleiten;", "tokens": ["Nicht", "um", "sie", "wei\u00b7ter", ",", "wei\u00b7ter", "zu", "ge\u00b7lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "ADV", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Denn sie bed\u00fcrfen keines Lohns, und strafen", "tokens": ["Denn", "sie", "be\u00b7d\u00fcr\u00b7fen", "kei\u00b7nes", "Lohns", ",", "und", "stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Willst du sie nicht \u2013 wenn wir im Tod entschlafen,", "tokens": ["Willst", "du", "sie", "nicht", "\u2013", "wenn", "wir", "im", "Tod", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "$(", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Was ist Vergeltung, was sind Seligkeiten?", "tokens": ["Was", "ist", "Ver\u00b7gel\u00b7tung", ",", "was", "sind", "Se\u00b7lig\u00b7kei\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$,", "PWS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Des Lebens Schule hat uns so gestaltet,", "tokens": ["Des", "Le\u00b7bens", "Schu\u00b7le", "hat", "uns", "so", "ge\u00