"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "$,", "FM", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "M\u00f6cht' ich mit ihr auf ewig untergehn;", "tokens": ["M\u00f6cht'", "ich", "mit", "ihr", "auf", "e\u00b7wig", "un\u00b7ter\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.55": {"text": "Nicht weil mich fr\u00fche Trauer \u00fcberflutet,", "tokens": ["Nicht", "weil", "mich", "fr\u00fc\u00b7he", "Trau\u00b7er", "\u00fc\u00b7berf\u00b7lu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Und nun in finsterm Trotz mein Herz verblutet,", "tokens": ["Und", "nun", "in", "fins\u00b7term", "Trotz", "mein", "Herz", "ver\u00b7blu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "Nein! \u2013 Wenn ich Schweres litt und Unnennbares,", "tokens": ["Nein", "!", "\u2013", "Wenn", "ich", "Schwe\u00b7res", "litt", "und", "Un\u00b7nenn\u00b7ba\u00b7res", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "$(", "KOUS", "PPER", "NN", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.58": {"text": "Ich habe Himmelsfreuden schon genossen", "tokens": ["Ich", "ha\u00b7be", "Him\u00b7mels\u00b7freu\u00b7den", "schon", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.59": {"text": "Und l\u00e4ngst den Tod in meine Brust geschlossen", "tokens": ["Und", "l\u00e4ngst", "den", "Tod", "in", "mei\u00b7ne", "Brust", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.60": {"text": "Als ein vertrautes Bild, ein sternenklares.", "tokens": ["Als", "ein", "ver\u00b7trau\u00b7tes", "Bild", ",", "ein", "ster\u00b7nen\u00b7kla\u00b7res", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.61": {"text": "Und nun der Mitwelt dieses Bild enth\u00fcllen,", "tokens": ["Und", "nun", "der", "Mit\u00b7welt", "die\u00b7ses", "Bild", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.62": {"text": "Das m\u00f6cht' ich, siegreich und verhei\u00dfungsvoll;", "tokens": ["Das", "m\u00f6cht'", "ich", ",", "sieg\u00b7reich", "und", "ver\u00b7hei\u00b7\u00dfungs\u00b7voll", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.63": {"text": "Denn keine T\u00e4uschung, die verschwinden soll,", "tokens": ["Denn", "kei\u00b7ne", "T\u00e4u\u00b7schung", ",", "die", "ver\u00b7schwin\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.64": {"text": "Kann mit so s\u00fc\u00dfem Trost