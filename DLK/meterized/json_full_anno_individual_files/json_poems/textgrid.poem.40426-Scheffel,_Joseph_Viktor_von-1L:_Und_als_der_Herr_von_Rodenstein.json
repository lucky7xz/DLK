{"textgrid.poem.40426": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und als der Herr von Rodenstein", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als der Herr von Rodenstein", "tokens": ["Und", "als", "der", "Herr", "von", "Ro\u00b7dens\u00b7tein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Frankenstein sich wandte,", "tokens": ["Zum", "Fran\u00b7ken\u00b7stein", "sich", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Empfing er seinen Ehrenwein,", "tokens": ["Emp\u00b7fing", "er", "sei\u00b7nen", "Eh\u00b7ren\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wie es Brauch im Lande.", "tokens": ["So", "wie", "es", "Brauch", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Beerbach vor dem Rathaus bracht'", "tokens": ["In", "Beer\u00b7bach", "vor", "dem", "Rat\u00b7haus", "bracht'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Zentgraf mit den Bauern", "tokens": ["Der", "Zent\u00b7graf", "mit", "den", "Bau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Kauzenkrug. Der Alte lacht:", "tokens": ["Den", "Kau\u00b7zen\u00b7krug", ".", "Der", "Al\u00b7te", "lacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbnur her mit Euerm Sauern!", "tokens": ["\u00bb", "nur", "her", "mit", "Eu\u00b7erm", "Sau\u00b7ern", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ihr Mannen macht das Armbein krumm,", "tokens": ["Ihr", "Man\u00b7nen", "macht", "das", "Arm\u00b7bein", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Willekumm gaht um, gaht um,", "tokens": ["Der", "Wil\u00b7le\u00b7kumm", "gaht", "um", ",", "gaht", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Holliro, das Bauernk\u00e4uzlein", "tokens": ["Hol\u00b7li\u00b7ro", ",", "das", "Bau\u00b7ern\u00b7k\u00e4uz\u00b7lein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Gaht um, gaht um!\u00ab", "tokens": ["Gaht", "um", ",", "gaht", "um", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Als er von dort sich durchgezerrt", "tokens": ["Als", "er", "von", "dort", "sich", "durch\u00b7ge\u00b7zerrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Frankensteiner Linde,", "tokens": ["Zur", "Fran\u00b7ken\u00b7stei\u00b7ner", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stand Weg und Durchpa\u00df dicht gesperrt", "tokens": ["Stand", "Weg", "und", "Durch\u00b7pa\u00df", "dicht", "ge\u00b7sperrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom jungen Burggesinde:", "tokens": ["Vom", "jun\u00b7gen", "Burg\u00b7ge\u00b7sin\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Reiterstiefel lebensgro\u00df", "tokens": ["Ein", "Rei\u00b7ter\u00b7stie\u00b7fel", "le\u00b7bens\u00b7gro\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Ton, ein feinbemalter,", "tokens": ["Von", "Ton", ",", "ein", "fein\u00b7be\u00b7mal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ward ihm gef\u00fcllt kredenzt aufs Ro\u00df", "tokens": ["Ward", "ihm", "ge\u00b7f\u00fcllt", "kre\u00b7denzt", "aufs", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und alles sang den Psalter:", "tokens": ["Und", "al\u00b7les", "sang", "den", "Psal\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbihr Mannen, macht das Armbein krumm,", "tokens": ["\u00bb", "ihr", "Man\u00b7nen", ",", "macht", "das", "Arm\u00b7bein", "krumm", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Willekumm gaht um, gaht um,", "tokens": ["Der", "Wil\u00b7le\u00b7kumm", "gaht", "um", ",", "gaht", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Holliro, der gro\u00dfe Stiefel", "tokens": ["Hol\u00b7li\u00b7ro", ",", "der", "gro\u00b7\u00dfe", "Stie\u00b7fel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gaht um, gaht um!\u00ab", "tokens": ["Gaht", "um", ",", "gaht", "um", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Im Burghof gr\u00fc\u00dft' ein zweiter Schwarm", "tokens": ["Im", "Burg\u00b7hof", "gr\u00fc\u00dft'", "ein", "zwei\u00b7ter", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn mit Kartaunenz\u00fcndung,", "tokens": ["Ihn", "mit", "Kar\u00b7tau\u00b7nen\u00b7z\u00fcn\u00b7dung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da schwang der Burgherr selbst im Arm", "tokens": ["Da", "schwang", "der", "Burg\u00b7herr", "selbst", "im", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des zweiten Stiefels R\u00fcndung.", "tokens": ["Des", "zwei\u00b7ten", "Stie\u00b7fels", "R\u00fcn\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Schlo\u00dfbergs Feinsten go\u00df man ein", "tokens": ["Des", "Schlo\u00df\u00b7bergs", "Feins\u00b7ten", "go\u00df", "man", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PIS", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00fcrdig sprach der Ritter:", "tokens": ["Und", "w\u00fcr\u00b7dig", "sprach", "der", "Rit\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbherr Nachbar, nit auf eynem Bein!", "tokens": ["\u00bb", "herr", "Nach\u00b7bar", ",", "nit", "auf", "ey\u00b7nem", "Bein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der hier schmeckt auch nicht bitter.", "tokens": ["Der", "hier", "schmeckt", "auch", "nicht", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Ihr Mannen, macht das Armbein krumm,", "tokens": ["Ihr", "Man\u00b7nen", ",", "macht", "das", "Arm\u00b7bein", "krumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Willekumm gaht um, gaht um,", "tokens": ["Der", "Wil\u00b7le\u00b7kumm", "gaht", "um", ",", "gaht", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Holliro, der gro\u00dfe Stiefel", "tokens": ["Hol\u00b7li\u00b7ro", ",", "der", "gro\u00b7\u00dfe", "Stie\u00b7fel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gaht um, gaht um.\u00ab", "tokens": ["Gaht", "um", ",", "gaht", "um", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Rodenstein trank aus und rief:", "tokens": ["Der", "Ro\u00b7dens\u00b7tein", "trank", "aus", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbgott segne deine Nase!", "tokens": ["\u00bb", "gott", "seg\u00b7ne", "dei\u00b7ne", "Na\u00b7se", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die meine bog sich beinah schief", "tokens": ["Die", "mei\u00b7ne", "bog", "sich", "bei\u00b7nah", "schief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "VVFIN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von solchem Strom im Glase.", "tokens": ["Von", "sol\u00b7chem", "Strom", "im", "Gla\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt w\u00f6ll'n wir in dem Rittersaal", "tokens": ["Jetzt", "w\u00f6ll'n", "wir", "in", "dem", "Rit\u00b7ter\u00b7saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ausruhn vom ersten Tosen;", "tokens": ["Aus\u00b7ruhn", "vom", "ers\u00b7ten", "To\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mir ahnt, dort f\u00fcllt dein Eh'gemahl", "tokens": ["Mir", "ahnt", ",", "dort", "f\u00fcllt", "dein", "Eh'\u00b7ge\u00b7mahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Trinkhorn Karls des Gro\u00dfen.", "tokens": ["Das", "Trink\u00b7horn", "Karls", "des", "Gro\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und nochmals hei\u00dft's: das Armbein krumm,", "tokens": ["Und", "noch\u00b7mals", "hei\u00dft's", ":", "das", "Arm\u00b7bein", "krumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "$.", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Willekumm gaht um, gaht um,", "tokens": ["Der", "Wil\u00b7le\u00b7kumm", "gaht", "um", ",", "gaht", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Holliro, des Kaisers H\u00f6rnlein", "tokens": ["Hol\u00b7li\u00b7ro", ",", "des", "Kai\u00b7sers", "H\u00f6rn\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Gaht um, gaht um.\u00ab", "tokens": ["Gaht", "um", ",", "gaht", "um", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "... Beim Abschied andern Morgens war", "tokens": ["...", "Beim", "Ab\u00b7schied", "an\u00b7dern", "Mor\u00b7gens", "war"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "ADJA", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Nebel weit und breite,", "tokens": ["Ein", "Ne\u00b7bel", "weit", "und", "brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da bracht' man ihm das Stammbuch dar", "tokens": ["Da", "bracht'", "man", "ihm", "das", "Stamm\u00b7buch", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Eintrag, eh' er scheide.", "tokens": ["Zum", "Ein\