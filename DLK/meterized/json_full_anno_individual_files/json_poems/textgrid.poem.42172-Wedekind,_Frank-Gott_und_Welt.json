{"textgrid.poem.42172": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Gott und Welt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Mensch von Fleisch und Blut,", "tokens": ["Ich", "bin", "ein", "Mensch", "von", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich fange keine Grillen;", "tokens": ["Ich", "fan\u00b7ge", "kei\u00b7ne", "Gril\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann des Fleisches Durst so gut", "tokens": ["Ich", "kann", "des", "Flei\u00b7sches", "Durst", "so", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie den der Seele stillen.", "tokens": ["Wie", "den", "der", "See\u00b7le", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schwinge br\u00fcnstig mich empor", "tokens": ["Ich", "schwin\u00b7ge", "br\u00fcns\u00b7tig", "mich", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Gott in schwacher Stunde;", "tokens": ["Zu", "Gott", "in", "schwa\u00b7cher", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "