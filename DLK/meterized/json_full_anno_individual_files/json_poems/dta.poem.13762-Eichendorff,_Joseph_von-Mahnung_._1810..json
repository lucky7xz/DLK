{"dta.poem.13762": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl mancher, dem die wirblichten Geschichten", "tokens": ["Wohl", "man\u00b7cher", ",", "dem", "die", "wirb\u00b7lich\u00b7ten", "Ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Zeit das ehrlich deutsche Herz zerschlagen,", "tokens": ["Der", "Zeit", "das", "ehr\u00b7lich", "deut\u00b7sche", "Herz", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mag, wie Prinz Hamlet, zu sich selber sagen:", "tokens": ["Mag", ",", "wie", "Prinz", "Ham\u00b7let", ",", "zu", "sich", "sel\u00b7ber", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWAV", "NN", "NE", "$,", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weh! da\u00df zur Welt ich kam, sie einzurichten!", "tokens": ["Weh", "!", "da\u00df", "zur", "Welt", "ich", "kam", ",", "sie", "ein\u00b7zu\u00b7rich\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}