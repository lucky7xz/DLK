{"dta.poem.993": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Es lacht in dem steigenden jahr dir               ", "tokens": ["Es", "lacht", "in", "dem", "stei\u00b7gen\u00b7den", "jahr", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPER"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der duft aus dem garten noch leis               ", "tokens": ["Der", "duft", "aus", "dem", "gar\u00b7ten", "noch", "leis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "VVFIN", "ADV", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Flicht in dem flatternden haar dir               ", "tokens": ["Flicht", "in", "dem", "flat\u00b7tern\u00b7den", "haar", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPER"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Eppich und ehrenpreis             ", "tokens": ["Ep\u00b7pich", "und", "eh\u00b7ren\u00b7preis"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die wehende saat ist wie gold noch               ", "tokens": ["Die", "we\u00b7hen\u00b7de", "saat", "ist", "wie", "gold", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KOKOM", "NN", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vielleicht nicht so hoch mehr und reich               ", "tokens": ["Viel\u00b7leicht", "nicht", "so", "hoch", "mehr", "und", "reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "ADJD", "ADV", "KON", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Rosen begr&#252;ssen dich hold noch               ", "tokens": ["Ro\u00b7sen", "be\u00b7gr", "&#252;", "ssen", "dich", "hold", "noch"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ward auch ihr glanz etwas bleich             ", "tokens": ["Ward", "auch", "ihr", "glanz", "et\u00b7was", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Verschweigen wir was uns verwehrt ist               ", "tokens": ["Ver\u00b7schwei\u00b7gen", "wir", "was", "uns", "ver\u00b7wehrt", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PWS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geloben wir gl&#252;cklich zu sein               ", "tokens": ["Ge\u00b7lo\u00b7ben", "wir", "gl", "&#252;", "ck\u00b7lich", "zu", "sein"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["NN", "PPER", "NE", "$(", "ADJD", "PTKZU", "VAINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn auch nicht mehr uns beschert ist               ", "tokens": ["Wenn", "auch", "nicht", "mehr", "uns", "be\u00b7schert", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKNEG", "ADV", "PPER", "VVPP", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als noch ein rundgang zu zwein.             ", "tokens": ["Als", "noch", "ein", "rund\u00b7gang", "zu", "zwein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}