{"dta.poem.18658": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Erschaffen und Beleben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Hans Adam war ein Erdenklos,", "tokens": ["Hans", "A\u00b7dam", "war", "ein", "Er\u00b7denk\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Gott zum Menschen machte,", "tokens": ["Den", "Gott", "zum", "Men\u00b7schen", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch bracht\u2019 er aus der Mutter Schooss", "tokens": ["Doch", "bracht'", "er", "aus", "der", "Mut\u00b7ter", "Scho\u00b7oss"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch vieles Ungeschlachte.", "tokens": ["Noch", "vie\u00b7les", "Un\u00b7ge\u00b7schlach\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Elohim zur Nas\u2019 hinein", "tokens": ["Die", "E\u00b7lo\u00b7him", "zur", "Nas'", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den besten Geist ihm bliesen,", "tokens": ["Den", "bes\u00b7ten", "Geist", "ihm", "blie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun schien er schon was mehr zu seyn,", "tokens": ["Nun", "schien", "er", "schon", "was", "mehr", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PWS", "PIS", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn er fing an zu niesen.", "tokens": ["Denn", "er", "fing", "an", "zu", "nie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch mit Gebein und Glied und Kopf", "tokens": ["Doch", "mit", "Ge\u00b7bein", "und", "Glied", "und", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blieb er ein halber Klumpen,", "tokens": ["Blieb", "er", "ein", "hal\u00b7ber", "Klum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis endlich Noah f\u00fcr den Tropf", "tokens": ["Bis", "end\u00b7lich", "Noah", "f\u00fcr", "den", "Tropf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NE", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das Wahre fand, den Humpen.", "tokens": ["Das", "Wah\u00b7re", "fand", ",", "den", "Hum\u00b7pen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Klumpe f\u00fchlt sogleich den Schwung,", "tokens": ["Der", "Klum\u00b7pe", "f\u00fchlt", "sog\u00b7leich", "den", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sobald er sich benetzet,", "tokens": ["So\u00b7bald", "er", "sich", "be\u00b7net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wie der Teig durch S\u00e4uerung", "tokens": ["So", "wie", "der", "Teig", "durch", "S\u00e4u\u00b7e\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sich in Bewegung setzet.", "tokens": ["Sich", "in", "Be\u00b7we\u00b7gung", "set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So, Hafis, mag dein holder Sang,", "tokens": ["So", ",", "Ha\u00b7fis", ",", "mag", "dein", "hol\u00b7der", "Sang", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein heiliges Exempel", "tokens": ["Dein", "hei\u00b7li\u00b7ges", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Uns f\u00fchren, bey der Gl\u00e4ser Klang,", "tokens": ["Uns", "f\u00fch\u00b7ren", ",", "bey", "der", "Gl\u00e4\u00b7ser", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu unsres Sch\u00f6pfers Tempel.", "tokens": ["Zu", "uns\u00b7res", "Sch\u00f6p\u00b7fers", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}