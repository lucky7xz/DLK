{"dta.poem.8318": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachl\u00e4\u00dfigkeit der Menschen  \n die wunderbahre und weise Einrich-  \n tung ihres K\u00f6rpers zu erkennen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist mehr bem\u00fcht, was auser\nihm zu sehn,", "tokens": ["Der", "Mensch", "ist", "mehr", "be\u00b7m\u00fcht", ",", "was", "au\u00b7ser", "ihm", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,", "PRELS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als das was an ihm ist, recht gr\u00fcnd-", "tokens": ["Als", "das", "was", "an", "ihm", "ist", ",", "recht", "gr\u00fcn\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PDS", "PRELS", "APPR", "PPER", "VAFIN", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er will aus Neubegier die ganze Welt ergr\u00fcnden,", "tokens": ["Er", "will", "aus", "Neu\u00b7be\u00b7gier", "die", "gan\u00b7ze", "Welt", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und denket kaum daran, was an ihm selbst zu fin-", "tokens": ["Und", "den\u00b7ket", "kaum", "da\u00b7ran", ",", "was", "an", "ihm", "selbst", "zu", "fin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,", "PRELS", "APPR", "PPER", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er ist die kleine Welt, der Allmacht Meisterst\u00fck,", "tokens": ["Er", "ist", "die", "klei\u00b7ne", "Welt", ",", "der", "All\u00b7macht", "Meis\u00b7ter\u00b7st\u00fck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dennoch sieht er nicht, auf seinem Leib zur\u00fck,", "tokens": ["Und", "den\u00b7noch", "sieht", "er", "nicht", ",", "auf", "sei\u00b7nem", "Leib", "zu\u00b7r\u00fck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Daran kein Glied zu sehn, kein Theilgen zu er-", "tokens": ["Da\u00b7ran", "kein", "Glied", "zu", "sehn", ",", "kein", "Theil\u00b7gen", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PIAT", "NN", "PTKZU", "VVINF", "$,", "PIAT", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die nicht von weiser Macht des Sch\u00f6pfers Zei-", "tokens": ["Die", "nicht", "von", "wei\u00b7ser", "Macht", "des", "Sch\u00f6p\u00b7fers", "Zei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Eitelkeit schaut zwar in glatte Spiegel ein,", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "schaut", "zwar", "in", "glat\u00b7te", "Spie\u00b7gel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Aus Eigenlieb entz\u00fckt, ihr Bild im Gegenschein,", "tokens": ["Aus", "Ei\u00b7gen\u00b7lieb", "ent\u00b7z\u00fckt", ",", "ihr", "Bild", "im", "Ge\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie es gepuzzet ist, bemerkend anzublikken,", "tokens": ["Wie", "es", "ge\u00b7puz\u00b7zet", "ist", ",", "be\u00b7mer\u00b7kend", "an\u00b7zu\u00b7bli\u00b7kken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.12": {"text": "Um ihre Sch\u00f6nheit sich darinnen abzudr\u00fckken:", "tokens": ["Um", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "sich", "da\u00b7rin\u00b7nen", "ab\u00b7zu\u00b7dr\u00fck\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein man siehet nicht mit Andacht, welche Pracht,", "tokens": ["Al\u00b7lein", "man", "sie\u00b7het", "nicht", "mit", "An\u00b7dacht", ",", "wel\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Bau des K\u00f6rpers schm\u00fckt, den ", "tokens": ["Den", "Bau", "des", "K\u00f6r\u00b7pers", "schm\u00fckt", ",", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Wie wir doch schuldig sind, den Sch\u00f6pfer zu er-", "tokens": ["Wie", "wir", "doch", "schul\u00b7dig", "sind", ",", "den", "Sch\u00f6p\u00b7fer", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der uns so wunderbar die Glieder hat gegeben.", "tokens": ["Der", "uns", "so", "wun\u00b7der\u00b7bar", "die", "Glie\u00b7der", "hat", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Leib ist wie ein Haus, darin die Seele wohnt,", "tokens": ["Der", "Leib", "ist", "wie", "ein", "Haus", ",", "da\u00b7rin", "die", "See\u00b7le", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Darin der rege Geist, als im Verborgnen thront,", "tokens": ["Da\u00b7rin", "der", "re\u00b7ge", "Geist", ",", "als", "im", "Ver\u00b7borg\u00b7nen", "thront", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,", "KOUS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Glieder sind recht fest und k\u00fcnstlich dran ver-", "tokens": ["Die", "Glie\u00b7der", "sind", "recht", "fest", "und", "k\u00fcnst\u00b7lich", "dran", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKVZ", "KON", "ADJD", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Da\u00df man erstaunen mu\u00df, so bald man das beden-", "tokens": ["Da\u00df", "man", "er\u00b7stau\u00b7nen", "mu\u00df", ",", "so", "bald", "man", "das", "be\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,", "ADV", "ADV", "PIS", "ART", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.21": {"text": "Sehn wir ein R\u00e4derwerk, da\u00df sich bewegen kan,", "tokens": ["Sehn", "wir", "ein", "R\u00e4\u00b7der\u00b7werk", ",", "da\u00df", "sich", "be\u00b7we\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und eine Kunstmaschin in ihrem Gange an;", "tokens": ["Und", "ei\u00b7ne", "Kunst\u00b7ma\u00b7schin", "in", "ih\u00b7rem", "Gan\u00b7ge", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So werden wir entz\u00fckt, wir w\u00fcnschen gleich zu", "tokens": ["So", "wer\u00b7den", "wir", "ent\u00b7z\u00fckt", ",", "wir", "w\u00fcn\u00b7schen", "gleich", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wie diese Theilgen sich fest in einander schliessen;", "tokens": ["Wie", "die\u00b7se", "Theil\u00b7gen", "sich", "fest", "in", "ein\u00b7an\u00b7der", "schlies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "PRF", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Die k\u00fcnstlichste Maschin, die sich bewegt und dreht,", "tokens": ["Die", "k\u00fcnst\u00b7lichs\u00b7te", "Ma\u00b7schin", ",", "die", "sich", "be\u00b7wegt", "und", "dreht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Durch R\u00e4der die gespannt, in ihren Lauffe geht,", "tokens": ["Durch", "R\u00e4\u00b7der", "die", "ge\u00b7spannt", ",", "in", "ih\u00b7ren", "Lauf\u00b7fe", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJD", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wird nur ein schlechtes Werk, wenn wir uns un-", "tokens": ["Wird", "nur", "ein", "schlech\u00b7tes", "Werk", ",", "wenn", "wir", "uns", "un"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Den Wunder-vollen Gang des K\u00f6rpers zu er-", "tokens": ["Den", "Wun\u00b7der\u00b7vol\u00b7len", "Gang", "des", "K\u00f6r\u00b7pers", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "O! Menschen! richtet doch eur l\u00fcsternd Augen-", "tokens": ["O", "!", "Men\u00b7schen", "!", "rich\u00b7tet", "doch", "eur", "l\u00fcs\u00b7ternd", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "$.", "VVFIN", "ADV", "PPOSAT", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Auf eurem K\u00f6rper selbst, betrachtet welch ein Werk", "tokens": ["Auf", "eu\u00b7rem", "K\u00f6r\u00b7per", "selbst", ",", "be\u00b7trach\u00b7tet", "welch", "ein", "Werk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "VVPP", "PWAT", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Bau des Leibes ist, wie k\u00fcnstlich er sich len-", "tokens": ["Der", "Bau", "des", "Lei\u00b7bes", "ist", ",", "wie", "k\u00fcnst\u00b7lich", "er", "sich", "len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "PWAV", "ADJD", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Und wie das Glied sich regt, nachdem die Seele", "tokens": ["Und", "wie", "das", "Glied", "sich", "regt", ",", "nach\u00b7dem", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wer diese Wunder kennt, die ", "tokens": ["Wer", "die\u00b7se", "Wun\u00b7der", "kennt", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Den weisesten Entzwek des Sch\u00f6pfers, dran", "tokens": ["Den", "wei\u00b7ses\u00b7ten", "Ent\u00b7zwek", "des", "Sch\u00f6p\u00b7fers", ",", "dran"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Der wird so s\u00fcndlich nicht des H\u00f6chsten Werk be-", "tokens": ["Der", "wird", "so", "s\u00fcnd\u00b7lich", "nicht", "des", "H\u00f6chs\u00b7ten", "Werk", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "PTKNEG", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Vielmehr sich drauf bem\u00fchn, wie es sey zu beschir-", "tokens": ["Viel\u00b7mehr", "sich", "drauf", "be\u00b7m\u00fchn", ",", "wie", "es", "sey", "zu", "be\u00b7schir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "PAV", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "APPR", "TRUNC"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}