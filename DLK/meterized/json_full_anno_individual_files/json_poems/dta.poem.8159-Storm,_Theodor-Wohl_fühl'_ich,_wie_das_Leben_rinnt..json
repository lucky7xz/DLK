{"dta.poem.8159": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wohl f\u00fchl' ich, wie das Leben rinnt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Und da\u00df ich endlich scheiden mu\u00df,", "tokens": ["Und", "da\u00df", "ich", "end\u00b7lich", "schei\u00b7den", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df endlich doch das letzte Lied", "tokens": ["Da\u00df", "end\u00b7lich", "doch", "das", "letz\u00b7te", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und endlich kommt der letzte Ku\u00df.", "tokens": ["Und", "end\u00b7lich", "kommt", "der", "letz\u00b7te", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch h\u00e4ng' ich fest an deinem Mund'", "tokens": ["Noch", "h\u00e4ng'", "ich", "fest", "an", "dei\u00b7nem", "Mund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In schmerzlich bangender Begier;", "tokens": ["In", "schmerz\u00b7lich", "ban\u00b7gen\u00b7der", "Be\u00b7gier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du giebst der Jugend letzten Ku\u00df,", "tokens": ["Du", "giebst", "der", "Ju\u00b7gend", "letz\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die letzte Rose giebst du mir.", "tokens": ["Die", "letz\u00b7te", "Ro\u00b7se", "giebst", "du", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du schenkst aus jenem Zauberkelch", "tokens": ["Du", "schenkst", "aus", "je\u00b7nem", "Zau\u00b7ber\u00b7kelch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den letzten goldnen Trunk mir ein;", "tokens": ["Den", "letz\u00b7ten", "gold\u00b7nen", "Trunk", "mir", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist aus jener M\u00e4rchenwelt", "tokens": ["Du", "bist", "aus", "je\u00b7ner", "M\u00e4r\u00b7chen\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein allerletzter Abendschein.", "tokens": ["Mein", "al\u00b7ler\u00b7letz\u00b7ter", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Am Himmel steht der letzte Stern,", "tokens": ["Am", "Him\u00b7mel", "steht", "der", "letz\u00b7te", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O halte nicht dein Herz zur\u00fcck;", "tokens": ["O", "hal\u00b7te", "nicht", "dein", "Herz", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu deinen F\u00fc\u00dfen sink ich hin,", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "sink", "ich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O f\u00fchl's, du bist mein letztes Gl\u00fcck!", "tokens": ["O", "f\u00fchl's", ",", "du", "bist", "mein", "letz\u00b7tes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df einmal noch durch meine Brust", "tokens": ["La\u00df", "ein\u00b7mal", "noch", "durch", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des vollsten Lebens Schauer wehn,", "tokens": ["Des", "volls\u00b7ten", "Le\u00b7bens", "Schau\u00b7er", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh seufzend in die gro\u00dfe Nacht", "tokens": ["Eh", "seuf\u00b7zend", "in", "die", "gro\u00b7\u00dfe", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch meine Sterne untergehn.", "tokens": ["Auch", "mei\u00b7ne", "Ster\u00b7ne", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}