{"dta.poem.13425": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Gru\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr mehr als englischen Gesichter!", "tokens": ["Ihr", "mehr", "als", "eng\u00b7li\u00b7schen", "Ge\u00b7sich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Gratien in dieser Stadt!", "tokens": ["Ihr", "Gra\u00b7ti\u00b7en", "in", "die\u00b7ser", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es war ja nur ein armer Dichter,", "tokens": ["Es", "war", "ja", "nur", "ein", "ar\u00b7mer", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den euer Gru\u00df beehret hat!", "tokens": ["Den", "eu\u00b7er", "Gru\u00df", "be\u00b7eh\u00b7ret", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch strahlt aus Minen und Geberden", "tokens": ["Euch", "strahlt", "aus", "Mi\u00b7nen", "und", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Holdseeligkeit und Edelmuth,", "tokens": ["Hold\u00b7see\u00b7lig\u00b7keit", "und", "E\u00b7del\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jhr k\u00f6nnet noch wohl Musen werden,", "tokens": ["Ihr", "k\u00f6n\u00b7net", "noch", "wohl", "Mu\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es scheint, ihr seyd den Dichtern gut.", "tokens": ["Es", "scheint", ",", "ihr", "seyd", "den", "Dich\u00b7tern", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Euch ist die ganze Welt gewogen,", "tokens": ["Euch", "ist", "die", "gan\u00b7ze", "Welt", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Welch eine Wohllust, euch zu sehn!", "tokens": ["Welch", "ei\u00b7ne", "Wohl\u00b7lust", ",", "euch", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man spricht, wenn ihr vorbeygezogen:", "tokens": ["Man", "spricht", ",", "wenn", "ihr", "vor\u00b7bey\u00b7ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind sie nicht allerliebst und sch\u00f6n!", "tokens": ["Sind", "sie", "nicht", "al\u00b7ler\u00b7liebst", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hielte mir vor kein Verbrechen,", "tokens": ["Ich", "hiel\u00b7te", "mir", "vor", "kein", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sollt ich an Paris Stelle seyn,", "tokens": ["Sollt", "ich", "an", "Pa\u00b7ris", "Stel\u00b7le", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NE", "NN", "VAINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Die g\u00fcldne Frucht euch zuzusprechen,", "tokens": ["Die", "g\u00fcld\u00b7ne", "Frucht", "euch", "zu\u00b7zu\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sagte: Kinder! theilt euch drein!", "tokens": ["Und", "sag\u00b7te", ":", "Kin\u00b7der", "!", "theilt", "euch", "drein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit was f\u00fcr rednerischen Blicken", "tokens": ["Mit", "was", "f\u00fcr", "red\u00b7ne\u00b7ri\u00b7schen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Spielt euer sittsam Auge frey!", "tokens": ["Spielt", "eu\u00b7er", "sitt\u00b7sam", "Au\u00b7ge", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie scheinen niemand vorzur\u00fccken,", "tokens": ["Sie", "schei\u00b7nen", "nie\u00b7mand", "vor\u00b7zu\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob er deren unwerth sey;", "tokens": ["Als", "ob", "er", "de\u00b7ren", "un\u00b7werth", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PDS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was Herzen habt ihr doch gefangen,", "tokens": ["Was", "Her\u00b7zen", "habt", "ihr", "doch", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihnen Fesseln angelegt!", "tokens": ["Und", "ih\u00b7nen", "Fes\u00b7seln", "an\u00b7ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bin ja nur vorbeygegangen!", "tokens": ["Ich", "bin", "ja", "nur", "vor\u00b7bey\u00b7ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie hat mich euer Gru\u00df bewegt!", "tokens": ["Wie", "hat", "mich", "eu\u00b7er", "Gru\u00df", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der gr\u00fcne Gott der Lauberh\u00fctten,", "tokens": ["Der", "gr\u00fc\u00b7ne", "Gott", "der", "Lau\u00b7ber\u00b7h\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sein bey andern s\u00fcsser Zwang", "tokens": ["Und", "sein", "bey", "an\u00b7dern", "s\u00fcs\u00b7ser", "Zwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Legt nur bey Sch\u00f6nen sich aufs Bitten,", "tokens": ["Legt", "nur", "bey", "Sch\u00f6\u00b7nen", "sich", "aufs", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unsern so ber\u00fchmten Gang;", "tokens": ["In", "un\u00b7sern", "so", "be\u00b7r\u00fchm\u00b7ten", "Gang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allwo er vor der D\u00e4mmrungsk\u00fchle,", "tokens": ["All\u00b7wo", "er", "vor", "der", "D\u00e4mm\u00b7rungs\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sich der Sommer eingestellt,", "tokens": ["Wenn", "sich", "der", "Som\u00b7mer", "ein\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf manchen pr\u00e4chtigen Gest\u00fchle", "tokens": ["Auf", "man\u00b7chen", "pr\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7st\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein denkend Wesen unterh\u00e4lt.", "tokens": ["Ein", "den\u00b7kend", "We\u00b7sen", "un\u00b7ter\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier darf die Schwermuth nicht erscheinen,", "tokens": ["Hier", "darf", "die", "Schwer\u00b7muth", "nicht", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wohnen Lachen, Scherz und Ruh,", "tokens": ["Da", "woh\u00b7nen", "La\u00b7chen", ",", "Scherz", "und", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Keuschheit wandelt mit den Reinen,", "tokens": ["Die", "Keuschheit", "wan\u00b7delt", "mit", "den", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und h\u00f6ret den Gespr\u00e4chen zu,", "tokens": ["Und", "h\u00f6\u00b7ret", "den", "Ge\u00b7spr\u00e4\u00b7chen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schwamm kurz vorher ein Aug in Thr\u00e4nen,", "tokens": ["Schwamm", "kurz", "vor\u00b7her", "ein", "Aug", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O! dieser Qvell verseiget hie,", "tokens": ["O", "!", "die\u00b7ser", "Qvell", "ver\u00b7sei\u00b7get", "hie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir aber schw\u00e4rmen nur die Sch\u00f6nen", "tokens": ["Mir", "a\u00b7ber", "schw\u00e4r\u00b7men", "nur", "die", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In der gereizten Phantasie.", "tokens": ["In", "der", "ge\u00b7reiz\u00b7ten", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie, die von dem Geschlecht der Frauen,", "tokens": ["Sie", ",", "die", "von", "dem", "Ge\u00b7schlecht", "der", "Frau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ihren jungfr\u00e4ulichen Gru\u00df,", "tokens": ["Durch", "ih\u00b7ren", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein wohlgesinnet Herz erbauen,", "tokens": ["Mein", "wohl\u00b7ge\u00b7sin\u00b7net", "Herz", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es ihn lange f\u00fchlen mu\u00df,", "tokens": ["Da\u00df", "es", "ihn", "lan\u00b7ge", "f\u00fch\u00b7len", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schleicht immer unter gr\u00fcnen Ranken,", "tokens": ["Schleicht", "im\u00b7mer", "un\u00b7ter", "gr\u00fc\u00b7nen", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr wirket doch auf mein Gem\u00fcth,", "tokens": ["Ihr", "wir\u00b7ket", "doch", "auf", "mein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich aber weih euch in Gedanken", "tokens": ["Ich", "a\u00b7ber", "weih", "euch", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bereits mein allerneustes Lied.", "tokens": ["Be\u00b7reits", "mein", "al\u00b7ler\u00b7neus\u00b7tes", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie hei\u00dft man euch, beliebte Sch\u00f6nen?", "tokens": ["Wie", "hei\u00dft", "man", "euch", ",", "be\u00b7lieb\u00b7te", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6pfung letztes Wunderwerk?", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "letz\u00b7tes", "Wun\u00b7der\u00b7werk", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn euch soll mein Gesang ert\u00f6nen,", "tokens": ["Denn", "euch", "soll", "mein", "Ge\u00b7sang", "er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr seyd der Dichtkunst Augenmerk,", "tokens": ["Ihr", "seyd", "der", "Dicht\u00b7kunst", "Au\u00b7gen\u00b7merk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "M\u00f6cht ich euch kennen! welches Eden", "tokens": ["M\u00f6cht", "ich", "euch", "ken\u00b7nen", "!", "wel\u00b7ches", "E\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist eurer Menschlichkeit Behalt?", "tokens": ["Ist", "eu\u00b7rer", "Menschlich\u00b7keit", "Be\u00b7halt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "D\u00fcrft ihr mit fremder Tugend reden?", "tokens": ["D\u00fcrft", "ihr", "mit", "frem\u00b7der", "Tu\u00b7gend", "re\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "