{"dta.poem.19492": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Im Konzert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Heut im Konzerte hielt ich Zwiegespr\u00e4ch", "tokens": ["Heut", "im", "Kon\u00b7zer\u00b7te", "hielt", "ich", "Zwie\u00b7ge\u00b7spr\u00e4ch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mit einem allerliebsten M\u00e4dchenhals,", "tokens": ["Mit", "ei\u00b7nem", "al\u00b7ler\u00b7liebs\u00b7ten", "M\u00e4d\u00b7chen\u00b7hals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der aus derselben Bank geschimmert schon", "tokens": ["Der", "aus", "der\u00b7sel\u00b7ben", "Bank", "ge\u00b7schim\u00b7mert", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN", "VVPP", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein fr\u00fcher Mal ... Du hattest, sagt' ich ihm,", "tokens": ["Ein", "fr\u00fc\u00b7her", "Mal", "...", "Du", "hat\u00b7test", ",", "sagt'", "ich", "ihm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein schmales Kettlein an, besinne dich!", "tokens": ["Ein", "schma\u00b7les", "Ket\u00b7tlein", "an", ",", "be\u00b7sin\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vielteilig, sein gef\u00fcgt, von blassem Gold,", "tokens": ["Viel\u00b7tei\u00b7lig", ",", "sein", "ge\u00b7f\u00fcgt", ",", "von", "blas\u00b7sem", "Gold", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "S\u00fc\u00df leuchtend aus dem Dunkel des Gewands.", "tokens": ["S\u00fc\u00df", "leuch\u00b7tend", "aus", "dem", "Dun\u00b7kel", "des", "Ge\u00b7wands", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verloren ging's? Vielleicht ist's nur verlegt?", "tokens": ["Ver\u00b7lo\u00b7ren", "ging's", "?", "Viel\u00b7leicht", "ist's", "nur", "ver\u00b7legt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zerbrach es eben erst der Finger Hast?", "tokens": ["Zer\u00b7brach", "es", "e\u00b7ben", "erst", "der", "Fin\u00b7ger", "Hast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Trug's ein Gespiel davon, ein schmeichelndes?", "tokens": ["Trug's", "ein", "Ge\u00b7spiel", "da\u00b7von", ",", "ein", "schmei\u00b7cheln\u00b7des", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PAV", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Warf, dich bet\u00f6rend, eine Hand dir's um,", "tokens": ["Warf", ",", "dich", "be\u00b7t\u00f6\u00b7rend", ",", "ei\u00b7ne", "Hand", "dir's", "um", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVPP", "$,", "ART", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Treue brach? Du hassest jetzt das Band?", "tokens": ["Die", "Treu\u00b7e", "brach", "?", "Du", "has\u00b7sest", "jetzt", "das", "Band", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Du trauerst, H\u00e4lschen? Heute neigst du dich", "tokens": ["Du", "trau\u00b7erst", ",", "H\u00e4l\u00b7schen", "?", "Heu\u00b7te", "neigst", "du", "dich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ein bischen tiefer als das letzte Mal?", "tokens": ["Ein", "bi\u00b7schen", "tie\u00b7fer", "als", "das", "letz\u00b7te", "Mal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Der eigenartige Satz: Die Fl\u00f6te klagt:", "tokens": ["Der", "ei\u00b7gen\u00b7ar\u00b7ti\u00b7ge", "Satz", ":", "Die", "Fl\u00f6\u00b7te", "klagt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "\u201edas H\u00e4lschen neigt sich etwas tiefer heut!\u201c", "tokens": ["\u201e", "das", "H\u00e4l\u00b7schen", "neigt", "sich", "et\u00b7was", "tie\u00b7fer", "heut", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "ADV", "ADJD", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "\u201eo dunkles Schicksal!\u201c dr\u00f6hnt verh\u00e4ngnisvoll", "tokens": ["\u201e", "o", "dunk\u00b7les", "Schick\u00b7sal", "!", "\u201c", "dr\u00f6hnt", "ver\u00b7h\u00e4ng\u00b7nis\u00b7voll"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "FM", "ADJA", "NN", "$.", "$(", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Das melancholische Violoncell ...", "tokens": ["Das", "me\u00b7lan\u00b7cho\u00b7li\u00b7sche", "Vi\u00b7o\u00b7lon\u00b7cell", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Ein feines Gl\u00f6ckchen aber spottet hell:", "tokens": ["Ein", "fei\u00b7nes", "Gl\u00f6ck\u00b7chen", "a\u00b7ber", "spot\u00b7tet", "hell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "\u201edas Kettlein steckt im blauen Sammt des Schreins.", "tokens": ["\u201e", "das", "Ket\u00b7tlein", "steckt", "im", "blau\u00b7en", "Sammt", "des", "Schreins", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Aus einer reinen Laune blieb's zu Haus.\u201c", "tokens": ["Aus", "ei\u00b7ner", "rei\u00b7nen", "Lau\u00b7ne", "blie\u00b7b's", "zu", "Haus", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "APPR", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}