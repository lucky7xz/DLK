{"textgrid.poem.41202": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blick' auf zur Halle, Knabe! Sined's Brust", "tokens": ["Blick'", "auf", "zur", "Hal\u00b7le", ",", "Kna\u00b7be", "!", "Si\u00b7ne\u00b7d's", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "APPRART", "NN", "$,", "NN", "$.", "NE", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Beginnet schwer zu athmen. Da\u00df vielleicht", "tokens": ["Be\u00b7gin\u00b7net", "schwer", "zu", "ath\u00b7men", ".", "Da\u00df", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKZU", "VVINF", "$.", "KOUS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wetter sich an fernen Bergen hebt.", "tokens": ["Ein", "Wet\u00b7ter", "sich", "an", "fer\u00b7nen", "Ber\u00b7gen", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und trat ins Freie. Siehe, mit dem Weh'n", "tokens": ["Und", "trat", "ins", "Frei\u00b7e", ".", "Sie\u00b7he", ",", "mit", "dem", "Weh'n"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "VVIMP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Donau zog's herunter, wie die Nacht,", "tokens": ["Der", "Do\u00b7nau", "zo\u00b7g's", "her\u00b7un\u00b7ter", ",", "wie", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Gebirgig, keileschwanger. Keiner doch", "tokens": ["Ge\u00b7bir\u00b7gig", ",", "kei\u00b7le\u00b7schwan\u00b7ger", ".", "Kei\u00b7ner", "doch"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "PIAT", "$.", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Keile ri\u00df sich los. Nur murreten", "tokens": ["Der", "Kei\u00b7le", "ri\u00df", "sich", "los", ".", "Nur", "mur\u00b7re\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Die Donner manchmal dumpf, durchzitterten", "tokens": ["Die", "Don\u00b7ner", "manch\u00b7mal", "dumpf", ",", "durch\u00b7zit\u00b7ter\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Noch matte Blitze die Gewitternacht.", "tokens": ["Noch", "mat\u00b7te", "Blit\u00b7ze", "die", "Ge\u00b7wit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So zog es deine Th\u00fcrme, Wien! vorbei,", "tokens": ["So", "zog", "es", "dei\u00b7ne", "Th\u00fcr\u00b7me", ",", "Wi\u00b7en", "!", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "NE", "$.", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Und wandte sich auf einmal in's Gefild", "tokens": ["Und", "wand\u00b7te", "sich", "auf", "ein\u00b7mal", "in's", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Quaden hin, und dort, dort brach es aus.", "tokens": ["Der", "Qua\u00b7den", "hin", ",", "und", "dort", ",", "dort", "brach", "es", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie liegt gest\u00fcrzet \u2013 Knabe! mein Harfenspiel", "tokens": ["Sie", "liegt", "ge\u00b7st\u00fcr\u00b7zet", "\u2013", "Kna\u00b7be", "!", "mein", "Har\u00b7fen\u00b7spiel"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "$(", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Heraus, mit allem seinen erseufzenden", "tokens": ["He\u00b7raus", ",", "mit", "al\u00b7lem", "sei\u00b7nen", "er\u00b7seuf\u00b7zen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PIS", "PPOSAT", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tief aufgest\u00fcrmten Wehgekl\u00e4nge!", "tokens": ["Tief", "auf\u00b7ge\u00b7st\u00fcrm\u00b7ten", "Weh\u00b7ge\u00b7kl\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bringe mein Harfenspiel! \u2013 Ach gest\u00fcrzet,", "tokens": ["Brin\u00b7ge", "mein", "Har\u00b7fen\u00b7spiel", "!", "\u2013", "Ach", "ge\u00b7st\u00fcr\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$(", "ITJ", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Gest\u00fcrzet liegt sie! Feuer vom Himmel fra\u00df", "tokens": ["Ge\u00b7st\u00fcr\u00b7zet", "liegt", "sie", "!", "Feu\u00b7er", "vom", "Him\u00b7mel", "fra\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den fetten hohen Wipfel, verschleuderte", "tokens": ["Den", "fet\u00b7ten", "ho\u00b7hen", "Wip\u00b7fel", ",", "ver\u00b7schleu\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Wald der Aeste, schlug in hundert", "tokens": ["Den", "Wald", "der", "A\u00b7es\u00b7te", ",", "schlug", "in", "hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "CARD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dampfende Br\u00e4nde den Stamm der Eiche!", "tokens": ["Damp\u00b7fen\u00b7de", "Br\u00e4n\u00b7de", "den", "Stamm", "der", "Ei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Neun Herbste sind hin\u00fcber, da hattet ihr", "tokens": ["Neun", "Herbs\u00b7te", "sind", "hin\u00b7\u00fc\u00b7ber", ",", "da", "hat\u00b7tet", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "ADV", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Wunsch des Barden, S\u00f6hne von Teut! geh\u00f6rt,", "tokens": ["Den", "Wunsch", "des", "Bar\u00b7den", ",", "S\u00f6h\u00b7ne", "von", "Teut", "!", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "APPR", "NE", "$.", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und hattet nach des Barden Wunsche", "tokens": ["Und", "hat\u00b7tet", "nach", "des", "Bar\u00b7den", "Wun\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Diesen bedeutenden Baum gepflanzet", "tokens": ["Die\u00b7sen", "be\u00b7deu\u00b7ten\u00b7den", "Baum", "ge\u00b7pflan\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVPP"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Den Baum gepflanzet, ", "tokens": ["Den", "Baum", "ge\u00b7pflan\u00b7zet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Gr\u00f6\u00dften Deutschlands, ", "tokens": ["Die", "Gr\u00f6\u00df\u00b7ten", "Deutschlands", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gepflanzet, da\u00df in seinem Schatten", "tokens": ["Ge\u00b7pflan\u00b7zet", ",", "da\u00df", "in", "sei\u00b7nem", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihrer Umarmungen Enkel d\u00e4chten.", "tokens": ["Ih\u00b7rer", "Um\u00b7ar\u00b7mun\u00b7gen", "En\u00b7kel", "d\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Neun Herbste sah'n ihn sprie\u00dfen. Er deckete", "tokens": ["Neun", "Herbs\u00b7te", "sah'n", "ihn", "sprie\u00b7\u00dfen", ".", "Er", "de\u00b7cke\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit breiten Armen \u00f6fter der Jugend Spiel,", "tokens": ["Mit", "brei\u00b7ten", "Ar\u00b7men", "\u00f6f\u00b7ter", "der", "Ju\u00b7gend", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Liebe Fl\u00fcstern, und der Greise", "tokens": ["Der", "Lie\u00b7be", "Fl\u00fcs\u00b7tern", ",", "und", "der", "Grei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Biedergespr\u00e4ch, ein erhaben Denkmal!", "tokens": ["Bie\u00b7der\u00b7ge\u00b7spr\u00e4ch", ",", "ein", "er\u00b7ha\u00b7ben", "Denk\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Nun liegt er nieder, und der Umarmungen", "tokens": ["Nun", "liegt", "er", "nie\u00b7der", ",", "und", "der", "Um\u00b7ar\u00b7mun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist ach vergessen! Friederich wecket ihn", "tokens": ["Ist", "ach", "ver\u00b7ges\u00b7sen", "!", "Frie\u00b7de\u00b7rich", "we\u00b7cket", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "NE", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den alten Zwist, die grauen Locken", "tokens": ["Den", "al\u00b7ten", "Zwist", ",", "die", "grau\u00b7en", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Birgt er noch einmal im Herrscherhelme;", "tokens": ["Birgt", "er", "noch", "ein\u00b7mal", "im", "Herr\u00b7scher\u00b7hel\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Mit Schwert und Lanze steht er, und fertiget", "tokens": ["Mit", "Schwert", "und", "Lan\u00b7ze", "steht", "er", ",", "und", "fer\u00b7ti\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach seinen Starken r\u00fcstige Boten hin:", "tokens": ["Nach", "sei\u00b7nen", "Star\u00b7ken", "r\u00fcs\u00b7ti\u00b7ge", "Bo\u00b7ten", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbnein! Joseph's Erbe soll nicht wachsen!\u00ab", "tokens": ["\u00bb", "nein", "!", "Jo\u00b7se\u00b7ph's", "Er\u00b7be", "soll", "nicht", "wach\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$.", "NE", "NN", "VMFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Sag't es, und klopfet umher am Schilde.", "tokens": ["Sag't", "es", ",", "und", "klop\u00b7fet", "um\u00b7her", "am", "Schil\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Sie geh'n und sagen. M\u00e4chtig erklingt ihr Schild", "tokens": ["Sie", "geh'n", "und", "sa\u00b7gen", ".", "M\u00e4ch\u00b7tig", "er\u00b7klingt", "ihr", "Schild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$.", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im ganzen Brennenreiche. So f\u00e4hrt der Sturm", "tokens": ["Im", "gan\u00b7zen", "Bren\u00b7nen\u00b7rei\u00b7che", ".", "So", "f\u00e4hrt", "der", "Sturm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Wasserwelt hinan, und zahllos", "tokens": ["Die", "Was\u00b7ser\u00b7welt", "hi\u00b7nan", ",", "und", "zahl\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werfen die Wogen ihr sch\u00e4umend Haupt auf. \u2013", "tokens": ["Wer\u00b7fen", "die", "Wo\u00b7gen", "ihr", "sch\u00e4u\u00b7mend", "Haupt", "auf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "NN", "PTKVZ", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Ha, Sohn des Himmels! f\u00e4rbet Heldenblut", "tokens": ["Ha", ",", "Sohn", "des", "Him\u00b7mels", "!", "f\u00e4r\u00b7bet", "Hel\u00b7den\u00b7blut"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "NN", "ART", "NN", "$.", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Antlitz? Klagst du die Gewaltigen,", "tokens": ["Dein", "Ant\u00b7litz", "?", "Klagst", "du", "die", "Ge\u00b7wal\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die fallen werden? Fallen werden sie!", "tokens": ["Die", "fal\u00b7len", "wer\u00b7den", "?", "Fal\u00b7len", "wer\u00b7den", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VAFIN", "$.", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir wittert offner Gr\u00e4ber feuchter Duft.", "tokens": ["Mir", "wit\u00b7tert", "off\u00b7ner", "Gr\u00e4\u00b7ber", "feuch\u00b7ter", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir ahnet fernher leises Lautes so,", "tokens": ["Mir", "ah\u00b7net", "fern\u00b7her", "lei\u00b7ses", "Lau\u00b7tes", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "ADV