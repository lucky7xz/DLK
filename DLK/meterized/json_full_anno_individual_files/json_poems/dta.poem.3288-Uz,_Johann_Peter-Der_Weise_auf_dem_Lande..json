{"dta.poem.3288": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Weise auf dem Lande.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "O Wald! o Schatten gr\u00fcner G\u00e4nge!", "tokens": ["O", "Wald", "!", "o", "Schat\u00b7ten", "gr\u00fc\u00b7ner", "G\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebte Flur voll Fr\u00fchlings Pracht!", "tokens": ["Ge\u00b7lieb\u00b7te", "Flur", "voll", "Fr\u00fch\u00b7lings", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich hat vom st\u00e4dtischen Gedr\u00e4nge", "tokens": ["Mich", "hat", "vom", "st\u00e4d\u00b7ti\u00b7schen", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein g\u00fcnstig Gl\u00fcck zu euch gebracht:", "tokens": ["Mein", "g\u00fcns\u00b7tig", "Gl\u00fcck", "zu", "euch", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ich, nach unruhvollen Stunden,", "tokens": ["Wo", "ich", ",", "nach", "un\u00b7ruh\u00b7vol\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ruhe, die dem Weisen lacht,", "tokens": ["Die", "Ru\u00b7he", ",", "die", "dem", "Wei\u00b7sen", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jm Schoose der Natur gefunden.", "tokens": ["Jm", "Schoo\u00b7se", "der", "Na\u00b7tur", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchle mich wie neugebohren,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "wie", "neu\u00b7ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und fang erst nun zu leben an,", "tokens": ["Und", "fang", "erst", "nun", "zu", "le\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit, fern vom Trotze reicher Thoren,", "tokens": ["Seit", ",", "fern", "vom", "Trot\u00b7ze", "rei\u00b7cher", "Tho\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPRART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hier in Freyheit athmen kann.", "tokens": ["Ich", "hier", "in", "Frey\u00b7heit", "ath\u00b7men", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es krieche wer nach Ehre flieget!", "tokens": ["Es", "krie\u00b7che", "wer", "nach", "Eh\u00b7re", "flie\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich werde nie ein grosser Mann,", "tokens": ["Ich", "wer\u00b7de", "nie", "ein", "gros\u00b7ser", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil ich mich knechtisch nicht geschmieget.", "tokens": ["Weil", "ich", "mich", "knech\u00b7tisch", "nicht", "ge\u00b7schmie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es m\u00f6gen andre h\u00f6her trachten:", "tokens": ["Es", "m\u00f6\u00b7gen", "and\u00b7re", "h\u00f6\u00b7her", "trach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie m\u00f6gen, hungrig nach Gewinn,", "tokens": ["Sie", "m\u00f6\u00b7gen", ",", "hung\u00b7rig", "nach", "Ge\u00b7winn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Joche der Gesch\u00e4fte schmachten,", "tokens": ["Jm", "Jo\u00b7che", "der", "Ge\u00b7sch\u00e4f\u00b7te", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich der Knechtschaft m\u00fcde bin!", "tokens": ["Da", "ich", "der", "Knecht\u00b7schaft", "m\u00fc\u00b7de", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie dr\u00e4ngen sich durch List und Gaben", "tokens": ["Sie", "dr\u00e4n\u00b7gen", "sich", "durch", "List", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An ihre Ruderb\u00e4nke hin;", "tokens": ["An", "ih\u00b7re", "Ru\u00b7der\u00b7b\u00e4n\u00b7ke", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dieweil sie Sklavenseelen haben.", "tokens": ["Die\u00b7weil", "sie", "Skla\u00b7ven\u00b7see\u00b7len", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du gl\u00e4nzend Nichts! o Rauch der Ehre!", "tokens": ["Du", "gl\u00e4n\u00b7zend", "Nichts", "!", "o", "Rauch", "der", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PIS", "$.", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich kauf ich nicht mit wahrem Weh.", "tokens": ["Dich", "kauf", "ich", "nicht", "mit", "wah\u00b7rem", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Geist sey, nach der Weisheit Lehre,", "tokens": ["Mein", "Geist", "sey", ",", "nach", "der", "Weis\u00b7heit", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So stille, wie die Sommersee:", "tokens": ["So", "stil\u00b7le", ",", "wie", "die", "Som\u00b7mer\u00b7see", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ruhig im Genu\u00df der Freuden,", "tokens": ["So", "ru\u00b7hig", "im", "Ge\u00b7nu\u00df", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als dort, im perlenreichen Klee,", "tokens": ["Als", "dort", ",", "im", "per\u00b7len\u00b7rei\u00b7chen", "Klee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die unschuldvollen L\u00e4mmer weiden!", "tokens": ["Die", "un\u00b7schuld\u00b7vol\u00b7len", "L\u00e4m\u00b7mer", "wei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O seht, wie \u00fcber gr\u00fcne H\u00fcgel", "tokens": ["O", "seht", ",", "wie", "\u00fc\u00b7ber", "gr\u00fc\u00b7ne", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag, bekr\u00e4nzt mit Rosen, naht!", "tokens": ["Der", "Tag", ",", "be\u00b7kr\u00e4nzt", "mit", "Ro\u00b7sen", ",", "naht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhn k\u00fchlen Zephyrs linde Fl\u00fcgel:", "tokens": ["Jhn", "k\u00fch\u00b7len", "Ze\u00b7phyrs", "lin\u00b7de", "Fl\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Thau gl\u00e4nzt sein bebl\u00fchmter Pfad.", "tokens": ["Vom", "Thau", "gl\u00e4nzt", "sein", "be\u00b7bl\u00fchm\u00b7ter", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie taumelt Flora durch die Triften!", "tokens": ["Wie", "tau\u00b7melt", "Flo\u00b7ra", "durch", "die", "Trif\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lerche steigt aus trunkner Saat,", "tokens": ["Die", "Ler\u00b7che", "steigt", "aus", "trunk\u00b7ner", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und singt in unbew\u00f6lkten L\u00fcften.", "tokens": ["Und", "singt", "in", "un\u00b7be\u00b7w\u00f6lk\u00b7ten", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dort, wo im Schatten schlanker Buchen", "tokens": ["Dort", ",", "wo", "im", "Schat\u00b7ten", "schlan\u00b7ker", "Bu\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Quelle zwischen Bluhmen schw\u00e4tzt;", "tokens": ["Die", "Quel\u00b7le", "zwi\u00b7schen", "Bluh\u00b7men", "schw\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh ich die Muse mich besuchen,", "tokens": ["Seh", "ich", "die", "Mu\u00b7se", "mich", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und werde durch ihr Lied erg\u00f6tzt.", "tokens": ["Und", "wer\u00b7de", "durch", "ihr", "Lied", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie singt entz\u00fcckt in g\u00fcldne Saiten,", "tokens": ["Sie", "singt", "ent\u00b7z\u00fcckt", "in", "g\u00fcld\u00b7ne", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Inde\u00df, von Morgenthau benetzt,", "tokens": ["In\u00b7de\u00df", ",", "von", "Mor\u00b7gent\u00b7hau", "be\u00b7netzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Haare flatternd sich verbreiten.", "tokens": ["Die", "Haa\u00b7re", "flat\u00b7ternd", "sich", "ver\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch s\u00fcsser t\u00f6nt um frische Rosen", "tokens": ["Noch", "s\u00fcs\u00b7ser", "t\u00f6nt", "um", "fri\u00b7sche", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr angenehmes Hirtenrohr;", "tokens": ["Ihr", "an\u00b7ge\u00b7neh\u00b7mes", "Hir\u00b7ten\u00b7rohr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Amor kommt, ihr liebzukosen,", "tokens": ["Und", "A\u00b7mor", "kommt", ",", "ihr", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ieder Ton entz\u00fcckt sein Ohr.", "tokens": ["Und", "ie\u00b7der", "Ton", "ent\u00b7z\u00fcckt", "sein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch er versucht, wies ihm gelinget:", "tokens": ["Auch", "er", "ver\u00b7sucht", ",", "wies", "ihm", "ge\u00b7lin\u00b7get", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein schwaches Murmeln quillt hervor,", "tokens": ["Ein", "schwa\u00b7ches", "Mur\u00b7meln", "quillt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das unge\u00fcbte Hand erzwinget.", "tokens": ["Das", "un\u00b7ge\u00b7\u00fcb\u00b7te", "Hand", "er\u00b7zwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Geht hin, die ihr nach Golde schnaubet!", "tokens": ["Geht", "hin", ",", "die", "ihr", "nach", "Gol\u00b7de", "schnau\u00b7bet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht Freude, die mein Herz verschm\u00e4ht!", "tokens": ["Sucht", "Freu\u00b7de", ",", "die", "mein", "Herz", "ver\u00b7schm\u00e4ht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Betr\u00fcgt, verrathet, schindet, raubet", "tokens": ["Be\u00b7tr\u00fcgt", ",", "ver\u00b7ra\u00b7thet", ",", "schin\u00b7det", ",", "rau\u00b7bet"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und erndet, was die Wittwe s\u00e4t!", "tokens": ["Und", "ern\u00b7det", ",", "was", "die", "Witt\u00b7we", "s\u00e4t", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure"