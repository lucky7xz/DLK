{"textgrid.poem.36783": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Tempel des Jupiter Stator", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir erbaute das siegende Rom, o Jupiter Stator,", "tokens": ["Dir", "er\u00b7bau\u00b7te", "das", "sie\u00b7gen\u00b7de", "Rom", ",", "o", "Ju\u00b7pi\u00b7ter", "Sta\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "$,", "FM", "NN", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dankbar ein S\u00e4ulenhaus, weil du es siegen gelehrt.", "tokens": ["Dank\u00b7bar", "ein", "S\u00e4u\u00b7len\u00b7haus", ",", "weil", "du", "es", "sie\u00b7gen", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Herrscher, durch deine Macht triumphirte der R\u00f6mer und beugte", "tokens": ["Herr\u00b7scher", ",", "durch", "dei\u00b7ne", "Macht", "tri\u00b7um\u00b7phir\u00b7te", "der", "R\u00f6\u00b7mer", "und", "beug\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+-++-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Seinem Scepter die Welt, die du f\u00fcr deinen bestimmt.", "tokens": ["Sei\u00b7nem", "Scep\u00b7ter", "die", "Welt", ",", "die", "du", "f\u00fcr", "dei\u00b7nen", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Freilich warst du ein heidnischer Gott, und glichest den Menschen,", "tokens": ["Frei\u00b7lich", "warst", "du", "ein", "heid\u00b7ni\u00b7scher", "Gott", ",", "und", "gli\u00b7chest", "den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Doch die Menschen daf\u00fcr glichen dem G\u00f6ttergeschlecht.", "tokens": ["Doch", "die", "Men\u00b7schen", "da\u00b7f\u00fcr", "gli\u00b7chen", "dem", "G\u00f6t\u00b7ter\u00b7ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nun ist's anders, am Haus des Olympiers h\u00e4ngen Ger\u00fcste,", "tokens": ["Nun", "ist's", "an\u00b7ders", ",", "am", "Haus", "des", "O\u00b7lym\u00b7piers", "h\u00e4n\u00b7gen", "Ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und mit Zirkel und Maa\u00df forscht man das Dasein ihm aus.", "tokens": ["Und", "mit", "Zir\u00b7kel", "und", "Maa\u00df", "forscht", "man", "das", "Da\u00b7sein", "ihm", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "PIS", "ART", "NN", "PPER", "PTKVZ", "$."], "meter"