{"dta.poem.10391": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XvII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wie die Wellenschaumgeborene", "tokens": ["Wie", "die", "Wel\u00b7len\u00b7schaum\u00b7ge\u00b7bo\u00b7re\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Stralt mein Lieb in Sch\u00f6nheitsglanz,", "tokens": ["Stralt", "mein", "Lieb", "in", "Sch\u00f6n\u00b7heits\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie ist das auserkorene", "tokens": ["Denn", "sie", "ist", "das", "au\u00b7ser\u00b7ko\u00b7re\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Br\u00e4utchen eines fremden Manns.", "tokens": ["Br\u00e4ut\u00b7chen", "ei\u00b7nes", "frem\u00b7den", "Manns", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herz, mein Herz, du vielgeduldiges,", "tokens": ["Herz", ",", "mein", "Herz", ",", "du", "viel\u00b7ge\u00b7dul\u00b7di\u00b7ges", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "PIS", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Grolle nicht ob dem Verrath;", "tokens": ["Grol\u00b7le", "nicht", "ob", "dem", "Ver\u00b7rath", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trag es, trag es, und entschuldig' es,", "tokens": ["Trag", "es", ",", "trag", "es", ",", "und", "ent\u00b7schul\u00b7dig'", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was die holde Th\u00f6rin that.", "tokens": ["Was", "die", "hol\u00b7de", "Th\u00f6\u00b7rin", "that", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}