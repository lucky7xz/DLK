{"textgrid.poem.51196": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Landstra\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieht eine arme Pilgerin,", "tokens": ["Zieht", "ei\u00b7ne", "ar\u00b7me", "Pil\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geb\u00fcckt und schwach, am d\u00fcrren Stab", "tokens": ["Ge\u00b7b\u00fcckt", "und", "schwach", ",", "am", "d\u00fcr\u00b7ren", "Stab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur gnadenreichen Jungfrau hin;", "tokens": ["Zur", "gna\u00b7den\u00b7rei\u00b7chen", "Jung\u00b7frau", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Rosenkranz rollt auf und ab,", "tokens": ["Der", "Ro\u00b7sen\u00b7kranz", "rollt", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Obwohl er sie nicht hindern kann,", "tokens": ["Ob\u00b7wohl", "er", "sie", "nicht", "hin\u00b7dern", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch ihres Leibes zu gedenken", "tokens": ["Auch", "ih\u00b7res", "Lei\u00b7bes", "zu", "ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und auf den r\u00fcst'gen Wandersmann", "tokens": ["Und", "auf", "den", "r\u00fcst'\u00b7gen", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem\u00fctig ihren Blick zu lenken.", "tokens": ["De\u00b7m\u00fc\u00b7tig", "ih\u00b7ren", "Blick", "zu", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbmein junger Herr! erbarmet Euch,", "tokens": ["\u00bb", "mein", "jun\u00b7ger", "Herr", "!", "er\u00b7bar\u00b7met", "Euch", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Gott Euch mag barmherzig sein!", "tokens": ["Wie", "Gott", "Euch", "mag", "barm\u00b7her\u00b7zig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er geb Euch einst sein Himmelreich", "tokens": ["Er", "geb", "Euch", "einst", "sein", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinen Segen obendrein!\u00ab \u2013", "tokens": ["Und", "sei\u00b7nen", "Se\u00b7gen", "o\u00b7ben\u00b7drein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbich glaube nicht an deinen Gott,", "tokens": ["\u00bb", "ich", "glau\u00b7be", "nicht", "an", "dei\u00b7nen", "Gott", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr den dort deine Kugeln rollen;", "tokens": ["F\u00fcr", "den", "dort", "dei\u00b7ne", "Ku\u00b7geln", "rol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drum schien' es mir ein arger Spott,", "tokens": ["Drum", "schien'", "es", "mir", "ein", "ar\u00b7ger", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00fcrd ich dir eine Gabe zollen!\u00ab", "tokens": ["W\u00fcrd", "ich", "dir", "ei\u00b7ne", "Ga\u00b7be", "zol\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch fort geht ihrer Rede Lauf:", "tokens": ["Doch", "fort", "geht", "ih\u00b7rer", "Re\u00b7de", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbgott segne Euer junges Haupt", "tokens": ["\u00bb", "gott", "seg\u00b7ne", "Eu\u00b7er", "jun\u00b7ges", "Haupt"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und heb Euch seinen Segen auf,", "tokens": ["Und", "heb", "Euch", "sei\u00b7nen", "Se\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis Ihr allendlich an ihn glaubt!\u00ab", "tokens": ["Bis", "Ihr", "al\u00b7lend\u00b7lich", "an", "ihn", "glaubt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Und dankend nimmt sie meinen Sold", "tokens": ["Und", "dan\u00b7kend", "nimmt", "sie", "mei\u00b7nen", "Sold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und betet fort auf ihren Wegen;", "tokens": ["Und", "be\u00b7tet", "fort", "auf", "ih\u00b7ren", "We\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich habe mich davongetrollt", "tokens": ["Ich", "ha\u00b7be", "mich", "da\u00b7von\u00b7ge\u00b7trollt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihrem gut kathol'schen Segen.", "tokens": ["Mit", "ih\u00b7rem", "gut", "ka\u00b7thol'\u00b7schen", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bei allen G\u00f6ttern dieser Welt", "tokens": ["Bei", "al\u00b7len", "G\u00f6t\u00b7tern", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leg ich ein kleines S\u00fcmmchen an;", "tokens": ["Leg", "ich", "ein", "klei\u00b7nes", "S\u00fcmm\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sagt: wann dereinst der W\u00fcrfel f\u00e4llt,", "tokens": ["Sagt", ":", "wann", "de\u00b7reinst", "der", "W\u00fcr\u00b7fel", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob es mir wohl noch fehlen kann?", "tokens": ["Ob", "es", "mir", "wohl", "noch", "feh\u00b7len", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und leugnen alle einst die Schuld,", "tokens": ["Und", "leug\u00b7nen", "al\u00b7le", "einst", "die", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df gewi\u00df, es steht mein Lieben", "tokens": ["Ich", "wei\u00df", "ge\u00b7wi\u00df", ",", "es", "steht", "mein", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Im goldnen Buch der h\u00f6chsten Huld", "tokens": ["Im", "gold\u00b7nen", "Buch", "der", "h\u00f6chs\u00b7ten", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir zahlbar dann und gut geschrieben!", "tokens": ["Mir", "zahl\u00b7bar", "dann", "und", "gut", "ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein schrankenloser Leichtsinn soll", "tokens": ["Ein", "schran\u00b7ken\u00b7lo\u00b7ser", "Leicht\u00b7sinn", "soll"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem Streit mein Knappe sein;", "tokens": ["In", "die\u00b7sem", "Streit", "mein", "Knap\u00b7pe", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So leb ich mut- und freudevoll,", "tokens": ["So", "leb", "ich", "mut", "und", "freu\u00b7de\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Solang nur Herz und H\u00e4nde rein!", "tokens": ["So\u00b7lang", "nur", "Herz", "und", "H\u00e4n\u00b7de", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lieb es, so mir halb bewu\u00dft", "tokens": ["Ich", "lieb", "es", ",", "so", "mir", "halb", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am j\u00e4hen Abgrund hinzustreifen,", "tokens": ["Am", "j\u00e4\u00b7hen", "Ab\u00b7grund", "hin\u00b7zu\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber mir la\u00df ich mit Lust", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "la\u00df", "ich", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Das Aug ins grundlos Blaue greifen!", "tokens": ["Das", "Aug", "ins", "grund\u00b7los", "Blau\u00b7e", "grei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zieht eine arme Pilgerin,", "tokens": ["Zieht", "ei\u00b7ne", "ar\u00b7me", "Pil\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geb\u00fcckt und schwach, am d\u00fcrren Stab", "tokens": ["Ge\u00b7b\u00fcckt", "und", "schwach", ",", "am", "d\u00fcr\u00b7ren", "Stab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur gnadenreichen Jungfrau hin;", "tokens": ["Zur", "gna\u00b7den\u00b7rei\u00b7chen", "Jung\u00b7frau", "hin", ";"