{"textgrid.poem.39806": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1. Zum neuen Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht der Wunsch, der holde Knabe,", "tokens": ["Es", "geht", "der", "Wunsch", ",", "der", "hol\u00b7de", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am neuen Jahr von Haus zu Haus,", "tokens": ["Am", "neu\u00b7en", "Jahr", "von", "Haus", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht sich in eines Jeden Habe", "tokens": ["Sieht", "sich", "in", "ei\u00b7nes", "Je\u00b7den", "Ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl um, und sp\u00fcrt die L\u00fccken aus.", "tokens": ["Wohl", "um", ",", "und", "sp\u00fcrt", "die", "L\u00fc\u00b7cken", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er geht, im Himmel zu bestellen,", "tokens": ["Er", "geht", ",", "im", "Him\u00b7mel", "zu", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin er, irdisch zwar gezeugt,", "tokens": ["Wo\u00b7hin", "er", ",", "ir\u00b7disch", "zwar", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wie ein G\u00f6tterkind mit hellen", "tokens": ["Doch", "wie", "ein", "G\u00f6t\u00b7ter\u00b7kind", "mit", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Lust bewegten Schwingen fleugt.", "tokens": ["Von", "Lust", "be\u00b7weg\u00b7ten", "Schwin\u00b7gen", "fleugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer will die Bitten alle z\u00e4hlen,", "tokens": ["Wer", "will", "die", "Bit\u00b7ten", "al\u00b7le", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die er mit L\u00e4cheln \u00fcbernimmt,", "tokens": ["Die", "er", "mit", "L\u00e4\u00b7cheln", "\u00fc\u00b7ber\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Heer, das ihm aus tausend Kehlen,", "tokens": ["Das", "Heer", ",", "das", "ihm", "aus", "tau\u00b7send", "Keh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Seufzern leis' entgegen schwimmt!", "tokens": ["Von", "Seuf\u00b7zern", "leis'", "ent\u00b7ge\u00b7gen", "schwimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was Geiz begehrt, was Ehre fodert,", "tokens": ["Was", "Geiz", "be\u00b7gehrt", ",", "was", "Eh\u00b7re", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,", "PRELS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was Armut weinend ihm empfiehlt,", "tokens": ["Was", "Ar\u00b7mut", "wei\u00b7nend", "ihm", "emp\u00b7fiehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was aus verliebtem Sehnen lodert,", "tokens": ["Was", "aus", "ver\u00b7lieb\u00b7tem", "Seh\u00b7nen", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was aus beklemmter Brust sich stiehlt!", "tokens": ["Was", "aus", "be\u00b7klemm\u00b7ter", "Brust", "sich", "stiehlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch Eines ist, was edle Herzen", "tokens": ["Doch", "Ei\u00b7nes", "ist", ",", "was", "ed\u00b7le", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "$,", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Himmelsboten anvertraun,", "tokens": ["Dem", "Him\u00b7mels\u00b7bo\u00b7ten", "an\u00b7ver\u00b7traun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nicht aus L\u00fcsten und aus Schmerzen", "tokens": ["Die", "nicht", "aus", "L\u00fcs\u00b7ten", "und", "aus", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des P\u00f6bels ihre Zukunft baun;", "tokens": ["Des", "P\u00f6\u00b7bels", "ih\u00b7re", "Zu\u00b7kunft", "baun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die selbst ihr Leben nur empfinden", "tokens": ["Die", "selbst", "ihr", "Le\u00b7ben", "nur", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als eines gro\u00dfen Leibes Glied:", "tokens": ["Als", "ei\u00b7nes", "gro\u00b7\u00dfen", "Lei\u00b7bes", "Glied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eins ist, was sie dem Wunsche k\u00fcnden,", "tokens": ["Eins", "ist", ",", "was", "sie", "dem", "Wun\u00b7sche", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und also lautet es im Lied:", "tokens": ["Und", "al\u00b7so", "lau\u00b7tet", "es", "im", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Flieg' auf durch diese Nebelschichte,", "tokens": ["Flieg'", "auf", "durch", "die\u00b7se", "Ne\u00b7bel\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die unsern Winterhimmel dr\u00fcckt,", "tokens": ["Die", "un\u00b7sern", "Win\u00b7ter\u00b7him\u00b7mel", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und flehe zu dem ew'gen Lichte,", "tokens": ["Und", "fle\u00b7he", "zu", "dem", "ew'\u00b7gen", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es uns seine Sonne schickt;", "tokens": ["Da\u00df", "es", "uns", "sei\u00b7ne", "Son\u00b7ne", "schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl zittert durch der Wolken Decke", "tokens": ["Wohl", "zit\u00b7tert", "durch", "der", "Wol\u00b7ken", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Stral nach diesen, jenen Gaun,", "tokens": ["Ein", "Stral", "nach", "die\u00b7sen", ",", "je\u00b7nen", "Gaun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch Gottes Sonnenschein erstrecke", "tokens": ["Doch", "Got\u00b7tes", "Son\u00b7nen\u00b7schein", "er\u00b7stre\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich \u00fcber alle deutschen Aun!", "tokens": ["Sich", "\u00fc\u00b7ber", "al\u00b7le", "deut\u00b7schen", "Aun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir bitten um des Himmels Gaben", "tokens": ["Wir", "bit\u00b7ten", "um", "des", "Him\u00b7mels", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr kein verworfenes Geschlecht:", "tokens": ["F\u00fcr", "kein", "ver\u00b7wor\u00b7fe\u00b7nes", "Ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es trug so lang sein Haupt erhaben,", "tokens": ["Es", "trug", "so", "lang", "sein", "Haupt", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Licht und Leben hat's ein Recht:", "tokens": ["Auf", "Licht", "und", "Le\u00b7ben", "hat's", "ein", "Recht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei Allen, so im Lande wohnen,", "tokens": ["Bei", "Al\u00b7len", ",", "so", "im", "Lan\u00b7de", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sei es in diesem Jahre Licht,", "tokens": ["Sei", "es", "in", "die\u00b7sem", "Jah\u00b7re", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Licht in den H\u00fctten, auf den Thronen;", "tokens": ["Licht", "in", "den", "H\u00fct\u00b7ten", ",", "auf", "den", "Thro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Hinauf, o Wunsch! und s\u00e4ume nicht!", "tokens": ["Hin\u00b7auf", ",", "o", "Wunsch", "!", "und", "s\u00e4u\u00b7me", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$.", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es geht der Wunsch, der holde Knabe,", "tokens": ["Es", "geht", "der", "Wunsch", ",", "der", "hol\u00b7de", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am neuen Jahr von Haus zu Haus,", "tokens": ["Am", "neu\u00b7en", "Jahr", "von", "Haus", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht sich in eines Jeden Habe", "tokens": ["Sieht", "sich", "in", "ei\u00b7nes", "Je\u00b7den", "Ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl um, und sp\u00fcrt die L\u00fccken aus.", "tokens": ["Wohl", "um", ",", "und", "sp\u00fcrt", "die", "L\u00fc\u00b7cken", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er geht, im Himmel zu bestellen,", "tokens": ["Er", "geht", ",", "im", "Him\u00b7mel", "zu", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin er, irdisch zwar gezeugt,", "tokens": ["Wo\u00b7hin", "er", ",", "ir\u00b7disch", "zwar", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wie ein G\u00f6tterkind mit hellen", "tokens": ["Doch", "wie", "ein", "G\u00f6t\u00b7ter\u00b7kind", "mit", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Lust bewegten Schwingen fleugt.", "tokens": ["Von", "Lust", "be\u00b7weg\u00b7ten", "Schwin\u00b7gen", "fleugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iam