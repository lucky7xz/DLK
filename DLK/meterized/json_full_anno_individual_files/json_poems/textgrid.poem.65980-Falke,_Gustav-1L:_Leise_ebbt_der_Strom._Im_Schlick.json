{"textgrid.poem.65980": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leise ebbt der Strom. Im Schlick", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise ebbt der Strom. Im Schlick", "tokens": ["Lei\u00b7se", "ebbt", "der", "Strom", ".", "Im", "Schlick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ragen plumpe Fischerk\u00e4hne,", "tokens": ["Ra\u00b7gen", "plum\u00b7pe", "Fi\u00b7scher\u00b7k\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen gleiten, stille Schw\u00e4ne,", "tokens": ["Drau\u00b7\u00dfen", "glei\u00b7ten", ",", "stil\u00b7le", "Schw\u00e4\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den wei\u00dfen Segeln andre.", "tokens": ["Mit", "den", "wei\u00b7\u00dfen", "Se\u00b7geln", "and\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Strecke \u00fcberwandre", "tokens": ["Und", "die", "Stre\u00b7cke", "\u00fc\u00b7berw\u00b7and\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Breiter Bahn ich mit dem Blick", "tokens": ["Brei\u00b7ter", "Bahn", "ich", "mit", "dem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ans niedere Gel\u00e4nde", "tokens": ["Bis", "ans", "nie\u00b7de\u00b7re", "Ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00fcben, wo sich Wiesen breiten,", "tokens": ["Dr\u00fc\u00b7ben", ",", "wo", "sich", "Wie\u00b7sen", "brei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PRF", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo die bunten K\u00fche schreiten", "tokens": ["Wo", "die", "bun\u00b7ten", "K\u00fc\u00b7he", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zwischen \u00fcppigem Krautgest\u00e4nde,", "tokens": ["Zwi\u00b7schen", "\u00fcp\u00b7pi\u00b7gem", "Kr\u00b7aut\u00b7ge\u00b7st\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Und die groben Weidenk\u00f6pfe,", "tokens": ["Und", "die", "gro\u00b7ben", "Wei\u00b7den\u00b7k\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Knorrig, bissig, Sauert\u00f6pfe,", "tokens": ["Knor\u00b7rig", ",", "bis\u00b7sig", ",", "Sau\u00b7er\u00b7t\u00f6p\u00b7fe", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie im Zorn die Haare spie\u00dfen.", "tokens": ["Wie", "im", "Zorn", "die", "Haa\u00b7re", "spie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Weiter oben sammeln, schlie\u00dfen,", "tokens": ["Wei\u00b7ter", "o\u00b7ben", "sam\u00b7meln", ",", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wie ein Wall, sich gr\u00fcne Wipfel", "tokens": ["Wie", "ein", "Wall", ",", "sich", "gr\u00fc\u00b7ne", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NE", "$,", "PRF", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Um das D\u00f6rfchen. H\u00f6chste Gipfel", "tokens": ["Um", "das", "D\u00f6rf\u00b7chen", ".", "H\u00f6chs\u00b7te", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "ART", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Zeigen Pappeln. Nur der Hahn", "tokens": ["Zei\u00b7gen", "Pap\u00b7peln", ".", "Nur", "der", "Hahn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Auf des Kirchleins gold'ner Spitze", "tokens": ["Auf", "des", "Kirch\u00b7leins", "gold'\u00b7ner", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Sieht von einem stolzeren Sitze", "tokens": ["Sieht", "von", "ei\u00b7nem", "stol\u00b7ze\u00b7ren", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Rings die Welt sich aufgethan:", "tokens": ["Rings", "die", "Welt", "sich", "auf\u00b7ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Weite unbegrenzte Fl\u00e4che,", "tokens": ["Wei\u00b7te", "un\u00b7be\u00b7grenz\u00b7te", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Segenstrotzend Feld an Felder,", "tokens": ["Se\u00b7gen\u00b7strot\u00b7zend", "Feld", "an", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Landmanns ungem\u00fcnzte Gelder,", "tokens": ["Land\u00b7manns", "un\u00b7ge\u00b7m\u00fcnz\u00b7te", "Gel\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wiesen, Moore, Waldesr\u00e4nder.", "tokens": ["Wie\u00b7sen", ",", "Moo\u00b7re", ",", "Wal\u00b7des\u00b7r\u00e4n\u00b7der", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und dazwischen blaue B\u00e4nder,", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "blau\u00b7e", "B\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Die Kan\u00e4le, Weiher, B\u00e4che.", "tokens": ["Die", "Ka\u00b7n\u00e4\u00b7le", ",", "Wei\u00b7her", ",", "B\u00e4\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Aber unten, ihm zu F\u00fc\u00dfen,", "tokens": ["A\u00b7ber", "un\u00b7ten", ",", "ihm", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Sieht er wei\u00dfe Segel gr\u00fc\u00dfen,", "tokens": ["Sieht", "er", "wei\u00b7\u00dfe", "Se\u00b7gel", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Schwarze Schlote niedergleiten.", "tokens": ["Schwar\u00b7ze", "Schlo\u00b7te", "nie\u00b7derg\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Kommen, Gehen. Aller Weiten", "tokens": ["Kom\u00b7men", ",", "Ge\u00b7hen", ".", "Al\u00b7ler", "Wei\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Unsichtbare F\u00e4den weben", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7re", "F\u00e4\u00b7den", "we\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Nach verborgenem Gesetze,", "tokens": ["Nach", "ver\u00b7bor\u00b7ge\u00b7nem", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Dort an einem Riesennetze.", "tokens": ["Dort", "an", "ei\u00b7nem", "Rie\u00b7sen\u00b7net\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und es tr\u00e4gt der Strom das Leben", "tokens": ["Und", "es", "tr\u00e4gt", "der", "Strom", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Ruhig zwischen Uferbreiten,", "tokens": ["Ru\u00b7hig", "zwi\u00b7schen", "U\u00b7fer\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Die zum Meer sich m\u00e4hlich weiten.", "tokens": ["Die", "zum", "Meer", "sich", "m\u00e4h\u00b7lich", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leis zum Strande rinnt die Welle,", "tokens": ["Leis", "zum", "Stran\u00b7de", "rinnt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die schwanke Binse schmiegt sich,", "tokens": ["Und", "die", "schwan\u00b7ke", "Bin\u00b7se", "schmiegt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Windet sich und bebt und wiegt sich.", "tokens": ["Win\u00b7det", "sich", "und", "bebt", "und", "wiegt", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "KON", "VVFIN", "PRF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwielicht wechselt ab mit Helle,", "tokens": ["Zwie\u00b7licht", "wech\u00b7selt", "ab", "mit", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sich vor der Abendsonne,", "tokens": ["Wie", "sich", "vor", "der", "A\u00b7bend\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine schweifende Kolonne,", "tokens": ["Ei\u00b7ne", "schwei\u00b7fen\u00b7de", "Ko\u00b7lon\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Leichte Wolken hastig dr\u00e4ngen,", "tokens": ["Leich\u00b7te", "Wol\u00b7ken", "has\u00b7tig", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die auf ihren hohen G\u00e4ngen,", "tokens": ["Die", "auf", "ih\u00b7ren", "ho\u00b7hen", "G\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Unter sich den Tanz der Wogen,", "tokens": ["Un\u00b7ter", "sich", "den", "Tanz", "der", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "\u00dcber sich den Glanz der Sterne,", "tokens": ["\u00dc\u00b7ber", "sich", "den", "Glanz", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Kommen lautlos hergezogen,", "tokens": ["Kom\u00b7men", "laut\u00b7los", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Abgesandte welcher Ferne?", "tokens": ["Ab\u00b7ge\u00b7sand\u00b7te", "wel\u00b7cher", "Fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber tiefer, Wellenteiler,", "tokens": ["A\u00b7ber", "tie\u00b7fer", ",", "Wel\u00b7len\u00b7tei\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kraftbeschwingte Luftdurcheiler,", "tokens": ["Kraft\u00b7be\u00b7schwing\u00b7te", "Luft\u00b7durc\u00b7hei\u00b7ler", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tummeln sich im Auf und Nieder", "tokens": ["Tum\u00b7meln", "sich", "im", "Auf", "und", "Nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6wen mit dem Schneegefieder.", "tokens": ["M\u00f6\u00b7wen", "mit", "dem", "Schnee\u00b7ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Wie um blaue Blumenkronen", "tokens": ["Wie", "um", "blau\u00b7e", "Blu\u00b7men\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wei\u00dfe Schmetterlinge fl\u00fcgeln,", "tokens": ["Wei\u00b7\u00dfe", "Schmet\u00b7ter\u00b7lin\u00b7ge", "fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schaukeln ohne Schwingenschonen", "tokens": ["Schau\u00b7keln", "oh\u00b7ne", "Schwin\u00b7gen\u00b7scho\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leicht sie \u00fcber Wellenh\u00fcgeln.", "tokens": ["Leicht", "sie", "\u00fc\u00b7ber", "Wel\u00b7len\u00b7h\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwischen Wasser, zwischen Himmel:", "tokens": ["Zwi\u00b7schen", "Was\u00b7ser", ",", "zwi\u00b7schen", "Him\u00b7mel", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segel, V\u00f6gel, ein Gewimmel", "tokens": ["Se\u00b7gel", ",", "V\u00f6\u00b7gel", ",", "ein", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Regen Lebens, lautlos hastend.", "tokens": ["Re\u00b7gen", "Le\u00b7bens", ",", "laut\u00b7los", "has\u00b7tend", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "l