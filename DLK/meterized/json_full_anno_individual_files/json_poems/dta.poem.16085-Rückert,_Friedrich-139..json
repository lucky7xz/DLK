{"dta.poem.16085": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "139.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du klagest: Was ich dort dem Mann hab' angetragen,", "tokens": ["Du", "kla\u00b7gest", ":", "Was", "ich", "dort", "dem", "Mann", "hab'", "an\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "PPER", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hats nicht zugesagt, und hats nicht abgeschlagen.", "tokens": ["Er", "hats", "nicht", "zu\u00b7ge\u00b7sagt", ",", "und", "hats", "nicht", "ab\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,", "KON", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und fragest: Soll ich nun damit zufrieden seyn?", "tokens": ["Und", "fra\u00b7gest", ":", "Soll", "ich", "nun", "da\u00b7mit", "zu\u00b7frie\u00b7den", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VMFIN", "PPER", "ADV", "PAV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Frag' ich noch einmal, da\u00df er Ja sag' oder Nein?", "tokens": ["Frag'", "ich", "noch", "ein\u00b7mal", ",", "da\u00df", "er", "Ja", "sag'", "o\u00b7der", "Nein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "KON", "PTKANT", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Ja, wenn das harte Wort du ohne Herzverdru\u00df", "tokens": ["Ja", ",", "wenn", "das", "har\u00b7te", "Wort", "du", "oh\u00b7ne", "Herz\u00b7ver\u00b7dru\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kanst h\u00f6ren, mach ihm den, da\u00df er es sagen mu\u00df.", "tokens": ["Kanst", "h\u00f6\u00b7ren", ",", "mach", "ihm", "den", ",", "da\u00df", "er", "es", "sa\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VVFIN", "PPER", "ART", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}