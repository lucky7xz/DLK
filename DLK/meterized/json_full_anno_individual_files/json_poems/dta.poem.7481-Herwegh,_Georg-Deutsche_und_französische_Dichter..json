{"dta.poem.7481": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche und franz\u00f6sische Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gem\u00e4lde, Spiegel, Uhren und Tapeten,", "tokens": ["Ge\u00b7m\u00e4l\u00b7de", ",", "Spie\u00b7gel", ",", "Uh\u00b7ren", "und", "Ta\u00b7pe\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und rings, wie bei dem t\u00fcrkischen Sultane,", "tokens": ["Und", "rings", ",", "wie", "bei", "dem", "t\u00fcr\u00b7ki\u00b7schen", "Sul\u00b7ta\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Sammt und Seide strotzende Divane,", "tokens": ["Von", "Sammt", "und", "Sei\u00b7de", "strot\u00b7zen\u00b7de", "Di\u00b7va\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch Kruzifixe, nie davor zu beten.", "tokens": ["Auch", "Kru\u00b7zi\u00b7fi\u00b7xe", ",", "nie", "da\u00b7vor", "zu", "be\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So lieben's \u00fcberm Rheine die Poeten;", "tokens": ["So", "lie\u00b7ben's", "\u00fc\u00b7berm", "Rhei\u00b7ne", "die", "Po\u00b7et\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ums Haubt gewunden farbige Turbane,", "tokens": ["Ums", "Haubt", "ge\u00b7wun\u00b7den", "far\u00b7bi\u00b7ge", "Tur\u00b7ba\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAPP", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch Wolken Weihrauchs rauschend im Kaftane \u2014", "tokens": ["Durch", "Wol\u00b7ken", "Weih\u00b7rauchs", "rau\u00b7schend", "im", "Kaf\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind das noch Dichter, noch Anachoreten?", "tokens": ["Sind", "das", "noch", "Dich\u00b7ter", ",", "noch", "A\u00b7nac\u00b7ho\u00b7re\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hoch \u00fcber meinem Volk, in der Mansarde,", "tokens": ["Hoch", "\u00fc\u00b7ber", "mei\u00b7nem", "Volk", ",", "in", "der", "Man\u00b7sar\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umduftet von des Gartens bl\u00fchndem Flieder,", "tokens": ["Um\u00b7duf\u00b7tet", "von", "des", "Gar\u00b7tens", "bl\u00fchn\u00b7dem", "Flie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Hut von Rosen eine Festkokarde,", "tokens": ["Am", "Hut", "von", "Ro\u00b7sen", "ei\u00b7ne", "Fest\u00b7ko\u00b7kar\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Inde\u00df die jungen Spatzen auf und nieder", "tokens": ["In\u00b7de\u00df", "die", "jun\u00b7gen", "Spat\u00b7zen", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorm Fenster schildern, eine Ehrengarde \u2014", "tokens": ["Vorm", "Fens\u00b7ter", "schil\u00b7dern", ",", "ei\u00b7ne", "Eh\u00b7ren\u00b7gar\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schreib' Ich f\u00fcr mein deutsches M\u00e4dchen Lieder.", "tokens": ["So", "schreib'", "Ich", "f\u00fcr", "mein", "deut\u00b7sches", "M\u00e4d\u00b7chen", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}