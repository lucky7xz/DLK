{"textgrid.poem.59358": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an meinen Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df auch wir schliefen!", "tokens": ["Ach", ",", "da\u00df", "auch", "wir", "schlie\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die bl\u00fchenden Tiefen,", "tokens": ["Die", "bl\u00fc\u00b7hen\u00b7den", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Str\u00f6me, die Auen", "tokens": ["Die", "Str\u00f6\u00b7me", ",", "die", "Au\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So heimlich aufschauen,", "tokens": ["So", "heim\u00b7lich", "auf\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Als ob sie all riefen:", "tokens": ["Als", "ob", "sie", "all", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PIAT", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u00bbdein Bruder ist tot!", "tokens": ["\u00bb", "dein", "Bru\u00b7der", "ist", "tot", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Unter Rosen rot", "tokens": ["Un\u00b7ter", "Ro\u00b7sen", "rot"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Ach, da\u00df wir auch schliefen!\u00ab", "tokens": ["Ach", ",", "da\u00df", "wir", "auch", "schlie\u00b7fen", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbhast doch keine Schwingen,", "tokens": ["\u00bb", "hast", "doch", "kei\u00b7ne", "Schwin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch Wolken zu dringen!", "tokens": ["Durch", "Wol\u00b7ken", "zu", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mu\u00dft immerfort schauen", "tokens": ["Mu\u00dft", "im\u00b7mer\u00b7fort", "schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Str\u00f6me, die Auen \u2013", "tokens": ["Die", "Str\u00f6\u00b7me", ",", "die", "Au\u00b7en", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die werden dir singen", "tokens": ["Die", "wer\u00b7den", "dir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von ihm Tag und Nacht,", "tokens": ["Von", "ihm", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mit Wahnsinnesmacht", "tokens": ["Mit", "Wahn\u00b7sin\u00b7nes\u00b7macht"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.8": {"text": "Die Seele umschlingen.\u00ab", "tokens": ["Die", "See\u00b7le", "um\u00b7schlin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "So singt, wie Sirenen,", "tokens": ["So", "singt", ",", "wie", "Si\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+----", "measure": "dactylic.init"}, "line.2": {"text": "Von hellblauen, sch\u00f6nen", "tokens": ["Von", "hell\u00b7blau\u00b7en", ",", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "VVINF", "$,", "ADJA"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Vergangenen Zeiten,", "tokens": ["Ver\u00b7gan\u00b7ge\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Abend vom weiten", "tokens": ["Der", "A\u00b7bend", "vom", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Versinkt dann im T\u00f6nen,", "tokens": ["Ver\u00b7sinkt", "dann", "im", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Erst Busen, dann Mund,", "tokens": ["Erst", "Bu\u00b7sen", ",", "dann", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im bl\u00fchenden Grund.", "tokens": ["Im", "bl\u00fc\u00b7hen\u00b7den", "Grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "O schweiget Sirenen!", "tokens": ["O", "schwei\u00b7get", "Si\u00b7re\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "O wecket nicht wieder!", "tokens": ["O", "we\u00b7cket", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Denn zaubrische Lieder", "tokens": ["Denn", "zaub\u00b7ri\u00b7sche", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gebunden hier tr\u00e4umen", "tokens": ["Ge\u00b7bun\u00b7den", "hier", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf Feldern und B\u00e4umen,", "tokens": ["Auf", "Fel\u00b7dern", "und", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und ziehen mich nieder", "tokens": ["Und", "zie\u00b7hen", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So m\u00fcde vor Weh", "tokens": ["So", "m\u00fc\u00b7de", "vor", "Weh"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Zu tiefstillem See \u2013", "tokens": ["Zu", "tief\u00b7stil\u00b7lem", "See", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.8": {"text": "O weckt nicht die Lieder!", "tokens": ["O", "weckt", "nicht", "die", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Du kanntest die Wellen", "tokens": ["Du", "kann\u00b7test", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Des Sees, sie schwellen", "tokens": ["Des", "Sees", ",", "sie", "schwel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In magischen Ringen.", "tokens": ["In", "ma\u00b7gi\u00b7schen", "Rin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein wehm\u00fctig Singen", "tokens": ["Ein", "weh\u00b7m\u00fc\u00b7tig", "Sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Tief unter den Quellen", "tokens": ["Tief", "un\u00b7ter", "den", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im Schlummer dort h\u00e4lt", "tokens": ["Im", "Schlum\u00b7mer", "dort", "h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Verzaubert die Welt.", "tokens": ["Ver\u00b7zau\u00b7bert", "die", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Wohl kennst du die Wellen.", "tokens": ["Wohl", "kennst", "du", "die", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "K\u00fchl wird's auf den G\u00e4ngen,", "tokens": ["K\u00fchl", "wird's", "auf", "den", "G\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Vor alten Ges\u00e4ngen", "tokens": ["Vor", "al\u00b7ten", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "M\u00f6cht's Herz mir zerspringen.", "tokens": ["M\u00f6cht's", "Herz", "mir", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So will ich denn singen!", "tokens": ["So", "will", "ich", "denn", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schmerz fliegt ja auf Kl\u00e4ngen", "tokens": ["Schmerz", "fliegt", "ja", "auf", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zu himmlischer Lust,", "tokens": ["Zu", "himm\u00b7li\u00b7scher", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und still wird die Brust", "tokens": ["Und", "still", "wird", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Auf k\u00fchl gr\u00fcnen G\u00e4ngen.", "tokens": ["Auf", "k\u00fchl", "gr\u00fc\u00b7nen", "G\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df fahren die Tr\u00e4ume!", "tokens": ["La\u00df", "fah\u00b7ren", "die", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Mond scheint durch B\u00e4ume,", "tokens": ["Der", "Mond", "scheint", "durch", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die W\u00e4lder nur rauschen,", "tokens": ["Die", "W\u00e4l\u00b7der", "nur", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die T\u00e4ler still lauschen,", "tokens": ["Die", "T\u00e4\u00b7ler", "still", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie einsam die R\u00e4ume!", "tokens": ["Wie", "ein\u00b7sam", "die", "R\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ach, niemand ist mein!", "tokens": ["Ach", ",", "nie\u00b7mand", "ist", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VAFIN", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Herz, wie so allein!", "tokens": ["Herz", ",", "wie", "so", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "La\u00df fahren die Tr\u00e4ume!", "tokens": ["La\u00df", "fah\u00b7ren", "die", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Herr wird dich f\u00fchren.", "tokens": ["Der", "Herr", "wird", "dich", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tief kann ich ja sp\u00fcren", "tokens": ["Tief", "kann", "ich", "ja", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der Sterne still Walten.", "tokens": ["Der", "Ster\u00b7ne", "still", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Erde Gestalten", "tokens": ["Der", "Er\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kaum h\u00f6rbar sich r\u00fchren.", "tokens": ["Kaum", "h\u00f6r\u00b7bar", "sich", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durch Nacht und durch Graus", "tokens": ["Durch", "Nacht", "und", "durch", "Graus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Gen Morgen, nach Haus \u2013", "tokens": ["Gen", "Mor\u00b7gen", ",", "nach", "Haus", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ja, Gott wird mich f\u00fchren.", "tokens": ["Ja", ",", "Gott", "wird", "mich", "f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Ach, da\u00df auch wir schliefen!", "tokens": ["Ach", ",", "da\u00df", "auch", "wir", "schlie\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die bl\u00fchenden Tiefen,", "tokens": ["Die", "bl\u00fc\u00b7hen\u00b7den", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Str\u00f6me, die Auen", "tokens": ["Die", "Str\u00f6\u00b7me", ",", "die", "Au\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So heimlich aufschauen,", "tokens": ["So", "heim\u00b7lich", "auf\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Als ob sie all riefen:", "tokens": ["Als", "ob", "sie", "all", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PIAT", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u00bbdein Bruder ist tot!", "tokens": ["\u00bb", "dein", "Bru\u00b7der", "ist", "tot", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Unter Rosen rot", "tokens": ["Un\u00b7ter", "Ro\u00b7sen", "rot"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Ach, da\u00df wir auch schliefen!\u00ab", "tokens": ["Ach", ",", "da\u00df", "wir", "auch", "schlie\u00b7fen", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbhast doch keine Schwingen,", "tokens": ["\u00bb", "hast", "doch", "kei\u00b7ne", "Schwin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch Wolken zu dringen!", "tokens": ["Durch", "Wol\u00b7ken", "zu", "drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mu\u00dft immerfort schauen", "tokens": ["Mu\u00dft", "im\u00b7mer\u00b7fort", "schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Str\u00f6me, die Auen \u2013", "tokens": ["Die", "Str\u00f6\u00b7me", ",", "die", "Au\u00b7en", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die werden dir singen", "tokens": ["Die", "wer\u00b7den", "dir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von ihm Tag und Nacht,", "tokens": ["Von", "ihm", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mit Wahnsinnesmacht", "tokens": ["Mit", "Wahn\u00b7sin\u00b7nes\u00b7macht"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.8": {"text": "Die Seele umschlingen.\u00ab", "tokens": ["Die", "See\u00b7le", "um\u00b7schlin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "So singt, wie Sirenen,", "tokens": ["So", "singt", ",", "wie", "Si\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+----", "measure": "dactylic.init"}, "line.2": {"text": "Von hellblauen, sch\u00f6nen", "tokens": ["Von", "hell\u00b7blau\u00b7en", ",", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "VVINF", "$,", "ADJA"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Vergangenen Zeiten,", "tokens": ["Ver\u00b7gan\u00b7ge\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Abend vom weiten", "tokens": ["Der", "A\u00b7bend", "vom", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Versinkt dann im T\u00f6nen,", "tokens": ["Ver\u00b7sinkt", "dann", "im", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Erst Busen, dann Mund,", "tokens": ["Erst", "Bu\u00b7sen", ",", "dann", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im bl\u00fchenden Grund.", "tokens": ["Im", "bl\u00fc\u00b7hen\u00b7den", "Grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "O schweiget Sirenen!", "tokens": ["O", "schwei\u00b7get", "Si\u00b7re\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "O wecket nicht wieder!", "tokens": ["O", "we\u00b7cket", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Denn zaubrische Lieder", "tokens": ["Denn", "zaub\u00b7ri\u00b7sche", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gebunden hier tr\u00e4umen", "tokens": ["Ge\u00b7bun\u00b7den", "hier", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf Feldern und B\u00e4umen,", "tokens": ["Auf", "Fel\u00b7dern", "und", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und ziehen mich nieder", "tokens": ["Und", "zie\u00b7hen", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So m\u00fcde vor Weh", "tokens": ["So", "m\u00fc\u00b7de", "vor", "Weh"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Zu tiefstillem See \u2013", "tokens": ["Zu", "tief\u00b7stil\u00b7lem", "See", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.8": {"text": "O weckt nicht die Lieder!", "tokens": ["O", "weckt", "nicht", "die", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Du kanntest die Wellen", "tokens": ["Du", "kann\u00b7test", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Des Sees, sie schwellen", "tokens": ["Des", "Sees", ",", "sie", "schwel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In magischen Ringen.", "tokens": ["In", "ma\u00b7gi\u00b7schen", "Rin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein wehm\u00fctig Singen", "tokens": ["Ein", "weh\u00b7m\u00fc\u00b7tig", "Sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Tief unter den Quellen", "tokens": ["Tief", "un\u00b7ter", "den", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im Schlummer dort h\u00e4lt", "tokens": ["Im", "Schlum\u00b7mer", "dort", "h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Verzaubert die Welt.", "tokens": ["Ver\u00b7zau\u00b7bert", "die", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Wohl kennst du die Wellen.", "tokens": ["Wohl", "kennst", "du", "die", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.14": {"line.1": {"text": "K\u00fchl wird's auf den G\u00e4ngen,", "tokens": ["K\u00fchl", "wird's", "auf", "den", "G\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Vor alten Ges\u00e4ngen", "tokens": ["Vor", "al\u00b7ten", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "M\u00f6cht's Herz mir zerspringen.", "tokens": ["M\u00f6cht's", "Herz", "mir", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So will ich denn singen!", "tokens": ["So", "will", "ich", "denn", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schmerz fliegt ja auf Kl\u00e4ngen", "tokens": ["Schmerz", "fliegt", "ja", "auf", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zu himmlischer Lust,", "tokens": ["Zu", "himm\u00b7li\u00b7scher", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und still wird die Brust", "tokens": ["Und", "still", "wird", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Auf k\u00fchl gr\u00fcnen G\u00e4ngen.", "tokens": ["Auf", "k\u00fchl", "gr\u00fc\u00b7nen", "G\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "La\u00df fahren die Tr\u00e4ume!", "tokens": ["La\u00df", "fah\u00b7ren", "die", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Mond scheint durch B\u00e4ume,", "tokens": ["Der", "Mond", "scheint", "durch", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die W\u00e4lder nur rauschen,", "tokens": ["Die", "W\u00e4l\u00b7der", "nur", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die T\u00e4ler still lauschen,", "tokens": ["Die", "T\u00e4\u00b7ler", "still", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie einsam die R\u00e4ume!", "tokens": ["Wie", "ein\u00b7sam", "die", "R\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ach, niemand ist mein!", "tokens": ["Ach", ",", "nie\u00b7mand", "ist", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VAFIN", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Herz, wie so allein!", "tokens": ["Herz", ",", "wie", "so", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "La\u00df fahren die Tr\u00e4ume!", "tokens": ["La\u00df", "fah\u00b7ren", "die", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.16": {"line.1": {"text": "Der Herr wird dich f\u00fchren.", "tokens": ["Der", "Herr", "wird", "dich", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tief kann ich ja sp\u00fcren", "tokens": ["Tief", "kann", "ich", "ja", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der Sterne still Walten.", "tokens": ["Der", "Ster\u00b7ne", "still", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Erde Gestalten", "tokens": ["Der", "Er\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kaum h\u00f6rbar sich r\u00fchren.", "tokens": ["Kaum", "h\u00f6r\u00b7bar", "sich", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durch Nacht und durch Graus", "tokens": ["Durch", "Nacht", "und", "durch", "Graus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Gen Morgen, nach Haus \u2013", "tokens": ["Gen", "Mor\u00b7gen", ",", "nach", "Haus", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ja, Gott wird mich f\u00fchren.", "tokens": ["Ja", ",", "Gott", "wird", "mich", "f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}