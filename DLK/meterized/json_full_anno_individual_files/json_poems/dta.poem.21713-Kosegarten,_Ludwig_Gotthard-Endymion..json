{"dta.poem.21713": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Endymion.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner J&#252;ngling du schl&#228;fst? Und nicht das Ko-\nsen der G\u00f6ttinn,", "tokens": ["Sch\u00f6\u00b7ner", "J", "&#252;", "ng\u00b7ling", "du", "schl", "&#228;", "fst", "?", "Und", "nicht", "das", "Ko", "sen", "der", "G\u00f6t\u00b7tinn", ","], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "VVFIN", "PPER", "VVFIN", "$(", "VVFIN", "$.", "KON", "PTKNEG", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Nicht ihr ambrosischer Kuss scheuchet den se-", "tokens": ["Nicht", "ihr", "am\u00b7bro\u00b7si\u00b7scher", "Kuss", "scheu\u00b7chet", "den", "se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schlafe, Begl\u00fcckter! Nur schlafend besuchen die", "tokens": ["Schla\u00b7fe", ",", "Be\u00b7gl\u00fcck\u00b7ter", "!", "Nur", "schla\u00b7fend", "be\u00b7su\u00b7chen", "die"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ADV", "ADJD", "VVFIN", "ART"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "G\u00f6tter den Menschen;", "tokens": ["G\u00f6t\u00b7ter", "den", "Men\u00b7schen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wachend f\u00e4llt er sofort, herbes Verh\u00e4ngniss,", "tokens": ["Wa\u00b7chend", "f\u00e4llt", "er", "so\u00b7fort", ",", "her\u00b7bes", "Ver\u00b7h\u00e4ng\u00b7niss", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "dir heim", "tokens": ["dir", "heim"], "token_info": ["word", "word"], "pos": ["PPER", "PTKVZ"], "meter": "-+", "measure": "iambic.single"}}}}}