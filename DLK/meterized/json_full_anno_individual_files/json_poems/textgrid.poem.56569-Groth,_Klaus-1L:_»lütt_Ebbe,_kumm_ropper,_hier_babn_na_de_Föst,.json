{"textgrid.poem.56569": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbl\u00fctt Ebbe, kumm ropper, hier babn na de F\u00f6st,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.85", "no:0.11"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbl\u00fctt Ebbe, kumm ropper, hier babn na de F\u00f6st,", "tokens": ["\u00bb", "l\u00fctt", "Eb\u00b7be", ",", "kumm", "rop\u00b7per", ",", "hier", "babn", "na", "de", "F\u00f6st", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "ADJD", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Krup \u00fcnner, ja kik mal, hier bu't wi en Nest.", "tokens": ["Krup", "\u00fcn\u00b7ner", ",", "ja", "kik", "mal", ",", "hier", "bu't", "wi", "en", "Nest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADV", "NN", "ADV", "$,", "ADV", "FM", "FM", "FM", "FM", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du sittst as Gardrutjen \u0119r Hahn \u00fcnnert Bett,", "tokens": ["Du", "sittst", "as", "Gard\u00b7rut\u00b7jen", "\u0119r", "Hahn", "\u00fcn\u00b7nert", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NE", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "As en Mus in en Heeddis', wa nett, o wa nett!\u00ab \u2013", "tokens": ["As", "en", "Mus", "in", "en", "Heed\u00b7dis'", ",", "wa", "nett", ",", "o", "wa", "nett", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "PWAV", "ADJD", "$,", "FM", "FM", "FM", "$.", "$(", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Du Spitzbov, du Gaudeef, man weg, ga man weg!", "tokens": ["Du", "Spitz\u00b7bov", ",", "du", "Gau\u00b7de\u00b7ef", ",", "man", "weg", ",", "ga", "man", "weg", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,", "PIS", "PTKVZ", "$,", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weest noch vergangn Jahr? O wa slech, o wa slech!", "tokens": ["Weest", "noch", "ver\u00b7gangn", "Jahr", "?", "O", "wa", "slech", ",", "o", "wa", "slech", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$.", "NE", "XY", "XY", "$,", "FM", "FM", "FM", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wa seet ik un br\u00f6, harr ni Korn oder Kr\u00f6m,", "tokens": ["Wa", "seet", "ik", "un", "br\u00f6", ",", "harr", "ni", "Korn", "o\u00b7der", "Kr\u00f6m", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$,", "ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Un Spatz flog to D\u00f6rp, r\u00e4sonneer inne B\u00f6m.", "tokens": ["Un", "Spatz", "flog", "to", "D\u00f6rp", ",", "r\u00e4\u00b7son\u00b7neer", "in\u00b7ne", "B\u00f6m", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}}, "stanza.3": {"line.1": {"text": "Du Spitzbov! du Gaudeef! \u2013 \u00bbL\u00fctt Ebbe, swig still,", "tokens": ["Du", "Spitz\u00b7bov", "!", "du", "Gau\u00b7de\u00b7ef", "!", "\u2013", "\u00bb", "L\u00fctt", "Eb\u00b7be", ",", "swig", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PPER", "NN", "$.", "$(", "$(", "NE", "NE", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vuntjahr ward't ganz anners: will mi b\u0119tern \u2013 ik will!", "tokens": ["Vunt\u00b7jahr", "ward't", "ganz", "an\u00b7ners", ":", "will", "mi", "b\u0119tern", "\u2013", "ik", "will", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "$.", "VMFIN", "NE", "VVINF", "$(", "NE", "VMFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Mi steken de Fettdun \u2013 kumm, kik mal wa sch\u00f6n!", "tokens": ["Mi", "ste\u00b7ken", "de", "Fett\u00b7dun", "\u2013", "kumm", ",", "kik", "mal", "wa", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "ADJD", "$,", "VVIMP", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vuntjahr ward dat anners, schast sehn, schast man sehn!", "tokens": ["Vunt\u00b7jahr", "ward", "dat", "an\u00b7ners", ",", "schast", "sehn", ",", "schast", "man", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "VVINF", "$,", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "De Hadbar kumt bald, wahnt uns dicht \u0153wern Kopp,", "tokens": ["De", "Had\u00b7bar", "kumt", "bald", ",", "wahnt", "uns", "dicht", "\u0153wern", "Kopp", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bu't en Hus as en Korf, stellt sik baben derop,", "tokens": ["Bu't", "en", "Hus", "as", "en", "Korf", ",", "stellt", "sik", "ba\u00b7ben", "de\u00b7rop", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Op een Been, opt anner, de N\u0153s inne Fl\u00fcnk!", "tokens": ["Op", "e\u00b7en", "Be\u00b7en", ",", "opt", "an\u00b7ner", ",", "de", "N\u0153s", "in\u00b7ne", "Fl\u00fcnk", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "FM.la", "FM.la", "$,", "NE", "NE", "ADJA", "NN", "$."], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wa klappert he fr\u00fcndli: Gudn Morn, Nawer L\u00fcnk!", "tokens": ["Wa", "klap\u00b7pert", "he", "fr\u00fcnd\u00b7li", ":", "Gudn", "Morn", ",", "Na\u00b7wer", "L\u00fcnk", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "NE", "NE", "$,", "NE", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Un denn schint de S\u00fcnn hier lankt Dack rein so blank,", "tokens": ["Un", "denn", "schint", "de", "S\u00fcnn", "hier", "lankt", "Dack", "rein", "so", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "NE", "NE", "ADV", "VVFIN", "NN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Un denn treckt de Rok hier vunn Sch\u00f6steen hentlank,", "tokens": ["Un", "denn", "treckt", "de", "Rok", "hier", "vunn", "Sch\u00f6s\u00b7teen", "hent\u00b7lank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "NE", "NE", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Un denn kumt Annstina mit Weten un Kaff:", "tokens": ["Un", "denn", "kumt", "Anns\u00b7ti\u00b7na", "mit", "We\u00b7ten", "un", "Kaff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "NE", "APPR", "NN", "FM", "FM", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Tuck, tuck! \u2013 Kikriki! un wi beidn krigt wat af.", "tokens": ["Tuck", ",", "tuck", "!", "\u2013", "Kik\u00b7ri\u00b7ki", "!", "un", "wi", "beidn", "krigt", "wat", "af", "."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "$(", "NE", "$.", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+--+-+-+---", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Ok heff ik man sehn, hier de Koppel int Gras:", "tokens": ["Ok", "heff", "ik", "man", "sehn", ",", "hier", "de", "Kop\u00b7pel", "int", "Gras", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "PIS", "VVINF", "$,", "ADV", "NE", "NE", "VVFIN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nawer Anton will Rogg sei'n, dat kumt uns to pass;", "tokens": ["Na\u00b7wer", "An\u00b7ton", "will", "Rogg", "sei'n", ",", "dat", "kumt", "uns", "to", "pass", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "NE", "NE", "$,", "PAV", "VVFIN", "PPER", "FM", "FM", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Un denn hier de Bom v\u0153r uns Kinner to fleegn,", "tokens": ["Un", "denn", "hier", "de", "Bom", "v\u0153r", "uns", "Kin\u00b7ner", "to", "fle\u00b7egn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Un wi merrn dermank, watn Vergn\u00f6gn, watn Vergn\u00f6gn!\u00ab", "tokens": ["Un", "wi", "merrn", "der\u00b7mank", ",", "watn", "Ver\u00b7gn\u00f6gn", ",", "watn", "Ver\u00b7gn\u00f6gn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-----+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Du Spitzbov, lat sehn: dats dat Nest? dats dat Nest?", "tokens": ["Du", "Spitz\u00b7bov", ",", "lat", "sehn", ":", "dats", "dat", "Nest", "?", "dats", "dat", "Nest", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "VVINF", "$.", "ADV", "ART", "NN", "$.", "ADV", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mak to, un hal Feddern un Dun, dats dat Best!", "tokens": ["Mak", "to", ",", "un", "hal", "Fed\u00b7dern", "un", "Dun", ",", "dats", "dat", "Best", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "FM", "FM", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ol Anton sin Pudelm\u00fctz liggt g\u00fcnd achtern Tun:", "tokens": ["Ol", "An\u00b7ton", "sin", "Pu\u00b7del\u00b7m\u00fctz", "liggt", "g\u00fcnd", "ach\u00b7tern", "Tun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Pl\u00fcck af, mak man to, lats man bu'n, lats man bu'n!", "tokens": ["Pl\u00fcck", "af", ",", "mak", "man", "to", ",", "lats", "man", "bu'n", ",", "lats", "man", "bu'n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "$,", "VVFIN", "PIS", "VVINF", "$,", "VVFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "\u00bbl\u00fctt Ebbe, kumm ropper, hier babn na de F\u00f6st,", "tokens": ["\u00bb", "l\u00fctt", "Eb\u00b7be", ",", "kumm", "rop\u00b7per", ",", "hier", "babn", "na", "de", "F\u00f6st", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "ADJD", "ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Krup \u00fcnner, ja kik mal, hier bu't wi en Nest.", "tokens": ["Krup", "\u00fcn\u00b7ner", ",", "ja", "kik", "mal", ",", "hier", "bu't", "wi", "