{"dta.poem.8842": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Doch jene Zeiten, wenn es fern, o sagt, wie bringt", "tokens": ["Doch", "je\u00b7ne", "Zei\u00b7ten", ",", "wenn", "es", "fern", ",", "o", "sagt", ",", "wie", "bringt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "KOUS", "PPER", "ADJD", "$,", "FM", "VVFIN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihr ein Lied zu singen denkt, so singt ein regel-", "tokens": ["Wenn", "ihr", "ein", "Lied", "zu", "sin\u00b7gen", "denkt", ",", "so", "singt", "ein", "re\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das meine schwankt am G\u00e4ngelband der losen Phantasie", "tokens": ["Das", "mei\u00b7ne", "schwankt", "am", "G\u00e4n\u00b7gel\u00b7band", "der", "lo\u00b7sen", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ein Nebenbuhler hatte schon entzogen mir dies sch\u00f6ne", "tokens": ["Ein", "Ne\u00b7ben\u00b7buh\u00b7ler", "hat\u00b7te", "schon", "ent\u00b7zo\u00b7gen", "mir", "dies", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "PPER", "PDS", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Doch bracht' ich wieder es zu mir, wiewohl er mich be-", "tokens": ["Doch", "bracht'", "ich", "wie\u00b7der", "es", "zu", "mir", ",", "wie\u00b7wohl", "er", "mich", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich h\u00f6re hoffend schon voraus, wie mich dein erstes Du", "tokens": ["Ich", "h\u00f6\u00b7re", "hof\u00b7fend", "schon", "vo\u00b7raus", ",", "wie", "mich", "dein", "ers\u00b7tes", "Du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "PPOSAT", "ADJA", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "O w\u00e4re schon die bange Zeit, und dieses stolze Sie", "tokens": ["O", "w\u00e4\u00b7re", "schon", "die", "ban\u00b7ge", "Zeit", ",", "und", "die\u00b7ses", "stol\u00b7ze", "Sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "KON", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Es windet sich der Liebe Geist um deiner Glieder Eben-", "tokens": ["Es", "win\u00b7det", "sich", "der", "Lie\u00b7be", "Geist", "um", "dei\u00b7ner", "Glie\u00b7der", "E\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Wie um die Worte des Gesangs die weiche Melodie", "tokens": ["Wie", "um", "die", "Wor\u00b7te", "des", "Ge\u00b7sangs", "die", "wei\u00b7che", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Wann liegt mein Haupt auf deinem Schoo\u00df, indem sich", "tokens": ["Wann", "liegt", "mein", "Haupt", "auf", "dei\u00b7nem", "Schoo\u00df", ",", "in\u00b7dem", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PRF"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.11": {"text": "Um deine schlanke H\u00fcfte schlingt, und um dein sch\u00f6nes", "tokens": ["Um", "dei\u00b7ne", "schlan\u00b7ke", "H\u00fcf\u00b7te", "schlingt", ",", "und", "um", "dein", "sch\u00f6\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}