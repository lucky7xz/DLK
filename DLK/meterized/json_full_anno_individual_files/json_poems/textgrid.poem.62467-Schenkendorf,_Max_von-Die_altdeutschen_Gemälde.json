{"textgrid.poem.62467": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Die altdeutschen Gem\u00e4lde", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir winkt ein alter sch\u00f6ner Saal,", "tokens": ["Mir", "winkt", "ein", "al\u00b7ter", "sch\u00f6\u00b7ner", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Br\u00fcder haben ihn gebaut,", "tokens": ["Zwei", "Br\u00fc\u00b7der", "ha\u00b7ben", "ihn", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hab' ich in dem reinsten Strahl", "tokens": ["Da", "hab'", "ich", "in", "dem", "reins\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Vaterland geschaut.", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das war in jener tr\u00fcben Zeit", "tokens": ["Das", "war", "in", "je\u00b7ner", "tr\u00fc\u00b7ben", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein holder stiller Wallfahrtsort,", "tokens": ["Ein", "hol\u00b7der", "stil\u00b7ler", "Wall\u00b7fahr\u00b7tsort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sich der V\u00e4ter Herrlichkeit", "tokens": ["Wo", "sich", "der", "V\u00e4\u00b7ter", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbarg im sichern Port.", "tokens": ["Ver\u00b7barg", "im", "si\u00b7chern", "Port", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der M\u00e4rtyrer und Heil'gen Schaar,", "tokens": ["Der", "M\u00e4r\u00b7ty\u00b7rer", "und", "Heil'\u00b7gen", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Helden Gottes treu und k\u00fchn,", "tokens": ["Viel", "Hel\u00b7den", "Got\u00b7tes", "treu", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die zarten Frauen mild und klar,", "tokens": ["Die", "zar\u00b7ten", "Frau\u00b7en", "mild", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die f\u00fcr den Heiland gl\u00fchn;", "tokens": ["Die", "f\u00fcr", "den", "Hei\u00b7land", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Manch' Bild der allerreinsten Magd,", "tokens": ["Man\u00b7ch'", "Bild", "der", "al\u00b7ler\u00b7reins\u00b7ten", "Magd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie Gottes Engel ihr erschien,", "tokens": ["Wie", "Got\u00b7tes", "En\u00b7gel", "ihr", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wie sie um den Sohn geklagt,", "tokens": ["Bald", "wie", "sie", "um", "den", "Sohn", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald wie die Weisen knien.", "tokens": ["Bald", "wie", "die", "Wei\u00b7sen", "kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was frommer Flei\u00df und keusche Kunst", "tokens": ["Was", "from\u00b7mer", "Flei\u00df", "und", "keu\u00b7sche", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gepflegt in alter deutscher Welt,", "tokens": ["Ge\u00b7pflegt", "in", "al\u00b7ter", "deut\u00b7scher", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ward hier nach Gottes Rath und Gunst", "tokens": ["Ward", "hier", "nach", "Got\u00b7tes", "Rath", "und", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gerettet aufgestellt.", "tokens": ["Ge\u00b7ret\u00b7tet", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es kam wol manches treue Herz", "tokens": ["Es", "kam", "wol", "man\u00b7ches", "treu\u00b7e", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah die lieben Bilder an,", "tokens": ["Und", "sah", "die", "lie\u00b7ben", "Bil\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesegnet sei der tiefe Schmerz,", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "der", "tie\u00b7fe", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der da in ihm begann.", "tokens": ["Der", "da", "in", "ihm", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Liebesbrunst zum Vaterland", "tokens": ["O", "Lie\u00b7bes\u00b7brunst", "zum", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zu der alten Heldenzeit,", "tokens": ["Und", "zu", "der", "al\u00b7ten", "Hel\u00b7den\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bittre Lust, und Gottes Hand", "tokens": ["Du", "bitt\u00b7re", "Lust", ",", "und", "Got\u00b7tes", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Habt uns vom Joch befreit.", "tokens": ["Habt", "uns", "vom", "Joch", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun schauen wir euch anders an,", "tokens": ["Nun", "schau\u00b7en", "wir", "euch", "an\u00b7ders", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr sprechet uns auch fr\u00f6hlich zu,", "tokens": ["Ihr", "spre\u00b7chet", "uns", "auch", "fr\u00f6h\u00b7lich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Bilder, doch ein rechter Mann", "tokens": ["Ihr", "Bil\u00b7der", ",", "doch", "ein", "rech\u00b7ter", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begehrt noch keine Ruh.", "tokens": ["Be\u00b7gehrt", "noch", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr m\u00fcsset erst an K\u00fcnstler Hand", "tokens": ["Ihr", "m\u00fcs\u00b7set", "erst", "an", "K\u00fcnst\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch unsre freien L\u00e4nder gehn,", "tokens": ["Durch", "uns\u00b7re", "frei\u00b7en", "L\u00e4n\u00b7der", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man soll an keiner deutschen Wand", "tokens": ["Man", "soll", "an", "kei\u00b7ner", "deut\u00b7schen", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr Heidenbilder sehn.", "tokens": ["Mehr", "Hei\u00b7den\u00b7bil\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr lieben Heil'gen kommt heraus", "tokens": ["Ihr", "lie\u00b7ben", "Heil'\u00b7gen", "kommt", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und segnet uns, wir flehen euch,", "tokens": ["Und", "seg\u00b7net", "uns", ",", "wir", "fle\u00b7hen", "euch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr holden M\u00e4gdlein schm\u00fcckt das Haus,", "tokens": ["Ihr", "hol\u00b7den", "M\u00e4gd\u00b7lein", "schm\u00fcckt", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Ritter sch\u00fctzt das Reich!", "tokens": ["Ihr", "Rit\u00b7ter", "sch\u00fctzt", "das", "Reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du steh' noch lange, Bildersaal,", "tokens": ["Du", "steh'", "noch", "lan\u00b7ge", ",", "Bil\u00b7der\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Br\u00fcder, \u00fcbet euer Amt,", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "\u00fc\u00b7bet", "eu\u00b7er", "Amt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df an der frommen Vorzeit Strahl", "tokens": ["Da\u00df", "an", "der", "from\u00b7men", "Vor\u00b7zeit", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich manche Brust entflammt!", "tokens": ["Sich", "man\u00b7che", "Brust", "ent\u00b7flammt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mir winkt ein alter sch\u00f6ner Saal,", "tokens": ["Mir", "winkt", "ein", "al\u00b7ter", "sch\u00f6\u00b7ner", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2":