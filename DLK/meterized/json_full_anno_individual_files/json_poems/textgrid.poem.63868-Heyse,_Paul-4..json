{"textgrid.poem.63868": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Jedermann an sich nur denkt, find' ich nicht eben wunderbar,", "tokens": ["Da\u00df", "Je\u00b7der\u00b7mann", "an", "sich", "nur", "denkt", ",", "find'", "ich", "nicht", "e\u00b7ben", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auch da\u00df man gern den N\u00e4chsten kr\u00e4nkt, find' ich nicht eben wunderbar.", "tokens": ["Auch", "da\u00df", "man", "gern", "den", "N\u00e4chs\u00b7ten", "kr\u00e4nkt", ",", "find'", "ich", "nicht", "e\u00b7ben", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Das Ich regiert so Gro\u00df wie Klein, und da\u00df, wer einen Bach entdeckt,", "tokens": ["Das", "Ich", "re\u00b7giert", "so", "Gro\u00df", "wie", "Klein", ",", "und", "da\u00df", ",", "wer", "ei\u00b7nen", "Bach", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "NE", "$,", "KON", "KOUS", "$,", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "In flugs nach seiner M\u00fchle lenkt, find' ich nicht eben wunderbar.", "tokens": ["In", "flugs", "nach", "sei\u00b7ner", "M\u00fch\u00b7le", "lenkt", ",", "find'", "ich", "nicht", "e\u00b7ben", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Nackt sind die Menschen selten sch\u00f6n. Da\u00df Jeder drum ein M\u00e4ntelchen", "tokens": ["Nackt", "sind", "die", "Men\u00b7schen", "sel\u00b7ten", "sch\u00f6n", ".", "Da\u00df", "Je\u00b7der", "drum", "ein", "M\u00e4n\u00b7tel\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$.", "KOUS", "PIS", "PAV", "ART", "NN"], "meter": "+--+-+-+-+-+-+--", "measure": "iambic.septa.invert"}, "line.6": {"text": "Um seine schn\u00f6de Bl\u00f6\u00dfe h\u00e4ngt, find' ich nicht eben wunderbar.", "tokens": ["Um", "sei\u00b7ne", "schn\u00f6\u00b7de", "Bl\u00f6\u00b7\u00dfe", "h\u00e4ngt", ",", "find'", "ich", "nicht", "e\u00b7ben", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Gern nimmt man mit sich selbst vorlieb, und da\u00df nicht jeder Schelm und Schuft", "tokens": ["Gern", "nimmt", "man", "mit", "sich", "selbst", "vor\u00b7lieb", ",", "und", "da\u00df", "nicht", "je\u00b7der", "Schelm", "und", "Schuft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PRF", "ADV", "ADJD", "$,", "KON", "KOUS", "PTKNEG", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Selbst an den n\u00e4chsten Baum sich henkt, find' ich nicht eben wunderbar.", "tokens": ["Selbst", "an", "den", "n\u00e4chs\u00b7ten", "Baum", "sich", "henkt", ",", "find'", "ich", "nicht", "e\u00b7ben", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Doch da\u00df die Luft hier unterm Mond, so voll von eklem Mi\u00dfged\u00fcft,", "tokens": ["Doch", "da\u00df", "die", "Luft", "hier", "un\u00b7term", "Mond", ",", "so", "voll", "von", "ek\u00b7lem", "Mi\u00df\u00b7ge\u00b7d\u00fcft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Und vollends, da\u00df zuweilen noch mir, der ich l\u00e4ngst gewitzigt ward,", "tokens": ["Und", "vol\u00b7lends", ",", "da\u00df", "zu\u00b7wei\u00b7len", "noch", "mir", ",", "der", "ich", "l\u00e4ngst", "ge\u00b7wit\u00b7zigt", "ward", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "ADV", "PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure":