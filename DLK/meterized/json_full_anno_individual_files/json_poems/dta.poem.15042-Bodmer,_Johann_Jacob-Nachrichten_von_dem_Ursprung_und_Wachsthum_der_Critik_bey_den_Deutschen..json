{"dta.poem.15042": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zernichtung unsers Seyns, Gedancke voll Entsetzen,", "tokens": ["Zer\u00b7nich\u00b7tung", "un\u00b7sers", "Seyns", ",", "Ge\u00b7dan\u00b7cke", "voll", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nur ein feiges Hertz bequem ist zu ergetzen,", "tokens": ["Der", "nur", "ein", "fei\u00b7ges", "Hertz", "be\u00b7quem", "ist", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das gern das Hoffnungsrecht zur Ewigkeit vermi\u00dft,", "tokens": ["Das", "gern", "das", "Hoff\u00b7nungs\u00b7recht", "zur", "E\u00b7wig\u00b7keit", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil\u2019s zum Unsterblichseyn zu bl\u00f6d und furchtsam ist!", "tokens": ["Weil's", "zum", "U\u00b7nsterb\u00b7lich\u00b7seyn", "zu", "bl\u00f6d", "und", "furcht\u00b7sam", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "PPOSAT", "PTKA", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Das sich zuwieder w\u00fcnscht und hoffet im Verschwinden", "tokens": ["Das", "sich", "zu\u00b7wie\u00b7der", "w\u00fcnscht", "und", "hof\u00b7fet", "im", "Ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADV", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein finstres, nichtigs, Gl\u00fcck und Besserseyn zu finden;", "tokens": ["Ein", "finst\u00b7res", ",", "nich\u00b7tigs", ",", "Gl\u00fcck", "und", "Bes\u00b7ser\u00b7seyn", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PIS", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das den Gedancken liebt, da\u00df seine Seel im Ru\u00df", "tokens": ["Das", "den", "Ge\u00b7dan\u00b7cken", "liebt", ",", "da\u00df", "sei\u00b7ne", "Seel", "im", "Ru\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich unter todten Schutt dereinst verlieren mu\u00df.", "tokens": ["Sich", "un\u00b7ter", "tod\u00b7ten", "Schutt", "de\u00b7reinst", "ver\u00b7lie\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}