{"textgrid.poem.34797": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr guten Christen, la\u00dft euch nicht", "tokens": ["Ihr", "gu\u00b7ten", "Chris\u00b7ten", ",", "la\u00dft", "euch", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVIMP", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Satans List umgarnen!", "tokens": ["Von", "Sa\u00b7tans", "List", "um\u00b7gar\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sing euch das Tannh\u00e4userlied,", "tokens": ["Ich", "sing", "euch", "das", "Tann\u00b7h\u00e4u\u00b7ser\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Um eure Seelen zu warnen.", "tokens": ["Um", "eu\u00b7re", "See\u00b7len", "zu", "war\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der edle Tannh\u00e4user, ein Ritter gut,", "tokens": ["Der", "ed\u00b7le", "Tann\u00b7h\u00e4u\u00b7ser", ",", "ein", "Rit\u00b7ter", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Wollt Lieb' und Lust gewinnen,", "tokens": ["Wollt", "Lieb'", "und", "Lust", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da zog er in den Venusberg,", "tokens": ["Da", "zog", "er", "in", "den", "Ve\u00b7nus\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Blieb sieben Jahre drinnen.", "tokens": ["Blieb", "sie\u00b7ben", "Jah\u00b7re", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbfrau Venus, meine sch\u00f6ne Frau,", "tokens": ["\u00bb", "frau", "Ve\u00b7nus", ",", "mei\u00b7ne", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leb wohl, mein holdes Leben!", "tokens": ["Leb", "wohl", ",", "mein", "hol\u00b7des", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich will nicht l\u00e4nger bleiben bei dir,", "tokens": ["Ich", "will", "nicht", "l\u00e4n\u00b7ger", "blei\u00b7ben", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "APPR", "PPER", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Du sollst mir Urlaub geben.\u00ab", "tokens": ["Du", "sollst", "mir", "Ur\u00b7laub", "ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbtannh\u00e4user, edler Ritter mein,", "tokens": ["\u00bb", "tann\u00b7h\u00e4u\u00b7ser", ",", "ed\u00b7ler", "Rit\u00b7ter", "mein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast heut mich nicht gek\u00fcsset;", "tokens": ["Hast", "heut", "mich", "nicht", "ge\u00b7k\u00fcs\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "K\u00fc\u00df mich geschwind, und sage mir:", "tokens": ["K\u00fc\u00df", "mich", "ge\u00b7schwind", ",", "und", "sa\u00b7ge", "mir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Was du bei mir vermisset?", "tokens": ["Was", "du", "bei", "mir", "ver\u00b7mis\u00b7set", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Habe ich nicht den s\u00fc\u00dfesten Wein", "tokens": ["Ha\u00b7be", "ich", "nicht", "den", "s\u00fc\u00b7\u00dfes\u00b7ten", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Tagt\u00e4glich dir kredenzet?", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "dir", "kre\u00b7den\u00b7zet", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Und hab ich nicht mit Rosen dir", "tokens": ["Und", "hab", "ich", "nicht", "mit", "Ro\u00b7sen", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tagt\u00e4glich das Haupt bekr\u00e4nzet?\u00ab", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "das", "Haupt", "be\u00b7kr\u00e4n\u00b7zet", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbfrau Venus, meine sch\u00f6ne Frau,", "tokens": ["\u00bb", "frau", "Ve\u00b7nus", ",", "mei\u00b7ne", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von s\u00fc\u00dfem Wein und K\u00fcssen", "tokens": ["Von", "s\u00fc\u00b7\u00dfem", "Wein", "und", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist meine Seele geworden krank;", "tokens": ["Ist", "mei\u00b7ne", "See\u00b7le", "ge\u00b7wor\u00b7den", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VAPP", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich schmachte nach Bitternissen.", "tokens": ["Ich", "schmach\u00b7te", "nach", "Bit\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Wir haben zuviel gescherzt und gelacht,", "tokens": ["Wir", "ha\u00b7ben", "zu\u00b7viel", "ge\u00b7scherzt", "und", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich sehne mich nach Tr\u00e4nen,", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und statt mit Rosen m\u00f6cht ich mein Haupt", "tokens": ["Und", "statt", "mit", "Ro\u00b7sen", "m\u00f6cht", "ich", "mein", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit spitzigen Dornen kr\u00f6nen.\u00ab", "tokens": ["Mit", "spit\u00b7zi\u00b7gen", "Dor\u00b7nen", "kr\u00f6\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbtannh\u00e4user, edler Ritter mein,", "tokens": ["\u00bb", "tann\u00b7h\u00e4u\u00b7ser", ",", "ed\u00b7ler", "Rit\u00b7ter", "mein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du willst dich mit mir zanken;", "tokens": ["Du", "willst", "dich", "mit", "mir", "zan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast geschworen vieltausendmal,", "tokens": ["Du", "hast", "ge\u00b7schwo\u00b7ren", "viel\u00b7tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Niemals von mir zu wanken.", "tokens": ["Nie\u00b7mals", "von", "mir", "zu", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Komm, la\u00df uns in die Kammer gehn,", "tokens": ["Komm", ",", "la\u00df", "uns", "in", "die", "Kam\u00b7mer", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu spielen der heimlichen Minne;", "tokens": ["Zu", "spie\u00b7len", "der", "heim\u00b7li\u00b7chen", "Min\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mein sch\u00f6ner lilienwei\u00dfer Leib", "tokens": ["Mein", "sch\u00f6\u00b7ner", "li\u00b7li\u00b7en\u00b7wei\u00b7\u00dfer", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erheitert deine Sinne.\u00ab", "tokens": ["Er\u00b7hei\u00b7tert", "dei\u00b7ne", "Sin\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbfrau Venus, meine sch\u00f6ne Frau,", "tokens": ["\u00bb", "frau", "Ve\u00b7nus", ",", "mei\u00b7ne", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Reiz wird ewig bl\u00fchen;", "tokens": ["Dein", "Reiz", "wird", "e\u00b7wig", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie viele einst f\u00fcr dich gegl\u00fcht,", "tokens": ["Wie", "vie\u00b7le", "einst", "f\u00fcr", "dich", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So werden noch viele gl\u00fchen.", "tokens": ["So", "wer\u00b7den", "noch", "vie\u00b7le", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Doch denk ich der G\u00f6tter und Helden, die einst", "tokens": ["Doch", "denk", "ich", "der", "G\u00f6t\u00b7ter", "und", "Hel\u00b7den", ",", "die", "einst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "$,", "PRELS", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sich z\u00e4rtlich daran geweidet,", "tokens": ["Sich", "z\u00e4rt\u00b7lich", "da\u00b7ran", "ge\u00b7wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dein sch\u00f6ner lilienwei\u00dfer Leib,", "tokens": ["Dein", "sch\u00f6\u00b7ner", "li\u00b7li\u00b7en\u00b7wei\u00b7\u00dfer", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er wird mir schier verleidet.", "tokens": ["Er", "wird", "mir", "schier", "ver\u00b7lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dein sch\u00f6ner lilienwei\u00dfer Leib", "tokens": ["Dein", "sch\u00f6\u00b7ner", "li\u00b7li\u00b7en\u00b7wei\u00b7\u00dfer", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erf\u00fcllt mich fast mit Entsetzen,", "tokens": ["Er\u00b7f\u00fcllt", "mich", "fast", "mit", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gedenk ich, wie viele werden sich", "tokens": ["Ge\u00b7denk", "ich", ",", "wie", "vie\u00b7le", "wer\u00b7den", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWAV", "PIS", "VAFIN", "PRF"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Noch sp\u00e4terhin dran ergetzen!\u00ab", "tokens": ["Noch", "sp\u00e4\u00b7ter\u00b7hin", "dran", "er\u00b7get\u00b7zen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PAV", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbtannh\u00e4user, edler Ritter mein,", "tokens": ["\u00bb", "tann\u00b7h\u00e4u\u00b7ser", ",", "ed\u00b7ler", "Rit\u00b7ter", "mein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sollst du mir nicht sagen,", "tokens": ["Das", "sollst", "du", "mir", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollte lieber, du schl\u00fcgest mich,", "tokens": ["Ich", "woll\u00b7te", "lie\u00b7ber", ",", "du", "schl\u00fc\u00b7gest", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie du mich oft geschlagen.", "tokens": ["Wie", "du", "mich", "oft", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ich wollte lieber, du schl\u00fcgest mich,", "tokens": ["Ich", "woll\u00b7te", "lie\u00b7ber", ",", "du", "schl\u00fc\u00b7gest", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als da\u00df du Beleidigung spr\u00e4chest,", "tokens": ["Als", "da\u00df", "du", "Be\u00b7lei\u00b7di\u00b7gung", "spr\u00e4\u00b7chest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und mir, undankbar kalter Christ,", "tok