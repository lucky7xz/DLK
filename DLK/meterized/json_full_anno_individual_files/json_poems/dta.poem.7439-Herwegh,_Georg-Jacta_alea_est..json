{"dta.poem.7439": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Jacta alea est.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab's gewagt! und meine Fehde,", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!", "und", "mei\u00b7ne", "Feh\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie w\u00e4hre fort;", "tokens": ["Sie", "w\u00e4h\u00b7re", "fort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich hab's gewagt! so steh' ich Rede", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!", "so", "steh'", "ich", "Re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Manneswort.", "tokens": ["F\u00fcr", "Man\u00b7nes\u00b7wort", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und vor des Thrones Stufen,", "tokens": ["Und", "vor", "des", "Thro\u00b7nes", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn ihr nach meinem Rechte fragt,", "tokens": ["Wenn", "ihr", "nach", "mei\u00b7nem", "Rech\u00b7te", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Will ich mit Hutten rufen:", "tokens": ["Will", "ich", "mit", "Hut\u00b7ten", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich hab's gewagt!", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Von gestern ist mein Brief und Siegel,", "tokens": ["Von", "ge\u00b7stern", "ist", "mein", "Brief", "und", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mein Pergament;", "tokens": ["Mein", "Per\u00b7ga\u00b7ment", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich wei\u00df, da\u00df au\u00dfer meinem Spiegel", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "au\u00b7\u00dfer", "mei\u00b7nem", "Spie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich Niemand kennt.", "tokens": ["Mich", "Nie\u00b7mand", "kennt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ihr la\u00dft die D\u00e4mmrung gelten,", "tokens": ["Ihr", "la\u00dft", "die", "D\u00e4mm\u00b7rung", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bevor der helle Morgen tagt \u2014", "tokens": ["Be\u00b7vor", "der", "hel\u00b7le", "Mor\u00b7gen", "tagt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wohlan \u2014 wer will ", "tokens": ["Wo\u00b7hlan", "wer", "will"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$(", "PWS", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Ich hab's gewagt!", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ja, gibt der greise Knecht die Z\u00f6lle", "tokens": ["Ja", ",", "gibt", "der", "grei\u00b7se", "Knecht", "die", "Z\u00f6l\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Laster frei,", "tokens": ["Dem", "Las\u00b7ter", "frei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dann sei der ", "tokens": ["Dann", "sei", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der Tyrannei.", "tokens": ["Der", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schaut her, die Ihr am Alten", "tokens": ["Schaut", "her", ",", "die", "Ihr", "am", "Al\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Euch Euer Leben m\u00fcde tragt,", "tokens": ["Euch", "Eu\u00b7er", "Le\u00b7ben", "m\u00fc\u00b7de", "tragt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Werft Euer Haubt in Falten:", "tokens": ["Werft", "Eu\u00b7er", "Haubt", "in", "Fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich hab's gewagt!", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich sah in manch gepriesnem Tempel", "tokens": ["Ich", "sah", "in", "manch", "ge\u00b7pri\u00b7es\u00b7nem", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Unnatur,", "tokens": ["Die", "Un\u00b7na\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf manch erlauchter Stirn den Stempel", "tokens": ["Auf", "manch", "er\u00b7lauch\u00b7ter", "Stirn", "den", "Stem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Kain nur;", "tokens": ["Des", "Kain", "nur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und ich ward ungeduldig,", "tokens": ["Und", "ich", "ward", "un\u00b7ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Alles zagt und Niemand klagt,", "tokens": ["Da\u00df", "Al\u00b7les", "zagt", "und", "Nie\u00b7mand", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich donnerte ein: \u201eSchuldig!\u201c", "tokens": ["Ich", "don\u00b7ner\u00b7te", "ein", ":", "\u201e", "Schul\u00b7dig", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich hab's gewagt!", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich sah viel feige Riesen strecken", "tokens": ["Ich", "sah", "viel", "fei\u00b7ge", "Rie\u00b7sen", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Boden sich,", "tokens": ["Zu", "Bo\u00b7den", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Manch \u00fcberm\u00fctig Zwerglein recken", "tokens": ["Manch", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", "Zwerg\u00b7lein", "re\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich f\u00fcrchterlich;", "tokens": ["Sich", "f\u00fcrch\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich lacht' und sprach: O Zwerge,", "tokens": ["Ich", "lacht'", "und", "sprach", ":", "O", "Zwer\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ob ihr auch aus dem Kothe ragt,", "tokens": ["Ob", "ihr", "auch", "aus", "dem", "Ko\u00b7the", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr seid drum keine Berge!", "tokens": ["Ihr", "seid", "drum", "kei\u00b7ne", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich hab's gewagt!", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ich sah im Hohepriesterkleide", "tokens": ["Ich", "sah", "im", "Ho\u00b7he\u00b7pries\u00b7ter\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Unvernunft,", "tokens": ["Die", "Un\u00b7ver\u00b7nunft", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gleich Rohr zerbrechen ihre Eide", "tokens": ["Gleich", "Rohr", "zer\u00b7bre\u00b7chen", "ih\u00b7re", "Ei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Henkerzunft;", "tokens": ["Die", "Hen\u00b7ker\u00b7zunft", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich sah von schn\u00f6den Hunden", "tokens": ["Ich", "sah", "von", "schn\u00f6\u00b7den", "Hun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Freiheit Edelwild gejagt,", "tokens": ["Der", "Frei\u00b7heit", "E\u00b7del\u00b7wild", "ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wusch ihm still die Wunden:", "tokens": ["Und", "wusch", "ihm", "still", "die", "Wun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich hab's gewagt!", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "D\u00fcrft' ich an einer Marmors\u00e4ule", "tokens": ["D\u00fcrft'", "ich", "an", "ei\u00b7ner", "Mar\u00b7mor\u00b7s\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Simson stehn,", "tokens": ["Ein", "Sim\u00b7son", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In meiner Faust Heraklis Keule", "tokens": ["In", "mei\u00b7ner", "Faust", "Her\u00b7ak\u00b7lis", "Keu\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Schwunge drehn,", "tokens": ["Zum", "Schwun\u00b7ge", "drehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wenn die Pal\u00e4ste brechen \u2014", "tokens": ["Wenn", "die", "Pa\u00b7l\u00e4s\u00b7te", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Gott, was hast du mir's versagt? \u2014", "tokens": ["O", "Gott", ",", "was", "hast", "du", "mir's", "ver\u00b7sagt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "NE", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu den Despoten sprechen:", "tokens": ["Zu", "den", "Des\u00b7po\u00b7ten", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ich hab's gewagt!", "tokens": ["Ich", "hab's", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}