{"dta.poem.21032": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcVI.  \n Der Jungfrauschafft.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Viel machten viel aus mir/ viel lachten nur darzu/", "tokens": ["Viel", "mach\u00b7ten", "viel", "aus", "mir", "/", "viel", "lach\u00b7ten", "nur", "dar\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "$(", "ADV", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich war und war auch nicht/ itzt lieg ich in der Ruh.", "tokens": ["Ich", "war", "und", "war", "auch", "nicht", "/", "itzt", "lieg", "ich", "in", "der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VAFIN", "ADV", "PTKNEG", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wil ich meinen Tod zu melden nicht verschie-", "tokens": ["Doch", "wil", "ich", "mei\u00b7nen", "Tod", "zu", "mel\u00b7den", "nicht", "ver\u00b7schie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ben/ (ben.", "tokens": ["ben", "/", "(", "ben", "."], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVPP", "$(", "$(", "VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ich bin durch einen Ritt im Ringelrennen blie-", "tokens": ["Ich", "bin", "durch", "ei\u00b7nen", "Ritt", "im", "Rin\u00b7gel\u00b7ren\u00b7nen", "blie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}