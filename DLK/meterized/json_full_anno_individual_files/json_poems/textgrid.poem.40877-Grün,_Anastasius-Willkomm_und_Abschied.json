{"textgrid.poem.40877": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Willkomm und Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im deutschen Lagerfelde sprang Max vom sch\u00e4umenden Ro\u00df,", "tokens": ["Im", "deut\u00b7schen", "La\u00b7ger\u00b7fel\u00b7de", "sprang", "Max", "vom", "sch\u00e4u\u00b7men\u00b7den", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit gl\u00e4nzenden Augen gr\u00fc\u00dft' ihn der Krieger bunter Tro\u00df.", "tokens": ["Mit", "gl\u00e4n\u00b7zen\u00b7den", "Au\u00b7gen", "gr\u00fc\u00dft'", "ihn", "der", "Krie\u00b7ger", "bun\u00b7ter", "Tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "An seinen Busen flogen die F\u00fcrsten im Kriegsgewand,", "tokens": ["An", "sei\u00b7nen", "Bu\u00b7sen", "flo\u00b7gen", "die", "F\u00fcrs\u00b7ten", "im", "Kriegs\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie that so wohl ihm wieder ein Druck von deutscher Hand!", "tokens": ["Wie", "that", "so", "wohl", "ihm", "wie\u00b7der", "ein", "Druck", "von", "deut\u00b7scher", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ausbreitet der alte Kaiser nach ihm den zitternden Arm,", "tokens": ["Aus\u00b7brei\u00b7tet", "der", "al\u00b7te", "Kai\u00b7ser", "nach", "ihm", "den", "zit\u00b7tern\u00b7den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ha, Friedrich, schlie\u00df' ans Herze den Sohn nun fest und warm!", "tokens": ["Ha", ",", "Fried\u00b7rich", ",", "schlie\u00df'", "ans", "Her\u00b7ze", "den", "Sohn", "nun", "fest", "und", "warm", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "APPRART", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ei was zerdr\u00fcckst du die Thr\u00e4ne, die aus dem Aug' dir gewollt,", "tokens": ["Ei", "was", "zer\u00b7dr\u00fcckst", "du", "die", "Thr\u00e4\u00b7ne", ",", "die", "aus", "dem", "Aug'", "dir", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VMPP", "$,"], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Bist du so geizig mit Thr\u00e4nen, so wie du's bist mit Gold?", "tokens": ["Bist", "du", "so", "gei\u00b7zig", "mit", "Thr\u00e4\u00b7nen", ",", "so", "wie", "du's", "bist", "mit", "Gold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$,", "ADV", "KOKOM", "PIS", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sprach nun Max zum Kreise: \u00bbMein Vater und ihr Herrn,", "tokens": ["Da", "sprach", "nun", "Max", "zum", "Krei\u00b7se", ":", "\u00bb", "Mein", "Va\u00b7ter", "und", "ihr", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "APPRART", "NN", "$.", "$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eins bitt' ich: La\u00dft ob Flandern ergl\u00e4nzen des Friedens Stern!", "tokens": ["Eins", "bitt'", "ich", ":", "La\u00dft", "ob", "Flan\u00b7dern", "er\u00b7gl\u00e4n\u00b7zen", "des", "Frie\u00b7dens", "Stern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "VVIMP", "KOUS", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie einst der Frevel, pilgert jetzt Reue durch das Land,", "tokens": ["Wie", "einst", "der", "Fre\u00b7vel", ",", "pil\u00b7gert", "jetzt", "Reu\u00b7e", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "VVFIN", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Reue windet der Rache das Schwert ja aus der Hand.\u00ab", "tokens": ["Und", "Reu\u00b7e", "win\u00b7det", "der", "Ra\u00b7che", "das", "Schwert", "ja", "aus", "der", "Hand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbmein F\u00fcrst, ihr seid zu m\u00fcde, ger\u00e4cht mu\u00df Deutschland sein!", "tokens": ["\u00bb", "mein", "F\u00fcrst", ",", "ihr", "seid", "zu", "m\u00fc\u00b7de", ",", "ge\u00b7r\u00e4cht", "mu\u00df", "Deutschland", "sein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,", "VVPP", "VMFIN", "NE", "VAINF", "$."], "meter": "-+-+-+--+---", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir stehen All' f\u00fcr Einen, wie Einer f\u00fcr Alle ein!\u00ab", "tokens": ["Wir", "ste\u00b7hen", "All'", "f\u00fcr", "Ei\u00b7nen", ",", "wie", "Ei\u00b7ner", "f\u00fcr", "Al\u00b7le", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "$,", "PWAV", "PIS", "APPR", "PIS", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So brauste Albert der Sachse. Wie's ihm vom Auge flammt!", "tokens": ["So", "braus\u00b7te", "Al\u00b7bert", "der", "Sach\u00b7se", ".", "Wie's", "ihm", "vom", "Au\u00b7ge", "flammt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$.", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "H\u00e4tt's allen deutschen F\u00fcrsten stets so vom Aug' geflammt!", "tokens": ["H\u00e4tt's", "al\u00b7len", "deut\u00b7schen", "F\u00fcrs\u00b7ten", "stets", "so", "vom", "Aug'", "ge\u00b7flammt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbwohlan!\u00ab rief Max nun d\u00fcster, \u00bbihr Freunde lebet wohl!", "tokens": ["\u00bb", "wo\u00b7hlan", "!", "\u00ab", "rief", "Max", "nun", "d\u00fcs\u00b7ter", ",", "\u00bb", "ihr", "Freun\u00b7de", "le\u00b7bet", "wohl", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "$(", "VVFIN", "NE", "ADV", "ADJD", "$,", "$(", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf, meine Getreuen! wir ziehen ins freundliche Tyrol,", "tokens": ["Auf", ",", "mei\u00b7ne", "Ge\u00b7treu\u00b7en", "!", "wir", "zie\u00b7hen", "ins", "freund\u00b7li\u00b7che", "Ty\u00b7rol", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Zwar hei\u00dft's ein Bauernkittel, doch lernte schon mancher Mann,", "tokens": ["Zwar", "hei\u00dft's", "ein", "Bau\u00b7ern\u00b7kit\u00b7tel", ",", "doch", "lern\u00b7te", "schon", "man\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df in den rauhen Falten man's Herz recht w\u00e4rmen kann.", "tokens": ["Da\u00df", "in", "den", "rau\u00b7hen", "Fal\u00b7ten", "man's", "Herz", "recht", "w\u00e4r\u00b7men", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Denn nicht zerstampfe die Saaten des eignen Lands mein Pferd,", "tokens": ["Denn", "nicht", "zer\u00b7stamp\u00b7fe", "die", "Saa\u00b7ten", "des", "eig\u00b7nen", "Lands", "mein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von meines Volkes Blute sei rein mein gutes Schwert!", "tokens": ["Von", "mei\u00b7nes", "Vol\u00b7kes", "Blu\u00b7te", "sei", "rein", "mein", "gu\u00b7tes", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VAFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So zieht denn hin, ihr F\u00fcrsten! Doch schont die B\u00fcrger im Land!", "tokens": ["So", "zieht", "denn", "hin", ",", "ihr", "F\u00fcrs\u00b7ten", "!", "Doch", "schont", "die", "B\u00fcr\u00b7ger", "im", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn wahrlich, sie sind die Perlen in meinem Kronenband.\u00ab", "tokens": ["Denn", "wahr\u00b7lich", ",", "sie", "sind", "die", "Per\u00b7len", "in", "mei\u00b7nem", "Kro\u00b7nen\u00b7band", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbja wohl, mein Max; doch Perlen sind jetzt ein theurer Kauf,", "tokens": ["\u00bb", "ja", "wohl", ",", "mein", "Max", ";", "doch", "Per\u00b7len", "sind", "jetzt", "ein", "theu\u00b7rer", "Kauf", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "PPOSAT", "NE", "$.", "ADV", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Drum kn\u00fcpft' ich diese Perlen an ", "tokens": ["Drum", "kn\u00fcpft'", "ich", "die\u00b7se", "Per\u00b7len", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So rief nun Kunz und guckte im Kreis der F\u00fcrsten heraus,", "tokens": ["So", "rief", "nun", "Kunz", "und", "guck\u00b7te", "im", "Kreis", "der", "F\u00fcrs\u00b7ten", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als ob's: \u00bbVergi\u00df-mein-nicht!\u00ab riefe aus vollem Tulpenstrau\u00df.", "tokens": ["Als", "ob's", ":", "\u00bb", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "!", "\u00ab", "rie\u00b7fe", "aus", "vol\u00b7lem", "Tul\u00b7pen\u00b7strau\u00df", "."], "token_info": ["word", "word", "punct