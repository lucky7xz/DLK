{"textgrid.poem.61567": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst in meiner Seele lesen", "tokens": ["Du", "willst", "in", "mei\u00b7ner", "See\u00b7le", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und still mein bestes Teil empfahn;", "tokens": ["Und", "still", "mein", "bes\u00b7tes", "Teil", "em\u00b7pfahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schau' mein unverg\u00e4nglich Wesen", "tokens": ["So", "schau'", "mein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Spiegel meiner Lieder an.", "tokens": ["Im", "Spie\u00b7gel", "mei\u00b7ner", "Lie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin die Weise, die dich r\u00fchret,", "tokens": ["Ich", "bin", "die", "Wei\u00b7se", ",", "die", "dich", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin das Wort, das zu dir spricht,", "tokens": ["Ich", "bin", "das", "Wort", ",", "das", "zu", "dir", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Hauch, den deine Seele sp\u00fcret,", "tokens": ["Der", "Hauch", ",", "den", "dei\u00b7ne", "See\u00b7le", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich bin's \u2013 und dennoch bin ich's nicht.", "tokens": ["Ich", "bin's", "\u2013", "und", "den\u00b7noch", "bin", "ich's", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KON", "ADV", "VAFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn sieh, noch oft mit hei\u00dfem Ringen", "tokens": ["Denn", "sieh", ",", "noch", "oft", "mit", "hei\u00b7\u00dfem", "Rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Schuld und Tr\u00fcbsal irrt mein Gang,", "tokens": ["Durch", "Schuld", "und", "Tr\u00fcb\u00b7sal", "irrt", "mein", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch dr\u00fcber zieht auf reinen Schwingen", "tokens": ["Doch", "dr\u00fc\u00b7ber", "zieht", "auf", "rei\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "APPR", "ADJA", "NN"], 