{"dta.poem.19329": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nur auf Eine Art sich Gott h\u00e4tt' offenbart,", "tokens": ["Wenn", "nur", "auf", "Ei\u00b7ne", "Art", "sich", "Gott", "h\u00e4tt'", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "PRF", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu offenbar h\u00e4tt' ihn des Menschen Geist gewahrt.", "tokens": ["Zu", "of\u00b7fen\u00b7bar", "h\u00e4tt'", "ihn", "des", "Men\u00b7schen", "Geist", "ge\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch nun verh\u00fcllen ihn viel Offenbarungen,", "tokens": ["Doch", "nun", "ver\u00b7h\u00fcl\u00b7len", "ihn", "viel", "Of\u00b7fen\u00b7ba\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und unvollkommen sind die Gottgewahrungen.", "tokens": ["Und", "un\u00b7voll\u00b7kom\u00b7men", "sind", "die", "Gott\u00b7ge\u00b7wah\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Glaubensweisen Streit zeigt seine Herrlichkeit,", "tokens": ["Der", "Glau\u00b7bens\u00b7wei\u00b7sen", "Streit", "zeigt", "sei\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Er ist Eins, um den sich unser Wahn entzweit.", "tokens": ["Denn", "Er", "ist", "Eins", ",", "um", "den", "sich", "un\u00b7ser", "Wahn", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$,", "KOUI", "ART", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}