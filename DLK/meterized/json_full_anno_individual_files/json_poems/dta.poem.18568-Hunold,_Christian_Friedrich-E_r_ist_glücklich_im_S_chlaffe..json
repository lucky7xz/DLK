{"dta.poem.18568": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "E r ist gl\u00fccklich im  S chlaffe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Erw\u00fcnschte Nacht! ihr angenehmen Schatten!", "tokens": ["Er\u00b7w\u00fcnschte", "Nacht", "!", "ihr", "an\u00b7ge\u00b7neh\u00b7men", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was vor ein Strahl umzircket mein Gezehlt?", "tokens": ["Was", "vor", "ein", "Strahl", "um\u00b7zir\u00b7cket", "mein", "Ge\u00b7zehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will sich mit mir noch eine Sonne gatten/", "tokens": ["Will", "sich", "mit", "mir", "noch", "ei\u00b7ne", "Son\u00b7ne", "gat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sich anitzt zu meinem Bette stellt?", "tokens": ["Die", "sich", "a\u00b7nitzt", "zu", "mei\u00b7nem", "Bet\u00b7te", "stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach Engels-Kind sey tausendmahl willkommen/", "tokens": ["Ach", "En\u00b7gels\u00b7Kind", "sey", "tau\u00b7send\u00b7mahl", "will\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie bin ich doch so unverhofft begl\u00fcckt?", "tokens": ["Wie", "bin", "ich", "doch", "so", "un\u00b7ver\u00b7hofft", "be\u00b7gl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo hast du denn die Gnade hergenommen/", "tokens": ["Wo", "hast", "du", "denn", "die", "Gna\u00b7de", "her\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df deine Brust noch meinen Geist erquickt?", "tokens": ["Da\u00df", "dei\u00b7ne", "Brust", "noch", "mei\u00b7nen", "Geist", "er\u00b7quickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Komm lege dich auf dieses sanffte K\u00fcssen/", "tokens": ["Komm", "le\u00b7ge", "dich", "auf", "die\u00b7ses", "sanff\u00b7te", "K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PRF", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Hier findest du der Liebe Sammel-Platz", "tokens": ["Hier", "fin\u00b7dest", "du", "der", "Lie\u00b7be", "Sam\u00b7mel\u00b7Platz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dein Zucker soll mir alle Quaal vers\u00fcssen", "tokens": ["Dein", "Zu\u00b7cker", "soll", "mir", "al\u00b7le", "Qua\u00b7al", "ver\u00b7s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Vergn\u00fcge mich du auserwehlter Schatz.", "tokens": ["Ver\u00b7gn\u00fc\u00b7ge", "mich", "du", "au\u00b7ser\u00b7wehl\u00b7ter", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Umarme doch mit wollen weichen H\u00e4nden", "tokens": ["Um\u00b7ar\u00b7me", "doch", "mit", "wol\u00b7len", "wei\u00b7chen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den heissen Leib/ der sich nach K\u00fchlung sehnt/", "tokens": ["Den", "heis\u00b7sen", "Leib", "/", "der", "sich", "nach", "K\u00fch\u00b7lung", "sehnt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Erhebe dich mit deinen zarten Lenden", "tokens": ["Er\u00b7he\u00b7be", "dich", "mit", "dei\u00b7nen", "zar\u00b7ten", "Len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Schau wie die Lust schon alle Glieder dehnt.", "tokens": ["Schau", "wie", "die", "Lust", "schon", "al\u00b7le", "Glie\u00b7der", "dehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Es sind mein Kind zwar allzu enge Schrancken/", "tokens": ["Es", "sind", "mein", "Kind", "zwar", "all\u00b7zu", "en\u00b7ge", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "PTKA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Allein es geht mit s\u00fcssen Zwang hinein/", "tokens": ["Al\u00b7lein", "es", "geht", "mit", "s\u00fcs\u00b7sen", "Zwang", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ach Zucker-Kost der k\u00fctzlenden Gedancken/", "tokens": ["Ach", "Zu\u00b7cke\u00b7rKost", "der", "k\u00fctz\u00b7len\u00b7den", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Dabey das Marck mu\u00df ausgezehret seyn.", "tokens": ["Da\u00b7bey", "das", "Marck", "mu\u00df", "aus\u00b7ge\u00b7zeh\u00b7ret", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ach la\u00df uns doch die Freude recht geniessen", "tokens": ["Ach", "la\u00df", "uns", "doch", "die", "Freu\u00b7de", "recht", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Bem\u00fche dich und f\u00f6rder ihren Lauff.", "tokens": ["Be\u00b7m\u00fc\u00b7he", "dich", "und", "f\u00f6r\u00b7der", "ih\u00b7ren", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Itzt wird sich gleich der s\u00fcsse Thau ergiessen", "tokens": ["Itzt", "wird", "sich", "gleich", "der", "s\u00fcs\u00b7se", "Thau", "er\u00b7gies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ach Kind! Ach Schatz! thu deine Muschel auf.", "tokens": ["Ach", "Kind", "!", "Ach", "Schatz", "!", "thu", "dei\u00b7ne", "Mu\u00b7schel", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ITJ", "NN", "$.", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Nur noch einmahl. - - Wie bist du gar verschwunden?", "tokens": ["Nur", "noch", "ein\u00b7mahl", ".", "Wie", "bist", "du", "gar", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$.", "$(", "$(", "PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verfluchter Traum/ der mich so sehr betr\u00fcgt!", "tokens": ["Ver\u00b7fluch\u00b7ter", "Traum", "/", "der", "mich", "so", "sehr", "be\u00b7tr\u00fcgt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Wo bleiben nun die Anmuhts-vollen Stunden?", "tokens": ["Wo", "blei\u00b7ben", "nun", "die", "Anm\u00b7uhts\u00b7vol\u00b7len", "Stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Wo ist mein Schatz/ der mir in Armen liegt?", "tokens": ["Wo", "ist", "mein", "Schatz", "/", "der", "mir", "in", "Ar\u00b7men", "liegt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Ach alles ist nur Phantasie zu nennen!", "tokens": ["Ach", "al\u00b7les", "ist", "nur", "Phan\u00b7ta\u00b7sie", "zu", "nen\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VAFIN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Die f\u00fchrt mich offt auf diese falsche Bahn.", "tokens": ["Die", "f\u00fchrt", "mich", "offt", "auf", "die\u00b7se", "fal\u00b7sche", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ach Sch\u00f6nste soll ich stets vergebens brennen?", "tokens": ["Ach", "Sch\u00f6ns\u00b7te", "soll", "ich", "stets", "ver\u00b7ge\u00b7bens", "bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Was hat dir doch dein treuer Knecht gethan.", "tokens": ["Was", "hat", "dir", "doch", "dein", "treu\u00b7er", "Knecht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Ich seuffze zwar/ alleine gantz vergebens/", "tokens": ["Ich", "seuff\u00b7ze", "zwar", "/", "al\u00b7lei\u00b7ne", "gantz", "ver\u00b7ge\u00b7bens", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Was hilfft die Nacht/ wenn mich die Sonne qu\u00e4lt.", "tokens": ["Was", "hilfft", "die", "Nacht", "/", "wenn", "mich", "die", "Son\u00b7ne", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Ich sehe schon das Ende meines Lebens/", "tokens": ["Ich", "se\u00b7he", "schon", "das", "En\u00b7de", "mei\u00b7nes", "Le\u00b7bens", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Wo mich dein Strahl der Liebe nicht beseelt.", "tokens": ["Wo", "mich", "dein", "Strahl", "der", "Lie\u00b7be", "nicht", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}