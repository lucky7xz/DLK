{"dta.poem.8407": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mailied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.57", "da:0.42"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und der Himmel blau;", "tokens": ["Und", "der", "Him\u00b7mel", "blau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Schwalben kehren wieder,", "tokens": ["Schwal\u00b7ben", "keh\u00b7ren", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Erstlingslieder", "tokens": ["Und", "die", "Erst\u00b7lings\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kleiner V\u00f6gelein", "tokens": ["Klei\u00b7ner", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Zwitschern durch den Hain.", "tokens": ["Zwit\u00b7schern", "durch", "den", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aus dem Bl\u00fctenstrauch", "tokens": ["Aus", "dem", "Bl\u00fc\u00b7ten\u00b7strauch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Weht der Liebe Hauch:", "tokens": ["Weht", "der", "Lie\u00b7be", "Hauch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seit der Lenz erschienen,", "tokens": ["Seit", "der", "Lenz", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Waltet sie im Gr\u00fcnen,", "tokens": ["Wal\u00b7tet", "sie", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Malt die Blumen bunt,", "tokens": ["Malt", "die", "Blu\u00b7men", "bunt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Roth des M\u00e4dchens Mund.", "tokens": ["Roth", "des", "M\u00e4d\u00b7chens", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Br\u00fcder, k\u00fcsset ihn!", "tokens": ["Br\u00fc\u00b7der", ",", "k\u00fcs\u00b7set", "ihn", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Denn die Jahre fliehn!", "tokens": ["Denn", "die", "Jah\u00b7re", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einen Kuss in Ehren", "tokens": ["Ei\u00b7nen", "Kuss", "in", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kann euch niemand wehren!", "tokens": ["Kann", "euch", "nie\u00b7mand", "weh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "K\u00fcsst ihn, Br\u00fcder, k\u00fcsst,", "tokens": ["K\u00fcsst", "ihn", ",", "Br\u00fc\u00b7der", ",", "k\u00fcsst", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Weil er kusslich ist!", "tokens": ["Weil", "er", "kuss\u00b7lich", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Seht, der Tauber girrt,", "tokens": ["Seht", ",", "der", "Tau\u00b7ber", "girrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Seht, der Tauber schwirrt", "tokens": ["Seht", ",", "der", "Tau\u00b7ber", "schwirrt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Um sein liebes T\u00e4ubchen!", "tokens": ["Um", "sein", "lie\u00b7bes", "T\u00e4ub\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nehmt euch auch ein Weibchen,", "tokens": ["Nehmt", "euch", "auch", "ein", "Weib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie der Tauber thut,", "tokens": ["Wie", "der", "Tau\u00b7ber", "thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und seid wohlgemut!", "tokens": ["Und", "seid", "wohl\u00b7ge\u00b7mut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}