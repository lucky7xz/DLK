{"dta.poem.10753": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Aliud eiusdem.   Vff zwo Schwestern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Weg weg mit aller gunst der drey mahl drey G\u00f6ttinnen/", "tokens": ["Weg", "weg", "mit", "al\u00b7ler", "gunst", "der", "drey", "mahl", "drey", "G\u00f6t\u00b7tin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN", "ART", "CARD", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ich so manichmahl von ", "tokens": ["Die", "ich", "so", "ma\u00b7nich\u00b7mahl", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu h\u00fclss geruffen hab; weg weg jhr T\u00f6chter drey", "tokens": ["Zu", "h\u00fclss", "ge\u00b7ruf\u00b7fen", "hab", ";", "weg", "weg", "jhr", "T\u00f6ch\u00b7ter", "drey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VVPP", "VAFIN", "$.", "ADV", "ADV", "PPOSAT", "NN", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eurynomes, vor euch komt mehr nit mein gefchrey.", "tokens": ["Eu\u00b7ry\u00b7no\u00b7mes", ",", "vor", "euch", "komt", "mehr", "nit", "mein", "gefc\u00b7hrey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPER", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Ich wei\u00df/ o Gl\u00fcck/ ich wei\u00df zwey Schwestern au\u00dferkohren/", "tokens": ["Ich", "wei\u00df", "/", "o", "Gl\u00fcck", "/", "ich", "wei\u00df", "zwey", "Schwes\u00b7tern", "au\u00b7\u00dfer\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "FM", "NN", "$(", "PPER", "VVFIN", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aller G\u00f6ttinnen Volck zu trotz/ zu hon gebohren/", "tokens": ["Al\u00b7ler", "G\u00f6t\u00b7tin\u00b7nen", "Volck", "zu", "trotz", "/", "zu", "hon", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKZU", "APPR", "$(", "APPR", "NN", "VVPP", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Voll Himmlischen verstands/ mehr als die ", "tokens": ["Voll", "Himm\u00b7li\u00b7schen", "ver\u00b7stands", "/", "mehr", "als", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "VVPP", "$(", "PIAT", "KOKOM", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll mehr Holdseeligkeit als ", "tokens": ["Voll", "mehr", "Hold\u00b7see\u00b7lig\u00b7keit", "als"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PIAT", "NN", "KOKOM"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Ich will nit dreymahl drey/ nicht drey/ nicht eyne preysen/", "tokens": ["Ich", "will", "nit", "drey\u00b7mahl", "drey", "/", "nicht", "drey", "/", "nicht", "ey\u00b7ne", "prey\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "CARD", "$(", "PTKNEG", "CARD", "$(", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich will allein dem par vorthin die Ehr beweisen/", "tokens": ["Ich", "will", "al\u00b7lein", "dem", "par", "vor\u00b7thin", "die", "Ehr", "be\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "FM", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dem au\u00dferwehlten par/ die au\u00dferlesen Ehr/", "tokens": ["Dem", "au\u00b7\u00dfer\u00b7wehl\u00b7ten", "par", "/", "die", "au\u00b7\u00dfer\u00b7le\u00b7sen", "Ehr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dardurch erschallen mag jhr Lob je mehr vnd mehr.", "tokens": ["Dar\u00b7durch", "er\u00b7schal\u00b7len", "mag", "jhr", "Lob", "je", "mehr", "vnd", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wer wei\u00df ob ich dann auch die gunst nit m\u00f6cht erlangen/", "tokens": ["Wer", "wei\u00df", "ob", "ich", "dann", "auch", "die", "gunst", "nit", "m\u00f6cht", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd new geschicklichkeit/ Natur vnd Geist empfangen/", "tokens": ["Vnd", "new", "ge\u00b7schick\u00b7lich\u00b7keit", "/", "Na\u00b7tur", "vnd", "Geist", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$(", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "O Ph&#339;be Gebhard? gibt nit solcher gunsten pfandt", "tokens": ["O", "Ph", "&#339;", "be", "Geb\u00b7hard", "?", "gibt", "nit", "sol\u00b7cher", "guns\u00b7ten", "pfandt"], "token_info": ["word", "word", "XML_entity", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "ADJA", "NN", "$.", "VVFIN", "PTKNEG", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Tausent Tausent k\u00fc\u00df wolw\u00fcrdigeste Handt?", "tokens": ["Die", "Tau\u00b7sent", "Tau\u00b7sent", "k\u00fc\u00df", "wol\u00b7w\u00fcr\u00b7di\u00b7ges\u00b7te", "Handt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}