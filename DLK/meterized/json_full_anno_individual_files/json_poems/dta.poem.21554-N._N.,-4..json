{"dta.poem.21554": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht wahr, Du bist ein gro\u00dfes Thier?", "tokens": ["Nicht", "wahr", ",", "Du", "bist", "ein", "gro\u00b7\u00dfes", "Thier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sprich, was ist zum Dichten n\u00fctze?", "tokens": ["So", "sprich", ",", "was", "ist", "zum", "Dich\u00b7ten", "n\u00fct\u00b7ze", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eine Perryfeder, ein St\u00fcck Papier,", "tokens": ["Ei\u00b7ne", "Per\u00b7ry\u00b7fe\u00b7der", ",", "ein", "St\u00fcck", "Pa\u00b7pier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+----+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ein Tintenfa\u00df und \u2014 ein Sch\u00e4del voll Gr\u00fctze!", "tokens": ["Ein", "Tin\u00b7ten\u00b7fa\u00df", "und", "ein", "Sch\u00e4\u00b7del", "voll", "Gr\u00fct\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "$(", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}