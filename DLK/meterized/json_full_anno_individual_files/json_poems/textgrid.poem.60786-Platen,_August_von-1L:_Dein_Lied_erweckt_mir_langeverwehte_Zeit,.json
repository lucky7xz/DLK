, "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo ach! Verwirrung br\u00fctet, und innerhalb", "tokens": ["Wo", "ach", "!", "Ver\u00b7wir\u00b7rung", "br\u00fc\u00b7tet", ",", "und", "in\u00b7ner\u00b7halb"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ITJ", "$.", "NN", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Mauern Ilions und ausw\u00e4rts", "tokens": ["Der", "Mau\u00b7ern", "I\u00b7li\u00b7ons", "und", "aus\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "S\u00fcndiget blinde Begier. Du rufst mich", "tokens": ["S\u00fcn\u00b7di\u00b7get", "blin\u00b7de", "Be\u00b7gier", ".", "Du", "rufst", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "An Goethes Grab. Gern werf ich den sch\u00f6nsten Zweig", "tokens": ["An", "Goe\u00b7thes", "Grab", ".", "Gern", "werf", "ich", "den", "sch\u00f6ns\u00b7ten", "Zweig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$.", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf seine Ruhstatt! Sanfterer Tage Sohn,", "tokens": ["Auf", "sei\u00b7ne", "Ruh\u00b7statt", "!", "Sanf\u00b7te\u00b7rer", "Ta\u00b7ge", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und selbst als Greis noch liebet\u00e4ndelnd,", "tokens": ["Und", "selbst", "als", "Greis", "noch", "lie\u00b7be\u00b7t\u00e4n\u00b7delnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wu\u00dft er die m\u00e4chtige Brust zu z\u00e4hmen,", "tokens": ["Wu\u00dft", "er", "die", "m\u00e4ch\u00b7ti\u00b7ge", "Brust", "zu", "z\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Eintauschend Weisheit f\u00fcr die Begeisterung:", "tokens": ["Ein\u00b7tau\u00b7schend", "Weis\u00b7heit", "f\u00fcr", "die", "Be\u00b7geis\u00b7te\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht dies gelingt mir! Jeglicher Puls in mir", "tokens": ["Nicht", "dies", "ge\u00b7lingt", "mir", "!", "Jeg\u00b7li\u00b7cher", "Puls", "in", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PDS", "VVFIN", "PPER", "$.", "PIAT", "NN", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wallt feurig auf; nicht blo\u00dfe T\u00f6ne,", "tokens": ["Wallt", "feu\u00b7rig", "auf", ";", "nicht", "blo\u00b7\u00dfe", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$.", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Funken entspr\u00fchn der bewegten Leier!", "tokens": ["Fun\u00b7ken", "ent\u00b7spr\u00fchn", "der", "be\u00b7weg\u00b7ten", "Lei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Nicht kann ich harmlos mich in die Pflanzenwelt", "tokens": ["Nicht", "kann", "ich", "harm\u00b7los", "mich", "in", "die", "Pflan\u00b7zen\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADJD", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Einspinnen, anschaun kantigen Bergkristall", "tokens": ["Ein\u00b7spin\u00b7nen", ",", "an\u00b7schaun", "kan\u00b7ti\u00b7gen", "Berg\u00b7kris\u00b7tall"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVINF", "VMFIN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sorgf\u00e4ltig, Freund! Zu tief ergreift mich", "tokens": ["Sorg\u00b7f\u00e4l\u00b7tig", ",", "Freund", "!", "Zu", "tief", "er\u00b7greift", "mich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "$.", "PTKA", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Menschlichen Wechselgeschicks Entfaltung.", "tokens": ["Menschli\u00b7chen", "Wech\u00b7sel\u00b7ge\u00b7schicks", "Ent\u00b7fal\u00b7tung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "L\u00e4ngst ist der Brust ehrgeiziger Trieb entflohn,", "tokens": ["L\u00e4ngst", "ist", "der", "Brust", "ehr\u00b7gei\u00b7zi\u00b7ger", "Trieb", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Jugend Erbteil; aber wofern mir soll", "tokens": ["Der", "Ju\u00b7gend", "Erb\u00b7teil", ";", "a\u00b7ber", "wo\u00b7fern", "mir", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "ADV", "KOUS", "PPER", "VMFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Annahn der Ruhm, mag Hand in Hand er", "tokens": ["An\u00b7nahn", "der", "Ruhm", ",", "mag", "Hand", "in", "Hand", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "VMFIN", "NN", "APPR", "NN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gehn mit dem pr\u00fcfenden Todesengel!", "tokens": ["Gehn", "mit", "dem", "pr\u00fc\u00b7fen\u00b7den", "To\u00b7de\u00b7sen\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Von dieser Zeit Parteiungen hoff ich nichts;", "tokens": ["Von", "die\u00b7ser", "Zeit", "Par\u00b7tei\u00b7un\u00b7gen", "hoff", "ich", "nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch wann ich darf ausruhen, wie Goethe ruht,", "tokens": ["Doch", "wann", "ich", "darf", "aus\u00b7ru\u00b7hen", ",", "wie", "Goe\u00b7the", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VMFIN", "VVINF", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann sein mir auch sp\u00e4treife Kr\u00e4nze", "tokens": ["Dann", "sein", "mir", "auch", "sp\u00e4t\u00b7rei\u00b7fe", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAINF", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf den versinkenden Sarg geworfen.", "tokens": ["Auf", "den", "ver\u00b7sin\u00b7ken\u00b7den", "Sarg", "ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Ich lebe ganz bei K\u00fcnftigen, halb nur jetzt:", "tokens": ["Ich", "le\u00b7be", "ganz", "bei", "K\u00fcnf\u00b7ti\u00b7gen", ",", "halb", "nur", "jetzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht blo\u00df ein Zierat m\u00fc\u00dfigem Zeitvertreib", "tokens": ["Nicht", "blo\u00df", "ein", "Zie\u00b7rat", "m\u00fc\u00b7\u00dfi\u00b7gem", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sei meine Dichtkunst, nein \u2013 sie gie\u00dfe", "tokens": ["Sei", "mei\u00b7ne", "Dicht\u00b7kunst", ",", "nein", "\u2013", "sie", "gie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PTKANT", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tauigen Glanz in die welke Blume!", "tokens": ["Tau\u00b7i\u00b7gen", "Glanz", "in", "die", "wel\u00b7ke", "Blu\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["AD