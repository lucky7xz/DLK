{"textgrid.poem.41448": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aesop bewies zu seiner Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aesop bewies zu seiner Zeit", "tokens": ["A\u00b7e\u00b7sop", "be\u00b7wies", "zu", "sei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die schwerste Kunst in unsern Tagen,", "tokens": ["Die", "schwers\u00b7te", "Kunst", "in", "un\u00b7sern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kunst, die Narren zu ertragen,", "tokens": ["Die", "Kunst", ",", "die", "Nar\u00b7ren", "zu", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zunft, die immer sich verneut.", "tokens": ["Die", "Zunft", ",", "die", "im\u00b7mer", "sich", "ver\u00b7neut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Bube, den nichts fr\u00f6hlich machte,", "tokens": ["Ein", "Bu\u00b7be", ",", "den", "nichts", "fr\u00f6h\u00b7lich", "mach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als