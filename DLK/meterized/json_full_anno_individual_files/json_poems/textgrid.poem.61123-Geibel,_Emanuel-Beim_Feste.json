{"textgrid.poem.61123": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Beim Feste", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O f\u00fcllt die Pokale mit zyprischem Wein!", "tokens": ["O", "f\u00fcllt", "die", "Po\u00b7ka\u00b7le", "mit", "zyp\u00b7ri\u00b7schem", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "La\u00dft blinken im Becher den purpurnen Schein!", "tokens": ["La\u00dft", "blin\u00b7ken", "im", "Be\u00b7cher", "den", "pur\u00b7pur\u00b7nen", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Schl\u00fcrft hastigen Zuges den raschen Genu\u00df!", "tokens": ["Schl\u00fcrft", "has\u00b7ti\u00b7gen", "Zu\u00b7ges", "den", "ra\u00b7schen", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So kurz ist die Jugend, so fl\u00fcchtig der Ku\u00df.", "tokens": ["So", "kurz", "ist", "die", "Ju\u00b7gend", ",", "so", "fl\u00fcch\u00b7tig", "der", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Es flammen die Rosen in duftiger Glut,", "tokens": ["Es", "flam\u00b7men", "die", "Ro\u00b7sen"