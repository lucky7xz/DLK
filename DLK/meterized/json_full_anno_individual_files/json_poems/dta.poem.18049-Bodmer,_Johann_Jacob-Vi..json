{"dta.poem.18049": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es suchte seine Weid in Hungersnoth", "tokens": ["Es", "such\u00b7te", "sei\u00b7ne", "Weid", "in", "Hun\u00b7gers\u00b7noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Gei\u00dfbock, wie es ihm sein Naturell gebot.", "tokens": ["Ein", "Gei\u00df\u00b7bock", ",", "wie", "es", "ihm", "sein", "Na\u00b7tu\u00b7rell", "ge\u00b7bot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er gieng sehr hoch in einer ", "tokens": ["Er", "gieng", "sehr", "hoch", "in", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Thier kam\u2019 ihm daselbsten zu.", "tokens": ["Kein", "Thier", "kam'", "ihm", "da\u00b7selbs\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sah er einen grimmgen L\u00f6wen", "tokens": ["Da", "sah", "er", "ei\u00b7nen", "grimm\u00b7gen", "L\u00f6\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weit unter ihm im ebnen Boden stehen.", "tokens": ["Weit", "un\u00b7ter", "ihm", "im", "eb\u00b7nen", "Bo\u00b7den", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der sprach zu ihm mit hingelegtem Grimme", "tokens": ["Der", "sprach", "zu", "ihm", "mit", "hin\u00b7ge\u00b7leg\u00b7tem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und einem sanften Ton der Stimme:", "tokens": ["Und", "ei\u00b7nem", "sanf\u00b7ten", "Ton", "der", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mich wundert, da\u00df du magst dein Leben", "tokens": ["Mich", "wun\u00b7dert", ",", "da\u00df", "du", "magst", "dein", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Um solche schlechte Speise geben.", "tokens": ["Um", "sol\u00b7che", "schlech\u00b7te", "Spei\u00b7se", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dein Steig ist alles Schreckens voll,", "tokens": ["Dein", "Steig", "ist", "al\u00b7les", "Schre\u00b7ckens", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So hoch da\u00df niemand ihn betreten soll.", "tokens": ["So", "hoch", "da\u00df", "nie\u00b7mand", "ihn", "be\u00b7tre\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Auf Speise dort zu gehn, ist gar nicht gut;", "tokens": ["Auf", "Spei\u00b7se", "dort", "zu", "gehn", ",", "ist", "gar", "nicht", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mi\u00dfl\u00e4nge dem, der dieses thut,", "tokens": ["Mi\u00df\u00b7l\u00e4n\u00b7ge", "dem", ",", "der", "die\u00b7ses", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So hie\u00df es, ihm gesch\u00e4he recht:", "tokens": ["So", "hie\u00df", "es", ",", "ihm", "ge\u00b7sch\u00e4\u00b7he", "recht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hierunten ist der Weg nicht schlecht,", "tokens": ["Hier\u00b7un\u00b7ten", "ist", "der", "Weg", "nicht", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Hierunten wo der Klee, und Gras und Blumen stehn.", "tokens": ["Hier\u00b7un\u00b7ten", "wo", "der", "Klee", ",", "und", "Gras", "und", "Blu\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "$,", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hier giebt es auserlesne Weide.", "tokens": ["Hier", "giebt", "es", "au\u00b7ser\u00b7les\u00b7ne", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Kehr denn hieher auf diese Heide.", "tokens": ["Kehr", "denn", "hie\u00b7her", "auf", "die\u00b7se", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PAV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Verla\u00df die Felsen ohne Reue,", "tokens": ["Ver\u00b7la\u00df", "die", "Fel\u00b7sen", "oh\u00b7ne", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und komm herab auf meine Treue.", "tokens": ["Und", "komm", "her\u00b7ab", "auf", "mei\u00b7ne", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Der Gei\u00dfbock ward in seinen Sinnen,", "tokens": ["Der", "Gei\u00df\u00b7bock", "ward", "in", "sei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Des L\u00f6wens b\u00f6sen Willen innen.", "tokens": ["Des", "L\u00f6\u00b7wens", "b\u00f6\u00b7sen", "Wil\u00b7len", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ich weis, sprach er, du sagest recht:", "tokens": ["Ich", "weis", ",", "sprach", "er", ",", "du", "sa\u00b7gest", "recht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Das Wort ist gut, der Wille schlecht.", "tokens": ["Das", "Wort", "ist", "gut", ",", "der", "Wil\u00b7le", "schlecht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Dein Hertz ist b\u00f6s, dein Rath ist gut.", "tokens": ["Dein", "Hertz", "ist", "b\u00f6s", ",", "dein", "Rath", "ist", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "H\u00e4tt ich darunten meine Hut", "tokens": ["H\u00e4tt", "ich", "da\u00b7run\u00b7ten", "mei\u00b7ne", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "So gut als hier, ich n\u00e4hme bald", "tokens": ["So", "gut", "als", "hier", ",", "ich", "n\u00e4h\u00b7me", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dort meinen bessern Aufenthalt.", "tokens": ["Dort", "mei\u00b7nen", "bes\u00b7sern", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Nun mag ich dort nicht sicher ruhn,", "tokens": ["Nun", "mag", "ich", "dort", "nicht", "si\u00b7cher", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Drum kan ich dir nicht Folge thun.", "tokens": ["Drum", "kan", "ich", "dir", "nicht", "Fol\u00b7ge", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}