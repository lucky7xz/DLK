{"textgrid.poem.56477": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lider \u00fcbereist, das Ohr verstopft", "tokens": ["Die", "Li\u00b7der", "\u00fc\u00b7be\u00b7reist", ",", "das", "Ohr", "ver\u00b7stopft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Staub der Jahre, ruht ihr eure Zeit.", "tokens": ["Vom", "Staub", "der", "Jah\u00b7re", ",", "ruht", "ihr", "eu\u00b7re", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur manchmal ruft euch noch ein Traum, der klopft", "tokens": ["Nur", "manch\u00b7mal", "ruft", "euch", "noch", "ein", "Traum", ",", "der", "klopft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von fern an eure tote Ewigkeit,", "tokens": ["Von", "fern", "an", "eu\u00b7re", "to\u00b7te", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In einem Himmel, der wie Schnee so fahl", "tokens": ["In", "ei\u00b7nem", "Him\u00b7mel", ",", "der", "wie", "Schnee", "so", "fahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "KOKOM", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und von dem Zug der Jahre schon versteint.", "tokens": ["Und", "von", "dem", "Zug", "der", "Jah\u00b7re", "schon", "ver\u00b7steint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf eurem eingefallenen Totenmal", "tokens": ["Auf", "eu\u00b7rem", "ein\u00b7ge\u00b7fal\u00b7le\u00b7nen", "To\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wird eine Lilie stehn, die euch beweint.", "tokens": ["Wird", "ei\u00b7ne", "Li\u00b7lie", "stehn", ",", "die", "euch", "be\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der M\u00e4rznacht Sturm wird euren Schlaf betaun.", "tokens": ["Der", "M\u00e4rz\u00b7nacht", "Sturm", "wird", "eu\u00b7ren", "Schlaf", "be\u00b7taun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der gro\u00dfe Mond, der in dem Osten dampft,", "tokens": ["Der", "gro\u00b7\u00dfe", "Mond", ",", "der", "in", "dem", "Os\u00b7ten", "dampft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir