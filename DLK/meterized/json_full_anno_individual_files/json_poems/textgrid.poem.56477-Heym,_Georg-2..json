"token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Des goldenen Tages Br\u00fccke spannt sich weit", "tokens": ["Des", "gol\u00b7de\u00b7nen", "Ta\u00b7ges", "Br\u00fc\u00b7cke", "spannt", "sich", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und t\u00f6nt wie einer gro\u00dfen Leier Ton.", "tokens": ["Und", "t\u00f6nt", "wie", "ei\u00b7ner", "gro\u00b7\u00dfen", "Lei\u00b7er", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Pappeln rauschen mit dem Trauerkleid", "tokens": ["Die", "Pap\u00b7peln", "rau\u00b7schen", "mit", "dem", "Trau\u00b7er\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Stra\u00dfe fort, wo weit der Abend schon", "tokens": ["Die", "Stra\u00b7\u00dfe", "fort", ",", "wo", "weit", "der", "A\u00b7bend", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit Silberb\u00e4chen \u00fcberschwemmt das Land,", "tokens": ["Mit", "Sil\u00b7ber\u00b7b\u00e4\u00b7chen", "\u00fc\u00b7bersc\u00b7hwemmt", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und grenzenlos die ferne Weite brennt.", "tokens": ["Und", "gren\u00b7zen\u00b7los", "die", "fer\u00b7ne", "Wei\u00b7te", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die D\u00e4mmerung steigt wie ein dunkler Brand", "tokens": ["Die", "D\u00e4m\u00b7me\u00b7rung", "steigt", "wie", "ein", "dunk\u00b7ler", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Zug entlang, der in die Himmel rennt.", "tokens": ["Den", "Zug", "ent\u00b7lang", ",", "der", "in", "die", "Him\u00b7mel", "rennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Totenhain, und Lorbeer, Baum an Baum,", "tokens": ["Ein", "To\u00b7ten\u00b7hain", ",", "und", "Lor\u00b7beer", ",", "Baum", "an", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "$,", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie gr\u00fcne Flammen, die der Wind bewegt.", "tokens": ["Wie", "gr\u00fc\u00b7ne", "Flam\u00b7men", ",", "die", "der", "Wind", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie flackern riesig in den Himmelsraum,", "tokens": ["Sie", "fla\u00b7ckern", "rie\u00b7sig", "in", "den", "Him\u00b7mels\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo schon ein blasser Stern die Fl\u00fcgel schl\u00e4gt.", "tokens": ["Wo", "schon", "ein", "blas\u00b7ser", "Stern", "die", "Fl\u00fc\u00b7gel", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wie gro\u00dfe G\u00e4nse auf dem S\u00e4ulenschaft", "tokens": ["Wie", "gro\u00b7\u00dfe", "G\u00e4n\u00b7se", "auf", "dem", "S\u00e4u\u00b7len\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sitzt der Vampire Volk und friert im Frost.", "tokens": ["Sitzt", "der", "Vam\u00b7pi\u00b7re", "Volk", "und", "friert", "im", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sie pr\u00fcfen ihrer Eisenkrallen Kraft", "tokens": ["Sie", "pr\u00fc\u00b7fen", "ih\u00b7rer", "Ei\u00b7sen\u00b7kral\u00b7len", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihre Schn\u00e4bel an der Kreuze Rost.", "tokens": ["Und", "ih\u00b7re", "Schn\u00e4\u00b7bel", "an", "der", "Kreu\u00b7ze", "Rost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "