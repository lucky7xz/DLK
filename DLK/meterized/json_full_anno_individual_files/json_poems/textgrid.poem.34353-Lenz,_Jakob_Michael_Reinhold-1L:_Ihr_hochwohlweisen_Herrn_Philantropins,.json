{"textgrid.poem.34353": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr hochwohlweisen Herrn Philantropins,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr hochwohlweisen Herrn Philantropins,", "tokens": ["Ihr", "hoch\u00b7wohl\u00b7wei\u00b7sen", "Herrn", "Phil\u00b7an\u00b7tro\u00b7pins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Knien bitt ich, h\u00f6rt die Thr\u00e4nen Aretins;", "tokens": ["Auf", "Kni\u00b7en", "bitt", "ich", ",", "h\u00f6rt", "die", "Thr\u00e4\u00b7nen", "A\u00b7ret\u00b7ins", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Proben eurer Lieb auf meinem R\u00fccken,", "tokens": ["Die", "Pro\u00b7ben", "eu\u00b7rer", "Lieb", "auf", "mei\u00b7nem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzeiht, sie k\u00f6nnen nicht mein Naturell ersticken.", "tokens": ["Ver\u00b7zeiht", ",", "sie", "k\u00f6n\u00b7nen", "nicht", "mein", "Na\u00b7tu\u00b7rell", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ich bitte um ein Wort und sag ich mehr,", "tokens": ["Ich", "bit\u00b7te", "um", "ein", "Wort", "und", "sag", "ich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So l\u00e4chelt eine Welt von Pr\u00fcgeln auf mich her.", "tokens": ["So", "l\u00e4\u00b7chelt", "ei\u00b7ne", "Welt", "von", "Pr\u00fc\u00b7geln", "auf", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bei dem was ihr verspracht aus unserm Volk zu machen,", "tokens": ["Bei", "dem", "was", "ihr", "ver\u00b7spracht", "aus", "un\u00b7serm", "Volk", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Fieng mir das Herz im Leibe an zu lachen,", "tokens": ["Fi\u00b7eng", "mir", "das", "Herz", "im", "Lei\u00b7be", "an", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Der Othem stund mir still, das Wasser lief", "tokens": ["Der", "O\u00b7them", "stund", "mir", "still", ",", "das", "Was\u00b7ser", "lief"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mir aus dem Maule Ellentief;", "tokens": ["Mir", "aus", "dem", "Mau\u00b7le", "El\u00b7len\u00b7tief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Doch als ich so dem Ding ein wenig nachgedacht,", "tokens": ["Doch", "als", "ich", "so", "dem", "Ding", "ein", "we\u00b7nig", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da ward, verzeih mirs Gott, das Herz mir schwer gemacht:", "tokens": ["Da", "ward", ",", "ver\u00b7zeih", "mirs", "Gott", ",", "das", "Herz", "mir", "schwer", "ge\u00b7macht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "VVIMP", "NE", "NN", "$,", "ART", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das werden K\u00f6pfe nur ihr lieben Herrn! auf Erden", "tokens": ["Das", "wer\u00b7den", "K\u00f6p\u00b7fe", "nur", "ihr", "lie\u00b7ben", "Herrn", "!", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ach lauter Dratmaschienen werden!", "tokens": ["Ach", "lau\u00b7ter", "Drat\u00b7ma\u00b7schie\u00b7nen", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Das reitet, ficht und tanzt nach euren Winken,", "tokens": ["Das", "rei\u00b7tet", ",", "ficht", "und", "tanzt", "nach", "eu\u00b7ren", "Win\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Darf weder essen, weder trinken", "tokens": ["Darf", "we\u00b7der", "es\u00b7sen", ",", "we\u00b7der", "trin\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "KON", "VVINF", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Noch schlafen, noch verdaun, als zur gesetzten Zeit", "tokens": ["Noch", "schla\u00b7fen", ",", "noch", "ver\u00b7daun", ",", "als", "zur", "ge\u00b7setz\u00b7ten", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und kackt sogar mit Sittsamkeit.", "tokens": ["Und", "kackt", "so\u00b7gar", "mit", "Sitt\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ihr Heilande der Welt habts nie erfahren,", "tokens": ["Ihr", "Hei\u00b7lan\u00b7de", "der", "Welt", "habts", "nie", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Da\u00df grosse Tugenden nie ohne Laster waren,", "tokens": ["Da\u00df", "gros\u00b7se", "Tu\u00b7gen\u00b7den", "nie", "oh\u00b7ne", "Las\u00b7ter", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df \u00e4u\u00dferlich Gepr\u00e4g nur sch\u00f6ngeschminkter Mist,", "tokens": ["Da\u00df", "\u00e4u\u00b7\u00dfer\u00b7lich", "Ge\u00b7pr\u00e4g", "nur", "sch\u00f6n\u00b7ge\u00b7schmink\u00b7ter", "Mist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df Schlaffigkeit das gr\u00f6\u00dfte Laster ist.", "tokens": ["Da\u00df", "Schlaf\u00b7fig\u00b7keit", "das", "gr\u00f6\u00df\u00b7te", "Las\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Da\u00df unsrer innrer Trieb das Beste aus uns machet", "tokens": ["Da\u00df", "uns\u00b7rer", "inn\u00b7rer", "Trieb", "das", "Bes\u00b7te", "aus", "uns", "ma\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und eurer siechen Kunst der Gott im Menschen lachet.", "tokens": ["Und", "eu\u00b7rer", "sie\u00b7chen", "Kunst", "der", "Gott", "im", "Men\u00b7schen", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das g\u00e4be Puppchen nur nach eurem Bild geschnitzt,", "tokens": ["Das", "g\u00e4\u00b7be", "Pupp\u00b7chen", "nur", "nach", "eu\u00b7rem", "Bild", "ge\u00b7schnitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit sch\u00f6nfrisirtem Haar, wo nichts darunter sitzt,", "tokens": ["Mit", "sch\u00f6nf\u00b7ri\u00b7sir\u00b7tem", "Haar", ",", "wo", "nichts", "da\u00b7run\u00b7ter", "sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mit nimmer ruhigen, verw\u00fcnschten Plapperm\u00fchlen", "tokens": ["Mit", "nim\u00b7mer", "ru\u00b7hi\u00b7gen", ",", "ver\u00b7w\u00fcnschten", "Plap\u00b7per\u00b7m\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Die noch f\u00fcr Gott, noch Welt, noch f\u00fcr sich selber f\u00fchlen.", "tokens": ["Die", "noch", "f\u00fcr", "Gott", ",", "noch", "Welt", ",", "noch", "f\u00fcr", "sich", "sel\u00b7ber", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,", "ADV", "NN", "$,", "ADV", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Lehrt ihnen was daf\u00fcr und dann schickt sie nach Haus,", "tokens": ["Lehrt", "ih\u00b7nen", "was", "da\u00b7f\u00fcr", "und", "dann", "schickt", "sie", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PAV", "KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "So werde was da will, nur nicht ein Affe draus.", "tokens": ["So", "wer\u00b7de", "was", "da", "will", ",", "nur", "nicht", "ein", "Af\u00b7fe", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PWS", "ADV", "VMFIN", "$,", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr hochwohlweisen Herrn Philantropins,", "tokens": ["Ihr", "hoch\u00b7wohl\u00b7wei\u00b7sen", "Herrn", "Phil\u00b7an\u00b7tro\u00b7pins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Knien bitt ich, h\u00f6rt die Thr\u00e4nen Aretins;", "tokens": ["Auf", "Kni\u00b7en", "bitt", "ich", ",", "h\u00f6rt", "die", "Thr\u00e4\u00b7nen", "A\u00b7ret\u00b7ins", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Proben eurer Lieb auf meinem R\u00fccken,", "tokens": ["Die", "Pro\u00b7ben", "eu\u00b7rer", "Lieb", "auf", "mei\u00b7nem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzeiht, sie k\u00f6nnen nicht mein Naturell ersticken.", "tokens": ["Ver\u00b7zeiht", ",", "sie", "k\u00f6n\u00b7nen", "nicht", "mein", "Na\u00b7tu\u00b7rell", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", 