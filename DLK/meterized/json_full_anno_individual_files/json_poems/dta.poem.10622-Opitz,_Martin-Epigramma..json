{"dta.poem.10622": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Es streitte wer da will/ ich bleibe doch darbey/", "tokens": ["Es", "streit\u00b7te", "wer", "da", "will", "/", "ich", "blei\u00b7be", "doch", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADV", "VMFIN", "$(", "PPER", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df der Poeten Printz Homerus blindt gewesen/", "tokens": ["Da\u00df", "der", "Po\u00b7et\u00b7en", "Printz", "Ho\u00b7me\u00b7rus", "blindt", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "NE", "VVFIN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer hat von Venus nicht in seinem Buch gelesen/", "tokens": ["Wer", "hat", "von", "Ve\u00b7nus", "nicht", "in", "sei\u00b7nem", "Buch", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NE", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df jhr von Diomed die Handt verwundet sey?", "tokens": ["Da\u00df", "jhr", "von", "Dio\u00b7med", "die", "Handt", "ver\u00b7wun\u00b7det", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Er hette ja viel mehr sie sonsten treffen sollen/", "tokens": ["Er", "het\u00b7te", "ja", "viel", "mehr", "sie", "sons\u00b7ten", "tref\u00b7fen", "sol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd nicht die zarte Handt/ so gantz vnschuldig war/", "tokens": ["Vnd", "nicht", "die", "zar\u00b7te", "Handt", "/", "so", "gantz", "vn\u00b7schul\u00b7dig", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$(", "ADV", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er hett\u2019 auch ohne Wehr ged\u00fcrfft in die Gefahr/", "tokens": ["Er", "hett'", "auch", "oh\u00b7ne", "Wehr", "ge\u00b7d\u00fcrfft", "in", "die", "Ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Homerus ist doch blindt/ sie sagen was sie wollen.", "tokens": ["Ho\u00b7me\u00b7rus", "ist", "doch", "blindt", "/", "sie", "sa\u00b7gen", "was", "sie", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVFIN", "$(", "PPER", "VVINF", "PWS", "PPER", "VMFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}