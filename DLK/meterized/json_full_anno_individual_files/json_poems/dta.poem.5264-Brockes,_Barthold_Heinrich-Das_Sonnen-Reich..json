{"dta.poem.5264": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Sonnen-Reich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus der duncklen Erde steigen Pflantzen in die d\u00fcn-\nne Luft:", "tokens": ["Aus", "der", "dunck\u00b7len", "Er\u00b7de", "stei\u00b7gen", "Pflant\u00b7zen", "in", "die", "d\u00fcn", "ne", "Luft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "APPR", "ART", "TRUNC", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Warum solten unsre Seelen nicht, aus irdscher\nL\u00fcfte Duft,", "tokens": ["Wa\u00b7rum", "sol\u00b7ten", "uns\u00b7re", "See\u00b7len", "nicht", ",", "aus", "ird\u00b7scher", "L\u00fcf\u00b7te", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "PTKNEG", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "In die Himmels L\u00fcfte steigen, um in ihren reinen\nH\u00f6h'n,", "tokens": ["In", "die", "Him\u00b7mels", "L\u00fcf\u00b7te", "stei\u00b7gen", ",", "um", "in", "ih\u00b7ren", "rei\u00b7nen", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,", "KOUI", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Durch ein reines Licht erleuchtet, GOttes Werck\nauch dort zu sehn?", "tokens": ["Durch", "ein", "rei\u00b7nes", "Licht", "er\u00b7leuch\u00b7tet", ",", "Got\u00b7tes", "Werck", "auch", "dort", "zu", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "NN", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Mit nicht auszudr\u00fcckender, inniger, und sanfter\nFreude", "tokens": ["Mit", "nicht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken\u00b7der", ",", "in\u00b7ni\u00b7ger", ",", "und", "sanf\u00b7ter", "Freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PTKNEG", "PTKVZ", "$,", "ADJD", "$,", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Find\u2019 ich auf den Garten Beeten jetzt, zur fr\u00fchen Fr\u00fch-", "tokens": ["Find'", "ich", "auf", "den", "Gar\u00b7ten", "Bee\u00b7ten", "jetzt", ",", "zur", "fr\u00fc\u00b7hen", "Fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVFIN", "ADV", "$,", "APPRART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Eine recht bewundrungs-wehrte Hertzen-Seel-und Augen-", "tokens": ["Ei\u00b7ne", "recht", "be\u00b7wun\u00b7drungs\u00b7wehr\u00b7te", "Hert\u00b7zen\u00b7Seel\u00b7\u00b7und", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "So mein gantzes Wesen f\u00fcllet mit besondrer Lieblichkeit.", "tokens": ["So", "mein", "gant\u00b7zes", "We\u00b7sen", "f\u00fcl\u00b7let", "mit", "be\u00b7sond\u00b7rer", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Die noch gestern gantze Beeten sind anjetzt schon voller", "tokens": ["Die", "noch", "ge\u00b7stern", "gant\u00b7ze", "Bee\u00b7ten", "sind", "an\u00b7jetzt", "schon", "vol\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADJA"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Voller ofnen luckern Stellen. Seht, wie kleine gr\u00fcne Spitzen,", "tokens": ["Vol\u00b7ler", "of\u00b7nen", "lu\u00b7ckern", "Stel\u00b7len", ".", "Seht", ",", "wie", "klei\u00b7ne", "gr\u00fc\u00b7ne", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$.", "VVFIN", "$,", "PWAV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Zwischen kleinen Erden-Kl\u00f6ssen, aus den duncklen Spal-", "tokens": ["Zwi\u00b7schen", "klei\u00b7nen", "Er\u00b7den\u00b7K\u00b7l\u00f6s\u00b7sen", ",", "aus", "den", "dunck\u00b7len", "Spa\u00b7l"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Seht wie sie nicht hie und da, sondern \u00fcberall, entspringen.", "tokens": ["Seht", "wie", "sie", "nicht", "hie", "und", "da", ",", "son\u00b7dern", "\u00fc\u00b7be\u00b7rall", ",", "ent\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "PTKNEG", "ADV", "KON", "ADV", "$,", "KON", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Ein vern\u00fcnftig Menschen-Auge kann nicht sonder Anmuht", "tokens": ["Ein", "ver\u00b7n\u00fcnf\u00b7tig", "Men\u00b7schen\u00b7Au\u00b7ge", "kann", "nicht", "son\u00b7der", "An\u00b7muht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VMFIN", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "In so s\u00fcsser Meng und Ordnung kleine gr\u00fcne Pfriemen", "tokens": ["In", "so", "s\u00fcs\u00b7ser", "Meng", "und", "Ord\u00b7nung", "klei\u00b7ne", "gr\u00fc\u00b7ne", "Pfrie\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Die aus ihrer duncklen Wohnung mit so strengem Druck", "tokens": ["Die", "aus", "ih\u00b7rer", "dunck\u00b7len", "Woh\u00b7nung", "mit", "so", "stren\u00b7gem", "Druck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Da\u00df noch \u00f6fters an den Spitzen kleine Erden-Kl\u00f6sse kle-", "tokens": ["Da\u00df", "noch", "\u00f6f\u00b7ters", "an", "den", "Spit\u00b7zen", "klei\u00b7ne", "Er\u00b7den\u00b7K\u00b7l\u00f6s\u00b7se", "kle"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}