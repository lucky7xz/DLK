{"dta.poem.8456": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Siegeslied bei Eroberung des  \n heiligen Grabes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Erl\u00f6ser, an dein Grab,", "tokens": ["Er\u00b7l\u00f6\u00b7ser", ",", "an", "dein", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und t\u00f6nen hohe Jubel dir,", "tokens": ["Und", "t\u00f6\u00b7nen", "ho\u00b7he", "Ju\u00b7bel", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schauen froh hinab.", "tokens": ["Und", "schau\u00b7en", "froh", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Beschattet von dem Felsgestr\u00e4uch,", "tokens": ["Be\u00b7schat\u00b7tet", "von", "dem", "Fels\u00b7ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umtanzen wir die Gruft,", "tokens": ["Um\u00b7tan\u00b7zen", "wir", "die", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und streuen manchen Palmenzweig", "tokens": ["Und", "streu\u00b7en", "man\u00b7chen", "Pal\u00b7men\u00b7zweig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frohlockend durch die Luft.", "tokens": ["Froh\u00b7lo\u00b7ckend", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dein Vater sah von seinem Thron", "tokens": ["Dein", "Va\u00b7ter", "sah", "von", "sei\u00b7nem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herab auf unsre Schlacht;", "tokens": ["Her\u00b7ab", "auf", "uns\u00b7re", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle Sarazenen flohn,", "tokens": ["Und", "al\u00b7le", "Sa\u00b7ra\u00b7ze\u00b7nen", "flohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlten Gottes Macht.", "tokens": ["Und", "f\u00fchl\u00b7ten", "Got\u00b7tes", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Kison rieselt purpurhell", "tokens": ["Der", "Ki\u00b7son", "rie\u00b7selt", "pur\u00b7pur\u00b7hell"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Sarazenenmord,", "tokens": ["Vom", "Sa\u00b7ra\u00b7ze\u00b7nen\u00b7mord", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und blutig wallt Siloas Quell", "tokens": ["Und", "blu\u00b7tig", "wallt", "Si\u00b7loas", "Quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "NE", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Durch seine Binsen fort.", "tokens": ["Durch", "sei\u00b7ne", "Bin\u00b7sen", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl uns! Die Siegerfahne tanzt", "tokens": ["Wohl", "uns", "!", "Die", "Sie\u00b7ger\u00b7fah\u00b7ne", "tanzt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Golgatha herab,", "tokens": ["Vom", "Gol\u00b7ga\u00b7tha", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rauscht, auf einen Fels gepflanzt,", "tokens": ["Und", "rauscht", ",", "auf", "ei\u00b7nen", "Fels", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcber Jesus Grab.", "tokens": ["Hoch", "\u00fc\u00b7ber", "Je\u00b7sus", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Engel trat, in Feur geh\u00fcllt,", "tokens": ["Ein", "En\u00b7gel", "trat", ",", "in", "Feur", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsre Vorderreihn;", "tokens": ["In", "uns\u00b7re", "Vor\u00b7der\u00b7reihn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Schwert, das seine Rechte f\u00fcllt,", "tokens": ["Das", "Schwert", ",", "das", "sei\u00b7ne", "Rech\u00b7te", "f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blinkt' auf den Feind hinein;", "tokens": ["Blinkt'", "auf", "den", "Feind", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und eine Purpurfahne flog,", "tokens": ["Und", "ei\u00b7ne", "Pur\u00b7pur\u00b7fah\u00b7ne", "flog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Gottes Lichtgewand,", "tokens": ["Wie", "Got\u00b7tes", "Licht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald niedrig und bald wieder hoch,", "tokens": ["Bald", "nied\u00b7rig", "und", "bald", "wie\u00b7der", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner linken Hand.", "tokens": ["In", "sei\u00b7ner", "lin\u00b7ken", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mit seiner Purpurfahne Wehn", "tokens": ["Mit", "sei\u00b7ner", "Pur\u00b7pur\u00b7fah\u00b7ne", "Wehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam Sieg auf unser Heer;", "tokens": ["Kam", "Sieg", "auf", "un\u00b7ser", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Feind, kaum hatt' er ihn gesehn,", "tokens": ["Dem", "Feind", ",", "kaum", "hatt'", "er", "ihn", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entbebte Schwert und Speer.", "tokens": ["Ent\u00b7beb\u00b7te", "Schwert", "und", "Speer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Vom todeskalten Gottesschaur", "tokens": ["Vom", "to\u00b7des\u00b7kal\u00b7ten", "Got\u00b7tes\u00b7schaur"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward er hinweggeweht,", "tokens": ["Ward", "er", "hin\u00b7weg\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unsre Fahn' auf deine Maur", "tokens": ["Und", "uns\u00b7re", "Fahn'", "auf", "dei\u00b7ne", "Maur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jerusalem, erh\u00f6ht.", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", ",", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Von unsern Schultern blinkt das Kreuz,", "tokens": ["Von", "un\u00b7sern", "Schul\u00b7tern", "blinkt", "das", "Kreuz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von unsern Fahnen blinkts;", "tokens": ["Von", "un\u00b7sern", "Fah\u00b7nen", "blinkts", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Christenunterjocher scheuts;", "tokens": ["Der", "Chris\u00b7ten\u00b7un\u00b7ter\u00b7jo\u00b7cher", "scheuts", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wo es weht, da sinkts.", "tokens": ["Und", "wo", "es", "weht", ",", "da", "sinkts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da ist, hebt das Gemezel an,", "tokens": ["Da", "ist", ",", "hebt", "das", "Ge\u00b7me\u00b7zel", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Knabe selber Held;", "tokens": ["Der", "Kna\u00b7be", "sel\u00b7ber", "Held", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da blizen wir den Muselmann", "tokens": ["Da", "bli\u00b7zen", "wir", "den", "Mu\u00b7sel\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Zur\u00fcck vom Waffenfeld.", "tokens": ["Zu\u00b7r\u00fcck", "vom", "Waf\u00b7fen\u00b7feld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Befl\u00fcgle f\u00fcrder unsre Wehr", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "f\u00fcr\u00b7der", "uns\u00b7re", "Wehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinem R\u00e4cherbliz,", "tokens": ["Mit", "dei\u00b7nem", "R\u00e4\u00b7cher\u00b7bliz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und donnre dieser M\u00f6rder Heer", "tokens": ["Und", "donn\u00b7re", "die\u00b7ser", "M\u00f6r\u00b7der", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus deinem Lieblingssiz.", "tokens": ["Aus", "dei\u00b7nem", "Lieb\u00b7lings\u00b7siz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Flieg' immer, helles Kreuzpanier,", "tokens": ["Flieg'", "im\u00b7mer", ",", "hel\u00b7les", "Kreuz\u00b7pa\u00b7nier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den frommen Christen vor,", "tokens": ["Den", "from\u00b7men", "Chris\u00b7ten", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rausch' in Salem f\u00fcr und f\u00fcr", "tokens": ["Und", "rausch'", "in", "Sa\u00b7lem", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jehovas Lob empor.", "tokens": ["Je\u00b7ho\u00b7vas", "Lob", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}