{"textgrid.poem.42961": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Humorvolle Spinner", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spinnete K\u00f6pfe, gescheit und begabt,", "tokens": ["Spin\u00b7ne\u00b7te", "K\u00f6p\u00b7fe", ",", "ge\u00b7scheit", "und", "be\u00b7gabt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Weil ihr einen Pieps, einen Vogel habt,", "tokens": ["Weil", "ihr", "ei\u00b7nen", "Pieps", ",", "ei\u00b7nen", "Vo\u00b7gel", "habt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "NE", "VAFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Verlachen euch manche und meiden", "tokens": ["Ver\u00b7la\u00b7chen", "euch", "man\u00b7che", "und", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Euch. Ich mag euch leiden.", "tokens": ["Euch", ".", "Ich", "mag", "euch", "lei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Piepvogel lebt so hoch und frei", "tokens": ["Ein", "Piep\u00b7vo\u00b7gel", "lebt", "so", "hoch", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00dcber den Filzlatschen der Spie\u00dfer.", "tokens": ["\u00dc\u00b7ber", "den", "Filz\u00b7lat\u00b7schen", "der", "Spie\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Der Spie\u00dfer meint: Ein Bandwurm sei", "tokens": ["Der", "Spie\u00b7\u00dfer", "meint", ":", "Ein", "Band\u00b7wurm", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein stiller Genie\u00dfer.", "tokens": ["Kein", "stil\u00b7ler", "Ge\u00b7nie\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch Spie\u00dfermeinung ist nicht mal so wichtig", "tokens": ["Doch", "Spie\u00b7\u00dfer\u00b7mei\u00b7nung", "ist", "nicht", "mal", "so", "wich\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PTKNEG", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie das, was aus Piepvogel f\u00e4llt.", "tokens": ["Wie", "das", ",", "was", "aus", "Piep\u00b7vo\u00b7gel", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Nur der, der im Kopf nicht ganz richtig", "tokens": ["Nur", "der", ",", "der", "im", "Kopf", "nicht", "ganz", "rich\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "APPRART", "NN", "PTKNEG", "ADV", "ADJD"], "meter": "-+--+