{"dta.poem.9213": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Xi  \n Das unempfindliche Hertze.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wolte mich gerne verlieben/", "tokens": ["Ich", "wol\u00b7te", "mich", "ger\u00b7ne", "ver\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "F\u00fcrwahr ich kan nur nicht/", "tokens": ["F\u00fcr\u00b7wahr", "ich", "kan", "nur", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "ADV", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der k\u00fctzel ist lange vertrieben", "tokens": ["Der", "k\u00fct\u00b7zel", "ist", "lan\u00b7ge", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der junge leute sticht/", "tokens": ["Der", "jun\u00b7ge", "leu\u00b7te", "sticht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich seh die m\u00e4dgen an/", "tokens": ["Ich", "seh", "die", "m\u00e4d\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Als wie ein alter mann.", "tokens": ["Als", "wie", "ein", "al\u00b7ter", "mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Die pfeile der lieblichen blicke", "tokens": ["Die", "pfei\u00b7le", "der", "lieb\u00b7li\u00b7chen", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sind viel zu stumpff vor mich/", "tokens": ["Sind", "viel", "zu", "stumpff", "vor", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die reden die niedlichen stricke/", "tokens": ["Die", "re\u00b7den", "die", "nied\u00b7li\u00b7chen", "stri\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ziehn ab und sch\u00e4men sich/", "tokens": ["Ziehn", "ab", "und", "sch\u00e4\u00b7men", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PRF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil ihre leise krafft", "tokens": ["Weil", "ih\u00b7re", "lei\u00b7se", "krafft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So wenig nutzen schafft", "tokens": ["So", "we\u00b7nig", "nut\u00b7zen", "schafft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVINF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Die rosen der zierlichen wangen/", "tokens": ["Die", "ro\u00b7sen", "der", "zier\u00b7li\u00b7chen", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die lippen von rubin/", "tokens": ["Die", "lip\u00b7pen", "von", "ru\u00b7bin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die geben sich selber gefangen/", "tokens": ["Die", "ge\u00b7ben", "sich", "sel\u00b7ber", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Je mehr sie sich bem\u00fchn/", "tokens": ["Je", "mehr", "sie", "sich", "be\u00b7m\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Je mehr ist ihre last", "tokens": ["Je", "mehr", "ist", "ih\u00b7re", "last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mir eitel und verhasst.", "tokens": ["Mir", "ei\u00b7tel", "und", "ver\u00b7hasst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Die reihen der artigen h\u00e4nde/", "tokens": ["Die", "rei\u00b7hen", "der", "ar\u00b7ti\u00b7gen", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die alabaster haut/", "tokens": ["Die", "a\u00b7la\u00b7bas\u00b7ter", "haut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da mancher verliebter kein ende", "tokens": ["Da", "man\u00b7cher", "ver\u00b7lieb\u00b7ter", "kein", "en\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zu seiner wollust schaut/", "tokens": ["Zu", "sei\u00b7ner", "wol\u00b7lust", "schaut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die selben kommen mir", "tokens": ["Die", "sel\u00b7ben", "kom\u00b7men", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Todt und verdorben f\u00fcr.", "tokens": ["Todt", "und", "ver\u00b7dor\u00b7ben", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "APPR", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "5. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Die wunder-sch\u00f6ne brust/", "tokens": ["Die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "brust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag andern personen gefallen/", "tokens": ["Mag", "an\u00b7dern", "per\u00b7so\u00b7nen", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich hasse diese lust/", "tokens": ["Ich", "has\u00b7se", "die\u00b7se", "lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und die gelegenheit", "tokens": ["Und", "die", "ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Zu solcher eitelkeit.", "tokens": ["Zu", "sol\u00b7cher", "ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "6. Sie m\u00f6gen ihr angesicht weiden/", "tokens": ["Sie", "m\u00f6\u00b7gen", "ihr", "an\u00b7ge\u00b7sicht", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVPP", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Noch hundertmahl so sch\u00f6n/", "tokens": ["Noch", "hun\u00b7dert\u00b7mahl", "so", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie m\u00f6gen sich butzen und kleiden", "tokens": ["Sie", "m\u00f6\u00b7gen", "sich", "but\u00b7zen", "und", "klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KON", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und vor den spiegel stehn/", "tokens": ["Und", "vor", "den", "spie\u00b7gel", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun mehr so bleib ich schon", "tokens": ["Nun", "mehr", "so", "bleib", "ich", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Am liebsten weit darvon.", "tokens": ["Am", "liebs\u00b7ten", "weit", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "7. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Blickt mich nicht weiter an/", "tokens": ["Blickt", "mich", "nicht", "wei\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mein hertze sey zwischen uns richter/", "tokens": ["Mein", "hert\u00b7ze", "sey", "zwi\u00b7schen", "uns", "rich\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich nicht liebeu kan/", "tokens": ["Da\u00df", "ich", "nicht", "lie\u00b7beu", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bem\u00fcht euch anderswo/", "tokens": ["Be\u00b7m\u00fcht", "euch", "an\u00b7ders\u00b7wo", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich bleibe lieber so.", "tokens": ["Ich", "blei\u00b7be", "lie\u00b7ber", "so", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}