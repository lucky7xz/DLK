{"textgrid.poem.60066": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das letzte Roth am Himmel wich:", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das letzte Roth am Himmel wich:", "tokens": ["Das", "letz\u00b7te", "Roth", "am", "Him\u00b7mel", "wich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ging ich, liebevoll, im Gr\u00fcnen;", "tokens": ["Da", "ging", "ich", ",", "lie\u00b7be\u00b7voll", ",", "im", "Gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich ging und lobte Gott f\u00fcr dich,", "tokens": ["Ich", "ging", "und", "lob\u00b7te", "Gott", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr die Sternen, welche schienen.", "tokens": ["Und", "f\u00fcr", "die", "Ster\u00b7nen", ",", "wel\u00b7che", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und pl\u00f6tzlich kam ein Wolken-Heer,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "kam", "ein", "Wol\u00b7ken\u00b7Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ri\u00df hinweg die goldnen Sterne;", "tokens": ["Und", "ri\u00df", "hin\u00b7weg", "die", "gold\u00b7nen", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gelinde L\u00fcfte wurden schwer,", "tokens": ["Ge\u00b7lin\u00b7de", "L\u00fcf\u00b7te", "wur\u00b7den", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Donner rollten aus der Ferne.", "tokens": ["Und", "Don\u00b7ner", "roll\u00b7ten", "aus", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die St\u00fcrme heulten auf mich zu;", "tokens": ["Die", "St\u00fcr\u00b7me", "heul\u00b7ten", "auf", "mich", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Donner wollten mich erschrecken;", "tokens": ["Die", "Don\u00b7ner", "woll\u00b7ten", "mich", "er\u00b7schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich aber lie\u00df, in frommer Ruh,", "tokens": ["Ich", "a\u00b7ber", "lie\u00df", ",", "in", "from\u00b7mer", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}