{"dta.poem.23621": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "B\u00f6mische D\u00f6rfer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Pegasus der alte Schimmel", "tokens": ["Pe\u00b7ga\u00b7sus", "der", "al\u00b7te", "Schim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Apollo fehlet nie,", "tokens": ["Und", "A\u00b7pol\u00b7lo", "feh\u00b7let", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja der ganze G\u00f6tterhimmel", "tokens": ["Ja", "der", "gan\u00b7ze", "G\u00f6t\u00b7ter\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Prunkt in eurer Poesie.", "tokens": ["Prunkt", "in", "eu\u00b7rer", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mit dem W\u00f6rterbuche lesen", "tokens": ["Mit", "dem", "W\u00f6r\u00b7ter\u00b7bu\u00b7che", "le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df man jedes Maigedicht;", "tokens": ["Mu\u00df", "man", "je\u00b7des", "Mai\u00b7ge\u00b7dicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer die Cypris ist gewesen", "tokens": ["Wer", "die", "Cyp\u00b7ris", "ist", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NE", "VAFIN", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df ich armer Deutscher nicht.", "tokens": ["Wei\u00df", "ich", "ar\u00b7mer", "Deut\u00b7scher", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch Pandora, Flora, Iris,", "tokens": ["Auch", "Pan\u00b7do\u00b7ra", ",", "Flo\u00b7ra", ",", "I\u00b7ris", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NE", "$,", "NN", "$,", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Zeus, Aurora, Rhadamanth,", "tokens": ["Zeus", ",", "Au\u00b7ro\u00b7ra", ",", "Rha\u00b7da\u00b7man\u00b7th", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Midas, Isis und Osiris", "tokens": ["Mi\u00b7das", ",", "I\u00b7sis", "und", "O\u00b7si\u00b7ris"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "KON", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Sind mir g\u00e4nzlich unbekannt.", "tokens": ["Sind", "mir", "g\u00e4nz\u00b7lich", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sagt, f\u00fcr wen doch wollt ihr dichten?", "tokens": ["Sagt", ",", "f\u00fcr", "wen", "doch", "wollt", "ihr", "dich\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PWS", "ADV", "VMFIN", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr's gelehrte H\u00e4ufelein?", "tokens": ["F\u00fcr's", "ge\u00b7lehr\u00b7te", "H\u00e4u\u00b7fe\u00b7lein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, so m\u00fcsst ihr drauf verzichten,", "tokens": ["Nun", ",", "so", "m\u00fcsst", "ihr", "drauf", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutschlands Dichter je zu sein.", "tokens": ["Deutschlands", "Dich\u00b7ter", "je", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwar das deutsche Volk hat immer", "tokens": ["Zwar", "das", "deut\u00b7sche", "Volk", "hat", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen hochgelehrten Stand;", "tokens": ["Sei\u00b7nen", "hoch\u00b7ge\u00b7lehr\u00b7ten", "Stand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser aber hatte nimmer", "tokens": ["Die\u00b7ser", "a\u00b7ber", "hat\u00b7te", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Welt ein Vaterland.", "tokens": ["In", "der", "Welt", "ein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Besser drum, ihr singt und pfeifet", "tokens": ["Bes\u00b7ser", "drum", ",", "ihr", "singt", "und", "pfei\u00b7fet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PAV", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie's gem\u00e4\u00df dem deutschen Mund:", "tokens": ["Wie's", "ge\u00b7m\u00e4\u00df", "dem", "deut\u00b7schen", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Castor! Pollux! das begreifet", "tokens": ["Cas\u00b7tor", "!", "Pol\u00b7lux", "!", "das", "be\u00b7grei\u00b7fet"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "PDS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch sogar ein dummer Hund.", "tokens": ["Auch", "so\u00b7gar", "ein", "dum\u00b7mer", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}