{"dta.poem.478": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Da sie beichten wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sie dencket/ sch\u00f6nstes Kind/ den Himmel zu vers\u00f6hnen/", "tokens": ["Sie", "den\u00b7cket", "/", "sch\u00f6ns\u00b7tes", "Kind", "/", "den", "Him\u00b7mel", "zu", "ver\u00b7s\u00f6h\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durch Erniedrigung gerecht nach Haus\u2019 zu gehn", "tokens": ["Und", "durch", "Er\u00b7nied\u00b7ri\u00b7gung", "ge\u00b7recht", "nach", "Haus'", "zu", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJD", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach! sie wird aber nur der Himmels-Gunst verh\u00f6hnen", "tokens": ["Ach", "!", "sie", "wird", "a\u00b7ber", "nur", "der", "Him\u00b7mels\u00b7Gunst", "ver\u00b7h\u00f6h\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den dieser h\u00f6ret nicht/ was harte Hertzen flehn.", "tokens": ["Den", "die\u00b7ser", "h\u00f6\u00b7ret", "nicht", "/", "was", "har\u00b7te", "Hert\u00b7zen", "flehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVFIN", "PTKNEG", "$(", "PWS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}