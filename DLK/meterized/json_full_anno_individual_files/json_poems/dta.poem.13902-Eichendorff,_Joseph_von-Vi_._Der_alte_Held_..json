{"dta.poem.13902": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n  Der alte Held .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich habe gewagt und gesungen,", "tokens": ["\u201e", "ich", "ha\u00b7be", "ge\u00b7wagt", "und", "ge\u00b7sun\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da die Welt noch stumm lag und bleich,", "tokens": ["Da", "die", "Welt", "noch", "stumm", "lag", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "KON", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich habe den Bann bezwungen,", "tokens": ["Ich", "ha\u00b7be", "den", "Bann", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der die sch\u00f6ne Braut hielt umschlungen,", "tokens": ["Der", "die", "sch\u00f6\u00b7ne", "Braut", "hielt", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich habe erobert das Reich.\u201c", "tokens": ["Ich", "ha\u00b7be", "er\u00b7o\u00b7bert", "das", "Reich", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}