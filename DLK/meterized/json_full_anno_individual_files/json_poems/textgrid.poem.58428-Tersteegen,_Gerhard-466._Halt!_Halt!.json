{"textgrid.poem.58428": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "466. Halt! Halt!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie rennt der blinde Mensch, als w\u00e4r' er nicht bei Sinnen;", "tokens": ["Wie", "rennt", "der", "blin\u00b7de", "Mensch", ",", "als", "w\u00e4r'", "er", "nicht", "bei", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOKOM", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach, lauf so schnelle nicht, du l\u00e4ufst zur Ewigkeit,", "tokens": ["Ach", ",", "lauf", "so", "schnel\u00b7le", "nicht", ",", "du", "l\u00e4ufst", "zur", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denk, du kannst eine Seel' verlieren und gewinnen", "tokens": ["Denk", ",", "du", "kannst", "ei\u00b7ne", "Seel'", "ver\u00b7lie\u00b7ren", "und", "ge\u00b7win\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In diesem Augenblick der kurzen Gnadenzeit!", "tokens": ["In", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "der", "kur\u00b7zen", "Gna\u00b7den\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "