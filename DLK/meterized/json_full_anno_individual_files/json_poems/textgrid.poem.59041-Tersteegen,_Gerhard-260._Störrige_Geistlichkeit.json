{"textgrid.poem.59041": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "260. St\u00f6rrige Geistlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Innigsein ist zu gezwungen,", "tokens": ["Dein", "In\u00b7ni\u00b7gsein", "ist", "zu", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Liebe ist's nicht g'nug durchdrungen;", "tokens": ["Von", "Lie\u00b7be", "ist's", "nicht", "g'\u00b7nug", "durch\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sei abgeschieden, doch gelind,", "tokens": ["Sei", "ab\u00b7ge\u00b7schie\u00b7den", ",", "doch", "ge\u00b7lind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00fc\u00dfe wie das kleinste Kind!", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "wie", "das", "kleins\u00b7te", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Innigsein ist zu gezwungen,", "tokens": ["Dein", "In\u00b7ni\u00b7gsein", "ist", "zu", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Liebe ist's nich