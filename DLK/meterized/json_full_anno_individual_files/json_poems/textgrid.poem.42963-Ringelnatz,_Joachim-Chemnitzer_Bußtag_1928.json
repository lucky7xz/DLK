{"textgrid.poem.42963": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Chemnitzer Bu\u00dftag 1928", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich aber ging zum Tambour hin,", "tokens": ["Ich", "a\u00b7ber", "ging", "zum", "Tam\u00b7bour", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich nicht gern im Tr\u00fcben bin,", "tokens": ["Weil", "ich", "nicht", "gern", "im", "Tr\u00fc\u00b7ben", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weil im Tambour Lou verkehrt", "tokens": ["Und", "weil", "im", "Tam\u00b7bour", "Lou", "ver\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "NE", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und immer vieler M\u00e4nner harrt.", "tokens": ["Und", "im\u00b7mer", "vie\u00b7ler", "M\u00e4n\u00b7ner", "harrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dennoch ist die Lou apart", "tokens": ["Und", "den\u00b7noch", "ist", "die", "Lou", "a\u00b7part"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wird von mir verehrt.", "tokens": ["Und", "wird", "von", "mir", "ver\u00b7ehrt", "."], "token_info": ["word"