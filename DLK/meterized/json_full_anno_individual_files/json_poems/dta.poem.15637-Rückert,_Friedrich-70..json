{"dta.poem.15637": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erkenn' an einem Bild, da\u00df nicht an Gottes Huld", "tokens": ["Er\u00b7kenn'", "an", "ei\u00b7nem", "Bild", ",", "da\u00df", "nicht", "an", "Got\u00b7tes", "Huld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "PTKNEG", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es liegt, o Mensch, wenn dich zur\u00fcckh\u00e4lt eigne Schuld.", "tokens": ["Es", "liegt", ",", "o", "Mensch", ",", "wenn", "dich", "zu\u00b7r\u00fcck\u00b7h\u00e4lt", "eig\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "KOUS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwei Schiffe gehn den Flu\u00df hinab, von gleichem Bau;", "tokens": ["Zwei", "Schif\u00b7fe", "gehn", "den", "Flu\u00df", "hin\u00b7ab", ",", "von", "glei\u00b7chem", "Bau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch eins geht langsamer, und schneller eins, o schau!", "tokens": ["Doch", "eins", "geht", "lang\u00b7sa\u00b7mer", ",", "und", "schnel\u00b7ler", "eins", ",", "o", "schau", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "$,", "KON", "ADJD", "PIS", "$,", "FM", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bewegt die beiden nicht des Stromes gleiche Kraft?", "tokens": ["Be\u00b7wegt", "die", "bei\u00b7den", "nicht", "des", "Stro\u00b7mes", "glei\u00b7che", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch bleibt eins zur\u00fcck? was h\u00e4lt es denn in Haft?", "tokens": ["Und", "doch", "bleibt", "eins", "zu\u00b7r\u00fcck", "?", "was", "h\u00e4lt", "es", "denn", "in", "Haft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geladen hat es Stein', und jenes leichtes Holz;", "tokens": ["Ge\u00b7la\u00b7den", "hat", "es", "Stein'", ",", "und", "je\u00b7nes", "leich\u00b7tes", "Holz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$,", "KON", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darum geht es so tr\u00e4g', und jenes wie ein Bolz.", "tokens": ["Da\u00b7rum", "geht", "es", "so", "tr\u00e4g'", ",", "und", "je\u00b7nes", "wie", "ein", "Bolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVFIN", "$,", "KON", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Am Strome liegt es, da\u00df die beiden sich bewegen;", "tokens": ["Am", "Stro\u00b7me", "liegt", "es", ",", "da\u00df", "die", "bei\u00b7den", "sich", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "KOUS", "ART", "PIAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df eines bleibt zur\u00fcck, ist nicht am Strom gelegen.", "tokens": ["Da\u00df", "ei\u00b7nes", "bleibt", "zu\u00b7r\u00fcck", ",", "ist", "nicht", "am", "Strom", "ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PTKVZ", "$,", "VAFIN", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer aber hat das Schiff, das arme, so beladen,", "tokens": ["Wer", "a\u00b7ber", "hat", "das", "Schiff", ",", "das", "ar\u00b7me", ",", "so", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es theilnehmen voll nicht kann am Strom der Gnaden?", "tokens": ["Da\u00df", "es", "theil\u00b7neh\u00b7men", "voll", "nicht", "kann", "am", "Strom", "der", "Gna\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "PTKNEG", "VMFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}