{"dta.poem.192": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4ffer-Gedichte  Balis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Als n\u00e4chst der Ph&#339;bus kaum vergn\u00fcget ausgeschlaffen/", "tokens": ["Als", "n\u00e4chst", "der", "Ph", "&#339;", "bus", "kaum", "ver\u00b7gn\u00fc\u00b7get", "aus\u00b7ge\u00b7schlaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$(", "NE", "ADV", "VVFIN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihm im Augen noch der dunckle Schlummer", "tokens": ["Und", "ihm", "im", "Au\u00b7gen", "noch", "der", "dunck\u00b7le", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "stand/", "tokens": ["stand", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "War ", "tokens": ["War"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Weil er im Bette nichts als lauter Unruh fand.", "tokens": ["Weil", "er", "im", "Bet\u00b7te", "nichts", "als", "lau\u00b7ter", "Un\u00b7ruh", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PIS", "KOKOM", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er trieb die Liebe-Schaar auf eine fette Weide/", "tokens": ["Er", "trieb", "die", "Lie\u00b7be\u00b7Schaar", "auf", "ei\u00b7ne", "fet\u00b7te", "Wei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und legte sich betr\u00fcbt an eine Eichen hin/", "tokens": ["Und", "leg\u00b7te", "sich", "be\u00b7tr\u00fcbt", "an", "ei\u00b7ne", "Ei\u00b7chen", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er klagte/ da\u00df sein Gl\u00fcck des Schicksahls-Ha\u00df beneide/", "tokens": ["Er", "klag\u00b7te", "/", "da\u00df", "sein", "Gl\u00fcck", "des", "Schick\u00b7sahls\u00b7Ha\u00df", "be\u00b7nei\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Indem es \u00e4nderte der ", "tokens": ["In\u00b7dem", "es", "\u00e4n\u00b7der\u00b7te", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die K\u00fcsse/ brach er aus/ die mir ihr Mund gegeben/", "tokens": ["Die", "K\u00fcs\u00b7se", "/", "brach", "er", "aus", "/", "die", "mir", "ihr", "Mund", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$(", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die mehren meinen Schmertz nun sie versaget sind/", "tokens": ["Die", "meh\u00b7ren", "mei\u00b7nen", "Schmertz", "nun", "sie", "ver\u00b7sa\u00b7get", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "ADV", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Geister ziehen weg/ es endet sich mein Leben/", "tokens": ["Die", "Geis\u00b7ter", "zie\u00b7hen", "weg", "/", "es", "en\u00b7det", "sich", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nun ", "tokens": ["Nun"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Jhr Freundlich-seyn ist hin/ die Liebe ist verblichen/", "tokens": ["Ihr", "Freund\u00b7lich\u00b7seyn", "ist", "hin", "/", "die", "Lie\u00b7be", "ist", "ver\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein jeder Blick anjetzt mit Donner schwanger geht/", "tokens": ["Ein", "je\u00b7der", "Blick", "an\u00b7jetzt", "mit", "Don\u00b7ner", "schwan\u00b7ger", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das heisse Liebes-Feur ist aus der Brust entwichen/", "tokens": ["Das", "heis\u00b7se", "Lie\u00b7bes\u00b7Feur", "ist", "aus", "der", "Brust", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df sie wie ", "tokens": ["Da\u00df", "sie", "wie"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Ach! dencke ich anjetzt an die vers\u00fc\u00dften Stunden/", "tokens": ["Ach", "!", "den\u00b7cke", "ich", "an\u00b7jetzt", "an", "die", "ver\u00b7s\u00fc\u00df\u00b7ten", "Stun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Die ich mit ihr vor dem an diesem Ort gehabt", "tokens": ["Die", "ich", "mit", "ihr", "vor", "dem", "an", "die\u00b7sem", "Ort", "ge\u00b7habt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "APPR", "ART", "APPR", "PDAT", "NN", "VAPP"], "meter": "-+-----+-+-+", "measure": "dactylic.init"}, "line.20": {"text": "Wie ich der Lippen-Safft im Uberflu\u00df gefunden/", "tokens": ["Wie", "ich", "der", "Lip\u00b7pen\u00b7\u00b7S\u00b7afft", "im", "U\u00b7ber\u00b7flu\u00df", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Und auf der Zucker Brust mit Anmuth mich gelabt:", "tokens": ["Und", "auf", "der", "Zu\u00b7cker", "Brust", "mit", "An\u00b7muth", "mich", "ge\u00b7labt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So zin\u00dft der Augen-Quell sehr h\u00e4uffig heisse Z\u00e4hren/", "tokens": ["So", "zin\u00dft", "der", "Au\u00b7gen\u00b7Quell", "sehr", "h\u00e4uf\u00b7fig", "heis\u00b7se", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das Hertze schl\u00e4gt als wenns vor Angst zerspringen will/", "tokens": ["Das", "Hert\u00b7ze", "schl\u00e4gt", "als", "wenns", "vor", "Angst", "zer\u00b7sprin\u00b7gen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "KOKOM", "KOUS", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Unseeliges Geschick! verworffenes Verkehren!", "tokens": ["Un\u00b7see\u00b7li\u00b7ges", "Ge\u00b7schick", "!", "ver\u00b7worf\u00b7fe\u00b7nes", "Ver\u00b7keh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mu\u00df eben ", "tokens": ["Mu\u00df", "e\u00b7ben"], "token_info": ["word", "word"], "pos": ["VMFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Was sch\u00f6pffest du vor Lust aus meiner Angst und Qu\u00e4hlen?", "tokens": ["Was", "sch\u00f6pf\u00b7fest", "du", "vor", "Lust", "aus", "mei\u00b7ner", "Angst", "und", "Qu\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sag\u2019 ob dir Freude bringt mein Jammer/ meine Pein/", "tokens": ["Sag'", "ob", "dir", "Freu\u00b7de", "bringt", "mein", "Jam\u00b7mer", "/", "mei\u00b7ne", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "NN", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und warum trennest du zwey hei\u00df verliebte Seelen/", "tokens": ["Und", "wa\u00b7rum", "tren\u00b7nest", "du", "zwey", "hei\u00df", "ver\u00b7lieb\u00b7te", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "CARD", "ADJD", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df eine mu\u00df ein Feur die ander K\u00e4lte seyn?", "tokens": ["Da\u00df", "ei\u00b7ne", "mu\u00df", "ein", "Feur", "die", "an\u00b7der", "K\u00e4l\u00b7te", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Kaum hatten sich gesetzt des Hertzens tieffe Wunden/", "tokens": ["Kaum", "hat\u00b7ten", "sich", "ge\u00b7setzt", "des", "Hert\u00b7zens", "tief\u00b7fe", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "VVPP", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Als dein verworffner Zorn mir das/ was ich gefunden", "tokens": ["Als", "dein", "ver\u00b7worff\u00b7ner", "Zorn", "mir", "das", "/", "was", "ich", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ART", "$(", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zur Heilung meiner Pein/ mit Ungest\u00fcm wegnimmt.", "tokens": ["Zur", "Hei\u00b7lung", "mei\u00b7ner", "Pein", "/", "mit", "Un\u00b7ge\u00b7st\u00fcm", "weg\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$(", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Hie scho\u00df ein Thr\u00e4nen-Bach ihm aus den tr\u00fcben Augen/", "tokens": ["Hie", "scho\u00df", "ein", "Thr\u00e4\u00b7nen\u00b7Bach", "ihm", "aus", "den", "tr\u00fc\u00b7ben", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Zunge l\u00e4hmte sich/ der Worte-Laut verschwand/", "tokens": ["Die", "Zun\u00b7ge", "l\u00e4hm\u00b7te", "sich", "/", "der", "Wor\u00b7te\u00b7Laut", "ver\u00b7schwand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Geister wolten nichts mehr vorzubringen taugen/", "tokens": ["Die", "Geis\u00b7ter", "wol\u00b7ten", "nichts", "mehr", "vor\u00b7zu\u00b7brin\u00b7gen", "tau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "VVIZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Angst und Traurigkeit nahm bey ihn \u00fcberhand.", "tokens": ["Die", "Angst", "und", "Trau\u00b7rig\u00b7keit", "nahm", "bey", "ihn", "\u00fc\u00b7ber\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Doch endlich fing er an; was n\u00fctzet mir das weine", "tokens": ["Doch", "end\u00b7lich", "fing", "er", "an", ";", "was", "n\u00fct\u00b7zet", "mir", "das", "wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Mit meinen Klagen treibt die ", "tokens": ["Mit", "mei\u00b7nen", "Kla\u00b7gen", "treibt", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Nach Regen pfleget sonst die Sonne sch\u00f6n zu scheinen", "tokens": ["Nach", "Re\u00b7gen", "pfle\u00b7get", "sonst", "die", "Son\u00b7ne", "sch\u00f6n", "zu", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Allein die Hoffnung ist zu ", "tokens": ["Al\u00b7lein", "die", "Hoff\u00b7nung", "ist", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.42": {"text": "Jhr Sinn ist wie ", "tokens": ["Ihr", "Sinn", "ist", "wie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM"], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Jhr Hertze ist wie Stahl/ so hart wie Diamant/", "tokens": ["Ihr", "Hert\u00b7ze", "ist", "wie", "Stahl", "/", "so", "hart", "wie", "Di\u00b7a\u00b7mant", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "KOKOM", "NN", "$(", "ADV", "ADJD", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Weils auch die Thr\u00e4nen nicht der Augen zwingen k\u00f6nnen/", "tokens": ["Weils", "auch", "die", "Thr\u00e4\u00b7nen", "nicht", "der", "Au\u00b7gen", "zwin\u00b7gen", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "ART", "NN", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Das Feuer/ das mich brennt/ hat nicht bey ihr bestand.", "tokens": ["Das", "Feu\u00b7er", "/", "das", "mich", "brennt", "/", "hat", "nicht", "bey", "ihr", "be\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VAFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ich lebe ohne Lust/ ohn Schlaffen/ Essen/ Trincken/", "tokens": ["Ich", "le\u00b7be", "oh\u00b7ne", "Lust", "/", "ohn", "Schlaf\u00b7fen", "/", "Es\u00b7sen", "/", "Trin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word"