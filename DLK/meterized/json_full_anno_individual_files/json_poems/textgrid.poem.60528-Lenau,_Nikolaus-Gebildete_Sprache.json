os": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fort mu\u00df mit verwirktem Z\u00fcgel,", "tokens": ["Fort", "mu\u00df", "mit", "ver\u00b7wirk\u00b7tem", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gar nicht wei\u00df, wie ihm geschieht:", "tokens": ["Gar", "nicht", "wei\u00df", ",", "wie", "ihm", "ge\u00b7schieht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Also tr\u00e4gt das deutsche Wort,", "tokens": ["Al\u00b7so", "tr\u00e4gt", "das", "deut\u00b7sche", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Das von Meistern ward geritten,", "tokens": ["Das", "von", "Meis\u00b7tern", "ward", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Als sie sich den Kranz erstritten,", "tokens": ["Als", "sie", "sich", "den", "Kranz", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Manchen St\u00fcmper mit sich fort.", "tokens": ["Man\u00b7chen", "St\u00fcm\u00b7per", "mit", "sich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}