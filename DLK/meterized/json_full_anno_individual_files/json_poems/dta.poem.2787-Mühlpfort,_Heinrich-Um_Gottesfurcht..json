{"dta.poem.2787": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Um Gottesfurcht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ovater aller G\u00fct/ ich klage dir mit Schmertzen", "tokens": ["O\u00b7va\u00b7ter", "al\u00b7ler", "G\u00fct", "/", "ich", "kla\u00b7ge", "dir", "mit", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$(", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Bosheit meiner Seel und Gift in meinem Hertzen", "tokens": ["Die", "Bos\u00b7heit", "mei\u00b7ner", "Seel", "und", "Gift", "in", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die mich so gar besessen", "tokens": ["Die", "mich", "so", "gar", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich O h\u00f6chster GOtt!", "tokens": ["Da\u00df", "ich", "O", "h\u00f6chs\u00b7ter", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hab \u00fcberall vergessen", "tokens": ["Hab", "\u00fc\u00b7be\u00b7rall", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Heissen und Gebot.", "tokens": ["Dein", "Heis\u00b7sen", "und", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach keine Gottesfurcht ist mehr vor meinen Augen", "tokens": ["Ach", "kei\u00b7ne", "Got\u00b7tes\u00b7furcht", "ist", "mehr", "vor", "mei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PIAT", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die leyder g\u00e4ntzlich dich recht zu erkennen taugen.", "tokens": ["Die", "ley\u00b7der", "g\u00e4ntz\u00b7lich", "dich", "recht", "zu", "er\u00b7ken\u00b7nen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich kan ich ja nicht lieben", "tokens": ["Dich", "kan", "ich", "ja", "nicht", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch al\u00df ich Armer soll", "tokens": ["Noch", "al\u00df", "ich", "Ar\u00b7mer", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In deiner Furcht mich \u00fcben/", "tokens": ["In", "dei\u00b7ner", "Furcht", "mich", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das f\u00fchl ich gar zu wohl.", "tokens": ["Das", "f\u00fchl", "ich", "gar", "zu", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist ja meine S\u00fcnd und Bo\u00dfheit nicht zu messen", "tokens": ["Es", "ist", "ja", "mei\u00b7ne", "S\u00fcnd", "und", "Bo\u00df\u00b7heit", "nicht", "zu", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Offt hab ich dein Gesetz/ O grosser GOtt vergessen/", "tokens": ["Offt", "hab", "ich", "dein", "Ge\u00b7setz", "/", "O", "gros\u00b7ser", "Gott", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$(", "NE", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Offt la\u00df ich mir gefallen", "tokens": ["Offt", "la\u00df", "ich", "mir", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wollust dieser Welt", "tokens": ["Die", "Wol\u00b7lust", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der leyder ich f\u00fcr allen", "tokens": ["Der", "ley\u00b7der", "ich", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich t\u00e4glich dargestellt.", "tokens": ["Mich", "t\u00e4g\u00b7lich", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich seufz: O frommer GOtt! du wollest mir vergeben/", "tokens": ["Ich", "seufz", ":", "O", "from\u00b7mer", "Gott", "!", "du", "wol\u00b7lest", "mir", "ver\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich in Sicherheit verbracht mein junges Leben", "tokens": ["Da\u00df", "ich", "in", "Si\u00b7cher\u00b7heit", "ver\u00b7bracht", "mein", "jun\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem ich nicht gescheuet", "tokens": ["In\u00b7dem", "ich", "nicht", "ge\u00b7scheu\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Straffe die mir dort", "tokens": ["Die", "Straf\u00b7fe", "die", "mir", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von dir ist angedr\u00e4uet", "tokens": ["Von", "dir", "ist", "an\u00b7ge\u00b7dr\u00e4u\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir l\u00e4ngst in deinem Wort.", "tokens": ["Mir", "l\u00e4ngst", "in", "dei\u00b7nem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es ist dich f\u00fcrchten ja die wundersch\u00f6ne Tugend", "tokens": ["Es", "ist", "dich", "f\u00fcrch\u00b7ten", "ja", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weisheit h\u00f6chster Schatz die Meisterin der Jugend/", "tokens": ["Der", "Weis\u00b7heit", "h\u00f6chs\u00b7ter", "Schatz", "die", "Meis\u00b7te\u00b7rin", "der", "Ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohl dem der diese kennet", "tokens": ["Wohl", "dem", "der", "die\u00b7se", "ken\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ART", "PDS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und klebet fest ihr an", "tokens": ["Und", "kle\u00b7bet", "fest", "ihr", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wird ein Christ genennet", "tokens": ["Der", "wird", "ein", "Christ", "ge\u00b7nen\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der GOtt gefallen kan.", "tokens": ["Der", "Gott", "ge\u00b7fal\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch ferner bitt ich HErr du wollest von mir nehmen/", "tokens": ["Noch", "fer\u00b7ner", "bitt", "ich", "Herr", "du", "wol\u00b7lest", "von", "mir", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein angeborne S\u00fcnd und kr\u00e4fftig in mir z\u00e4hmen/", "tokens": ["Mein", "an\u00b7ge\u00b7bor\u00b7ne", "S\u00fcnd", "und", "kr\u00e4ff\u00b7tig", "in", "mir", "z\u00e4h\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJD", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das B\u00f6\u00df in meinem Willen/", "tokens": ["Das", "B\u00f6\u00df", "in", "mei\u00b7nem", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Samt der Vermessenheit/", "tokens": ["Samt", "der", "Ver\u00b7mes\u00b7sen\u00b7heit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Benebenst dem auch stillen", "tokens": ["Be\u00b7ne\u00b7benst", "dem", "auch", "stil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Den Spott der Ewigkeit.", "tokens": ["Den", "Spott", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gib deinen guten Geist/ den Geist der Furcht des HErren/", "tokens": ["Gib", "dei\u00b7nen", "gu\u00b7ten", "Geist", "/", "den", "Geist", "der", "Furcht", "des", "Her\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df mich nur in ihr mein gantzes Hertz versperren/", "tokens": ["Und", "la\u00df", "mich", "nur", "in", "ihr", "mein", "gant\u00b7zes", "Hertz", "ver\u00b7sper\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich an allen Orten/", "tokens": ["Da\u00df", "ich", "an", "al\u00b7len", "Or\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ich auch immer sey/", "tokens": ["Wo", "ich", "auch", "im\u00b7mer", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Wercken und mit Worten/", "tokens": ["Mit", "Wer\u00b7cken", "und", "mit", "Wor\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr deinem Zorn mich scheu.", "tokens": ["F\u00fcr", "dei\u00b7nem", "Zorn", "mich", "scheu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Erwecke wahre Reu und Leyd in meinem Hertzen/", "tokens": ["Er\u00b7we\u00b7cke", "wah\u00b7re", "Reu", "und", "Leyd", "in", "mei\u00b7nem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich mein eitles Thun beweinen m\u00f6g mit Schmertzen.", "tokens": ["Da\u00df", "ich", "mein", "eit\u00b7les", "Thun", "be\u00b7wei\u00b7nen", "m\u00f6g", "mit", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df ich ja beklage/", "tokens": ["Und", "da\u00df", "ich", "ja", "be\u00b7kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr meine Missethat/", "tokens": ["Mehr", "mei\u00b7ne", "Mis\u00b7se\u00b7that", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als deinen Zorn und Plage/", "tokens": ["Als", "dei\u00b7nen", "Zorn", "und", "Pla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die mich ergriffen hat.", "tokens": ["Die", "mich", "er\u00b7grif\u00b7fen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O seelig werd ich seyn/ im Fall ich kan erkennen/", "tokens": ["O", "see\u00b7lig", "werd", "ich", "seyn", "/", "im", "Fall", "ich", "kan", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "PPER", "VAINF", "$(", "APPRART", "NN", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du mein GOtt gerecht/ ich aber b\u00f6\u00df zu nennen/", "tokens": ["Da\u00df", "du", "mein", "Gott", "ge\u00b7recht", "/", "ich", "a\u00b7ber", "b\u00f6\u00df", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "$(", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja da\u00df ich billich leide", "tokens": ["Ja", "da\u00df", "ich", "bil\u00b7lich", "lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wolverdiente Pein/", "tokens": ["Die", "wol\u00b7ver\u00b7dien\u00b7te", "Pein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und weil ich selbst mich scheide", "tokens": ["Und", "weil", "ich", "selbst", "mich", "schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von dir/ mu\u00df straffbar seyn.", "tokens": ["Von", "dir", "/", "mu\u00df", "straff\u00b7bar", "seyn", "."], "token_info": ["word", "word", "punct", "word