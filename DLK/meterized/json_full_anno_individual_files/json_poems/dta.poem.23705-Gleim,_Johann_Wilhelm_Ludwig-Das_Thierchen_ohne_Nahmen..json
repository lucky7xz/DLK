{"dta.poem.23705": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Thierchen ohne Nahmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am zwanzigsten des Maien,", "tokens": ["Am", "zwan\u00b7zigs\u00b7ten", "des", "Mai\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "An dem du mich, o Doris,", "tokens": ["An", "dem", "du", "mich", ",", "o", "Do\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "$,", "FM", "NE", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Nicht immer k\u00fcssen woltest,", "tokens": ["Nicht", "im\u00b7mer", "k\u00fcs\u00b7sen", "wol\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sa\u00df an dem weissen Halse", "tokens": ["Sa\u00df", "an", "dem", "weis\u00b7sen", "Hal\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der freundlichen Filinde", "tokens": ["Der", "freund\u00b7li\u00b7chen", "Fi\u00b7lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein kleines schwarzes Thierchen.", "tokens": ["Ein", "klei\u00b7nes", "schwar\u00b7zes", "Thier\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich wei\u00df es nicht zu nennen;", "tokens": ["Ich", "wei\u00df", "es", "nicht", "zu", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dis wei\u00df ich, da\u00df es h\u00fcpfet,", "tokens": ["Dis", "wei\u00df", "ich", ",", "da\u00df", "es", "h\u00fcp\u00b7fet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wie Grasepferde h\u00fcpfen,", "tokens": ["Wie", "Gra\u00b7se\u00b7pfer\u00b7de", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und da\u00df es oft entwischet,", "tokens": ["Und", "da\u00df", "es", "oft", "ent\u00b7wi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wenn es erz\u00fcrnte Sch\u00f6nen,", "tokens": ["Wenn", "es", "er\u00b7z\u00fcrn\u00b7te", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Im freien Felde iagen.", "tokens": ["Im", "frei\u00b7en", "Fel\u00b7de", "i\u00b7a\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Ein Kenner der Insekten,", "tokens": ["Ein", "Ken\u00b7ner", "der", "In\u00b7sek\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Beschrieb mir i\u00fcngst das Thierchen.", "tokens": ["Be\u00b7schrieb", "mir", "i\u00b7\u00fcngst", "das", "Thier\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Er sprach: Es wird bei Sch\u00f6nen", "tokens": ["Er", "sprach", ":", "Es", "wird", "bei", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Geboren und erzogen,", "tokens": ["Ge\u00b7bo\u00b7ren", "und", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Es wohnet bei den Sch\u00f6nen,", "tokens": ["Es", "woh\u00b7net", "bei", "den", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und wagt sich nur zu M\u00e4nnern,", "tokens": ["Und", "wagt", "sich", "nur", "zu", "M\u00e4n\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Wenn sie mit Sch\u00f6nen spielen.", "tokens": ["Wenn", "sie", "mit", "Sch\u00f6\u00b7nen", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ein solch begl\u00fckktes Thierchen", "tokens": ["Ein", "solch", "be\u00b7gl\u00fckk\u00b7tes", "Thier\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.21": {"text": "Sa\u00df an dem weissen Halse", "tokens": ["Sa\u00df", "an", "dem", "weis\u00b7sen", "Hal\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Der freundlichen Filinde.", "tokens": ["Der", "freund\u00b7li\u00b7chen", "Fi\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}