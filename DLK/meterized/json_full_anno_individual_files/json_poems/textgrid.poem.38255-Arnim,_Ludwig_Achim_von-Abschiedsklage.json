{"textgrid.poem.38255": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abschiedsklage", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach in Trauern mu\u00df ich leben,", "tokens": ["Ach", "in", "Trau\u00b7ern", "mu\u00df", "ich", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! wie hab ichs denn verschuldt?", "tokens": ["Ach", "!", "wie", "hab", "ichs", "denn", "ver\u00b7schuldt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil mirs hat mein Schatz aufgeben,", "tokens": ["Weil", "mirs", "hat", "mein", "Schatz", "auf\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ichs leiden mit Geduld.", "tokens": ["Mu\u00df", "ichs", "lei\u00b7den", "mit", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vater und Mutter, die wollens nicht leiden,", "tokens": ["Va\u00b7ter", "und", "Mut\u00b7ter", ",", "die", "wol\u00b7lens", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Gelt mein Schatz, das wei\u00dft du wohl?", "tokens": ["Gelt", "mein", "Schatz", ",", "das", "wei\u00dft", "du", "wohl", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du hast recht in allen Sachen,", "tokens": ["Du", "hast", "recht", "in", "al\u00b7len", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kannst dein Gl\u00fcck noch besser machen,", "tokens": ["Kannst", "dein", "Gl\u00fcck", "noch", "bes\u00b7ser", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil ich dich nicht kriegen soll.", "tokens": ["Weil", "ich", "dich", "nicht", "krie\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rosmarin und Lorbeerbl\u00e4tter", "tokens": ["Ros\u00b7ma\u00b7rin", "und", "Lor\u00b7beer\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Verehr ich dir zu guter lezt,", "tokens": ["Ver\u00b7ehr", "ich", "dir", "zu", "gu\u00b7ter", "lezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das soll seyn das lezt Gedenken,", "tokens": ["Das", "soll", "seyn