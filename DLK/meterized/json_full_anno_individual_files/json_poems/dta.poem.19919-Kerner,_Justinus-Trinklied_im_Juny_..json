{"dta.poem.19919": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied im Juny .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was duftet von des Berges Haupt", "tokens": ["Was", "duf\u00b7tet", "von", "des", "Ber\u00b7ges", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So tief ins Thal hinab?", "tokens": ["So", "tief", "ins", "Thal", "hin\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Rebe ist's, die neubelaubt", "tokens": ["Die", "Re\u00b7be", "ist's", ",", "die", "neu\u00b7be\u00b7laubt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich bl\u00fchend hebt am Stab.", "tokens": ["Sich", "bl\u00fc\u00b7hend", "hebt", "am", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was regt sich in des Hauses Grund", "tokens": ["Was", "regt", "sich", "in", "des", "Hau\u00b7ses", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In den Gew\u00f6lben tief?", "tokens": ["In", "den", "Ge\u00b7w\u00f6l\u00b7ben", "tief", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wein ist's, der in Fasses Rund", "tokens": ["Der", "Wein", "ist's", ",", "der", "in", "Fas\u00b7ses", "Rund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon l\u00e4ngst gebunden schlief.", "tokens": ["Schon", "l\u00e4ngst", "ge\u00b7bun\u00b7den", "schlief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Bl\u00fcthe hat ihn aufgeregt,", "tokens": ["Die", "Bl\u00fc\u00b7the", "hat", "ihn", "auf\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Duft im Heimatland,", "tokens": ["Der", "Duft", "im", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er von Sehnsucht tiefbewegt", "tokens": ["Da\u00df", "er", "von", "Sehn\u00b7sucht", "tief\u00b7be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will sprengen jezt sein Band.", "tokens": ["Will", "spren\u00b7gen", "jezt", "sein", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zwingherren, Freunde! sind wir nicht,", "tokens": ["Zwing\u00b7her\u00b7ren", ",", "Freun\u00b7de", "!", "sind", "wir", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt die Pokale her!", "tokens": ["Bringt", "die", "Po\u00b7ka\u00b7le", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00dft den Armen jezt an's Licht,", "tokens": ["Und", "la\u00dft", "den", "Ar\u00b7men", "jezt", "an's", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er es w\u00fcnscht so sehr.", "tokens": ["Wie", "er", "es", "w\u00fcnscht", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hebt singend auf zu Berges H\u00f6h'", "tokens": ["Hebt", "sin\u00b7gend", "auf", "zu", "Ber\u00b7ges", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sch\u00e4umenden Pokal:", "tokens": ["Den", "sch\u00e4u\u00b7men\u00b7den", "Po\u00b7kal", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Befreyter! deine Heimat seh'", "tokens": ["Be\u00b7frey\u00b7ter", "!", "dei\u00b7ne", "Hei\u00b7mat", "seh'"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Duft und Sonnenstrahl!", "tokens": ["Im", "Duft", "und", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Seht, wie mit tausend Augen er", "tokens": ["Seht", ",", "wie", "mit", "tau\u00b7send", "Au\u00b7gen", "er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "CARD", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Heimat schaut entz\u00fcckt,", "tokens": ["Die", "Hei\u00b7mat", "schaut", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus der die Rebe bl\u00fcthenschwer", "tokens": ["Aus", "der", "die", "Re\u00b7be", "bl\u00fc\u00b7then\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm in die Augen blickt!", "tokens": ["Ihm", "in", "die", "Au\u00b7gen", "blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er braust, er singt: \u201ewillkommen du,", "tokens": ["Er", "braust", ",", "er", "singt", ":", "\u201e", "will\u00b7kom\u00b7men", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "$(", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eo Heimat voller Licht!", "tokens": ["\u201e", "o", "Hei\u00b7mat", "vol\u00b7ler", "Licht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eund jezt, ihr Lieben! trinkt nur zu!", "tokens": ["\u201e", "und", "jezt", ",", "ihr", "Lie\u00b7ben", "!", "trinkt", "nur", "zu", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "$,", "PPOSAT", "ADJA", "$.", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich bin der Lezte nicht!\u201c", "tokens": ["\u201e", "ich", "bin", "der", "Lez\u00b7te", "nicht", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du edler Saft! du dringst mit Macht", "tokens": ["Du", "ed\u00b7ler", "Saft", "!", "du", "dringst", "mit", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns in das Herz hinein!", "tokens": ["Uns", "in", "das", "Herz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohlan! sto\u00dft an! du sollst gebracht,", "tokens": ["Wo\u00b7hlan", "!", "sto\u00dft", "an", "!", "du", "sollst", "ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der theuren Heimat seyn!", "tokens": ["Der", "theu\u00b7ren", "Hei\u00b7mat", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und dem, der irrt am fremden Strand,", "tokens": ["Und", "dem", ",", "der", "irrt", "am", "frem\u00b7den", "Strand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dem in Kerkersnoth,", "tokens": ["Und", "dem", "in", "Ker\u00b7kers\u00b7noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ihm erschein' sein Heimatland,", "tokens": ["Da\u00df", "ihm", "er\u00b7schein'", "sein", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie dir noch vor dem Tod.", "tokens": ["Wie", "dir", "noch", "vor", "dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}