{"dta.poem.11273": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der unverstand.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die zeiten Bileams, die stellen sich itzt ein,", "tokens": ["Die", "zei\u00b7ten", "Bi\u00b7leams", ",", "die", "stel\u00b7len", "sich", "itzt", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn aller orten h\u00f6rt man dessen pferde schreyn;", "tokens": ["Denn", "al\u00b7ler", "or\u00b7ten", "h\u00f6rt", "man", "des\u00b7sen", "pfer\u00b7de", "schreyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PIS", "PDS", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch Bileams sein pferd, das sprach noch kluge sachen,", "tokens": ["Doch", "Bi\u00b7leams", "sein", "pferd", ",", "das", "sprach", "noch", "klu\u00b7ge", "sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "ADV", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Da es die heutigen aufs allert\u00fcmmste machen.", "tokens": ["Da", "es", "die", "heu\u00b7ti\u00b7gen", "aufs", "al\u00b7ler\u00b7t\u00fcmms\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}