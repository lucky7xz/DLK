{"textgrid.poem.26545": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df immerhin \u2013 droht auch dein Herz zu brechen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df immerhin \u2013 droht auch dein Herz zu brechen \u2013", "tokens": ["La\u00df", "im\u00b7mer\u00b7hin", "\u2013", "droht", "auch", "dein", "Herz", "zu", "bre\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$(", "VVFIN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Ha\u00df und von der Bosheit dich verwunden,", "tokens": ["Vom", "Ha\u00df", "und", "von", "der", "Bos\u00b7heit", "dich", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gieb selbst dem Spotte preis, was du empfunden,", "tokens": ["Gieb", "selbst", "dem", "Spot\u00b7te", "preis", ",", "was", "du", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fassen la\u00df dich stets bei deinen Schw\u00e4chen.", "tokens": ["Und", "fas\u00b7sen", "la\u00df", "dich", "stets", "bei", "dei\u00b7nen", "Schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es werden Tage kommen, die dich r\u00e4chen;", "tokens": ["Es", "wer\u00b7den", "Ta\u00b7ge", "kom\u00b7men", ",", "die", "dich", "r\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVINF", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn die Feinde, wider dich verbunden,", "tokens": ["Und", "wenn", "die", "Fein\u00b7de", ",", "wi\u00b7der", "dich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An ihren Freuden ihren Lohn gefunden \u2013", "tokens": ["An", "ih\u00b7ren", "Freu\u00b7den", "ih\u00b7ren", "Lohn", "ge\u00b7fun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann werden selbst die Steine f\u00fcr dich sprechen.", "tokens": ["Dann", "wer\u00b7den", "selbst", "die", "Stei\u00b7ne", "f\u00fcr", "dich", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann preis't man dich, statt dich, wie einst, zu schm\u00e4hen,", "tokens": ["Dann", "preis't", "man", "dich", ",", "statt", "dich", ",", "wie", "einst", ",", "zu", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$,", "KOUI", "PPER", "$,", "PWAV", "ADV", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was man nie und nimmer an dir pr\u00fcfte,", "tokens": ["Und", "was", "man", "nie", "und", "nim\u00b7mer", "an", "dir", "pr\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "KON", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird dann, bewundert, sich von selbst verstehen.", "tokens": ["Wird", "dann", ",", "be\u00b7wun\u00b7dert", ",", "sich", "von", "selbst", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "VVPP", "$,", "PRF", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn so nur werden frei die Weihrauchd\u00fcfte,", "tokens": ["Denn", "so", "nur", "wer\u00b7den", "frei", "die", "Weih\u00b7rauch\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die um der Menschheit Hochalt\u00e4re wehen \u2013", "tokens": ["Die", "um", "der", "Menschheit", "Hoch\u00b7al\u00b7t\u00e4\u00b7re", "we\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und so nur steigt ein Ph\u00f6nix in die L\u00fcfte!", "tokens": ["Und", "so", "nur", "steigt", "ein", "Ph\u00f6\u00b7nix", "in", "die", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "A