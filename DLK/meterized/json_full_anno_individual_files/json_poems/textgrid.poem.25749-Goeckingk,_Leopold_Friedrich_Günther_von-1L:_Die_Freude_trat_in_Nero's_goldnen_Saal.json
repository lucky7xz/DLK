{"textgrid.poem.25749": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Freude trat in Nero's goldnen Saal", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freude trat in Nero's goldnen Saal", "tokens": ["Die", "Freu\u00b7de", "trat", "in", "Nero's", "gold\u00b7nen", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zwar nie mit einem Fu\u00df'; sie liebt die kleinen Zimmer", "tokens": ["Zwar", "nie", "mit", "ei\u00b7nem", "Fu\u00df'", ";", "sie", "liebt", "die", "klei\u00b7nen", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Weisen nur; doch ach! sie wohnt auch da nicht immer:", "tokens": ["Der", "Wei\u00b7sen", "nur", ";", "doch", "ach", "!", "sie", "wohnt", "auch", "da", "nicht", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie kommt und geht, wie Gott es ihr befahl.", "tokens": ["Sie", "kommt", "und", "geht", ",", "wie", "Gott", "es", "ihr", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie, der es sonst so wohl bei mir gefiel,", "tokens": ["Sie", ",", "der", "es", "sonst", "so", "wohl", "bei", "mir", "ge\u00b7fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "War fort, als ich zur\u00fcck vom hohen Jura kehrte,", "tokens": ["War", "fort", ",", "als", "ich", "zu\u00b7r\u00fcck", "vom", "ho\u00b7hen", "Ju\u00b7ra", "kehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKVZ", "$,", "KOUS", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ach! das erste Wort, das ich am Schwelle h\u00f6rte:", "tokens": ["Und", "ach", "!", "das", "ers\u00b7te", "Wort", ",", "das", "ich", "am", "Schwel\u00b7le", "h\u00f6r\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u00bbdein Sohn ist todt.\u00ab \u2013 Verstumm', o Saitenspiel! \u2013", "tokens": ["\u00bb", "dein", "Sohn", "ist", "todt", ".", "\u00ab", "\u2013", "Ver\u00b7stumm'", ",", "o", "Sai\u00b7ten\u00b7spiel", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$(", "$(", "NN", "$,", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Fast preis' ich, Freund, dich gl\u00fccklich, da\u00df kein Sohn", "tokens": ["Fast", "preis'", "ich", ",", "Freund", ",", "dich", "gl\u00fcck\u00b7lich", ",", "da\u00df", "kein", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PRF", "ADJD", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dir sterben kann, nicht Schmerz dir eine Gattin klagen,", "tokens": ["Dir", "ster\u00b7ben", "kann", ",", "nicht", "Schmerz", "dir", "ei\u00b7ne", "Gat\u00b7tin", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "PTKNEG", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den du nicht lindern kannst! Denn ach! seit hundert Tagen", "tokens": ["Den", "du", "nicht", "lin\u00b7dern", "kannst", "!", "Denn", "ach", "!", "seit", "hun\u00b7dert", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMFIN", "$.", "KON", "XY", "$.", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Seufzt, doch umsonst! nach Lindrung Chloe schon.", "tokens": ["Seufzt", ",", "doch", "um\u00b7sonst", "!", "nach", "Lind\u00b7rung", "Chloe", "schon", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "$.", "APPR", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Auch ich bin Mensch, nicht Philosoph allein.", "tokens": ["Auch", "ich", "bin", "Mensch", ",", "nicht", "Phi\u00b7lo\u00b7soph", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "NN", "$,", "PTKNEG", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wenn sich um uns das Gl\u00fcck und Ungl\u00fcck nicht bek\u00fcmmert,", "tokens": ["Wenn", "sich", "um", "uns", "das", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "nicht", "be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPER", "ART", "NN", "KON", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kein K\u00f6nig uns umarmt, kein Liebling h\u00fclflos wimmert:", "tokens": ["Kein", "K\u00f6\u00b7nig", "uns", "um\u00b7armt", ",", "kein", "Lieb\u00b7ling", "h\u00fcl\u00b7flos", "wim\u00b7mert", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "$,", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie leicht ist's da, ein weiser Mann zu seyn!", "tokens": ["Wie", "leicht", "ist's", "da", ",", "ein", "wei\u00b7ser", "Mann", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "$,", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ich aber, den im Wrack' der Sand umfleu\u00dft,", "tokens": ["Ich", "a\u00b7ber", ",", "den", "im", "Wrack'", "der", "Sand", "um\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mu\u00df sehn, wie ich ans Land auf einem Brette fahre,", "tokens": ["Mu\u00df", "sehn", ",", "wie", "ich", "ans", "Land", "auf", "ei\u00b7nem", "Bret\u00b7te", "fah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Eh' die Verzweiflung mich, die schon in meine Haare", "tokens": ["Eh'", "die", "Ver\u00b7zwei\u00b7flung", "mich", ",", "die", "schon", "in", "mei\u00b7ne", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sich klammern will, herab zur Tiefe rei\u00dft.", "tokens": ["Sich", "klam\u00b7mern", "will", ",", "her\u00b7ab", "zur", "Tie\u00b7fe", "rei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VMFIN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Wohl mir, da\u00df ich ins Meer entschlossen sprang!", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich", "ins", "Meer", "ent\u00b7schlos\u00b7sen", "sprang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Gewonnen ist der Strand! das Land inde\u00df ist \u00f6de!", "tokens": ["Ge\u00b7won\u00b7nen", "ist", "der", "Strand", "!", "das", "Land", "in\u00b7de\u00df", "ist", "\u00f6\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "ART", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Allein genug, da\u00df mich, im Angesicht' der Rhede,", "tokens": ["Al\u00b7lein", "ge\u00b7nug", ",", "da\u00df", "mich", ",", "im", "An\u00b7ge\u00b7sicht'", "der", "Rhe\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Welle nicht mit sammt dem Wrack' verschlang.", "tokens": ["Die", "Wel\u00b7le", "nicht", "mit", "sammt", "dem", "Wrack'", "ver\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ich fall' am Ufer dankbar auf die Knie,", "tokens": ["Ich", "fall'", "am", "U\u00b7fer", "dank\u00b7bar", "auf", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df der noch Muth mir gibt, ein Leiden zu bestehen,", "tokens": ["Da\u00df", "der", "noch", "Muth", "mir", "gibt", ",", "ein", "Lei\u00b7den", "zu", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der uns durch Leiden pr\u00fcft. Wer kann ihn \u00fcbersehen?", "tokens": ["Der", "uns", "durch", "Lei\u00b7den", "pr\u00fcft", ".", "Wer", "kann", "ihn", "\u00fc\u00b7ber\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und was ist gegen ihn Philosophie?", "tokens": ["Und", "was", "ist", "ge\u00b7gen", "ihn", "Phi\u00b7lo\u00b7so\u00b7phie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Ach! h\u00e4tt' er nicht jenseit der finstern Gruft", "tokens": ["Ach", "!", "h\u00e4tt'", "er", "nicht", "jen\u00b7seit", "der", "fins\u00b7tern", "Gruft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Des Todes, noch ein Land des l\u00e4ngern, bessern Lebens,", "tokens": ["Des", "To\u00b7des", ",", "noch", "ein", "Land", "des", "l\u00e4n\u00b7gern", ",", "bes\u00b7sern", "Le\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So schnappte wahrlich hier itzt Chloe nicht vergebens,", "tokens": ["So", "schnapp\u00b7te", "wahr\u00b7lich", "hier", "itzt", "Chloe", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "NE", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Dem Fisch' am Strande gleich, nach seiner Luft.", "tokens": ["Dem", "Fisch'", "am", "Stran\u00b7de", "gleich", ",", "nach", "sei\u00b7ner", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "So weint' um einen Knaben nicht mein Herz,", "tokens": ["So", "weint'", "um", "ei\u00b7nen", "Kna\u00b7ben", "nicht", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Der seiner Eltern Schatz und aller Menschen Freude", "tokens": ["Der", "sei\u00b7ner", "El\u00b7tern", "Schatz", "und", "al\u00b7ler", "Men\u00b7schen", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Schon in der Wiege war; so st\u00e4nden wir itzt beide,", "tokens": ["Schon", "in", "der", "Wie\u00b7ge", "war", ";", "so", "st\u00e4n\u00b7den", "wir", "itzt", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mein trauter ", "tokens": ["Mein", "trau\u00b7ter"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Die Freude trat in Nero's goldnen Saal", "tokens": ["Die", "Freu\u00b7de", "trat", "in", "Nero's", "gold\u00b7nen", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zwar nie mit einem Fu\u00df'; sie liebt die kleinen Zimmer", "tokens": ["Zwar", "nie", "mit", "ei\u00b7nem", "Fu\u00df'", ";", "sie", "liebt", "die", "klei\u00b7nen", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Weisen nur; doch ach! sie wohnt auch da nicht immer:", "tokens": ["Der", "Wei\u00b7sen", "nur", ";", "doch", "ach", "!", "sie", "wohnt", "auch", "da", "nicht", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie kommt und geht, wie Gott es ihr befahl.", "tokens": ["Sie", "kommt", "und", "geht", ",", "wie", "Gott", "es", "ihr", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie, der es sonst so wohl bei mir gefiel,", "tokens": ["Sie", ",", "der", "es", "sonst", "so", "wohl", "bei", "mir", "ge\u00b7fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "War fort, als ich zur\u00fcck vom hohen Jura kehrte,", "tokens": ["War", "fort", ",", "als", "ich", "zu\u00b7r\u00fcck", "vom", "ho\u00b7hen", "Ju\u00b7ra", "kehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKVZ", "$,", "KOUS", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ach! das erste Wort, das ich am Schwelle h\u00f6rte:", "tokens": ["Und", "ach", "!", "das", "ers\u00b7te", "Wort", ",", "das", "ich", "am", "Schwel\u00b7le", "h\u00f6r\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u00bbdein Sohn ist todt.\u00ab \u2013 Verstumm', o Saitenspiel! \u2013", "tokens": ["\u00bb", "dein", "Sohn", "ist", "todt", ".", "\u00ab", "\u2013", "Ver\u00b7stumm'", ",", "o", "Sai\u00b7ten\u00b7spiel", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$(", "$(", "NN", "$,", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Fast preis' ich, Freund, dich gl\u00fccklich, da\u00df kein Sohn", "tokens": ["Fast", "preis'", "ich", ",", "Freund", ",", "dich", "gl\u00fcck\u00b7lich", ",", "da\u00df", "kein", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PRF", "ADJD", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dir sterben kann, nicht Schm