{"textgrid.poem.24985": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon meinen Spielgenossen hie\u00df ich Tr\u00e4umer;", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon meinen Spielgenossen hie\u00df ich Tr\u00e4umer;", "tokens": ["Schon", "mei\u00b7nen", "Spiel\u00b7ge\u00b7nos\u00b7sen", "hie\u00df", "ich", "Tr\u00e4u\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn wie ein Bruder engverwandt von je,", "tokens": ["Denn", "wie", "ein", "Bru\u00b7der", "eng\u00b7ver\u00b7wandt", "von", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchlt' ich, o Schmerz, du tiefer, allgeheimer,", "tokens": ["F\u00fchlt'", "ich", ",", "o", "Schmerz", ",", "du", "tie\u00b7fer", ",", "all\u00b7ge\u00b7hei\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "ADJD", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich dir und deinem dunklen Weh.", "tokens": ["Mich", "dir", "und", "dei\u00b7nem", "dunk\u00b7len", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn lachend \u00fcber mir des Lebens blauer", "tokens": ["Wenn", "la\u00b7chend", "\u00fc\u00b7ber", "mir", "des", "Le\u