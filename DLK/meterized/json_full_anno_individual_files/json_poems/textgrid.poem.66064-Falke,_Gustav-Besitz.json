{"textgrid.poem.66064": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Besitz", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne \u00fcberstrahlt dein Bild,", "tokens": ["Die", "Son\u00b7ne", "\u00fc\u00b7bers\u00b7trahlt", "dein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz wird warm und freut sich.", "tokens": ["Mein", "Herz", "wird", "warm", "und", "freut", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein liebes Bild.", "tokens": ["Dein", "lie\u00b7bes", "Bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Alles Licht ferner Tage erneut sich.", "tokens": ["Al\u00b7les", "Licht", "fer\u00b7ner", "Ta\u00b7ge", "er\u00b7neut", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "VVFIN", "PRF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "So recht in tiefstem dankbar sein,", "tokens": ["So", "recht", "in", "tiefs\u00b7tem", "dank\u00b7bar", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich dir durfte begegnen,", "tokens": ["Da\u00df", "ich", "dir", "durf\u00b7te", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Diese Frucht blieb mein.", "tokens": ["Die\u00b7se", "Frucht", "blieb", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Kann Liebe ein Leben reicher segnen?", "tokens": ["Kann", "Lie\u00b7be", "ein", "Le\u00b7ben", "rei\u00b7cher", "seg\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich durfte dich nicht besitzen, es war", "tokens": ["Ich", "durf\u00b7te", "dich", "nicht", "be\u00b7sit\u00b7zen", ",", "es", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$,", "PPER", "VAFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Viel Schmerz meiner Liebe beschieden.", "tokens": ["Viel", "Schmerz", "mei\u00b7ner", "Lie\u00b7be", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es war.", "tokens": ["Es", "war", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Nun ist alles Freude und Frieden.", "tokens": ["Nun", "ist", "al\u00b7les", "Freu\u00b7de", "und", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Die Sonne \u00fc