{"dta.poem.10068": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein bedeckter, doch heller Himmel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier steh' ich an dem Strand der Elbe,", "tokens": ["Hier", "steh'", "ich", "an", "dem", "Strand", "der", "El\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seh mit Lust, wie Lufft und Fluth", "tokens": ["Und", "seh", "mit", "Lust", ",", "wie", "Lufft", "und", "Fluth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In sanft und glatter Stille ruht.", "tokens": ["In", "sanft", "und", "glat\u00b7ter", "Stil\u00b7le", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zumahl ergetz\u2019 ich mich am himmlischen Gew\u00f6lbe,", "tokens": ["Zu\u00b7mahl", "er\u00b7getz'", "ich", "mich", "am", "himm\u00b7li\u00b7schen", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob gleich es ein Gewolck erf\u00fcllt,", "tokens": ["Ob", "gleich", "es", "ein", "Ge\u00b7wolck", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So zwar den Sonnen-Strahl verh\u00fcllt,", "tokens": ["So", "zwar", "den", "Son\u00b7nen\u00b7Strahl", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jedoch mit solcher klaren Pracht", "tokens": ["Je\u00b7doch", "mit", "sol\u00b7cher", "kla\u00b7ren", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich und das Firmament zu einem Schau-Platz macht,", "tokens": ["Sich", "und", "das", "Fir\u00b7ma\u00b7ment", "zu", "ei\u00b7nem", "Schau\u00b7Platz", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An dem so wenig unsre Augen,", "tokens": ["An", "dem", "so", "we\u00b7nig", "uns\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Indem er gar zu Wunder-sch\u00f6n,", "tokens": ["In\u00b7dem", "er", "gar", "zu", "Wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sich satt zu sehn,", "tokens": ["Sich", "satt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Als Hand und Feder sie recht zu beschreiben, taugen.", "tokens": ["Als", "Hand", "und", "Fe\u00b7der", "sie", "recht", "zu", "be\u00b7schrei\u00b7ben", ",", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich seh mit tausend zarten Bildern", "tokens": ["Ich", "seh", "mit", "tau\u00b7send", "zar\u00b7ten", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Von angenehm gebrochnen Wolcken, sich", "tokens": ["Von", "an\u00b7ge\u00b7nehm", "ge\u00b7broch\u00b7nen", "Wol\u00b7cken", ",", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "In s\u00fcsser Harmonie den gantzen Himmel schildern;", "tokens": ["In", "s\u00fcs\u00b7ser", "Har\u00b7mo\u00b7nie", "den", "gant\u00b7zen", "Him\u00b7mel", "schil\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Woran die Schatten selbst ein Schein,", "tokens": ["Wo\u00b7ran", "die", "Schat\u00b7ten", "selbst", "ein", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und nur von einem hellern Licht,", "tokens": ["Und", "nur", "von", "ei\u00b7nem", "hel\u00b7lern", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das hin und wieder durch sie bricht,", "tokens": ["Das", "hin", "und", "wie\u00b7der", "durch", "sie", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "KON", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dem Ansehn nach, zu Schatten worden seyn.", "tokens": ["Dem", "An\u00b7sehn", "nach", ",", "zu", "Schat\u00b7ten", "wor\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "NN", "VAPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durch diese ward das Licht noch mehr erhoben:", "tokens": ["Durch", "die\u00b7se", "ward", "das", "Licht", "noch", "mehr", "er\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unglaublich angenehm, und recht verwunderlich", "tokens": ["Un\u00b7glaub\u00b7lich", "an\u00b7ge\u00b7nehm", ",", "und", "recht", "ver\u00b7wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat ihrer Theil\u2019 unzehlbar Heer,", "tokens": ["Hat", "ih\u00b7rer", "Theil'", "un\u00b7zehl\u00b7bar", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Ansehn nach, ein Ungefehr", "tokens": ["Dem", "An\u00b7sehn", "nach", ",", "ein", "Un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So in einander eingeschoben,", "tokens": ["So", "in", "ein\u00b7an\u00b7der", "ein\u00b7ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df von verschiednen Creaturen", "tokens": ["Da\u00df", "von", "ver\u00b7schied\u00b7nen", "Crea\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Fast nicht zu zehlende Figuren", "tokens": ["Fast", "nicht", "zu", "zeh\u00b7len\u00b7de", "Fi\u00b7gu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "PTKZU", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In einem lieblich grau- und gelblich weissen Schein", "tokens": ["In", "ei\u00b7nem", "lieb\u00b7lich", "grau", "und", "gelb\u00b7lich", "weis\u00b7sen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "TRUNC", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darin zu sehen seyn.", "tokens": ["Da\u00b7rin", "zu", "se\u00b7hen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein sanft gemildert Licht ist allgemein,", "tokens": ["Ein", "sanft", "ge\u00b7mil\u00b7dert", "Licht", "ist", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00e4rbet H\u00fcgel, Th\u00e4ler, Felder,", "tokens": ["Und", "f\u00e4r\u00b7bet", "H\u00fc\u00b7gel", ",", "Th\u00e4\u00b7ler", ",", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch auf entlegnen H\u00f6hn erblickte W\u00e4lder:", "tokens": ["Auch", "auf", "ent\u00b7leg\u00b7nen", "H\u00f6hn", "er\u00b7blick\u00b7te", "W\u00e4l\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja \u00fcberall, wo B\u00e4ume, B\u00fcsch\u2019 und Hecken", "tokens": ["Ja", "\u00fc\u00b7be\u00b7rall", ",", "wo", "B\u00e4u\u00b7me", ",", "B\u00fcsch'", "und", "He\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "$,", "PWAV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Augen sich entdecken.", "tokens": ["Den", "Au\u00b7gen", "sich", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein Schatten ist bey diesem Licht zu sehn:", "tokens": ["Kein", "Schat\u00b7ten", "ist", "bey", "die\u00b7sem", "Licht", "zu", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich mercke sonderlich, mit vieler Lust, wie sch\u00f6n", "tokens": ["Ich", "mer\u00b7cke", "son\u00b7der\u00b7lich", ",", "mit", "vie\u00b7ler", "Lust", ",", "wie", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "APPR", "PIAT", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Harmonie der Himmels-Gluth", "tokens": ["Die", "Har\u00b7mo\u00b7nie", "der", "Him\u00b7mels\u00b7Gluth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch auf der Elbe glatten Fluth", "tokens": ["Auch", "auf", "der", "El\u00b7be", "glat\u00b7ten", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In solcher sanften Klarheit ruht.", "tokens": ["In", "sol\u00b7cher", "sanf\u00b7ten", "Klar\u00b7heit", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Des Wassers angenehme Gl\u00e4tte", "tokens": ["Des", "Was\u00b7sers", "an\u00b7ge\u00b7neh\u00b7me", "Gl\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist unvergleichlich sch\u00f6n,", "tokens": ["Ist", "un\u00b7ver\u00b7gleich\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und anders fast nicht anzusehn,", "tokens": ["Und", "an\u00b7ders", "fast", "nicht", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKNEG", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wenn zum Spiegel sie die Lufft gewehlet h\u00e4tte:", "tokens": ["Als", "wenn", "zum", "Spie\u00b7gel", "sie", "die", "Lufft", "ge\u00b7weh\u00b7let", "h\u00e4t\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPRART", "NN", "PPER", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und kurtz: Mir wird hiedurch entdecket,", "tokens": ["Und", "kurtz", ":", "Mir", "wird", "hie\u00b7durch", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "VAFIN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df auch so gar ein Dunst, ein Dufft", "tokens": ["Da\u00df", "auch", "so", "gar", "ein", "Dunst", ",", "ein", "Dufft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns Anmuth, Freud und Lust erwecket;", "tokens": ["Uns", "An\u00b7muth", ",", "Freud", "und", "Lust", "er\u00b7we\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und da\u00df, auch in bedeckter Lufft,", "tokens": ["Und", "da\u00df", ",", "auch", "in", "be\u00b7deck\u00b7ter", "Lufft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Noch eine Lieblichkeit", "tokens": ["Noch", "ei\u00b7ne", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und sonderbare Sch\u00f6nheit stecket.", "tokens": ["Und", "son\u00b7der\u00b7ba\u00b7re", "Sch\u00f6n\u00b7heit", "ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein GOTT! da ich hier Deine Wercke", "tokens": ["Mein", "GoTT", "!", "da", "ich", "hier", "Dei\u00b7ne", "Wer\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit inniger Bewundrung mercke,", "tokens": ["Mit", "in\u00b7ni\u00b7ger", "Be\u00b7wund\u00b7rung", "mer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ergetz\u2019 ich mich, zu Deiner Ehr.", "tokens": ["Er\u00b7getz'", "ich", "mich", ",", "zu", "Dei\u00b7ner", "Ehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! la\u00df mich ferner mehr und mehr", "tokens": ["Ach", "!", "la\u00df", "mich", "fer\u00b7ner", "mehr", "und", "mehr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie so zu mercken mich bestreben,", "tokens": ["Sie", "so", "zu", "mer\u00b7cken", "mich", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um so, in meiner Lust, zu Deinem Ruhm, zu leben!", "tokens": ["Um", "so", ",", "in", "mei\u00b7ner", "Lust", ",", "zu", "Dei\u00b7nem", "Ruhm", ",", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}