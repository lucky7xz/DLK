{"dta.poem.23746": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die M\u00fccke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Des Lichtes Glanz in dunkler Nacht", "tokens": ["Des", "Lich\u00b7tes", "Glanz", "in", "dunk\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Reizt einer M\u00fccke Unbedacht:", "tokens": ["Reizt", "ei\u00b7ner", "M\u00fc\u00b7cke", "Un\u00b7be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie spielt und nimmt nicht die Gefahr,", "tokens": ["Sie", "spielt", "und", "nimmt", "nicht", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihr das Leben kostet, wahr.", "tokens": ["Die", "ihr", "das", "Le\u00b7ben", "kos\u00b7tet", ",", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O ladet mich der goldne Schein", "tokens": ["O", "la\u00b7det", "mich", "der", "gold\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wollust dieses Lebens ein:", "tokens": ["Der", "Wol\u00b7lust", "die\u00b7ses", "Le\u00b7bens", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So denke stets mein Herz daran,", "tokens": ["So", "den\u00b7ke", "stets", "mein", "Herz", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie leicht ihr Reiz verderben kann!", "tokens": ["Wie", "leicht", "ihr", "Reiz", "ver\u00b7der\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}