{"dta.poem.18847": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Siebenschl\u00e4fer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Sechs Beg\u00fcnstigte des Hofes", "tokens": ["Sechs", "Be\u00b7g\u00fcns\u00b7tig\u00b7te", "des", "Ho\u00b7fes"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliehen vor des Kaisers Grimme,", "tokens": ["Flie\u00b7hen", "vor", "des", "Kai\u00b7sers", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der als Gott sich l\u00e4sst verehren,", "tokens": ["Der", "als", "Gott", "sich", "l\u00e4sst", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch als Gott sich nicht bew\u00e4hret:", "tokens": ["Doch", "als", "Gott", "sich", "nicht", "be\u00b7w\u00e4h\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ihn hindert eine Fliege", "tokens": ["Denn", "ihn", "hin\u00b7dert", "ei\u00b7ne", "Flie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Guter Bissen sich zu freuen.", "tokens": ["Gu\u00b7ter", "Bis\u00b7sen", "sich", "zu", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Diener scheuchen, wedlend,", "tokens": ["Sei\u00b7ne", "Die\u00b7ner", "scheu\u00b7chen", ",", "wed\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht verjagen sie die Fliege.", "tokens": ["Nicht", "ver\u00b7ja\u00b7gen", "sie", "die", "Flie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie umschw\u00e4rmt ihn, sticht und irret", "tokens": ["Sie", "um\u00b7schw\u00e4rmt", "ihn", ",", "sticht", "und", "ir\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Und verwirrt die ganze Tafel,", "tokens": ["Und", "ver\u00b7wirrt", "die", "gan\u00b7ze", "Ta\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Kehret wieder wie des h\u00e4mischen", "tokens": ["Keh\u00b7ret", "wie\u00b7der", "wie", "des", "h\u00e4\u00b7mi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KOKOM", "ART", "ADJA"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Fliegengottes Abgesandter.", "tokens": ["Flie\u00b7gen\u00b7got\u00b7tes", "Ab\u00b7ge\u00b7sand\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun! so sagen sich die Knaben,", "tokens": ["Nun", "!", "so", "sa\u00b7gen", "sich", "die", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollt\u2019 ein Flieglein Gott verhindern?", "tokens": ["Sollt'", "ein", "Flie\u00b7glein", "Gott", "ver\u00b7hin\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollt\u2019 ein Gott auch trinken, speisen,", "tokens": ["Sollt'", "ein", "Gott", "auch", "trin\u00b7ken", ",", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wir andern. Nein, der Eine", "tokens": ["Wie", "wir", "an\u00b7dern", ".", "Nein", ",", "der", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "$.", "PTKANT", "$,", "PRELS", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der die Sonn\u2019 erschuf, den Mond auch,", "tokens": ["Der", "die", "Sonn'", "er\u00b7schuf", ",", "den", "Mond", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Sterne Glut uns w\u00f6lbte,", "tokens": ["Und", "der", "Ster\u00b7ne", "Glut", "uns", "w\u00f6lb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Dieser ist\u2019s, wir fliehn! \u2014 Die zarten", "tokens": ["Die\u00b7ser", "ist's", ",", "wir", "fliehn", "!", "Die", "zar\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PPER", "VVINF", "$.", "$(", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Leicht beschuht, beputzte Knaben", "tokens": ["Leicht", "be\u00b7schuht", ",", "be\u00b7putz\u00b7te", "Kna\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nimmt ein Sch\u00e4fer auf, verbirgt sie,", "tokens": ["Nimmt", "ein", "Sch\u00e4\u00b7fer", "auf", ",", "ver\u00b7birgt", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und sich selbst in Felsenh\u00f6hle.", "tokens": ["Und", "sich", "selbst", "in", "Fel\u00b7sen\u00b7h\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sch\u00e4fershund er will nicht weichen,", "tokens": ["Sch\u00e4\u00b7fer\u00b7shund", "er", "will", "nicht", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Weggescheucht, den Fuss zerschmettert,", "tokens": ["Weg\u00b7ge\u00b7scheucht", ",", "den", "Fuss", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dr\u00e4ngt er sich an seinen Herren,", "tokens": ["Dr\u00e4ngt", "er", "sich", "an", "sei\u00b7nen", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und gesellt sich zum Verborgnen,", "tokens": ["Und", "ge\u00b7sellt", "sich", "zum", "Ver\u00b7borg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Zu den Lieblingen des Schlafes.", "tokens": ["Zu", "den", "Lieb\u00b7lin\u00b7gen", "des", "Schla\u00b7fes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und der F\u00fcrst dem sie entflohen,", "tokens": ["Und", "der", "F\u00fcrst", "dem", "sie", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebentr\u00fcstet, sinnt auf Strafen,", "tokens": ["Lie\u00b7bent\u00b7r\u00fcs\u00b7tet", ",", "sinnt", "auf", "Stra\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weisset ab so Schwerdt als Feuer,", "tokens": ["Weis\u00b7set", "ab", "so", "Schwerdt", "als", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ADV", "NE", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die H\u00f6hle sie mit Ziegeln", "tokens": ["In", "die", "H\u00f6h\u00b7le", "sie", "mit", "Zie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit Kalk sie l\u00e4sst vermauern.", "tokens": ["Und", "mit", "Kalk", "sie", "l\u00e4sst", "ver\u00b7mau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber jene schlafen immer,", "tokens": ["A\u00b7ber", "je\u00b7ne", "schla\u00b7fen", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und der Engel, ihr Besch\u00fctzer,", "tokens": ["Und", "der", "En\u00b7gel", ",", "ihr", "Be\u00b7sch\u00fct\u00b7zer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sagt, vor Gottes Thron, berichtend:", "tokens": ["Sagt", ",", "vor", "Got\u00b7tes", "Thron", ",", "be\u00b7rich\u00b7tend", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So zur Rechten, so zur Linken", "tokens": ["So", "zur", "Rech\u00b7ten", ",", "so", "zur", "Lin\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Hab\u2019 ich immer sie gewendet,", "tokens": ["Hab'", "ich", "im\u00b7mer", "sie", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dass die sch\u00f6nen, jungen Glieder", "tokens": ["Dass", "die", "sch\u00f6\u00b7nen", ",", "jun\u00b7gen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht des Moders Qualm verletze.", "tokens": ["Nicht", "des", "Mo\u00b7ders", "Qualm", "ver\u00b7let\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spalten riss ich in die Felsen", "tokens": ["Spal\u00b7ten", "riss", "ich", "in", "die", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dass die Sonne steigend, sinkend,", "tokens": ["Dass", "die", "Son\u00b7ne", "stei\u00b7gend", ",", "sin\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Junge Wangen frisch erneute.", "tokens": ["Jun\u00b7ge", "Wan\u00b7gen", "frisch", "er\u00b7neu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und so liegen sie beseligt. \u2014", "tokens": ["Und", "so", "lie\u00b7gen", "sie", "be\u00b7se\u00b7ligt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auch, auf heilen Vorderpfoten,", "tokens": ["Auch", ",", "auf", "hei\u00b7len", "Vor\u00b7der\u00b7pfo\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schl\u00e4ft das H\u00fcndlein s\u00fcssen Schlummers.", "tokens": ["Schl\u00e4ft", "das", "H\u00fcnd\u00b7lein", "s\u00fcs\u00b7sen", "Schlum\u00b7mers", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jahre fliehen, Jahre kommen,", "tokens": ["Jah\u00b7re", "flie\u00b7hen", ",", "Jah\u00b7re", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachen endlich auf die Knaben", "tokens": ["Wa\u00b7chen", "end\u00b7lich", "auf", "die", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Mauer, die vermorschte,", "tokens": ["Und", "die", "Mau\u00b7er", ",", "die", "ver\u00b7morschte", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Altershalben ist gefallen.", "tokens": ["Al\u00b7ter\u00b7shal\u00b7ben", "ist", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Jamblika sagt, der Sch\u00f6ne,", "tokens": ["Und", "Jamb\u00b7li\u00b7ka", "sagt", ",", "der", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.6": {"text": "Ausgebildete vor allen,", "tokens": ["Aus\u00b7ge\u00b7bil\u00b7de\u00b7te", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als der Sch\u00e4fer f\u00fcrchtend zaudert:", "tokens": ["Als", "der", "Sch\u00e4\u00b7fer", "f\u00fcrch\u00b7tend", "zau\u00b7dert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lauf ich hin! und hol\u2019 euch Speise,", "tokens": ["Lauf", "ich", "hin", "!", "und", "hol'", "euch", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Leben wag\u2019 ich und das Goldst\u00fcck! \u2014", "tokens": ["Le\u00b7ben", "wag'", "ich", "und", "das", "Gold\u00b7st\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ephesus, gar manches Jahr schon,", "tokens": ["E\u00b7phe\u00b7sus", ",", "gar", "man\u00b7ches", "Jahr", "schon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.11": {"text": "Ehrt die Lehre des Propheten", "tokens": ["Ehrt", "die", "Leh\u00b7re", "des", "Pro\u00b7phe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Jesus. (Friede sey dem Guten.)", "tokens": ["Je\u00b7sus", ".", "(", "Frie\u00b7de", "sey", "dem", "Gu\u00b7ten", ".", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "$(", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und er lief, da war der Thore", "tokens": ["Und", "er", "lief", ",", "da", "war", "der", "Tho\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wart und Thurn und alles anders.", "tokens": ["Wart", "und", "Thurn", "und", "al\u00b7les", "an\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "PIS", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch zum n\u00e4chsten Beckerladen", "tokens": ["Doch", "zum", "n\u00e4chs\u00b7ten", "Be\u00b7cker\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandt\u2019 er sich nach Brot in Eile. \u2014", "tokens": ["Wandt'", "er", "sich", "nach", "Brot", "in", "Ei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schelm! so rief der Becker, hast du,", "tokens": ["Schelm", "!", "so", "rief", "der", "Be\u00b7cker", ",", "hast", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ART", "NE", "$,", "VAFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "J\u00fcngling, einen Schatz gefunden!", "tokens": ["J\u00fcng\u00b7ling", ",", "ei\u00b7nen", "Schatz", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gieb mir, dich verr\u00e4th das Goldst\u00fcck,", "tokens": ["Gieb", "mir", ",", "dich", "ver\u00b7r\u00e4\u00b7th", "das", "Gold\u00b7st\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Mir die H\u00e4lfte zum Vers\u00f6hnen!", "tokens": ["Mir", "die", "H\u00e4lf\u00b7te", "zum", "Ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und sie hadern. \u2014 Vor den K\u00f6nig", "tokens": ["Und", "sie", "ha\u00b7dern", ".", "Vor", "den", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$.", "$(", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kommt der Handel; auch der K\u00f6nig", "tokens": ["Kommt", "der", "Han\u00b7del", ";", "auch", "der", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will nur theilen wie der Becker.", "tokens": ["Will", "nur", "thei\u00b7len", "wie", "der", "Be\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "KOKOM", "ART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun beth\u00e4tigt sich das Wunder,", "tokens": ["Nun", "be\u00b7th\u00e4\u00b7tigt", "sich", "das", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach und nach, aus hundert Zeichen.", "tokens": ["Nach", "und", "nach", ",", "aus", "hun\u00b7dert", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "$,", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem selbsterbauten Pallast", "tokens": ["An", "dem", "selbs\u00b7ter\u00b7bau\u00b7ten", "Pal\u00b7last"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiss er sich sein Recht zu sichern.", "tokens": ["Weiss", "er", "sich", "sein", "Recht", "zu", "si\u00b7chern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ein Pfeiler durchgegraben", "tokens": ["Denn", "ein", "Pfei\u00b7ler", "durch\u00b7ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchrt zu scharfbenamsten Sch\u00e4tzen.", "tokens": ["F\u00fchrt", "zu", "scharf\u00b7be\u00b7nams\u00b7ten", "Sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gleich versammlen sich Geschlechter", "tokens": ["Gleich", "ver\u00b7samm\u00b7len", "sich", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihre Sippschaft zu beweisen.", "tokens": ["Ih\u00b7re", "Sipp\u00b7schaft", "zu", "be\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und als Ururvater prangend", "tokens": ["Und", "als", "U\u00b7rur\u00b7va\u00b7ter", "pran\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Steht Jamblikas Jugendf\u00fclle.", "tokens": ["Steht", "Jamb\u00b7li\u00b7kas", "Ju\u00b7gend\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Wie von Ahnherrn h\u00f6rt er sprechen", "tokens": ["Wie", "von", "Ahn\u00b7herrn", "h\u00f6rt", "er", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hier von seinem Sohn und Enkeln.", "tokens": ["Hier", "von", "sei\u00b7nem", "Sohn", "und", "En\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Der Urenkel Schaar umgiebt ihn,", "tokens": ["Der", "Ur\u00b7en\u00b7kel", "Schaar", "um\u00b7giebt", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Als ein Volk von tapfern M\u00e4nnern,", "tokens": ["Als", "ein", "Volk", "von", "tap\u00b7fern", "M\u00e4n\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ihn den j\u00fcngsten zu verehren.", "tokens": ["Ihn", "den", "j\u00fcng\u00b7sten", "zu", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und ein Merkmal \u00fcbers andre", "tokens": ["Und", "ein", "Merk\u00b7mal", "\u00fc\u00b7bers", "and\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Dringt sich auf, Beweis vollendend;", "tokens": ["Dringt", "sich", "auf", ",", "Be\u00b7weis", "voll\u00b7en\u00b7dend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "$,", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sich und den Gef\u00e4hrten hat er", "tokens": ["Sich", "und", "den", "Ge\u00b7f\u00e4hr\u00b7ten", "hat", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "KON", "ART", "NN", "VAFIN", "PPER"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Die Pers\u00f6nlichkeit best\u00e4tigt.", "tokens": ["Die", "Per\u00b7s\u00f6n\u00b7lich\u00b7keit", "be\u00b7st\u00e4\u00b7tigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun, zur H\u00f6hle kehrt er wieder,", "tokens": ["Nun", ",", "zur", "H\u00f6h\u00b7le", "kehrt", "er", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Volk und K\u00f6nig ihn geleiten. \u2014", "tokens": ["Volk", "und", "K\u00f6\u00b7nig", "ihn", "ge\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zum K\u00f6nig, nicht zum Volke", "tokens": ["Nicht", "zum", "K\u00f6\u00b7nig", ",", "nicht", "zum", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrt der Auserw\u00e4hlte wieder:", "tokens": ["Kehrt", "der", "Au\u00b7ser\u00b7w\u00e4hl\u00b7te", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die Sieben, die von lang\u2019 her,", "tokens": ["Denn", "die", "Sie\u00b7ben", ",", "die", "von", "lang'", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "ADV", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Achte waren\u2019s mit dem Hunde,", "tokens": ["Ach\u00b7te", "wa\u00b7ren's", "mit", "dem", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Sich von aller Welt gesondert,", "tokens": ["Sich", "von", "al\u00b7ler", "Welt", "ge\u00b7son\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gabriels geheim Verm\u00f6gen", "tokens": ["Ga\u00b7bri\u00b7els", "ge\u00b7heim", "Ver\u00b7m\u00f6\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hat, gem\u00e4ss dem Willen Gottes,", "tokens": ["Hat", ",", "ge\u00b7m\u00e4ss", "dem", "Wil\u00b7len", "Got\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie dem Paradies geeignet,", "tokens": ["Sie", "dem", "Pa\u00b7ra\u00b7dies", "ge\u00b7eig\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die H\u00f6hle schien vermauert.", "tokens": ["Und", "die", "H\u00f6h\u00b7le", "schien", "ver\u00b7mau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}