ext": "O wie wohl wird mir von innen!", "tokens": ["O", "wie", "wohl", "wird", "mir", "von", "in\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "PPER", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erd' und Erdenqual zerrinnen,", "tokens": ["Erd'", "und", "Er\u00b7den\u00b7qual", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit allen meinen Sinnen", "tokens": ["Und", "mit", "al\u00b7len", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchle, habe, lieb' ich dich.", "tokens": ["F\u00fch\u00b7le", ",", "ha\u00b7be", ",", "lieb'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Fahre hin, du Erdental!", "tokens": ["Fah\u00b7re", "hin", ",", "du", "Er\u00b7den\u00b7tal", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon bin ich im Himmelssaal,", "tokens": ["Schon", "bin", "ich", "im", "Him\u00b7mels\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebe auf den sel'gen H\u00f6hen,", "tokens": ["Schwe\u00b7be", "auf", "den", "sel'\u00b7gen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Zehnmaltausend stehen", "tokens": ["Wo", "die", "Zehn\u00b7mal\u00b7tau\u00b7send", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Lobgesang erh\u00f6hen", "tokens": ["Und", "den", "Lob\u00b7ge\u00b7sang", "er\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den Frommen allzumal.", "tokens": ["Mit", "den", "From\u00b7men", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}