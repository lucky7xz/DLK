{"textgrid.poem.67066": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Ehre", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst war ich ruhig, schlummerte sorgenfrei", "tokens": ["Einst", "war", "ich", "ru\u00b7hig", ",", "schlum\u00b7mer\u00b7te", "sor\u00b7gen\u00b7frei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am stillen Moosquell, tr\u00e4umte von Stellas Ku\u00df \u2013", "tokens": ["Am", "stil\u00b7len", "Moos\u00b7quell", ",", "tr\u00e4um\u00b7te", "von", "Stel\u00b7las", "Ku\u00df", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NE", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da riefst du, da\u00df der Waldstrom stille", "tokens": ["Da", "riefst", "du", ",", "da\u00df", "der", "Wald\u00b7strom", "stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stand und erbebte, vom Eichenwipfel \u2013", "tokens": ["Stand", "und", "er\u00b7beb\u00b7te", ",", "vom", "Ei\u00b7chen\u00b7wip\u00b7fel", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "APPRART", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Auf sprang ich, f\u00fchlte taumelnd die Zauberkraft,", "tokens": ["Auf", "sprang", "ich", ",", "f\u00fchl\u00b7te", "tau\u00b7melnd", "die", "Z