{"textgrid.poem.52092": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich nechst meinen Freund und seinen Tod beklagt,", "tokens": ["Als", "ich", "nechst", "mei\u00b7nen", "Freund", "und", "sei\u00b7nen", "Tod", "be\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach Sylvius zu mir: Er ist ein Mensch gewesen:", "tokens": ["Sprach", "Syl\u00b7vius", "zu", "mir", ":", "Er", "ist", "ein", "Mensch", "ge\u00b7we\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Wenn dich sein fr\u00fcher Tod gleich gar zu tode plagt,", "tokens": ["Wenn", "dich", "sein", "fr\u00fc\u00b7her", "Tod", "gleich", "gar", "zu", "to\u00b7de", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch wirst Du weder Er durch solche Pein genesen.", "tokens": ["Doch", "wirst", "Du", "we\u00b7der", "Er", "durch", "sol\u00b7che", "Pein", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "KON", "PPER", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gedenck ich trage schon das f\u00fcnffte Weib hinaus,", "tokens": ["Ge\u00b7denck", "ich", "tra\u00b7ge", "schon", "das", "f\u00fcnff\u00b7te", "Weib", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(und lebe doch) wie sch\u00f6n und reich sie zu erheben:", "tokens": ["(", "und", "le\u00b7be", "doch", ")", "wie", "sch\u00f6n", "und", "reich", "sie", "zu", "er\u00b7he\u00b7ben", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ADV", "$(", "PWAV", "ADJD", "KON", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie weis' ist Sylvius, er erbt so manches Hau\u00df", "tokens": ["Wie", "weis'", "ist", "Syl\u00b7vius", ",", "er", "erbt", "so", "man\u00b7ches", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "NE", "$,", "PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und manchen Sack voll Geld, und wil dabey noch leben.", "tokens": ["Und", "man\u00b7chen", "Sack", "voll", "Geld", ",", "und", "wil", "da\u00b7bey", "noch", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "NN", "$,", "KON", "VMFIN", "PAV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als ich nechst meinen Freund und seinen Tod beklagt,", "tokens": ["Als", "ich",