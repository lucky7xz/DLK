{"textgrid.poem.53526": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Bruch", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was aber wird nun aus der Siegsallee?", "tokens": ["Was", "a\u00b7ber", "wird", "nun", "aus", "der", "Siegs\u00b7al\u00b7lee", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird man dieselbe, weil zu royalistisch,", "tokens": ["Wird", "man", "die\u00b7sel\u00b7be", ",", "weil", "zu", "ro\u00b7ya\u00b7lis\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PDAT", "$,", "KOUS", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zu autokratisch und zu monarchistisch,", "tokens": ["zu", "au\u00b7to\u00b7kra\u00b7tisch", "und", "zu", "mon\u00b7ar\u00b7chis\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "abfahren in den Neuen See?", "tokens": ["ab\u00b7fah\u00b7ren", "in", "den", "Neu\u00b7en", "See", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4\u00dft man bei jedem Denkmal die Statur?", "tokens": ["L\u00e4\u00dft", "man", "bei", "je\u00b7dem", "Denk\u00b7mal", "die", "Sta\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-