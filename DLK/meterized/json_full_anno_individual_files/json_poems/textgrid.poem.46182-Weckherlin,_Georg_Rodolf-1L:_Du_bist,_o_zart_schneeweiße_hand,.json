{"textgrid.poem.46182": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist, o zart schneewei\u00dfe hand,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist, o zart schneewei\u00dfe hand,", "tokens": ["Du", "bist", ",", "o", "zart", "schnee\u00b7wei\u00b7\u00dfe", "hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der beste zeug, die lieb zu weben,", "tokens": ["der", "bes\u00b7te", "zeug", ",", "die", "lieb", "zu", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "du bist der treu erw\u00fcnschtes pfand,", "tokens": ["du", "bist", "der", "treu", "er\u00b7w\u00fcnschtes", "pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "damit man kan vern\u00fcget leben;", "tokens": ["da\u00b7mit", "man", "kan", "ver\u00b7n\u00fc\u00b7get", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "wan auf der lieb, bit oder frag", "tokens": ["wan", "auf", "der", "lieb", ",", "bit", "o\u00b7der", "frag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJD", "$,", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "du wilt zu fridlichem vertrag", "tokens": ["du", "wilt", "zu", "frid\u00b7li\u00b7chem", "ver\u00b7trag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "stillschweigend deine zusag geben.", "tokens": ["still\u00b7schwei\u00b7gend", "dei\u00b7ne", "zu\u00b7sag", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du kanst, o hand, bald den verdru\u00df,", "tokens": ["Du", "kanst", ",", "o", "hand", ",", "bald", "den", "ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "FM", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bald das gefallen verursachen,", "tokens": ["bald", "das", "ge\u00b7fal\u00b7len", "ver\u00b7ur\u00b7sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "bezeugend zweier lieb beschlu\u00df,", "tokens": ["be\u00b7zeu\u00b7gend", "zwei\u00b7er", "lieb", "be\u00b7schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bist du die ursach, da\u00df sie lachen;", "tokens": ["bist", "du", "die", "ur\u00b7sach", ",", "da\u00df", "sie", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "gleichwie wan du der buhler scho\u00df", "tokens": ["gleich\u00b7wie", "wan", "du", "der", "buh\u00b7ler", "scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "zur\u00fcck versto\u00dfest hofnungslos,", "tokens": ["zu\u00b7r\u00fcck", "ver\u00b7sto\u00b7\u00dfest", "hof\u00b7nungs\u00b7los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "du sie kanst leichtlich weinen machen.", "tokens": ["du", "sie", "kanst", "leicht\u00b7lich", "wei\u00b7nen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VMFIN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schnell, laufend, zittrend und kunstreich", "tokens": ["Schnell", ",", "lau\u00b7fend", ",", "zitt\u00b7rend", "und", "kuns\u00b7treich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVPP", "KON", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "kanst du das instrument ber\u00fchren:", "tokens": ["kanst", "du", "das", "inst\u00b7ru\u00b7ment", "be\u00b7r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und der wolredenheit recht gleich", "tokens": ["und", "der", "wol\u00b7re\u00b7den\u00b7heit", "recht", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "in das geh\u00f6r die herzen f\u00fchren:", "tokens": ["in", "das", "ge\u00b7h\u00f6r", "die", "her\u00b7zen", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und mit schneewei\u00dfem hellen glanz", "tokens": ["und", "mit", "schnee\u00b7wei\u00b7\u00dfem", "hel\u00b7len", "glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "von deiner fingern leichtem danz", "tokens": ["von", "dei\u00b7ner", "fin\u00b7gern", "leich\u00b7tem", "danz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "den ton, die stim, die saiten zieren.", "tokens": ["den", "ton", ",", "die", "stim", ",", "die", "sai\u00b7ten", "zie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "PRELS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was immer uns der natur gunst", "tokens": ["Was", "im\u00b7mer", "uns", "der", "na\u00b7tur", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "verleihet, kanst du bald vergleichen", "tokens": ["ver\u00b7lei\u00b7het", ",", "kanst", "du", "bald", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und es mit farbenreicher kunst", "tokens": ["und", "es", "mit", "far\u00b7ben\u00b7rei\u00b7cher", "kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bald \u00fcberk\u00fcnstlich herausstreichen:", "tokens": ["bald", "\u00fc\u00b7ber\u00b7k\u00fcnst\u00b7lich", "her\u00b7aus\u00b7strei\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "du kanst mit des verstands gesatz", "tokens": ["du", "kanst", "mit", "des", "ver\u00b7stands", "ge\u00b7satz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und der gedanken gro\u00dfem schatz", "tokens": ["und", "der", "ge\u00b7dan\u00b7ken", "gro\u00b7\u00dfem", "schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "getreulich das papier bereichen.", "tokens": ["ge\u00b7treu\u00b7lich", "das", "pa\u00b7pier", "be\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In meiner hand, o s\u00fc\u00dfer lohn!", "tokens": ["In", "mei\u00b7ner", "hand", ",", "o", "s\u00fc\u00b7\u00dfer", "lohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wan du, o sch\u00f6ne hand, gefangen,", "tokens": ["wan", "du", ",", "o", "sch\u00f6\u00b7ne", "hand", ",", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "FM", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "gedrucket druckest du mit wohn", "tokens": ["ge\u00b7dru\u00b7cket", "dru\u00b7ckest", "du", "mit", "wohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und lust mein herz, hand und verlangen:", "tokens": ["und", "lust", "mein", "herz", ",", "hand", "und", "ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "gefangen fanget deine zucht", "tokens": ["ge\u00b7fan\u00b7gen", "fan\u00b7get", "dei\u00b7ne", "zucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "also, da\u00df niemals durch die flucht,", "tokens": ["al\u00b7so", ",", "da\u00df", "nie\u00b7mals", "durch", "die", "flucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "APPR", "ART", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "der dich gefangen, dir entgangen.", "tokens": ["der", "dich", "ge\u00b7fan\u00b7gen", ",", "dir", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKVZ", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gehalten bist du stolz und frei,", "tokens": ["Ge\u00b7hal\u00b7ten", "bist", "du", "stolz", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du \u00fcberwindest mich gebunden,", "tokens": ["du", "\u00fc\u00b7berw\u00b7in\u00b7dest", "mich", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "du bist getreu und ungetreu,", "tokens": ["du", "bist", "ge\u00b7treu", "und", "un\u00b7ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als triumfierend \u00fcberwunden:", "tokens": ["als", "tri\u00b7um\u00b7fie\u00b7rend", "\u00fc\u00b7berw\u00b7un\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "du pflegest mit unruh und ruh", "tokens": ["du", "pfle\u00b7gest", "mit", "un\u00b7ruh", "und", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "je sanfter und je blo\u00dfer du,", "tokens": ["je", "sanf\u00b7ter", "und", "je", "blo\u00b7\u00dfer", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "je tiefer mein herz zu verwunden.", "tokens": ["je", "tie\u00b7fer", "mein", "herz", "zu", "ver\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "O zartes, glattes helfenbein,", "tokens": ["O", "zar\u00b7tes", ",", "glat\u00b7tes", "hel\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "punct", "word", "