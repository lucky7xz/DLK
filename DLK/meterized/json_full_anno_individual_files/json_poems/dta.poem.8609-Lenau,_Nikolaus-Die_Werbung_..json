{"dta.poem.8609": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Werbung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Rings im Kreise lauscht die Menge", "tokens": ["Rings", "im", "Krei\u00b7se", "lauscht", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00e4rtiger Magyaren froh;", "tokens": ["B\u00e4r\u00b7ti\u00b7ger", "Ma\u00b7gy\u00b7a\u00b7ren", "froh", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aus dem Kreise rauschen Kl\u00e4nge,", "tokens": ["Aus", "dem", "Krei\u00b7se", "rau\u00b7schen", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ergreifen die mich so? \u2014", "tokens": ["Was", "er\u00b7grei\u00b7fen", "die", "mich", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tiefgebr\u00e4unt vom Sonnenbrande,", "tokens": ["Tief\u00b7ge\u00b7br\u00e4unt", "vom", "Son\u00b7nen\u00b7bran\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rothgegl\u00fcht von Weinesgluth,", "tokens": ["Roth\u00b7ge\u00b7gl\u00fcht", "von", "Wei\u00b7nes\u00b7gluth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Spielt da die Zigeunerbande,", "tokens": ["Spielt", "da", "die", "Zi\u00b7geu\u00b7ner\u00b7ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und emp\u00f6rt das Heldenblut.", "tokens": ["Und", "em\u00b7p\u00f6rt", "das", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u201ela\u00df die Geige wilder singen!", "tokens": ["\u201e", "la\u00df", "die", "Gei\u00b7ge", "wil\u00b7der", "sin\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "\u201ewilder schlag' das Zimbal du!\u201c", "tokens": ["\u201e", "wil\u00b7der", "schlag'", "das", "Zim\u00b7bal", "du", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "VVFIN", "ART", "NN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ruft der Werber, und es klingen", "tokens": ["Ruft", "der", "Wer\u00b7ber", ",", "und", "es", "klin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Seine Sporne hell dazu.", "tokens": ["Sei\u00b7ne", "Spor\u00b7ne", "hell", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Der Zigeuner h\u00f6rts, und voller", "tokens": ["Der", "Zi\u00b7geu\u00b7ner", "h\u00f6rts", ",", "und", "vol\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KON", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.14": {"text": "W\u00f6lkt sein Mund der Pfeife Dampf;", "tokens": ["W\u00f6lkt", "sein", "Mund", "der", "Pfei\u00b7fe", "Dampf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Lauter immer, immer toller", "tokens": ["Lau\u00b7ter", "im\u00b7mer", ",", "im\u00b7mer", "tol\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$,", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Braust der Instrumente Kampf,", "tokens": ["Braust", "der", "Inst\u00b7ru\u00b7men\u00b7te", "Kampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.17": {"text": "Braust die alte Heldenweise,", "tokens": ["Braust", "die", "al\u00b7te", "Hel\u00b7den\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die vor Zeiten wohl mit Macht", "tokens": ["Die", "vor", "Zei\u00b7ten", "wohl", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Frische Knaben, welke Greise", "tokens": ["Fri\u00b7sche", "Kna\u00b7ben", ",", "wel\u00b7ke", "Grei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Hinzog in die T\u00fcrkenschlacht.", "tokens": ["Hin\u00b7zog", "in", "die", "T\u00fcr\u00b7ken\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Wie des Werbers Augen gl\u00fch'n!", "tokens": ["Wie", "des", "Wer\u00b7bers", "Au\u00b7gen", "gl\u00fch'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Und wie all' die S\u00e4belnarben,", "tokens": ["Und", "wie", "all'", "die", "S\u00e4\u00b7bel\u00b7nar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Ehrenr\u00f6slein, purpurfarben,", "tokens": ["Eh\u00b7ren\u00b7r\u00f6s\u00b7lein", ",", "pur\u00b7purf\u00b7ar\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ihm auf Wang' und Stirne bl\u00fch'n!", "tokens": ["Ihm", "auf", "Wang'", "und", "Stir\u00b7ne", "bl\u00fch'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.25": {"text": "Klirrend gl\u00e4nzt das Schwert in Funken,", "tokens": ["Klir\u00b7rend", "gl\u00e4nzt", "das", "Schwert", "in", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Das sich oft im Blute wusch;", "tokens": ["Das", "sich", "oft", "im", "Blu\u00b7te", "wusch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.27": {"text": "Auf dem Csako, freudetrunken,", "tokens": ["Auf", "dem", "Csa\u00b7ko", ",", "freu\u00b7de\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Taumelt ihm der Federbusch. \u2014", "tokens": ["Tau\u00b7melt", "ihm", "der", "Fe\u00b7der\u00b7busch", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Aus der bunten Menge ragen", "tokens": ["Aus", "der", "bun\u00b7ten", "Men\u00b7ge", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Einen J\u00fcngling, stark und hoch,", "tokens": ["Ei\u00b7nen", "J\u00fcng\u00b7ling", ",", "stark", "und", "hoch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Sieht der Werber mit Behagen;", "tokens": ["Sieht", "der", "Wer\u00b7ber", "mit", "Be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "\u201ew\u00e4rest du ein Reiter doch!\u201c", "tokens": ["\u201e", "w\u00e4\u00b7rest", "du", "ein", "Rei\u00b7ter", "doch", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Ruft er aus mit lichtren Augen;", "tokens": ["Ruft", "er", "aus", "mit", "licht\u00b7ren", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "\u201esolcher Wuchs und solche Kraft", "tokens": ["\u201e", "sol\u00b7cher", "Wuchs", "und", "sol\u00b7che", "Kraft"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "\u201ew\u00fcrden dem Husaren taugen;", "tokens": ["\u201e", "w\u00fcr\u00b7den", "dem", "Hu\u00b7sa\u00b7ren", "tau\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "\u201ekomm und trinke Br\u00fcderschaft!\u201c", "tokens": ["\u201e", "komm", "und", "trin\u00b7ke", "Br\u00fc\u00b7der\u00b7schaft", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "KON", "VVFIN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Und es schwingt der freudigrasche", "tokens": ["Und", "es", "schwingt", "der", "freu\u00b7di\u00b7gra\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Jenem zu die volle Flasche.", "tokens": ["Je\u00b7nem", "zu", "die", "vol\u00b7le", "Fla\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Doch der J\u00fcngling h\u00f6rt es schweigend,", "tokens": ["Doch", "der", "J\u00fcng\u00b7ling", "h\u00f6rt", "es", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "In die Schatten der Gedanken,", "tokens": ["In", "die", "Schat\u00b7ten", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Die ihn bang und s\u00fc\u00df umranken,", "tokens": ["Die", "ihn", "bang", "und", "s\u00fc\u00df", "um\u00b7ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Still sein sch\u00f6nes Antlitz neigend.", "tokens": ["Still", "sein", "sch\u00f6\u00b7nes", "Ant\u00b7litz", "nei\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Ihn bewegt das edle Sehnen,", "tokens": ["Ihn", "be\u00b7wegt", "das", "ed\u00b7le", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Wie der Ahn ein Held zu seyn;", "tokens": ["Wie", "der", "Ahn", "ein", "Held", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Doch berieseln warme Thr\u00e4nen", "tokens": ["Doch", "be\u00b7rie\u00b7seln", "war\u00b7me", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Seiner Wangen Rosenschein.", "tokens": ["Sei\u00b7ner", "Wan\u00b7gen", "Ro\u00b7sen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Ausser denen, die da rauschen", "tokens": ["Aus\u00b7ser", "de\u00b7nen", ",", "die", "da", "rau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "$,", "PRELS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "In Musik, in Werberswort,", "tokens": ["In", "Mu\u00b7sik", ",", "in", "Wer\u00b7bers\u00b7wort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Scheint er Kl\u00e4ngen noch zu lauschen,", "tokens": ["Scheint", "er", "Kl\u00e4n\u00b7gen", "noch", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Hergeweht aus fernem Ort:", "tokens": ["Her\u00b7ge\u00b7weht", "aus", "fer\u00b7nem", "Ort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "\u201ekomm zur\u00fcck in meine Arme!\u201c", "tokens": ["\u201e", "komm", "zu\u00b7r\u00fcck", "in", "mei\u00b7ne", "Ar\u00b7me", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Fleht sein M\u00fctterlein so bang;", "tokens": ["Fleht", "sein", "M\u00fct\u00b7ter\u00b7lein", "so", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Und die Braut in ihrem Harme", "tokens": ["Und", "die", "Braut", "in", "ih\u00b7rem", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Fleht: \u201eO s\u00e4ume nimmer lang!\u201c", "tokens": ["Fleht", ":", "\u201e", "O", "s\u00e4u\u00b7me", "nim\u00b7mer", "lang", "!", "\u201c"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "$(", "NE", "VVFIN", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Und er sieht das H\u00fcttchen trauern,", "tokens": ["Und", "er", "sieht", "das", "H\u00fctt\u00b7chen", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Das ihn hegte mit den Seinen;", "tokens": ["Das", "ihn", "heg\u00b7te", "mit", "den", "Sei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ART", "PPOSS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "H\u00f6rt davor die Linde schauern,", "tokens": ["H\u00f6rt", "da\u00b7vor", "die", "Lin\u00b7de", "schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Und den Bach vor\u00fcberweinen. \u2014", "tokens": ["Und", "den", "Bach", "vor\u00b7\u00fc\u00