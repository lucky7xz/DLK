{"dta.poem.9230": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n Ein ander mahl schw\u00e4rtze mehr die Banck.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.71", "af:0.28"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Das heist die banck geschw\u00e4rtzt/ wann man die pr\u00fc-", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "/", "wann", "man", "die", "pr\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$(", "PWAV", "PIS", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum macher-lohne kriegt/ ", "tokens": ["Zum", "ma\u00b7cher\u00b7loh\u00b7ne", "kriegt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man zu boden liegt/", "tokens": ["Da\u00df", "man", "zu", "bo\u00b7den", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und sich nicht wohl besinnt/ was einen in der juppe", "tokens": ["Und", "sich", "nicht", "wohl", "be\u00b7sinnt", "/", "was", "ei\u00b7nen", "in", "der", "jup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "PTKNEG", "ADV", "VVPP", "$(", "PWS", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.5": {"text": "So schrecklich und abscheulich schmertzt.", "tokens": ["So", "schreck\u00b7lich", "und", "ab\u00b7scheu\u00b7lich", "schmertzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das heist die banck geschw\u00e4rtzt.", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Das kleid ist gantz verderbt/ ", "tokens": ["Das", "kleid", "ist", "gantz", "ver\u00b7derbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat sich schwartz gef\u00e4rbt/", "tokens": ["Und", "hat", "sich", "schwartz", "ge\u00b7f\u00e4rbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zweyhundert thaler gehn auf einmal vor die hunde;", "tokens": ["Zwey\u00b7hun\u00b7dert", "tha\u00b7ler", "gehn", "auf", "ein\u00b7mal", "vor", "die", "hun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum ist die gnade gantz verschertzt/", "tokens": ["Drum", "ist", "die", "gna\u00b7de", "gantz", "ver\u00b7schertzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das heist die banck geschw\u00e4rtzt.", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Das heist die banck geschw\u00e4rtzt/ der herr hats so", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "/", "der", "herr", "hats", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$(", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun mu\u00df der junge dran/ ", "tokens": ["Nun", "mu\u00df", "der", "jun\u00b7ge", "dran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hat nun all\u2019s gethan/", "tokens": ["Der", "hat", "nun", "all's", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und l\u00e4st sich unverdient bi\u00df auf den tod zerschmeissen/", "tokens": ["Und", "l\u00e4st", "sich", "un\u00b7ver\u00b7di\u00b7ent", "bi\u00df", "auf", "den", "tod", "zer\u00b7schmeis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es ist ein bi\u00dfgen grob geschertzt/", "tokens": ["Es", "ist", "ein", "bi\u00df\u00b7gen", "grob", "ge\u00b7schertzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das heist die banck geschw\u00e4rtzt.", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Das heist die banck geschw\u00e4rtzt/ so kom\u0303t ein Mah-", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "/", "so", "kom\u0303t", "ein", "Mah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$(", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum schl\u00e4gen gar zu fr\u00fch/ ", "tokens": ["Zum", "schl\u00e4\u00b7gen", "gar", "zu", "fr\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wei\u00df f\u00fcrwahr nicht wie/", "tokens": ["Und", "wei\u00df", "f\u00fcr\u00b7wahr", "nicht", "wie", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "KOKOM", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der herr begehrt ihn nicht/ er stehet auff dem sprunge", "tokens": ["Der", "herr", "be\u00b7gehrt", "ihn", "nicht", "/", "er", "ste\u00b7het", "auff", "dem", "sprun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "PPER", "PTKNEG", "$(", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wird bey allen ausgemertzt/", "tokens": ["Und", "wird", "bey", "al\u00b7len", "aus\u00b7ge\u00b7mertzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das heist die banck geschw\u00e4rtzt.", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Das heist die banck geschw\u00e4rtzt/ das heist ein sch\u00f6n", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "/", "das", "heist", "ein", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$(", "PDS", "VAFIN", "ART", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo iemand dieses spiel ", "tokens": ["Wo", "ie\u00b7mand", "die\u00b7ses", "spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch einmahl sehen wil", "tokens": ["Noch", "ein\u00b7mahl", "se\u00b7hen", "wil"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So geh und schw\u00e4rtz er nur den leuten ihre b\u00e4ncke/", "tokens": ["So", "geh", "und", "schw\u00e4rtz", "er", "nur", "den", "leu\u00b7ten", "ih\u00b7re", "b\u00e4n\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird er auch so h\u00fcbsch gehertzt/", "tokens": ["So", "wird", "er", "auch", "so", "h\u00fcbsch", "ge\u00b7hertzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das heist die banck geschw\u00e4rtzt.", "tokens": ["Das", "heist", "die", "banck", "ge\u00b7schw\u00e4rtzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}