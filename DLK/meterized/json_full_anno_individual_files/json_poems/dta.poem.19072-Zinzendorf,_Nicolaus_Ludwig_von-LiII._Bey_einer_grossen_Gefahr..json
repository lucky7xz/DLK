{"dta.poem.19072": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LiII.   Bey einer grossen Gefahr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Der Glaube bricht durch Stahl und Stein,", "tokens": ["Der", "Glau\u00b7be", "bricht", "durch", "Stahl", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fa\u00dft die Allmacht selber;", "tokens": ["Und", "fa\u00dft", "die", "All\u00b7macht", "sel\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Glaube wircket mehr allein,", "tokens": ["Der", "Glau\u00b7be", "wir\u00b7cket", "mehr", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als alle g\u00fcldne K\u00e4lber,", "tokens": ["Als", "al\u00b7le", "g\u00fcld\u00b7ne", "K\u00e4l\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn einer nichts, als glauben kan,", "tokens": ["Wenn", "ei\u00b7ner", "nichts", ",", "als", "glau\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "$,", "KOUS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kan er alles machen;", "tokens": ["So", "kan", "er", "al\u00b7les", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Erden Kr\u00e4fte sieht er an,", "tokens": ["Der", "Er\u00b7den", "Kr\u00e4f\u00b7te", "sieht", "er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als gantz geringe Sachen.", "tokens": ["Als", "gantz", "ge\u00b7rin\u00b7ge", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als JEsus noch nicht ausgelegt", "tokens": ["Als", "Je\u00b7sus", "noch", "nicht", "aus\u00b7ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00e4tze seiner H\u00f6hen,", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "sei\u00b7ner", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch eh man den, der alles tr\u00e4gt,", "tokens": ["Noch", "eh", "man", "den", ",", "der", "al\u00b7les", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Erden wandeln sehen,", "tokens": ["Auf", "Er\u00b7den", "wan\u00b7deln", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da thaten, die auf seinen Tag", "tokens": ["Da", "tha\u00b7ten", ",", "die", "auf", "sei\u00b7nen", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich freuten, lauter Wunder.", "tokens": ["Sich", "freu\u00b7ten", ",", "lau\u00b7ter", "Wun\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was kan man, (wers begreiffen mag,)", "tokens": ["Was", "kan", "man", ",", "(", "wers", "be\u00b7greif\u00b7fen", "mag", ",", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PIS", "$,", "$(", "ADV", "VVINF", "VMFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was wagt man nicht itzunder?", "tokens": ["Was", "wagt", "man", "nicht", "it\u00b7zun\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In Wahrheit, wenn das Christen-Volck", "tokens": ["In", "Wahr\u00b7heit", ",", "wenn", "das", "Chris\u00b7ten\u00b7Volck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur wolte, was es k\u00f6nte,", "tokens": ["Nur", "wol\u00b7te", ",", "was", "es", "k\u00f6n\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sich der Zeugen stoltze Wolck", "tokens": ["Wenn", "sich", "der", "Zeu\u00b7gen", "stolt\u00b7ze", "Wolck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf JEsus Winck zertrennte,", "tokens": ["Auf", "Je\u00b7sus", "Win\u00b7ck", "zer\u00b7trenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie st\u00fcrtzete das gantze Heer", "tokens": ["Sie", "st\u00fcrt\u00b7ze\u00b7te", "das", "gant\u00b7ze", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der fremden Kinder nieder,", "tokens": ["Der", "frem\u00b7den", "Kin\u00b7der", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und z\u00f6ge sich nur destomehr", "tokens": ["Und", "z\u00f6\u00b7ge", "sich", "nur", "des\u00b7to\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu ihrer Sonne wieder.", "tokens": ["Zu", "ih\u00b7rer", "Son\u00b7ne", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Starcken um des Salomo", "tokens": ["Die", "Star\u00b7cken", "um", "des", "Sa\u00b7lo\u00b7mo"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des K\u00f6nigs Ehren-Bette,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Eh\u00b7ren\u00b7Bet\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die weichen nicht, wie leichtes Stroh,", "tokens": ["Die", "wei\u00b7chen", "nicht", ",", "wie", "leich\u00b7tes", "Stroh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie stehn, als eine Kette;", "tokens": ["Sie", "stehn", ",", "als", "ei\u00b7ne", "Ket\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie stehn, und schweiffen nirgends hin,", "tokens": ["Sie", "stehn", ",", "und", "schwei\u00b7ffen", "nir\u00b7gends", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was aber sie bef\u00e4llet,", "tokens": ["Was", "a\u00b7ber", "sie", "be\u00b7f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das wird vor seinen Frevel-Sinn", "tokens": ["Das", "wird", "vor", "sei\u00b7nen", "Fre\u00b7vel\u00b7Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jm Zorn zur\u00fcck gequellet.", "tokens": ["Jm", "Zorn", "zu\u00b7r\u00fcck", "ge\u00b7quel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gelobet sey die Tapferkeit", "tokens": ["Ge\u00b7lo\u00b7bet", "sey", "die", "Tap\u00b7fer\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Streiter unsers F\u00fcrsten,", "tokens": ["Der", "Strei\u00b7ter", "un\u00b7sers", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verlacht sey die Verwegenheit", "tokens": ["Ver\u00b7lacht", "sey", "die", "Ver\u00b7we\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach ihrem Blut zu d\u00fcrsten.", "tokens": ["Nach", "ih\u00b7rem", "Blut", "zu", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie gut und sicher dient sichs nicht", "tokens": ["Wie", "gut", "und", "si\u00b7cher", "dient", "sichs", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "PIS", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem ewigen Monarchen;", "tokens": ["Dem", "e\u00b7wi\u00b7gen", "Mon\u00b7ar\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jm Feuer ist er Zuversicht,", "tokens": ["Jm", "Feu\u00b7er", "ist", "er", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vors Wasser baut er Archen.", "tokens": ["Vors", "Was\u00b7ser", "baut", "er", "Ar\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn die treuen Zeugen sehn,", "tokens": ["Und", "wenn", "die", "treu\u00b7en", "Zeu\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worauf sies Leben wagen,", "tokens": ["Wo\u00b7rauf", "sies", "Le\u00b7ben", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So m\u00f6gen sie nicht widerstehn,", "tokens": ["So", "m\u00f6\u00b7gen", "sie", "nicht", "wi\u00b7der\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lassen sich erschlagen.", "tokens": ["Und", "las\u00b7sen", "sich", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie wollen der Erl\u00f6sung nicht,", "tokens": ["Sie", "wol\u00b7len", "der", "Er\u00b7l\u00f6\u00b7sung", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sie vorm Leiden birget;", "tokens": ["Die", "sie", "vorm", "Lei\u00b7den", "bir\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Um jener Auferstehung Licht", "tokens": ["Um", "je\u00b7ner", "Auf\u00b7er\u00b7ste\u00b7hung", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist mancher gern erw\u00fcrget.", "tokens": ["Ist", "man\u00b7cher", "gern", "er\u00b7w\u00fcr\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Zeugen JEsu waren ja", "tokens": ["Die", "Zeu\u00b7gen", "Je\u00b7su", "wa\u00b7ren", "ja"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dem auch Glaubens-Helden,", "tokens": ["Vor", "dem", "auch", "Glau\u00b7bens\u00b7Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die man in Peltzen wandeln sah", "tokens": ["Die", "man", "in", "Pelt\u00b7zen", "wan\u00b7deln", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verfaulen in den W\u00e4lden.", "tokens": ["Ver\u00b7fau\u00b7len", "in", "den", "W\u00e4l\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und des die Welt nicht w\u00fcrdig war,", "tokens": ["Und", "des", "die", "Welt", "nicht", "w\u00fcr\u00b7dig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ist im Elend gangen;", "tokens": ["Der", "ist", "im", "E\u00b7lend", "gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den F\u00fcrsten \u00fcber GOttes Schaar", "tokens": ["Den", "F\u00fcrs\u00b7ten", "\u00fc\u00b7ber", "Got\u00b7tes", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den haben sie gehangen.", "tokens": ["Den", "ha\u00b7ben", "sie", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir wollen unter GOttes Schutz,", "tokens": ["Wir", "wol\u00b7len", "un\u00b7ter", "Got\u00b7tes", "Schutz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Satan zu vertreiben,", "tokens": ["Den", "Sa\u00b7tan", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und seinem Hohn-Geschrey zu Trutz,", "tokens": ["Und", "sei\u00b7nem", "Hohn\u00b7Ge\u00b7schrey", "zu", "Trutz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit unsern V\u00e4tern ", "tokens": ["Mit", "un\u00b7sern", "V\u00e4\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Soll aber unser Rosen-Art", "tokens": ["Soll", "a\u00b7ber", "un\u00b7ser", "Ro\u00b7sen\u00b7Art"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch unter Dornen weyden,", "tokens": ["Auch", "un\u00b7ter", "Dor\u00b7nen", "wey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So wollen wir dann leiden.", "tokens": ["So", "wol\u00b7len", "wir", "dann", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}