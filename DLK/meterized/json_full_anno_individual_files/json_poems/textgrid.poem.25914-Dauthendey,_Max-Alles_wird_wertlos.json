{"textgrid.poem.25914": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Alles wird wertlos", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich Abschied nahm von deinem Mund,", "tokens": ["Als", "ich", "Ab\u00b7schied", "nahm", "von", "dei\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hielt mich noch dein Haar wie Arme fest;", "tokens": ["Hielt", "mich", "noch", "dein", "Haar", "wie", "Ar\u00b7me", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich ward stumm von der Stille jener Stund,", "tokens": ["Ich", "ward", "stumm", "von", "der", "Stil\u00b7le", "je\u00b7ner", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und von deiner Tr\u00e4ne blind,", "tokens": ["Und", "von", "dei\u00b7ner", "Tr\u00e4\u00b7ne", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mich nicht mehr verl\u00e4\u00dft.", "tokens": ["Die", "mich", "nicht", "mehr", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stan