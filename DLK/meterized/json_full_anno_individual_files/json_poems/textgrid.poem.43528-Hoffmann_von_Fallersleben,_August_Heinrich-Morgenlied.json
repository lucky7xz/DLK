{"textgrid.poem.43528": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sterne sind erblichen", "tokens": ["Die", "Ster\u00b7ne", "sind", "er\u00b7bli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ihrem g\u00fcldnen Schein.", "tokens": ["Mit", "ih\u00b7rem", "g\u00fcld\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald ist die Nacht entwichen,", "tokens": ["Bald", "ist", "die", "Nacht", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Morgen dringt herein.", "tokens": ["Der", "Mor\u00b7gen", "dringt", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch waltet tiefes Schweigen", "tokens": ["Noch", "wal\u00b7tet", "tie\u00b7fes", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Thal und \u00fcberall;", "tokens": ["Im", "Thal", "und", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf frischbethauten Zweigen", "tokens": ["Auf", "frischbe\u00b7