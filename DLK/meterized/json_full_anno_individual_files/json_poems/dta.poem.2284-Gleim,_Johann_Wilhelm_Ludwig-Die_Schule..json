{"dta.poem.2284": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Schule.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kinder! habt nur Lust zu lernen;", "tokens": ["Kin\u00b7der", "!", "habt", "nur", "Lust", "zu", "ler\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht! es fehlt euch nicht an Lehrern.", "tokens": ["Seht", "!", "es", "fehlt", "euch", "nicht", "an", "Leh\u00b7rern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuer, Wasser, Luft und Erde,", "tokens": ["Feu\u00b7er", ",", "Was\u00b7ser", ",", "Luft", "und", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ihr seht, und h\u00f6rt, und f\u00fchlet,", "tokens": ["Was", "ihr", "seht", ",", "und", "h\u00f6rt", ",", "und", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles kan euch unterrichten.", "tokens": ["Al\u00b7les", "kan", "euch", "un\u00b7ter\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Halt nur erst den sch\u00f6nen Willen", "tokens": ["Halt", "nur", "erst", "den", "sch\u00f6\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Allem etwas abzulernen.", "tokens": ["Al\u00b7lem", "et\u00b7was", "ab\u00b7zu\u00b7ler\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lernet dann, und werdet kl\u00fcger,", "tokens": ["Ler\u00b7net", "dann", ",", "und", "wer\u00b7det", "kl\u00fc\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lernt vom L\u00f6wen tapfer streiten;", "tokens": ["Lernt", "vom", "L\u00f6\u00b7wen", "tap\u00b7fer", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lernt vom Adler mutig fliegen;", "tokens": ["Lernt", "vom", "Ad\u00b7ler", "mu\u00b7tig", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Lernt vom Nautul k\u00fcnstlich schiffen;", "tokens": ["Lernt", "vom", "Nau\u00b7tul", "k\u00fcnst\u00b7lich", "schif\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Lernt vom Biber sicher bauen;", "tokens": ["Lernt", "vom", "Bi\u00b7ber", "si\u00b7cher", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Lernt von Bienen S\u00fc\u00dfigkeiten,", "tokens": ["Lernt", "von", "Bie\u00b7nen", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und von Spinnen feine Faden.", "tokens": ["Und", "von", "Spin\u00b7nen", "fei\u00b7ne", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Lernt auch etwas vom Kaninchen!", "tokens": ["Lernt", "auch", "et\u00b7was", "vom", "Ka\u00b7nin\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Aber eh ihr etwas lernet,", "tokens": ["A\u00b7ber", "eh", "ihr", "et\u00b7was", "ler\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Lernt von mir und meiner Sch\u00f6ne", "tokens": ["Lernt", "von", "mir", "und", "mei\u00b7ner", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Gut zu spielen, gut zu k\u00fcssen:", "tokens": ["Gut", "zu", "spie\u00b7len", ",", "gut", "zu", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Seht nur her! wir halten Schule.", "tokens": ["Seht", "nur", "her", "!", "wir", "hal\u00b7ten", "Schu\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}