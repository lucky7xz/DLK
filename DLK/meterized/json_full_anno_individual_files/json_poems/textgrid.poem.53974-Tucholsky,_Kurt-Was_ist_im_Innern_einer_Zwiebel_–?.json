{"textgrid.poem.53974": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Was ist im Innern einer Zwiebel \u2013?", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun nimmt wohl bald der Bauer Geld aus der Schatullen", "tokens": ["Nun", "nimmt", "wohl", "bald", "der", "Bau\u00b7er", "Geld", "aus", "der", "Scha\u00b7tul\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und macht sich auf mit seiner Kuh zum Bullen \u2013", "tokens": ["und", "macht", "sich", "auf", "mit", "sei\u00b7ner", "Kuh", "zum", "Bul\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit seiner Kuh.", "tokens": ["mit", "sei\u00b7ner", "Kuh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nun wirft wohl diese Kuh ein K\u00e4lbchen sonder Schaden,", "tokens": ["Nun", "wirft", "wohl", "die\u00b7se", "Kuh", "ein", "K\u00e4lb\u00b7chen", "son\u00b7der", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und dieses K\u00e4lbchen legt dort einen runden Fladen \u2013", "tokens": ["und", "die\u00b7ses", "K\u00e4lb\u00b7chen", "legt", "dort", "ei\u0