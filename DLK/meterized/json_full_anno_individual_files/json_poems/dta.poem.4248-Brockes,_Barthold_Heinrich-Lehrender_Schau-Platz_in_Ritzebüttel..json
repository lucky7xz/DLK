{"dta.poem.4248": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lehrender Schau-Platz  \n in Ritzeb\u00fcttel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird von einem gr\u00fcnen Schau-Platz das hoch erhabne", "tokens": ["Es", "wird", "von", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Schau\u00b7Platz", "das", "hoch", "er\u00b7hab\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJD", "ADJA"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schlo\u00df umringet,", "tokens": ["Schlo\u00df", "um\u00b7rin\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der mit den Fenstern gleicher H\u00f6h\u2019. Auf diesem Schau-", "tokens": ["Der", "mit", "den", "Fens\u00b7tern", "glei\u00b7cher", "H\u00f6h'", ".", "Auf", "die\u00b7sem", "Schau"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$.", "APPR", "PDAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Platz sahe man,", "tokens": ["Platz", "sa\u00b7he", "man", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.5": {"text": "Als wie aus einer Gallerie, manch lehrreich Schau-Spiel", "tokens": ["Als", "wie", "aus", "ei\u00b7ner", "Gal\u00b7le\u00b7rie", ",", "manch", "lehr\u00b7reich", "Schau\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "$,", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u00f6fters an,", "tokens": ["\u00f6f\u00b7ters", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Das denen, die mit Ernst drauf achten, so Nutzen als Ver-", "tokens": ["Das", "de\u00b7nen", ",", "die", "mit", "Ernst", "drauf", "ach\u00b7ten", ",", "so", "Nut\u00b7zen", "als", "Ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PRELS", "$,", "PRELS", "APPR", "NE", "PAV", "VVINF", "$,", "ADV", "NN", "KOKOM", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "gn\u00fcgen bringet.", "tokens": ["gn\u00fc\u00b7gen", "brin\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Es ist der Schau-Platz so errichtet, von solcher H\u00f6he,", "tokens": ["Es", "ist", "der", "Schau\u00b7Platz", "so", "er\u00b7rich\u00b7tet", ",", "von", "sol\u00b7cher", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-++--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "so bequehm,", "tokens": ["so", "be\u00b7quehm", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df man, aus unsern offnen Fenstern, auf selbigen, recht", "tokens": ["Da\u00df", "man", ",", "aus", "un\u00b7sern", "off\u00b7nen", "Fens\u00b7tern", ",", "auf", "sel\u00b7bi\u00b7gen", ",", "recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "$,", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "angenehm,", "tokens": ["an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Die Spieler spielen sehen kann.", "tokens": ["Die", "Spie\u00b7ler", "spie\u00b7len", "se\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es stellt das gr\u00fcnende Theater die Unschuld, Ruhe, Lust", "tokens": ["Es", "stellt", "das", "gr\u00fc\u00b7nen\u00b7de", "The\u00b7a\u00b7ter", "die", "Un\u00b7schuld", ",", "Ru\u00b7he", ",", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+--++--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "und Zier", "tokens": ["und", "Zier"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Des Sorgen- losen Hirten-Standes recht lebhaft und nat\u00fcr-", "tokens": ["Des", "Sor\u00b7gen", "lo\u00b7sen", "Hir\u00b7ten\u00b7Stan\u00b7des", "recht", "leb\u00b7haft", "und", "na\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "ADJA", "NN", "ADV", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "lich f\u00fcr.", "tokens": ["lich", "f\u00fcr", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "APPR", "$."], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Die hier agirende Personen erbauen, durch ihr Beyspiel,", "tokens": ["Die", "hier", "a\u00b7gi\u00b7ren\u00b7de", "Per\u00b7so\u00b7nen", "er\u00b7bau\u00b7en", ",", "durch", "ihr", "Bey\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "mehr,", "tokens": ["mehr", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Wenn wir auf ihr Betragen achten,", "tokens": ["Wenn", "wir", "auf", "ihr", "Be\u00b7tra\u00b7gen", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Die Eintracht, die Vertr\u00e4glichkeit, die Sanftmuht und", "tokens": ["Die", "Ein\u00b7tracht", ",", "die", "Ver\u00b7tr\u00e4g\u00b7lich\u00b7keit", ",", "die", "Sanft\u00b7muht", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Geduld betrachten,", "tokens": ["Ge\u00b7duld", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Als wenn, mit eitelm Wort-Gepr\u00e4nge, aus einer trocknen", "tokens": ["Als", "wenn", ",", "mit", "ei\u00b7telm", "Wor\u00b7tGe\u00b7pr\u00e4n\u00b7ge", ",", "aus", "ei\u00b7ner", "trock\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOUS", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Sitten-Lehr,", "tokens": ["Sit\u00b7ten\u00b7Lehr", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Die, der sie lehret, selbst nicht h\u00e4lt, viel\u2019 \u00f6fters uns viel", "tokens": ["Die", ",", "der", "sie", "leh\u00b7ret", ",", "selbst", "nicht", "h\u00e4lt", ",", "viel'", "\u00f6f\u00b7ters", "uns", "viel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "PTKNEG", "VVFIN", "$,", "ADV", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.18": {"text": "Prahlens machten.", "tokens": ["Prah\u00b7lens", "mach\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Damit in diesem sch\u00f6nen Schau-Spiel auch alles nicht", "tokens": ["Da\u00b7mit", "in", "die\u00b7sem", "sch\u00f6\u00b7nen", "Schau\u00b7Spiel", "auch", "al\u00b7les", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PDAT", "ADJA", "NN", "ADV", "PIS", "PTKNEG"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "zu traurig sey,", "tokens": ["zu", "trau\u00b7rig", "sey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und uns die Zeit nicht lange daure, sind kleine Harlequins", "tokens": ["Und", "uns", "die", "Zeit", "nicht", "lan\u00b7ge", "dau\u00b7re", ",", "sind", "klei\u00b7ne", "Har\u00b7le\u00b7quins"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "dabey,", "tokens": ["da\u00b7bey", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Die, mit po\u00dfierlichem Geh\u00fcpf, mit schnellen und gekr\u00fcmmten", "tokens": ["Die", ",", "mit", "po\u00df\u00b7ier\u00b7li\u00b7chem", "Ge\u00b7h\u00fcpf", ",", "mit", "schnel\u00b7len", "und", "ge\u00b7kr\u00fcmm\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Spr\u00fcngen,", "tokens": ["Spr\u00fcn\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Den Schauern mancherley Vergn\u00fcgen, bey ernstlichen", "tokens": ["Den", "Schau\u00b7ern", "man\u00b7cher\u00b7ley", "Ver\u00b7gn\u00fc\u00b7gen", ",", "bey", "ernst\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Gedanken, bringen.", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Des holden Schau-Spiels Titel ist: ", "tokens": ["Des", "hol\u00b7den", "Schau\u00b7Spiels", "Ti\u00b7tel", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "der Unschuld Bild.", "tokens": ["der", "Un\u00b7schuld", "Bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Doch halt! es sey dir, liebster Leser! die Wahrheit l\u00e4nger", "tokens": ["Doch", "halt", "!", "es", "sey", "dir", ",", "liebs\u00b7ter", "Le\u00b7ser", "!", "die", "Wahr\u00b7heit", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "ART", "NN", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "nicht verh\u00fcllt.", "tokens": ["nicht", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Der Schau-Platz ist ein gr\u00fcner Wall. Die hier agirende", "tokens": ["Der", "Schau\u00b7Platz", "ist", "ein", "gr\u00fc\u00b7ner", "Wall", ".", "Die", "hier", "a\u00b7gi\u00b7ren\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Personen", "tokens": ["Per\u00b7so\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Sind eine Heerde frommer Schafe, nebst ihrer jungen", "tokens": ["Sind", "ei\u00b7ne", "Heer\u00b7de", "from\u00b7mer", "Scha\u00b7fe", ",", "nebst", "ih\u00b7rer", "jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "L\u00e4mmer Schaar,", "tokens": ["L\u00e4m\u00b7mer", "Schaar", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Die hier, recht zwischen hohles Erz, GOtt Lob! nur gr\u00fcssen-", "tokens": ["Die", "hier", ",", "recht", "zwi\u00b7schen", "hoh\u00b7les", "Erz", ",", "Gott", "Lob", "!", "nur", "gr\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "$,", "ADJD", "APPR", "ADJA", "NN", "$,", "NN", "NN", "$.", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "der Canonen,", "tokens": ["der", "Ca\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.19": {"text": "Die Nahrung \u00fcberfl\u00fc\u00dfig finden, ohn\u2019 alle Sorgen, ohn\u2019", "tokens": ["Die", "Nah\u00b7rung", "\u00fc\u00b7berf\u00b7l\u00fc\u00b7\u00dfig", "fin\u00b7den", ",", "ohn'", "al\u00b7le", "Sor\u00b7gen", ",", "ohn'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,", "KOUI", "PIAT", "NN", "$,", "KOUI"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Gefahr.", "tokens": ["Ge\u00b7fahr", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Jhr ruhiger zufried\u2019ner Stand, ihr sanft- und Sorgen- loses", "tokens": ["Ihr", "ru\u00b7hi\u00b7ger", "zu\u00b7frie\u00b7d'\u00b7ner", "Stand", ",", "ihr", "sanft", "und", "Sor\u00b7gen", "lo\u00b7ses"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,", "PPOSAT", "TRUNC", "KON", "TRUNC", "NN"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Leben", "tokens": ["Le\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Hat meinem aufgebrachten Sinn, wenn Leidenschaften sich", "tokens": ["Hat", "mei\u00b7nem", "auf\u00b7ge\u00b7brach\u00b7ten", "Sinn", ",", "wenn", "Lei\u00b7den\u00b7schaf\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "NN", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "erheben,", "tokens": ["er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Und mir die Ruhe rauben wollten, die ruhige Gelas-", "tokens": ["Und", "mir", "die", "Ru\u00b7he", "rau\u00b7ben", "woll\u00b7ten", ",", "die", "ru\u00b7hi\u00b7ge", "Ge\u00b7las"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.26": {"text": "senheit", "tokens": ["sen\u00b7heit"], "token_info": ["word"], "pos": ["NN"], "meter": "--", "measure": "unknown.measure.zero"}, "line.27": {"text": "Gar \u00f6fters wieder hergestellt. Wie, dacht\u2019 ich, will ich meine", "tokens": ["Gar", "\u00f6f\u00b7ters", "wie\u00b7der", "her\u00b7ge\u00b7stellt", ".", "Wie", ",", "dacht'", "ich", ",", "will", "ich", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$.", "PWAV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.28": {"text": "Zeit", "tokens": ["Zeit"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Denn unvern\u00fcnftiger, als sie, weil ich vern\u00fcnftig bin, ver-", "tokens": ["Denn", "un\u00b7ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", ",", "als", "si