{"dta.poem.18914": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von Wolermeltem Herrn   &c.  \n  H. Reichs Cantzlern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Er kan mit solcher krafft/ vnd wunderreich\u1ebd schein", "tokens": ["Er", "kan", "mit", "sol\u00b7cher", "krafft", "/", "vnd", "wun\u00b7der\u00b7reich\u1ebd", "schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "$(", "KON", "XY", "XY"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die forcht in vnserm feind/ den muht in Vns", "tokens": ["Die", "forcht", "in", "vn\u00b7serm", "feind", "/", "den", "muht", "in", "Vns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$(", "ART", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "vermehren/", "tokens": ["ver\u00b7meh\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Dz jn als eine Son\u0303/ nicht einen ", "tokens": ["Dz", "jn", "als", "ei\u00b7ne", "So\u00f1", "/", "nicht", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "ART", "NN", "$(", "PTKNEG", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit wunder m\u00f6niglich mu\u00df sehen/ segnen/ ehren.", "tokens": ["Mit", "wun\u00b7der", "m\u00f6\u00b7nig\u00b7lich", "mu\u00df", "se\u00b7hen", "/", "seg\u00b7nen", "/", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VMFIN", "VVINF", "$(", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zufl\u00fcchtig suchen jhn reich vnd arm/ gro\u00df vn\u0303 klein/", "tokens": ["Zu\u00b7fl\u00fcch\u00b7tig", "su\u00b7chen", "jhn", "reich", "vnd", "arm", "/", "gro\u00df", "v\u00f1", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$(", "ADJD", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die pleget seines rahts vn\u0303 trosts Er zugewehren;", "tokens": ["Die", "ple\u00b7get", "sei\u00b7nes", "rahts", "v\u00f1", "trosts", "Er", "zu\u00b7ge\u00b7weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NE", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Er als der Welt ", "tokens": ["Da\u00df", "Er", "als", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "haupt zu sein", "tokens": ["haupt", "zu", "sein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKZU", "VAINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "(allf\u00e4hig) allein wehrt zu r\u00fchmen/ zu begehren.", "tokens": ["(", "all\u00b7f\u00e4\u00b7hig", ")", "al\u00b7lein", "wehrt", "zu", "r\u00fch\u00b7men", "/", "zu", "be\u00b7geh\u00b7ren", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "ADV", "VVFIN", "PTKZU", "VVINF", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Darumb verbleibet Er (ein wunder aller zeit)", "tokens": ["Da\u00b7rumb", "ver\u00b7blei\u00b7bet", "Er", "(", "ein", "wun\u00b7der", "al\u00b7ler", "zeit", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch seiner Gotsforcht/ Trew vnnd Wei\u00dfheit", "tokens": ["Durch", "sei\u00b7ner", "Gots\u00b7forcht", "/", "Trew", "vnnd", "Wei\u00df\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "edle thaten/", "tokens": ["ed\u00b7le", "tha\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Der inhalt aller lehr; der probstein weyser leut;", "tokens": ["Der", "in\u00b7halt", "al\u00b7ler", "lehr", ";", "der", "probs\u00b7tein", "wey\u00b7ser", "leut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$.", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Des Fridens lehr vnd lieb; ein beyspil der Soldaten;", "tokens": ["Des", "Fri\u00b7dens", "lehr", "vnd", "lieb", ";", "ein", "bey\u00b7spil", "der", "Sol\u00b7da\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADJD", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Spiegel guter R\u00e4ht; Sigweyser in dem", "tokens": ["Der", "Spie\u00b7gel", "gu\u00b7ter", "R\u00e4ht", ";", "Sig\u00b7wey\u00b7ser", "in", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "streit;", "tokens": ["streit", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der Tugent gantze Sum; Lehrbuch der Poten-", "tokens": ["Der", "Tu\u00b7gent", "gant\u00b7ze", "Sum", ";", "Lehr\u00b7buch", "der", "Po\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "NN", "ART", "TRUNC"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "taten.", "tokens": ["ta\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}