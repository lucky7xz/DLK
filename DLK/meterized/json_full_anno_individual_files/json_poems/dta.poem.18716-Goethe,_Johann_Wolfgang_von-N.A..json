{"dta.poem.18716": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wer wird von der Welt verlangen?", "tokens": ["Wer", "wird", "von", "der", "Welt", "ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was sie selbst vermisst und tr\u00e4umet,", "tokens": ["Was", "sie", "selbst", "ver\u00b7misst", "und", "tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fcckw\u00e4rts oder seitw\u00e4rts blickend", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "o\u00b7der", "seit\u00b7w\u00e4rts", "bli\u00b7ckend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets den Tag des Tags vers\u00e4umet.", "tokens": ["Stets", "den", "Tag", "des", "Tags", "ver\u00b7s\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr Bem\u00fchn ist guter Wille,", "tokens": ["Ihr", "Be\u00b7m\u00fchn", "ist", "gu\u00b7ter", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hinkt nur nach dem raschen Leben", "tokens": ["Hinkt", "nur", "nach", "dem", "ra\u00b7schen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und was du vor Jahren brauchtest,", "tokens": ["Und", "was", "du", "vor", "Jah\u00b7ren", "brauch\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00f6chte sie dir heute geben.", "tokens": ["M\u00f6ch\u00b7te", "sie", "dir", "heu\u00b7te", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}