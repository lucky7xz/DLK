{"dta.poem.14377": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie auf Bergen hoch bin ich", "tokens": ["Wie", "auf", "Ber\u00b7gen", "hoch", "bin", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADJD", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So einsam gestellt", "tokens": ["So", "ein\u00b7sam", "ge\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und gr\u00fc\u00dfe herzinnig,", "tokens": ["Und", "gr\u00fc\u00b7\u00dfe", "her\u00b7zin\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Was sch\u00f6n auf der Welt.", "tokens": ["Was", "sch\u00f6n", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}