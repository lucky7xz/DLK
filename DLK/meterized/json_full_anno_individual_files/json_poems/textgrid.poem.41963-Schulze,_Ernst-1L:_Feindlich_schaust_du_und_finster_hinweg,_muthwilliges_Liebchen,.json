{"textgrid.poem.41963": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Feindlich schaust du und finster hinweg, muthwilliges Liebchen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feindlich schaust du und finster hinweg, muthwilliges Liebchen,", "tokens": ["Feind\u00b7lich", "schaust", "du", "und", "fins\u00b7ter", "hin\u00b7weg", ",", "mut\u00b7hwil\u00b7li\u00b7ges", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADJD", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Pl\u00f6tzlicher Unmuth thront auf der gerunzelten Stirn,", "tokens": ["Pl\u00f6tz\u00b7li\u00b7cher", "Un\u00b7muth", "thront", "auf", "der", "ge\u00b7run\u00b7zel\u00b7ten", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Jeglichen heiteren Gott verscheucht aus dem Auge des Zorns Blitz,", "tokens": ["Jeg\u00b7li\u00b7chen", "hei\u00b7te\u00b7ren", "Gott", "ver\u00b7scheucht", "aus", "dem", "Au\u00b7ge", "des", "Zorns", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und unfreundliches Roth f\u00e4rbet die Wange dir jetzt.", "tokens": ["Und", "un\u00b7freund\u00b7li\u00b7ches", "Roth", "f\u00e4r\u00b7bet", "die", "Wan\u00b7ge", "dir", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was ich auch rede, du h\u00f6rest es nicht, und red' ich auch Sch\u00f6nes,", "tokens": ["Was", "ich", "auch", "re\u00b7de", ",", "du", "h\u00f6\u00b7rest", "es", "nicht", ",", "und", "red'", "ich", "auch", "Sch\u00f6\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "ADV", "NE", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Kein s\u00fc\u00dflohnender Blick lehret mich da\u00df ich gefiel.", "tokens": ["Kein", "s\u00fc\u00df\u00b7loh\u00b7nen\u00b7der", "Blick", "leh\u00b7ret", "mich", "da\u00df", "ich", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Ha, so hab' ich dich nimmer gesehn! doch z\u00fcrne nur; Neuheit", "tokens": ["Ha", ",", "so", "hab'", "ich", "dich", "nim\u00b7mer", "ge\u00b7sehn", "!", "doch", "z\u00fcr\u00b7ne", "nur", ";", "Neu\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$.", "ADV", "VVFIN", "ADV", "$.", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "N\u00e4hrt die Liebe, zu lang dauert ein ewiger Lenz.", "tokens": ["N\u00e4hrt", "die", "Lie\u00b7be", ",", "zu", "lang", "dau\u00b7ert", "ein", "e\u00b7wi\u00b7ger", "Lenz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PTKA", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Tauscht doch oft mit den Waffen des Mars Cytherea den G\u00fcrtel,", "tokens": ["Tauscht", "doch", "oft", "mit", "den", "Waf\u00b7fen", "des", "Mars", "Cy\u00b7the\u00b7rea", "den", "G\u00fcr\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "NE", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Und in Panzer und Helm schleppt sich ihr Knabe daher.", "tokens": ["Und", "in", "Pan\u00b7zer", "und", "Helm", "schleppt", "sich", "ihr", "Kna\u00b7be", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Reizend kleidet der Trotz die Reizende; bist du auch stets mir", "tokens": ["Rei\u00b7zend", "klei\u00b7det", "der", "Trotz", "die", "Rei\u00b7zen\u00b7de", ";", "bist", "du", "auch", "stets", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADV", "PPER"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Nicht zu gefallen bem\u00fcht, immer gef\u00e4llst du mir mehr.", "tokens": ["Nicht", "zu", "ge\u00b7fal\u00b7len", "be\u00b7m\u00fcht", ",", "im\u00b7mer", "ge\u00b7f\u00e4llst", "du", "mir", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Grazie, wind' um die d\u00fcstere Stirn die Trauercypresse,", "tokens": ["Gra\u00b7zie", ",", "wind'", "um", "die", "d\u00fcs\u00b7te\u00b7re", "Stirn", "die", "Trau\u00b7er\u00b7cyp\u00b7res\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Flicht mit t\u00e4ndelnder Hand duftende Rosen in's Haar;", "tokens": ["Flicht", "mit", "t\u00e4n\u00b7deln\u00b7der", "Hand", "duf\u00b7ten\u00b7de", "Ro\u00b7sen", "in's", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "L\u00e4chle mich an mit dem schmachtenden Blick hingebender Sehnsucht,", "tokens": ["L\u00e4ch\u00b7le", "mich", "an", "mit", "dem", "schmach\u00b7ten\u00b7den", "Blick", "hin\u00b7ge\u00b7ben\u00b7der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.16": {"text": "Scheuche den K\u00fchneren rasch fort mit der Flamme des Zorns:", "tokens": ["Scheu\u00b7che", "den", "K\u00fch\u00b7ne\u00b7ren", "rasch", "fort", "mit", "der", "Flam\u00b7me", "des", "Zorns", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Stets doch knie' ich vor deinem Altar, und jede Verwandlung", "tokens": ["Stets", "doch", "knie'", "ich", "vor", "dei\u00b7nem", "Al\u00b7tar", ",", "und", "je\u00b7de", "Ver\u00b7wand\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Scheint mir die holdere, stets sah ich dich nimmer so sch\u00f6n.", "tokens": ["Scheint", "mir", "die", "hol\u00b7de\u00b7re", ",", "stets", "sah", "ich", "dich", "nim\u00b7mer", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.19": {"text": "K\u00fcssen m\u00f6cht' ich den reizenden Mund, der die K\u00fcsse mir abschl\u00e4gt,", "tokens": ["K\u00fcs\u00b7sen", "m\u00f6cht'", "ich", "den", "rei\u00b7zen\u00b7den", "Mund", ",", "der", "die", "K\u00fcs\u00b7se", "mir", "ab\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "M\u00f6cht' an der wogenden Brust ruhen, die z\u00fcrnend sich hebt,", "tokens": ["M\u00f6cht'", "an", "der", "wo\u00b7gen\u00b7den", "Brust", "ru\u00b7hen", ",", "die", "z\u00fcr\u00b7nend", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Hangen an deinem Blick der sich abkehrt, feindlicher Gluth voll,", "tokens": ["Han\u00b7gen", "an", "dei\u00b7nem", "Blick", "der", "sich", "ab\u00b7kehrt", ",", "feind\u00b7li\u00b7cher", "Gluth", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "PRF", "VVFIN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.22": {"text": "Und festhalten die Hand, welche der Fessel sich str\u00e4ubt.", "tokens": ["Und", "fest\u00b7hal\u00b7ten", "die", "Hand", ",", "wel\u00b7che", "der", "Fes\u00b7sel", "sich", "str\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-++--+---+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Also schm\u00fcckte Klorinde sich einst zur tobenden Feldschlacht,", "tokens": ["Al\u00b7so", "schm\u00fcck\u00b7te", "Klo\u00b7rin\u00b7de", "sich", "einst", "zur", "to\u00b7ben\u00b7den", "Feld\u00b7schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "K\u00fchn nach der Palme des Siegs strebte das muthige Weib:", "tokens": ["K\u00fchn", "nach", "der", "Pal\u00b7me", "des", "Siegs", "streb\u00b7te", "das", "mut\u00b7hi\u00b7ge", "Weib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Doch kaum f\u00fchlte sie Tankreds Blick, so kr\u00e4nzte sie, selbst sich", "tokens": ["Doch", "kaum", "f\u00fchl\u00b7te", "sie", "Tan\u00b7kreds", "Blick", ",", "so", "kr\u00e4nz\u00b7te", "sie", ",", "selbst", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NE", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "PRF"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Z\u00fcrnend, mit Myrtengeflecht, meidend die Fehde, den Helm.", "tokens": ["Z\u00fcr\u00b7nend", ",", "mit", "Myr\u00b7ten\u00b7ge\u00b7flecht", ",", "mei\u00b7dend", "die", "Feh\u00b7de", ",", "den", "Helm", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NN", "$,", "ADJD", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Lockender ist die verbotene Frucht, und Tyndaris h\u00e4tte,", "tokens": ["Lo\u00b7cken\u00b7der", "ist", "die", "ver\u00b7bo\u00b7te\u00b7ne", "Frucht", ",", "und", "Tyn\u00b7da\u00b7ris", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "NE", "VAFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "War sie dem Troer bestimmt, nimmer den Troer geliebt.", "tokens": ["War", "sie", "dem", "Troer", "be\u00b7stimmt", ",", "nim\u00b7mer", "den", "Troer", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Z\u00fcrne nur fort, nie warst du mir reizender; wahrlich ich selbst will