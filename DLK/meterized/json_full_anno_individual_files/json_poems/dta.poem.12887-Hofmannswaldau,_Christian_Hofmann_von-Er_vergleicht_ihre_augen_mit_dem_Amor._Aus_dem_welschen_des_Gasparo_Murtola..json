{"dta.poem.12887": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er vergleicht ihre augen mit dem  \n Amor.  \n Aus dem welschen des   Gasparo Murtola.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jn Daphnens sch\u00f6uen augen", "tokens": ["In", "Daph\u00b7nens", "sch\u00f6u\u00b7en", "au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird Amor uns gar artig vorgestellt.", "tokens": ["Wird", "A\u00b7mor", "uns", "gar", "ar\u00b7tig", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das reine licht, so hier durch enge circkel f\u00e4llt,", "tokens": ["Das", "rei\u00b7ne", "licht", ",", "so", "hier", "durch", "en\u00b7ge", "cir\u00b7ckel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan ihm zu seiner fackel taugen.", "tokens": ["Kan", "ihm", "zu", "sei\u00b7ner", "fa\u00b7ckel", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die sternen hat er ihm zum bogen auserwehlt,", "tokens": ["Die", "ster\u00b7nen", "hat", "er", "ihm", "zum", "bo\u00b7gen", "au\u00b7ser\u00b7wehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "PPER", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die pfeile schnitzet er aus ihren scharffen blicken;", "tokens": ["Die", "pfei\u00b7le", "schnit\u00b7zet", "er", "aus", "ih\u00b7ren", "scharf\u00b7fen", "bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df es endlich auch an keinen fl\u00fcgeln fehlt,", "tokens": ["Und", "da\u00df", "es", "end\u00b7lich", "auch", "an", "kei\u00b7nen", "fl\u00fc\u00b7geln", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So m\u00fcssen sich dazu die augen-lieder schicken.", "tokens": ["So", "m\u00fcs\u00b7sen", "sich", "da\u00b7zu", "die", "au\u00b7gen\u00b7lie\u00b7der", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}