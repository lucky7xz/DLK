{"textgrid.poem.60010": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich sehen willst Du, Lalage,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich sehen willst Du, Lalage,", "tokens": ["Mich", "se\u00b7hen", "willst", "Du", ",", "La\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Ph\u00f6bus hoch begeisterte!", "tokens": ["Des", "Ph\u00f6\u00b7bus", "hoch", "be\u00b7geis\u00b7ter\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich kleinen S\u00e4nger kleiner Lieder?", "tokens": ["Mich", "klei\u00b7nen", "S\u00e4n\u00b7ger", "klei\u00b7ner", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil Dein Gliph\u00e4stion mich liebt", "tokens": ["Weil", "Dein", "Gli\u00b7ph\u00e4s\u00b7ti\u00b7on", "mich", "liebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, als den j\u00fcngsten seiner Br\u00fcder,", "tokens": ["Und", ",", "als", "den", "j\u00fcng\u00b7sten", "sei\u00b7ner", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Preise f\u00fcr die kleinen Lieder", "tokens": ["Zum", "Prei\u00b7se", "f\u00fcr", "die", "klei\u00b7nen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mir unverdiente Kr\u00e4nze giebt?", "tokens": ["Mir", "un\u00b7ver\u00b7dien\u00b7te", "Kr\u00e4n\u00b7ze", "giebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach! zu bezauberndem Gesang", "tokens": ["Ach", "!", "zu", "be\u00b7zau\u00b7bern\u00b7dem", "Ge\u00b7sang"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist Feuer nicht in meinem Busen;", "tokens": ["Ist", "Feu\u00b7er", "nicht", "in", "mei\u00b7nem", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nur die gef\u00e4lligste der Musen", "tokens": ["Nur", "die", "ge\u00b7f\u00e4l\u00b7ligs\u00b7te", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "H\u00f6rt dieser Fl\u00f6te leichten Klang,", "tokens": ["H\u00f6rt", "die\u00b7ser", "Fl\u00f6\u00b7te", "leich\u00b7ten", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der nie zu G\u00f6tterohren drang.", "tokens": ["Der", "nie", "zu", "G\u00f6t\u00b7te\u00b7roh\u00b7ren", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wenn K\u00f6nige die Welt bekriegen,", "tokens": ["Wenn", "K\u00f6\u00b7ni\u00b7ge", "die", "Welt", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dann forsch' ich nicht nach ihren Siegen;", "tokens": ["Dann", "for\u00b7sch'", "ich", "nicht", "nach", "ih\u00b7ren", "Sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Dann, ungest\u00f6rt in meiner Ruh,", "tokens": ["Dann", ",", "un\u00b7ge\u00b7st\u00f6rt", "in", "mei\u00b7ner", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Seh' ich den holden Knaben zu,", "tokens": ["Seh'", "ich", "den", "hol\u00b7den", "Kna\u00b7ben", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die, ohne L\u00e4nder zu verw\u00fcsten,", "tokens": ["Die", ",", "oh\u00b7ne", "L\u00e4n\u00b7der", "zu", "ver\u00b7w\u00fcs\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sich mit dem Silberbogen r\u00fcsten,", "tokens": ["Sich", "mit", "dem", "Sil\u00b7ber\u00b7bo\u00b7gen", "r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und ihnen stimm' ich Lieder an.", "tokens": ["Und", "ih\u00b7nen", "stimm'", "ich", "Lie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Hier zeichnen sie; die schlauen G\u00f6tter,", "tokens": ["Hier", "zeich\u00b7nen", "sie", ";", "die", "schlau\u00b7en", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Auf Rosen- und auf Myrthenbl\u00e4tter", "tokens": ["Auf", "Ro\u00b7sen", "und", "auf", "Myr\u00b7then\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Zu ihren Schlachten sich den Plan;", "tokens": ["Zu", "ih\u00b7ren", "Schlach\u00b7ten", "sich", "den", "Plan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Dort h\u00f6r' ich aus verschwiegnen B\u00fcschen", "tokens": ["Dort", "h\u00f6r'", "ich", "aus", "ver\u00b7schwieg\u00b7nen", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die unsichtbaren Pfeile zischen:", "tokens": ["Die", "un\u00b7sicht\u00b7ba\u00b7ren", "Pfei\u00b7le", "zi\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Getroffen sinkt die Sch\u00e4ferin", "tokens": ["Ge\u00b7trof\u00b7fen", "sinkt", "die", "Sch\u00e4\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Auf den bebl\u00fcmten Rasen hin.", "tokens": ["Auf", "den", "be\u00b7bl\u00fcm\u00b7ten", "Ra\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die Suada, die das Ungeheuer", "tokens": ["Die", "Sua\u00b7da", ",", "die", "das", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Mit Recht und Menschlichkeit vers\u00f6hnt,", "tokens": ["Mit", "Recht", "und", "Menschlich\u00b7keit", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Die, s\u00fc\u00df wie Deine goldne Leyer,", "tokens": ["Die", ",", "s\u00fc\u00df", "wie", "Dei\u00b7ne", "gold\u00b7ne", "Le\u00b7yer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "KOKOM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.30": {"text": "Von angenehmen Lippen t\u00f6nt,", "tokens": ["Von", "an\u00b7ge\u00b7neh\u00b7men", "Lip\u00b7pen", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "O h\u00e4tt' ein Gott sie mir verliehen!", "tokens": ["O", "h\u00e4tt'", "ein", "Gott", "sie", "mir", "ver\u00b7lie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Dann lebten, weit um mich herum,", "tokens": ["Dann", "leb\u00b7ten", ",", "weit", "um", "mich", "he\u00b7rum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Nur B\u00fcrger aus Elysium,", "tokens": ["Nur", "B\u00fcr\u00b7ger", "aus", "E\u00b7ly\u00b7si\u00b7um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und jede Bosheit m\u00fc\u00dfte fliehen.", "tokens": ["Und", "je\u00b7de", "Bos\u00b7heit", "m\u00fc\u00df\u00b7te", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Ein \u00fcberredender Merkur,", "tokens": ["Ein", "\u00fc\u00b7berr\u00b7e\u00b7den\u00b7der", "Mer\u00b7kur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wollt' ich die sanftre Weisheit lehren,", "tokens": ["Wollt'", "ich", "die", "sanft\u00b7re", "Weis\u00b7heit", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Gezeugt im Schoo\u00dfe der Natur,", "tokens": ["Ge\u00b7zeugt", "im", "Schoo\u00b7\u00dfe", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Gebildet in der Freundschaft Ch\u00f6ren?", "tokens": ["Ge\u00b7bil\u00b7det", "in", "der", "Freund\u00b7schaft", "Ch\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Mir aber gab der Himmel nur", "tokens": ["Mir", "a\u00b7ber", "gab", "der", "Him\u00b7mel", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ein Herz voll z\u00e4rtlicher Gef\u00fchle,", "tokens": ["Ein", "Herz", "voll", "z\u00e4rt\u00b7li\u00b7cher", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Dem auch die allerkleinsten Spiele", "tokens": ["Dem", "auch", "die", "al\u00b7ler\u00b7kleins\u00b7ten", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Der jungen Freunde heilig sind,", "tokens": ["Der", "jun\u00b7gen", "Freun\u00b7de", "hei\u00b7lig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Das nie Gesang und Jubel st\u00f6ret,", "tokens": ["Das", "nie", "Ge\u00b7sang", "und", "Ju\u00b7bel", "st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Der Tugend leisre Stimme h\u00f6ret,", "tokens": ["Der", "Tu\u00b7gend", "leis\u00b7re", "Stim\u00b7me", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Und gute Seelen leicht gewinnt.", "tokens": ["Und", "gu\u00b7te", "See\u00b7len", "leicht", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Nur sie kann dieses Herz begl\u00fccken:", "tokens": ["Nur", "sie", "kann", "die\u00b7ses", "Herz", "be\u00b7gl\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Dein Sch\u00e4fer widerstand ihm nicht;", "tokens": ["Dein", "Sch\u00e4\u00b7fer", "wi\u00b7der\u00b7stand", "ihm", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Er kennt im Freundes-Angesicht", "tokens": ["Er", "kennt", "im", "Freun\u00b7des\u00b7An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Die reine Wonne, das Entz\u00fccken,", "tokens": ["Die", "rei\u00b7ne", "Won\u00b7ne", ",", "das", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Das, ohne Wort, aus treuen Blicken", "tokens": ["Das", ",", "oh\u00b7ne", "Wort", ",", "aus", "treu\u00b7en", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "KOUI", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Oft m\u00e4chtiger als ein Gedicht,", "tokens": ["Oft", "m\u00e4ch\u00b7ti\u00b7ger", "als", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Und s\u00fc\u00dfer als die Suada, spricht.", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "als", "die", "Sua\u00b7da", ",", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dich, Lalage, Dich sah' ich schon", "tokens": ["Dich", ",", "La\u00b7la\u00b7ge", ",", "Dich", "sah'", "ich", "schon"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Tempel, den Gliph\u00e4stion", "tokens": ["Im", "Tem\u00b7pel", ",", "den", "Gli\u00b7ph\u00e4s\u00b7ti\u00b7on"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der Tugend und den Musen weihte;", "tokens": ["Der", "Tu\u00b7gend", "und", "den", "Mu\u00b7sen", "weih\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo, voller Ehrfurcht, diese Hand,", "tokens": ["Wo", ",", "vol\u00b7ler", "Ehr\u00b7furcht", ",", "die\u00b7se", "Hand", ","], "t