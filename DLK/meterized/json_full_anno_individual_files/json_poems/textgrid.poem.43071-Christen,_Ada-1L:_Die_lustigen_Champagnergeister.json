{"textgrid.poem.43071": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die lustigen Champagnergeister", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die lustigen Champagnergeister", "tokens": ["Die", "lus\u00b7ti\u00b7gen", "Cham\u00b7pag\u00b7ner\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die drehen mich jetzt im Kreis", "tokens": ["Die", "dre\u00b7hen", "mich", "jetzt", "im", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und im Kopfe summt mir", "tokens": ["Und", "im", "Kop\u00b7fe", "summt", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Eine seltsam wirbelnde Weis'.", "tokens": ["Ei\u00b7ne", "selt\u00b7sam", "wir\u00b7beln\u00b7de", "Weis'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "O weh, im Magen ist mir", "tokens": ["O", "weh", ",", "im", "Ma\u00b7gen", "ist", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "APPRART", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch gar so wunderlich;", "tokens": ["Auch", "gar", "so", "wun\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch das allergr\u00f6\u00dfte Uebel", "tokens": ["Doch", "das", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "Ue\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist, da\u00df ich denk' an Dich!", "tokens": ["Ist", ",", "da\u00df", "ich", "denk'", "an", "Dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie glauben, da\u00df ich betrunken sei", "tokens": ["Sie", "glau\u00b7ben", ",", "da\u00df", "ich", "be\u00b7trun\u00b7ken", "sei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wollen mit mir spielen;", "tokens": ["Und", "wol\u00b7len", "mit", "mir", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O h\u00fctet euch, gerad' im Rausch", "tokens": ["O", "h\u00fc\u00b7tet", "euch", ",", "ge\u00b7rad'", "im", "Rausch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwachen die b\u00f6sen Grillen.", "tokens": ["Er\u00b7wa\u00b7chen", "die", "b\u00f6\u00b7sen", "Gril\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Denn wenn ich's recht toll getrieben,", "tokens": ["Denn", "wenn", "ich's", "recht", "toll", "ge\u00b7trie\u00b7ben", ","]