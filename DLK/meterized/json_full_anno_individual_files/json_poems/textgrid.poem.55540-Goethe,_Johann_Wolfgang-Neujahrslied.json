{"textgrid.poem.55540": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrslied", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer k\u00f6mmt! Wer kauft von meiner War'!", "tokens": ["Wer", "k\u00f6mmt", "!", "Wer", "kauft", "von", "mei\u00b7ner", "Wa\u00b7r'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Devisen auf das neue Jahr,", "tokens": ["De\u00b7vi\u00b7sen", "auf", "das", "neu\u00b7e", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr alle St\u00e4nde.", "tokens": ["F\u00fcr", "al\u00b7le", "St\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und fehlt auch einer hie und da,", "tokens": ["Und", "fehlt", "auch", "ei\u00b7ner", "hie", "und", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein einz'ger Handschuh pa\u00dft sich ja", "tokens": ["Ein", "einz'\u00b7ger", "Hand\u00b7schuh", "pa\u00dft", "sich", "ja"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An zwanzig H\u00e4nde.", "tokens": ["An", "zwan\u00b7zig", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du Jugend, die du t\u00e4ndelnd liebst,", "tokens": ["Du", "Ju\u00b7gend", ",", "die", "du", "t\u00e4n\u00b7delnd", "liebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein K\u00fc\u00dfchen um ein K\u00fc\u00dfchen gibst,", "tokens": ["Ein", "K\u00fc\u00df\u00b7chen", "um", "ein", "K\u00fc\u00df\u00b7chen", "gibst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unschuldig heiter.", "tokens": ["Un\u00b7schul\u00b7dig", "hei\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Jetzt lebst du noch ein wenig dumm;", "tokens": ["Jetzt", "lebst", "du", "noch", "ein", "we\u00b7nig", "dumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geh nur erst dieses Jahr herum,", "tokens": ["Geh", "nur", "erst", "die\u00b7ses", "Jahr", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bist du weiter.", "tokens": ["So", "bist", "du", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die ihr schon Amors Wege kennt", "tokens": ["Die", "ihr", "schon", "A\u00b7mors", "We\u00b7ge", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schon ein bi\u00dfchen lichter brennt,", "tokens": ["Und", "schon", "ein", "bi\u00df\u00b7chen", "lich\u00b7ter", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr macht mir bange.", "tokens": ["Ihr", "macht", "mir", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zum Ernst, ihr Kinder, von dem Spa\u00df!", "tokens": ["Zum", "Ernst", ",", "ihr", "Kin\u00b7der", ",", "von", "dem", "Spa\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Jahr! zur h\u00f6chsten Not noch das,", "tokens": ["Das", "Jahr", "!", "zur", "h\u00f6chs\u00b7ten", "Not", "noch", "das", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPRART", "ADJA", "NN", "ADV", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst w\u00e4hrt's zu lange.", "tokens": ["Sonst", "w\u00e4hrt's", "zu", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du junger Mann, du junge Frau,", "tokens": ["Du", "jun\u00b7ger", "Mann", ",", "du", "jun\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lebt nicht zu treu, nicht zu genau", "tokens": ["Lebt", "nicht", "zu", "treu", ",", "nicht", "zu", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PTKA", "ADJD", "$,", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In enger Ehe.", "tokens": ["In", "en\u00b7ger", "E\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Eifersucht qu\u00e4lt manches Haus", "tokens": ["Die", "Ei\u00b7fer\u00b7sucht", "qu\u00e4lt", "man\u00b7ches", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und tr\u00e4gt am Ende doch nichts aus", "tokens": ["Und", "tr\u00e4gt", "am", "En\u00b7de", "doch", "nichts", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "PIS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als doppelt Wehe.", "tokens": ["Als", "dop\u00b7pelt", "We\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der Witwer w\u00fcnscht in seiner Not,", "tokens": ["Der", "Wit\u00b7wer", "w\u00fcnscht", "in", "sei\u00b7ner", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur sel'gen Frau durch schnellen Tod", "tokens": ["Zur", "sel'\u00b7gen", "Frau", "durch", "schnel\u00b7len", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gef\u00fchrt zu werden.", "tokens": ["Ge\u00b7f\u00fchrt", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du guter Mann, nicht so verzagt!", "tokens": ["Du", "gu\u00b7ter", "Mann", ",", "nicht", "so", "ver\u00b7zagt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das, was dir fehlt, das, was dich plagt,", "tokens": ["Das", ",", "was", "dir", "fehlt", ",", "das", ",", "was", "dich", "plagt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Findst du auf Erden.", "tokens": ["Findst", "du", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ihr, die ihr Misogyne hei\u00dft,", "tokens": ["Ihr", ",", "die", "ihr", "Mi\u00b7so\u00b7gy\u00b7ne", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wein heb euern gro\u00dfen Geist", "tokens": ["Der", "Wein", "heb", "eu\u00b7ern", "gro\u00b7\u00dfen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Best\u00e4ndig h\u00f6her.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "h\u00f6\u00b7her", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zwar Wein beschw\u00f6ret oft den Kopf,", "tokens": ["Zwar", "Wein", "be\u00b7schw\u00f6\u00b7ret", "oft", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch der tut manchem Ehetropf", "tokens": ["Doch", "der", "tut", "man\u00b7chem", "E\u00b7he\u00b7tropf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wohl zehnmal weher.", "tokens": ["Wohl", "zehn\u00b7mal", "we\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Der Himmel geb zur Fr\u00fchlingszeit", "tokens": ["Der", "Him\u00b7mel", "geb", "zur", "Fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir manches Lied voll Munterkeit,", "tokens": ["Mir", "man\u00b7ches", "Lied", "voll", "Mun\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und euch gefall es.", "tokens": ["Und", "euch", "ge\u00b7fall", "es", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "PPER", "$."], "meter": "-+-+-", "measure": "iambi