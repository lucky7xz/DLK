{"dta.poem.21652": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Genius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Mir winkt ein Stern, wenn dieser Stern nicht gl\u00f6mme,", "tokens": ["Mir", "winkt", "ein", "Stern", ",", "wenn", "die\u00b7ser", "Stern", "nicht", "gl\u00f6m\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PDAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich h\u00fcllte schaurig Todesdunkel ein,", "tokens": ["Mich", "h\u00fcll\u00b7te", "schau\u00b7rig", "To\u00b7des\u00b7dun\u00b7kel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich tr\u00e4gt ein Strom, wenn ich auf ihm nicht schw\u00f6mme,", "tokens": ["Mich", "tr\u00e4gt", "ein", "Strom", ",", "wenn", "ich", "auf", "ihm", "nicht", "schw\u00f6m\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im W\u00fcstensand verdorrte mein Gebein.", "tokens": ["Im", "W\u00fcs\u00b7ten\u00b7sand", "ver\u00b7dorr\u00b7te", "mein", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O bleib mir treu, du Leitstern meiner Pfade,", "tokens": ["O", "bleib", "mir", "treu", ",", "du", "Leits\u00b7tern", "mei\u00b7ner", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versiege nicht, du Linderin der Gluth!", "tokens": ["Ver\u00b7sie\u00b7ge", "nicht", ",", "du", "Lin\u00b7de\u00b7rin", "der", "Gluth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr seid mir Himmel, Seligkeit und Gnade,", "tokens": ["Ihr", "seid", "mir", "Him\u00b7mel", ",", "Se\u00b7lig\u00b7keit", "und", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Darin ersch\u00f6pft mein ganzes Wesen ruht.", "tokens": ["Da\u00b7rin", "er\u00b7sch\u00f6pft", "mein", "gan\u00b7zes", "We\u00b7sen", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr k\u00f6nnt allein des Herzens Sehnsucht k\u00fchlen,", "tokens": ["Ihr", "k\u00f6nnt", "al\u00b7lein", "des", "Her\u00b7zens", "Sehn\u00b7sucht", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Stern, du Strom herz\u2019eigner Dichterkraft,", "tokens": ["Du", "Stern", ",", "du", "Strom", "her\u00b7z'\u00b7eig\u00b7ner", "Dich\u00b7ter\u00b7kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Nur wenn ich bilde, mag ich Mensch mich f\u00fchlen,", "tokens": ["Nur", "wenn", "ich", "bil\u00b7de", ",", "mag", "ich", "Mensch", "mich", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Seele lebt nur, wenn sie schafft.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "lebt", "nur", ",", "wenn", "sie", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}