{"dta.poem.10343": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.85", "nl:0.14"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der arme Peter wankt vorbei,", "tokens": ["Der", "ar\u00b7me", "Pe\u00b7ter", "wankt", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar langsam, leichenbla\u00df und scheu.", "tokens": ["Gar", "lang\u00b7sam", ",", "lei\u00b7chen\u00b7bla\u00df", "und", "scheu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bleiben fast, wenn sie ihn sehn,", "tokens": ["Es", "blei\u00b7ben", "fast", ",", "wenn", "sie", "ihn", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Leute auf der Stra\u00dfe stehn.", "tokens": ["Die", "Leu\u00b7te", "auf", "der", "Stra\u00b7\u00dfe", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die M\u00e4dchen fl\u00fcstern sich in's Ohr:", "tokens": ["Die", "M\u00e4d\u00b7chen", "fl\u00fcs\u00b7tern", "sich", "in's", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eder stieg wohl aus dem Grab' hervor.\u201c", "tokens": ["\u201e", "der", "stieg", "wohl", "aus", "dem", "Grab'", "her\u00b7vor", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach nein, Ihr lieben Jungfr\u00e4ulein,", "tokens": ["Ach", "nein", ",", "Ihr", "lie\u00b7ben", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der legt sich erst in's Grab hinein.", "tokens": ["Der", "legt", "sich", "erst", "in's", "Grab", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er hat verloren seinen Schatz,", "tokens": ["Er", "hat", "ver\u00b7lo\u00b7ren", "sei\u00b7nen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum ist das Grab der beste Platz", "tokens": ["Drum", "ist", "das", "Grab", "der", "bes\u00b7te", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo er am besten liegen mag,", "tokens": ["Wo", "er", "am", "bes\u00b7ten", "lie\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKA", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlafen bis zum j\u00fcngsten Tag.", "tokens": ["Und", "schla\u00b7fen", "bis", "zum", "j\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}