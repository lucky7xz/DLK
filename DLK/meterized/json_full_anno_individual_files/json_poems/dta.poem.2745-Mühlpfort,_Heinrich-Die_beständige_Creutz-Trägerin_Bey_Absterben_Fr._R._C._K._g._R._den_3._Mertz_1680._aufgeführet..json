{"dta.poem.2745": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die best\u00e4ndige Creutz-Tr\u00e4gerin/  \n  Bey Absterben Fr. R. C. K. g. R. den 3. Mertz  \n 1680. aufgef\u00fchret.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh aus des Creutzes Last und aus der Tr\u00fcbsal Hitze/", "tokens": ["Geh", "aus", "des", "Creut\u00b7zes", "Last", "und", "aus", "der", "Tr\u00fcb\u00b7sal", "Hit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NE", "KON", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nunmehr O Seelige in Edens Garten ein/", "tokens": ["Nun\u00b7mehr", "O", "See\u00b7li\u00b7ge", "in", "E\u00b7dens", "Gar\u00b7ten", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "APPR", "NE", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Wetter schreckt dich jetzt/ es dr\u00e4uen keine Blitze/", "tokens": ["Kein", "Wet\u00b7ter", "schreckt", "dich", "jetzt", "/", "es", "dr\u00e4u\u00b7en", "kei\u00b7ne", "Blit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nach des Grabes Nacht folgt heller Sonnen-", "tokens": ["Und", "nach", "des", "Gra\u00b7bes", "Nacht", "folgt", "hel\u00b7ler", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du hast mit Ruhm gek\u00e4mpfft und mit Gedult getragen", "tokens": ["Du", "hast", "mit", "Ruhm", "ge\u00b7k\u00e4mpfft", "und", "mit", "Ge\u00b7dult", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "KON", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Creutz-Fahn/ welches dir dein Heyland \u00fcberreicht.", "tokens": ["Das", "Creutz\u00b7Fahn", "/", "wel\u00b7ches", "dir", "dein", "Hey\u00b7land", "\u00fc\u00b7berr\u00b7eicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Welt ist triumphirt/ der Satan ligt geschlagen/", "tokens": ["Die", "Welt", "ist", "tri\u00b7um\u00b7phirt", "/", "der", "Sa\u00b7tan", "ligt", "ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Versichert da\u00df kein Sieg der Helden deinem gleicht.", "tokens": ["Ver\u00b7si\u00b7chert", "da\u00df", "kein", "Sieg", "der", "Hel\u00b7den", "dei\u00b7nem", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOUS", "PIAT", "NN", "ART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es r\u00fchme Scythien die Mann-behertzten Frauen/", "tokens": ["Es", "r\u00fch\u00b7me", "Scyt\u00b7hien", "die", "Mann\u00b7be\u00b7hertz\u00b7ten", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So an die rechte Brust der Lantze Staal gesetzt/", "tokens": ["So", "an", "die", "rech\u00b7te", "Brust", "der", "Lant\u00b7ze", "Staal", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du warst in gr\u00f6\u00dfrem Muth und L\u00f6wen-Hertz zu schauen/", "tokens": ["Du", "warst", "in", "gr\u00f6\u00df\u00b7rem", "Muth", "und", "L\u00f6\u00b7wen\u00b7Hertz", "zu", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "We\u00f1 du dein Glaubens Schwerd hast auf den Feind gewetzt.", "tokens": ["We\u00f1", "du", "dein", "Glau\u00b7bens", "Schwerd", "hast", "auf", "den", "Feind", "ge\u00b7wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So ist dein Kampff mehr schwer als einer je gewesen/", "tokens": ["So", "ist", "dein", "Kampff", "mehr", "schwer", "als", "ei\u00b7ner", "je", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "ART", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den uns das stoltze Rom in seinen Fechtern wie\u00df/", "tokens": ["Den", "uns", "das", "stolt\u00b7ze", "Rom", "in", "sei\u00b7nen", "Fech\u00b7tern", "wie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es hat zwar Crocodil und Tiegerthier erlesen/", "tokens": ["Es", "hat", "zwar", "Cro\u00b7co\u00b7dil", "und", "Tie\u00b7ger\u00b7thier", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die es/ O Grausamkeit! auff nackte Menschen lie\u00df!", "tokens": ["Die", "es", "/", "O", "Grau\u00b7sam\u00b7keit", "!", "auff", "nack\u00b7te", "Men\u00b7schen", "lie\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "NE", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du aber hast gek\u00e4mpfft gar mit der alten Schlangen/", "tokens": ["Du", "a\u00b7ber", "hast", "ge\u00b7k\u00e4mpfft", "gar", "mit", "der", "al\u00b7ten", "Schlan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die nach der Seelen stets mit offnem Rachen schnappt.", "tokens": ["Die", "nach", "der", "See\u00b7len", "stets", "mit", "off\u00b7nem", "Ra\u00b7chen", "schnappt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So hat den Python nicht Apollens Arm gefangen/", "tokens": ["So", "hat", "den", "Py\u00b7thon", "nicht", "A\u00b7pol\u00b7lens", "Arm", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "NE", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als du den schn\u00f6den F\u00fcrst der Finstern\u00fc\u00df ertappt.", "tokens": ["Als", "du", "den", "schn\u00f6\u00b7den", "F\u00fcrst", "der", "Fins\u00b7ter\u00b7n\u00fc\u00df", "er\u00b7tappt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Erblaste ", "tokens": ["Er\u00b7blas\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Den hochber\u00fchmten Kampf/ trugst du dein Creutz nur vor?", "tokens": ["Den", "hoch\u00b7be\u00b7r\u00fchm\u00b7ten", "Kampf", "/", "trugst", "du", "dein", "Creutz", "nur", "vor", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hast du zu diesem dich im Creutz allein gewendet/", "tokens": ["Hast", "du", "zu", "die\u00b7sem", "dich", "im", "Creutz", "al\u00b7lein", "ge\u00b7wen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "PRF", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der an desselben Holtz sein Leben auch verlohr.", "tokens": ["Der", "an", "des\u00b7sel\u00b7ben", "Holtz", "sein", "Le\u00b7ben", "auch", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In solchem Zeichen hast du Ritterin gesieget.", "tokens": ["In", "sol\u00b7chem", "Zei\u00b7chen", "hast", "du", "Rit\u00b7te\u00b7rin", "ge\u00b7sie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es schweige doch die Welt von ihrem K\u00e4mpffen still.", "tokens": ["Es", "schwei\u00b7ge", "doch", "die", "Welt", "von", "ih\u00b7rem", "K\u00e4mpf\u00b7fen", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dein gut Gewissen ists/ das dich allein vergn\u00fcget/", "tokens": ["Dein", "gut", "Ge\u00b7wis\u00b7sen", "ists", "/", "das", "dich", "al\u00b7lein", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVPP", "VAFIN", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dein wahres Christenthum das kr\u00e4fftigste Sigill.", "tokens": ["Dein", "wah\u00b7res", "Chris\u00b7ten\u00b7thum", "das", "kr\u00e4ff\u00b7tigs\u00b7te", "Si\u00b7gill", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und wie ein Fechter sonst/ nicht wie die Andabaten/", "tokens": ["Und", "wie", "ein", "Fech\u00b7ter", "sonst", "/", "nicht", "wie", "die", "A\u00b7nda\u00b7ba\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "$(", "PTKNEG", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vergeb\u2019ne Streiche thut und mit den L\u00fcfften spielt;", "tokens": ["Ver\u00b7ge\u00b7b'\u00b7ne", "Strei\u00b7che", "thut", "und", "mit", "den", "L\u00fcff\u00b7ten", "spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.31": {"text": "So liest du dir auch hier nicht andre H\u00fclffe rathen/", "tokens": ["So", "liest", "du", "dir", "auch", "hier", "nicht", "and\u00b7re", "H\u00fclf\u00b7fe", "ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du hast durch GOttes Geist geleitet scharff gezielt.", "tokens": ["Du", "hast", "durch", "Got\u00b7tes", "Geist", "ge\u00b7lei\u00b7tet", "scharff", "ge\u00b7zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "VVPP", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Lieffst mit genauem Flei\u00df in des Beruffes Schrancken/", "tokens": ["Lieffst", "mit", "ge\u00b7nau\u00b7em", "Flei\u00df", "in", "des", "Be\u00b7ruf\u00b7fes", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und woltest Tag und Nacht in der Bereitschafft stehn.", "tokens": ["Und", "wol\u00b7test", "Tag", "und", "Nacht", "in", "der", "Be\u00b7reit\u00b7schafft", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "KON", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es mag sich noch Athen umb seine L\u00e4uffer zancken/", "tokens": ["Es", "mag", "sich", "noch", "A\u00b7then", "umb", "sei\u00b7ne", "L\u00e4uf\u00b7fer", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "NE", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Und wer am ersten soll gekr\u00f6nt als Sieger gehn.", "tokens": ["Und", "wer", "am", "ers\u00b7ten", "soll", "ge\u00b7kr\u00f6nt", "als", "Sie\u00b7ger", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "VMFIN", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dort ward ein schlechter Platz in kurtzer Zeit durchrennet", "tokens": ["Dort", "ward", "ein", "schlech\u00b7ter", "Platz", "in", "kurt\u00b7zer", "Zeit", "durch\u00b7ren\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und blo\u00df ein Krantz ertheilt von Eich\u2019 und Myhrrten Laub.", "tokens": ["Und", "blo\u00df", "ein", "Krantz", "er\u00b7theilt", "von", "Eich'", "und", "Myhrr\u00b7ten", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wer deinen Siegs-Krantz sieht und deine Krone kennet/", "tokens": ["Wer", "dei\u00b7nen", "Siegs\u00b7Krantz", "sieht", "und", "dei\u00b7ne", "Kro\u00b7ne", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der sagt/ das Perlen Gla\u00df und Diamanten Staub.", "tokens": ["Der", "sagt", "/", "das", "Per\u00b7len", "Gla\u00df", "und", "Di\u00b7a\u00b7man\u00b7ten", "Staub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "ART", "NN", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie eyffrig hast du nicht bey deinem Fahn gehalten?", "tokens": ["Wie", "eyf\u00b7frig", "hast", "du", "nicht", "bey", "dei\u00b7nem", "Fahn", "ge\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Es war ja Christi Creutz der Schwachheit Trost und Stab.", "tokens": ["Es", "war", "ja", "Chris\u00b7ti", "Creutz", "der", "Schwach\u00b7heit", "Trost", "und", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "NN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der Ertz-Schrein/ der den Schatz des Himmels pflegt zu halten/", "tokens": ["Der", "Ertz\u00b7Schrein", "/", "der", "den", "Schatz", "des", "Him\u00b7mels", "pflegt", "zu", "hal\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Der Nachla\u00df aller Schuld/ der Gottsfurcht Opffer Gab.", "tokens": ["Der", "Nachl\u00b7a\u00df", "al\u00b7ler", "Schuld", "/", "der", "Gotts\u00b7furcht", "Opf\u00b7fer", "Gab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Das hast du Seelige mit solchem Muth getragen/", "tokens": ["Das", "hast", "du", "See\u00b7li\u00b7ge", "mit", "sol\u00b7chem", "Muth", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df seine Last dir leicht und sein Joch sanffte schien.", "tokens": ["Da\u00df", "sei\u00b7ne", "Last", "dir", "leicht", "und", "sein", "Joch", "sanff\u00b7te", "schien", "."], "token_