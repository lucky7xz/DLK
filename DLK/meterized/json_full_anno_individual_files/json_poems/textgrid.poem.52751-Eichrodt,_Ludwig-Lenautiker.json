{"textgrid.poem.52751": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lenautiker", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meine Liebe, die seitdem verwest,", "tokens": ["Auf", "mei\u00b7ne", "Lie\u00b7be", ",", "die", "seit\u00b7dem", "ver\u00b7west", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Geier gierig \u00fcber Leichen,", "tokens": ["Wie", "Gei\u00b7er", "gie\u00b7rig", "\u00fc\u00b7ber", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hinst\u00fcrzten wieder der Verzweiflungspest", "tokens": ["Hin\u00b7st\u00fcrz\u00b7ten", "wie\u00b7der", "der", "Ver\u00b7zwei\u00b7flungs\u00b7pest"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gluth\u00e4ugige Gedanken ohne Weichen:", "tokens": ["Glut\u00b7h\u00e4u\u00b7gi\u00b7ge", "Ge\u00b7dan\u00b7ken", "oh\u00b7ne", "Wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Erinnerung, des Wahnsinns Mutter, sie,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", ",", "des", "Wahn\u00b7sinns", "Mut\u00b7ter", ",", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sammt ihrer hurtigen Schwester, Phantasie,", "tokens": ["Sammt", "ih\u00b7rer", "hur\u00b7ti\u00b7gen", "Schwes\u00b7ter", ",", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Erfa\u00dfte mich auf \u00f6der, weiter Reise \u2013", "tokens": ["Er\u00b7fa\u00df\u00b7te", "mich", "auf", "\u00f6\u00b7der", ",", "wei\u00b7ter", "Rei\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "$,", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Genossen, die der Zufall zu mir spie,", "tokens": ["Ge\u00b7nos\u00b7sen", ",", "die", "der", "Zu\u00b7fall", "zu", "mir", "spie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Schreckten die Geier und die Speise.", "tokens": ["Schreck\u00b7ten", "die", "Gei\u00b7er", "und", "die", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Durch's Thor der Wolken rollte voll und rund", "tokens": ["Durch's", "Thor", "der", "Wol\u00b7ken", "roll\u00b7te", "voll", "und", "rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die br\u00fcllende Gewalt des Donnerl\u00e4rmes,", "tokens": ["Die", "br\u00fcl\u00b7len\u00b7de", "Ge\u00b7walt", "des", "Don\u00b7ner\u00b7l\u00e4r\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Jovis Zorn verk\u00fcndet durch den Mund", "tokens": ["Wie", "Jo\u00b7vis", "Zorn", "ver\u00b7k\u00fcn\u00b7det", "durch", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des fu\u00dfbeschwingten, redekund'gen Hermes.", "tokens": ["Des", "fu\u00df\u00b7be\u00b7schwing\u00b7ten", ",", "re\u00b7de\u00b7kun\u00b7d'\u00b7gen", "Her\u00b7mes", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dann Todtenstille \u2013 \u00fcber Moos und Uld,", "tokens": ["Dann", "Tod\u00b7ten\u00b7stil\u00b7le", "\u2013", "\u00fc\u00b7ber", "Moos", "und", "Uld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gleichwie verstummen wegen arger Schuld,", "tokens": ["Gleich\u00b7wie", "ver\u00b7stum\u00b7men", "we\u00b7gen", "ar\u00b7ger", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gleich einem b\u00f6sen lastenden Gewissen,", "tokens": ["Gleich", "ei\u00b7nem", "b\u00f6\u00b7sen", "las\u00b7ten\u00b7den", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So schweigt der See, da\u00df vor'ge Tageshuld", "tokens": ["So", "schweigt", "der", "See", ",", "da\u00df", "vor'\u00b7ge", "Ta\u00b7ges\u00b7huld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein'n Mord aus seiner Brust gerissen.", "tokens": ["Ein'n", "Mord", "aus", "sei\u00b7ner", "Brust", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha, wie der Blitz in's w\u00fcste Wasser zischt", "tokens": ["Ha", ",", "wie", "der", "Blitz", "in's", "w\u00fcs\u00b7te", "Was\u00b7ser", "zischt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und greller Schein die Felsen blendet,", "tokens": ["Und", "grel\u00b7ler", "Schein", "die", "Fel\u00b7sen", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht anders, wenn ein Hoffnungsstrahl sich mischt", "tokens": ["Nicht", "an\u00b7ders", ",", "wenn", "ein", "Hoff\u00b7nungs\u00b7strahl", "sich", "mischt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In eine Brust, die qualenvoll verendet.", "tokens": ["In", "ei\u00b7ne", "Brust", ",", "die", "qua\u00b7len\u00b7voll", "ver\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt rasseln Schlossen in das bange Thal,", "tokens": ["Jetzt", "ras\u00b7seln", "Schlos\u00b7sen", "in", "das", "ban\u00b7ge", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wei\u00df, gro\u00df, gedr\u00e4ngt, in z\u00fcgelloser Zahl,", "tokens": ["Wei\u00df", ",", "gro\u00df", ",", "ge\u00b7dr\u00e4ngt", ",", "in", "z\u00fc\u00b7gel\u00b7lo\u00b7ser", "Zahl", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie Sparterpfeile in den Kn\u00e4u'l der Perser,", "tokens": ["Wie", "Spar\u00b7ter\u00b7pfei\u00b7le", "in", "den", "Kn\u00e4u'l", "der", "Per\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie K\u00fcrassiere auf der St\u00e4tte blut'ger Wahl", "tokens": ["Wie", "K\u00fc\u00b7ras\u00b7sie\u00b7re", "auf", "der", "St\u00e4t\u00b7te", "blut'\u00b7ger", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "R\u00fcckprallen vor dem Knall der M\u00f6rser.", "tokens": ["R\u00fcck\u00b7pral\u00b7len", "vor", "dem", "Knall", "der", "M\u00f6r\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Nacht durchfluthet rauschend die Natur:", "tokens": ["Und", "Nacht", "durch\u00b7flut\u00b7het", "rau\u00b7schend", "die", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Fluch des scheidend zornigen Tages,", "tokens": ["Ein", "Fluch", "des", "schei\u00b7dend", "zor\u00b7ni\u00b7gen", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wieder donnert's wie ein R\u00fctlischwur", "tokens": ["Und", "wie\u00b7der", "don\u00b7nert's", "wie", "ein", "R\u00fct\u00b7lischwur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von tausend M\u00e4nnern rauhen Schlages.", "tokens": ["Von", "tau\u00b7send", "M\u00e4n\u00b7nern", "rau\u00b7hen", "Schla\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So ritten wir durch's krachende Geh\u00f6lz \u2013", "tokens": ["So", "rit\u00b7ten", "wir", "durch's", "kra\u00b7chen\u00b7de", "Ge\u00b7h\u00f6lz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis uns der purpurkalte Farbenschmelz", "tokens": ["Bis", "uns", "der", "pur\u00b7pur\u00b7kal\u00b7te", "Far\u00b7ben\u00b7schmelz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im Osten schauerte sein Licht entgegen,", "tokens": ["Im", "Os\u00b7ten", "schau\u00b7er\u00b7te", "sein", "Licht", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bis, hinter uns der ungeheure Fels,", "tokens": ["Bis", ",", "hin\u00b7ter", "uns", "der", "un\u00b7ge\u00b7heu\u00b7re", "Fels", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wir trabten auf vertrauten Wegen.", "tokens": ["Wir", "trab\u00b7ten", "auf", "ver\u00b7trau\u00b7ten", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf meine Liebe, die seitdem verwest,", "tokens": ["Auf", "mei\u00b7ne", "Lie\u00b7be", ",", "die", "seit\u00b7dem", "ver\u00b7west", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Geier gierig \u00fcber Leichen,", "tokens": ["Wie", "Gei\u00b7er", "gie\u00b7rig", "\u00fc\u00b7ber", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hinst\u00fcrzten wieder der Verzweiflungspest", "tokens": ["Hin\u00b7st\u00fcrz\u00b7ten", "wie\u00b7der", "der", "Ver\u00b7zwei\u00b7flungs\u00b7p