{"textgrid.poem.66820": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: O kleine goldene Schlange mit den gr\u00fcnen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O kleine goldene Schlange mit den gr\u00fcnen", "tokens": ["O", "klei\u00b7ne", "gol\u00b7de\u00b7ne", "Schlan\u00b7ge", "mit", "den", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Juwelenaugen! Seliges Reptil,", "tokens": ["Ju\u00b7we\u00b7len\u00b7au\u00b7gen", "!", "Se\u00b7li\u00b7ges", "Rep\u00b7til", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den rosigen Kn\u00f6chel mit dem Z\u00fcnglein k\u00fcssend,", "tokens": ["Den", "ro\u00b7si\u00b7gen", "Kn\u00f6\u00b7chel", "mit", "dem", "Z\u00fcn\u00b7glein", "k\u00fcs\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das l\u00fcstern nach dem zarten Fleische leckt!", "tokens": ["Das", "l\u00fcs\u00b7tern", "nach", "dem", "zar\u00b7ten", "Flei\u00b7sche", "leckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt streift die rechte Hand dich leicht hinauf,", "tokens": ["Jetzt", "streift", "die", "rech\u00b7te", "Hand", "dich", "leicht", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Geschmeidig schmiegst du dich der weichen F\u00fclle", "tokens": ["Ge\u00b7schmei\u00b7dig", "schmiegst", "du", "dich", "der", "wei\u00b7chen", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Des sch\u00f6nsten Armes, den Natur gebildet,", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "Ar\u00b7mes", ",", "den", "Na\u00b7tur", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und zitterst lustberauscht vom Kopf zum Schw\u00e4nzchen,", "tokens": ["Und", "zit\u00b7terst", "lust\u00b7be\u00b7rauscht", "vom", "Kopf", "zum", "Schw\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Z\u00e4rtlich und fest geringelt um Josepha.", "tokens": ["Z\u00e4rt\u00b7lich", "und", "fest", "ge\u00b7rin\u00b7gelt", "um", "Jo\u00b7se\u00b7pha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "APPR", "NE", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Josephas Busen atmet Walzertakt,", "tokens": ["Jo\u00b7se\u00b7phas", "Bu\u00b7sen", "at\u00b7met", "Wal\u00b7zer\u00b7takt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Durch T\u00fcll und Spitzen bricht die Sch\u00f6nheit nackt.", "tokens": ["Durch", "T\u00fcll", "und", "Spit\u00b7zen", "bricht", "die", "Sch\u00f6n\u00b7heit", "nackt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Kein Joseph brennte durch vor dieser Eva.", "tokens": ["Kein", "Jo\u00b7se\u00b7ph", "brenn\u00b7te", "durch", "vor", "die\u00b7ser", "E\u00b7va", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NE", "VVFIN", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Der Kavaliere N\u00fcstern bl\u00e4hen sich,", "tokens": ["Der", "Ka\u00b7va\u00b7lie\u00b7re", "N\u00fcs\u00b7tern", "bl\u00e4\u00b7hen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Begierde zuckt und zittert, kaum gez\u00e4umt,", "tokens": ["Be\u00b7gier\u00b7de", "zuckt", "und", "zit\u00b7tert", ",", "kaum", "ge\u00b7z\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Hopser schweigt, Blick und Champagner sch\u00e4umt.", "tokens": ["Der", "Hop\u00b7ser", "schweigt", ",", "Blick", "und", "Cham\u00b7pag\u00b7ner", "sch\u00e4umt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Erdbeere und Vanille k\u00fcssen sich", "tokens": ["Erd\u00b7bee\u00b7re", "und", "Va\u00b7nil\u00b7le", "k\u00fcs\u00b7sen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "In den kristallnen Bettchen, um zu sterben", "tokens": ["In", "den", "kris\u00b7tall\u00b7nen", "Bett\u00b7chen", ",", "um", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Den Tod der Liebe in Josephas Mund.", "tokens": ["Den", "Tod", "der", "Lie\u00b7be", "in", "Jo\u00b7se\u00b7phas", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Es flammt der Saal, durch offne Fenster f\u00e4cheln", "tokens": ["Es", "flammt", "der", "Saal", ",", "durch", "off\u00b7ne", "Fens\u00b7ter", "f\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Vorh\u00e4nge Maienhauch den G\u00e4sten zu,", "tokens": ["Vor\u00b7h\u00e4n\u00b7ge", "Mai\u00b7en\u00b7hauch", "den", "G\u00e4s\u00b7ten", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.21": {"text": "Und K\u00fchlung saugen durstige Atlasschuh.", "tokens": ["Und", "K\u00fch\u00b7lung", "sau\u00b7gen", "durs\u00b7ti\u00b7ge", "At\u00b7las\u00b7schuh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "O kleine goldene Schlange! Schillre, schillre", "tokens": ["O", "klei\u00b7ne", "gol\u00b7de\u00b7ne", "Schlan\u00b7ge", "!", "Schill\u00b7re", ",", "schill\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "ADJA", "ADJA", "NN", "$.", "NN", "$,", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Nur immer her zu mir in meine Nische,", "tokens": ["Nur", "im\u00b7mer", "her", "zu", "mir", "in", "mei\u00b7ne", "Ni\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wo von dem Schwindel ich Erholung schl\u00fcrfe!", "tokens": ["Wo", "von", "dem", "Schwin\u00b7del", "ich", "Er\u00b7ho\u00b7lung", "schl\u00fcr\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Willst du vielleicht ein M\u00e4rchen mir erz\u00e4hlen,", "tokens": ["Willst", "du", "viel\u00b7leicht", "ein", "M\u00e4r\u00b7chen", "mir", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Ein M\u00e4rchen von \u2013 Angstschauer sch\u00fcttelt mich \u2013", "tokens": ["Ein", "M\u00e4r\u00b7chen", "von", "\u2013", "Angst\u00b7schau\u00b7er", "sch\u00fct\u00b7telt", "mich", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$(", "NN", "VVFIN", "PPER", "$("], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ein M\u00e4rchen von \u2013 ich beuge mich hinaus,", "tokens": ["Ein", "M\u00e4r\u00b7chen", "von", "\u2013", "ich", "beu\u00b7ge", "mich", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$(", "PPER", "VVFIN", "PRF", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Mein St\u00f6hnen an das Herz der Nacht zu pressen \u2013", "tokens": ["Mein", "St\u00f6h\u00b7nen", "an", "das", "Herz", "der", "Nacht", "zu", "pres\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Ein M\u00e4rchen von den St\u00e4tten deiner Wiege,", "tokens": ["Ein", "M\u00e4r\u00b7chen", "von", "den", "St\u00e4t\u00b7ten", "dei\u00b7ner", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Den fernen goldnen Minen Jeniseisks?", "tokens": ["Den", "fer\u00b7nen", "gold\u00b7nen", "Mi\u00b7nen", "Je\u00b7ni\u00b7seisks", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ich starre in das Dunkel. Pl\u00f6tzlich schwimmt", "tokens": ["Ich", "star\u00b7re", "in", "das", "Dun\u00b7kel", ".", "Pl\u00f6tz\u00b7lich", "schwimmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der helle Ballsaal vor den Augen mir", "tokens": ["Der", "hel\u00b7le", "Ball\u00b7saal", "vor", "den", "Au\u00b7gen", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Mit tausend Lampen. In die Finsternis", "tokens": ["Mit", "tau\u00b7send", "Lam\u00b7pen", ".", "In", "die", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Verlischt er j\u00e4h. Vergitterte Kasernen", "tokens": ["Ver\u00b7lischt", "er", "j\u00e4h", ".", "Ver\u00b7git\u00b7ter\u00b7te", "Ka\u00b7ser\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Auftauchen d\u00fcstermassig. Posten stehn", "tokens": ["Auf\u00b7tau\u00b7chen", "d\u00fcs\u00b7ter\u00b7mas\u00b7sig", ".", "Pos\u00b7ten", "stehn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "An den verriegelten Portalen schnapsend.", "tokens": ["An", "den", "ver\u00b7rie\u00b7gel\u00b7ten", "Por\u00b7ta\u00b7len", "schnap\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.37": {"text": "Da springt vor meinem tiefer starrenden Blick", "tokens": ["Da", "springt", "vor", "mei\u00b7nem", "tie\u00b7fer", "star\u00b7ren\u00b7den", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "Die T\u00fcre auf ... In Kisten Mann an Mann", "tokens": ["Die", "T\u00fc\u00b7re", "auf", "...", "In", "Kis\u00b7ten", "Mann", "an", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "$(", "APPR", "NE", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und \u00fcber'nander aufgeschichtet liegen", "tokens": ["Und", "\u00fc\u00b7ber'\u00b7nan\u00b7der", "auf\u00b7ge\u00b7schich\u00b7tet", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Die Minengr\u00e4ber. Dunst erw\u00fcrgt den Odem,", "tokens": ["Die", "Mi\u00b7nen\u00b7gr\u00e4\u00b7ber", ".", "Dunst", "er\u00b7w\u00fcrgt", "den", "O\u00b7dem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Und Wanzenhorden wallen, krabbeln, fallen", "tokens": ["Und", "Wan\u00b7zen\u00b7hor\u00b7den", "wal\u00b7len", ",", "krab\u00b7beln", ",", "fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NN", "VVINF", "$,", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Wie Timurlenks Armeen und saufen Blut.", "tokens": ["Wie", "Ti\u00b7mur\u00b7lenks", "Ar\u00b7me\u00b7en", "und", "sau\u00b7fen", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "KON", "VVFIN", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "In Lumpendecken eingewickelt n\u00e4cht'gen", "tokens": ["In", "Lum\u00b7pen\u00b7de\u00b7cken", "ein\u00b7ge\u00b7wi\u00b7ckelt", "n\u00e4cht'\u0