{"textgrid.poem.58282": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "320. Mein Alles allein nur Jesus soll sein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Jesus ist mein Heil, worin ich kann bestehen,", "tokens": ["Nur", "Je\u00b7sus", "ist", "mein", "Heil", ",", "wo\u00b7rin", "ich", "kann", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Jesus ist der Brunn, draus Gnad und Wahrheit fleu\u00dft,", "tokens": ["Nur", "Je\u00b7sus", "ist", "der", "Brunn", ",", "draus", "Gnad", "und", "Wahr\u00b7heit", "fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "$,", "PAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur Jesus ist das Bild, worauf ich blo\u00df mu\u00df sehen,", "tokens": ["Nur", "Je\u00b7sus", "ist", "das", "Bild", ",", "wo\u00b7rauf", "ich", "blo\u00df", "mu\u00df", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur Jesus ist der Hirt, der mich stets f\u00fchrt und speist,", "tokens": ["Nur", "Je\u00b7sus", "ist", "der", "Hirt", ",", "der", "mich", "stets", "f\u00fchrt", "und", "speist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur Jesus ist die Kraft, durch die ich \u00fcberwinde,", "tokens": ["Nur", "Je\u00b7sus", "ist", "die", "Kraft", ",", "durch", "die", "ich", "\u00fc\u00b7berw\u00b7in\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur Jesus ist der Grund der wahren Heiligkeit,", "tokens": ["Nur", "Je\u00b7sus", "ist", "der", "Grund", "der", "wah\u00b7ren", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur Jesus ist mein Schatz, worin ich alles finde,", "tokens": ["Nur", "Je\u00b7sus", "ist", "mein", "Schatz", ",", "wo\u00b7rin", "ich", "al\u00b7les", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nur Jesus soll es sein in Zeit und Ewigkeit!", "tokens": ["Nur", "Je\u00b7sus", "soll", "es", "sein", "in", "Zeit", "und", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "PPER", "VAINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1"