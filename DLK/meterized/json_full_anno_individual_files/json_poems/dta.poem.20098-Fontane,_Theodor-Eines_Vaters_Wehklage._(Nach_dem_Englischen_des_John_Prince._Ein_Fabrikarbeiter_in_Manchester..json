{"dta.poem.20098": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Eines Vaters Wehklage.  \n (Nach dem Englischen des John Prince.  Ein Fabrikarbeiter in Manchester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Hell schien der Mond, und seinen blassen Schimmer", "tokens": ["Hell", "schien", "der", "Mond", ",", "und", "sei\u00b7nen", "blas\u00b7sen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ausweinend in mein k\u00fcmmerliches Zimmer,", "tokens": ["Aus\u00b7wei\u00b7nend", "in", "mein", "k\u00fcm\u00b7mer\u00b7li\u00b7ches", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlieh er drin viel silberfarbnen Duft", "tokens": ["Ver\u00b7lieh", "er", "drin", "viel", "sil\u00b7ber\u00b7farb\u00b7nen", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der schlummerstillen, tr\u00e4umerischen Luft.", "tokens": ["Der", "schlum\u00b7mer\u00b7stil\u00b7len", ",", "tr\u00e4u\u00b7me\u00b7ri\u00b7schen", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da war\u2019s, da klopfte, weh mir! an mein Fenster", "tokens": ["Da", "wa\u00b7r's", ",", "da", "klopf\u00b7te", ",", "weh", "mir", "!", "an", "mein", "Fens\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das schrecklich r\u00e4thselvollste der Gespenster,", "tokens": ["Das", "schreck\u00b7lich", "r\u00e4th\u00b7sel\u00b7volls\u00b7te", "der", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da hat der Tod, ich war umsonst verschanzt,", "tokens": ["Da", "hat", "der", "Tod", ",", "ich", "war", "um\u00b7sonst", "ver\u00b7schanzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An meinem Herd sein Banner aufgepflanzt.", "tokens": ["An", "mei\u00b7nem", "Herd", "sein", "Ban\u00b7ner", "auf\u00b7ge\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Trauertag! die letzte seiner Stunden", "tokens": ["O", "Trau\u00b7er\u00b7tag", "!", "die", "letz\u00b7te", "sei\u00b7ner", "Stun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlug meinem Herzen unheilbare Wunden:", "tokens": ["Schlug", "mei\u00b7nem", "Her\u00b7zen", "un\u00b7heil\u00b7ba\u00b7re", "Wun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Blume gleich, die schon im Lenz geknickt,", "tokens": ["Der", "Blu\u00b7me", "gleich", ",", "die", "schon", "im", "Lenz", "ge\u00b7knickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hab ich in ihr mein sterbend Kind erblickt.", "tokens": ["Hab", "ich", "in", "ihr", "mein", "ster\u00b7bend", "Kind", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch hielt ich zitternd es auf meinen Knien,", "tokens": ["Noch", "hielt", "ich", "zit\u00b7ternd", "es", "auf", "mei\u00b7nen", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als es vom Tode schon gestempelt schien,", "tokens": ["Als", "es", "vom", "To\u00b7de", "schon", "ge\u00b7stem\u00b7pelt", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und als sein liebes, liebes Auge brach,", "tokens": ["Und", "als", "sein", "lie\u00b7bes", ",", "lie\u00b7bes", "Au\u00b7ge", "brach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sein letzter Seufzer mir zum Herzen sprach,", "tokens": ["Sein", "letz\u00b7ter", "Seuf\u00b7zer", "mir", "zum", "Her\u00b7zen", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Entflohen war, still ohne Kampf, sein Geist,", "tokens": ["Ent\u00b7flo\u00b7hen", "war", ",", "still", "oh\u00b7ne", "Kampf", ",", "sein", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADJD", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da f\u00fchlt ich mich auf immerdar verwaist.", "tokens": ["Da", "f\u00fchlt", "ich", "mich", "auf", "im\u00b7mer\u00b7dar", "ver\u00b7waist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bald ward er, den gehegt ich und gepflegt,", "tokens": ["Bald", "ward", "er", ",", "den", "ge\u00b7hegt", "ich", "und", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ART", "VVFIN", "PPER", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sanft schlummernd in sein Erdenbett gelegt;", "tokens": ["Sanft", "schlum\u00b7mernd", "in", "sein", "Er\u00b7den\u00b7bett", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mitleidge Seelen schlossen einen Kreis,", "tokens": ["Mit\u00b7leid\u00b7ge", "See\u00b7len", "schlos\u00b7sen", "ei\u00b7nen", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Still betend standen sie, und weinten leis.", "tokens": ["Still", "be\u00b7tend", "stan\u00b7den", "sie", ",", "und", "wein\u00b7ten", "leis", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Der Pfarrer sprach; ich aber h\u00f6rte nur", "tokens": ["Der", "Pfar\u00b7rer", "sprach", ";", "ich", "a\u00b7ber", "h\u00f6r\u00b7te", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "\u201ewirst deinen Liebling hier nicht wiedersehn!", "tokens": ["\u201e", "wirst", "dei\u00b7nen", "Lieb\u00b7ling", "hier", "nicht", "wie\u00b7der\u00b7sehn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bald ist der Liebe letzter Dienst geschehn.\u201c", "tokens": ["Bald", "ist", "der", "Lie\u00b7be", "letz\u00b7ter", "Dienst", "ge\u00b7schehn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dann schlich, oft r\u00fcckw\u00e4rts schauend, ich von", "tokens": ["Dann", "schlich", ",", "oft", "r\u00fcck\u00b7w\u00e4rts", "schau\u00b7end", ",", "ich", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,", "PPER", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Im Weltgew\u00fchl den Schmerz zu \u00fcbermannen.", "tokens": ["Im", "Welt\u00b7ge\u00b7w\u00fchl", "den", "Schmerz", "zu", "\u00fc\u00b7berm\u00b7an\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, du mein Trost und deiner Mutter Stolz,", "tokens": ["Ja", ",", "du", "mein", "Trost", "und", "dei\u00b7ner", "Mut\u00b7ter", "Stolz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die, wenn du krank, in Thr\u00e4nen schon zerschmolz,", "tokens": ["Die", ",", "wenn", "du", "krank", ",", "in", "Thr\u00e4\u00b7nen", "schon", "zer\u00b7schmolz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADJD", "$,", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist dahin! doch ward dir eine Welt,", "tokens": ["Du", "bist", "da\u00b7hin", "!", "doch", "ward", "dir", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$.", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo man der Tugend keine Netze stellt.", "tokens": ["Wo", "man", "der", "Tu\u00b7gend", "kei\u00b7ne", "Net\u00b7ze", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du darfst im Licht und in der Wahrheit sein,", "tokens": ["Du", "darfst", "im", "Licht", "und", "in", "der", "Wahr\u00b7heit", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Derweil ich hier gefangen und allein,", "tokens": ["Der\u00b7weil", "ich", "hier", "ge\u00b7fan\u00b7gen", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Allein! der Barke gleich, auf offnen Meeren,", "tokens": ["Al\u00b7lein", "!", "der", "Bar\u00b7ke", "gleich", ",", "auf", "off\u00b7nen", "Mee\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn sich die Elemente rings emp\u00f6ren;", "tokens": ["Wenn", "sich", "die", "E\u00b7le\u00b7men\u00b7te", "rings", "em\u00b7p\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Allein! ein Harfenspiel, das halb zertr\u00fcmmert,", "tokens": ["Al\u00b7lein", "!", "ein", "Har\u00b7fen\u00b7spiel", ",", "das", "halb", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nur f\u00fcrder noch in Klaget\u00f6nen wimmert.", "tokens": ["Nur", "f\u00fcr\u00b7der", "noch", "in", "Kla\u00b7ge\u00b7t\u00f6\u00b7nen", "wim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich traure heimlich: w\u00fcrde sonst ja mehren", "tokens": ["Ich", "trau\u00b7re", "heim\u00b7lich", ":", "w\u00fcr\u00b7de", "sonst", "ja", "meh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Qualen, die an deiner Mutter zehren,", "tokens": ["Die", "Qua\u00b7len", ",", "die", "an", "dei\u00b7ner", "Mut\u00b7ter", "zeh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den tiefen Schmerz, um den sie seufzt und weint,", "tokens": ["Den", "tie\u00b7fen", "Schmerz", ",", "um", "den", "sie", "seufzt", "und", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "ART", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ausgepr\u00e4gt in jedem Zug erscheint.", "tokens": ["Der", "aus\u00b7ge\u00b7pr\u00e4gt", "in", "je\u00b7dem", "Zug", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Oft am Kamine sitzen wir zusammen,", "tokens": ["Oft", "am", "Ka\u00b7mi\u00b7ne", "sit\u00b7zen", "wir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schauen, dein gedenkend, in die Flammen,", "tokens": ["Und", "schau\u00b7en", ",", "dein", "ge\u00b7den\u00b7kend", ",", "in", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PPOSAT", "VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und sprechen von der Wangenr\u00f6the \u2014 ach!", "tokens": ["Und", "spre\u00b7chen", "von", "der", "Wan\u00b7gen\u00b7r\u00f6\u00b7the", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die langes Leben l\u00fcgnerisch versprach.", "tokens": ["Die", "lan\u00b7ges", "Le\u00b7ben", "l\u00fcg\u00b7ne\u00b7risch", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wir denken jedes Blicks und Wortes dann,", "tokens": ["Wir", "den\u00b7ken", "je\u00b7des", "Blicks", "und", "Wor\u00b7tes", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "li