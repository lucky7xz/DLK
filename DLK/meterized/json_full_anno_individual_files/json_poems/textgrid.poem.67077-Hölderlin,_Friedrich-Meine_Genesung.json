{"textgrid.poem.67077": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Meine Genesung", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jede Bl\u00fcte war gefallen", "tokens": ["Je\u00b7de", "Bl\u00fc\u00b7te", "war", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Stamme; Mut und Kraft,", "tokens": ["Von", "dem", "Stam\u00b7me", ";", "Mut", "und", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcrder meine Bahn zu wallen,", "tokens": ["F\u00fcr\u00b7der", "mei\u00b7ne", "Bahn", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War im Kampfe mir erschlafft;", "tokens": ["War", "im", "Kamp\u00b7fe", "mir", "er\u00b7schlafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weggeschwunden Lust und Leben,", "tokens": ["Weg\u00b7ge\u00b7schwun\u00b7den", "Lust", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fcher Jahre stolze Ruh;", "tokens": ["Fr\u00fc\u00b7her", "Jah\u00b7re", "stol\u00b7ze", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Meinem Grame hingegeben,", "tokens": ["Mei\u00b7nem", "Gra\u00b7me", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wankt ich still dem Grabe zu.", "tokens": ["Wankt", "ich", "still", "dem", "Gra\u00b7be", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Himmel, wie das Herz vergebens", "tokens": ["Him\u00b7mel", ",", "wie", "das", "Herz", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft nach edler Liebe rang,", "tokens": ["Oft", "nach", "ed\u00b7ler", "Lie\u00b7be", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft get\u00e4uscht des Erdelebens", "tokens": ["Oft", "ge\u00b7t\u00e4uscht", "des", "Er\u00b7de\u00b7le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4um und Hoffnungen umschlang!", "tokens": ["Tr\u00e4um", "und", "Hoff\u00b7nun\u00b7gen", "um\u00b7schlang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, den Kummer abzuwenden,", "tokens": ["Ach", ",", "den", "Kum\u00b7mer", "ab\u00b7zu\u00b7wen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bat ich, freundliche Natur!", "tokens": ["Bat", "ich", ",", "freund\u00b7li\u00b7che", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oft von deinen Mutterh\u00e4nden", "tokens": ["Oft", "von", "dei\u00b7nen", "Mut\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Tropfen Freude nur.", "tokens": ["Ei\u00b7nen", "Trop\u00b7fen", "Freu\u00b7de", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha, an deinem G\u00f6ttermahle", "tokens": ["Ha", ",", "an", "dei\u00b7nem", "G\u00f6t\u00b7ter\u00b7mah\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trink ich nun Vergessenheit,", "tokens": ["Trink", "ich", "nun", "Ver\u00b7ges\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der vollen Zauberschale", "tokens": ["In", "der", "vol\u00b7len", "Zau\u00b7ber\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reichst du Kraft und S\u00fc\u00dfigkeit.", "tokens": ["Reichst", "du", "Kraft", "und", "S\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In Entz\u00fcckungen verloren", "tokens": ["In", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Staun ich die Verwandlung an!", "tokens": ["Staun", "ich", "die", "Ver\u00b7wand\u00b7lung", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flur und Hain ist neugeboren,", "tokens": ["Flur", "und", "Hain", "ist", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00f6ttlich strahlt der Lenz heran. \u2013", "tokens": ["G\u00f6tt\u00b7lich", "strahlt", "der", "Lenz", "he\u00b7ran", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich wieder Kraft gewinne,", "tokens": ["Da\u00df", "ich", "wie\u00b7der", "Kraft", "ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei wie einst und selig bin,", "tokens": ["Frei", "wie", "einst", "und", "se\u00b7lig", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADV", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dank ich deinem Himmelssinne,", "tokens": ["Dank", "ich",