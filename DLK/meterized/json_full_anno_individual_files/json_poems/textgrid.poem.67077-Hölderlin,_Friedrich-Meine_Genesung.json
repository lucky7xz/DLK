 "line.4": {"text": "Lyda, s\u00fc\u00dfe Retterin!", "tokens": ["Ly\u00b7da", ",", "s\u00fc\u00b7\u00dfe", "Ret\u00b7te\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Labung l\u00e4chelte dem M\u00fcden,", "tokens": ["La\u00b7bung", "l\u00e4\u00b7chel\u00b7te", "dem", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hohen Mut dein Auge zu,", "tokens": ["Ho\u00b7hen", "Mut", "dein", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hohen Mut, wie du zufrieden,", "tokens": ["Ho\u00b7hen", "Mut", ",", "wie", "du", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gut zu sein und gro\u00df wie du.", "tokens": ["Gut", "zu", "sein", "und", "gro\u00df", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "KON", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Stark in meiner Freuden F\u00fclle", "tokens": ["Stark", "in", "mei\u00b7ner", "Freu\u00b7den", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wall ich f\u00fcrder nun die Bahn,", "tokens": ["Wall", "ich", "f\u00fcr\u00b7der", "nun", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reizend in der Wolkenh\u00fclle", "tokens": ["Rei\u00b7zend", "in", "der", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammt das ferne Ziel mich an.", "tokens": ["Flammt", "das", "fer\u00b7ne", "Ziel", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mags den Peinigern gelingen!", "tokens": ["Mags", "den", "Pei\u00b7ni\u00b7gern", "ge\u00b7lin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag die bleiche Sorge sich", "tokens": ["Mag", "die", "blei\u00b7che", "Sor\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Um die stille Klause schwingen!", "tokens": ["Um", "die", "stil\u00b7le", "Klau\u00b7se", "schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lyda! Lyda tr\u00f6stet mich!", "tokens": ["Ly\u00b7da", "!", "Ly\u00b7da", "tr\u00f6s\u00b7tet", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}