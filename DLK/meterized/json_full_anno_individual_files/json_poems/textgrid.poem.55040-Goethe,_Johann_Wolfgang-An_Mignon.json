{"textgrid.poem.55040": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An Mignon", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber Tal und Flu\u00df getragen", "tokens": ["\u00dc\u00b7ber", "Tal", "und", "Flu\u00df", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehet rein der Sonne Wagen.", "tokens": ["Zie\u00b7het", "rein", "der", "Son\u00b7ne", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, sie regt in ihrem Lauf,", "tokens": ["Ach", ",", "sie", "regt", "in", "ih\u00b7rem", "Lauf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So wie deine, meine Schmerzen,", "tokens": ["So", "wie", "dei\u00b7ne", ",", "mei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tief im Herzen,", "tokens": ["Tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Immer morgens wieder auf.", "tokens": ["Im\u00b7mer", "mor\u00b7gens", "wie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum will mir die Nacht noch frommen,", "tokens": ["Kaum", "will", "mir", "die", "Nacht", "noch", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denn die Tr\u00e4ume selber kommen", "tokens": ["Denn", "die", "Tr\u00e4u\u00b7me", "sel\u00b7ber", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun in trauriger Gestalt,", "tokens": ["Nun", "in", "trau\u00b7ri\u00b7ger", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich f\u00fchle dieser Schmerzen,", "tokens": ["Und", "ich", "f\u00fch\u00b7le", "die\u00b7ser", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Still im Herzen,", "tokens": ["Still", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Heimlich bildende Gewalt.", "tokens": ["Heim\u00b7lich", "bil\u00b7den\u00b7de", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon seit manchen sch\u00f6nen Jahren", "tokens": ["Schon", "seit", "man\u00b7chen", "sch\u00f6\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seh ich unten Schiffe fahren;", "tokens": ["Seh", "ich", "un\u00b7ten", "Schif\u00b7fe", "fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes kommt an seinen Ort", "tokens": ["Je\u00b7des", "kommt", "an", "sei\u00b7nen", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ach, die steten Schmerzen,", "tokens": ["A\u00b7ber", "ach", ",", "die", "ste\u00b7ten", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fest im Herzen,", "tokens": ["Fest", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Schwimmen nicht im Strome fort.", "tokens": ["Schwim\u00b7men", "nicht", "im", "Stro\u00b7me", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n in Kleidern mu\u00df ich kommen,", "tokens": ["Sch\u00f6n", "in", "Klei\u00b7dern", "mu\u00df", "ich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Schrank sind sie genommen,", "tokens": ["Aus", "dem", "Schrank", "sind", "sie", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "