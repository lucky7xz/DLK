{"dta.poem.8180": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Kritik.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Liebes Herz, da\u00df wir Poeten", "tokens": ["Lie\u00b7bes", "Herz", ",", "da\u00df", "wir", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon genug der Liebeslieder,", "tokens": ["Schon", "ge\u00b7nug", "der", "Lie\u00b7bes\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, zu viel gedichtet h\u00e4tten.", "tokens": ["Ja", ",", "zu", "viel", "ge\u00b7dich\u00b7tet", "h\u00e4t\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKA", "PIS", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, es sind so kl\u00e4glich wenig,", "tokens": ["Ach", ",", "es", "sind", "so", "kl\u00e4g\u00b7lich", "we\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich z\u00e4hlte sie im Stillen,", "tokens": ["Denn", "ich", "z\u00e4hl\u00b7te", "sie", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum genug, dein Nadelb\u00fcchlein", "tokens": ["Kaum", "ge\u00b7nug", ",", "dein", "Na\u00b7del\u00b7b\u00fcch\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schicklich damit anzuf\u00fcllen.", "tokens": ["Schick\u00b7lich", "da\u00b7mit", "an\u00b7zu\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lieder, die von Liebe reimen,", "tokens": ["Lie\u00b7der", ",", "die", "von", "Lie\u00b7be", "rei\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen Tag f\u00fcr Tage wieder;", "tokens": ["Kom\u00b7men", "Tag", "f\u00fcr", "Ta\u00b7ge", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wir zwei Verliebte sprechen:", "tokens": ["Doch", "wir", "zwei", "Ver\u00b7lieb\u00b7te", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das sind keine Liebeslieder.", "tokens": ["Das", "sind", "kei\u00b7ne", "Lie\u00b7bes\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}