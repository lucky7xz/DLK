{"dta.poem.10199": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "I.                  \n  Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch oben fliegt ein Kranichheer nach Norden,", "tokens": ["Hoch", "o\u00b7ben", "fliegt", "ein", "Kra\u00b7nich\u00b7heer", "nach", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ihren Fl\u00fcgeln tropft die Morgensonne.", "tokens": ["Von", "ih\u00b7ren", "Fl\u00fc\u00b7geln", "tropft", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tief unten liegt der Ursulinenorden,", "tokens": ["Tief", "un\u00b7ten", "liegt", "der", "Ur\u00b7su\u00b7li\u00b7ne\u00b7nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Klostergarten tr\u00e4umt die alte Nonne.", "tokens": ["Im", "Klos\u00b7ter\u00b7gar\u00b7ten", "tr\u00e4umt", "die", "al\u00b7te", "Non\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von oben braust es m\u00e4chtig in Accorden", "tokens": ["Von", "o\u00b7ben", "braust", "es", "m\u00e4ch\u00b7tig", "in", "Ac\u00b7cor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nach unten tief in hoher Fr\u00fchlingswonne.", "tokens": ["Nach", "un\u00b7ten", "tief", "in", "ho\u00b7her", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verflogen \u2026 Oben ist es still geworden \u2014", "tokens": ["Ver\u00b7flo\u00b7gen", "\u2026", "O\u00b7ben", "ist", "es", "still", "ge\u00b7wor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "VAFIN", "PPER", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die greise Nonne betet zur Madonne.", "tokens": ["Die", "grei\u00b7se", "Non\u00b7ne", "be\u00b7tet", "zur", "Ma\u00b7don\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}