{"dta.poem.19541": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Strandkloster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Bollwerk und Mauer trutzen", "tokens": ["Boll\u00b7werk", "und", "Mau\u00b7er", "trut\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Wellenwurf schon ein Jahrtausend ja,", "tokens": ["Dem", "Wel\u00b7len\u00b7wurf", "schon", "ein", "Jahr\u00b7tau\u00b7send", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir singen, elf Capuzen,", "tokens": ["Wir", "sin\u00b7gen", ",", "elf", "Ca\u00b7pu\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein kr\u00e4ftig schallend Deo gloria!", "tokens": ["Ein", "kr\u00e4f\u00b7tig", "schal\u00b7lend", "Deo", "glo\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Kutten, stark gewoben,", "tokens": ["Die", "Kut\u00b7ten", ",", "stark", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umhingen uns in braunen Lappen lang,", "tokens": ["Um\u00b7hin\u00b7gen", "uns", "in", "brau\u00b7nen", "Lap\u00b7pen", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sind gemach verstoben,", "tokens": ["Sie", "sind", "ge\u00b7mach", "ver\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die St\u00e4ubchen irrten durch den Klostergang.", "tokens": ["Die", "St\u00e4ub\u00b7chen", "irr\u00b7ten", "durch", "den", "Klos\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Orgel im Empore", "tokens": ["Die", "Or\u00b7gel", "im", "Em\u00b7po\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spielt unser zw\u00f6lftes todtes Br\u00fcderlein,", "tokens": ["Spielt", "un\u00b7ser", "zw\u00f6lf\u00b7tes", "tod\u00b7tes", "Br\u00fc\u00b7derl\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier rieselt uns im Chore", "tokens": ["Hier", "rie\u00b7selt", "uns", "im", "Cho\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der morsche Kalk sanft ins Geripp herein.", "tokens": ["Der", "mor\u00b7sche", "Kalk", "sanft", "ins", "Ge\u00b7ripp", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Es glitt vor tausend Jahren", "tokens": ["Es", "glitt", "vor", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Strand ein Sarazenensegel nah,", "tokens": ["Dem", "Strand", "ein", "Sa\u00b7ra\u00b7ze\u00b7nen\u00b7se\u00b7gel", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sobald's vorbeigefahren,", "tokens": ["So\u00b7bald's", "vor\u00b7bei\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Anstimmten wir ein kr\u00e4ftig Gloria.", "tokens": ["An\u00b7stimm\u00b7ten", "wir", "ein", "kr\u00e4f\u00b7tig", "Glo\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erg\u00f6tzt von unserm Singen,", "tokens": ["Er\u00b7g\u00f6tzt", "von", "un\u00b7serm", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nahm der Pirat zu uns zur\u00fcck den Lauf,", "tokens": ["Nahm", "der", "Pi\u00b7rat", "zu", "uns", "zu\u00b7r\u00fcck", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Zw\u00f6lf K\u00f6pfe lie\u00df er springen,", "tokens": ["Zw\u00f6lf", "K\u00f6p\u00b7fe", "lie\u00df", "er", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Blut scho\u00df wie aus Brunnenr\u00f6hren auf.", "tokens": ["Das", "Blut", "scho\u00df", "wie", "aus", "Brun\u00b7nen\u00b7r\u00f6h\u00b7ren", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir singen ohne Kehlen,", "tokens": ["Wir", "sin\u00b7gen", "oh\u00b7ne", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir sitzen fr\u00f6hlich ohne Sch\u00e4del da,", "tokens": ["Wir", "sit\u00b7zen", "fr\u00f6h\u00b7lich", "oh\u00b7ne", "Sch\u00e4\u00b7del", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir singen mit den Seelen", "tokens": ["Wir", "sin\u00b7gen", "mit", "den", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein kr\u00e4ftig schallend Deo gloria!", "tokens": ["Ein", "kr\u00e4f\u00b7tig", "schal\u00b7lend", "Deo", "glo\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Morgenstrahl, der schiefe,", "tokens": ["Der", "Mor\u00b7gen\u00b7strahl", ",", "der", "schie\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchs rechte Fenster \u00e4ugelt er herein,", "tokens": ["Durchs", "rech\u00b7te", "Fens\u00b7ter", "\u00e4u\u00b7gelt", "er", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergoldend in der Tiefe", "tokens": ["Ver\u00b7gol\u00b7dend", "in", "der", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein lustiglich psallierend Todtenbein.", "tokens": ["Ein", "lus\u00b7tig\u00b7lich", "psal\u00b7lie\u00b7rend", "Tod\u00b7ten\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der Abendstrahl, der schr\u00e4ge,", "tokens": ["Der", "A\u00b7bend\u00b7strahl", ",", "der", "schr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchs linke Fenster blinzelt er herein,", "tokens": ["Durchs", "lin\u00b7ke", "Fens\u00b7ter", "blin\u00b7zelt", "er", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und z\u00e4hlt, ob allerwege", "tokens": ["Und", "z\u00e4hlt", ",", "ob", "al\u00b7ler\u00b7we\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir richtig unser elf Gespenster sei'n.", "tokens": ["Wir", "rich\u00b7tig", "un\u00b7ser", "elf", "Ge\u00b7spens\u00b7ter", "sei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "ADJD", "PPOSAT", "CARD", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Oft \u00fcbert\u00e4ubt das Dr\u00f6hnen", "tokens": ["Oft", "\u00fc\u00b7bert\u00b7\u00e4ubt", "das", "Dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Meers die Noten unsrer Litanei,", "tokens": ["Des", "Meers", "die", "No\u00b7ten", "uns\u00b7rer", "Li\u00b7ta\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus unsern Orgelt\u00f6nen", "tokens": ["Aus", "un\u00b7sern", "Or\u00b7gel\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erhebt sich oft ein schriller M\u00f6wenschrei \u2014", "tokens": ["Er\u00b7hebt", "sich", "oft", "ein", "schril\u00b7ler", "M\u00f6\u00b7wen\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Bollwerk und Mauer trutzen", "tokens": ["Boll\u00b7werk", "und", "Mau\u00b7er", "trut\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Wellenwurf noch tausend Jahre ja,", "tokens": ["Dem", "Wel\u00b7len\u00b7wurf", "noch", "tau\u00b7send", "Jah\u00b7re", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "CARD", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir singen, elf Capuzen,", "tokens": ["Wir", "sin\u00b7gen", ",", "elf", "Ca\u00b7pu\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein kr\u00e4ftig schallend Deo gloria!", "tokens": ["Ein", "kr\u00e4f\u00b7tig", "schal\u00b7lend", "Deo", "glo\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}