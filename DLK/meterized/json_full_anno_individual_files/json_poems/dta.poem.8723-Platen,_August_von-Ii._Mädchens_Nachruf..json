{"dta.poem.8723": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n  M\u00e4dchens Nachruf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und gesammelt liegt die Frucht:", "tokens": ["Und", "ge\u00b7sam\u00b7melt", "liegt", "die", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, mit meinen Freuden allen", "tokens": ["Ach", ",", "mit", "mei\u00b7nen", "Freu\u00b7den", "al\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahm auch er die rasche Flucht!", "tokens": ["Nahm", "auch", "er", "die", "ra\u00b7sche", "Flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter niederm H\u00fcttendache", "tokens": ["Un\u00b7ter", "nie\u00b7derm", "H\u00fct\u00b7ten\u00b7da\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohn' ich, jener im Pallast,", "tokens": ["Wohn'", "ich", ",", "je\u00b7ner", "im", "Pal\u00b7last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PDAT", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch aus f\u00fcrstlichem Gemache", "tokens": ["Doch", "aus", "f\u00fcrst\u00b7li\u00b7chem", "Ge\u00b7ma\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trieb ihn Muth und Kampfeshast.", "tokens": ["Trieb", "ihn", "Muth", "und", "Kamp\u00b7fes\u00b7hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als des Fr\u00fchroths erstes Tagen", "tokens": ["Als", "des", "Fr\u00fch\u00b7roths", "ers\u00b7tes", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich vom Traume heut erweckt,", "tokens": ["Mich", "vom", "Trau\u00b7me", "heut", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War mit Dienern, Rossen, Wagen", "tokens": ["War", "mit", "Die\u00b7nern", ",", "Ros\u00b7sen", ",", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser ganze Raum bedeckt.", "tokens": ["Die\u00b7ser", "gan\u00b7ze", "Raum", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und er kam im Jugendflore,", "tokens": ["Und", "er", "kam", "im", "Ju\u00b7gend\u00b7flo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hob sich auf sein Pferd im Nu,", "tokens": ["Hob", "sich", "auf", "sein", "Pferd", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPRART", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bebend stand ich unter'm Thore,", "tokens": ["Be\u00b7bend", "stand", "ich", "un\u00b7ter'm", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah dem sch\u00f6nen Reiter zu.", "tokens": ["Sah", "dem", "sch\u00f6\u00b7nen", "Rei\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und im leichten Morgenkleide", "tokens": ["Und", "im", "leich\u00b7ten", "Mor\u00b7gen\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Trat zu ihm die Braut hervor,", "tokens": ["Trat", "zu", "ihm", "die", "Braut", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesmal ohne Gold und Seide,", "tokens": ["Dies\u00b7mal", "oh\u00b7ne", "Gold", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch wie er im Jugendflor.", "tokens": ["Doch", "wie", "er", "im", "Ju\u00b7gend\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Vor der Trennung nicht erschrocken,", "tokens": ["Vor", "der", "Tren\u00b7nung", "nicht", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fc\u00dft' er noch ihr Stirn und Mund,", "tokens": ["K\u00fc\u00dft'", "er", "noch", "ihr", "Stirn", "und", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey den Lippen, bey den Locken", "tokens": ["Bey", "den", "Lip\u00b7pen", ",", "bey", "den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwur er den begl\u00fcckten Bund.", "tokens": ["Schwur", "er", "den", "be\u00b7gl\u00fcck\u00b7ten", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ritt mit Dienern und Vasallen,", "tokens": ["Ritt", "mit", "Die\u00b7nern", "und", "Va\u00b7sal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Dankte meinem Gru\u00dfe kaum:", "tokens": ["Dank\u00b7te", "mei\u00b7nem", "Gru\u00b7\u00dfe", "kaum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwalben ziehen, Bl\u00e4tter fallen,", "tokens": ["Schwal\u00b7ben", "zie\u00b7hen", ",", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So zerflie\u00dft der Liebe Traum!", "tokens": ["So", "zer\u00b7flie\u00dft", "der", "Lie\u00b7be", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}