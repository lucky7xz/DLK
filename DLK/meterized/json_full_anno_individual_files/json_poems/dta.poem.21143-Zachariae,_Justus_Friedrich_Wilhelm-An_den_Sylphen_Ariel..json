{"dta.poem.21143": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Sylphen Ariel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Besch\u00fctzer meiner Sch\u00f6ne,", "tokens": ["Be\u00b7sch\u00fct\u00b7zer", "mei\u00b7ner", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wachsamer Ariel,", "tokens": ["Wach\u00b7sa\u00b7mer", "A\u00b7riel", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Erschein auf diese T\u00f6ne,", "tokens": ["Er\u00b7schein", "auf", "die\u00b7se", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimm von mir Befehl.", "tokens": ["Und", "nimm", "von", "mir", "Be\u00b7fehl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Selinden zu bewachen,", "tokens": ["Se\u00b7lin\u00b7den", "zu", "be\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sey k\u00fcnftig dein Beruf!", "tokens": ["Sey", "k\u00fcnf\u00b7tig", "dein", "Be\u00b7ruf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nichts mu\u00df dich gr\u00f6\u00dfer machen,", "tokens": ["Nichts", "mu\u00df", "dich", "gr\u00f6\u00b7\u00dfer", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Seit Gabalis dich schuf.", "tokens": ["Seit", "Ga\u00b7ba\u00b7lis", "dich", "schuf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich finde nicht der Morgen", "tokens": ["Dich", "fin\u00b7de", "nicht", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey meines M\u00e4dchens Putz.", "tokens": ["Bey", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Putz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In weit erhabnern Sorgen", "tokens": ["In", "weit", "er\u00b7hab\u00b7nern", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beweise sich dein Schutz.", "tokens": ["Be\u00b7wei\u00b7se", "sich", "dein", "Schutz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Belindens braunen Locken", "tokens": ["Be\u00b7lin\u00b7dens", "brau\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gab Pope dich zur Wacht,", "tokens": ["Gab", "Po\u00b7pe", "dich", "zur", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt nimm so unerschrocken", "tokens": ["Jetzt", "nimm", "so", "un\u00b7er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Selindens Herz in Acht.", "tokens": ["Se\u00b7lin\u00b7dens", "Herz", "in", "Acht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn, \u00fcberdeckt mit Tressen,", "tokens": ["Wenn", ",", "\u00fc\u00b7berd\u00b7eckt", "mit", "Tres\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Stutzer um sie schwebt,", "tokens": ["Der", "Stut\u00b7zer", "um", "sie", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seinen Blick vermessen", "tokens": ["Und", "sei\u00b7nen", "Blick", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Narr nach ihr erhebt;", "tokens": ["Der", "Narr", "nach", "ihr", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So scheuche dein Gefieder", "tokens": ["So", "scheu\u00b7che", "dein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den leeren Stutzer fort,", "tokens": ["Den", "lee\u00b7ren", "Stut\u00b7zer", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und donnre Narren nieder", "tokens": ["Und", "donn\u00b7re", "Nar\u00b7ren", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch ein gescheutes Wort.", "tokens": ["Durch", "ein", "ge\u00b7scheu\u00b7tes", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Erhalt in ihren Herzen", "tokens": ["Er\u00b7halt", "in", "ih\u00b7ren", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Spott, der siegreich ist,", "tokens": ["Den", "Spott", ",", "der", "sieg\u00b7reich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn in gezwungnen Scherzen", "tokens": ["Wenn", "in", "ge\u00b7zwung\u00b7nen", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Landwitz sich ergie\u00dft.", "tokens": ["Der", "Land\u00b7witz", "sich", "er\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein niederschlagend Lachen", "tokens": ["Ein", "nie\u00b7der\u00b7schla\u00b7gend", "La\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bewafn\u2019 ihr Angesicht,", "tokens": ["Be\u00b7wafn'", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Junker klein zu machen,", "tokens": ["Den", "Jun\u00b7ker", "klein", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der aus Banisen spricht.", "tokens": ["Der", "aus", "Ba\u00b7ni\u00b7sen", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bedeckt nun die Gefilde", "tokens": ["Be\u00b7deckt", "nun", "die", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Abend Thau und Ruh;", "tokens": ["Von", "A\u00b7bend", "Thau", "und", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So setze meinem Bilde", "tokens": ["So", "set\u00b7ze", "mei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Reizung zu.", "tokens": ["Der", "Lie\u00b7be", "Rei\u00b7zung", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gieb, da\u00df ich so sie r\u00fchre,", "tokens": ["Gieb", ",", "da\u00df", "ich", "so", "sie", "r\u00fch\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "So wie sie mich ger\u00fchrt,", "tokens": ["So", "wie", "sie", "mich", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als sie an dem Claviere", "tokens": ["Als", "sie", "an", "dem", "Cla\u00b7vie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Mein z\u00e4rtlich Herz entf\u00fchrt.", "tokens": ["Mein", "z\u00e4rt\u00b7lich", "Herz", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}