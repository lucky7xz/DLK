{"dta.poem.4042": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Emilie an Klara .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.85", "fr:0.14"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Wie dank' ich dir, du Liebe, da\u00df du mir", "tokens": ["Wie", "dank'", "ich", "dir", ",", "du", "Lie\u00b7be", ",", "da\u00df", "du", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "PPER", "NN", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vertrauen abgewonnen, da\u00df ich dir", "tokens": ["Ver\u00b7trau\u00b7en", "ab\u00b7ge\u00b7won\u00b7nen", ",", "da\u00df", "ich", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein still Geheimni\u00df ausgesprochen.", "tokens": ["Mein", "still", "Ge\u00b7heim\u00b7ni\u00df", "aus\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin nun ruhiger \u2012 wie nenn' ichs dir?", "tokens": ["Ich", "bin", "nun", "ru\u00b7hi\u00b7ger", "\u2012", "wie", "nenn'", "ichs", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "PWAV", "VVFIN", "PIS", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und an die sch\u00f6nen Tage denk' ich, wenn ich oft", "tokens": ["Und", "an", "die", "sch\u00f6\u00b7nen", "Ta\u00b7ge", "denk'", "ich", ",", "wenn", "ich", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hinaus ging mit dem Bruder, und wir oben", "tokens": ["Hin\u00b7aus", "ging", "mit", "dem", "Bru\u00b7der", ",", "und", "wir", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf unserm H\u00fcgel beieinander sa\u00dfen,", "tokens": ["Auf", "un\u00b7serm", "H\u00fc\u00b7gel", "bei\u00b7ein\u00b7an\u00b7der", "sa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ich den Lieben bei den H\u00e4nden hielt,", "tokens": ["Und", "ich", "den", "Lie\u00b7ben", "bei", "den", "H\u00e4n\u00b7den", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und mirs gefallen lie\u00df am offnen Feld'", "tokens": ["Und", "mirs", "ge\u00b7fal\u00b7len", "lie\u00df", "am", "off\u00b7nen", "Feld'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVPP", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und an der Stra\u00df', und ins Gew\u00f6lb' hinauf", "tokens": ["Und", "an", "der", "Stra\u00df'", ",", "und", "ins", "Ge\u00b7w\u00f6lb'", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KON", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des gr\u00fcnen Ahorns staunt', an dem wir lagen.", "tokens": ["Des", "gr\u00fc\u00b7nen", "A\u00b7horns", "staunt'", ",", "an", "dem", "wir", "la\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ein Sehnen war in mir, doch war ich still.", "tokens": ["Ein", "Seh\u00b7nen", "war", "in", "mir", ",", "doch", "war", "ich", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es bl\u00fchten uns der ersten Hoffnung Tage,", "tokens": ["Es", "bl\u00fch\u00b7ten", "uns", "der", "ers\u00b7ten", "Hoff\u00b7nung", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Tage des Erwachens.", "tokens": ["Die", "Ta\u00b7ge", "des", "Er\u00b7wa\u00b7chens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Holde D\u00e4mm'rung!", "tokens": ["Hol\u00b7de", "D\u00e4m\u00b7m'\u00b7rung", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "So sch\u00f6n ists, wenn die g\u00fctige Natur", "tokens": ["So", "sch\u00f6n", "ists", ",", "wenn", "die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ins Leben lockt ihr Kind. Es singen nur", "tokens": ["Ins", "Le\u00b7ben", "lockt", "ihr", "Kind", ".", "Es", "sin\u00b7gen", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Schlummersang am Abend unsre M\u00fctter.", "tokens": ["Den", "Schlum\u00b7mer\u00b7sang", "am", "A\u00b7bend", "uns\u00b7re", "M\u00fct\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie brauchen nie das Morgenlied zu singen.", "tokens": ["Sie", "brau\u00b7chen", "nie", "das", "Mor\u00b7gen\u00b7lied", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die\u00df singt die andre Mutter uns, die gute;", "tokens": ["Die\u00df", "singt", "die", "and\u00b7re", "Mut\u00b7ter", "uns", ",", "die", "gu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die wunderbare, die uns Lebenslust", "tokens": ["Die", "wun\u00b7der\u00b7ba\u00b7re", ",", "die", "uns", "Le\u00b7bens\u00b7lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In unsern Busen athmet, uns mit s\u00fc\u00dfen", "tokens": ["In", "un\u00b7sern", "Bu\u00b7sen", "ath\u00b7met", ",", "uns", "mit", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Verhei\u00dfungen erweckt.", "tokens": ["Ver\u00b7hei\u00b7\u00dfun\u00b7gen", "er\u00b7weckt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wie ist mir, Liebe!", "tokens": ["Wie", "ist", "mir", ",", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ich kann an Jugend heute nur, und nur", "tokens": ["Ich", "kann", "an", "Ju\u00b7gend", "heu\u00b7te", "nur", ",", "und", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "ADV", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An Jugend denken.", "tokens": ["An", "Ju\u00b7gend", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sieh! ein heitrer Tag", "tokens": ["Sieh", "!", "ein", "hei\u00b7trer", "Tag"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ists eben auch. Seit fr\u00fchem Morgen sitz' ich", "tokens": ["Ists", "e\u00b7ben", "auch", ".", "Seit", "fr\u00fc\u00b7hem", "Mor\u00b7gen", "sitz'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "$.", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am lieben Fenster, und es wehn die L\u00fcfte,", "tokens": ["Am", "lie\u00b7ben", "Fens\u00b7ter", ",", "und", "es", "wehn", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die z\u00e4rtlichen, herein, mir blickt das Licht", "tokens": ["Die", "z\u00e4rt\u00b7li\u00b7chen", ",", "her\u00b7ein", ",", "mir", "blickt", "das", "Licht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch meine B\u00e4ume, die zu nahe mir", "tokens": ["Durch", "mei\u00b7ne", "B\u00e4u\u00b7me", ",", "die", "zu", "na\u00b7he", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PTKA", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gewachsen sind, und m\u00e4hlig mit den Bl\u00fcthen", "tokens": ["Ge\u00b7wach\u00b7sen", "sind", ",", "und", "m\u00e4h\u00b7lig", "mit", "den", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das ferne Land verh\u00fcllen, da\u00df ich mich", "tokens": ["Das", "fer\u00b7ne", "Land", "ver\u00b7h\u00fcl\u00b7len", ",", "da\u00df", "ich", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bescheiden mu\u00df, und hie und da noch kaum", "tokens": ["Be\u00b7schei\u00b7den", "mu\u00df", ",", "und", "hie", "und", "da", "noch", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "$,", "KON", "ADV", "KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Hinaus mich find' aus diesem freundlichen", "tokens": ["Hin\u00b7aus", "mich", "find'", "aus", "die\u00b7sem", "freund\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Gef\u00e4ngni\u00df! und es fliegen \u00fcber ihnen", "tokens": ["Ge\u00b7f\u00e4ng\u00b7ni\u00df", "!", "und", "es", "flie\u00b7gen", "\u00fc\u00b7ber", "ih\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Schwalben und die Lerchen, und es singen", "tokens": ["Die", "Schwal\u00b7ben", "und", "die", "Ler\u00b7chen", ",", "und", "es", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Stunde durch genug die Nachtigallen,", "tokens": ["Die", "Stun\u00b7de", "durch", "ge\u00b7nug", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wie sie hei\u00dfen, all die Lieblinge", "tokens": ["Und", "wie", "sie", "hei\u00b7\u00dfen", ",", "all", "die", "Lieb\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVINF", "$,", "PIAT", "ART", "NN"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Der sch\u00f6nen Jahrszeit; eigne Namen m\u00f6cht'", "tokens": ["Der", "sch\u00f6\u00b7nen", "Jahrs\u00b7zeit", ";", "eig\u00b7ne", "Na\u00b7men", "m\u00f6cht'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich ihnen geben, und den Blumen auch,", "tokens": ["Ich", "ih\u00b7nen", "ge\u00b7ben", ",", "und", "den", "Blu\u00b7men", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVINF", "$,", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Den stillen, die aus dunklem Beete duften,", "tokens": ["Den", "stil\u00b7len", ",", "die", "aus", "dunk\u00b7lem", "Bee\u00b7te", "duf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Zu mir herauf wie junge Sterne gl\u00e4nzend.", "tokens": ["Zu", "mir", "her\u00b7auf", "wie", "jun\u00b7ge", "Ster\u00b7ne", "gl\u00e4n\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "KOKOM", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wie es lebt und gl\u00fccklich ist im Wachsthum,", "tokens": ["Und", "wie", "es", "lebt", "und", "gl\u00fcck\u00b7lich", "ist", "im", "Wach\u00b7sthum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seiner Reise sich entgegen freut!", "tokens": ["Und", "sei\u00b7ner", "Rei\u00b7se", "sich", "ent\u00b7ge\u00b7gen", "freut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Es findet jedes seine Stelle doch,", "tokens": ["Es", "fin\u00b7det", "je\u00b7des", "sei\u00b7ne", "Stel\u00b7le", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Haus, die Speise, die das Herz ihm s\u00e4ttigt,", "tokens": ["Sein", "Haus", ",", "die", "Spei\u00b7se", ",", "die", "das", "Herz", "ihm", "s\u00e4t\u00b7tigt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jedes segnest du mit eignem Segen,", "tokens": ["Und", "je\u00b7des", "seg\u00b7nest", "du", "mit", "eig\u00b7nem", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Natur! und giebst dich ihnen zum Gesch\u00e4ft,", "tokens": ["Na\u00b7tur", "!", "und", "giebst", "dich", "ih\u00b7nen", "zum", "Ge\u00b7sch\u00e4ft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und tr\u00e4gst und n\u00e4hrst zu ihrer Bl\u00fcthenfreud'", "tokens": ["Und", "tr\u00e4gst", "und", "n\u00e4hrst", "zu", "ih\u00b7rer", "Bl\u00fcthen\u00b7freud'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihrer Frucht sie fort, du g\u00fctige!", "tokens": ["Und", "ih\u00b7rer", "Frucht", "sie", "fort", ",", "du", "g\u00fc\u00b7ti\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und klagtest du doch \u00f6fters, trauernd Herz!", "tokens": ["Und", "klag\u00b7test", "du", "doch", "\u00f6f\u00b7ters", ",", "trau\u00b7ernd", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verga\u00dfest mir den Glauben, danktest nicht,", "tokens": ["Ver\u00b7ga\u00b7\u00dfest", "mir", "den", "Glau\u00b7ben", ",", "dank\u00b7test", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dachtest nicht, wenn dir dein Thun zu wenig", "tokens": ["Und", "dach\u00b7test", "nicht", ",", "wenn", "dir", "dein", "Thun", "zu", "we\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word", 