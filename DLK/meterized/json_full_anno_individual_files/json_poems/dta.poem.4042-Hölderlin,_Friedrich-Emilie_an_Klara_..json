rd", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Vater aus dem Feld', und bind' ihm sie", "tokens": ["Dem", "Va\u00b7ter", "aus", "dem", "Feld'", ",", "und", "bind'", "ihm", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Einen Straus, die drunten in dem Garten,", "tokens": ["In", "Ei\u00b7nen", "Straus", ",", "die", "drun\u00b7ten", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die der Bach erzog; ich wills schon richten,", "tokens": ["Und", "die", "der", "Bach", "er\u00b7zog", ";", "ich", "wills", "schon", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ihm's gefallen soll. Und dir? dir bring' ich", "tokens": ["Da\u00df", "ihm's", "ge\u00b7fal\u00b7len", "soll", ".", "Und", "dir", "?", "dir", "bring'", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$.", "KON", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Genug des Neuen. Da ist's immer anders.", "tokens": ["Ge\u00b7nug", "des", "Neu\u00b7en", ".", "Da", "ist's", "im\u00b7mer", "an\u00b7ders", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jetzt bl\u00fchn die Weiden; jetzt vergolden sich", "tokens": ["Jetzt", "bl\u00fchn", "die", "Wei\u00b7den", ";", "jetzt", "ver\u00b7gol\u00b7den", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Wiesen; jetzt beginnt der Buche Gr\u00fcn,", "tokens": ["Die", "Wie\u00b7sen", ";", "jetzt", "be\u00b7ginnt", "der", "Bu\u00b7che", "Gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und jetzt der Eiche \u2012 nun! leb' wohl indessen!", "tokens": ["Und", "jetzt", "der", "Ei\u00b7che", "\u2012", "nun", "!", "leb'", "wohl", "in\u00b7des\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$(", "ADV", "$.", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}}}}}