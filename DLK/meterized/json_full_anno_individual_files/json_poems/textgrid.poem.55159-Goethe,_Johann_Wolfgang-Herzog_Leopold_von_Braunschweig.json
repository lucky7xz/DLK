{"textgrid.poem.55159": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Leopold von Braunschweig", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich ergriff mit Gewalt der alte Herrscher des Flusses,", "tokens": ["Dich", "er\u00b7griff", "mit", "Ge\u00b7walt", "der", "al\u00b7te", "Herr\u00b7scher", "des", "Flus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "H\u00e4lt dich und teilet mit dir ewig sein str\u00f6mendes Reich.", "tokens": ["H\u00e4lt", "dich", "und", "tei\u00b7let", "mit", "dir", "e\u00b7wig", "sein", "str\u00f6\u00b7men\u00b7des", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ruhig schlummerst du nun beim stilleren Rauschen der Urne,", "tokens": ["Ru\u00b7hig", "schlum\u00b7merst", "du", "nun", "beim", "stil\u00b7le\u00b7ren", "Rau\u00b7schen", "der", "Ur\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Bis dich st\u00fcrmende Flut wieder zu Taten erweckt.", "tokens": ["Bis", "dich", "st\u00fcr\u00b7men\u00b7de", "Flut", "wie\u00b7der", "zu", "Ta\u00b7ten", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "H\u00fclfreich werde dem Volke! so wie du, ein Sterblicher, wolltest,", "tokens": ["H\u00fclf\u00b7reich", "wer\u00b7de", "dem", "Vol\u00b7ke", "!", "so", "wie", "du", ",", "ein", "Sterb\u00b7li\u00b7cher", ",", "woll\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "ADV", "KOKOM", "PPER", "$,", "ART", "NN", "$,", "VMFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Und vollend als ein Gott, was dir als Menschen mi\u00dflang.", "tokens": ["Und", "vol\u00b7lend", "als", "ein", "Gott", ",", "was", "dir", "als", "Men\u00b7schen", "mi\u00df\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$,", "PWS", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Dich ergriff mit Gewalt der alte Herrscher des Flusses,", "tokens": ["Dich", "er\u00b7griff", "mit", "Ge\u00b7walt", "der", "al\u00b7te", "Herr\u00b7scher", "des", "Flus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "H\u00e4lt dich und teilet mit dir ewig sein str\u00f6mendes Reich.", "tokens": ["H\u00e4lt", "dich", "und", "tei\u00b7let", "mit", "dir", "e\u00b7wig", "sein", "str\u00f6\u00b7men\u00b7des", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "PPER", "ADJD"