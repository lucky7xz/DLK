{"dta.poem.5310": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00f6lich seyn bey seiner Arbeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sehn wir auf der Kirschen-Bl\u00fcth',", "tokens": ["Sehn", "wir", "auf", "der", "Kir\u00b7schen\u00b7B\u00b7l\u00fcth'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit betrachtendem Gem\u00fcth,", "tokens": ["Mit", "be\u00b7trach\u00b7ten\u00b7dem", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen ihrem frischen Gr\u00fcnen,", "tokens": ["Zwi\u00b7schen", "ih\u00b7rem", "fri\u00b7schen", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Weissen, dunckle Bienen", "tokens": ["Auf", "dem", "Weis\u00b7sen", ",", "dunck\u00b7le", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit gesch\u00e4ft\u2019 gem Sumsen schweben,", "tokens": ["Mit", "ge\u00b7sch\u00e4ft'", "gem", "Sum\u00b7sen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald sich setzen, bald sich heben,", "tokens": ["Bald", "sich", "set\u00b7zen", ",", "bald", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "$,", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bald, im Schweben, sich bestreben,", "tokens": ["Bald", ",", "im", "Schwe\u00b7ben", ",", "sich", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "$,", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An die kleinen Hinter-Schienen", "tokens": ["An", "die", "klei\u00b7nen", "Hin\u00b7ter\u00b7Schie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gelb gesammlet Wachs zu kleben;", "tokens": ["Gelb", "ge\u00b7samm\u00b7let", "Wachs", "zu", "kle\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Scheint der gantze Baum zu leben,", "tokens": ["Scheint", "der", "gant\u00b7ze", "Baum", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und mir fiel dar\u00fcber ein:", "tokens": ["Und", "mir", "fiel", "da\u00b7r\u00fc\u00b7ber", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.12": {"text": "Kann, nach Salomonis Lehre,", "tokens": ["Kann", ",", "nach", "Sa\u00b7lo\u00b7mo\u00b7nis", "Leh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sonder Ausnahm\u2019 auf der Erden,", "tokens": ["Son\u00b7der", "Aus\u00b7nahm'", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "Wenn er noch so gl\u00fccklich w\u00e4re,", "tokens": ["Wenn", "er", "noch", "so", "gl\u00fcck\u00b7lich", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Keiner recht vergn\u00fcget werden,", "tokens": ["Kei\u00b7ner", "recht", "ver\u00b7gn\u00fc\u00b7get", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Als durch dieses blos allein:", "tokens": ["Als", "durch", "die\u00b7ses", "blos", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Bey der Arbeit fr\u00f6lich seyn;", "tokens": ["Bey", "der", "Ar\u00b7beit", "fr\u00f6\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "So kann dieses kleine Thier,", "tokens": ["So", "kann", "die\u00b7ses", "klei\u00b7ne", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Liebster Leser, dir und mir", "tokens": ["Liebs\u00b7ter", "Le\u00b7ser", ",", "dir", "und", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Ein begl\u00fccktes Beyspiel geben.", "tokens": ["Ein", "be\u00b7gl\u00fcck\u00b7tes", "Bey\u00b7spiel", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Machst du es nun eben so;", "tokens": ["Machst", "du", "es", "nun", "e\u00b7ben", "so", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Bist du, bey der Arbeit, froh:", "tokens": ["Bist", "du", ",", "bey", "der", "Ar\u00b7beit", ",", "froh", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "ART", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Wirst du auch vergn\u00fcget leben.", "tokens": ["Wirst", "du", "auch", "ver\u00b7gn\u00fc\u00b7get", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}