{"dta.poem.18261": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An unsre Modedichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Verfallt ihr der ewigen Nacht,", "tokens": ["Ver\u00b7fallt", "ihr", "der", "e\u00b7wi\u00b7gen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Weil ihr zu viel gedichtet", "tokens": ["Weil", "ihr", "zu", "viel", "ge\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKA", "PIS", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und weil ihr zu wenig gedacht!", "tokens": ["Und", "weil", "ihr", "zu", "we\u00b7nig", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKA", "PIS", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}