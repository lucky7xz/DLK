{"textgrid.poem.44411": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Bitte", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schilt mich nicht arbeitscheu und tr\u00e4ge,", "tokens": ["Schilt", "mich", "nicht", "ar\u00b7beit\u00b7scheu", "und", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich zum Werke sp\u00e4t mich rege,", "tokens": ["Weil", "ich", "zum", "Wer\u00b7ke", "sp\u00e4t", "mich", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem Armen gleich ich ganz und gar,", "tokens": ["Dem", "Ar\u00b7men", "gleich", "ich", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tonnen Goldes schuldig war;", "tokens": ["Der", "Ton\u00b7nen", "Gol\u00b7des", "schul\u00b7dig", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Ganze konnt er ab nicht tragen,", "tokens": ["Das", "Gan\u00b7ze", "konnt", "er", "ab", "nicht", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKVZ", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was sollt er sich mit Groschen plagen?", "tokens": ["Was", "sollt", "er", "sich", "mit", "Gro\u00b7schen", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Stell einen J\u00e4ger auch dir vor,", "tokens": ["Stell", "ei\u00b7nen", "J\u00e4\u00b7ger", "auch", "dir", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Kugeln lud er fr\u00fch sein Rohr", "tokens": ["Mit", "Ku\u00b7geln", "lud", "er", "fr\u00fch", "sein", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und geht hinaus ins tauige Feld,", "tokens": ["Und", "geht", "hin\u00b7aus", "ins", "tau\u00b7i\u00b7ge", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Dem Hirsche nach sein Streben stellt;", "tokens": ["Dem", "Hir\u00b7sche", "nach", "sein", "Stre\u00b7ben", "stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Hase l\u00e4uft, es fliegt das Huhn,", "tokens": ["Der", "Ha\u00b7se", "l\u00e4uft", ",", "es", "fliegt", "das", "Huhn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er aber l\u00e4\u00dft die Arme ruhn,", "tokens": ["Er", "a\u00b7ber", "l\u00e4\u00dft", "die", "Ar\u00b7me", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Bringt nicht den Hirsch sein gutes Gl\u00fcck,", "tokens": ["Bringt", "nicht", "den", "Hirsch", "sein", "gu\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Kehrt ohne Beut er sp\u00e4t zur\u00fcck,", "tokens": ["Kehrt", "oh\u00b7ne", "Beut", "er", "sp\u00e4t", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die andern alle schwer beladen.", "tokens": ["Die", "an\u00b7dern", "al\u00b7le", "schwer", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Warum hatt er nicht Schrot geladen!", "tokens": ["Wa\u00b7rum", "hatt", "er", "nicht", "Schrot", "ge\u00b7la\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Schilt mich nicht arbeitscheu und tr\u00e4ge,", "tokens": ["Schilt", "mich", "nicht", "ar\u00b7beit\u00b7scheu", "und", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+