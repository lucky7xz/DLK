{"textgrid.poem.62411": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Gebet bei der Gefangenschaft des Papstes", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r' auf deines Volkes Flehen,", "tokens": ["H\u00f6r'", "auf", "dei\u00b7nes", "Vol\u00b7kes", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heiland, la\u00df vor\u00fcber gehen", "tokens": ["Hei\u00b7land", ",", "la\u00df", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Kirche Todeswehen.", "tokens": ["Dei\u00b7ner", "Kir\u00b7che", "To\u00b7des\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ihr deine Huld gespendet,", "tokens": ["Was", "ihr", "dei\u00b7ne", "Huld", "ge\u00b7spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach ihr Kleinod ist entwendet,", "tokens": ["Ach", "ihr", "Klei\u00b7nod", "ist", "ent\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nig, deine Braut gesch\u00e4ndet.", "tokens": ["K\u00f6\u00b7nig", ",", "dei\u00b7ne", "Braut", "ge\u00b7sch\u00e4n\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00e4uber haben Hohn gesprochen,", "tokens": ["R\u00e4u\u00b7ber", "ha\u00b7ben", "Hohn", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind mit L\u00e4sterung und Pochen", "tokens": ["Sind", "mit", "L\u00e4s\u00b7te\u00b7rung", "und", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dein Heiligthum gebrochen.", "tokens": ["In", "dein", "Hei\u00b7lig\u00b7thum", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deine Heerde wird zerstreuet,", "tokens": ["Dei\u00b7ne", "Heer\u00b7de", "wird", "zer\u00b7streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil der Wolf, der dich nicht scheuet,", "tokens": ["Weil", "der", "Wolf", ",", "der", "dich", "nicht", "scheu\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr mit neuen S\u00fcnden dr\u00e4uet.", "tokens": ["Ihr", "mit", "neu\u00b7en", "S\u00fcn\u00b7den", "dr\u00e4u\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Thr\u00e4nen rufen dich und Lieder,", "tokens": ["Thr\u00e4\u00b7nen", "ru\u00b7fen", "dich", "und", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nig, sende Hilfe nieder,", "tokens": ["K\u00f6\u00b7nig", ",", "sen\u00b7de", "Hil\u00b7fe", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib ihr ihren Hirten wieder.", "tokens": ["Gib", "ihr", "ih\u00b7ren", "Hir\u00b7ten", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wollest den Gefang'nen st\u00e4rken,", "tokens": ["Wol\u00b7lest", "den", "Ge\u00b7fang'\u00b7nen", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei des heil'gen Amtes Werken \u2013", "tokens": ["Bei", "des", "heil'\u00b7gen", "Am\u00b7tes", "Wer\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Hilf' ihn lassen merken.", "tokens": ["Dei\u00b7ne", "Hil\u00b7f'", "ihn", "las\u00b7sen", "mer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Da Sanct Peter war in N\u00f6then,", "tokens": ["Da", "Sanct", "Pe\u00b7ter", "war", "in", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "VAFIN", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Eilten Christen mit Gebeten", "tokens": ["Eil\u00b7ten", "Chris\u00b7ten", "mit", "Ge\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihren Bischof zu vertreten.", "tokens": ["Ih\u00b7ren", "Bi\u00b7schof", "zu", "ver\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und als Paulus lag gebunden,", "tokens": ["Und", "als", "Pau\u00b7lus", "lag", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben Heil'ge sich gefunden,", "tokens": ["Ha\u00b7ben", "Heil'\u00b7ge", "sich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um zu lindern seine Wunden.", "tokens": ["Um", "zu", "lin\u00b7dern", "sei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Paul und Peter, Kirchens\u00e4ulen,", "tokens": ["Paul", "und", "Pe\u00b7ter", ",", "Kir\u00b7chen\u00b7s\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil'ge Schirmer, wollet eilen", "tokens": ["Heil'\u00b7ge", "Schir\u00b7mer", ",", "wol\u00b7let", "ei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsers Vaters Herz zu heilen;", "tokens": ["Un\u00b7sers", "Va\u00b7ters", "Herz", "zu", "hei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Die, mit zornerf\u00fcllten Mienen,", "tokens": ["Die", ",", "mit", "zor\u00b7ner\u00b7f\u00fcll\u00b7ten", "Mie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst dem Attila erschienen", "tokens": ["Einst", "dem", "At\u00b7ti\u00b7la", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihn zwangen euch zu dienen,", "tokens": ["Und", "ihn", "zwan\u00b7gen", "euch", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wollet nun den Frevler lohnen,", "tokens": ["Wol\u00b7let", "nun", "den", "Frev\u00b7ler", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der zertreten eure Kronen \u2013", "tokens": ["Der", "zer\u00b7tre\u00b7ten", "eu\u00b7re", "Kro\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollet l\u00e4nger sein nicht schonen.", "tokens": ["Wol\u00b7let", "l\u00e4n\u00b7ger", "sein", "nicht", "scho\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aergster aus dem argen Heere,", "tokens": ["A\u00b7ergs\u00b7ter", "aus", "dem", "ar\u00b7gen", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchl' er des Gerichtes Schwere,", "tokens": ["F\u00fchl'", "er", "des", "Ge\u00b7rich\u00b7tes", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr, um deines Namen Ehre.", "tokens": ["Herr", ",", "um", "dei\u00b7nes", "Na\u00b7men", "Eh\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wappne dich mit deinem Blitze! \u2013", "tokens": ["Wapp\u00b7ne", "dich", "mit", "dei\u00b7nem", "Blit\u00b7ze", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn, der an der Frevler Spitze,", "tokens": ["Ihn", ",", "der", "an", "der", "Frev\u00b7ler", "Spit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Triff in seinem H\u00f6llensitze.", "tokens": ["Triff", "in", "sei\u00b7nem", "H\u00f6l\u00b7len\u00b7sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Da\u00df umsonst nicht deine Wunden,", "tokens": ["Da\u00df", "um\u00b7sonst", "nicht", "dei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei, wie Sodoma verschwunden,", "tokens": ["Sei", ",", "wie", "So\u00b7do\u00b7ma", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nirgend seine Stadt gefunden.", "tokens": ["Nir\u00b7gend", "sei\u00b7ne", "Stadt", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "H\u00f6r' auf deines Volkes Flehen,", "tokens": ["H\u00f6r'", "auf", "dei\u00b7nes", "Vol\u00b7kes", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heiland, la\u00df vor\u00fcber gehen", "tokens": ["Hei\u00b7land", ",", "la\u00df", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Kirche Todeswehen.", "tokens": ["Dei\u00b7n