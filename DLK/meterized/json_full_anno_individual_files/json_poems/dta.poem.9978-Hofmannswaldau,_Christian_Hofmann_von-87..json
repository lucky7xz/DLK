{"dta.poem.9978": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Offtmahls werd ich weggeraubet/ bleibe dennoch wo ich bin/", "tokens": ["Offt\u00b7mahls", "werd", "ich", "weg\u00b7ge\u00b7rau\u00b7bet", "/", "blei\u00b7be", "den\u00b7noch", "wo", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$(", "VVFIN", "ADV", "PWAV", "PPER", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Komme keinem in die h\u00e4nde/ k\u00fcnstlich jedem ich entrinn.", "tokens": ["Kom\u00b7me", "kei\u00b7nem", "in", "die", "h\u00e4n\u00b7de", "/", "k\u00fcnst\u00b7lich", "je\u00b7dem", "ich", "ent\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "$(", "ADJD", "PIS", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ich bin von den kleinsten dingen un\u0303 von gr\u00f6sten auch zugleich/", "tokens": ["Ich", "bin", "von", "den", "kleins\u00b7ten", "din\u00b7gen", "u\u00f1", "von", "gr\u00f6s\u00b7ten", "auch", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ADJA", "ADV", "ADV", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Keiner kan mich recht beschauen/ da\u00df ich nicht auch tod erbleich.", "tokens": ["Kei\u00b7ner", "kan", "mich", "recht", "be\u00b7schau\u00b7en", "/", "da\u00df", "ich", "nicht", "auch", "tod", "er\u00b7bleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADJD", "VVINF", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}