{"textgrid.poem.40673": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn, deutsche Herzen, deutsches Land zu spalten,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn, deutsche Herzen, deutsches Land zu spalten,", "tokens": ["Wenn", ",", "deut\u00b7sche", "Her\u00b7zen", ",", "deut\u00b7sches", "Land", "zu", "spal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs Neu die Blitze spr\u00fchn vom Vatikane,", "tokens": ["Aufs", "Neu", "die", "Blit\u00b7ze", "spr\u00fchn", "vom", "Va\u00b7ti\u00b7ka\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh' ich im Geist als Vorbild, das uns mahne,", "tokens": ["Seh'", "ich", "im", "Geist", "als", "Vor\u00b7bild", ",", "das", "uns", "mah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KOUS", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zwei deutsche Freunde, die durch Rom einst wallten.", "tokens": ["Zwei", "deut\u00b7sche", "Freun\u00b7de", ",", "die", "durch", "Rom", "einst", "wall\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "APPR", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Getrennt, versprengt im Menschenozeane,", "tokens": ["Ge\u00b7trennt", ",", "ver\u00b7sprengt", "im", "Men\u00b7sche\u00b7no\u00b7zea\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sucht irrend Freund den Freund, \u2013 vergeblich Walten!", "tokens": ["Sucht", "ir\u00b7rend", "Freund", "den", "Freund", ",", "\u2013", "ver\u00b7geb\u00b7lich", "Wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "ART", "NN", "$,", "$(", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis von Sankt Peter Glockenrufe hallten,", "tokens": ["Bis", "von", "Sankt", "Pe\u00b7ter", "Glo\u00b7cken\u00b7ru\u00b7fe", "hall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "VVFIN", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Pontifex sich zeigt' auf dem Altane.", "tokens": ["Der", "Pon\u00b7ti\u00b7fex", "sich", "zeigt'", "auf", "dem", "Al\u00b7ta\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er spendet Segen, schleudert Bannesstrahle,", "tokens": ["Er", "spen\u00b7det", "Se\u00b7gen", ",", "schleu\u00b7dert", "Ban\u00b7nes\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs Knie sinkt alles Volk mit Einem Male,", "tokens": ["Aufs", "Knie", "sinkt", "al\u00b7les", "Volk", "mit", "Ei\u00b7nem", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich beugend vor dem Haupt tiar'umwunden;", "tokens": ["Sich", "beu\u00b7gend", "vor", "dem", "Haupt", "ti\u00b7a\u00b7r'\u00b7um\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie S\u00e4ulen blieben nur zwei M\u00e4nner stehen,", "tokens": ["Wie", "S\u00e4u\u00b7len", "blie\u00b7ben", "nur", "zwei", "M\u00e4n\u00b7ner", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freunde sind's, sie haben sich ersehen,", "tokens": ["Die", "Freun\u00b7de", "sin\u00b7d's", ",", "sie", "ha\u00b7ben", "sich", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPER", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und, ", "tokens": ["Und", ","], "token_info": ["word", "punct"], "pos": ["KON", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Wenn, deutsche Herzen, deutsches Land zu spalten,", "tokens": ["Wenn", ",", "deut\u00b7sche", "Her\u00b7zen", ",", "deut\u00b7sches", "Land", "zu", "spal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "m