{"textgrid.poem.53701": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Drei Minuten Geh\u00f6r!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von euch, die ihr den Hammer schwingt,", "tokens": ["Von", "euch", ",", "die", "ihr", "den", "Ham\u00b7mer", "schwingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von euch, die ihr auf Kr\u00fccken hinkt,", "tokens": ["von", "euch", ",", "die", "ihr", "auf", "Kr\u00fc\u00b7cken", "hinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "von euch, die ihr die Feder f\u00fchrt,", "tokens": ["von", "euch", ",", "die", "ihr", "die", "Fe\u00b7der", "f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "von euch, die ihr die Kessel sch\u00fcrt,", "tokens": ["von", "euch", ",", "die", "ihr", "die", "Kes\u00b7sel", "sch\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "von euch, die mit den treuen H\u00e4nden", "tokens": ["von", "euch", ",", "die", "mit", "den", "treu\u00b7en", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "dem Manne ihre Liebe spenden \u2013", "tokens": ["dem", "Man\u00b7ne", "ih\u00b7re", "Lie\u00b7be", "spen\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "von euch, den Jungen und den Alten \u2013:", "tokens": ["von", "euch", ",", "den", "Jun\u00b7gen", "und", "den", "Al\u00b7ten", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "KON", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr sollt drei Minuten inne halten.", "tokens": ["Ihr", "sollt", "drei", "Mi\u00b7nu\u00b7ten", "in\u00b7ne", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "CARD", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wir sind ja nicht unter Kriegsgewinnern.", "tokens": ["Wir", "sind", "ja", "nicht", "un\u00b7ter", "Kriegs\u00b7ge\u00b7win\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wir wollen uns einmal erinnern.", "tokens": ["Wir", "wol\u00b7len", "uns", "ein\u00b7mal", "e\u00b7rin\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die erste Minute geh\u00f6re dem Mann.", "tokens": ["Die", "ers\u00b7te", "Mi\u00b7nu\u00b7te", "ge\u00b7h\u00f6\u00b7re", "dem", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer trat vor Jahren in Feldgrau an?", "tokens": ["Wer", "trat", "vor", "Jah\u00b7ren", "in", "Feld\u00b7grau", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zu Hause die Kinder \u2013 zu Hause weint Mutter . . .", "tokens": ["Zu", "Hau\u00b7se", "die", "Kin\u00b7der", "\u2013", "zu", "Hau\u00b7se", "weint", "Mut\u00b7ter", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$(", "APPR", "NN", "VVFIN", "NN", "$.", "$.", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr: feldgraues Kanonenfutter \u2013!", "tokens": ["Ihr", ":", "feld\u00b7grau\u00b7es", "Ka\u00b7no\u00b7nen\u00b7fut\u00b7ter", "\u2013", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "$.", "ADJA", "NN", "$(", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Ihr zogt in den lehmigen Ackergraben.", "tokens": ["Ihr", "zogt", "in", "den", "leh\u00b7mi\u00b7gen", "A\u00b7cker\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Da saht ihr keinen F\u00fcrstenknaben:", "tokens": ["Da", "saht", "ihr", "kei\u00b7nen", "F\u00fcrs\u00b7ten\u00b7kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "der soff sich einen in der Etappe", "tokens": ["der", "soff", "sich", "ei\u00b7nen", "in", "der", "E\u00b7tap\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PRF", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und ging mit den Damen in die Klappe.", "tokens": ["und", "ging", "mit", "den", "Da\u00b7men", "in", "die", "Klap\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ihr wurdet geschliffen. Ihr wurdet gedrillt.", "tokens": ["Ihr", "wur\u00b7det", "ge\u00b7schlif\u00b7fen", ".", "Ihr", "wur\u00b7det", "ge\u00b7drillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Wart ihr noch Gottes Ebenbild?", "tokens": ["Wart", "ihr", "noch", "Got\u00b7tes", "E\u00b7ben\u00b7bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In der Kaserne \u2013 im Schilderhaus", "tokens": ["In", "der", "Ka\u00b7ser\u00b7ne", "\u2013", "im", "Schil\u00b7der\u00b7haus"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "APPRART", "NN"], "meter": "--+---+-+", "measure": "anapaest.init"}, "line.12": {"text": "wart ihr niedriger als die schmutzigste Laus.", "tokens": ["wart", "ihr", "nied\u00b7ri\u00b7ger", "als", "die", "schmut\u00b7zigs\u00b7te", "Laus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Der Offizier war eine Perle,", "tokens": ["Der", "Of\u00b7fi\u00b7zier", "war", "ei\u00b7ne", "Per\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "aber ihr wart nur \u203aKerle\u2039!", "tokens": ["a\u00b7ber", "ihr", "wart", "nur", "\u203a", "Ker\u00b7le", "\u2039", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$(", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.15": {"text": "Ein elender Schie\u00df- und Gr\u00fc\u00dfautomat.", "tokens": ["Ein", "e\u00b7len\u00b7der", "Schie\u00df", "und", "Gr\u00fc\u00df\u00b7au\u00b7to\u00b7mat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "\u00bbsie Schwein! H\u00e4nde an die Hosennaht \u2013!\u00ab", "tokens": ["\u00bb", "sie", "Schwein", "!", "H\u00e4n\u00b7de", "an", "die", "Ho\u00b7sen\u00b7naht", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "NN", "$.", "NN", "APPR", "ART", "NN", "$(", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Verwundete mochten sich kr\u00fcmmen und biegen:", "tokens": ["Ver\u00b7wun\u00b7de\u00b7te", "moch\u00b7ten", "sich", "kr\u00fcm\u00b7men", "und", "bie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "PRF", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.18": {"text": "kam ein Prinz, dann hattet ihr stramm zu liegen.", "tokens": ["kam", "ein", "Prinz", ",", "dann", "hat\u00b7tet", "ihr", "stramm", "zu", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Und noch im Massengrab wart ihr die Schweine:", "tokens": ["Und", "noch", "im", "Mas\u00b7sen\u00b7grab", "wart", "ihr", "die", "Schwei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die Offiziere lagen alleine!", "tokens": ["Die", "Of\u00b7fi\u00b7zie\u00b7re", "la\u00b7gen", "al\u00b7lei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Ihr wart des Todes billige Ware . . .", "tokens": ["Ihr", "wart", "des", "To\u00b7des", "bil\u00b7li\u00b7ge", "Wa\u00b7re", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "So ging das vier lange blutige Jahre.", "tokens": ["So", "ging", "das", "vier", "lan\u00b7ge", "blu\u00b7ti\u00b7ge", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "CARD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Erinnert ihr euch \u2013?", "tokens": ["E\u00b7rin\u00b7nert", "ihr", "euch", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$(", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Die zweite Minute geh\u00f6re der Frau.", "tokens": ["Die", "zwei\u00b7te", "Mi\u00b7nu\u00b7te", "ge\u00b7h\u00f6\u00b7re", "der", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wem wurden zu Haus die Haare grau?", "tokens": ["Wem", "wur\u00b7den", "zu", "Haus", "die", "Haa\u00b7re", "grau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wer schreckte, wenn der Tag vorbei,", "tokens": ["Wer", "schreck\u00b7te", ",", "wenn", "der", "Tag", "vor\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in den N\u00e4chten auf mit einem Schrei?", "tokens": ["in", "den", "N\u00e4ch\u00b7ten", "auf", "mit", "ei\u00b7nem", "Schrei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wer ist es vier Jahre hindurch gewesen,", "tokens": ["Wer", "ist", "es", "vier", "Jah\u00b7re", "hin\u00b7durch", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "CARD", "NN", "PAV", "VAPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "der anstand in langen Polonaisen,", "tokens": ["der", "an\u00b7stand", "in", "lan\u00b7gen", "Po\u00b7lo\u00b7nai\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "indessen Prinzessinnen und ihre Gatten", "tokens": ["in\u00b7des\u00b7sen", "Prin\u00b7zes\u00b7sin\u00b7nen", "und", "ih\u00b7re", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "alles, alles, alles hatten \u2013 \u2013?", "tokens": ["al\u00b7les", ",", "al\u00b7les", ",", "al\u00b7les", "hat\u00b7ten", "\u2013", "\u2013", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "$,", "PIS", "$,", "PIS", "VAFIN", "$(", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wem schrieben sie einen kurzen Brief,", "tokens": ["Wem", "schrie\u00b7ben", "sie", "ei\u00b7nen", "kur\u00b7zen", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "da\u00df wieder einer in Flandern schlief?", "tokens": ["da\u00df", "wie\u00b7der", "ei\u00b7ner", "in", "Flan\u00b7dern", "schlief", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dazu ein Formular mit zwei Zetteln . . .", "tokens": ["Da\u00b7zu", "ein", "For\u00b7mu\u00b7lar", "mit", "zwei", "Zet\u00b7teln", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "CARD", "NN", "$.", "$.", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "wer mu\u00dfte hier um die Renten betteln?", "tokens": ["wer", "mu\u00df\u00b7te", "hier", "um", "die", "Ren\u00b7ten", "bet\u00b7teln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Tr\u00e4nen und Kr\u00e4mpfe und wildes Schrein.", "tokens": ["Tr\u00e4\u00b7nen", "und", "Kr\u00e4mp\u00b7fe", "und", "wil\u00b7des", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Er hatte Ruhe. Ihr wart allein.", "tokens": ["Er", "hat\u00b7te", "Ru\u00b7he", ".", "Ihr", "wart", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Oder sie schickten ihn, hinkend am Kn\u00fcppel,", "tokens": ["O\u00b7der", "sie", "schick\u00b7ten", "ihn", ",", "hin\u00b7kend", "am", "Kn\u00fcp\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "euch in die Arme zur\u00fcck als Kr\u00fcppel.", "tokens": ["euch", "in", "die", "Ar\u00b7me", "zu\u00b7r\u00fcck", "als", "Kr\u00fcp\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "So sah sie aus, die wunderbare", "tokens": ["So", "sah", "sie", "aus", ",", "die", "wun\u00