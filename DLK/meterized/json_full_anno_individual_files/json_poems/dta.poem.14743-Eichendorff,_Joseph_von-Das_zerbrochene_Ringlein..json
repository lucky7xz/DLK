{"dta.poem.14743": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das zerbrochene Ringlein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r' ich das M\u00fchlrad gehen:", "tokens": ["H\u00f6r'", "ich", "das", "M\u00fchl\u00b7rad", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df nicht, was ich will \u2014", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "will"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich m\u00f6cht' am liebsten sterben,", "tokens": ["Ich", "m\u00f6cht'", "am", "liebs\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da w\u00e4r's auf einmal still!", "tokens": ["Da", "w\u00e4r's", "auf", "ein\u00b7mal", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}