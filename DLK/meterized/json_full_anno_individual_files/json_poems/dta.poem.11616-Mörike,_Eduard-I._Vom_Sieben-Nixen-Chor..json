{"dta.poem.11616": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Vom Sieben-Nixen-Chor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Sizt ein Mann von ernster Sch\u00f6ne,", "tokens": ["Sizt", "ein", "Mann", "von", "erns\u00b7ter", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sizt der Magier Drakone", "tokens": ["Sizt", "der", "Ma\u00b7gier", "Dra\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Auf dem Gartenhausbalkone", "tokens": ["Auf", "dem", "Gar\u00b7ten\u00b7haus\u00b7bal\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Prinzessin Liligi;", "tokens": ["Mit", "Prin\u00b7zes\u00b7sin", "Li\u00b7li\u00b7gi", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lehrt sie allda seine Lehre", "tokens": ["Lehrt", "sie", "all\u00b7da", "sei\u00b7ne", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Erde, von dem Himmel,", "tokens": ["Von", "der", "Er\u00b7de", ",", "von", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem Traum der Elemente,", "tokens": ["Von", "dem", "Traum", "der", "E\u00b7le\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "Vom Geschick im Sternenkreise.", "tokens": ["Vom", "Ge\u00b7schick", "im", "Ster\u00b7nen\u00b7krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df es aber nun genug seyn,", "tokens": ["La\u00df", "es", "a\u00b7ber", "nun", "ge\u00b7nug", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitternacht ist lang vor\u00fcber, \u2014", "tokens": ["Mit\u00b7ter\u00b7nacht", "ist", "lang", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "ADJD", "PTKVZ", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht Prinzessin Liligi, \u2014", "tokens": ["Spricht", "Prin\u00b7zes\u00b7sin", "Li\u00b7li\u00b7gi", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "NE", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nach solchen Wunderdingen,", "tokens": ["Und", "nach", "sol\u00b7chen", "Wun\u00b7der\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4chtigen und ungewohnten,", "tokens": ["M\u00e4ch\u00b7ti\u00b7gen", "und", "un\u00b7ge\u00b7wohn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00fcstet mich nach Kinderm\u00e4hrchen,", "tokens": ["L\u00fcs\u00b7tet", "mich", "nach", "Kin\u00b7der\u00b7m\u00e4hr\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieber Mann, ich wei\u00df nicht wie!", "tokens": ["Lie\u00b7ber", "Mann", ",", "ich", "wei\u00df", "nicht", "wie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eh\u00f6rst du gern das Lied vom Winde,", "tokens": ["\u201e", "h\u00f6rst", "du", "gern", "das", "Lied", "vom", "Win\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das nicht End' noch Anfang hat,", "tokens": ["Das", "nicht", "End'", "noch", "An\u00b7fang", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "NN", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder gern vom K\u00f6nigskinde,", "tokens": ["O\u00b7der", "gern", "vom", "K\u00f6\u00b7nigs\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gerne von der Muschelstadt?\u201c", "tokens": ["Ger\u00b7ne", "von", "der", "Mu\u00b7schel\u00b7stadt", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Singe du so heut wie gestern", "tokens": ["Sin\u00b7ge", "du", "so", "heut", "wie", "ge\u00b7stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von des Meeres Lustrevier,", "tokens": ["Von", "des", "Mee\u00b7res", "Lus\u00b7tre\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Haus der sieben Schwestern,", "tokens": ["Von", "dem", "Haus", "der", "sie\u00b7ben", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vom K\u00f6nigssohne mir.", "tokens": ["Und", "vom", "K\u00f6\u00b7nigs\u00b7soh\u00b7ne", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201ezwischen gr\u00fcnen Wasserw\u00e4nden", "tokens": ["\u201e", "zwi\u00b7schen", "gr\u00fc\u00b7nen", "Was\u00b7ser\u00b7w\u00e4n\u00b7den"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sizt der Sieben-Nixen-Chor;", "tokens": ["Sizt", "der", "Sie\u00b7ben\u00b7Ni\u00b7xen\u00b7Chor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wasserrosen in den H\u00e4nden,", "tokens": ["Was\u00b7ser\u00b7ro\u00b7sen", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauschen sie zum Licht empor.", "tokens": ["Lau\u00b7schen", "sie", "zum", "Licht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wenn oftmals auf der H\u00f6he", "tokens": ["Und", "wenn", "oft\u00b7mals", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schiffe fahren, schattengleich,", "tokens": ["Schif\u00b7fe", "fah\u00b7ren", ",", "schat\u00b7ten\u00b7gleich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steigt ein siebenfaches Wehe", "tokens": ["Steigt", "ein", "sie\u00b7ben\u00b7fa\u00b7ches", "We\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem stillen Wasserreich.", "tokens": ["Aus", "dem", "stil\u00b7len", "Was\u00b7ser\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann, beim Spiel von Zauberglocken,", "tokens": ["Dann", ",", "beim", "Spiel", "von", "Zau\u00b7ber\u00b7glo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drehn die Schwestern sich im Tanz,", "tokens": ["Drehn", "die", "Schwes\u00b7tern", "sich", "im", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fctteln wild die gr\u00fcnen Locken", "tokens": ["Sch\u00fct\u00b7teln", "wild", "die", "gr\u00fc\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verlieren Gurt und Kranz.", "tokens": ["Und", "ver\u00b7lie\u00b7ren", "Gurt", "und", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und das Meer beginnt zu schwanken,", "tokens": ["Und", "das", "Meer", "be\u00b7ginnt", "zu", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Well' auf Welle steigt und springt,", "tokens": ["Well'", "auf", "Wel\u00b7le", "steigt", "und", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Elemente zanken", "tokens": ["Al\u00b7le", "E\u00b7le\u00b7men\u00b7te", "zan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Um das Schiff, bis es versinkt.\u201c", "tokens": ["Um", "das", "Schiff", ",", "bis", "es", "ver\u00b7sinkt", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Also sang in Zaubert\u00f6nen", "tokens": ["Al\u00b7so", "sang", "in", "Zau\u00b7ber\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00df der Magier Drakone", "tokens": ["S\u00fc\u00df", "der", "Ma\u00b7gier", "Dra\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu der lieblichen Prinzessin;", "tokens": ["Zu", "der", "lieb\u00b7li\u00b7chen", "Prin\u00b7zes\u00b7sin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und zuweilen, im Gesange,", "tokens": ["Und", "zu\u00b7wei\u00b7len", ",", "im", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Neiget er der Lippen Milde", "tokens": ["Nei\u00b7get", "er", "der", "Lip\u00b7pen", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu dem feuchten Rosenmunde,", "tokens": ["Zu", "dem", "feuch\u00b7ten", "Ro\u00b7sen\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu den hyazintheblauen,", "tokens": ["Zu", "den", "hy\u00b7a\u00b7zint\u00b7he\u00b7blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schon in Schlaf gesenkten Augen", "tokens": ["Schon", "in", "Schlaf", "ge\u00b7senk\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der beth\u00f6rten Jungfrau hin.", "tokens": ["Der", "be\u00b7th\u00f6r\u00b7ten", "Jung\u00b7frau", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Diese meint im leichten Schlummer,", "tokens": ["Die\u00b7se", "meint", "im", "leich\u00b7ten", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Stets noch h\u00f6re sie die Lehre", "tokens": ["Stets", "noch", "h\u00f6\u00b7re", "sie", "die", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Von der Erde, von dem Himmel,", "tokens": ["Von", "der", "Er\u00b7de", ",", "von", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Vom Geschick im Sternenkreise,", "tokens": ["Vom", "Ge\u00b7schick", "im", "Ster\u00b7nen\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Doch zulezt erwachet sie:", "tokens": ["Doch", "zu\u00b7lezt", "er\u00b7wa\u00b7chet", "sie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "La\u00df es aber nun genug seyn!", "tokens": ["La\u00df", "es", "a\u00b7ber", "nun", "ge\u00b7nug", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitternacht ist lang vor\u00fcber,", "tokens": ["Mit\u00b7ter\u00b7nacht", "ist", "lang", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nach solchen Wunderdingen,", "tokens": ["Und", "nach", "sol\u00b7chen", "Wun\u00b7der\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4chtigen und ungewohnten,", "tokens": ["M\u00e4ch\u00b7ti\u00b7gen", "und", "un\u00b7ge\u00b7wohn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00fcstet mich nach Kinderm\u00e4hrchen,", "tokens": ["L\u00fcs\u00b7tet", "mich", "nach", "Kin\u00b7der\u00b7m\u00e4hr\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber Mann, ich wei\u00df nicht wie!", "tokens": ["Lie\u00b7ber", "Mann", ",", "ich", "wei\u00df", "nicht", "wie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": 