{"dta.poem.7673": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind denn der K\u00f6rner durch die weggefegte Spreue", "tokens": ["Sind", "denn", "der", "K\u00f6r\u00b7ner", "durch", "die", "weg\u00b7ge\u00b7feg\u00b7te", "Spreu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geworden mehr, da\u00df dich ihr Anblick mehr erfreue?", "tokens": ["Ge\u00b7wor\u00b7den", "mehr", ",", "da\u00df", "dich", "ihr", "An\u00b7blick", "mehr", "er\u00b7freu\u00b7e", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nein, K\u00f6rnlein selber sind wol mit der Spreu entlaufen;", "tokens": ["Nein", ",", "K\u00f6rn\u00b7lein", "sel\u00b7ber", "sind", "wol", "mit", "der", "Spreu", "ent\u00b7lau\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was aber blieb ist nun ein reiner K\u00f6rnerhaufen.", "tokens": ["Was", "a\u00b7ber", "blieb", "ist", "nun", "ein", "rei\u00b7ner", "K\u00f6r\u00b7ner\u00b7hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."