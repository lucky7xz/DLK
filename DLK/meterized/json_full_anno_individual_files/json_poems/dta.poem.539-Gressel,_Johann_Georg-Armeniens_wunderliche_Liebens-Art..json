{"dta.poem.539": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Armeniens  wunderliche Liebens-Art.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Soll uns Armenie mit ihrer Gunst anlachen/", "tokens": ["Soll", "uns", "Ar\u00b7me\u00b7nie", "mit", "ih\u00b7rer", "Gunst", "an\u00b7la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "So mu\u00df man sie durch Geld zuvor verliebet machen/", "tokens": ["So", "mu\u00df", "man", "sie", "durch", "Geld", "zu\u00b7vor", "ver\u00b7lie\u00b7bet", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "APPR", "NN", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Braucht man dis Kunst-st\u00fcck nicht/ so triegt die Meynung oft/", "tokens": ["Braucht", "man", "dis", "Kunst\u00b7st\u00fcck", "nicht", "/", "so", "triegt", "die", "Mey\u00b7nung", "oft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDS", "NN", "PTKNEG", "$(", "ADV", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und man erlangt das nicht/ worauf man sonst gehofft.", "tokens": ["Und", "man", "er\u00b7langt", "das", "nicht", "/", "wo\u00b7rauf", "man", "sonst", "ge\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PDS", "PTKNEG", "$(", "PWAV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat sie die krumme Hand mit Gaben nicht vergn\u00fcget/", "tokens": ["Hat", "sie", "die", "krum\u00b7me", "Hand", "mit", "Ga\u00b7ben", "nicht", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kriegt man keinen Blick ob man gleich vor ihr lieget/", "tokens": ["So", "kriegt", "man", "kei\u00b7nen", "Blick", "ob", "man", "gleich", "vor", "ihr", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "KOUS", "PIS", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie dencket/ weil sie sch\u00f6n und liebens-w\u00fcrdig ist/", "tokens": ["Sie", "den\u00b7cket", "/", "weil", "sie", "sch\u00f6n", "und", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es schw\u00e4rtzte ihre Zier wenn ohne Geld gek\u00fc\u00dft.", "tokens": ["Es", "schw\u00e4rtz\u00b7te", "ih\u00b7re", "Zier", "wenn", "oh\u00b7ne", "Geld", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOUS", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie will der ", "tokens": ["Sie", "will", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Man soll durchs Gold wie ", "tokens": ["Man", "soll", "durchs", "Gold", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPRART", "NN", "KOKOM"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Denn gleich wie dessen Gold durch Stein und H\u00fcter brach/", "tokens": ["Denn", "gleich", "wie", "des\u00b7sen", "Gold", "durch", "Stein", "und", "H\u00fc\u00b7ter", "brach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "PDS", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So gibt sie alsofort auf das Geschencke nach.", "tokens": ["So", "gibt", "sie", "al\u00b7so\u00b7fort", "auf", "das", "Ge\u00b7schen\u00b7cke", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Sie l\u00e4st sich nicht durch List noch Schmeichelung besiegen/", "tokens": ["Sie", "l\u00e4st", "sich", "nicht", "durch", "List", "noch", "Schmei\u00b7che\u00b7lung", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie liebet den Gebrauch mit g\u00fcldnen Kugeln kriegen/", "tokens": ["Sie", "lie\u00b7bet", "den", "Ge\u00b7brauch", "mit", "g\u00fcld\u00b7nen", "Ku\u00b7geln", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der grosse ", "tokens": ["Der", "gros\u00b7se"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Weil er ihr diese Kunst zu ihren Vortheil lehrt.", "tokens": ["Weil", "er", "ihr", "die\u00b7se", "Kunst", "zu", "ih\u00b7ren", "Vor\u00b7theil", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vom Freyen h\u00e4lt sie nichts/ noch weniger von Nehmen/", "tokens": ["Vom", "Frey\u00b7en", "h\u00e4lt", "sie", "nichts", "/", "noch", "we\u00b7ni\u00b7ger", "von", "Neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIS", "$(", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Des Nehmens aber wil sie selber sich nicht sch\u00e4men/", "tokens": ["Des", "Neh\u00b7mens", "a\u00b7ber", "wil", "sie", "sel\u00b7ber", "sich", "nicht", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "ADV", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie siehet auf das Geld/ ist dieses mit darbey/", "tokens": ["Sie", "sie\u00b7het", "auf", "das", "Geld", "/", "ist", "die\u00b7ses", "mit", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "VAFIN", "PDAT", "APPR", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So glaub er/ da\u00df er schon der Vestung Meister sey.", "tokens": ["So", "glaub", "er", "/", "da\u00df", "er", "schon", "der", "Ves\u00b7tung", "Meis\u00b7ter", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer will/ mag diese Kunst nur einmahl an ihr \u00fcben/", "tokens": ["Wer", "will", "/", "mag", "die\u00b7se", "Kunst", "nur", "ein\u00b7mahl", "an", "ihr", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "VMFIN", "PDAT", "NN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sie wird ihn alsofort mit grosser Inbrunst lieben/", "tokens": ["Sie", "wird", "ihn", "al\u00b7so\u00b7fort", "mit", "gros\u00b7ser", "In\u00b7brunst", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn spricht sie: Wer kan wol von Stahl und Eysen seyn/", "tokens": ["Denn", "spricht", "sie", ":", "Wer", "kan", "wol", "von", "Stahl", "und", "Ey\u00b7sen", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PWS", "VMFIN", "ADV", "APPR", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ist doch ein heisser Ku\u00df auch bey den Engeln rein", "tokens": ["Ist", "doch", "ein", "heis\u00b7ser", "Ku\u00df", "auch", "bey", "den", "En\u00b7geln", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allein was wird daraus? ach Kinder/ Kinder/ Kinder!", "tokens": ["Al\u00b7lein", "was", "wird", "da\u00b7raus", "?", "ach", "Kin\u00b7der", "/", "Kin\u00b7der", "/", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "PAV", "$.", "XY", "NN", "$(", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie aber bleibt getrost/ und buhlt nichts destominder/", "tokens": ["Sie", "a\u00b7ber", "bleibt", "ge\u00b7trost", "/", "und", "buhlt", "nichts", "des\u00b7to\u00b7min\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "VVPP", "$(", "KON", "VVFIN", "PIS", "PAV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie schreyet immer fort/ ich nehme dennoch Geld/", "tokens": ["Sie", "schre\u00b7yet", "im\u00b7mer", "fort", "/", "ich", "neh\u00b7me", "den\u00b7noch", "Geld", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$(", "PPER", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ob mich schon manches Kind vor seine Mutter h\u00e4lt.", "tokens": ["Ob", "mich", "schon", "man\u00b7ches", "Kind", "vor", "sei\u00b7ne", "Mut\u00b7ter", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}