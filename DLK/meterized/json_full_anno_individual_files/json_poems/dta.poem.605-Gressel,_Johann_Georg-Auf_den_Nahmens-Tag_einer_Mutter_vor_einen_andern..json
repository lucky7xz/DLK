{"dta.poem.605": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Nahmens-Tag einer Mutter/  \n vor einen andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hochwertheste! so steht es wohl", "tokens": ["Hoch\u00b7wert\u00b7hes\u00b7te", "!", "so", "steht", "es", "wohl"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Um Sie und uns zu allen Zeiten", "tokens": ["Um", "Sie", "und", "uns", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "KON", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gl\u00fccke will sie wie es soll", "tokens": ["Das", "Gl\u00fc\u00b7cke", "will", "sie", "wie", "es", "soll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "PWAV", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer Lebens-Zeit begleiten.", "tokens": ["In", "ih\u00b7rer", "Le\u00b7bens\u00b7Zeit", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Giebt Gott/ da\u00df dieses frohe Licht/", "tokens": ["Giebt", "Gott", "/", "da\u00df", "die\u00b7ses", "fro\u00b7he", "Licht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "KOUS", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch offte so ein zu uns bricht/", "tokens": ["Noch", "off\u00b7te", "so", "ein", "zu", "uns", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "APPR", "PPER", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Hochwertheste! denn steht es wohl.", "tokens": ["Hoch\u00b7wert\u00b7hes\u00b7te", "!", "denn", "steht", "es", "wohl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$.", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}