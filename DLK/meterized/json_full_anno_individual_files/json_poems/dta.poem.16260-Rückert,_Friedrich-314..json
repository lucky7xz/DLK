{"dta.poem.16260": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "314.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kunst veredelt, was sie mit der Hand ber\u00fchrt,", "tokens": ["Die", "Kunst", "ver\u00b7e\u00b7delt", ",", "was", "sie", "mit", "der", "Hand", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darum der h\u00f6chste Rang ihr im Verkehr geb\u00fchrt.", "tokens": ["Da\u00b7rum", "der", "h\u00f6chs\u00b7te", "Rang", "ihr", "im", "Ver\u00b7kehr", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPOSAT", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie findet Holz und Stein, und braucht den Zauberstab,", "tokens": ["Sie", "fin\u00b7det", "Holz", "und", "Stein", ",", "und", "braucht", "den", "Zau\u00b7ber\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ihnen Lebensschein und Geistesformen gab.", "tokens": ["Der", "ih\u00b7nen", "Le\u00b7bens\u00b7schein", "und", "Geis\u00b7tes\u00b7for\u00b7men", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ungebildetes ihr in die Hand gekommen,", "tokens": ["Was", "un\u00b7ge\u00b7bil\u00b7de\u00b7tes", "ihr", "in", "die", "Hand", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie es hindurch gieng, hat es Bildung angenommen.", "tokens": ["Wie", "es", "hin\u00b7durch", "gieng", ",", "hat", "es", "Bil\u00b7dung", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PAV", "VVFIN", "$,", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und auch das Handwerk hat in allen seinen Gilden", "tokens": ["Und", "auch", "das", "Hand\u00b7werk", "hat", "in", "al\u00b7len", "sei\u00b7nen", "Gil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dis mit der Kunst gemein, den rohen Stoff zu bilden.", "tokens": ["Dis", "mit", "der", "Kunst", "ge\u00b7mein", ",", "den", "ro\u00b7hen", "Stoff", "zu", "bil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Der Handel aber, der von Hand und Handeln tr\u00e4gt", "tokens": ["Der", "Han\u00b7del", "a\u00b7ber", ",", "der", "von", "Hand", "und", "Han\u00b7deln", "tr\u00e4gt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Namen, hat dem Stoff kein Zeichen aufgepr\u00e4gt.", "tokens": ["Den", "Na\u00b7men", ",", "hat", "dem", "Stoff", "kein", "Zei\u00b7chen", "auf\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gleichg\u00fcltig handelt er mit Allem; sein Behandeln,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "han\u00b7delt", "er", "mit", "Al\u00b7lem", ";", "sein", "Be\u00b7han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PIS", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt zu veredeln, will es nur in Geld verwandeln.", "tokens": ["Statt", "zu", "ver\u00b7e\u00b7deln", ",", "will", "es", "nur", "in", "Geld", "ver\u00b7wan\u00b7deln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nicht edler wird die Waar', indem sie durch die Hand", "tokens": ["Nicht", "ed\u00b7ler", "wird", "die", "Waa\u00b7r'", ",", "in\u00b7dem", "sie", "durch", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des Kaufmanns geht und wird gef\u00fchrt von Land zu Land.", "tokens": ["Des", "Kauf\u00b7manns", "geht", "und", "wird", "ge\u00b7f\u00fchrt", "von", "Land", "zu", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VAFIN", "VVPP", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch wird sie theuerer nach Ma\u00df, Gewicht und Elle,", "tokens": ["Doch", "wird", "sie", "theu\u00b7e\u00b7rer", "nach", "Ma\u00df", ",", "Ge\u00b7wicht", "und", "El\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Indem er \u00fcberall sie bringt zur rechten Stelle.", "tokens": ["In\u00b7dem", "er", "\u00fc\u00b7be\u00b7rall", "sie", "bringt", "zur", "rech\u00b7ten", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dis lern von ihm, ohn' ihn zu loben noch zu schelten:", "tokens": ["Dis", "lern", "von", "ihm", ",", "ohn'", "ihn", "zu", "lo\u00b7ben", "noch", "zu", "schel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mach' alles, was du hast, am rechten Orte gelten.", "tokens": ["Mach'", "al\u00b7les", ",", "was", "du", "hast", ",", "am", "rech\u00b7ten", "Or\u00b7te", "gel\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VAFIN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}