{"dta.poem.21656": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Arbeiter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es summt und dr\u00f6hnt mit dumpfem Ton", "tokens": ["Es", "summt", "und", "dr\u00f6hnt", "mit", "dum\u00b7pfem", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und qualmt und raucht ringsum,", "tokens": ["Und", "qualmt", "und", "raucht", "ring\u00b7sum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Mann an Mann in schwerer Frohn", "tokens": ["Und", "Mann", "an", "Mann", "in", "schwe\u00b7rer", "Frohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An seinem Platze stumm.", "tokens": ["An", "sei\u00b7nem", "Plat\u00b7ze", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Hammer sinkt, die Esse spr\u00fcht,", "tokens": ["Der", "Ham\u00b7mer", "sinkt", ",", "die", "Es\u00b7se", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Eisen in der Flamme gl\u00fcht.", "tokens": ["Das", "Ei\u00b7sen", "in", "der", "F