{"textgrid.poem.52006": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erd ist nicht so kalt, wann sie der Winter dr\u00fcckt,", "tokens": ["Die", "Erd", "ist", "nicht", "so", "kalt", ",", "wann", "sie", "der", "Win\u00b7ter", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als der verschrumpne Leib, an dem du dich wilt w\u00e4rmen:", "tokens": ["Als", "der", "ver\u00b7schrump\u00b7ne", "Leib", ",", "an", "dem", "du", "dich", "wilt", "w\u00e4r\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht nicht umb ein Aa\u00df so viele Wespen schw\u00e4rmen,", "tokens": ["Man", "sieht", "nicht", "umb", "ein", "Aa\u00df", "so", "vie\u00b7le", "Wes\u00b7pen", "schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Fl\u00f6he von ihr ziehn, wenn sie den Buckel r\u00fcckt.", "tokens": ["Als", "Fl\u00f6\u00b7he", "von", "ihr", "ziehn", ",", "wenn", "sie", "den", "Bu\u00b7ckel", "r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein Beinhau\u00df blecket so, wie ihre Knochen stehn,", "tokens": ["Kein", "Be\u00b7in\u00b7hau\u00df", "blec\u00b7ket", "so", ",", "wie", "ih\u00b7re", "Kno\u00b7chen", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Die dis Gerippe hier hat unterm Rocke liegen:", "tokens": ["Die", "dis", "Ge\u00b7rip\u00b7pe", "hier", "hat", "un\u00b7term", "Ro\u00b7cke", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADV", "VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Flu\u00df f\u00fchrt so viel Ey\u00df, als du dort wirst bekriegen,", "tokens": ["Kein", "Flu\u00df", "f\u00fchrt", "so", "viel", "Ey\u00df", ",", "als", "du", "dort", "wirst", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Woraus bey andern sonst die W\u00e4rmbde pflegt zu gehn.", "tokens": ["Wo\u00b7raus", "bey", "an\u00b7dern", "sonst", "die", "W\u00e4rmb\u00b7de", "pflegt", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PIS", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Bruder, setzt dir dann der b\u00f6se Feind so zu,", "tokens": ["Mein", "Bru\u00b7der", ",", "setzt", "dir", "dann", "der", "b\u00f6\u00b7se", "Feind", "so", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wilst ohn allen Danck hin auff den Teppicht treten,", "tokens": ["Und", "wilst", "ohn", "al\u00b7len", "Danck", "hin", "auff", "den", "Tep\u00b7picht", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wirstu den Drachen nicht von alten Thalern beten,", "tokens": ["Wirs\u00b7tu", "den", "Dra\u00b7chen", "nicht", "von", "al\u00b7ten", "Tha\u00b7lern", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bethe, da\u00df du stracks m\u00f6gst kommen zu der Ruh.", "tokens": ["So", "be\u00b7the", ",", "da\u00df", "du", "stracks", "m\u00f6gst", "kom\u00b7men", "zu", "der", "Ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Erd ist nicht so kalt, wann sie der Wi