{"dta.poem.4270": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken im Mondschein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! es kann, in tausend Freuden,", "tokens": ["Herr", "!", "es", "kann", ",", "in", "tau\u00b7send", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "$,", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele durchs Gesicht,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "durchs", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Mondes heiterm Licht,", "tokens": ["In", "des", "Mon\u00b7des", "hei\u00b7term", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich an Deinen Wundern weiden.", "tokens": ["Sich", "an", "Dei\u00b7nen", "Wun\u00b7dern", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gieb! da\u00df alles, was ich sehe", "tokens": ["Gieb", "!", "da\u00df", "al\u00b7les", ",", "was", "ich", "se\u00b7he"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "KOUS", "PIS", "$,", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Fl\u00e4che, wo ich stehe,", "tokens": ["Auf", "der", "Fl\u00e4\u00b7che", ",", "wo", "ich", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In des Himmels tiefen H\u00f6he,", "tokens": ["In", "des", "Him\u00b7mels", "tie\u00b7fen", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blo\u00df nur Dir zum Ruhm geschehe!", "tokens": ["Blo\u00df", "nur", "Dir", "zum", "Ruhm", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seh\u2019 ich, wie so hell und rein", "tokens": ["Seh'", "ich", ",", "wie", "so", "hell", "und", "rein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier der Mond im Wiederschein,", "tokens": ["Hier", "der", "Mond", "im", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Spiegel-glatten Fluht,", "tokens": ["Auf", "der", "Spie\u00b7gel\u00b7glat\u00b7ten", "Fluht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheinet, gl\u00e4nzet, glimmt und ruht,", "tokens": ["Schei\u00b7net", ",", "gl\u00e4n\u00b7zet", ",", "glimmt", "und", "ruht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie er sie so hell bestrahlet,", "tokens": ["Wie", "er", "sie", "so", "hell", "be\u00b7strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sein Bild im Wasser mahlet;", "tokens": ["Und", "sein", "Bild", "im", "Was\u00b7ser", "mah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann ich, wenn ein L\u00fcftgen wehet,", "tokens": ["Kann", "ich", ",", "wenn", "ein", "L\u00fcft\u00b7gen", "we\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dadurch ein pl\u00f6tzlichs Regen,", "tokens": ["Und", "da\u00b7durch", "ein", "pl\u00f6tz\u00b7lichs", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Und ein schuppigtes Bewegen,", "tokens": ["Und", "ein", "schup\u00b7pig\u00b7tes", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Auf des Wassers Gl\u00e4tt\u2019, entsteht,", "tokens": ["Auf", "des", "Was\u00b7sers", "Gl\u00e4tt'", ",", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf dem glatt- und dunklen Blauen", "tokens": ["Auf", "dem", "glat\u00b7t", "und", "dunk\u00b7len", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Noch ein neu Vergn\u00fcgen schauen,", "tokens": ["Noch", "ein", "neu", "Ver\u00b7gn\u00fc\u00b7gen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da, auf eben dieser Stelle,", "tokens": ["Da", ",", "auf", "e\u00b7ben", "die\u00b7ser", "Stel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Durch des Mondes regen Glanz,", "tokens": ["Durch", "des", "Mon\u00b7des", "re\u00b7gen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Recht ein grosser Silber-Kranz,", "tokens": ["Recht", "ein", "gros\u00b7ser", "Sil\u00b7ber\u00b7Kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Eine Licht- und Feuer-Quelle,", "tokens": ["Ei\u00b7ne", "Licht", "und", "Feu\u00b7er\u00b7Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Schnell sich zu erheben scheint,", "tokens": ["Schnell", "sich", "zu", "er\u00b7he\u00b7ben", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Welches cirkelt, wirbelt, glimmt,", "tokens": ["Wel\u00b7ches", "cir\u00b7kelt", ",", "wir\u00b7belt", ",", "glimmt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "So, da\u00df in dem blauen Dunkeln", "tokens": ["So", ",", "da\u00df", "in", "dem", "blau\u00b7en", "Dun\u00b7keln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Man, get\u00e4uscht, nicht anders meynt,", "tokens": ["Man", ",", "ge\u00b7t\u00e4uscht", ",", "nicht", "an\u00b7ders", "meynt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "VVPP", "$,", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als da\u00df Feur und Gluht hier schwimmt,", "tokens": ["Als", "da\u00df", "Feur", "und", "Gluht", "hier", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Millionen Funken funkeln.", "tokens": ["Mil\u00b7lion\u00b7en", "Fun\u00b7ken", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bedachte, wie so schnell", "tokens": ["Ich", "be\u00b7dach\u00b7te", ",", "wie", "so", "schnell"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier auf dieser Unterwelt", "tokens": ["Hier", "auf", "die\u00b7ser", "Un\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles sich so leicht verstellt,", "tokens": ["Al\u00b7les", "sich", "so", "leicht", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was erst eben, glatt und hell,", "tokens": ["Was", "erst", "e\u00b7ben", ",", "glatt", "und", "hell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unbeweglich, rein und klar,", "tokens": ["Un\u00b7be\u00b7weg\u00b7lich", ",", "rein", "und", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Still, poliert und ruhig war,", "tokens": ["Still", ",", "po\u00b7liert", "und", "ru\u00b7hig", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wird im Augenblick verzogen.", "tokens": ["Wird", "im", "Au\u00b7gen\u00b7blick", "ver\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Pl\u00f6tzlich zeiget Unruh\u2019 sich,", "tokens": ["Pl\u00f6tz\u00b7lich", "zei\u00b7get", "Un\u00b7ruh'", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "PRF", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.11": {"text": "Pl\u00f6tzlich sieht man rege Wogen,", "tokens": ["Pl\u00f6tz\u00b7lich", "sieht", "man", "re\u00b7ge", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wo es erst dem Spiegel glich.", "tokens": ["Wo", "es", "erst", "dem", "Spie\u00b7gel", "glich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Aber auch da, wo sichs reget,", "tokens": ["A\u00b7ber", "auch", "da", ",", "wo", "sichs", "re\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und verwirret sich beweget,", "tokens": ["Und", "ver\u00b7wir\u00b7ret", "sich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "In dem Sinken und Erh\u00f6h\u2019n,", "tokens": ["In", "dem", "Sin\u00b7ken", "und", "Er\u00b7h\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Siehet man mehr Glanz entsteh\u2019n;", "tokens": ["Sie\u00b7het", "man", "mehr", "Glanz", "ent\u00b7steh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "So l\u00e4\u00dft oft, wenns widrig gehet,", "tokens": ["So", "l\u00e4\u00dft", "oft", ",", "wenns", "wid\u00b7rig", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Und ein Ungl\u00fccks-Sturm oft wehet,", "tokens": ["Und", "ein", "Un\u00b7gl\u00fccks\u00b7Sturm", "oft", "we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Sich mehr Feur der Andacht seh\u2019n.", "tokens": ["Sich", "mehr", "Feur", "der", "An\u00b7dacht", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PRF", "PIAT", "NN", "ART", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}