{"textgrid.poem.66481": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Treue Gef\u00e4hrtin", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treue Gef\u00e4hrtin", "tokens": ["Treu\u00b7e", "Ge\u00b7f\u00e4hr\u00b7tin"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf der Gedanken", "tokens": ["Auf", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Rastlos durchwandertem", "tokens": ["Rast\u00b7los", "durch\u00b7wan\u00b7der\u00b7tem"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJA"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "N\u00e4chtlichem Pfad:", "tokens": ["N\u00e4cht\u00b7li\u00b7chem", "Pfad", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dir will ich endlich", "tokens": ["Dir", "will", "ich", "end\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ein lang gehegtes,", "tokens": ["Ein", "lang", "ge\u00b7heg\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dir will ich dankbar", "tokens": ["Dir", "will", "ich", "dank\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Weihen ein Lied!", "tokens": ["Wei\u00b7hen", "ein", "Lied", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wenn von der Seele", "tokens": ["Wenn", "von", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Finsterem Ringen", "tokens": ["Fins\u00b7te\u00b7rem", "Rin\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Weit mir entflohen", "tokens": ["Weit", "mir", "ent\u00b7flo\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der stille Gott,", "tokens": ["Der", "stil\u00b7le", "Gott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dann wie ein Pharus", "tokens": ["Dann", "wie", "ein", "Pha\u00b7rus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Winktest du segnend mir", "tokens": ["Wink\u00b7test", "du", "seg\u00b7nend", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Zu des Gesanges", "tokens": ["Zu", "des", "Ge\u00b7san\u00b7ges"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.8": {"text": "Erleuchtetem Pfad.", "tokens": ["Er\u00b7leuch\u00b7te\u00b7tem", "Pfad", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Denn aus der Tiefe", "tokens": ["Denn", "aus", "der", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dr\u00e4ngt sich und hebt sich", "tokens": ["Dr\u00e4ngt", "sich", "und", "hebt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "PRF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wer sich gegeben", "tokens": ["Wer", "sich", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PRF", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem ewigen Licht,", "tokens": ["Dem", "e\u00b7wi\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und es umschlingen ihn", "tokens": ["Und", "es", "um\u00b7schlin\u00b7gen", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Erdend\u00e4monen,", "tokens": ["Er\u00b7den\u00b7d\u00e4\u00b7mo\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sorgen und Schmerzen,", "tokens": ["Sor\u00b7gen", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Drohend umsonst.", "tokens": ["Dro\u00b7hend", "um\u00b7sonst", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Stolz wie der Adler", "tokens": ["Stolz", "wie", "der", "Ad\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum Wolkenhause", "tokens": ["Zum", "Wol\u00b7ken\u00b7hau\u00b7se"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Des ewigen Vaters", "tokens": ["Des", "e\u00b7wi\u00b7gen", "Va\u00b7ters"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Fl\u00fcgel hebt:", "tokens": ["Die", "Fl\u00fc\u00b7gel", "hebt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In blitzgewohnten", "tokens": ["In", "blitz\u00b7ge\u00b7wohn\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sicheren F\u00e4ngen", "tokens": ["Si\u00b7che\u00b7ren", "F\u00e4n\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "F\u00fchrt er zum Aether", "tokens": ["F\u00fchrt", "er", "zum", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.8": {"text": "Der Jugend Bild:", "tokens": ["Der", "Ju\u00b7gend", "Bild", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "So aus der N\u00e4chte", "tokens": ["So", "aus", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dampfendem Chaos", "tokens": ["Damp\u00b7fen\u00b7dem", "Chaos"], "token_info": ["word", "word"], "pos": ["PAV", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ringt sich die Seele", "tokens": ["Ringt", "sich", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gest\u00e4rkten Flugs;", "tokens": ["Ge\u00b7st\u00e4rk\u00b7ten", "Flugs", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit der geraubten", "tokens": ["Mit", "der", "ge\u00b7raub\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ewigen Jugend", "tokens": ["E\u00b7wi\u00b7gen", "Ju\u00b7gend"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Hebt sie zum Licht sich", "tokens": ["Hebt", "sie", "zum", "Licht", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PRF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Athmend empor.", "tokens": ["Ath\u00b7mend", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Unter ihr w\u00e4lzen sich", "tokens": ["Un\u00b7ter", "ihr", "w\u00e4l\u00b7zen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PRF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "V\u00f6lkergeschicke,", "tokens": ["V\u00f6l\u00b7ker\u00b7ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "K\u00e4mpfe der Edlen", "tokens": ["K\u00e4mp\u00b7fe", "der", "Ed\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Um Licht und Recht.", "tokens": ["Um", "Licht", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ob auch erliegend", "tokens": ["Ob", "auch", "er\u00b7lie\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Mi\u00dfgunst Pfeilen,", "tokens": ["Der", "Mi\u00df\u00b7gunst", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Palmen umkr\u00e4nzen", "tokens": ["Pal\u00b7men", "um\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Des Siegers Haupt.", "tokens": ["Des", "Sie\u00b7gers", "Haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Denn seit der erste", "tokens": ["Denn", "seit", "der", "ers\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gewaltige Dulder", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ge", "Dul\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Siegreich b\u00fc\u00dfte", "tokens": ["Sieg\u00b7reich", "b\u00fc\u00df\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Den Flammenraub,", "tokens": ["Den", "Flam\u00b7men\u00b7raub", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "H\u00e4lt fest am Kampfe,", "tokens": ["H\u00e4lt", "fest", "am", "Kamp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "An seinem Rechte,", "tokens": ["An", "sei\u00b7nem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dem uralt ewigen,", "tokens": ["Dem", "ur\u00b7alt", "e\u00b7wi\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wagend der Mensch.", "tokens": ["Wa\u00b7gend", "der", "Mensch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Hat er aus Funken", "tokens": ["Hat", "er", "aus", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Des Gottheitlichtes", "tokens": ["Des", "Got\u00b7theit\u00b7lich\u00b7tes"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sich neu geschaffen", "tokens": ["Sich", "neu", "ge\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ADJD", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Seine Welt:", "tokens": ["Sei\u00b7ne", "Welt", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Leben nun mu\u00df sie!", "tokens": ["Le\u00b7ben", "nun", "mu\u00df", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ob Tausende fallen,", "tokens": ["Ob", "Tau\u00b7sen\u00b7de", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sie kann nur wachsen,", "tokens": ["Sie", "kann", "nur", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Nicht untergehn.", "tokens": ["Nicht", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Geschlechter welken,", "tokens": ["Ge\u00b7schlech\u00b7ter", "wel\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und w\u00e4hnten trunken", "tokens": ["Und", "w\u00e4hn\u00b7ten", "trun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Im reichsten Lichte", "tokens": ["Im", "reichs\u00b7ten", "Lich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gewandelt zu sein,", "tokens": ["Ge\u00b7wan\u00b7delt", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und neue Geschlechter", "tokens": ["Und", "neu\u00b7e", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sehn zu den V\u00e4tern", "tokens": ["Sehn", "zu", "den", "V\u00e4\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wie in die D\u00e4mmrung", "tokens": [