{"dta.poem.8050": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Graf Eberhards Wei\u00dfdorn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Graf Eberhard im Bart", "tokens": ["Graf", "E\u00b7ber\u00b7hard", "im", "Bart"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vom W\u00fcrtemberger Land,", "tokens": ["Vom", "W\u00fcr\u00b7tem\u00b7ber\u00b7ger", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er\u0307 kam auf frommer Fahrt", "tokens": ["E\u1e59", "kam", "auf", "from\u00b7mer", "Fahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu Pal\u00e4stina\u2019s Strand.", "tokens": ["Zu", "Pa\u00b7l\u00e4s\u00b7ti\u00b7na's", "Strand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Daselbst er einsmals ritt", "tokens": ["Da\u00b7selbst", "er", "eins\u00b7mals", "ritt"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch einen frischen Wald.", "tokens": ["Durch", "ei\u00b7nen", "fri\u00b7schen", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein gr\u00fcnes Reis er schnitt", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Reis", "er", "schnitt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von einem Wei\u00dfdorn bald.", "tokens": ["Von", "ei\u00b7nem", "Wei\u00df\u00b7dorn", "bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er steckt\u2019 es mit Bedacht", "tokens": ["Er", "steckt'", "es", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf seinen Eisenhut.", "tokens": ["Auf", "sei\u00b7nen", "Ei\u00b7sen\u00b7hut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er trug es in der Schlacht", "tokens": ["Er", "trug", "es", "in", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und \u00fcber Meeres Flut.", "tokens": ["Und", "\u00fc\u00b7ber", "Mee\u00b7res", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als er war daheim,", "tokens": ["Und", "als", "er", "war", "da\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er\u2019s in die Erde steckt,", "tokens": ["Er's", "in", "die", "Er\u00b7de", "steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo bald manch neuen Keim", "tokens": ["Wo", "bald", "manch", "neu\u00b7en", "Keim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der milde Fr\u00fchling weckt.", "tokens": ["Der", "mil\u00b7de", "Fr\u00fch\u00b7ling", "weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Graf, getreu und gut,", "tokens": ["Der", "Graf", ",", "ge\u00b7treu", "und", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Besucht\u2019 es jedes Jahr,", "tokens": ["Be\u00b7sucht'", "es", "je\u00b7des", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erfreute dran den Muth,", "tokens": ["Er\u00b7freu\u00b7te", "dran", "den", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie es gewachsen war.", "tokens": ["Wie", "es", "ge\u00b7wach\u00b7sen", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Herr war alt und la\u00df,", "tokens": ["Der", "Herr", "war", "alt", "und", "la\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVIMP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Reislein war ein Baum,", "tokens": ["Das", "Reis\u00b7lein", "war", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darunter oftmals sa\u00df", "tokens": ["Da\u00b7run\u00b7ter", "oft\u00b7mals", "sa\u00df"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Greis in tiefem Traum.", "tokens": ["Der", "Greis", "in", "tie\u00b7fem", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die W\u00f6lbung, hoch und breit,", "tokens": ["Die", "W\u00f6l\u00b7bung", ",", "hoch", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit sanftem Rauschen mahnt", "tokens": ["Mit", "sanf\u00b7tem", "Rau\u00b7schen", "mahnt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn an die alte Zeit", "tokens": ["Ihn", "an", "die", "al\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und an das ferne Land.", "tokens": ["Und", "an", "das", "fer\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}