{"dta.poem.8811": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Als spiegelhaltende Sclavin gewahre die ganze Welt!", "tokens": ["Als", "spie\u00b7gel\u00b7hal\u00b7ten\u00b7de", "Scla\u00b7vin", "ge\u00b7wah\u00b7re", "die", "gan\u00b7ze", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich such' um deine Gestalt her den Schatten des ew'gen", "tokens": ["Ich", "such'", "um", "dei\u00b7ne", "Ge\u00b7stalt", "her", "den", "Schat\u00b7ten", "des", "ew'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APZR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Segler, suchend was nicht ist, umfahre die ganze", "tokens": ["Der", "Seg\u00b7ler", ",", "su\u00b7chend", "was", "nicht", "ist", ",", "um\u00b7fah\u00b7re", "die", "gan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PWS", "PTKNEG", "VAFIN", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was t\u00e4uschen Jene so tief sich? Enth\u00fcllte nur mir allein", "tokens": ["Was", "t\u00e4u\u00b7schen", "Je\u00b7ne", "so", "tief", "sich", "?", "Ent\u00b7h\u00fcll\u00b7te", "nur", "mir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PDS", "ADV", "ADJD", "PRF", "$.", "VVFIN", "ADV", "PPER", "ADV"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Dein r\u00e4thselbannendes Antlitz die wahre, die ganze", "tokens": ["Dein", "r\u00e4th\u00b7sel\u00b7ban\u00b7nen\u00b7des", "Ant\u00b7litz", "die", "wah\u00b7re", ",", "die", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "$,", "ART", "ADJA"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der Sofi geisele wund sich, mich ritze die Rose blos,", "tokens": ["Der", "So\u00b7fi", "gei\u00b7se\u00b7le", "wund", "sich", ",", "mich", "rit\u00b7ze", "die", "Ro\u00b7se", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PRF", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Er scheid' und trenne was eins ist, ich paare die ganze", "tokens": ["Er", "scheid'", "und", "tren\u00b7ne", "was", "eins", "ist", ",", "ich", "paa\u00b7re", "die", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PWS", "PIS", "VAFIN", "$,", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+--++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und was ich thue, verdank' ich dem Meister im Ost", "tokens": ["Und", "was", "ich", "thue", ",", "ver\u00b7dank'", "ich", "dem", "Meis\u00b7ter", "im", "Ost"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Da\u00df ich dir huldige, ", "tokens": ["Da\u00df", "ich", "dir", "hul\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}