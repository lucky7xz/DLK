{"textgrid.poem.58528": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "566. Herrliche Fr\u00fcchte der Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Kreuzesbaum, voll sch\u00f6ner Fr\u00fcchte,", "tokens": ["O", "Kreu\u00b7zes\u00b7baum", ",", "voll", "sch\u00f6\u00b7ner", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der so begierlich im Gesichte", "tokens": ["Der", "so", "be\u00b7gier\u00b7lich", "im", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und herzerquickend im Genie\u00df", "tokens": ["Und", "herz\u00b7er\u00b7qui\u00b7ckend", "im", "Ge\u00b7nie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weit mehr als der im Paradies,", "tokens": ["Weit", "mehr", "als", "der", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "KOKOM", "ART", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wodurch sich Eva lie\u00df betr\u00fcgen", "tokens": ["Wo\u00b7durch", "sich", "E\u00b7va", "lie\u00df", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch ein ert\u00f6tend Scheinvergn\u00fcgen!", "tokens": ["Durch", "ein", "er\u00b7t\u00f6\u00b7tend", "Schein\u00b7ver\u0