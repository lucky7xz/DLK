{"dta.poem.5865": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sind der Meinungen, so mancherley, da\u00df man", "tokens": ["Es", "sind", "der", "Mei\u00b7nun\u00b7gen", ",", "so", "man\u00b7cher\u00b7ley", ",", "da\u00df", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "PIS", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieselbigen nicht z\u00e4hlen kan.", "tokens": ["Die\u00b7sel\u00b7bi\u00b7gen", "nicht", "z\u00e4h\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Jedweden will sein Witz durch andre Wege f\u00fchren,", "tokens": ["Jed\u00b7we\u00b7den", "will", "sein", "Witz", "durch", "and\u00b7re", "We\u00b7ge", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein jeder will die Welt nach seinem Kopff formiren.", "tokens": ["Ein", "je\u00b7der", "will", "die", "Welt", "nach", "sei\u00b7nem", "Kopff", "for\u00b7mi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}