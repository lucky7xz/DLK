{"textgrid.poem.38107": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Springel- oder Lange-Tanz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dat geit hir gegen den Sommer, gegen de leve Sommertidt,", "tokens": ["Dat", "geit", "hir", "ge\u00b7gen", "den", "Som\u00b7mer", ",", "ge\u00b7gen", "de", "le\u00b7ve", "Som\u00b7mer\u00b7tidt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "De Kinderken gahn spehlen an dem Dahl, dat sprack en Wyff.", "tokens": ["De", "Kin\u00b7der\u00b7ken", "gahn", "speh\u00b7len", "an", "dem", "Dahl", ",", "dat", "sprack", "en", "Wyff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "\u00bbach M\u00f6nnecken min leve Moder, moste ick aldar tom Aventanz gahn", "tokens": ["\u00bb", "ach", "M\u00f6n\u00b7ne\u00b7cken", "min", "le\u00b7ve", "Mo\u00b7der", ",", "mos\u00b7te", "ick", "al\u00b7dar", "tom", "A\u00b7ven\u00b7tanz", "gahn"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "XY", "NN", "PPOSAT", "ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+----+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Dar ick h\u00f6r de Pipen gahn und de leven Trummel schlan!\u00ab", "tokens": ["Dar", "ick", "h\u00f6r