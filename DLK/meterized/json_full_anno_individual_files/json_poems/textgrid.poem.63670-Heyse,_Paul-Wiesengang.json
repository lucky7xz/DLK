{"textgrid.poem.63670": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Wiesengang", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der letzte Grummetwagen ist herein,", "tokens": ["Der", "letz\u00b7te", "Grum\u00b7met\u00b7wa\u00b7gen", "ist", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rund sind die Wiesen leer; das Reich ist mein;", "tokens": ["Rund", "sind", "die", "Wie\u00b7sen", "leer", ";", "das", "Reich", "ist", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJD", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Kein \u00c4hrenfeld, das mir verschlossen bleibt,", "tokens": ["Kein", "\u00c4h\u00b7ren\u00b7feld", ",", "das", "mir", "ver\u00b7schlos\u00b7sen", "bleibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur Stoppeln, \u00fcber die der Ostwind st\u00e4ubt.", "tokens": ["Nur", "Stop\u00b7peln", ",", "\u00fc\u00b7ber", "die", "der", "Ost\u00b7wind", "st\u00e4ubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wandre, wandre. In erschrocknem Lauf", "tokens": ["Ich", "wand\u00b7re", ",", "wand\u00b7re", ".", "In", "er\u00b7schrock\u00b7nem", "Lauf"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Springt dann und wann ein H\u00e4schen vor mir auf.", "tokens": ["Springt", "dann", "und", "wann", "ein", "H\u00e4\u00b7schen", "vor", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "PWAV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die scheue Feldmaus schl\u00fcpft behend ins Loch,", "tokens": ["Die", "scheu\u00b7e", "Feld\u00b7maus", "schl\u00fcpft", "be\u00b7hend", "ins", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur ihres Schw\u00e4nzleins Spitze seh' ich noch.", "tokens": ["Nur", "ih\u00b7res", "Schw\u00e4nz\u00b7leins", "Spit\u00b7ze", "seh'", "ich", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zeitlosen ringsumher. Ihr bleiches Rot", "tokens": ["Zeit\u00b7lo\u00b7sen", "rings\u00b7um\u00b7her", ".", "Ihr", "blei\u00b7ches", "Rot"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "L\u00fcgt nur das Leben; doch ihr Saft bringt Tod.", "tokens": ["L\u00fcgt", "nur", "das", "Le\u00b7ben", ";", "doch", "ihr", "Saft", "bringt", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "ADV", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nichts Farbenfrohes, keiner Blume Spur;", "tokens": ["Nichts", "Far\u00b7ben\u00b7fro\u00b7hes", ",", "kei\u00b7ner", "Blu\u00b7me", "Spur", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "$,", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Winterschlaf anschickt sich die Natur,", "tokens": ["Zum", "Win\u00b7ter\u00b7schlaf", "an\u00b7schickt", "sich", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie sich ein sch\u00f6nes Weib am Abend sp\u00e4t", "tokens": ["Wie", "sich", "ein", "sch\u00f6\u00b7nes", "Weib", "am", "A\u00b7bend", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Schmucks entkleidet, eh' sie schlafen geht.", "tokens": ["Des", "Schmucks", "ent\u00b7klei\u00b7det", ",", "eh'", "sie", "schla\u00b7fen", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch wer sie liebt, der findet tausendmal", "tokens": ["Doch", "wer", "sie", "liebt", ",", "der", "fin\u00b7det", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie holder so, als in des Festes Saal.", "tokens": ["Sie", "hol\u00b7der", "so", ",", "als", "in", "des", "Fes\u00b7tes", "Saal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Geborgen vor der Schmeichler \u00f6dem Schwarm", "tokens": ["Ge\u00b7bor\u00b7gen", "vor", "der", "Schmeich\u00b7ler", "\u00f6\u00b7dem", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4lt er sie jetzt am Busen, liebewarm,", "tokens": ["H\u00e4lt", "er", "sie", "jetzt", "am", "Bu\u00b7sen", ",", "lie\u00b7be\u00b7warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Entz\u00fcckt von ihres Auges letztem Gru\u00df,", "tokens": ["Ent\u00b7z\u00fcckt", "von", "ih\u00b7res", "Au\u00b7ges", "letz\u00b7tem", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den vorm Entschlummern sie ihm g\u00f6nnen mu\u00df,", "tokens": ["Den", "vorm", "Ent\u00b7schlum\u00b7mern", "sie", "ihm", "g\u00f6n\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wie dort der Sonne letzter Schimmer m\u0