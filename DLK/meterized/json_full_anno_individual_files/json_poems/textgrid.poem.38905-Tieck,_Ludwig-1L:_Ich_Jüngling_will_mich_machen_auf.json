{"textgrid.poem.38905": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich J\u00fcngling will mich machen auf", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich J\u00fcngling will mich machen auf", "tokens": ["Ich", "J\u00fcng\u00b7ling", "will", "mich", "ma\u00b7chen", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VMFIN", "PRF", "VVINF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gehn durch die bunte Welt dahin,", "tokens": ["Und", "gehn", "durch", "die", "bun\u00b7te", "Welt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es bringt der mannichfaltge Lauf", "tokens": ["Es", "bringt", "der", "man\u00b7nich\u00b7falt\u00b7ge", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir wundersame Bilder in 'n Sinn.", "tokens": ["Mir", "wun\u00b7der\u00b7sa\u00b7me", "Bil\u00b7der", "in", "'n", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohin? Wohin?", "tokens": ["Wo\u00b7hin", "?", "Wo\u00b7hin", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Freiheit ist mein erster Gewinn.", "tokens": ["Die", "Frei\u00b7heit", "ist", "mein", "ers\u00b7ter", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Wohlauf! die Stadt liegt hinter mir,", "tokens": ["Wohl\u00b7auf", "!", "die", "Stadt", "liegt", "hin\u00b7ter", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor mir liegt Wald und Bach,", "tokens": ["Vor", "mir", "liegt", "Wald", "und", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wandle fort in dem Lust-Revier,", "tokens": ["Ich", "wand\u00b7le", "fort", "in", "dem", "Lust\u00b7Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein' Sorge wandelt mir nach; \u2013", "tokens": ["Kein'", "Sor\u00b7ge", "wan\u00b7delt", "mir", "nach", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Doch ach! doch ach!", "tokens": ["Doch", "ach", "!", "doch", "ach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Was wird im innersten Busen mir wach?", "tokens": ["Was", "wird", "im", "in\u00b7ners\u00b7ten", "Bu\u00b7sen", "mir", "wach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Was willst du Wald? du Blume von mir?", "tokens": ["Was", "willst", "du", "Wald", "?", "du", "Blu\u00b7me", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "$.", "PPER", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bin ich dir schon bekannt?", "tokens": ["Bin", "ich", "dir", "schon", "be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vertraulich thut ihr und freundlich hier,", "tokens": ["Ver\u00b7trau\u00b7lich", "thut", "ihr", "und", "freund\u00b7lich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr seid mir fremdes Land,", "tokens": ["Ihr", "seid", "mir", "frem\u00b7des", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So abgewandt,", "tokens": ["So", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ihr seid mir nie als Freunde genannt.", "tokens": ["Ihr", "seid", "mir", "nie", "als", "Freun\u00b7de", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Und doch sind wir Freund', und doch deine Freund',", "tokens": ["Und", "doch", "sind", "wir", "Freund'", ",", "und", "doch", "dei\u00b7ne", "Freund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$,", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erinnre dich nur recht tief in der Brust,", "tokens": ["E\u00b7rinn\u00b7re", "dich", "nur", "recht", "tief", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie wir uralte Bekannte seind,", "tokens": ["Wie", "wir", "ur\u00b7al\u00b7te", "Be\u00b7kann\u00b7te", "seind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der Nahmen unser dir wohl bewu\u00dft,", "tokens": ["Der", "Nah\u00b7men", "un\u00b7ser", "dir", "wohl", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "S\u00fc\u00df-Lust, S\u00fc\u00df-Lust,", "tokens": ["S\u00fc\u00df\u00b7Lust", ",", "S\u00fc\u00df\u00b7Lust", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Du hast uns endlich folgen gewu\u00dft.", "tokens": ["Du", "hast", "uns", "end\u00b7lich", "fol\u00b7gen", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Heraus dein Sehnen dich trieb an's Frey,", "tokens": ["He\u00b7raus", "dein", "Seh\u00b7nen", "dich", "trieb", "an's", "Frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sonst sa\u00dfest verschlossen in dir,", "tokens": ["Sonst", "sa\u00b7\u00dfest", "ver\u00b7schlos\u00b7sen", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du dachtest wohl nicht, wie herrlich der May,", "tokens": ["Du", "dach\u00b7test", "wohl", "nicht", ",", "wie", "herr\u00b7lich", "der", "May", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir lockten, du wandelst nun hier,", "tokens": ["Wir", "lock\u00b7ten", ",", "du", "wan\u00b7delst", "nun", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und f\u00fcr und f\u00fcr", "tokens": ["Und", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "KON", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sind Br\u00fcder und Freunde so du wie wir.", "tokens": ["Sind", "Br\u00fc\u00b7der", "und", "Freun\u00b7de", "so", "du", "wie", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "PPER", "KOKOM", "PPER", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "So hab' ich die Freiheit nur darum gesucht,", "tokens": ["So", "hab'", "ich", "die", "Frei\u00b7heit", "nur", "da\u00b7rum", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um euer armer Knecht zu seyn,", "tokens": ["Um", "eu\u00b7er", "ar\u00b7mer", "Knecht", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel lieber begeb' ich mich gleich auf die Flucht", "tokens": ["Viel", "lie\u00b7ber", "be\u00b7geb'", "ich", "mich", "gleich", "auf", "die", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und kehr' in das alte Hausdunkel hinein,", "tokens": ["Und", "kehr'", "in", "das", "al\u00b7te", "Haus\u00b7dun\u00b7kel", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So Blum' wie Hain,", "tokens": ["So", "Blum'", "wie", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sie herrschen schon m\u00e4chtig die Seele mein.", "tokens": ["Sie", "herr\u00b7schen", "schon", "m\u00e4ch\u00b7tig", "die", "See\u00b7le", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "PPOSAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Was wollt ihr gaukelnde Farben s\u00fc\u00df,", "tokens": ["Was", "wollt", "ihr", "gau\u00b7keln\u00b7de", "Far\u00b7ben", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was sprichst du lockender Vogelgesang?", "tokens": ["Was", "sprichst", "du", "lo\u00b7cken\u00b7der", "Vo\u00b7gel\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Farben und Lieder sie zaubern gewi\u00df,", "tokens": ["Die", "Far\u00b7ben", "und", "Lie\u00b7der", "sie", "zau\u00b7bern", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Schon f\u00fchl' ich das Herz im Busen so bang,", "tokens": ["Schon", "f\u00fchl'", "ich", "das", "Herz", "im", "Bu\u00b7sen", "so", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie lang, wie lang,", "tokens": ["Wie", "lang", ",", "wie", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ertrag' ich in mir den entz\u00fcckenden Klang.", "tokens": ["Er\u00b7trag'", "ich", "in", "mir", "den", "ent\u00b7z\u00fc\u00b7cken\u00b7den", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Kommt Geister aus eurem Hinterhalt", "tokens": ["Kommt", "Geis\u00b7ter", "aus", "eu\u00b7rem", "Hin\u00b7ter\u00b7halt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und zeigt mir ein redlich Gesicht,", "tokens": ["Und", "zeigt", "mir", "ein", "red\u00b7lich", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Entsteiget den Bergen, verlasset den Wald,", "tokens": ["Ent\u00b7stei\u00b7get", "den", "Ber\u00b7gen", ",", "ver\u00b7las\u00b7set", "den", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und wagt euch hervor an Tageslicht!", "tokens": ["Und", "wagt", "euch", "her\u00b7vor", "an", "Ta\u00b7ges\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo nicht, wo nicht,", "tokens": ["Wo", "nicht", ",", "wo", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PWAV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich wieder zur\u00fcck in das Hausdunkel fl\u00fccht'!", "tokens": ["Ich", "wie\u00b7der", "zu\u00b7r\u00fcck", "in", "das", "Haus\u00b7dun\u00b7kel", "fl\u00fccht'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Nicht kannst du wollen den Freunden entfliehn,", "tokens": ["Nicht", "kannst", "du", "wol\u00b7len", "den", "Freun\u00b7den", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie magst du in's Dunkel zur\u00fcck?", "tokens": ["Wie", "magst", "du", "in's", "Dun\u00b7kel", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir k\u00f6nnen uns nicht aus den Blumen ziehn,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "uns", "nicht", "aus", "den", "Blu\u00b7men", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zeigen dem irdischen Blick,", "tokens": ["Und", "zei\u00b7gen", "dem", "ir\u00b7di\u00b7schen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Dein Gl\u00fcck, dein Gl\u00fcck", "tokens": ["Dein", "Gl\u00fcck", ",", "dein", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+", "mea