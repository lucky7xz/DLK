{"dta.poem.11133": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen ungelehrten doctor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer deckt ein sammtner peltz den h\u00f6ltzernen verstand,", "tokens": ["Hjer", "deckt", "ein", "sammt\u00b7ner", "peltz", "den", "h\u00f6lt\u00b7zer\u00b7nen", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einem geistlichen ward er zu schlecht erkannt;", "tokens": ["Zu", "ei\u00b7nem", "geist\u00b7li\u00b7chen", "ward", "er", "zu", "schlecht", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VAFIN", "PPER", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nichts nutze war er auch in der juristen orden;", "tokens": ["Nichts", "nut\u00b7ze", "war", "er", "auch", "in", "der", "ju\u00b7ris\u00b7ten", "or\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus desperation ist er ein doctor worden.", "tokens": ["Aus", "des\u00b7pe\u00b7ra\u00b7ti\u00b7on", "ist", "er", "ein", "doc\u00b7tor", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "PPER", "ART", "NN", "VAPP", "$."], "meter": "---+-++--+-+-", "measure": "iambic.penta.relaxed"}}}}}