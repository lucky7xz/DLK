{"textgrid.poem.43935": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[hat jemahls Furcht und Scham, du ungemeines Kind]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat jemahls Furcht und Scham, du ungemeines Kind,", "tokens": ["Hat", "je\u00b7mahls", "Furcht", "und", "Scham", ",", "du", "un\u00b7ge\u00b7mei\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem niemand an Verstand und Sch\u00f6nheit abgewinnt,", "tokens": ["Dem", "nie\u00b7mand", "an", "Ver\u00b7stand", "und", "Sch\u00f6n\u00b7heit", "ab\u00b7ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den angesezten Kiel mir in der Hand verr\u00fccket,", "tokens": ["Den", "an\u00b7ge\u00b7sez\u00b7ten", "Kiel", "mir", "in", "der", "Hand", "ver\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist es warlich wohl auf diesen Tag geschehn,", "tokens": ["So", "ist", "es", "war\u00b7lich", "wohl", "auf", "die\u00b7sen", "Tag", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An dem, weil ich nunmehr dein Antliz recht gesehn,", "tokens": ["An", "dem", ",", "weil", "ich", "nun\u00b7mehr", "dein", "Ant\u00b7liz", "recht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die k\u00fchne Feder sich zu deinem Lobe schicket.", "tokens": ["Die", "k\u00fch\u00b7ne", "Fe\u00b7der", "sich", "zu", "dei\u00b7nem", "Lo\u00b7be", "schi\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwar geb ich gerne zu, da\u00df keines Dichters Flei\u00df", "tokens": ["Zwar", "geb", "ich", "ger\u00b7ne", "zu", ",", "da\u00df", "kei\u00b7nes", "Dich\u00b7ters", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein seltenes Verdienst recht abzuschildern weis", "tokens": ["Dein", "sel\u00b7te\u00b7nes", "Ver\u00b7dienst", "recht", "ab\u00b7zu\u00b7schil\u00b7dern", "weis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVIZU", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und da\u00df dein Conterfey die Mahlerkunst besch\u00e4me;", "tokens": ["Und", "da\u00df", "dein", "Con\u00b7ter\u00b7fey", "die", "Mah\u00b7ler\u00b7kunst", "be\u00b7sch\u00e4\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jedennoch weil ich jezt von deiner Gunst den Geist,", "tokens": ["Je\u00b7den\u00b7noch", "weil", "ich", "jezt", "von", "dei\u00b7ner", "Gunst", "den", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von deiner G\u00fctigkeit so Farb als Pinsel nehme,", "tokens": ["Von", "dei\u00b7ner", "G\u00fc\u00b7tig\u00b7keit", "so", "Farb", "als", "Pin\u00b7sel", "neh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So waget meine Faust dies, was unm\u00f6glich heist,", "tokens": ["So", "wa\u00b7get", "mei\u00b7ne", "Faust", "dies", ",", "was", "un\u00b7m\u00f6g\u00b7lich", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PDS", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und ist noch ungewis, ob, wenn ich dich besinge,", "tokens": ["Und", "ist", "noch", "un\u00b7ge\u00b7wis", ",", "ob", ",", "wenn", "ich", "dich", "be\u00b7sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dies Unternehmen mir Ruhm oder Schande bringe.", "tokens": ["Dies", "Un\u00b7ter\u00b7neh\u00b7men", "mir", "Ruhm", "o\u00b7der", "Schan\u00b7de", "brin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Sorgfalt der Natur berief auf einen Tag,", "tokens": ["Die", "Sorg\u00b7falt", "der", "Na\u00b7tur", "be\u00b7rief", "auf", "ei\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem der Aeolus in seiner Grotte lag", "tokens": ["An", "dem", "der", "A\u00b7e\u00b7o\u00b7lus", "in", "sei\u00b7ner", "Grot\u00b7te", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und das gefangne Heer bis auf den West verwahrte,", "tokens": ["Und", "das", "ge\u00b7fang\u00b7ne", "Heer", "bis", "auf", "den", "West", "ver\u00b7wahr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Schaar der Tugenden in ihren F\u00fcrstensaal,", "tokens": ["Die", "Schaar", "der", "Tu\u00b7gen\u00b7den", "in", "ih\u00b7ren", "F\u00fcrs\u00b7ten\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem auch das Louvre nicht den Ruhm des Vorzugs stahl,", "tokens": ["Dem", "auch", "das", "Louv\u00b7re", "nicht", "den", "Ruhm", "des", "Vor\u00b7zugs", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil Kunst und Werth an ihm nicht die Verschwendung sparte.", "tokens": ["Weil", "Kunst", "und", "Werth", "an", "ihm", "nicht", "die", "Ver\u00b7schwen\u00b7dung", "spar\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ihr Wort war ein Gesez, ihr Wincken ein Befehl", "tokens": ["Ihr", "Wort", "war", "ein", "Ge\u00b7sez", ",", "ihr", "Win\u00b7cken", "ein", "Be\u00b7fehl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "