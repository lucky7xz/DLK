{"textgrid.poem.60153": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von uns herab gew\u00fcnschet, kommt mit Glanze", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von uns herab gew\u00fcnschet, kommt mit Glanze", "tokens": ["Von", "uns", "her\u00b7ab", "ge\u00b7w\u00fcn\u00b7schet", ",", "kommt", "mit", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bekleidet, festlich dieser Tag daher:", "tokens": ["Be\u00b7klei\u00b7det", ",", "fest\u00b7lich", "die\u00b7ser", "Tag", "da\u00b7her", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "PDAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im sp\u00e4ten Herbst mit frischem Blumen-Cranze", "tokens": ["Im", "sp\u00e4\u00b7ten", "Herbst", "mit", "fri\u00b7schem", "Blu\u00b7men\u00b7Cran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch ausgeschm\u00fcckt ist er.", "tokens": ["Noch", "aus\u00b7ge\u00b7schm\u00fcckt", "ist", "er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihm jubelt alles Volk im frohen Geiste", "tokens": ["Ihm", "ju\u00b7belt", "al\u00b7les", "Volk", "im", "fro\u00b7hen", "Geis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vereiniget aus einem Munde zu:", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7get", "aus", "ei\u00b7nem", "Mun\u00b7de", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie lebt, wir w\u00e4ren ohne Sie Verwayste.", "tokens": ["Sie", "lebt", ",", "wir", "w\u00e4\u00b7ren", "oh\u00b7ne", "Sie", "Ver\u00b7ways\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir sehn noch ohne Ruh", "tokens": ["Wir", "sehn", "noch", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den K\u00f6nig in dem Kriegesfelde liegen,", "tokens": ["Den", "K\u00f6\u00b7nig", "in", "dem", "Krie\u00b7ges\u00b7fel\u00b7de", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihn bald zur\u00fccke rufen Sie und wir;", "tokens": ["Ihn", "bald", "zu\u00b7r\u00fc\u00b7cke", "ru\u00b7fen", "Sie", "und", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er aber bleibt nach Schlachten und nach Siegen", "tokens": ["Er", "a\u00b7ber", "bleibt", "nach", "Schlach\u00b7ten", "und", "nach", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch immer fern von Ihr.", "tokens": ["Noch", "im\u00b7mer", "fern", "von", "Ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch ferne von dem Bruder, von der Schwester,", "tokens": ["Noch", "fer\u00b7ne", "von", "dem", "Bru\u00b7der", ",", "von", "der", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weit von Preussens drittem Friederich.", "tokens": ["Und", "weit", "von", "Preus\u00b7sens", "drit\u00b7tem", "Frie\u00b7de\u00b7rich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So hebt aus dem Erhabensten der Nester", "tokens": ["So", "hebt", "aus", "dem", "Er\u00b7ha\u00b7bens\u00b7ten", "der", "Nes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Adler zornig sich,", "tokens": ["Ein", "Ad\u00b7ler", "zor\u00b7nig", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und k\u00e4mpfet m\u00e4chtig in der Luft mit dreyen,", "tokens": ["Und", "k\u00e4mp\u00b7fet", "m\u00e4ch\u00b7tig", "in", "der", "Luft", "mit", "drey\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die neidisch wieder ihn verschworen sind,", "tokens": ["Die", "nei\u00b7disch", "wie\u00b7der", "ihn", "ver\u00b7schwo\u00b7ren", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und kommt nicht wieder, wenn die Jungen schreyen,", "tokens": ["Und", "kommt", "nicht", "wie\u00b7der", ",", "wenn", "die", "Jun\u00b7gen", "schre\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Bis er den Streit gewinnt.", "tokens": ["Bis", "er", "den", "Streit", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dem Held glich Hercul nicht, nicht Alexander.", "tokens": ["Dem", "Held", "glich", "Her\u00b7cul", "nicht", ",", "nicht", "A\u00b7lex\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "PTKNEG", "$,", "PTKNEG", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald mit den K\u00f6pfen unter sich gekehrt,", "tokens": ["Bald", "mit", "den", "K\u00f6p\u00b7fen", "un\u00b7ter", "sich", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "St\u00fcrzt Er verbundne Adler aus einander,", "tokens": ["St\u00fcrzt", "Er", "ver\u00b7bund\u00b7ne", "Ad\u00b7ler", "aus", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerhauen durch sein Schwerdt.", "tokens": ["Zer\u00b7hau\u00b7en", "durch", "sein", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sties nicht sein Arm einst mitten in dem Lande", "tokens": ["Sties", "nicht", "sein", "Arm", "einst", "mit\u00b7ten", "in", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An des zwok\u00f6pfigt starken Adlers Klau?", "tokens": ["An", "des", "zwo\u00b7k\u00f6p\u00b7figt", "star\u00b7ken", "Ad\u00b7lers", "Klau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er hieb sie ab. Der Adler flog mit Schande", "tokens": ["Er", "hieb", "sie", "ab", ".", "Der", "Ad\u00b7ler", "flog", "mit", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und blutig zu der Frau,", "tokens": ["Und", "blu\u00b7tig", "zu", "der", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die, mit drey Cronen pr\u00e4chtig ausgezieret,", "tokens": ["Die", ",", "mit", "drey", "Cro\u00b7nen", "pr\u00e4ch\u00b7tig", "aus\u00b7ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "CARD", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herscht, ihres grossen Geistes voll,", "tokens": ["Herscht", ",", "ih\u00b7res", "gros\u00b7sen", "Geis\u00b7tes", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und von des Krieges Jammer noch ger\u00fchret", "tokens": ["Und", "von", "des", "Krie\u00b7ges", "Jam\u00b7mer", "noch", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Herzen werden soll.", "tokens": ["Im", "Her\u00b7zen", "wer\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dir aber donnern in die Seele Schlachten;", "tokens": ["Dir", "a\u00b7ber", "don\u00b7nern", "in", "die", "See\u00b7le", "Schlach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Weinen ward dein Auge noch bewegt,", "tokens": ["Zum", "Wei\u00b7nen", "ward", "dein", "Au\u00b7ge", "noch", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn grosse Siege deinem Helden brachten", "tokens": ["Wenn", "gros\u00b7se", "Sie\u00b7ge", "dei\u00b7nem", "Hel\u00b7den", "brach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Lorbeer, den Er tr\u00e4gt.", "tokens": ["Den", "Lor\u00b7beer", ",", "den", "Er", "tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihn bringet, unter viel erstritnen Cr\u00e4nzen,", "tokens": ["Ihn", "brin\u00b7get", ",", "un\u00b7ter", "viel", "er\u00b7strit\u00b7nen", "Cr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der goldne Siegeswagen bald zur\u00fcck;", "tokens": ["Der", "gold\u00b7ne", "Sie\u00b7ges\u00b7wa\u00b7gen", "bald", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mehr als Sonnenblicke wird dir gl\u00e4nzen", "tokens": ["Und", "mehr", "als", "Son\u00b7nen\u00b7bli\u00b7cke", "wird", "dir", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein lang entbehrter Blick.", "tokens": ["Sein", "lang", "ent\u00b7behr\u00b7ter", "Blick", "."], "token_info": ["word", "wo