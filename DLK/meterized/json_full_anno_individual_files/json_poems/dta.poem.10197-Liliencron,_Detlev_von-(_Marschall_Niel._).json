{"dta.poem.10197": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Marschall Niel. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Verrauscht die hei\u00dfe Zeit der Jugendtage,", "tokens": ["Ver\u00b7rauscht", "die", "hei\u00b7\u00dfe", "Zeit", "der", "Ju\u00b7gend\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verklungen Becherklang und wilde Geigen.", "tokens": ["Ver\u00b7klun\u00b7gen", "Be\u00b7cher\u00b7klang", "und", "wil\u00b7de", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich lehrte zeitig Hiobs tiefe Klage:", "tokens": ["Dich", "lehr\u00b7te", "zei\u00b7tig", "Hiobs", "tie\u00b7fe", "Kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Thoren schwatzen und die Klugen schweigen.", "tokens": ["Die", "Tho\u00b7ren", "schwat\u00b7zen", "und", "die", "Klu\u00b7gen", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du legst das Wort vorsichtig auf die Wage,", "tokens": ["Du", "legst", "das", "Wort", "vor\u00b7sich\u00b7tig", "auf", "die", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mu\u00dft der Welt die Heuchelmaske zeigen.", "tokens": ["Und", "mu\u00dft", "der", "Welt", "die", "Heu\u00b7chel\u00b7mas\u00b7ke", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein Fr\u00fchling doch \u2014 ach, eine Wundersage,", "tokens": ["Dein", "Fr\u00fch\u00b7ling", "doch", "ach", ",", "ei\u00b7ne", "Wun\u00b7der\u00b7sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$(", "ITJ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dir singt kein Vogel mehr in gr\u00fcnen Zweigen.", "tokens": ["Dir", "singt", "kein", "Vo\u00b7gel", "mehr", "in", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}