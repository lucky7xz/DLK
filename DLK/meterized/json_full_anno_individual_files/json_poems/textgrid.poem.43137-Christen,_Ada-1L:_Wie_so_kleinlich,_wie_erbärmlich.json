 "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn Ihr k\u00f6nnt es nimmer glauben,", "tokens": ["Denn", "Ihr", "k\u00f6nnt", "es", "nim\u00b7mer", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich meine Ketten brach;", "tokens": ["Da\u00df", "ich", "mei\u00b7ne", "Ket\u00b7ten", "brach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ich zerbrach sie doch! O glaubet,", "tokens": ["Ich", "zer\u00b7brach", "sie", "doch", "!", "O", "glau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meine Selbstverachtung schwand,", "tokens": ["Mei\u00b7ne", "Selbst\u00b7ver\u00b7ach\u00b7tung", "schwand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Als ich Euch so feig, so h\u00fcndisch,", "tokens": ["Als", "ich", "Euch", "so", "feig", ",", "so", "h\u00fcn\u00b7disch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "So verachtungsw\u00fcrdig fand.", "tokens": ["So", "ver\u00b7ach\u00b7tungs\u00b7w\u00fcr\u00b7dig", "fand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", 