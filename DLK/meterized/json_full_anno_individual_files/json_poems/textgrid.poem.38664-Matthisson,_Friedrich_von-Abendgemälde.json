{"textgrid.poem.38664": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Abendgem\u00e4lde", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Birkenlabyrinthe", "tokens": ["Durch", "Bir\u00b7ken\u00b7la\u00b7by\u00b7rin\u00b7the"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Malt abendliche Glut", "tokens": ["Malt", "a\u00b7bend\u00b7li\u00b7che", "Glut"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Mit warmer Zaubertinte", "tokens": ["Mit", "war\u00b7mer", "Zau\u00b7ber\u00b7tin\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Rohrbachs leise Flut;", "tokens": ["Des", "Rohr\u00b7bachs", "lei\u00b7se", "Flut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bepurpurt fliehn die Wellen", "tokens": ["Be\u00b7pur\u00b7purt", "fliehn", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hinab zum Gartenteich,", "tokens": ["Hin\u00b7ab", "zum", "Gar\u00b7ten\u00b7teich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umhegt von Steinkornellen", "tokens": ["Um\u00b7hegt", "von", "Stein\u00b7kor\u00b7nel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und glattem Nu\u00dfgestr\u00e4uch.", "tokens": ["Und", "glat\u00b7tem", "Nu\u00df\u00b7ge\u00b7str\u00e4uch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gebirg und Hain verschmelzen", "tokens": ["Ge\u00b7birg", "und", "Hain", "ver\u00b7schmel\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im r\u00f6tlichen Ged\u00fcft;", "tokens": ["Im", "r\u00f6t\u00b7li\u00b7chen", "Ge\u00b7d\u00fcft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der M\u00fchle Fl\u00fcgel w\u00e4lzen", "tokens": ["Der", "M\u00fch\u00b7le", "Fl\u00fc\u00b7gel", "w\u00e4l\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich an umz\u00e4unter Trift;", "tokens": ["Sich", "an", "um\u00b7z\u00e4un\u00b7ter", "Trift", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus dunkler Fichtengruppe", "tokens": ["Aus", "dunk\u00b7ler", "Fich\u00b7ten\u00b7grup\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Wallt am beschilften Moor", "tokens": ["Wallt", "am", "be\u00b7schilf\u00b7ten", "Moor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In dichtgedr\u00e4ngtem Truppe", "tokens": ["In", "dicht\u00b7ge\u00b7dr\u00e4ng\u00b7tem", "Trup\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das leichte Wild hervor.", "tokens": ["Das", "leich\u00b7te", "Wild", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die alte Ritterveste", "tokens": ["Die", "al\u00b7te", "Rit\u00b7ter\u00b7ves\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hebt k\u00fchn im goldnen Glanz", "tokens": ["Hebt", "k\u00fchn", "im", "gold\u00b7nen", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Thurms bemooste Reste", "tokens": ["Des", "Thurms", "be\u00b7moos\u00b7te", "Res\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus finstrer Ulmen Kranz;", "tokens": ["Aus", "finst\u00b7rer", "Ul\u00b7men", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Matt gl\u00fcht, im bleichern Strale,", "tokens": ["Matt", "gl\u00fcht", ",", "im", "blei\u00b7chern", "Stra\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Eppich halb verh\u00fcllt,", "tokens": ["Von", "Ep\u00b7pich", "halb", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Am gothischen Portale", "tokens": ["Am", "go\u00b7thi\u00b7schen", "Por\u00b7ta\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Der graue Wappenschild.", "tokens": ["Der", "grau\u00b7e", "Wap\u00b7pen\u00b7schild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wann Feyn und Geister walten,", "tokens": ["Wann", "Feyn", "und", "Geis\u00b7ter", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erstehn, wie Nebelduft", "tokens": ["Er\u00b7stehn", ",", "wie", "Ne\u00b7bel\u00b7duft"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Mondlicht, die Gestalten", "tokens": ["Im", "Mond\u00b7licht", ",", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Helden aus der Gruft.", "tokens": ["Der", "Hel\u00b7den", "aus", "der", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Dunstgebilde wallen,", "tokens": ["Die", "Dunst\u00b7ge\u00b7bil\u00b7de", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In d\u00fcstrer Majest\u00e4t,", "tokens": ["In", "d\u00fcst\u00b7rer", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im \u00f6den Raum der Hallen,", "tokens": ["Im", "\u00f6\u00b7den", "Raum", "der", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom hohen Gras umweht.", "tokens": ["Vom", "ho\u00b7hen", "Gras", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fern ob dem blauen Strome,", "tokens": ["Fern", "ob", "dem", "blau\u00b7en", "Stro\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Am Felsen wild und schrof,", "tokens": ["Am", "Fel\u00b7sen", "wild", "und", "schrof", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Winkt, unterm Schattendome", "tokens": ["Winkt", ",", "un\u00b7term", "Schat\u00b7ten\u00b7do\u00b7me"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Eich', ein Fischerhof.", "tokens": ["Der", "Eich'", ",", "ein", "Fi\u00b7scher\u00b7hof", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Quell' entsch\u00e4umt der Klippe,", "tokens": ["Die", "Quell'", "ent\u00b7sch\u00e4umt", "der", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Funken bla\u00df bestreut,", "tokens": ["Mit", "Fun\u00b7ken", "bla\u00df", "be\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom alten Baumgerippe", "tokens": ["Vom", "al\u00b7ten", "Baum\u00b7ge\u00b7rip\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Romantisch \u00fcberdr\u00e4ut.", "tokens": ["Ro\u00b7man\u00b7tisch", "\u00fc\u00b7berd\u00b7r\u00e4ut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Umgrenzt von Hain und Matten,", "tokens": ["Um\u00b7grenzt", "von", "Hain", "und", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Yoriks Meierei,", "tokens": ["Wie", "Y\u00b7o\u00b7riks", "Mei\u00b7e\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Blikt aus Platanenschatten", "tokens": ["Blikt", "aus", "Pla\u00b7ta\u00b7nen\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein l\u00e4ndlich Sorgenfrei.", "tokens": ["Ein", "l\u00e4nd\u00b7lich", "Sor\u00b7gen\u00b7frei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier gr\u00fcnen Thyrsusst\u00e4be", "tokens": ["Hier", "gr\u00fc\u00b7nen", "Thyr\u00b7sus\u00b7st\u00e4\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bey Wies' und Gartenland;", "tokens": ["Bey", "Wies'", "und", "Gar\u00b7ten\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort ringelt ihr Gewebe", "tokens": ["Dort", "rin\u00b7gelt", "ihr", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Bohn' an weisser Wand.", "tokens": ["Die", "Bohn'", "an", "weis\u00b7ser", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Am Fenster gl\u00fcht die Nelke,", "tokens": ["Am", "Fens\u00b7ter", "gl\u00fcht", "die", "Nel\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Rosen schw\u00e4rmt der West;", "tokens": ["Um", "Ro\u00b7sen", "schw\u00e4rmt", "der", "West", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Ruh baut am Geb\u00e4lke", "tokens": ["In", "Ruh", "baut", "am", "Ge\u00b7b\u00e4l\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die treue Schwalb' ihr Nest;", "tokens": ["Die", "treu\u00b7e", "Schwalb'", "ihr", "Nest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dumpf schwirrt am Brunnentroge", "tokens": ["Dumpf", "schwirrt", 