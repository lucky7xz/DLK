{"dta.poem.994": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Gieb ein lied mir wieder               ", "tokens": ["Gieb", "ein", "lied", "mir", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Im klaren tone deiner freudentage &#8212;               ", "tokens": ["Im", "kla\u00b7ren", "to\u00b7ne", "dei\u00b7ner", "freu\u00b7den\u00b7ta\u00b7ge", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "XML_entity"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du weisst es ja: mir wich der friede               ", "tokens": ["Du", "weisst", "es", "ja", ":", "mir", "wich", "der", "frie\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine hand ist zag             ", "tokens": ["Und", "mei\u00b7ne", "hand", "ist", "zag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo dunkle seelen sinnen               ", "tokens": ["Wo", "dunk\u00b7le", "see\u00b7len", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erscheinen bilder seltne hohe               ", "tokens": ["Er\u00b7schei\u00b7nen", "bil\u00b7der", "selt\u00b7ne", "ho\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch fehlt das leuchtende erinnern               ", "tokens": ["Doch", "fehlt", "das", "leuch\u00b7ten\u00b7de", "e\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "VVINF"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die farbe hell und froh             ", "tokens": ["Die", "far\u00b7be", "hell", "und", "froh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo sieche seelen reden               ", "tokens": ["Wo", "sie\u00b7che", "see\u00b7len", "re\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lindern schmeichelhafte t\u00f6ne               ", "tokens": ["Da", "lin\u00b7dern", "schmei\u00b7chel\u00b7haf\u00b7te", "t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist die stimme tief und edel               ", "tokens": ["Da", "ist", "die", "stim\u00b7me", "tief", "und", "e\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nicht zum sang so sch\u00f6n.             ", "tokens": ["Doch", "nicht", "zum", "sang", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}