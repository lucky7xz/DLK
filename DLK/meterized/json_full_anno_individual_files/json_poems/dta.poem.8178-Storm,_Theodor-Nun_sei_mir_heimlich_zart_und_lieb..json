{"dta.poem.8178": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Nun sei mir heimlich zart und lieb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Setz' deinen Fu\u00df auf meinen nun!", "tokens": ["Setz'", "dei\u00b7nen", "Fu\u00df", "auf", "mei\u00b7nen", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir sagt es: ich verlie\u00df die Welt,", "tokens": ["Mir", "sagt", "es", ":", "ich", "ver\u00b7lie\u00df", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um ganz allein auf dir zu ruhn;", "tokens": ["Um", "ganz", "al\u00b7lein", "auf", "dir", "zu", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und dir: o lie\u00dfe mich die Welt,", "tokens": ["Und", "dir", ":", "o", "lie\u00b7\u00dfe", "mich", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "FM", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00f6nnt' ich friedlich und allein,", "tokens": ["Und", "k\u00f6nnt'", "ich", "fried\u00b7lich", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie deines leichten Fu\u00dfes jetzt,", "tokens": ["Wie", "dei\u00b7nes", "leich\u00b7ten", "Fu\u00b7\u00dfes", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So deines Lebens Tr\u00e4ger sein!", "tokens": ["So", "dei\u00b7nes", "Le\u00b7bens", "Tr\u00e4\u00b7ger", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}