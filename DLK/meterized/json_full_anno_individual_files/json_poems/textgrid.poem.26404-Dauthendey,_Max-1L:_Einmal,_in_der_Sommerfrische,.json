{"textgrid.poem.26404": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einmal, in der Sommerfrische,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal, in der Sommerfrische,", "tokens": ["Ein\u00b7mal", ",", "in", "der", "Som\u00b7mer\u00b7fri\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand auf einem Gasthaustische", "tokens": ["Stand", "auf", "ei\u00b7nem", "Gast\u00b7haus\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sch\u00f6n poliert ein Grammophon,", "tokens": ["Sch\u00f6n", "po\u00b7liert", "ein", "Gram\u00b7mo\u00b7phon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses hatte Menschenton.", "tokens": ["Die\u00b7ses", "hat\u00b7te", "Men\u00b7schen\u00b7ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pr\u00e4chtig schrie sein Blechzylinder.", "tokens": ["Pr\u00e4ch\u00b7tig", "schrie", "sein", "Blech\u00b7zy\u00b7lin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solches lockt zuerst die Kinder,", "tokens": ["Sol\u00b7ches", "lockt", "zu\u00b7erst", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch auch Damen ist Geschrei", "tokens": ["Doch", "auch", "Da\u00b7men", "ist", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VAFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht so g\u00e4nzlich Einerlei.", "tokens": ["Nicht", "so", "g\u00e4nz\u00b7lich", "Ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manche stand mit langem Halse", "tokens": ["Man\u00b7che", "stand", "mit", "lan\u00b7gem", "Hal\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Trichter und der Walze.", "tokens": ["An", "dem", "Trich\u00b7ter", "und", "der", "Wal\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn nicht Jeder sieht gleich, wie", "tokens": ["Denn", "nicht", "Je\u00b7der", "sieht", "gleich", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PTKNEG", "PIS", "VVFIN", "ADV", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor sich geht die Melodie.", "tokens": ["Vor", "sich", "geht", "die", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keiner glaubt von diesem Dinge,", "tokens": ["Kei\u00b7ner", "glaubt", "von", "die\u00b7sem", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df es Stimmen fertig bringe.", "tokens": ["Da\u00df", "es", "Stim\u00b7men", "fer\u00b7tig", "brin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand gar vermutet h\u00e4tt',", "tokens": ["Nie\u00b7mand", "gar", "ver\u00b7mu\u00b7tet", "h\u00e4tt'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Dinge ein Quartett.", "tokens": ["In", "dem", "Din\u00b7ge", "ein", "Quar\u00b7tett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ist 'ne Nummer abgelaufen,", "tokens": ["Ist", "'ne", "Num\u00b7mer", "ab\u00b7ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darf man sich 'ne andere kaufen.", "tokens": ["Darf", "man", "sich", "'ne", "an\u00b7de\u00b7re", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und weil es die Walze kann,", "tokens": ["Und", "weil", "es", "die", "Wal\u00b7ze", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kommt auch ein Tenor daran.", "tokens": ["Kommt", "auch", "ein", "Te\u00b7nor", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Der Tenor br\u00fcllt aus dem Trichter,", "tokens": ["Der", "Te\u00b7nor", "br\u00fcllt", "aus", "dem", "Trich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und verz\u00fcckt sind die Gesichter.", "tokens": ["Und", "ver\u00b7z\u00fcckt", "sind", "die", "Ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche Dam' h\u00e4tt's gern heraus,", "tokens": ["Man\u00b7che", "Dam'", "h\u00e4tt's", "gern", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sieht der Tenor wohl aus!", "tokens": ["Wie", "sieht", "der", "Te\u00b7nor", "wohl", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und mein Gott, wer h\u00e4tt's erwartet!", "tokens": ["Und", "mein", "Gott", ",", "wer", "h\u00e4tt's", "er\u00b7war\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schicksale sind abgekartet!", "tokens": ["Schick\u00b7sa\u00b7le", "sind", "ab\u00b7ge\u00b7kar\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Eine Dame \u2013 das kommt vor \u2013", "tokens": ["Ei\u00b7ne", "Da\u00b7me", "\u2013", "das", "kommt", "vor", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird besessen vom Tenor.", "tokens": ["Wird", "be\u00b7ses\u00b7sen", "vom", "Te\u00b7nor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, er singt so unverfroren", "tokens": ["Ach", ",", "er", "singt", "so", "un\u00b7ver\u00b7fro\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich ins Herz ihr und die Ohren.", "tokens": ["Sich", "ins", "Herz", "ihr", "und", "die", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PPER", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Walze, die sich schiebt,", "tokens": ["Aus", "der", "Wal\u00b7ze", ",", "die", "sich", "schiebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singt ein Mann, den's nicht mehr gibt.", "tokens": ["Singt", "ein", "Mann", ",", "den's", "nicht", "mehr", "gibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihn, der einst hineingeschrieen,", "tokens": ["Ihn", ",", "der", "einst", "hin\u00b7ein\u00b7ge\u00b7schri\u00b7een", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' die Dame an sich ziehen;", "tokens": ["M\u00f6cht'", "die", "Da\u00b7me", "an", "sich", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Dam', mit einem Wort,", "tokens": ["Und", "die", "Dam'", ",", "mit", "ei\u00b7nem", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht nicht mehr vom Trichter fort.", "tokens": ["Geht", "nicht", "mehr", "vom", "Trich\u00b7ter", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ach, total tut sie erwarmen,", "tokens": ["Ach", ",", "to\u00b7tal", "tut", "sie", "er\u00b7war\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' den Trichter fest umarmen.", "tokens": ["M\u00f6cht'", "den", "Trich\u00b7ter", "fest", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich kauft sies Grammophon.", "tokens": ["End\u00b7lich", "kauft", "sies", "Gram\u00b7mo\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tt' sie nur was mehr davon!", "tokens": ["H\u00e4tt'", "sie", "nur", "was", "mehr", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "PIS", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber ich darf's nicht verhehlen,", "tokens": ["A\u00b7ber", "ich", "dar\u00b7f's", "nicht", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sie tat nur die Nachbarn qu\u00e4len.", "tokens": ["Sie", "tat", "nur", "die", "Nach\u00b7barn", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kaum kam der Tenor ins Haus,", "tokens": ["Kaum", "kam", "der", "Te\u00b7nor", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Stirbt ein jedes Stockwerk aus.", "tokens": ["Stirbt", "ein", "je\u00b7des", "Stock\u00b7werk", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und auch sie w\u00e4r' dran gestorben,", "tokens": ["Und", "auch", "sie", "w\u00e4r'", "dran", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "W\u00e4rs Geh\u00f6r nicht erst verdorben.", "tokens": ["W\u00e4rs", "Ge\u00b7h\u00f6r", "nicht", "erst", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt ihr's nicht mehr schaden kann,", "tokens": ["Jetzt", "ih\u00b7r's", "nicht", "mehr", "scha\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sie wurde taub daran.", "tokens": ["Denn", "sie", "wur\u00b7de", "taub", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Doch weil sie nicht blind, die Tauben,", "tokens": ["Doch", "weil", "sie", "nicht", "blind", ",", "die", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schraubt sie weiter an der Schrauben,", "tokens": ["Schraubt", "sie", "wei\u00b7ter", "an", "der", "Schrau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schont auch gar nicht den Tenor,", "tokens": ["Schont", "auch", "gar", "nicht", "den", "Te\u00b7nor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Bis er seine Stimm' verlor.", "tokens": ["Bis", "er", "sei\u00b7ne", "Stimm'", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wenn sich auch die Walzen drehen,", "tokens": ["Wenn", "sich", "auch", "die", "Wal\u00b7zen", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kein Tenor tut mehr entstehen;", "tokens": ["Kein", "Te\u00b7nor", "tut", "mehr", "ent\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn das Grammophon, das hat", "tokens": ["Denn", "das", "Gram\u00b7mo\u00b7phon", ",", "das", "hat"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PDS", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich mal die Sache satt.", "tokens": ["End\u00b7lich", "mal", "die", "Sa\u00b7che", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nur die Dam' ist noch vorhanden.", "tokens": ["Nur", "die", "Dam'", "ist", "noch", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "