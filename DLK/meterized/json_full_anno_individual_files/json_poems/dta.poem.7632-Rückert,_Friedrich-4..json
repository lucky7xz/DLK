{"dta.poem.7632": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tage sehen wir, die theuren, gerne schwinden,", "tokens": ["Die", "Ta\u00b7ge", "se\u00b7hen", "wir", ",", "die", "theu\u00b7ren", ",", "ger\u00b7ne", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um etwas theureres herangereift zu finden,", "tokens": ["Um", "et\u00b7was", "theu\u00b7re\u00b7res", "her\u00b7an\u00b7ge\u00b7reift", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein seltenes Gew\u00e4chs, das wir im Garten treiben,", "tokens": ["Ein", "sel\u00b7te\u00b7nes", "Ge\u00b7w\u00e4chs", ",", "das", "wir", "im", "Gar\u00b7ten", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Kind das wir erziehn, ein B\u00fcchlein das wir schreiben.", "tokens": ["Ein", "Kind", "das", "wir", "er\u00b7ziehn", ",", "ein", "B\u00fcch\u00b7lein", "das", "wir", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVINF", "$,", "ART", "NN", "