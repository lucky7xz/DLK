{"textgrid.poem.64382": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "105. Das befreite Deutschland", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der junge Tag, in der wallenden Locken Gold", "tokens": ["Wenn", "der", "jun\u00b7ge", "Tag", ",", "in", "der", "wal\u00b7len\u00b7den", "Lo\u00b7cken", "Gold"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Err\u00f6tend aus Wiege des Meers, zu dem Himmel schaut,", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "aus", "Wie\u00b7ge", "des", "Meers", ",", "zu", "dem", "Him\u00b7mel", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "So erwachen mit ihm die S\u00e4nger des Hains:", "tokens": ["So", "er\u00b7wa\u00b7chen", "mit", "ihm", "die", "S\u00e4n\u00b7ger", "des", "Hains", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie begr\u00fc\u00dfen der Erd' und des Himmels Kind mit Wonnegesang.", "tokens": ["Sie", "be\u00b7gr\u00fc\u00b7\u00dfen", "der", "Erd'", "und", "des", "Him\u00b7mels", "Kind", "mit", "Won\u00b7ne\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "--+--+--+-+-+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Nur der H\u00f6hle Sohn, der die H\u00fcrde bei Nacht belaurt,", "tokens": ["Nur", "der", "H\u00f6h\u00b7le", "Sohn", ",", "der", "die", "H\u00fcr\u00b7de", "bei", "Nacht", "be\u00b7laurt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entfernt sich mit schleichendem Tritt, und es fleugt der Kauz", "tokens": ["Ent\u00b7fernt", "sich", "mit", "schlei\u00b7chen\u00b7dem", "Tritt", ",", "und", "es", "fleugt", "der", "Kauz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Zu verdorrtem Ge\u00e4st. Die Strahlen des Tags", "tokens": ["Zu", "ver\u00b7dorr\u00b7tem", "Ge\u00b7\u00e4st", ".", "Die", "Strah\u00b7len", "des", "Tags"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sind verha\u00dft dem Gez\u00fccht, das im finstern Graun sich Wohlsein ersp\u00e4ht.", "tokens": ["Sind", "ver\u00b7ha\u00dft", "dem", "Ge\u00b7z\u00fccht", ",", "das", "im", "fins\u00b7tern", "Graun", "sich", "Wohl\u00b7sein", "er\u00b7sp\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "PRF", "NN", "VVPP", "$."], "meter": "--+--+--+-+-+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Es erschalle laut, o ihr S\u00e4nger des Vaterlands,", "tokens": ["Es", "er\u00b7schal\u00b7le", "laut", ",", "o", "ihr", "S\u00e4n\u00b7ger", "des", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "FM", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der gl\u00fchende deutsche Gesang aus der vollen Brust!", "tokens": ["Der", "gl\u00fc\u00b7hen\u00b7de", "deut\u00b7sche", "Ge\u00b7sang", "aus", "der", "vol\u00b7len", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Wir sind frei! wir sind frei! \u2013 O Freiheit, erk\u00e4mpft", "tokens": ["Wir", "sind", "frei", "!", "wir", "sind", "frei", "!", "\u2013", "O", "Frei\u00b7heit", ",", "er\u00b7k\u00e4mpft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "ADJD", "$.", "$(", "NE", "NN", "$,", "VVFIN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mit dem Schwerte des Ruhms, wer sich dein nicht freut, von hinnen mit dem!", "tokens": ["Mit", "dem", "Schwer\u00b7te", "des", "Ruhms", ",", "wer", "sich", "dein", "nicht", "freut", ",", "von", "hin\u00b7nen", "mit", "dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PWS", "PRF", "PPOSAT", "PTKNEG", "VVFIN", "$,", "APPR", "ADV", "APPR", "ART", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wir sind frei durchs Schwert! Und durch uns ist Europa frei!", "tokens": ["Wir", "sind", "frei", "durchs", "Schwert", "!", "Und", "durch", "uns", "ist", "Eu\u00b7ro\u00b7pa", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "$.", "KON", "APPR", "PPER", "VAFIN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Es r\u00fchmte des schnellen Erfolgs sich der eitle Feind.", "tokens": ["Es", "r\u00fchm\u00b7te", "des", "schnel\u00b7len", "Er\u00b7folgs", "sich", "der", "eit\u00b7le", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+---+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was im Krieg er in zwanzig Sonnen errang", "tokens": ["Was", "im", "Krieg", "er", "in", "zwan\u00b7zig", "Son\u00b7nen", "er\u00b7rang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "PPER", "APPR", "CARD", "NN", "VVFIN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und erschlich, er verlor's nach erneutem Kampf im achtzehnten Mond!", "tokens": ["Und", "er\u00b7schlich", ",", "er", "ver\u00b7lor's", "nach", "er\u00b7neu\u00b7tem", "Kampf", "im", "acht\u00b7zehn\u00b7ten", "Mond", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "NE", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Was in zwanzig Sonnen errang und erschlich der Feind?", "tokens": ["Was", "in", "zwan\u00b7zig", "Son\u00b7nen", "er\u00b7rang", "und", "er\u00b7schlich", "der", "Feind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "CARD", "NN", "VVFIN", "KON", "ADJD", "ART", "NN", "$."], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O nein! er beschlich uns vorl\u00e4ngst in der Sitten Gift,", "tokens": ["O", "nein", "!", "er", "be\u00b7schlich", "uns", "vor\u00b7l\u00e4ngst", "in", "der", "Sit\u00b7ten", "Gift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "ADJD", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "In der glei\u00dfenden Sprache Schaum; in dem Tand,", "tokens": ["In", "der", "glei\u00b7\u00dfen\u00b7den", "Spra\u00b7che", "Schaum", ";", "in", "dem", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der f\u00fcr Wesen ihm gilt, und auch uns so galt, in schn\u00f6destem Wahn.", "tokens": ["Der", "f\u00fcr", "We\u00b7sen", "ihm", "gilt", ",", "und", "auch", "uns", "so", "galt", ",", "in", "schn\u00f6\u00b7des\u00b7tem", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "$,", "KON", "ADV", "PPER", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-+--+", "measure": "trochaic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir verlie\u00dfen Gott, da verbarg Er sich uns; doch blieb", "tokens": ["Wir", "ver\u00b7lie\u00b7\u00dfen", "Gott", ",", "da", "ver\u00b7barg", "Er", "sich", "uns", ";", "doch", "blieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "PPER", "$.", "ADV", "VVFIN"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sein Zeuge, das Leiden, bei uns, und erweckte uns", "tokens": ["Sein", "Zeu\u00b7ge", ",", "das", "Lei\u00b7den", ",", "bei", "uns", ",", "und", "er\u00b7weck\u00b7te", "uns"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aus dem Schlafe der Schmach, aus dem Todesschlaf!", "tokens": ["Aus", "dem", "Schla\u00b7fe", "der", "Schmach", ",", "aus", "dem", "To\u00b7des\u00b7schlaf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und es kehrte zur\u00fcck die versch\u00e4mte Demut, Glaube mit ihr;", "tokens": ["Und", "es", "kehr\u00b7te", "zu\u00b7r\u00fcck", "die", "ver\u00b7sch\u00e4m\u00b7te", "De\u00b7mut", ",", "Glau\u00b7be", "mit", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$,", "NN", "APPR", "PPOSAT", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Und die holde Hoffnung gef\u00fchrt an der Liebe Hand,", "tokens": ["Und", "die", "hol\u00b7de", "Hoff\u00b7nung", "ge\u00b7f\u00fchrt", "an", "der", "Lie\u00b7be", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Mut wie nur Gott ihn verleiht, durch Vertraun in Ihn.", "tokens": ["Und", "Mut", "wie", "nur", "Gott", "ihn", "ver\u00b7leiht", ",", "durch", "Ver\u00b7traun", "in", "Ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "ADV", "NN", "PPER", "VVFIN", "$,", "APPR", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Da erhuben sich schnell so F\u00fcrsten als Volk", "tokens": ["Da", "er\u00b7hu\u00b7ben", "sich", "schnell", "so", "F\u00fcrs\u00b7ten", "als", "Volk"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ADV", "NN", "KOUS", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "In der St\u00e4rke des Herrn, es ergriffen den Feind die Schrecken des Herrn!", "tokens": ["In", "der", "St\u00e4r\u00b7ke", "des", "Herrn", ",", "es", "er\u00b7grif\u00b7fen", "den", "Feind", "die", "Schre\u00b7cken", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PPER", "VVPP", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+-+--+", "measure": "anapaest.tetra.plus"}}, "stanza.8": {"line.1": {"text": "Du bist frei, o Land der bew\u00e4hrten und festen Treu!", "tokens": ["Du", "bist", "frei", ",", "o", "Land", "der", "be\u00b7w\u00e4hr\u00b7ten", "und", "fes\u00b7ten", "Treu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "FM", "NN", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Verdien' es zu sein, von Europa das Herz! Beharr'", "tokens": ["Ver\u00b7dien'", "es", "zu", "sein", ",", "von", "Eu\u00b7ro\u00b7pa", "das", "Herz", "!", "Be\u00b7harr'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "PTKZU", "VAINF", "$,", "APPR", "NE", "ART", "NN", "$.", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In vereintem Gem\u00fct zu hegen die Glut,", "tokens": ["In", "ver\u00b7ein\u00b7tem", "Ge\u00b7m\u00fct", "zu", "he\u00b7gen", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die, an himmlischem Strahl sich entz\u00fcndend, leuchtet, w\u00e4rmt und belebt.", "tokens": ["Die", ",", "an", "himm\u00b7li\u00b7schem", "Strahl", "sich", "ent\u00b7z\u00fcn\u00b7dend", ",", "leuch\u00b7tet", ",", "w\u00e4rmt", "und", "be\u00b7lebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "PRF", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "--+--+--+-+-+--+", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Wenn der junge Tag, in der wallenden Locken Gold", "tokens": ["Wenn", "der", "jun\u00b7ge", "Tag", ",", "in", "der", "wal\u00b7len\u00b7den", "Lo\u00b7cken", "Gold"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Err\u00f6tend aus Wiege des Meers, zu dem Himmel schaut,", "token