{"dta.poem.20694": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebhaberhut .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einer weltbekannten Stadt,                 ", "tokens": ["In", "ei\u00b7ner", "welt\u00b7be\u00b7kann\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die rate Kaufmannswaaren", "tokens": ["Die", "ra\u00b7te", "Kauf\u00b7manns\u00b7waa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wundersch\u00f6ne Weiber hat,", "tokens": ["Und", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Wei\u00b7ber", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kam schnell ein Mann gefahren,", "tokens": ["Kam", "schnell", "ein", "Mann", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Eh sich\u2019s sein Weibchen vorgestellt,", "tokens": ["Eh", "sich's", "sein", "Weib\u00b7chen", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und voller Furcht und Schrecken", "tokens": ["Und", "vol\u00b7ler", "Furcht", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Entwich ihr junger Liebesheld;", "tokens": ["Ent\u00b7wich", "ihr", "jun\u00b7ger", "Lie\u00b7bes\u00b7held", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach aber zum Entdecken", "tokens": ["Ach", "a\u00b7ber", "zum", "Ent\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "ADV", "APPRART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.9": {"text": "Der Heimlichkeit gab\u2019s viel Gefahr,", "tokens": ["Der", "Heim\u00b7lich\u00b7keit", "gab's", "viel", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Weil er, zu sehr getrieben,", "tokens": ["Weil", "er", ",", "zu", "sehr", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PTKA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Rasch aus dem Fenster sprang, so war", "tokens": ["Rasch", "aus", "dem", "Fens\u00b7ter", "sprang", ",", "so", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sein Hut noch da geblieben,", "tokens": ["Sein", "Hut", "noch", "da", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Lag auf dem Tischchen unverh\u00fcllt,", "tokens": ["Lag", "auf", "dem", "Tischchen", "un\u00b7ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Viel Argwohn zu erregen,", "tokens": ["Viel", "Arg\u00b7wohn", "zu", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Doch sie, mit Weiberlist erf\u00fcllt,", "tokens": ["Doch", "sie", ",", "mit", "Wei\u00b7ber\u00b7list", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Springt schlau dem Mann entgegen,", "tokens": ["Springt", "schlau", "dem", "Mann", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und ruft: Willkommen, s\u00fc\u00dfer Mann!", "tokens": ["Und", "ruft", ":", "Will\u00b7kom\u00b7men", ",", "s\u00fc\u00b7\u00dfer", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Du sollst den Hut probieren,", "tokens": ["Du", "sollst", "den", "Hut", "pro\u00b7bie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Ein Tr\u00f6delweib bot mir ihn an;", "tokens": ["Ein", "Tr\u00f6\u00b7del\u00b7weib", "bot", "mir", "ihn", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Er ist mit goldnen Schn\u00fcren", "tokens": ["Er", "ist", "mit", "gold\u00b7nen", "Schn\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Reich eingefa\u00dft und noch ganz neu,", "tokens": ["Reich", "ein\u00b7ge\u00b7fa\u00dft", "und", "noch", "ganz", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und ward aus Noth vergeudet. \u2014", "tokens": ["Und", "ward", "aus", "Noth", "ver\u00b7geu\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Dem Mann gef\u00e4llt die Schmeichelei,", "tokens": ["Dem", "Mann", "ge\u00b7f\u00e4llt", "die", "Schmei\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Er k\u00fc\u00dft das Weib und leidet", "tokens": ["Er", "k\u00fc\u00dft", "das", "Weib", "und", "lei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Da\u00df sie auf sein Tuppe den Hut", "tokens": ["Da\u00df", "sie", "auf", "sein", "Tup\u00b7pe", "den", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.26": {"text": "Im Puderhaare dr\u00fccket,", "tokens": ["Im", "Pu\u00b7der\u00b7haa\u00b7re", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Ruft selber aus: er l\u00e4\u00dft mir gut!", "tokens": ["Ruft", "sel\u00b7ber", "aus", ":", "er", "l\u00e4\u00dft", "mir", "gut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und dankt ihr halb entz\u00fccket,", "tokens": ["Und", "dankt", "ihr", "halb", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Indem sein Aug\u2019 im Spiegel gafft,", "tokens": ["In\u00b7dem", "sein", "Aug'", "im", "Spie\u00b7gel", "gafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Den Zierrath seines Kopfes,", "tokens": ["Den", "Zier\u00b7rath", "sei\u00b7nes", "Kop\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Den sie ihm heimlich angeschafft. \u2014", "tokens": ["Den", "sie", "ihm", "heim\u00b7lich", "an\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sie lacht des armen Tropfes", "tokens": ["Sie", "lacht", "des", "ar\u00b7men", "Trop\u00b7fes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Sehr oft auf ihres Lieblings Schoo\u00df,", "tokens": ["Sehr", "oft", "auf", "ih\u00b7res", "Lieb\u00b7lings", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und spricht mit losem Muthe:", "tokens": ["Und", "spricht", "mit", "lo\u00b7sem", "Mu\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Mein Schatz! wir kamen wohlfeil los", "tokens": ["Mein", "Schatz", "!", "wir", "ka\u00b7men", "wohl\u00b7feil", "los"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Mit dem verge\u00dfnen Hute.", "tokens": ["Mit", "dem", "ver\u00b7ge\u00df\u00b7nen", "Hu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}