sure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Schwermuth, die auf meinem Herzen br\u00fctet,", "tokens": ["Schwer\u00b7muth", ",", "die", "auf", "mei\u00b7nem", "Her\u00b7zen", "br\u00fc\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Abschiedsqual, die feurig in mir w\u00fcthet,", "tokens": ["Ab\u00b7schieds\u00b7qual", ",", "die", "feu\u00b7rig", "in", "mir", "w\u00fct\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Macht mein Lied zum hohen Sterbgesang.", "tokens": ["Macht", "mein", "Lied", "zum", "ho\u00b7hen", "Sterb\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In gebrochnen, seufzenden Akkorden,", "tokens": ["In", "ge\u00b7broch\u00b7nen", ",", "seuf\u00b7zen\u00b7den", "Ak\u00b7kor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit erstickten, halbgesagten Worten", "tokens": ["Mit", "er\u00b7stick\u00b7ten", ",", "halb\u00b7ge\u00b7sag\u00b7ten", "Wor\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sing' ich dir des Herzens vollen Drang.", "tokens": ["Sing'", "ich", "dir", "des", "Her\u00b7zens", "vol\u00b7len", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Alles ist um mich in Flor gekleidet,", "tokens": ["Al\u00b7les", "ist", "um", "mich", "in", "Flor", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Schleier einer Wittwe schreitet", "tokens": ["In", "dem", "Schlei\u00b7er", "ei\u00b7ner", "Witt\u00b7we", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vor mir her die seufzende Natur.", "tokens": ["Vor", "mir", "her", "die", "seuf\u00b7zen\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Alles, alles schwimmt um mich in Thr\u00e4nen!", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "schwimmt", "um", "mich", "in", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "APPR", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nirgends find' ich Widerstrahl des Sch\u00f6nen,", "tokens": ["Nir\u00b7gends", "find'", "ich", "Wi\u00b7der\u00b7strahl", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Gr\u00e4ber, furchtbarg\u00e4hnend seh' ich nur.", "tokens": ["Gr\u00e4\u0