{"textgrid.poem.66368": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Mein letztes Wort an Regina", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siegerin! versenkt in tiefes Trauern,", "tokens": ["Sie\u00b7ge\u00b7rin", "!", "ver\u00b7senkt", "in", "tie\u00b7fes", "Trau\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ohne Kraft, die Thr\u00e4nen auszuschauern,", "tokens": ["Oh\u00b7ne", "Kraft", ",", "die", "Thr\u00e4\u00b7nen", "aus\u00b7zu\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weih' ich dir dies bange Abschiedslied.", "tokens": ["Weih'", "ich", "dir", "dies", "ban\u00b7ge", "Ab\u00b7schieds\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PDS", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Abgespannt sind alle meine Saiten,", "tokens": ["Ab\u00b7ge\u00b7spannt", "sind", "al\u00b7le", "mei\u00b7ne", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sieh, wie mir die H\u00e4nd' heruntergleiten,", "tokens": ["Sieh", ",", "wie", "mir", "die", "H\u00e4nd'", "her\u00b7un\u00b7ter\u00b7glei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ach, vom lagen Ringen sind sie m\u00fcd.", "tokens": ["Ach", ",", "vom", "la\u00b7gen", "Rin\u00b7gen", "sind", "sie", "m\u00fcd", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "VVFIN", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Schwermuth, die auf meinem Herzen br\u00fctet,", "tokens": ["Schwer\u00b7muth", ",", "die", "auf", "mei\u00b7nem", "Her\u00b7zen", "br\u00fc\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Abschiedsqual, die feurig in mir w\u00fcthet,", "tokens": ["Ab\u00b7schieds\u00b7qual", ",", "die", "feu\u00b7rig", "in", "mir", "w\u00fct\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Macht mein Lied zum hohen Sterbgesang.", "tokens": ["Macht", "mein", "Lied", "zum", "ho\u00b7hen", "Sterb\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In gebrochnen, seufzenden Akkorden,", "tokens": ["In", "ge\u00b7broch\u00b7nen", ",", "seuf\u00b7zen\u00b7den", "Ak\u00b7kor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit erstickten, halbgesagten Worten", "tokens": ["Mit", "er\u00b7stick\u00b7ten", ",", "halb\u00b7ge\u00b7sag\u00b7ten", "Wor\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sing' ich dir des Herzens vollen Drang.", "tokens": ["Sing'", "ich", "dir", "des", "Her\u00b7zens", "vol\u00b7len", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Alles ist um mich in Flor gekleidet,", "tokens": ["Al\u00b7les", "ist", "um", "mich", "in", "Flor", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Schleier einer Wittwe schreitet", "tokens": ["In", "dem", "Schlei\u00b7er", "ei\u00b7ner", "Witt\u00b7we", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vor mir her die seufzende Natur.", "tokens": ["Vor", "mir", "her", "die", "seuf\u00b7zen\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Alles, alles schwimmt um mich in Thr\u00e4nen!", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "schwimmt", "um", "mich", "in", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "APPR", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nirgends find' ich Widerstrahl des Sch\u00f6nen,", "tokens": ["Nir\u00b7gends", "find'", "ich", "Wi\u00b7der\u00b7strahl", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Gr\u00e4ber, furchtbarg\u00e4hnend seh' ich nur.", "tokens": ["Gr\u00e4\u00b7ber", ",", "furcht\u00b7bar\u00b7g\u00e4h\u00b7nend", "seh'", "ich", "nur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Denn du gehst \u2013 des Abschieds Todesstunde", "tokens": ["Denn", "du", "gehst", "\u2013", "des", "Ab\u00b7schieds", "To\u00b7dess\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$(", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlitzt mir, ach! die halbgeheilte Wunde", "tokens": ["Schlitzt", "mir", ",", "ach", "!", "die", "halb\u00b7ge\u00b7heil\u00b7te", "Wun\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ITJ", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit der Hand von Eisen wieder auf.", "tokens": ["Mit", "der", "Hand", "von", "Ei\u00b7sen", "wie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Einsam, ohne dich, o Serafine!", "tokens": ["Ein\u00b7sam", ",", "oh\u00b7ne", "dich", ",", "o", "Se\u00b7ra\u00b7fi\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "PPER", "$,", "FM", "FM", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wein' ich mit vom Gram durchfurchter Miene", "tokens": ["Wein'", "ich", "mit", "vom", "Gram", "durch\u00b7furch\u00b7ter", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aus dem Kerkerstaub zu Gott hinauf.", "tokens": ["Aus", "dem", "Ker\u00b7ker\u00b7staub", "zu", "Gott", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "O du Schmuck vom weiblichen Geschlechte!", "tokens": ["O", "du", "Schmuck", "vom", "weib\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Komm, ergreif mir die gesunkne Rechte,", "tokens": ["Komm", ",", "er\u00b7greif", "mir", "die", "ge\u00b7sunk\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich dir die H\u00e4nde dr\u00fccken kann.", "tokens": ["Da\u00df", "ich", "dir", "die", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach nur einmal mit dem Aug' voll Liebe,", "tokens": ["Ach", "nur", "ein\u00b7mal", "mit", "dem", "Aug'", "voll", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit dem Blick, von sanfter Wehmuth tr\u00fcbe,", "tokens": ["Mit", "dem", "Blick", ",", "von", "sanf\u00b7ter", "Weh\u00b7muth", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sieh mich einmal noch, Erw\u00e4hlte, an.", "tokens": ["Sieh", "mich", "ein\u00b7mal", "noch", ",", "Er\u00b7w\u00e4hl\u00b7te", ",", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$,", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Eile dann hinunter von dem Walle,", "tokens": ["Ei\u00b7le", "dann", "hin\u00b7un\u00b7ter", "von", "dem", "Wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach! \u2013 begleitet von dem dumpfen Schalle", "tokens": ["Ach", "!", "\u2013", "be\u00b7glei\u00b7tet", "von", "dem", "dum\u00b7pfen", "Schal\u00b7le"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "$(", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meiner Seufzer eile dann hinab.", "tokens": ["Mei\u00b7ner", "Seuf\u00b7zer", "ei\u00b7le", "dann", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gottes Engel, der die Unschuld h\u00fctet", "tokens": ["Got\u00b7tes", "En\u00b7gel", ",", "der", "die", "Un\u00b7schuld", "h\u00fc\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und den Feinden um mich her gebietet,", "tokens": ["Und", "den", "Fein\u00b7den", "um", "mich", "her", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "APZR", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Leite dich mit seinem goldnen Stab.", "tokens": ["Lei\u00b7te", "dich", "mit", "sei\u00b7nem", "gold\u00b7nen", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn du w\u00fchlst im strahlenden Gewebe", "tokens": ["Wenn", "du", "w\u00fchlst", "im", "strah\u00b7len\u00b7den", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner Saiten, Zauberin, so schwebe", "tokens": ["Dei\u00b7ner", "Sai\u00b7ten", ",", "Zau\u00b7be\u00b7rin", ",", "so", "schwe\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Geistig meine Angstgestalt um dich.", "tokens": ["Geis\u00b7tig", "mei\u00b7ne", "Angst\u00b7ge\u00b7stalt", "um", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Denk: Dort droben leidet der Betr\u00fcbte,", "tokens": ["Denk", ":", "Dort", "dro\u00b7ben", "lei\u00b7det", "der", "Be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der mit Feuerungest\u00fcm mich liebte,", "tokens": ["Der", "mit", "Feu\u00b7e\u00b7run\u00b7ge\u00b7st\u00fcm", "mich", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und nun klagt und jammert er um mich.", "tokens": ["Und", "nun", "klagt", "und", "jam\u00b7mert", "er", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Falte dann f\u00fcr mich die frommen H\u00e4nde,", "tokens": ["Fal\u00b7te", "dann", "f\u00fcr", "mich", "die", "from\u00b7men", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df Gott meinen langen Jammer ende,", "tokens": ["Da\u00df", "Gott", "mei\u00b7nen", "lan\u00b7gen", "Jam\u00b7mer", "en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Mit der Freiheit, oder mit dem Tod.", "tokens": ["Mit", "der", "Frei\u00b7heit", ",", "o\u00b7der", "mit", "dem", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, dein Mitleid wird dir Jova lohnen,", "tokens": ["Ach", ",", "dein", "Mit\u00b7leid", "wird", "dir", "Jo\u00b7va", "loh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der schon oft die sch\u00f6nste seiner Kronen", "tokens": ["Der", "schon", "oft", "die", "sch\u00f6ns\u00b7te", "sei\u00b7ner", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ART", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Einer Seele, wie die deine, bot.", "tokens": ["Ei\u00b7ner", "See\u00b7le", ",", "wie", "die", "dei\u00b7ne", ",", "bot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "PPOSAT", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Sterb' ich, so besuche meinen H\u00fcgel,", "tokens": ["Sterb'", "ich", ",", "so", "be\u00b7su\u00b7che", "mei\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo mein Leib, bedeckt vom Mutterfl\u00fcgel", "tokens": ["Wo", "mein", "Leib", ",", "be\u00b7deckt", "vom", "Mut\u00b7ter\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unsrer lieben Erd', im Grabe ruht.", "tokens": ["Uns\u00b7rer", "lie\u00b7ben", "Erd'", ",", "im", "Gra\u00b7be", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Denk' in deiner sch\u00f6nen Seel': Hier unten", "tokens": ["Denk'", "in", "dei\u00b7ner", "sch\u00f6\u00b7nen", "Seel'", ":", "Hier", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "NN", "$.", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hat der Arme einmal Ruh' gefunden", "tokens": ["Hat", "der", "Ar\u00b7me", "ein\u00b7mal", "Ruh'", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Vor dem Weltha\u00df und Tyrannenwuth.", "tokens": ["Vor", "dem", "Welt\u00b7ha\u00df", "und", "Ty\u00b7ran\u00b7nen\u00b7wuth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Ach dann wird sein Schatten um dich schweben,", "tokens": ["Ach", "dann", "wird", "sein", "Schat\u00b7ten", "um", "dich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und wenn Thr\u00e4nen dir herunterbeben,", "tokens": ["Und", "wenn", "Thr\u00e4\u00