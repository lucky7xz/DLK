ter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aus dem Kerkerstaub zu Gott hinauf.", "tokens": ["Aus", "dem", "Ker\u00b7ker\u00b7staub", "zu", "Gott", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "O du Schmuck vom weiblichen Geschlechte!", "tokens": ["O", "du", "Schmuck", "vom", "weib\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Komm, ergreif mir die gesunkne Rechte,", "tokens": ["Komm", ",", "er\u00b7greif", "mir", "die", "ge\u00b7sunk\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich dir die H\u00e4nde dr\u00fccken kann.", "tokens": ["Da\u00df", "ich", "dir", "die", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach nur einmal mit dem Aug' voll Liebe,", "tokens": ["Ach", "nur", "ein\u00b7mal", "mit", "dem", "Aug'", "voll", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit dem Blick, von sanfter Wehmuth tr\u00fcbe,", "tokens": ["Mit", "dem", "Blick", ",", "von", "sanf\u00b7ter", "Weh\u00b7muth", "t