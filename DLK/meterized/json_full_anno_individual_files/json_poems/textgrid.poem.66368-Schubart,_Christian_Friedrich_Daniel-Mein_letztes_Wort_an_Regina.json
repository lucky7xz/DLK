nta"}, "line.5": {"text": "Und den Feinden um mich her gebietet,", "tokens": ["Und", "den", "Fein\u00b7den", "um", "mich", "her", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "APZR", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Leite dich mit seinem goldnen Stab.", "tokens": ["Lei\u00b7te", "dich", "mit", "sei\u00b7nem", "gold\u00b7nen", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.18": {"line.1": {"text": "Wenn du w\u00fchlst im strahlenden Gewebe", "tokens": ["Wenn", "du", "w\u00fchlst", "im", "strah\u00b7len\u00b7den", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner Saiten, Zauberin, so schwebe", "tokens": ["Dei\u00b7ner", "Sai\u00b7ten", ",", "Zau\u00b7be\u00b7rin", ",", "so", "schwe\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Geistig meine Angstgestalt um dich.", "tokens": ["Geis\u00b7tig", "mei\u00b7ne", "Angst\u00b7ge\u00b7stalt", "um", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Denk: Dort droben leidet der Betr\u00fcbte,", "tokens": ["Denk", ":", "Dort", "dro\u00b7ben", "lei\u00b7det", "der", "Be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der mit Feuerungest\u00fcm mich liebte,", "tokens": ["Der", "mit", "Feu\u00b7e\u00b7run\u00b7ge\u00b7st\u00fcm", "mich", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und nun klagt und jammert er um mich.", "tokens": ["Und", "nun", "klagt", "und", "jam\u00b7mert", "er", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.19": {"line.1": {"text": "Falte dann f\u00fcr mich die frommen H\u00e4nde,", "tokens": ["Fal\u00b7te", "dann", "f\u00fcr", "mich", "die", "from\u00b7men", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df Gott meinen langen Jammer ende,", "tokens": ["Da\u00df", "Gott", "mei\u00b7nen", "lan\u00b7gen", "Jam\u00b7mer", "en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Mit der Freiheit, oder mit dem Tod.", "tokens": ["Mit", "der", "Frei\u00b7heit", ",", "o\u00b7der", "mit", "dem", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, dein Mitleid wird dir Jova lohnen,", "tokens": ["Ach", ",", "dein", "Mit\u00b7leid", "wird", "dir", 