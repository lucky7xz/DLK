{"textgrid.poem.64091": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Skythenweisheit", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Perserk\u00f6nig hielt zu Susa Hof:", "tokens": ["Der", "Per\u00b7ser\u00b7k\u00f6\u00b7nig", "hielt", "zu", "Su\u00b7sa", "Hof", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus allen Landen kamen die Satrapen", "tokens": ["Aus", "al\u00b7len", "Lan\u00b7den", "ka\u00b7men", "die", "Sa\u00b7tra\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und beugten in den Staub die stolzen H\u00e4upter;", "tokens": ["Und", "beug\u00b7ten", "in", "den", "Staub", "die", "stol\u00b7zen", "H\u00e4up\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie brachten alles K\u00f6stlichste zur Schatzung:", "tokens": ["Sie", "brach\u00b7ten", "al\u00b7les", "K\u00f6st\u00b7lichs\u00b7te", "zur", "Schat\u00b7zung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Meeres Perle und der Zeder Harz,", "tokens": ["Des", "Mee\u00b7res", "Per\u00b7le", "und", "der", "Ze\u00b7der", "Harz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Edelstein des Bergs, des Stromes Gold", "tokens": ["Der", "E\u00b7del\u00b7stein", "des", "Bergs", ",", "des", "Stro\u00b7mes", "Gold"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ward reich zu Xerxes' F\u00fc\u00dfen hingestreut", "tokens": ["Ward", "reich", "zu", "Xer\u00b7xes'", "F\u00fc\u00b7\u00dfen", "hin\u00b7ge\u00b7streut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und f\u00fcnfzig K\u00f6n'ge dienten ihm beim Mahl. \u2013", "tokens": ["Und", "f\u00fcnf\u00b7zig", "K\u00f6n'\u00b7ge", "dien\u00b7ten", "ihm", "beim", "Mahl", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da war ein Mann aus Skythenland gekommen,", "tokens": ["Da", "war", "ein", "Mann", "aus", "Sky\u00b7then\u00b7land", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Kein K\u00f6nig: ohne K\u00f6nig sind die Skythen \u2013", "tokens": ["\u2013", "Kein", "K\u00f6\u00b7nig", ":", "oh\u00b7ne", "K\u00f6\u00b7nig", "sind", "die", "Sky\u00b7then", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$.", "APPR", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 Nichts schatzend: denn die Skythen schatzen niemand \u2013", "tokens": ["\u2013", "Nichts", "schat\u00b7zend", ":", "denn", "die", "Sky\u00b7then", "schat\u00b7zen", "nie\u00b7mand", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "ADJD", "$.", "KON", "ART", "NN", "VVFIN", "PIS", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geraubte Rosse heischend, welche Knechte", "tokens": ["Ge\u00b7raub\u00b7te", "Ros\u00b7se", "hei\u00b7schend", ",", "wel\u00b7che", "Knech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des K\u00f6nigs aus dem Grenzgebiet entf\u00fchrt,", "tokens": ["Des", "K\u00f6\u00b7nigs", "aus", "dem", "Grenz\u00b7ge\u00b7biet", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur seine beiden Knaben sein Geleit. \u2013", "tokens": ["Nur", "sei\u00b7ne", "bei\u00b7den", "Kna\u00b7ben", "sein", "Ge\u00b7leit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "PIAT", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Mann fand Gnade vor des K\u00f6nigs Augen,", "tokens": ["Der", "Mann", "fand", "Gna\u00b7de", "vor", "des", "K\u00f6\u00b7nigs", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Weil er so anders war, als seine Sklaven.", "tokens": ["Weil", "er", "so", "an\u00b7ders", "war", ",", "als", "sei\u00b7ne", "Skla\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er n\u00f6tigt ihn, zu bleiben Tag um Tag,", "tokens": ["Er", "n\u00f6\u00b7tigt", "ihn", ",", "zu", "blei\u00b7ben", "Tag", "um", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ob l\u00e4ngst der Zweck, um den er kam, erreicht;", "tokens": ["Ob", "l\u00e4ngst", "der", "Zweck", ",", "um", "den", "er", "kam", ",", "er\u00b7reicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "KOUI", "ART", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er zeigt ihm seine Sch\u00e4tze wie sein Heer,", "tokens": ["Er", "zeigt", "ihm", "sei\u00b7ne", "Sch\u00e4t\u00b7ze", "wie", "sein", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Priester Weisheit und der Frauen Reiz:", "tokens": ["Der", "Pries\u00b7ter", "Weis\u00b7heit", "und", "der", "Frau\u00b7en", "Reiz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "F\u00fcr alles hat der Gast ein sinnig Auge,", "tokens": ["F\u00fcr", "al\u00b7les", "hat", "der", "Gast", "ein", "sin\u00b7nig", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und, wenn er redet, stets ein sinnig Wort.", "tokens": ["Und", ",", "wenn", "er", "re\u00b7det", ",", "stets", "ein", "sin\u00b7nig", "Wort", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und als der Tag des Scheidens nun gekommen,", "tokens": ["Und", "als", "der", "Tag", "des", "Schei\u00b7dens", "nun", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Da spricht der K\u00f6nig: \u00bbH\u00f6re mich, Borast,", "tokens": ["Da", "spricht", "der", "K\u00f6\u00b7nig", ":", "\u00bb", "H\u00f6\u00b7re", "mich", ",", "Bo\u00b7rast", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "VVIMP", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ich darf nicht hoffen, dich zur\u00fcck zu halten,", "tokens": ["Ich", "darf", "nicht", "hof\u00b7fen", ",", "dich", "zu\u00b7r\u00fcck", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Denn deine Seele h\u00e4ngt an deinem Volk;", "tokens": ["Denn", "dei\u00b7ne", "See\u00b7le", "h\u00e4ngt", "an", "dei\u00b7nem", "Volk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doch la\u00df die Knaben mir: ich will sie hier", "tokens": ["Doch", "la\u00df", "die", "Kna\u00b7ben", "mir", ":", "ich", "will", "sie", "hier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "PPER", "$.", "PPER", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Mit meinen eignen k\u00f6niglich erziehn", "tokens": ["Mit", "mei\u00b7nen", "eig\u00b7nen", "k\u00f6\u00b7nig\u00b7lich", "er\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und sie dir reich und weise wieder senden.", "tokens": ["Und", "sie", "dir", "reich", "und", "wei\u00b7se", "wie\u00b7der", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADJD", "KON", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Du willst nicht? Sch\u00fcttle nicht das Haupt, Borast!", "tokens": ["Du", "willst", "nicht", "?", "Sch\u00fctt\u00b7le", "nicht", "das", "Haupt", ",", "Bo\u00b7rast", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$.", "NN", "PTKNEG", "ART", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Du mu\u00dft doch selbst gestehn, es birgt mein Hof", "tokens": ["Du", "mu\u00dft", "doch", "selbst", "ge\u00b7stehn", ",", "es", "birgt", "mein", "Hof"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVPP", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Viel tausend G\u00fcter, eurer Steppe fremd.", "tokens": ["Viel", "tau\u00b7send", "G\u00fc\u00b7ter", ",", "eu\u00b7rer", "Step\u00b7pe", "fremd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Verschm\u00e4hst du alle Sch\u00e4tze, wohl, so k\u00f6nnen", "tokens": ["Ver\u00b7schm\u00e4hst", "du", "al\u00b7le", "Sch\u00e4t\u00b7ze", ",", "wohl", ",", "so", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "$,", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Von unsern Magiern deine Knaben lernen", "tokens": ["Von", "un\u00b7sern", "Ma\u00b7giern", "dei\u00b7ne", "Kna\u00b7ben", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Jedwede h\u00f6chste, euch versagte Weisheit.\u00ab \u2013 \u00bbNein,", "tokens": ["Jed\u00b7we\u00b7de", "h\u00f6chs\u00b7te", ",", "euch", "ver\u00b7sag\u00b7te", "Weis\u00b7heit", ".", "\u00ab", "\u2013", "\u00bb", "Nein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "PPER", "VVFIN", "NN", "$.", "$(", "$(", "$(", "PTKANT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "O K\u00f6nig, la\u00df mich ziehn mit meinen S\u00f6hnen.", "tokens": ["O", "K\u00f6\u00b7nig", ",", "la\u00df", "mich", "ziehn", "mit", "mei\u00b7nen", "S\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Nur eine Weisheit gibt's und diese, Xerxes,", "tokens": ["Nur", "ei\u00b7ne", "Weis\u00b7heit", "gibt's", "und", "die\u00b7se", ",", "Xe\u00b7rxes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "PDS", "$,", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "Zu lernen komm zu uns ins Skythenland:", "tokens": ["Zu", "ler\u00b7nen", "komm", "zu", "uns", "ins", "Sky\u00b7then\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Hier ist sie nicht.\u00ab \u2013 \u00bbNun,\u00ab l\u00e4chelte der K\u00f6nig,", "tokens": ["Hier", "ist", "sie", "nicht", ".", "\u00ab", "\u2013", "\u00bb", "Nun", ",", "\u00ab", "l\u00e4\u00b7chel\u00b7te", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "$.", "$(", "$(", "$(", "ADV", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "\u00bbund welches w\u00e4re diese h\u00f6chste Weisheit?\u00ab", "tokens": ["\u00bb", "und", "wel\u00b7ches", "w\u00e4\u00b7re", "die\u00b7se", "h\u00f6chs\u00b7te", "Weis\u00b7heit", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWS", "VAFIN", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "\u00bbsie ist:\u00ab \u2013 sprach er und ging mit seinen Knaben \u2013", "tokens": ["\u00bb", "sie", "ist", ":", "\u00ab", "\u2013", "sprach", "er", "und", "ging", "mit", "sei\u00b7nen", "Kna\u00b7ben", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$.", "$(", "$(", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "\u00bbden Tod nicht f\u00fcrchten und die Wahrheit sagen.\u00ab \u2013", "tokens": ["\u00bb", "den", "Tod", "nicht", "f\u00fcrch\u00b7ten", "und", "die", "Wahr\u00b7heit", "sa\u00b7gen", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ART", "NN", "PTKNEG", "VVINF", "KON", "ART", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Perserk\u00f6nig hielt zu Susa Hof:", "tokens": ["Der", "Per\u00b7ser\u00b7k\u00f6\u00b7nig", "hielt", "zu", "Su\u00b7sa", "Hof", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus allen Landen kamen die Satrapen", "tokens": ["Aus", "al\u00b7len", "Lan\u00b7den", "ka\u00b7men", "die", "Sa\u00b7tra\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und beugten in den Staub die stolzen H\u00e4upter;", "tokens": ["Und", "beug\u00b7ten", "in", "den", "Staub", "die", "sto