{"dta.poem.19307": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Weiser, um mit Kraft den Vorzug zu beweisen", "tokens": ["Ein", "Wei\u00b7ser", ",", "um", "mit", "Kraft", "den", "Vor\u00b7zug", "zu", "be\u00b7wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Menschen vor dem Thier, wollt' auch sein Alter preisen.", "tokens": ["Des", "Men\u00b7schen", "vor", "dem", "Thier", ",", "wollt'", "auch", "sein", "Al\u00b7ter", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VMFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von Thieren soll der Mensch das h\u00f6chste Alter haben;", "tokens": ["Von", "Thie\u00b7ren", "soll", "der", "Mensch", "das", "h\u00f6chs\u00b7te", "Al\u00b7ter", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Fabel nur ist was man sagt von Hirsch und Raben.", "tokens": ["Denn", "Fa\u00b7bel", "nur", "ist", "was", "man", "sagt", "von", "Hirsch", "und", "Ra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "PWS", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch macht dir nun ein Thier den Vorzug streitig kaum,", "tokens": ["Doch", "macht", "dir", "nun", "ein", "Thier", "den", "Vor\u00b7zug", "strei\u00b7tig", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So thut es jeder Fels, so thut es mancher Baum.", "tokens": ["So", "thut", "es", "je\u00b7der", "Fels", ",", "so", "thut", "es", "man\u00b7cher", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum etwas anders mu\u00df dein Menschenvoraus seyn,", "tokens": ["Drum", "et\u00b7was", "an\u00b7ders", "mu\u00df", "dein", "Men\u00b7schen\u00b7vor\u00b7aus", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den dir alswie das Thier auch einr\u00e4umt Pflanz' und Stein:", "tokens": ["Den", "dir", "als\u00b7wie", "das", "Thier", "auch", "ein\u00b7r\u00e4umt", "Pflanz'", "und", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOKOM", "ART", "NN", "ADV", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df du in jedem Nu, in Gott und Welt und dir", "tokens": ["Da\u00df", "du", "in", "je\u00b7dem", "Nu", ",", "in", "Gott", "und", "Welt", "und", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ADV", "$,", "APPR", "NN", "KON", "NN", "KON", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mehr lebst als lebenslang Fels oder Baum und Thier.", "tokens": ["Mehr", "lebst", "als", "le\u00b7bens\u00b7lang", "Fels", "o\u00b7der", "Baum", "und", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "VVFIN", "NE", "KON", "NE", "KON", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}