{"dta.poem.15885": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durchbl\u00e4ttern wollt' ich auch f\u00fcr dich die Kinderschriften,", "tokens": ["Durch\u00b7bl\u00e4t\u00b7tern", "wollt'", "ich", "auch", "f\u00fcr", "dich", "die", "Kin\u00b7der\u00b7schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Kind, ob F\u00f6rderung dadurch dir sei zu stiften.", "tokens": ["Mein", "Kind", ",", "ob", "F\u00f6r\u00b7de\u00b7rung", "da\u00b7durch", "dir", "sei", "zu", "stif\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "NN", "PAV", "PPER", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht brauchen kanst du sie, wenn du kein Kind willst bleiben,", "tokens": ["Nicht", "brau\u00b7chen", "kanst", "du", "sie", ",", "wenn", "du", "kein", "Kind", "willst", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil rechte M\u00e4nner nie f\u00fcr blo\u00dfe Kinder schreiben.", "tokens": ["Weil", "rech\u00b7te", "M\u00e4n\u00b7ner", "nie", "f\u00fcr", "blo\u00b7\u00dfe", "Kin\u00b7der", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was braucht es mehr Beweis? von hundert Dichterlingen", "tokens": ["Was", "braucht", "es", "mehr", "Be\u00b7weis", "?", "von", "hun\u00b7dert", "Dich\u00b7ter\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "$.", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6rst du in diesem Buch die Kinderklapper klingen.", "tokens": ["H\u00f6rst", "du", "in", "die\u00b7sem", "Buch", "die", "Kin\u00b7der\u00b7klap\u00b7per", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vom einen Dichter, der der eine ist vor allen,", "tokens": ["Vom", "ei\u00b7nen", "Dich\u00b7ter", ",", "der", "der", "ei\u00b7ne", "ist", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ART", "NN", "$,", "PRELS", "ART", "PIS", "VAFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist kaum ein Fetzchen hier, das ihm im Schlaf entfallen.", "tokens": ["Ist", "kaum", "ein", "Fetz\u00b7chen", "hier", ",", "das", "ihm", "im", "Schlaf", "ent\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du lernst daraus, wiesehr er andre \u00fcbertrifft,", "tokens": ["Du", "lernst", "da\u00b7raus", ",", "wie\u00b7sehr", "er", "and\u00b7re", "\u00fc\u00b7bert\u00b7rifft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil nur so wenig taugt von ihm zur Kinderschrift.", "tokens": ["Weil", "nur", "so", "we\u00b7nig", "taugt", "von", "ihm", "zur", "Kin\u00b7der\u00b7schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIS", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}