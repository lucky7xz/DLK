{"dta.poem.23338": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wasser und Wein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer schuf das Wasser? wer den Wein?", "tokens": ["Wer", "schuf", "das", "Was\u00b7ser", "?", "wer", "den", "Wein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PWS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wasser her! schenket ein!", "tokens": ["Was\u00b7ser", "her", "!", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Wasser schuf nur Gott allein,", "tokens": ["Das", "Was\u00b7ser", "schuf", "nur", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wasser ist g\u00f6ttlich, und menschlich der Wein.", "tokens": ["Das", "Was\u00b7ser", "ist", "g\u00f6tt\u00b7lich", ",", "und", "menschlich", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Mehr ist das Wasser als der Wein,", "tokens": ["Mehr", "ist", "das", "Was\u00b7ser", "als", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wasser her! schenket ein!", "tokens": ["Was\u00b7ser", "her", "!", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich aber will bescheiden sein:", "tokens": ["Ich", "a\u00b7ber", "will", "be\u00b7schei\u00b7den", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich lobe das Wasser und \u2014 trinke den Wein.", "tokens": ["Ich", "lo\u00b7be", "das", "Was\u00b7ser", "und", "trin\u00b7ke", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}