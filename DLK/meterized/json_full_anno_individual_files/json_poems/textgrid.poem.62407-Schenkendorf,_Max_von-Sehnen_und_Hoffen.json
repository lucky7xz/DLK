s": ["NN", "VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu den Thr\u00e4nen, zu dem Grab.", "tokens": ["Zu", "den", "Thr\u00e4\u00b7nen", ",", "zu", "dem", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Winter flieht und Fr\u00fchling naht;", "tokens": ["Win\u00b7ter", "flieht", "und", "Fr\u00fch\u00b7ling", "naht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheuch' den Traum, du mu\u00dft erwachen,", "tokens": ["Scheuch'", "den", "Traum", ",", "du", "mu\u00dft", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcten schm\u00fccken schon den Pfad,", "tokens": ["Bl\u00fc\u00b7ten", "schm\u00fc\u00b7cken", "schon", "den", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am Ufer harrt ein Nachen;", "tokens": ["Und", "am", "U\u00b7fer", "harrt", "ein", "Na\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Steig' hinein mit gl\u00e4ub'gem Sinn,", "tokens": ["Steig'", "hin\u00b7ein", "mit", "gl\u00e4ub'\u00b7gem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schau' nach jenem Ufer hin.", "tokens": ["Schau'", "nach", "je\u00b7nem", "U\u00b7fer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Eines Lebens Athem weht", "tokens": ["Ei\u00b7nes", "Le\u00b7bens", "A\u00b7them", "weht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Sch\u00f6pfung weite R\u00e4ume,", "tokens": ["Durch", "der", "Sch\u00f6p\u00b7fung", "wei\u00b7te", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines Gottes Ruf ergeht", "tokens": ["Ei\u00b7nes", "Got\u00b7tes", "Ruf", "er\u00b7geht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An die Menschen, Sterne, B\u00e4ume,", "tokens": ["An", "die", "Men\u00b7schen", ",", "Ster\u00b7ne", ",", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Halte dran in Lieb' und Treu,", "tokens": ["Hal\u00b7te", "dran", "in", "Lieb'", "und", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einst wird alles jung und neu.", "tokens": ["Einst", "wird", "al\u00b7les", "jung", "und", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Der die Lieb' in unsrer Brust", "tokens": ["Der", "die", "Lieb'", "in", "uns\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Flammen all entz\u00fcndet,", "tokens": ["Und", "die", "Flam\u00b7men", "all", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat der holden, regen Lust", "tokens": ["Hat", "der", "hol\u00b7den", ",", "re\u00b7gen", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch den ew'gen Trost verk\u00fcndet:", "tokens": ["Auch", "den", "ew'\u00b7gen", "Trost", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kling', o s\u00fc\u00dfe Botschaft, fort,", "tokens": ["Kling'", ",", "o", "s\u00fc\u00b7\u00dfe", "Bot\u00b7schaft", ",", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leben ist so hier als dort.", "tokens": ["Le\u00b7ben", "ist", "so", "hier", "als", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "KOKOM", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": 