{"textgrid.poem.62407": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Sehnen und Hoffen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir an den Gr\u00e4bern stehn", "tokens": ["Wenn", "wir", "an", "den", "Gr\u00e4\u00b7bern", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Geliebten, der Gespielen,", "tokens": ["Der", "Ge\u00b7lieb\u00b7ten", ",", "der", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlen wir ein mildes Wehn", "tokens": ["F\u00fch\u00b7len", "wir", "ein", "mil\u00b7des", "Wehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre hei\u00dfe Wange k\u00fchlen,", "tokens": ["Uns\u00b7re", "hei\u00b7\u00dfe", "Wan\u00b7ge", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Licht, ein heller Strahl,", "tokens": ["Und", "ein", "Licht", ",", "ein", "hel\u00b7ler", "Strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leuchtet in dem Schauerthal.", "tokens": ["Leuch\u00b7tet", "in", "dem", "Schau\u00b7er\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Todeswehen, Grabesluft,", "tokens": ["To\u00b7des\u00b7we\u00b7hen", ",", "Gra\u00b7bes\u00b7luft", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Erde, sind es deine Bande,", "tokens": ["Er\u00b7de", ",", "sind", "es", "dei\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder kamst du Lebensluft,", "tokens": ["O\u00b7der", "kamst", "du", "Le\u00b7bens\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem fernen, sel'gen Strande,", "tokens": ["Von", "dem", "fer\u00b7nen", ",", "sel'\u00b7gen", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Winkest du von dr\u00fcben her,", "tokens": ["Win\u00b7kest", "du", "von", "dr\u00fc\u00b7ben", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Holdes Licht uns \u00fcber's Meer?", "tokens": ["Hol\u00b7des", "Licht", "uns", "\u00fc\u00b7ber's", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sehnen kann von Hoffen nicht,", "tokens": ["Seh\u00b7nen", "kann", "von", "Hof\u00b7fen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmel nicht von Erde lassen,", "tokens": ["Him\u00b7mel", "nicht", "von", "Er\u00b7de", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Sehnsucht sich verspricht,", "tokens": ["Was", "die", "Sehn\u00b7sucht", "sich", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag die Hoffnung fr\u00f6hlich fassen;", "tokens": ["Mag", "die", "Hoff\u00b7nung", "fr\u00f6h\u00b7lich", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmel neigt sich gern herab,", "tokens": ["Him\u00b7mel", "neigt", "sich", "gern", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu den Thr\u00e4nen, zu dem Grab.", "tokens": ["Zu", "den", "Thr\u00e4\u00b7nen", ",", "zu", "dem", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Winter flieht und Fr\u00fchling naht;", "tokens": ["Win\u00b7ter", "flieht", "und", "Fr\u00fch\u00b7ling", "naht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheuch' den Traum, du mu\u00dft erwachen,", "tokens": ["Scheuch'", "den", "Traum", ",", "du", "mu\u00dft", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcten schm\u00fccken schon den Pfad,", "tokens": ["Bl\u00fc\u00b7ten", "schm\u00fc\u00b7cken", "schon", "den", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am Ufer harrt ein Nachen;", "tokens": ["Und", "am", "U\u00b7fer", "harrt", "ein", "Na\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Steig' hinein mit gl\u00e4ub'gem Sinn,", "tokens": ["Steig'", "hin\u00b7ein", "mit", "gl\u00e4ub'\u00b7gem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schau' nach jenem Ufer hin.", "tokens": ["Schau'", "nach", "je\u00b7nem", "U\u00b7fer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eines Lebens Athem weht", "tokens": ["Ei\u00b7nes", "Le\u00b7bens", "A\u00b7them", "weht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Sch\u00f6pfung weite R\u00e4ume,", "tokens": ["Durch", "der", "Sch\u00f6p\u00b7fung", "wei\u00b7te", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines Gottes Ruf ergeht", "tokens": ["Ei\u00b7nes", "Got\u00b7tes", "Ruf", "er\u00b7geht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An die Menschen, Sterne, B\u00e4ume,", "tokens": ["An", "die", "Men\u00b7schen", ",", "Ster\u00b7ne", ",", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Halte dran in Lieb' und Treu,", "tokens": ["Hal\u00b7te", "dran", "in", "Lieb'", "und", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einst wird alles jung und neu.", "tokens": ["Einst", "wird", "al\u00b7les", "jung", "und", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der die Lieb' in unsrer Brust", "tokens": ["Der", "die", "Lieb'", "in", "uns\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Flammen all entz\u00fcndet,", "tokens": ["Und", "die", "Flam\u00b7men", "all", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat der holden, regen Lust", "tokens": ["Hat", "der", "hol\u00b7den", ",", "re\u00b7gen", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch den ew'gen Trost verk\u00fcndet:", "tokens": ["Auch", "den", "ew'\u00b7gen", "Trost", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kling', o s\u00fc\u00dfe Botschaft, fort,", "tokens": ["Kling'", ",", "o", "s\u00fc\u00b7\u00dfe", "Bot\u00b7schaft", ",", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leben ist so hier als dort.", "tokens": ["Le\u00b7ben", "ist", "so", "hier", "als", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "KOKOM", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Pflanzt es auf die Gr\u00e4ber hin", "tokens": ["Pflanzt", "es", "auf", "die", "Gr\u00e4\u00b7ber", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Hoffnung Siegeszeichen,", "tokens": ["Uns\u00b7rer", "Hoff\u00b7nung", "Sie\u00b7ges\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Lebens-K\u00f6nigin", "tokens": ["Da\u00df", "der", "Le\u00b7bens\u00b7K\u00f6\u00b7ni\u00b7gin"], "toke