{"textgrid.poem.65668": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Mainottenwitwe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieben Wunden vor der Stirne und drei Wunden auf der Brust,", "tokens": ["Sie\u00b7ben", "Wun\u00b7den", "vor", "der", "Stir\u00b7ne", "und", "drei", "Wun\u00b7den", "auf", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "KON", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "In der Faust das rothe Eisen und im Auge Siegeslust \u2013", "tokens": ["In", "der", "Faust", "das", "ro\u00b7the", "Ei\u00b7sen", "und", "im", "Au\u00b7ge", "Sie\u00b7ges\u00b7lust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "KON", "APPRART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Also lag er auf dem Felde, und im Kreis eng' um ihn her", "tokens": ["Al\u00b7so", "lag", "er", "auf", "dem", "Fel\u00b7de", ",", "und", "im", "Kreis", "eng'", "um", "ihn", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "APPRART", "NN", "ADV", "APPR", "PPER", "APZR"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Lagen seiner Feinde Waffen, Dolch und B\u00fcchse, Schwert und Speer.", "tokens": ["La\u00b7gen", "sei\u00b7ner", "Fein\u00b7de", "Waf\u00b7fen", ",", "Dolch", "und", "B\u00fcch\u00b7se", ",", "Schwert", "und", "Speer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Aber ihrer Tr\u00e4ger Leichen lagen ihm so nahe nicht,", "tokens": ["A\u00b7ber", "ih\u00b7rer", "Tr\u00e4\u00b7ger", "Lei\u00b7chen", "la\u00b7gen", "ihm", "so", "na\u00b7he", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Abgewendet von dem Helden barg im Staub sich ihr Gesicht.", "tokens": ["Ab\u00b7ge\u00b7wen\u00b7det", "von", "dem", "Hel\u00b7den", "barg", "im", "Staub", "sich", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Tochter, hole mir das Kr\u00e4nzlein, welches h\u00e4ngt in meinem Schrein,", "tokens": ["Toch\u00b7ter", ",", "ho\u00b7le", "mir", "das", "Kr\u00e4nz\u00b7lein", ",", "wel\u00b7ches", "h\u00e4ngt", "in", "mei\u00b7nem", "Schrein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Aber sa\u00df' es sanft \u2013 es wird wohl d\u00fcrre zum Zerbrechen sein.", "tokens": ["A\u00b7ber", "sa\u00df'", "es", "sanft", "\u2013", "es", "wird", "wohl", "d\u00fcr\u00b7re", "zum", "Zer\u00b7bre\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN", "ADV", "VVFIN", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Damit will ich heut' mich kr\u00e4nzen, wie an meinem Ehrentag,", "tokens": ["Da\u00b7mit", "will", "ich", "heut'", "mich", "kr\u00e4n\u00b7zen", ",", "wie", "an", "mei\u00b7nem", "Eh\u00b7ren\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Will auf diesem Felde feiern noch einmal mein Brautgelag.", "tokens": ["Will", "auf", "die\u00b7sem", "Fel\u00b7de", "fei\u00b7ern", "noch", "ein\u00b7mal", "mein", "Braut\u00b7ge\u00b7lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PDAT", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Schaff' auch sch\u00f6ne, frische Blumen f\u00fcr den Br\u00e4utigam herbei,", "tokens": ["Schaff'", "auch", "sch\u00f6\u00b7ne", ",", "fri\u00b7sche", "Blu\u00b7men", "f\u00fcr", "den", "Br\u00e4u\u00b7ti\u00b7gam", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "$,", "ADJA", "NN", "APPR", "ART", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Da\u00df das Lager weich und duftig meinem edlen Schl\u00e4fer sei,", "tokens": ["Da\u00df", "das", "La\u00b7ger", "weich", "und", "duf\u00b7tig", "mei\u00b7nem", "ed\u00b7len", "Schl\u00e4\u00b7fer", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Einen Rosensenker steck' ich ihm in jedes offne Mal,", "tokens": ["Ei\u00b7nen", "Ro\u00b7sen\u00b7sen\u00b7ker", "steck'", "ich", "ihm", "in", "je\u00b7des", "off\u00b7ne", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Da\u00df sie einst aus seinem H\u00fcgel sprie\u00dfen im Eurotasthal;", "tokens": ["Da\u00df", "sie", "einst", "aus", "sei\u00b7nem", "H\u00fc\u00b7gel", "sprie\u00b7\u00dfen", "im", "Eu\u00b7ro\u00b7tast\u00b7hal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Und von diesen Rosen wind' ich dir den Kranz, mein T\u00f6chterlein,", "tokens": ["Und", "von", "die\u00b7sen", "Ro\u00b7sen", "wind'", "ich", "dir", "den", "Kranz", ",", "mein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Wenn einmal ein Heldenknabe wird um Deine Liebe frein,", "tokens": ["Wenn", "ein\u00b7mal", "ein", "Hel\u00b7den\u00b7kna\u00b7be", "wird", "um", "Dei\u00b7ne", "Lie\u00b7be", "frein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Einer, der zum Werbegelde so viel T\u00fcrkensch\u00e4del gab,", "tokens": ["Ei\u00b7ner", ",", "der", "zum", "Wer\u00b7be\u00b7gel\u00b7de", "so", "viel", "T\u00fcr\u00b7ken\u00b7sch\u00e4\u00b7del", "gab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPRART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Als blutrothe Rosenst\u00f6cke bl\u00fchn auf deines Vaters Grab.", "tokens": ["Als", "blut\u00b7ro\u00b7the", "Ro\u00b7sen\u00b7st\u00f6\u00b7cke", "bl\u00fchn", "auf", "dei\u00b7nes", "Va\u00b7ters", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.19": {"text": "Aber morgen in der Fr\u00fche, wenn mein Br\u00e4utigam nun ruht,", "tokens": ["A\u00b7ber", "mor\u00b7gen", "in", "der", "Fr\u00fc\u00b7he", ",", "wenn", "mein", "Br\u00e4u\u00b7ti\u00b7gam", "nun", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NE", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Zieh' ich aus die Festgew\u00e4nder, nehm' den Kranz von meinem Hut,", "tokens": ["Zieh'", "ich", "aus", "die", "Fest\u00b7ge\u00b7w\u00e4n\u00b7der", ",", "nehm'", "den", "Kranz", "von", "mei\u00b7nem", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Und im grauen Witwenhemde schleich' ich durch den gr\u00fcnen Wald,", "tokens": ["Und", "im", "grau\u00b7en", "Wit\u00b7wen\u00b7hem\u00b7de", "schleich'", "ich", "durch", "den", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Nicht, zu lauschen, wo im Dickicht Nachtigallenschlag erschallt,", "tokens": ["Nicht", ",", "zu", "lau\u00b7schen", ",", "wo", "im", "Di\u00b7ckicht", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", "er\u00b7schallt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PTKZU", "VVINF", "$,", "PWAV", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Nein, um einen Baum zu suchen ohne Bl\u00fcth' und ohne Blatt,", "tokens": ["Nein", ",", "um", "ei\u00b7nen", "Baum", "zu", "su\u00b7chen", "oh\u00b7ne", "Bl\u00fcth'", "und", "oh\u00b7ne", "Blatt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Den die Turteltaubenwitwe sich zum Sitz ersehen hat,", "tokens": ["Den", "die", "Tur\u00b7tel\u00b7tau\u00b7ben\u00b7wit\u00b7we", "sich", "zum", "Sitz", "er\u00b7se\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PRF", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Und dabei die frische Quelle, die sie tr\u00fcbe macht zuvor,", "tokens": ["Und", "da\u00b7bei", "die", "fri\u00b7sche", "Quel\u00b7le", ",", "die", "sie", "tr\u00fc\u00b7be", "macht", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJA", "VVFIN", "ADV", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.26": {"text": "Eh' sie trinkt und eh' sie badet, seit sie ihren Mann verlor.", "tokens": ["Eh'", "sie", "trinkt", "und", "eh'", "sie", "ba\u00b7det", ",", "seit", "sie", "ih\u00b7ren", "Mann", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Da will ich mich niederlegen, wo kein Schattendach mich k\u00fchlt,", "tokens": ["Da", "will", "ich", "mich", "nie\u00b7der\u00b7le\u00b7gen", ",", "wo", "kein", "Schat\u00b7ten\u00b7dach", "mich", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,", "PWAV", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "Wo der Regengu\u00df die Thr\u00e4nen kalt mir von den Wangen sp\u00fclt,", "tokens": ["Wo", "der", "Re\u00b7gen\u00b7gu\u00df", "die", "Thr\u00e4\u00b7nen", "kalt", "mir", "von", "den", "Wan\u00b7gen", "sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Und mit meiner Turteltaube geh' ich einen Wettstreit an,", "tokens": ["Und", "mit", "mei\u00b7ner", "Tur\u00b7tel\u00b7tau\u00b7be", "geh'", "ich", "ei\u00b7nen", "Wett\u00b7streit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Wer am j\u00e4mmerlichsten klagen, wer am frohsten sterben kann.", "tokens": ["Wer", "am", "j\u00e4m\u00b7mer\u00b7lichs\u00b7ten", "kla\u00b7gen", ",", "wer", "am", "frohs\u00b7ten", "ster\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "VVINF", "$,", "PWS", "APPRART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Sieben Wunden vor der Stirne und drei Wunden auf der Brust,", "tokens": ["Sie\u00b7ben", "Wun\u00b7den", "vor", "der", "Stir\u00b7ne", "und", "drei", "Wun\u00b7den", "auf", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "KON", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "In der Faust das rothe Eisen und im Auge Siegeslust \u2013", "tokens": ["In", "der", "Faust", "das", "ro\u00b7the", "Ei\u00b7sen", "und", "im", "Au\u00b7ge", "Sie\u00b7ges\u00b7lust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "KON", "APPRART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Also lag er auf dem Felde, und im Kreis eng' um ihn her", "tokens": ["Al\u00b7so", "lag", "er", "auf", "dem", "Fel\u00b7de", ",", "und", "im", "Kreis", "eng'", "um", "ihn", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "APPRART", "NN", "ADV", "APPR", "PPER", "APZR"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Lagen seiner Feinde Waffen, Dolch und B\u00fcchse, Schwert und Speer.", "tokens": ["La\u00b7gen", "sei\u00b7ner", "Fein\u00b7de", "Waf\u00b7fen", ",", "Dolch", "und", "B\u00fcch\u00b7se", ",", "Schwert", "und", "Speer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Aber ihrer Tr\u00e4ger Leichen lagen ihm so nahe nicht,", "tokens": ["A\u00b7ber", "ih\u00b7rer", "Tr\u00e4\u00b7ger", "Lei\u00b7chen", "la\u00b7gen", "ihm", "so", "na\u00b7he", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Abgewendet von dem Helden barg im Staub sich ihr Gesicht.", "tokens": ["Ab\u00b7ge\u00b7wen\u00b7det", "von", "dem", "Hel\u00b7den", "barg", "im", "Staub", "sich", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Tochter, hole mir das Kr\u00e4nzlein, welches h\u00e4ngt in meinem Schrein,", "tokens": ["Toch\u00b7ter", ",", "ho\u00b7le", "mir", "das", "Kr\u00e4nz\u00b7lein", ",", "wel\u00b7ches", "h\u00e4ngt", "in", "mei\u00b7nem", "Schrein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Aber sa\u00df' es sanft \u2013 es wird wohl d\u00fcrre zum Zerbrechen sein.", "tokens": ["A\u00b7ber", "sa\u00df'", "es", "sanft", "\u2013", "es", "wird", "wohl", "d\u00fcr\u00b7re", "zum", "Zer\u00b7bre\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN", "ADV", "VVFIN", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Damit will ich heut' mich kr\u00e4nzen, wie an meinem Ehrentag,", "tokens": ["Da\u00b7mit", "will", "ich", "heut'", "mich", "kr\u00e4n\u00b7zen", ",", "wie", "an", "mei\u00b7nem", "Eh\u00b7ren\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Will auf diesem Felde feiern noch einmal mein Brautgelag.", "tokens": ["Will", "auf", "die\u00b7sem", "Fel\u00b7de", "fei\u00b7ern", "noch", "ein\u00b7mal", "mein", "Braut\u00b7ge\u00b7lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PDAT", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Schaff' auch sch\u00f6ne, frische Blumen f\u00fcr den Br\u00e4utigam herbei,", "tokens": ["Schaff'", "auch", "sch\u00f6\u00b7ne", ",", "fri\u00b7sche", "Blu\u00b7men", "f\u00fcr", "den", "Br\u00e4u\u00b7ti\u00b7gam", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "$,", "ADJA", "NN", "APPR", "ART", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Da\u00df das Lager weich und duftig meinem edlen Schl\u00e4fer sei,", "tokens": ["Da\u00df", "das", "La\u00b7ger", "weich", "und", "duf\u00b7tig", "mei\u00b7nem", "ed\u00b7len", "Schl\u00e4\u00b7fer", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Einen Rosensenker steck' ich ihm in jedes offne Mal,", "tokens": ["Ei\u00b7nen", "Ro\u00b7sen\u00b7sen\u00b7ker", "steck'", "ich", "ihm", "in", "je\u00b7des", "off\u00b7ne", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Da\u00df sie einst aus seinem H\u00fcgel sprie\u00dfen im Eurotasthal;", "tokens": ["Da\u00df", "sie", "einst", "aus", "sei\u00b7nem", "H\u00fc\u00b7gel", "sprie\u00b7\u00dfen", "im", "Eu\u00b7ro\u00b7tast\u00b7hal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Und von diesen Rosen wind' ich dir den Kranz, mein T\u00f6chterlein,", "tokens": ["Und", "von", "die\u00b7sen", "Ro\u00b7sen", "wind'", "ich", "dir", "den", "Kranz", ",", "mein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Wenn einmal ein Heldenknabe wird um Deine Liebe frein,", "tokens": ["Wenn", "ein\u00b7mal", "ein", "Hel\u00b7den\u00b7kna\u00b7be", "wird", "um", "Dei\u00b7ne", "Lie\u00b7be", "frein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Einer, der zum Werbegelde so viel T\u00fcrkensch\u00e4del gab,", "tokens": ["Ei\u00b7ner", ",", "der", "zum", "Wer\u00b7be\u00b7gel\u00b7de", "so", "viel", "T\u00fcr\u00b7ken\u00b7sch\u00e4\u00b7del", "gab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPRART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Als blutrothe Rosenst\u00f6cke bl\u00fchn auf deines Vaters Grab.", "tokens": ["Als", "blut\u00b7ro\u00b7the", "Ro\u00b7sen\u00b7st\u00f6\u00b7cke", "bl\u00fchn", "auf", "dei\u00b7nes", "Va\u00b7ters", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+++-+-+-+-+-+-+", "measure