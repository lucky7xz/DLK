{"dta.poem.23514": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Im Jahr 1812.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Kaiser doch erst\u00e4nde!", "tokens": ["Wenn", "der", "Kai\u00b7ser", "doch", "er\u00b7st\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! er schl\u00e4ft zu lange Zeit:", "tokens": ["Ach", "!", "er", "schl\u00e4ft", "zu", "lan\u00b7ge", "Zeit", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Knechtschaft hat kein Ende", "tokens": ["Uns\u00b7re", "Knecht\u00b7schaft", "hat", "kein", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein End' hat unser Leid.", "tokens": ["Und", "kein", "End'", "hat", "un\u00b7ser", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem sch\u00f6nen deutschen Lande", "tokens": ["Auf", "dem", "sch\u00f6\u00b7nen", "deut\u00b7schen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht der Fluch der Sklaverei \u2014", "tokens": ["Ruht", "der", "Fluch", "der", "Skla\u00b7ve\u00b7rei"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mach uns von der eignen Schande,", "tokens": ["Mach", "uns", "von", "der", "eig\u00b7nen", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem b\u00f6sen Fluche frei!", "tokens": ["Von", "dem", "b\u00f6\u00b7sen", "Flu\u00b7che", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kaiser Friedrich, auf! erwache!", "tokens": ["Kai\u00b7ser", "Fried\u00b7rich", ",", "auf", "!", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "PTKVZ", "$.", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem heil'gen Reichspanier", "tokens": ["Mit", "dem", "heil'\u00b7gen", "Reich\u00b7spa\u00b7nier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm zu der gerechten Rache!", "tokens": ["Komm", "zu", "der", "ge\u00b7rech\u00b7ten", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott der Herr er ist mit dir. \u2014", "tokens": ["Gott", "der", "Herr", "er", "ist", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VAFIN", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, es kr\u00e4chzen noch die Raben", "tokens": ["Ach", ",", "es", "kr\u00e4ch\u00b7zen", "noch", "die", "Ra\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Berg bei Tag und Nacht,", "tokens": ["Um", "den", "Berg", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und das Reich es bleibt begraben,", "tokens": ["Und", "das", "Reich", "es", "bleibt", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil der Kaiser nicht erwacht.", "tokens": ["Weil", "der", "Kai\u00b7ser", "nicht", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}