{"dta.poem.11306": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von den narren auf universit\u00e4ten.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man wundert sich,", "tokens": ["Man", "wun\u00b7dert", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Warum auf universit\u00e4ten,", "tokens": ["Wa\u00b7rum", "auf", "u\u00b7niv\u00b7er\u00b7si\u00b7t\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die man den sitz der wei\u00dfheit nennt,", "tokens": ["Die", "man", "den", "sitz", "der", "wei\u00df\u00b7heit", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So greulich viel das m\u00fcde pflaster treten,", "tokens": ["So", "greu\u00b7lich", "viel", "das", "m\u00fc\u00b7de", "pflas\u00b7ter", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bey denen man den wurm im ersten anblick kennt,", "tokens": ["Bey", "de\u00b7nen", "man", "den", "wurm", "im", "ers\u00b7ten", "an\u00b7blick", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein die ursach ist noch zu ergr\u00fcnden:", "tokens": ["Al\u00b7lein", "die", "ur\u00b7sach", "ist", "noch", "zu", "er\u00b7gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hier h\u00e4lt die goldne regel stich,", "tokens": ["Hier", "h\u00e4lt", "die", "gold\u00b7ne", "re\u00b7gel", "stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die wir beyn ", "tokens": ["Die", "wir", "beyn"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Denn es trifft richtig ein:", "tokens": ["Denn", "es", "trifft", "rich\u00b7tig", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Wenn zwey ", "tokens": ["Wenn", "zwey"], "token_info": ["word", "word"], "pos": ["KOUS", "CARD"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "So mu\u00df es da, wo weise leute leben,", "tokens": ["So", "mu\u00df", "es", "da", ",", "wo", "wei\u00b7se", "leu\u00b7te", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nothwendig auch viel thoren geben.", "tokens": ["Noth\u00b7wen\u00b7dig", "auch", "viel", "tho\u00b7ren", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}