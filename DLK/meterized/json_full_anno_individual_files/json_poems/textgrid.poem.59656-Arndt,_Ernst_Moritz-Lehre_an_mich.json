{"textgrid.poem.59656": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Lehre an mich", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! Lege deiner Jugend Harnisch an!", "tokens": ["Auf", "!", "Le\u00b7ge", "dei\u00b7ner", "Ju\u00b7gend", "Har\u00b7nisch", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "PPOSAT", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schnalle um auch deine Rittersporen!", "tokens": ["Und", "schnal\u00b7le", "um", "auch", "dei\u00b7ne", "Rit\u00b7ter\u00b7spo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Gl\u00fcck? Sein R\u00e4dlein rollet ab und an;", "tokens": ["Was", "Gl\u00fcck", "?", "Sein", "R\u00e4d\u00b7lein", "rol\u00b7let", "ab", "und", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was Ruhm? Ein dunst'ges Gaukelbild f\u00fcr Toren.", "tokens": ["Was", "Ruhm", "?", "Ein", "dunst'\u00b7ges", "Gau\u00b7kel\u00b7bild", "f\u00fcr", "To\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was bunter Tand, wonach die Menge greift?", "tokens": ["Was", "bun\u00b7ter", "Tand", ",", "wo\u00b7nach", "die", "Men\u00b7ge", "greift", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was Gold und Glanz und Titelklang und Orden?", "tokens": ["Was", "Gold", "und", "Glanz", "und", "Ti\u00b7tel\u00b7klang", "und", "Or\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du greife das, was nicht wie Zufall schweift,", "tokens": ["Du", "grei\u00b7fe", "das", ",", "was", "nicht", "wie", "Zu\u00b7fall", "schweift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "PTKNEG", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du halte fest, was kein Tyrann kann morden.", "tokens": ["Du", "hal\u00b7te", "fest", ",", "was", "kein", "Ty\u00b7rann", "kann", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PRELS", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was du in strenger Arbeit dir erwarbst,", "tokens": ["Was", "du", "in", "stren\u00b7ger", "Ar\u00b7beit", "dir", "er\u00b7warbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was du im schweren Kampfe dir errungen,", "tokens": ["Was", "du", "im", "schwe\u00b7ren", "Kamp\u00b7fe", "dir", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wodurch du reich sein wirst, auch wenn du darbst,", "tokens": ["Wo\u00b7durch", "du", "reich", "sein", "wirst", ",", "auch", "wenn", "du", "darbst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAINF", "VAFIN", "$,", "ADV", "KOUS", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und siegreich, wenn dich auch Gewalt bezwungen \u2013", "tokens": ["Und", "sieg\u00b7reich", ",", "wenn", "dich", "auch", "Ge\u00b7walt", "be\u00b7zwun\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "ADV", "NN", "VVPP", "$("], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Das zarte Unsichtbare such' hervor,", "tokens": ["Das", "zar\u00b7te", "Un\u00b7sicht\u00b7ba\u00b7re", "such'", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das d\u00fcnne F\u00fcnkchen aus der G\u00f6tterflamme,", "tokens": ["Das", "d\u00fcn\u00b7ne", "F\u00fcnk\u00b7chen", "aus", "der", "G\u00f6t\u00b7ter\u00b7flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und jauchze: Zittre, Bube! Zittre, Tor!", "tokens": ["Und", "jauch\u00b7ze", ":", "Zitt\u00b7re", ",", "Bu\u00b7be", "!", "Zitt\u00b7re", ",", "Tor", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "$,", "NN", "$.", "NE", "$,", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dies ist's, wodurch ich dir dein Nichts verdamme.", "tokens": ["Dies", "ist's", ",", "wo\u00b7durch", "ich", "dir", "dein", "Nichts", "ver\u00b7dam\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dies ist's, worauf die ganze Erdenlast,", "tokens": ["Dies", "ist's", ",", "wo\u00b7rauf", "die", "gan\u00b7ze", "Er\u00b7den\u00b7last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wirfst du sie drauf, nur liegt und nimmer dr\u00fccket,", "tokens": ["Wirfst", "du", "sie", "drauf", ",", "nur", "liegt", "und", "nim\u00b7mer", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Unsichtbare, was Gewalt nicht fa\u00dft,", "tokens": ["Das", "Un\u00b7sicht\u00b7ba\u00b7re", ",", "was", "Ge\u00b7walt", "nicht", "fa\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und, fa\u00dfte sie's, nicht von der Stelle r\u00fccket,", "tokens": ["Und", ",", "fa\u00df\u00b7te", "sie's", ",", "nicht", "von", "der", "Stel\u00b7le", "r\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Starke, was den bittern Feind, den Tod,", "tokens": ["Das", "Star\u00b7ke", ",", "was", "den", "bit\u00b7tern", "Feind", ",", "den", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit allen seinen Schrecken selbst mag t\u00f6ten,", "tokens": ["Mit", "al\u00b7len", "sei\u00b7nen", "Schre\u00b7cken", "selbst", "mag", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Frohe, was mit hellem Morgenrot", "tokens": ["Das", "Fro\u00b7he", ",", "was", "mit", "hel\u00b7lem", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des Ungl\u00fccks dickste Wetternacht mag r\u00f6ten.", "tokens": ["Des", "Un\u00b7gl\u00fccks", "dicks\u00b7te", "Wet\u00b7ter\u00b7nacht", "mag", "r\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dies nimm dir! Ruf auch die Gesellen auf,", "tokens": ["Dies", "nimm", "dir", "!", "Ruf", "auch", "die", "Ge\u00b7sel\u00b7len", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gespielen und Genossen tapfrer Jugend,", "tokens": ["Ge\u00b7spie\u00b7len", "und", "Ge\u00b7nos\u00b7sen", "tapf\u00b7rer", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die in der ernsten Arbeit dir den Lauf", "tokens": ["Die", "in", "der", "erns\u00b7ten", "Ar\u00b7beit", "dir", "den", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gestrecket auf der hei\u00dfen Bahn der Tugend:", "tokens": ["Ge\u00b7stre\u00b7cket", "auf", "der", "hei\u00b7\u00dfen", "Bahn", "der", "Tu\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durch das, was zornig schon den Knaben ri\u00df", "tokens": ["Durch", "das", ",", "was", "zor\u00b7nig", "schon", "den", "Kna\u00b7ben", "ri\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "$,", "PRELS", "ADJD", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hinweg vom Tand, wonach's die vielen l\u00fcstet;", "tokens": ["Hin\u00b7weg", "vom", "Tand", ",", "wo\u00b7nach's", "die", "vie\u00b7len", "l\u00fcs\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Steh nun als Mann im Sturm und Streit gewi\u00df,", "tokens": ["Steh", "nun", "als", "Mann", "im", "Sturm", "und", "Streit", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "NN", "APPRART", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf! Waffne deine Schar und sei ger\u00fcstet!", "tokens": ["Auf", "!", "Waff\u00b7ne", "dei\u00b7ne", "Schar", "und", "sei", "ge\u00b7r\u00fcs\u00b7tet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "PPOSAT", "NN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O sieh! Schon steht dein tapfrer Wappenknecht,", "tokens": ["O", "sieh", "!", "Schon", "steht", "dein", "tapf\u00b7rer", "Wap\u00b7pen\u00b7knecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der edle Stolz, und zucket mit dem Eisen;", "tokens": ["Der", "ed\u00b7le", "Stolz", ",", "und", "zu\u00b7cket", "mit", "dem", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drei Helfer sitzen auf, der Mut, das Recht,", "tokens": ["Drei", "Hel\u00b7fer", "sit\u00b7zen", "auf", ",", "der", "Mut", ",", "das", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Licht \u2013 sie wollen sich die Alten weisen;", "tokens": ["Das", "Licht", "\u2013", "sie", "wol\u00b7len", "sich", "die", "Al\u00b7ten", "wei\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Wahrheit tr\u00e4gt das leuchtende Panier,", "tokens": ["Die", "Wahr\u00b7heit", "tr\u00e4gt", "das", "leuch\u00b7ten\u00b7de", "Pa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Hoffnung schwingt die fliegende Standarte;", "tokens": ["Die", "Hoff\u00b7nung", "schwingt", "die", "flie\u00b7gen\u00b7de", "Stand\u00b7ar\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Auch unsichtbare K\u00e4mpfer folgen dir;", "tokens": ["Auch", "un\u00b7sicht\u00b7ba\u00b7re", "K\u00e4mp\u00b7fer", "fol\u00b7gen", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gebet und Wunsch sind H\u00fcter auf der Warte.", "tokens": ["Ge\u00b7bet", "und", "Wunsch", "sind", "H\u00fc\u00b7ter", "auf", "der", "War\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit solchen mutig drein auf Sieg und Tod!", "tokens": ["Mit", "sol\u00b7chen", "mu\u00b7tig", "drein", "auf", "Sieg", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es gilt, was Freien ziemlich sei, was Knechten;", "tokens": ["Es", "gilt", ",", "was", "Frei\u00b7en", "ziem\u00b7lich", "sei", ",", "was", "Knech\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "NN", "ADV", "VAFIN", "$,", "PWS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur einen Jammer gibt's, nur eine Not,", "tokens": ["Nur", "ei\u00b7nen", "Jam\u00b7mer", "gibt's", ",", "nur", "ei\u00b7ne", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr nichts und schlimmer gar f\u00fcr Frevel fechten.", "tokens": ["F\u00fcr", "nichts", "und", "schlim\u00b7mer", "gar", "f\u00fcr", "Fre\u00b7vel", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "ADJD", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hinein mit Gott! Dein kleines Schicksal rollt", "tokens": ["Hin\u00b7ein", "mit", "Gott", "!", "Dein", "klei\u00b7nes", "Schick\u00b7sal", "rollt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus seiner Hand mit Millionen Losen.", "tokens": ["Aus", "sei\u00b7ner", "Hand", "mit", "Mil\u00b7lion\u00b7en", "Lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punc