{"dta.poem.20513": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der himmel pflantzet mein gel\u00fccke/", "tokens": ["Der", "him\u00b7mel", "pflant\u00b7zet", "mein", "ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er lacht mich freundlich an durch tausend holde blicke/", "tokens": ["Er", "lacht", "mich", "freund\u00b7lich", "an", "durch", "tau\u00b7send", "hol\u00b7de", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "APPR", "CARD", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er macht aus winter fr\u00fchlings-zeit/", "tokens": ["Er", "macht", "aus", "win\u00b7ter", "fr\u00fch\u00b7lings\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wirckt mir selber zeug zu einem feyer-kleide/", "tokens": ["Er", "wirckt", "mir", "sel\u00b7ber", "zeug", "zu", "ei\u00b7nem", "feyer\u00b7klei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich bin von boy und flohr befreyt/", "tokens": ["Ich", "bin", "von", "boy", "und", "flohr", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "KON", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und meine wolle wird zur seide.", "tokens": ["Und", "mei\u00b7ne", "wol\u00b7le", "wird", "zur", "sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kan den port itzt recht erreichen/", "tokens": ["Ich", "kan", "den", "port", "itzt", "recht", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NE", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und darff nicht um das haupt der leeren hoffnung streichen/", "tokens": ["Und", "darff", "nicht", "um", "das", "haupt", "der", "lee\u00b7ren", "hoff\u00b7nung", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein ancker sinckt in s\u00fcsse ruh/", "tokens": ["Mein", "an\u00b7cker", "sinckt", "in", "s\u00fcs\u00b7se", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein auge hat mir selbst ein leit-stern werden m\u00fcssen/", "tokens": ["Dein", "au\u00b7ge", "hat", "mir", "selbst", "ein", "leit\u00b7stern", "wer\u00b7den", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ART", "ADJA", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja/ mein gelobtes land bist du/", "tokens": ["Ja", "/", "mein", "ge\u00b7lob\u00b7tes", "land", "bist", "du", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df mich das vorgeb\u00fcrge k\u00fcssen.", "tokens": ["La\u00df", "mich", "das", "vor\u00b7ge\u00b7b\u00fcr\u00b7ge", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlag doch nicht mehr die augen nieder/", "tokens": ["Schlag", "doch", "nicht", "mehr", "die", "au\u00b7gen", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist denn mein reiner schertz/ Rosette/ dir zu zuwider?", "tokens": ["Ist", "denn", "mein", "rei\u00b7ner", "schertz", "/", "Ro\u00b7set\u00b7te", "/", "dir", "zu", "zu\u00b7wi\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$(", "NE", "$(", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich bin dir ja nicht unbekandt/", "tokens": ["Ich", "bin", "dir", "ja", "nicht", "un\u00b7be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du kennest mein gesicht/ und auch mein treues hertze/", "tokens": ["Du", "ken\u00b7nest", "mein", "ge\u00b7sicht", "/", "und", "auch", "mein", "treu\u00b7es", "hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVPP", "$(", "KON", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum glaube/ da\u00df der liebe brand", "tokens": ["Drum", "glau\u00b7be", "/", "da\u00df", "der", "lie\u00b7be", "brand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich st\u00e4rcket zwischen freud und schertze.", "tokens": ["Sich", "st\u00e4r\u00b7cket", "zwi\u00b7schen", "freud", "und", "schert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "NE", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wilst du dich der natur entreissen?", "tokens": ["Wilst", "du", "dich", "der", "na\u00b7tur", "en\u00b7treis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Di\u00df kan die tugend selbst nicht eine tugend heissen/", "tokens": ["Di\u00df", "kan", "die", "tu\u00b7gend", "selbst", "nicht", "ei\u00b7ne", "tu\u00b7gend", "heis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das sch\u00f6ne blumwerck deiner brust", "tokens": ["Das", "sch\u00f6\u00b7ne", "blum\u00b7werck", "dei\u00b7ner", "brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht vor dich allein auff diese welt gebohren/", "tokens": ["Ist", "nicht", "vor", "dich", "al\u00b7lein", "auff", "die\u00b7se", "welt", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat es auch zu meiner lust", "tokens": ["Es", "hat", "es", "auch", "zu", "mei\u00b7ner", "lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des himmels ausspruch au\u00dferkohren.", "tokens": ["Des", "him\u00b7mels", "aus\u00b7spruch", "au\u00b7\u00dfer\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du must in dir nicht selbst verwesen/", "tokens": ["Du", "must", "in", "dir", "nicht", "selbst", "ver\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df mich um deinen mund die zucker-rosen lesen.", "tokens": ["La\u00df", "mich", "um", "dei\u00b7nen", "mund", "die", "zu\u00b7cke\u00b7rro\u00b7sen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch einen unverwehrten ku\u00df/", "tokens": ["Durch", "ei\u00b7nen", "un\u00b7ver\u00b7wehr\u00b7ten", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df doch den s\u00fcssen thau auff meine lippen rinnen/", "tokens": ["La\u00df", "doch", "den", "s\u00fcs\u00b7sen", "thau", "auff", "mei\u00b7ne", "lip\u00b7pen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df durch verliebten \u00fcberflu\u00df", "tokens": ["Da\u00df", "durch", "ver\u00b7lieb\u00b7ten", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die geister selbst sich k\u00fcssen k\u00f6nnen.", "tokens": ["Die", "geis\u00b7ter", "selbst", "sich", "k\u00fcs\u00b7sen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}