{"textgrid.poem.30255": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist, Cypressen-Baum, ein Baum gerader H\u00f6he,", "tokens": ["Du", "bist", ",", "Cy\u00b7pres\u00b7sen\u00b7Baum", ",", "ein", "Baum", "ge\u00b7ra\u00b7der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NE", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dran aber niemand sah, da\u00df sondre Fr\u00fccht viel stehe.", "tokens": ["Dran", "a\u00b7ber", "nie\u00b7mand", "sah", ",", "da\u00df", "sond\u00b7re", "Fr\u00fccht", "viel", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIS", "VVFIN", "$,", "KOUS", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Brauch war sonst nicht gro\u00df, als da\u00df man dich gebraucht,", "tokens": ["Dein", "Brauch", "war", "sonst", "nicht", "gro\u00df", ",", "als", "da\u00df", "man", "dich", "ge\u00b7braucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,", "KOKOM", "KOUS", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann weiland eine Leich im Feuer hat geraucht.", "tokens": ["Wann", "wei\u00b7land", "ei\u00b7ne", "Leich", "im", "Feu\u00b7er", "hat", "ge\u00b7raucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was hat der deutsche Krieg, der sich so lang erstrecket,", "tokens": ["Was", "hat", "der", "deut\u00b7sche", "Krieg", ",", "der", "sich", "so", "lang", "er\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Fr\u00fcchten und von Nutz doch immer au\u00dfgehecket?", "tokens": ["Von", "Fr\u00fcch\u00b7ten", "und", "von", "Nutz", "doch", "im\u00b7mer", "au\u00df\u00b7ge\u00b7he\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er wuchs und wuchs f\u00fcr sich: hat aber den Entgelt,", "tokens": ["Er", "wuchs", "und", "wuchs", "f\u00fcr", "sich", ":", "hat", "a\u00b7ber", "den", "Ent\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PRF", "$.", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df er dem deutschen Preis den Leichendienst bestellt.", "tokens": ["Da\u00df", "er", "dem", "deut\u00b7schen", "Preis", "den", "Lei\u00b7chen\u00b7dienst", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "