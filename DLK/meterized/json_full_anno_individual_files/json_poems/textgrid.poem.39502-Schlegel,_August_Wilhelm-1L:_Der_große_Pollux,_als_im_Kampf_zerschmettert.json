{"textgrid.poem.39502": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der gro\u00dfe Pollux, als im Kampf zerschmettert", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gro\u00dfe Pollux, als im Kampf zerschmettert", "tokens": ["Der", "gro\u00b7\u00dfe", "Pol\u00b7lux", ",", "als", "im", "Kampf", "zer\u00b7schmet\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kastor, der sch\u00f6ne, lag vor seinen F\u00fc\u00dfen,", "tokens": ["Kas\u00b7tor", ",", "der", "sch\u00f6\u00b7ne", ",", "lag", "vor", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rief, sein unsterblich Leben zu beschlie\u00dfen,", "tokens": ["Rief", ",", "sein", "uns\u00b7terb\u00b7lich", "Le\u00b7ben", "zu", "be\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Vaters Rechte, die aus Wolken wettert.", "tokens": ["Des", "Va\u00b7ters", "Rech\u00b7te", ",", "die", "aus", "Wol\u00b7ken", "wet\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zeus ordnet, da\u00df sie, jener halb verg\u00f6ttert,", "tokens": ["Zeus", "ord\u00b7net", ",", "da\u00df", "sie", ",", "je\u00b7ner", "halb", "ver\u00b7g\u00f6t\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPER", "$,", "PDAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Halb Schatte der, ihr wechselnd Loo\u00df vers\u00fc\u00dfen.", "tokens": ["Halb", "Schat\u00b7te", "der", ",", "ihr", "wech\u00b7selnd", "Loo\u00df", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "$,", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Wunderliebe, die mit holden Gr\u00fc\u00dfen", "tokens": ["O", "Wun\u00b7der\u00b7lie\u00b7be", ",", "die", "mit", "hol\u00b7den", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Orkus aufhellt, den Olymp entg\u00f6ttert!", "tokens": ["Den", "Or\u00b7kus", "auf\u00b7hellt", ",", "den", "O\u00b7lymp", "ent\u00b7g\u00f6t\u00b7tert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du Zwillingssohn des Himmels und der Erden,", "tokens": ["Du", "Zwil\u00b7lings\u00b7sohn", "des", "Him\u00b7mels", "und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Mensch! ", "tokens": ["O", "Mensch", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Dem Sterblichen in Lust wie in Beschwerden.", "tokens": ["Dem", "Sterb\u00b7li\u00b7chen", "in", "Lust", "wie", "in", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dir ist nur so der Stand der G\u00f6tter erblich;", "tokens": ["Dir", "ist", "nur", "so", "der", "Stand", "der", "G\u00f6t\u00b7ter", "er\u00b7blich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So wird dein Ew'ges sichtbar lieblich werden,", "tokens": ["So", "wird", "dein", "Ew'\u00b7ges", "sicht\u00b7bar", "lieb\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Ird'sches rein, stark, w\u00fcrdig, unverderblich.", "tokens": ["Dein", "Ird'\u00b7sches", "rein", ",", "stark", ",", "w\u00fcr\u00b7dig", ",", "un\u00b7ver\u00b7derb\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der gro\u00dfe Pollux, als im Kampf zerschmettert", "tokens": ["Der", "gro\u00b7\u00dfe", "Pol\u00b7lux", ",", "als", "im", "Kampf", "zer\u00b7schmet\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word