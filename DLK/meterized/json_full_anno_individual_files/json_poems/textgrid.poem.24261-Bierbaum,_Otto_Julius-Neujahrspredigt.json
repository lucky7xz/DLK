{"textgrid.poem.24261": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrspredigt", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns, Freunde, ins neue Jahr", "tokens": ["La\u00dft", "uns", ",", "Freun\u00b7de", ",", "ins", "neu\u00b7e", "Jahr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eingehn wie in ein sch\u00f6nes, gesichertes Haus,", "tokens": ["Ein\u00b7gehn", "wie", "in", "ein", "sch\u00f6\u00b7nes", ",", "ge\u00b7si\u00b7cher\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In dem die Liebe und der Friede wohnt", "tokens": ["In", "dem", "die", "Lie\u00b7be", "und", "der", "Frie\u00b7de", "wohnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Sch\u00f6nheit \u00fcberall heimisch ist.", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "\u00fc\u00b7be\u00b7rall", "hei\u00b7misch", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und la\u00dft uns, Freunde, heiter gelassenen Sinns,", "tokens": ["Und", "la\u00dft", "uns", ",", "Freun\u00b7de", ",", "hei\u00b7ter", "ge\u00b7las\u00b7se\u00b7nen", "Sinns", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit keinem Ha\u00df belastet, ohne Neid,", "tokens": ["Mit", "kei\u00b7nem", "Ha\u00df", "be\u00b7las\u00b7tet", ",", "oh\u00b7ne", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heil, liebe Freunde, im starken Herzen, la\u00dft uns", "tokens": ["Heil", ",", "lie\u00b7be", "Freun\u00b7de", ",", "im", "star\u00b7ken", "Her\u00b7zen", ",", "la\u00dft", "uns"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,", "VVIMP", "PPER"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "In dieses neue Haus einziehn, und lachend.", "tokens": ["In", "die\u00b7ses", "neu\u00b7e", "Haus", "ein\u00b7ziehn", ",", "und", "la\u00b7chend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVINF", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir sind wohl keiner wundenlos, unversehrt,", "tokens": ["Wir", "sind", "wohl", "kei\u00b7ner", "wun\u00b7den\u00b7los", ",", "un\u00b7ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jeder sp\u00fcrte, da\u00df Niedertr\u00e4chtigkeit", "tokens": ["Und", "je\u00b7der", "sp\u00fcr\u00b7te", ",", "da\u00df", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sehnenkr\u00e4ftige Bogen und giftige Pfeile hat,", "tokens": ["Seh\u00b7nen\u00b7kr\u00e4f\u00b7ti\u00b7ge", "Bo\u00b7gen", "und", "gif\u00b7ti\u00b7ge", "Pfei\u00b7le", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und da\u00df der Dummheit Kartaunen nicht blo\u00df br\u00fcllen,", "tokens": ["Und", "da\u00df", "der", "Dumm\u00b7heit", "Kar\u00b7tau\u00b7nen", "nicht", "blo\u00df", "br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sondern auch vieles zerst\u00f6ren k\u00f6nnen, das", "tokens": ["Son\u00b7dern", "auch", "vie\u00b7les", "zer\u00b7st\u00f6\u00b7ren", "k\u00f6n\u00b7nen", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "PIS", "VVINF", "VMINF", "$,", "PRELS"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Mit M\u00fche und Kunst errichtet ward, \u2013 und, ach,", "tokens": ["Mit", "M\u00fc\u00b7he", "und", "Kunst", "er\u00b7rich\u00b7tet", "ward", ",", "\u2013", "und", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VAFIN", "$,", "$(", "KON", "$,", "ITJ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Des Schlimmsten wurden wir uns wohl auch bewu\u00dft,", "tokens": ["Des", "Schlimms\u00b7ten", "wur\u00b7den", "wir", "uns", "wohl", "auch", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df Schwachheit unser Teil ist und irgendwo", "tokens": ["Da\u00df", "Schwach\u00b7heit", "un\u00b7ser", "Teil", "ist", "und", "ir\u00b7gend\u00b7wo"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VAFIN", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Jeder, wie fest er gef\u00fcgt sich d\u00fcnke,", "tokens": ["Je\u00b7der", ",", "wie", "fest", "er", "ge\u00b7f\u00fcgt", "sich", "d\u00fcn\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Locker und undicht ist im Baue.", "tokens": ["Lo\u00b7cker", "und", "un\u00b7dicht", "ist", "im", "Bau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Das aber, Freunde, fechte uns nicht an!", "tokens": ["Das", "a\u00b7ber", ",", "Freun\u00b7de", ",", "fech\u00b7te", "uns", "nicht", "an", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir wollen tapfer sein und, gilts Gefecht,", "tokens": ["Wir", "wol\u00b7len", "tap\u00b7fer", "sein", "und", ",", "gilts", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "KON", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Lachen in den Feind gehn, da wir ja", "tokens": ["Mit", "La\u00b7chen", "in", "den", "Feind", "gehn", ",", "da", "wir", "ja"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Edle k\u00e4mpfen und dem Tro\u00df voran", "tokens": ["Als", "Ed\u00b7le", "k\u00e4mp\u00b7fen", "und", "dem", "Tro\u00df", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "VVINF", "KON", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als Wissende: Es ist die Kraft in uns,", "tokens": ["Als", "Wis\u00b7sen\u00b7de", ":", "Es", "ist", "die", "Kraft", "in", "uns", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Allein zu stehn, gemeiner Art entr\u00fcckt.", "tokens": ["Al\u00b7lein", "zu", "stehn", ",", "ge\u00b7mei\u00b7ner", "Art", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn aber Dumpfheit alles niederschl\u00e4gt", "tokens": ["Wenn", "a\u00b7ber", "Dum\u00b7pf\u00b7heit", "al\u00b7les", "nie\u00b7der\u00b7schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "PIS", "VVFIN"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Und Kampf nicht lohnt und Widerwillen uns", "tokens": ["Und", "Kampf", "nicht", "lohnt", "und", "Wi\u00b7der\u00b7wil\u00b7len", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PTKNEG", "VVFIN", "KON", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Erfassen will, so wollen wir, Freunde, nicht", "tokens": ["Er\u00b7fas\u00b7sen", "will", ",", "so", "wol\u00b7len", "wir", ",", "Freun\u00b7de", ",", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "$,", "NN", "$,", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Mit Tr\u00fcbsal abziehn, sondern heiter", "tokens": ["Mit", "Tr\u00fcb\u00b7sal", "ab\u00b7ziehn", ",", "son\u00b7dern", "hei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVIZU", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das Schwert der Scheide schenken und mit Gesang", "tokens": ["Das", "Schwert", "der", "Schei\u00b7de", "schen\u00b7ken", "und", "mit", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "KON", "APPR", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Den Schritt wegwenden in die Einsamkeit.", "tokens": ["Den", "Schritt", "weg\u00b7wen\u00b7den", "in", "die", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dies, liebe Freunde, ist nach meinem Sinn", "tokens": ["Dies", ",", "lie\u00b7be", "Freun\u00b7de", ",", "ist", "nach", "mei\u00b7nem", "Sinn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "ADJA", "NN", "$,", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht das Beste, das das Jahr bescheren mag:", "tokens": ["Viel\u00b7leicht", "das", "Bes\u00b7te", ",", "das", "das", "Jahr", "be\u00b7sche\u00b7ren", "mag", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verborgenheit und Ruhe in uns selbst.", "tokens": ["Ver\u00b7bor\u00b7gen\u00b7heit", "und", "Ru\u00b7he", "in", "uns", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohl dem, der dies erf\u00e4hrt, doch selig der", "tokens": ["Wohl", "dem", ",", "der", "dies", "er\u00b7f\u00e4hrt", ",", "doch", "se\u00b7lig", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PDS", "VVFIN", "$,", "ADV", "ADJD", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(wie selig, wei\u00df ich, der es nun erfuhr),", "tokens": ["(", "wie", "se\u00b7lig", ",", "wei\u00df", "ich", ",", "der", "es", "nun", "er\u00b7fuhr", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "$,", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der nicht allein in dieses sch\u00f6ne Haus", "tokens": ["Der", "nicht", "al\u00b7lein", "in", "die\u00b7ses", "sch\u00f6\u00b7ne", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gelassener Beschaulichkeit zu gehen braucht.", "tokens": ["Ge\u00b7las\u00b7se\u00b7ner", "Be\u00b7schau\u00b7lich\u00b7keit", "zu", "ge\u00b7hen", "braucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In Einsamkeit vereint, das ist mein Spruch,", "tokens": ["In", "Ein\u00b7sam\u00b7keit", "ver\u00b7eint", ",", "das", "ist", "mein", "Spruch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und dies mein Wunsch, da\u00df jeder, der es wert,", "tokens": ["Und", "dies", "mein", "Wunsch", ",", "da\u00df", "je\u00b7der", ",", "der", "es", "wert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PPOSAT", "NN", "$,", "KOUS", "PIS", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Voll aus, bis auf den Grund ausf\u00fchlen m\u00f6ge, welch ein Gl\u00fcck", "tokens": ["Voll", "aus", ",", "bis", "auf", "den", "Grund", "aus\u00b7f\u00fch\u00b7len", "m\u00f6\u00b7ge", ",", "welch", "ein", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Dies Wort umschlie\u00dft: In Einsamkeit vereint.", "tokens": ["Dies", "Wort", "um\u00b7schlie\u00dft", ":", "In", "Ein\u00b7sam\u00b7keit", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "$.", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "La\u00dft uns, Freunde, ins neue Jahr", "tokens": ["La\u00dft", "uns", ",", "Freun\u00b7de", ",", "ins", "neu\u00b7e", "Jahr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eingehn wie in ein sch\u00f6nes, gesichertes Haus,", "tokens": ["Ein\u00b7gehn", "wie", "in", "ein", "sch\u00f6\u00b7nes", ",", "ge\u00b7si\u00b7cher\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In dem die Liebe und der Friede wohnt", "tokens": ["In", "dem", "die", "Lie\u00b7be", "und", "der", "Frie\u00b7de", "wohnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Sch\u00f6nheit \u00fcberall heimisch ist.", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "\u00fc\u00b7be\u00b7rall", "hei\u00b7misch", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Und la\u00dft uns, Freunde, heiter gelassenen Sinns,", "tokens": ["Und", "la\u00dft", "uns", ",", "Freun\u00b7de", ",", "hei\u00b7ter", "ge\u