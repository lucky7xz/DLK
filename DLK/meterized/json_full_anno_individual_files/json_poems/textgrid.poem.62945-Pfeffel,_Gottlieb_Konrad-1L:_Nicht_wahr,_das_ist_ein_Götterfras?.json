{"textgrid.poem.62945": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht wahr, das ist ein G\u00f6tterfras?", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht wahr, das ist ein G\u00f6tterfras?", "tokens": ["Nicht", "wahr", ",", "das", "ist", "ein", "G\u00f6t\u00b7ter\u00b7fras", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach Ritter Klaus beym Vogelstellen", "tokens": ["Sprach", "Rit\u00b7ter", "Klaus", "beym", "Vo\u00b7gel\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu seinem edeln Weidgesellen,", "tokens": ["Zu", "sei\u00b7nem", "e\u00b7deln", "Weid\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der mit ihm auf bethautem Gras", "tokens": ["Der", "mit", "ihm", "auf", "be\u00b7thau\u00b7tem", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein paar gebratne Schnepfen as.", "tokens": ["Ein", "paar", "ge\u00b7brat\u00b7ne", "Schnep\u00b7fen", "as", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieh, Bruder, blos dem Dr ... zu Liebe \u2013", "tokens": ["Sieh", ",", "Bru\u00b7der", ",", "blos", "dem", "Dr", "...", "zu", "Lie\u00b7be", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "ART", "NN", "$(", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich schreibe dieses Wort nicht aus;", "tokens": ["Ich", "schrei\u00b7be", "die\u00b7ses", "Wort", "nicht", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Weidmann braucht es, und ich schiebe", "tokens": ["Der", "Weid\u00b7mann", "braucht", "es", ",", "und", "ich", "schie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Den Unfug auf den Ritter Klaus. \u2013", "tokens": ["Den", "Un\u00b7fug", "auf", "den", "Rit\u00b7ter", "Klaus", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er sagte: blos dem Dr ... zu Liebe", "tokens": ["Er", "sag\u00b7te", ":", "blos", "dem", "Dr", "...", "zu", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ART", "NN", "$(", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Schie\u00df ich auf dieses Rabenvieh.", "tokens": ["Schie\u00df", "ich", "auf", "die\u00b7ses", "Ra\u00b7ben\u00b7vieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In einem