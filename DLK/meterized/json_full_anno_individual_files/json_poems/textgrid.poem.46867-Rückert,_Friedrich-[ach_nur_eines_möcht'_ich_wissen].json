{"textgrid.poem.46867": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach nur eines m\u00f6cht' ich wissen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach nur eines m\u00f6cht' ich wissen,", "tokens": ["Ach", "nur", "ei\u00b7nes", "m\u00f6cht'", "ich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sie dort von uns noch wissen,", "tokens": ["Ob", "sie", "dort", "von", "uns", "noch", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder davon Kunde missen,", "tokens": ["O\u00b7der", "da\u00b7von", "Kun\u00b7de", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wir schmerzlich sie vermissen,", "tokens": ["Wie", "wir", "schmerz\u00b7lich", "sie", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Herzen sind zerrissen,", "tokens": ["Wie", "die", "Her\u00b7zen", "sind", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denen sie nun sind entrissen.", "tokens": ["De\u00b7nen", "sie", "nun", "sind", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Nein, es soll sie dort nicht schmerzen,", "tokens": ["Nein", ",", "es", "soll", "sie", "dort", "nicht", "schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nachzuf\u00fchlen unsre Schmerzen;", "tokens": ["Nach\u00b7zu\u00b7f\u00fch\u00b7len", "uns\u00b7re", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch da\u00df sie an unsern Herzen", "tokens": ["Doch", "da\u00df", "sie", "an", "un\u00b7sern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Lagen, und sich lie\u00dfen herzen", "tokens": ["La\u00b7gen", ",", "und", "sich", "lie\u00b7\u00dfen", "her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PRF", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Vater-, Mutter- Scherzen,", "tokens": ["Un\u00b7ter", "Va\u00b7ter", ",", "Mut\u00b7ter", "Scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "$,", "TRUNC", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das soll sie im Traum umscherzen.", "tokens": ["Das", "soll", "sie", "im", "Traum", "um\u00b7scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN",