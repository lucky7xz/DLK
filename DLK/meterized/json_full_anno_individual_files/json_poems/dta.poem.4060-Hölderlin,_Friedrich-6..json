{"dta.poem.4060": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst mir anders bekannt! o Jugend! und bringen", "tokens": ["Sonst", "mir", "an\u00b7ders", "be\u00b7kannt", "!", "o", "Ju\u00b7gend", "!", "und", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ADJD", "$.", "FM", "NN", "$.", "KON", "VVINF"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gebete,", "tokens": ["Ge\u00b7be\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dich nicht wieder, Dich nie? f\u00fchret kein Pfad", "tokens": ["Dich", "nicht", "wie\u00b7der", ",", "Dich", "nie", "?", "f\u00fch\u00b7ret", "kein", "Pfad"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "ADV", "$,", "PPER", "ADV", "$.", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "mich zur\u00fcck?", "tokens": ["mich", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Soll es werden auch mir, wie den G\u00f6tterlosen,", "tokens": ["Soll", "es", "wer\u00b7den", "auch", "mir", ",", "wie", "den", "G\u00f6t\u00b7ter\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VAFIN", "ADV", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "die vormals", "tokens": ["die", "vor\u00b7mals"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Gl\u00e4nzenden Auges doch auch sa\u00dfen am seligen", "tokens": ["Gl\u00e4n\u00b7zen\u00b7den", "Au\u00b7ges", "doch", "auch", "sa\u00b7\u00dfen", "am", "se\u00b7li\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "ADV", "VVFIN", "APPRART", "ADJA"], "meter": "+--+-+-+-++--", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Tisch,", "tokens": ["Tisch", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Aber \u00fcbers\u00e4ttiget bald, die schw\u00e4rmenden G\u00e4ste,", "tokens": ["A\u00b7ber", "\u00fc\u00b7bers\u00b7\u00e4t\u00b7ti\u00b7get", "bald", ",", "die", "schw\u00e4r\u00b7men\u00b7den", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Nun verstummet, und nun, unter der L\u00fcste Ge-", "tokens": ["Nun", "ver\u00b7stum\u00b7met", ",", "und", "nun", ",", "un\u00b7ter", "der", "L\u00fcs\u00b7te", "Ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "sang,", "tokens": ["sang", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Unter bl\u00fchender Erd' entschlafen sind, bis dereinst", "tokens": ["Un\u00b7ter", "bl\u00fc\u00b7hen\u00b7der", "Erd'", "ent\u00b7schla\u00b7fen", "sind", ",", "bis", "de\u00b7reinst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$,", "KOUS", "ADV"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "sie", "tokens": ["sie"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Eines Wunders Gewalt, sie, die Versunkenen,", "tokens": ["Ei\u00b7nes", "Wun\u00b7ders", "Ge\u00b7walt", ",", "sie", ",", "die", "Ver\u00b7sun\u00b7ke\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "zwingt,", "tokens": ["zwingt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Wiederzukehren und neu auf gr\u00fcnendem Boden", "tokens": ["Wie\u00b7der\u00b7zu\u00b7keh\u00b7ren", "und", "neu", "auf", "gr\u00fc\u00b7nen\u00b7dem", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "zu wandeln. \u2014", "tokens": ["zu", "wan\u00b7deln", "."], "token_info": ["word", "word", "punct", "punct"], "pos": ["PTKZU", "VVFIN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Heiliger Odem durchstr\u00f6mt g\u00f6ttlich die lichte", "tokens": ["Hei\u00b7li\u00b7ger", "O\u00b7dem", "durch\u00b7str\u00f6mt", "g\u00f6tt\u00b7lich", "die", "lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "Gestalt,", "tokens": ["Ge\u00b7stalt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Wenn das Fest sich beseelt, und Fluten der Liebe", "tokens": ["Wenn", "das", "Fest", "sich", "be\u00b7seelt", ",", "und", "Flu\u00b7ten", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVPP", "$,", "KON", "NN", "ART", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "sich regen,", "tokens": ["sich", "re\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Und vom Himmel getr\u00e4nkt, rauscht der lebendige", "tokens": ["Und", "vom", "Him\u00b7mel", "ge\u00b7tr\u00e4nkt", ",", "rauscht", "der", "le\u00b7ben\u00b7di\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,", "VVFIN", "ART", "ADJA"], "meter": "--+--++-++-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Strom,", "tokens": ["Strom", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Wenn es drunten ert\u00f6nt, und ihre Sch\u00e4tze die", "tokens": ["Wenn", "es", "drun\u00b7ten", "er\u00b7t\u00f6nt", ",", "und", "ih\u00b7re", "Sch\u00e4t\u00b7ze", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "KON", "PPOSAT", "NN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Nacht zollt,", "tokens": ["Nacht", "zollt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Und aus B\u00e4chen herauf gl\u00e4nzt das begrabene", "tokens": ["Und", "aus", "B\u00e4\u00b7chen", "her\u00b7auf", "gl\u00e4nzt", "das", "be\u00b7gra\u00b7be\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV", "VVFIN", "ART", "ADJA"], "meter": "--+---+--+--", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Gold.", "tokens": ["Gold", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}