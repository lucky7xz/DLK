{"textgrid.poem.64332": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du heiliges und weites Meer,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du heiliges und weites Meer,", "tokens": ["Du", "hei\u00b7li\u00b7ges", "und", "wei\u00b7tes", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ist dein Anblick mir so hehr!", "tokens": ["Wie", "ist", "dein", "An\u00b7blick", "mir", "so", "hehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei mir im fr\u00fchen Strahl gegr\u00fc\u00dft,", "tokens": ["Sei", "mir", "im", "fr\u00fc\u00b7hen", "Strahl", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der zitternd deine Lippen k\u00fc\u00dft!", "tokens": ["Der", "zit\u00b7ternd", "dei\u00b7ne", "Lip\u00b7pen", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl mir, da\u00df ich, mit dir vertraut,", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich", ",", "mit", "dir", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "$,", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel tausendmal dich angeschaut!", "tokens": ["Viel", "tau\u00b7send\u00b7mal", "dich", "an\u00b7ge\u00b7schaut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kehrte jedesmal mein Blick", "tokens": ["Es", "kehr\u00b7te", "je\u00b7des\u00b7mal", "mein", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit innigem Gef\u00fchl zur\u00fcck.", "tokens": ["Mit", "in\u00b7ni\u00b7gem", "Ge\u00b7f\u00fchl", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich lausche dir mit trunknem Ohr;", "tokens": ["Ich", "lau\u00b7sche", "dir", "mit", "trunk\u00b7nem", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es steigt mein Geist mit dir empor,", "tokens": ["Es", "steigt", "mein", "Geist", "mit", "dir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und senket sich mit dir hinab", "tokens": ["Und", "sen\u00b7ket", "sich", "mit", "dir", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der Natur geheimes Grab.", "tokens": ["In", "der", "Na\u00b7tur", "ge\u00b7hei\u00b7mes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn sich zu dir die Sonne neigt,", "tokens": ["Wenn", "sich", "zu", "dir", "die", "Son\u00b7ne", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Err\u00f6tend in dein Lager steigt;", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "in", "dein", "La\u00b7ger", "steigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann t\u00f6net deiner Wogen Klang", "tokens": ["Dann", "t\u00f6\u00b7net", "dei\u00b7ner", "Wo\u00b7gen", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der m\u00fcden Erde Wiegensang.", "tokens": ["Der", "m\u00fc\u00b7den", "Er\u00b7de", "Wie\u00b7gen\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es h\u00f6ret dich der Abendstern,", "tokens": ["Es", "h\u00f6\u00b7ret", "dich", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und winket freundlich dir von fern;", "tokens": ["Und", "win\u00b7ket", "freund\u00b7lich", "dir", "von", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir l\u00e4chelt Luna, wenn ihr Licht", "tokens": ["Dir", "l\u00e4\u00b7chelt", "Lu\u00b7na", ",", "wenn", "ihr", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich millionenf\u00e4ltig bricht.", "tokens": ["Sich", "mil\u00b7li\u00b7o\u00b7nen\u00b7f\u00e4l\u00b7tig", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Oft eil' ich aus der Haine Ruh", "tokens": ["Oft", "eil'", "ich", "aus", "der", "Hai\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Wonne deinen Wogen zu,", "tokens": ["Mit", "Won\u00b7ne", "dei\u00b7nen", "Wo\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und senke mich hinab in dich,", "tokens": ["Und", "sen\u00b7ke", "mich", "hin\u00b7ab", "in", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fchle, labe, st\u00e4rke mich.", "tokens": ["Und", "k\u00fch\u00b7le", ",", "la\u00b7be", ",", "st\u00e4r\u00b7ke", "mich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Geist des Herrn den Dichter zeugt;", "tokens": ["Der", "Geist", "des", "Herrn", "den", "Dich\u00b7ter", "zeugt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde m\u00fctterlich ihn s\u00e4ugt;", "tokens": ["Die", "Er\u00b7de", "m\u00fct\u00b7ter\u00b7lich", "ihn", "s\u00e4ugt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf deiner Wogen blauem Scho\u00df", "tokens": ["Auf", "dei\u00b7ner", "Wo\u00b7gen", "blau\u00b7em", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wiegt seine Phantasei sich gro\u00df.", "tokens": ["Wiegt", "sei\u00b7ne", "Phan\u00b7ta\u00b7sei", "sich", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der blinde S\u00e4nger stand am Meer;", "tokens": ["Der", "blin\u00b7de", "S\u00e4n\u00b7ger", "stand", "am", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wogen rauschten um ihn her,", "tokens": ["Die", "Wo\u00b7gen", "rauschten", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Riesenthaten goldner Zeit", "tokens": ["Und", "Rie\u00b7sen\u00b7tha\u00b7ten", "gold\u00b7ner", 