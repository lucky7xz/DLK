{"textgrid.poem.41139": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der vor'ge Geist verk\u00fcndet einz'lne Leichen,", "tokens": ["Der", "vor'\u00b7ge", "Geist", "ver\u00b7k\u00fcn\u00b7det", "einz'l\u00b7ne", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor dieses Nachtgespensts Erscheinen", "tokens": ["Vor", "die\u00b7ses", "Nacht\u00b7ge\u00b7spensts", "Er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt man oft fern ein Klagen, Weinen,", "tokens": ["H\u00f6rt", "man", "oft", "fern", "ein", "Kla\u00b7gen", ",", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "ART", "NN", "$,", "NN", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der Glaskopf spricht: \u00bbDas ist ein Heulen", "tokens": ["Der", "Glas\u00b7kopf", "spricht", ":", "\u00bb", "Das", "ist", "ein", "Heu\u00b7len"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In der Waldeinsamkeit von Eulen.\u00ab", "tokens": ["In", "der", "Wal\u00b7de\u00b7in\u00b7sam\u00b7keit", "von", "Eu\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Doch bald auch er sieht wie der Bauer,", "tokens": ["Doch", "bald", "auch", "er", "sieht", "wie", "der", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Da\u00df hoch sitzt auf der Kirchhofsmauer", "tokens": ["Da\u00df", "hoch", "sitzt", "auf", "der", "Kirch\u00b7hofs\u00b7mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Klagfrau, nun auch ihm ein Graus,", "tokens": ["Die", "Klag\u00b7frau", ",", "nun", "auch", "ihm", "ein", "Graus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die strecket weitaus ihre Arme", "tokens": ["Die", "stre\u00b7cket", "weit\u00b7aus", "ih\u00b7re", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und rufet in die Nacht hinaus:", "tokens": ["Und", "ru\u00b7fet", "in", "die", "Nacht", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbda\u00df Gott sich eurer Seel' erbarme!", "tokens": ["\u00bb", "da\u00df", "Gott", "sich", "eu\u00b7rer", "Seel'", "er\u00b7bar\u00b7me", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bestellt, bestellet euer Haus!", "tokens": ["Be\u00b7stellt", ",", "be\u00b7stel\u00b7let", "eu\u00b7er", "Haus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Bald bricht der schwarze Tod hier aus!\u00ab", "tokens": ["Bald", "bricht", "der", "schwar\u00b7ze", "Tod", "hier", "aus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und drauf zerflie\u00dfet sie in Luft.", "tokens": ["Und", "drauf", "zer\u00b7flie\u00b7\u00dfet", "sie", "in", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Doch bald erscheint dann jene Seuche,", "tokens": ["Doch", "bald", "er\u00b7scheint", "dann", "je\u00b7ne", "Seu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zum Kirchhof tr\u00e4gt man Leich' an Leiche,", "tokens": ["Zum", "Kirch\u00b7hof", "tr\u00e4gt", "man", "Leich'", "an", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df bald ihm mangeln Grab und Gruft.", "tokens": ["Da\u00df", "bald", "ihm", "man\u00b7geln", "Grab", "und", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Oft einer geht ehrsam und fromm einher,", "tokens": ["Oft", "ei\u00b7ner", "geht", "ehr\u00b7sam", "und", "fromm", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und jeder meint, da\u00df er das wirklich w\u00e4r',", "tokens": ["Und", "je\u00b7der", "meint", ",", "da\u00df", "er", "das", "wirk\u00b7lich", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch ach und weh! ein Mantel ", "tokens": ["Doch", "ach", "und", "weh", "!", "ein", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "XY", "KON", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verbergend seines Innern tiefen Mist.", "tokens": ["Ver\u00b7ber\u00b7gend", "sei\u00b7nes", "In\u00b7nern", "tie\u00b7fen", "Mist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Oft einer geht einher in dieser Welt,", "tokens": ["Oft", "ei\u00b7ner", "geht", "ein\u00b7her", "in", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df jeder ihn f\u00fcr b\u00f6s und s\u00fcndhaft h\u00e4lt,", "tokens": ["Da\u00df", "je\u00b7der", "ihn", "f\u00fcr", "b\u00f6s", "und", "s\u00fcnd\u00b7haft", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er ist es nicht, sein \u00c4u\u00dfres macht das nur,", "tokens": ["Er", "ist", "es", "nicht", ",", "sein", "\u00c4u\u00df\u00b7res", "macht", "das", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "VVFIN", "PDS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gut ist und fromm die innere Natur.", "tokens": ["Gut", "ist", "und", "fromm", "die", "in\u00b7ne\u00b7re", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du kannst nicht sagen: Der ist rein, ja rein!", "tokens": ["Du", "kannst", "nicht", "sa\u00b7gen", ":", "Der", "ist", "rein", ",", "ja", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "PDS", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den l\u00e4\u00dft einst Gott in seinen Himmel ein!", "tokens": ["Den", "l\u00e4\u00dft", "einst", "Gott", "in", "sei\u00b7nen", "Him\u00b7mel", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Du kannst nicht sagen: Der ist schlimm, ja schlimm!", "tokens": ["Du", "kannst", "nicht", "sa\u00b7gen", ":", "Der", "ist", "schlimm", ",", "ja", "schlimm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "PDS", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der wird einst f\u00fchlen seines Gottes Grimm!", "tokens": ["Der", "wird", "einst", "f\u00fch\u00b7len", "sei\u00b7nes", "Got\u00b7tes", "Grimm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nein! nein! ", "tokens": ["Nein", "!", "nein", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKANT", "$.", "PTKANT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Gott, Gott durchschaut des Menschen Innres nur.", "tokens": ["Gott", ",", "Gott", "durch\u00b7schaut", "des", "Men\u00b7schen", "Inn\u00b7res", "nur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der schicket ganz nach ihrem innern Wert", "tokens": ["Der", "schi\u00b7cket", "ganz", "nach", "ih\u00b7rem", "in\u00b7nern", "Wert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Seele nach dem Tod hinab, hinauf,", "tokens": ["Die", "See\u00b7le", "nach", "dem", "Tod", "hin\u00b7ab", ",", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Oft anders, als am Grab ihr Lebenslauf", "tokens": ["Oft", "an\u00b7ders", ",", "als", "am", "Grab", "ihr", "Le\u00b7bens\u00b7lauf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "In wohlgesetzter Rede es begehrt.", "tokens": ["In", "wohl\u00b7ge\u00b7setz\u00b7ter", "Re\u00b7de", "es", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh die Raup' in ihrer Puppe", "tokens": ["Sieh", "die", "Raup'", "in", "ih\u00b7rer", "Pup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stillem, dunklem Schattenreich,", "tokens": ["Stil\u00b7lem", ",", "dunk\u00b7lem", "Schat\u00b7ten\u00b7reich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun getrennt von den Genossen,", "tokens": ["Nun", "ge\u00b7trennt", "von", "den", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einzig in sich selbst verschlossen,", "tokens": ["Ein\u00b7zig", "in", "sich", "selbst", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tot nicht, ob begraben gleich,", "tokens": ["Tot", "nicht", ",", "ob", "be\u00b7gra\u00b7ben", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaut nicht mehr den Tau der Triften,", "tokens": ["Schaut", "nicht", "mehr", "den", "Tau", "der", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist der Bl\u00fct' und Kr\u00e4uter bar,", "tokens": ["Ist", "der", "Bl\u00fct'", "und", "Kr\u00e4u\u00b7ter", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00e4nzlich nur sich selbst gegeben,", "tokens": ["G\u00e4nz\u00b7lich", "nur", "sich", "selbst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Tr\u00e4gt sie das vergangne Leben", "tokens": ["Tr\u00e4gt", "sie", "das", "ver\u00b7gang\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In sich als ein P\u00fcnktchen klar.", "tokens": ["In", "sich", "als", "ein", "P\u00fcnkt\u00b7chen", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und in solcher stillen Klause", "tokens": ["Und", "in", "sol\u00b7cher", "stil\u00b7len", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Streift sie ab ihr Erdgewand,", "tokens": ["Streift", "sie", "ab", "ihr", "Erd\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Reifen ihr die bunten Schwingen,", "tokens": ["Rei\u00b7fen", "ihr", "die", "bun\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die sie einst als Psyche bringen", "tokens": ["Die", "sie", "einst", "als", "Psy\u00b7che", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Himmelw\u00e4rts aus d\u00fcstrem Land.", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "aus", "d\u00fcst\u00b7rem", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Sieh die Raup' in ihrer Puppe!", "tokens": ["Sieh", "die", "Raup'", "in", "ih\u00b7rer", "Pup\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Glaube: da\u00df auch dich der Tod", "tokens": ["Glau\u00b7be", ":", "da\u00df", "auch", "dich", "der", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "ADV", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Einst nicht