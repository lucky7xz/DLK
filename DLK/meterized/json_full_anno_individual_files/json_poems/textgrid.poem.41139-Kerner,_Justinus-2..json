n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In sich als ein P\u00fcnktchen klar.", "tokens": ["In", "sich", "als", "ein", "P\u00fcnkt\u00b7chen", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und in solcher stillen Klause", "tokens": ["Und", "in", "sol\u00b7cher", "stil\u00b7len", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Streift sie ab ihr Erdgewand,", "tokens": ["Streift", "sie", "ab", "ihr", "Erd\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Reifen ihr die bunten Schwingen,", "tokens": ["Rei\u00b7fen", "ihr", "die", "bun\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die sie einst als Psyche bringen", "tokens": ["Die", "sie", "einst", "als", "Psy\u00b7che", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Himmelw\u00e4rts aus d\u00fcstrem Land.", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "aus", "d\u00fcst\u00b7rem", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Sieh die Raup' i