{"dta.poem.21420": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egesetzlose Willk\u00fcr herrschte nie,", "tokens": ["\u201e", "ge\u00b7setz\u00b7lo\u00b7se", "Will\u00b7k\u00fcr", "herrschte", "nie", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem schlimmsten Demagogen", "tokens": ["Dem", "schlimms\u00b7ten", "De\u00b7ma\u00b7go\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ward niemals ohne Urtheilspruch", "tokens": ["Ward", "nie\u00b7mals", "oh\u00b7ne", "Ur\u00b7theil\u00b7spruch"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Staatskokarde entzogen.", "tokens": ["Die", "Staats\u00b7ko\u00b7kar\u00b7de", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eso \u00fcbel war es in Deutschland nie,", "tokens": ["\u201e", "so", "\u00fc\u00b7bel", "war", "es", "in", "Deutschland", "nie", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "PPER", "APPR", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trotz aller Zeitbedr\u00e4ngni\u00df \u2013", "tokens": ["Trotz", "al\u00b7ler", "Zeit\u00b7be\u00b7dr\u00e4ng\u00b7ni\u00df", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Glaub\u2019 mir, verhungert ist nie ein Mensch", "tokens": ["Glaub'", "mir", ",", "ver\u00b7hun\u00b7gert", "ist", "nie", "ein", "Mensch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "VAFIN", "ADV", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "In einem deutschen Gef\u00e4ngni\u00df.", "tokens": ["In", "ei\u00b7nem", "deut\u00b7schen", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201ees bl\u00fchte in der Vergangenheit", "tokens": ["\u201e", "es", "bl\u00fch\u00b7te", "in", "der", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So manche sch\u00f6ne Erscheinung", "tokens": ["So", "man\u00b7che", "sch\u00f6\u00b7ne", "Er\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Des Glaubens und der Gem\u00fcthlichkeit;", "tokens": ["Des", "Glau\u00b7bens", "und", "der", "Ge\u00b7m\u00fcth\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt herrscht nur Zweifel, Verneinung.", "tokens": ["Jetzt", "herrscht", "nur", "Zwei\u00b7fel", ",", "Ver\u00b7nei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$,", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201edie praktische \u00e4u\u00dfere Freiheit wird einst", "tokens": ["\u201e", "die", "prak\u00b7ti\u00b7sche", "\u00e4u\u00b7\u00dfe\u00b7re", "Frei\u00b7heit", "wird", "einst"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das Ideal vertilgen,", "tokens": ["Das", "I\u00b7deal", "ver\u00b7til\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das wir im Busen getragen \u2013 es war", "tokens": ["Das", "wir", "im", "Bu\u00b7sen", "ge\u00b7tra\u00b7gen", "\u2013", "es", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$(", "PPER", "VAFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So rein wie der Traum der Liljen!", "tokens": ["So", "rein", "wie", "der", "Traum", "der", "Lil\u00b7jen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}