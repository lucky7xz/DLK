{"textgrid.poem.67464": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Huld und Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als die Mutter der Liebe den sch\u00f6nen Amor geboren,", "tokens": ["Als", "die", "Mut\u00b7ter", "der", "Lie\u00b7be", "den", "sch\u00f6\u00b7nen", "A\u00b7mor", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ART", "ADJA", "NE", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sprach zu den Grazien sie: \u00bbZiehet den Knaben mir auf", "tokens": ["Sprach", "zu", "den", "Gra\u00b7zi\u00b7en", "sie", ":", "\u00bb", "Zie\u00b7het", "den", "Kna\u00b7ben", "mir", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$.", "$(", "VVFIN", "ART", "NN", "PPER", "APPR"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ernst und sanft! Auch lehret ihn bald die ambrosischen K\u00fcnste,", "tokens": ["Ernst", "und", "sanft", "!", "Auch", "leh\u00b7ret", "ihn", "bald", "die", "am\u00b7bro\u00b7si\u00b7schen", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wohlzugefallen! sie sind allen Unsterblichen werth.\u00ab", "tokens": ["Wohl\u00b7zu\u00b7ge\u00b7fal\u00b7len", "!", "sie", "sind", "al\u00b7len", "U\u00b7nsterb\u00b7li\u00b7chen", "werth", ".", "\u00ab"], "token_info": ["word", "punct", "word", "w