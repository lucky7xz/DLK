{"textgrid.poem.39067": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das sind die Neunmalweisen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das sind die Neunmalweisen,", "tokens": ["Das", "sind", "die", "Neun\u00b7mal\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die predigen und preisen", "tokens": ["die", "pre\u00b7di\u00b7gen", "und", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und lehren alle Welt:", "tokens": ["und", "leh\u00b7ren", "al\u00b7le", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Gro\u00dfe und Geringe,", "tokens": ["Das", "Gro\u00b7\u00dfe", "und", "Ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "sie pr\u00fcften alle Dinge,", "tokens": ["sie", "pr\u00fcf\u00b7ten", "al\u00b7le", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "sie f\u00fchrten jede Klinge,", "tokens": ["sie", "f\u00fchr\u00b7ten", "je\u00b7de", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "sie standen jedem Held.", "tokens": ["sie", "stan\u00b7den", "je\u00b7dem", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von Dingen, die sie w\u00fc\u00dften", "tokens": ["Von", "Din\u00b7gen", ",", "die", "sie", "w\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und die geschehen m\u00fc\u00dften,", "tokens": ["und", "die", "ge\u00b7sche\u00b7hen", "m\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "was ist ihr Mund so voll!", "tokens": ["was", "ist", "ihr", "Mund", "so", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch wenn die H\u00f6rner blasen", "tokens": ["Doch", "wenn", "die", "H\u00f6r\u00b7ner", "bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und wenn die Trommeln rasen,", "tokens": ["und", "wenn", "die", "Trom\u00b7meln", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "da r\u00fcmpfen sie die Nasen,", "tokens": ["da", "r\u00fcmp\u00b7fen", "sie", "die", "Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "da ist die Jugend toll.", "tokens": ["da", "ist", "die", "Ju\u00b7gend", "toll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sehen stolz hernieder,", "tokens": ["Sie", "se\u00b7hen", "stolz", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sie schm\u00e4hn auf unsere Lieder,", "tokens": ["sie", "schm\u00e4hn", "auf", "un\u00b7se\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "da\u00df es nicht Taten sind:", "tokens": ["da\u00df", "es", "nicht", "Ta\u00b7ten", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u00bbwas schlagt ihr nur die Saiten,", "tokens": ["\u00bb", "was", "schlagt", "ihr", "nur", "die", "Sai\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und wollt mit Liedern streiten?", "tokens": ["und", "wollt", "mit", "Lie\u00b7dern", "strei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man sang zu allen Zeiten.", "tokens": ["Man", "sang", "zu", "al\u00b7len", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "sang immer in den Wind.", "tokens": ["sang", "im\u00b7mer", "in", "den", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Nein, wer die Welt will meistern,", "tokens": ["Nein", ",", "wer", "die", "Welt", "will", "meis\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der habe nicht mit Geistern", "tokens": ["der", "ha\u00b7be", "nicht", "mit", "Geis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VAFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und Tr\u00e4umen nur Verkehr,", "tokens": ["und", "Tr\u00e4u\u00b7men", "nur", "Ver\u00b7kehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "der lerne nicht blo\u00df singen,", "tokens": ["der", "ler\u00b7ne", "nicht", "blo\u00df", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "o nein, der lerne ringen", "tokens": ["o", "nein", ",", "der", "ler\u00b7ne", "rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["FM", "PTKANT", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und handeln und vollbringen!", "tokens": ["und", "han\u00b7deln", "und", "voll\u00b7brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und alle Tat ist schwer.\u00ab \u2013", "tokens": ["Und", "al\u00b7le", "Tat", "ist", "schwer", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohlan, ihr Neunmalweisen,", "tokens": ["Wo\u00b7hlan", ",", "ihr", "Neun\u00b7mal\u00b7wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wir wolln auch dankbar preisen:", "tokens": ["wir", "wolln", "auch", "dank\u00b7bar", "prei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollt ihr nur eins geruhn:", "tokens": ["Wollt", "ihr", "nur", "eins", "ge\u00b7ruhn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr wi\u00dft so sch\u00f6n zu raten \u2013", "tokens": ["Ihr", "wi\u00dft", "so", "sch\u00f6n", "zu", "ra\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "so g\u00f6nnt in euren Staaten,", "tokens": ["so", "g\u00f6nnt", "in", "eu\u00b7ren", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "so g\u00f6nnt uns Raum zu Taten,", "tokens": ["so", "g\u00f6nnt", "uns", "Raum", "zu", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So gebt uns doch zu tun!", "tokens": ["So", "gebt", "uns", "doch", "zu", "tun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir wissen selbst: die Leier,", "tokens": ["Wir", "wis\u00b7sen", "selbst", ":", "die", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die macht die Welt nicht freier,", "tokens": ["die", "macht", "die", "Welt", "nicht", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "taub ist der M\u00e4cht'gen Ohr.", "tokens": ["taub", "ist", "der", "M\u00e4cht'\u00b7gen", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch schwebt denn nicht, ich frage,", "tokens": ["Doch", "schwebt", "denn", "nicht", ",", "ich", "fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "mit munterm Fl\u00fcgelschlage", "tokens": ["mit", "mun\u00b7term", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "dem werdenden, dem Tage,", "tokens": ["dem", "wer\u00b7den\u00b7den", ",", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "die Lerche auch zuvor?", "tokens": ["die", "Ler\u00b7che", "auch", "zu\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und kommt auf schwarzen Wogen", "tokens": ["Und", "kommt", "auf", "schwar\u00b7zen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ein Sturm herangezogen,", "tokens": ["ein", "Sturm", "her\u00b7an\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "saht ihr die L\u00f6we nicht?", "tokens": ["saht", "ihr", "die", "L\u00f6\u00b7we", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wenn der Lenz soll kommen,", "tokens": ["Und", "wenn", "der", "Lenz", "soll", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "habt ihr da nicht vernommen", "tokens": ["habt", "ihr", "da", "nicht", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der Nachtigall, der frommen,", "tokens": ["der", "Nach\u00b7ti\u00b7gall", ",", "der", "from\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "prophetisches Gedicht?", "tokens": ["pro\u00b7phe\u00b7ti\u00b7sches", "Ge\u00b7dicht", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drum was die Neunmalweisen", "tokens": ["Drum", "was", "die", "Neun\u00b7mal\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PWS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "auch predigen und preisen,", "tokens": ["auch", "pre\u00b7di\u00b7gen", "und", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wir dulden ihren Spott!", "tokens": ["wir", "dul\u00b7den", "ih\u00b7ren", "Spott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir streuen doch zu Taten,", "tokens": ["Wir", "streu\u00b7en", "doch", "zu", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "zu k\u00fcnftigen, die Saaten:", "tokens": ["zu", "k\u00fcnf\u00b7ti\u00b7gen", ",", "die", "Saa\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun mag die Frucht beraten", "tokens": ["Nun", "mag", "die", "Frucht", "be\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "der allerh\u00f6chste Gott!", "tokens": ["der", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Gott", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das sind die Neunmalweisen,", "tokens": ["Das", "sind", "die", "Neun\u00b7mal\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die predigen und preisen", "tokens": ["die", "pre\u00b7di\u00b7gen", "und", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und lehren alle Welt:", "tokens": ["und", "leh\u00b7ren", "al\u00b7le", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Gro\u00dfe und Geringe,", "tokens": ["Das", "Gro\u00b7\u00dfe", "und", "Ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "sie pr\u00fcften alle Dinge,", "tokens": ["sie", "pr\u00fcf\u00b7ten", "al\u00b7le", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "sie f\u00fchrten jede Klinge,", "tokens": ["sie", "f\u00fchr\u00b7ten", "je\u00b7de", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "sie standen jedem Held.", "tokens": ["sie", "stan\u00b7den", "je\u00b7dem", "Held", "."], "token_info": ["word