{"dta.poem.19899": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Die Umarmung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Wie um ihren Stab die Rebe               ", "tokens": ["Wie", "um", "ih\u00b7ren", "Stab", "die", "Re\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00fcnstig ihre Ranke strikt,", "tokens": ["Br\u00fcns\u00b7tig", "ih\u00b7re", "Ran\u00b7ke", "strikt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Epheu sein Gewebe", "tokens": ["Wie", "der", "E\u00b7pheu", "sein", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Ulme Busen dr\u00fckt;", "tokens": ["An", "der", "Ul\u00b7me", "Bu\u00b7sen", "dr\u00fckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Taubenpaar sich schn\u00e4belt,", "tokens": ["Wie", "ein", "Tau\u00b7ben\u00b7paar", "sich", "schn\u00e4\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf ausgeforschtem Nest,", "tokens": ["Und", "auf", "aus\u00b7ge\u00b7forschtem", "Nest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Von der Liebe Rausch umnebelt,", "tokens": ["Von", "der", "Lie\u00b7be", "Rausch", "um\u00b7ne\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Haschen sich und w\u00fcrgen l\u00e4st:", "tokens": ["Ha\u00b7schen", "sich", "und", "w\u00fcr\u00b7gen", "l\u00e4st", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "D\u00fcrft\u2019 ich so dich rund umfangen!", "tokens": ["D\u00fcrft'", "ich", "so", "dich", "rund", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcrftest du, Geliebte, mich! \u2014", "tokens": ["D\u00fcrf\u00b7test", "du", ",", "Ge\u00b7lieb\u00b7te", ",", "mich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcrften so zusammenhangen", "tokens": ["D\u00fcrf\u00b7ten", "so", "zu\u00b7sam\u00b7men\u00b7han\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Lippen ewiglich! \u2014", "tokens": ["Uns\u00b7re", "Lip\u00b7pen", "e\u00b7wig\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann, von keines F\u00fcrsten Male,", "tokens": ["Dann", ",", "von", "kei\u00b7nes", "F\u00fcrs\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht von seines Gartens Frucht,", "tokens": ["Nicht", "von", "sei\u00b7nes", "Gar\u00b7tens", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch des Rebengottes Schaale,", "tokens": ["Noch", "des", "Re\u00b7ben\u00b7got\u00b7tes", "Schaa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrde dann mein Gaum versucht.", "tokens": ["W\u00fcr\u00b7de", "dann", "mein", "Gaum", "ver\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sterben wolt\u2019 ich im Genusse,", "tokens": ["Ster\u00b7ben", "wolt'", "ich", "im", "Ge\u00b7nus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ihn deine Lippe beut,", "tokens": ["Wie", "ihn", "dei\u00b7ne", "Lip\u00b7pe", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterben in dem langen Kusse", "tokens": ["Ster\u00b7ben", "in", "dem", "lan\u00b7gen", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollustvoller Trunkenheit. \u2014", "tokens": ["Wol\u00b7lust\u00b7vol\u00b7ler", "Trun\u00b7ken\u00b7heit", "."], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kom, o kom und las uns sterben!", "tokens": ["Kom", ",", "o", "kom", "und", "las", "uns", "ster\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "FM", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir entlodert schon der Geist.", "tokens": ["Mir", "ent\u00b7lo\u00b7dert", "schon", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fluch vermachet sey dem Erben,", "tokens": ["Fluch", "ver\u00b7ma\u00b7chet", "sey", "dem", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns von einander reist!", "tokens": ["Der", "uns", "von", "ein\u00b7an\u00b7der", "reist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unter Myrten, wo wir fallen,", "tokens": ["Un\u00b7ter", "Myr\u00b7ten", ",", "wo", "wir", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleib\u2019 uns Eine Gruft bevor!", "tokens": ["Bleib'", "uns", "Ei\u00b7ne", "Gruft", "be\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Seelen aber wallen", "tokens": ["Uns\u00b7re", "See\u00b7len", "a\u00b7ber", "wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In vereintem Hauch\u2019 empor;", "tokens": ["In", "ver\u00b7ein\u00b7tem", "Hauch'", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "In die seligen Gefilde,", "tokens": ["In", "die", "se\u00b7li\u00b7gen", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voller Wolgeruch und Pracht,", "tokens": ["Vol\u00b7ler", "Wol\u00b7ge\u00b7ruch", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denen stete Fr\u00fchlingsmilde", "tokens": ["De\u00b7nen", "ste\u00b7te", "Fr\u00fch\u00b7lings\u00b7mil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom entw\u00f6lkten Himmel lacht;", "tokens": ["Vom", "ent\u00b7w\u00f6lk\u00b7ten", "Him\u00b7mel", "lacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wo die B\u00e4ume sch\u00f6ner bl\u00fchen,", "tokens": ["Wo", "die", "B\u00e4u\u00b7me", "sch\u00f6\u00b7ner", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Quellen, wo der Wind,", "tokens": ["Wo", "die", "Quel\u00b7len", ",", "wo", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der V\u00f6gel Melodieen", "tokens": ["Und", "der", "V\u00f6\u00b7gel", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Lieblicher und reiner sind;", "tokens": ["Lieb\u00b7li\u00b7cher", "und", "rei\u00b7ner", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "VAFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.10": {"line.1": {"text": "Wo das Auge des Betr\u00fcbten", "tokens": ["Wo", "das", "Au\u00b7ge", "des", "Be\u00b7tr\u00fcb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Thr\u00e4nen ausgeweint,", "tokens": ["Sei\u00b7ne", "Thr\u00e4\u00b7nen", "aus\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Geliebte mit Geliebten", "tokens": ["Und", "Ge\u00b7lieb\u00b7te", "mit", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig das Geschik vereint;", "tokens": ["E\u00b7wig", "das", "Ge\u00b7schik", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wo nun Phaon, vol Bedauren,", "tokens": ["Wo", "nun", "Phaon", ",", "vol", "Be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Seiner Sapho sich erbarmt;", "tokens": ["Sei\u00b7ner", "Sa\u00b7pho", "sich", "er\u00b7barmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo Petrarka ruhig Lauren", "tokens": ["Wo", "Pe\u00b7trar\u00b7ka", "ru\u00b7hig", "Lau\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADJD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An der reinsten Quell\u2019 umarmt;", "tokens": ["An", "der", "reins\u00b7ten", "Quell'", "um\u00b7armt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und auf rundumschirmten Wiesen,", "tokens": ["Und", "auf", "rund\u00b7um\u00b7schirm\u00b7ten", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht von Argwohn mehr gest\u00f6rt,", "tokens": ["Nicht", "von", "Arg\u00b7wohn", "mehr", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fcklicher bei Heloisen", "tokens": ["Gl\u00fck\u00b7li\u00b7cher", "bei", "He\u00b7loi\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ab\u00e4lard die Liebe lehrt. \u2014", "tokens": ["A\u00b7b\u00e4\u00b7lard", "die", "Lie\u00b7be", "lehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "O des Himmels voller Freuden,", "tokens": ["O", "des", "Him\u00b7mels", "vol\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich da schon offen sah! \u2014", "tokens": ["Den", "ich", "da", "schon", "of\u00b7fen", "sah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kom! Von hinnen las uns scheiden!", "tokens": ["Kom", "!", "Von", "hin\u00b7nen", "las", "uns", "schei\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "APPR", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eia! w\u00e4ren wir schon da!", "tokens": ["Ei\u00b7a", "!", "w\u00e4\u00b7ren", "wir", "schon", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}