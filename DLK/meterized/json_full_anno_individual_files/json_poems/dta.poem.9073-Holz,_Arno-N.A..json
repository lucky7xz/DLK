{"dta.poem.9073": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verbl\u00fcht", "tokens": ["Ver\u00b7bl\u00fcht"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "die Kr\u00e4nze, die du gewunden,", "tokens": ["die", "Kr\u00e4n\u00b7ze", ",", "die", "du", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "verweht", "tokens": ["ver\u00b7weht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "die Lieder, die du gesungen,", "tokens": ["die", "Lie\u00b7der", ",", "die", "du", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und in deinen Haaren, in deinen sch\u00f6nen Haaren,", "tokens": ["und", "in", "dei\u00b7nen", "Haa\u00b7ren", ",", "in", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "klebt nun die", "tokens": ["klebt", "nun", "die"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Erde.", "tokens": ["Er\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Tot, tot, tot . . .", "tokens": ["Tot", ",", "tot", ",", "tot", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$.", "$.", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und deine Fl\u00fcgel, deine armen Fl\u00fcgel!", "tokens": ["Und", "dei\u00b7ne", "Fl\u00fc\u00b7gel", ",", "dei\u00b7ne", "ar\u00b7men", "Fl\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unbarmherzig heruntergeschnitten", "tokens": ["Un\u00b7barm\u00b7her\u00b7zig", "her\u00b7un\u00b7ter\u00b7ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "von den schimmernden Schultern \u2014 ah, ", "tokens": ["von", "den", "schim\u00b7mern\u00b7den", "Schul\u00b7tern", "ah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "ITJ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weine ", "tokens": ["Wei\u00b7ne"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Hier! Hier! ", "tokens": ["Hier", "!", "Hier", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$."], "meter": "++", "measure": "spondeus"}, "line.6": {"text": "n\u00e4chtlich, alln\u00e4chtlich,", "tokens": ["n\u00e4cht\u00b7lich", ",", "all\u00b7n\u00e4cht\u00b7lich", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "bis der Morgen", "tokens": ["bis", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "graut,", "tokens": ["graut", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "bis die Sonne", "tokens": ["bis", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "scheint,", "tokens": ["scheint", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "und die Welt,", "tokens": ["und", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "die kluge Welt, wieder gleichg\u00fcltig \u00fcber dein Grab rollt \u2014", "tokens": ["die", "klu\u00b7ge", "Welt", ",", "wie\u00b7der", "gleich\u00b7g\u00fcl\u00b7tig", "\u00fc\u00b7ber", "dein", "Grab", "rollt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Horch!", "tokens": ["Horch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Der Ahorn vor meinem Fenster rauscht,", "tokens": ["Der", "A\u00b7horn", "vor", "mei\u00b7nem", "Fens\u00b7ter", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "der Thau tropft,", "tokens": ["der", "Thau", "tropft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "und mein Herz", "tokens": ["und", "mein", "Herz"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "schl\u00e4gt.", "tokens": ["schl\u00e4gt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "Nacht, Nacht, Nacht . . .", "tokens": ["Nacht", ",", "Nacht", ",", "Nacht", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$.", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}