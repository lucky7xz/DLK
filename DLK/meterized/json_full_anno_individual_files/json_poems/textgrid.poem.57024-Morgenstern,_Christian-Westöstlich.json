{"textgrid.poem.57024": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "West\u00f6stlich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als er dies v. Korf erz\u00e4hlt,", "tokens": ["Als", "er", "dies", "v.", "Korf", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "f\u00fchlt sich dieser leicht gequ\u00e4lt;", "tokens": ["f\u00fchlt", "sich", "die\u00b7ser", "leicht", "ge\u00b7qu\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PDAT", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "denn f\u00fcr ihn ist Selbstverstehung,", "tokens": ["denn", "f\u00fcr", "ihn", "ist", "Selbst\u00b7ver\u00b7ste\u00b7hung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df man mit der Erdumdrehung", "tokens": ["da\u00df", "man", "mit", "der", "Erd\u00b7um\u00b7dre\u00b7hung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "schlafen m\u00fcsse, mit den Pfosten", "tokens": ["schla\u00b7fen", "m\u00fcs\u00b7se", ",", "mit", "den", "Pfos\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seines K\u00f6rpers strikt nach Osten.", "tokens": ["sei\u00b7nes", "K\u00f6r\u00b7pers", "