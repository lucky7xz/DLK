{"textgrid.poem.41349": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Prophezeiung", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, sterb' ich einst, so wird ein B\u00f6sewicht,", "tokens": ["Freund", ",", "sterb'", "ich", "einst", ",", "so", "wird", "ein", "B\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der jetzt noch schweigt, mir keinen Nachruhm g\u00f6nnen.", "tokens": ["Der", "jetzt", "noch", "schweigt", ",", "mir", "kei\u00b7nen", "Nach\u00b7ruhm", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber mich und meinen Werth erkennen.", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "und", "mei\u00b7nen", "Werth", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es mag geschehn! Den Schnarcher f\u00fcrcht' ich nicht.", "tokens": ["Es", "mag", "ge\u00b7schehn", "!", "Den", "Schnar\u00b7cher", "f\u00fcrcht'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus Demuth nur will ich ihn dir nicht nennen.", "tokens": ["Aus", "De\u00b7muth", "nur", "will", "ich", "ihn", "dir", "nicht", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VMFIN", "PPER", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "--+-+-+-+--", "measure": "anapaest.init"}, "line.6": {"text": "Sein Tadel ehrt, mehr als ein Lobgedicht.", "tokens": ["Sein", "Ta\u00b7del", "ehrt", ",", "mehr", "als", "ein", "Lob\u00b7ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Freund, sterb' ich einst, so wird ein B\u00f6sewicht,", "tokens": ["Freund", ",", "sterb'", "ich", "einst", ",", "so", "wird", "ein", "B\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der jetzt noch schweigt, mir keinen Nachruhm g\u00f6nnen.", "tokens": ["Der", "jetzt", "noch", "schweigt", ",", "mir", "kei\u00b7nen", "Nach\u00b7ruhm", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber mich und meinen Werth erkennen.", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "und", "mei\u00b7nen", "Werth", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iam