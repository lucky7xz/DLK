{"textgrid.poem.53731": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Stimmen in der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einer liegt nach gutem Mahle", "tokens": ["Ei\u00b7ner", "liegt", "nach", "gu\u00b7tem", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "tief im Bett als Hosenmatz.", "tokens": ["tief", "im", "Bett", "als", "Ho\u00b7sen\u00b7matz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Bauch die Bierkaltschale,", "tokens": ["In", "dem", "Bauch", "die", "Bier\u00b7kalt\u00b7scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auf dem Nachttisch Rudolf Stratz.", "tokens": ["auf", "dem", "Nacht\u00b7tisch", "Ru\u00b7dolf", "Stratz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohlig bl\u00fcht das Fett, das weiche,", "tokens": ["Woh\u00b7lig", "bl\u00fcht", "das", "Fett", ",", "das", "wei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "popul\u00e4r im ganzen Reiche . . .", "tokens": ["po\u00b7pu\u00b7l\u00e4r", "im", "gan\u00b7zen", "Rei\u00b7che", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NE", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Knackten M\u00f6bel \u2013?", "tokens": ["Knack\u00b7ten", "M\u00f6\u00b7bel", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Und er tr\u00e4umt von einem blassen", "tokens": ["Und", "er", "tr\u00e4umt", "von", "ei\u00b7nem", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Grand-Ouvert mit lauter Assen . . .", "tokens": ["Gran\u00b7dOu\u00b7vert", "mit", "lau\u00b7ter", "As\u00b7sen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sprach da einer \u2013?", "tokens": ["Sprach", "da", "ei\u00b7ner", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ART", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "In der Ecke zirpt es schwach.", "tokens": ["In", "der", "E\u00b7cke", "zirpt", "es", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und man h\u00f6rt die Schr\u00e4nke knistern", "tokens": ["Und", "man", "h\u00f6rt", "die", "Schr\u00e4n\u00b7ke", "knis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "und ein kleines Stimmchen fl\u00fcstern:", "tokens": ["und", "ein", "klei\u00b7nes", "Stimm\u00b7chen", "fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "\u00bbfechenbach.\u00ab", "tokens": ["\u00bb", "fe\u00b7chen\u00b7bach", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Leicht gest\u00f6rte Augenblicke", "tokens": ["Leicht", "ge\u00b7st\u00f6r\u00b7te", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in dem Traum des Schlafgefechts.", "tokens": ["in", "dem", "Traum", "des", "Schlaf\u00b7ge\u00b7fechts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tiefer atmend w\u00e4lzt der Dicke", "tokens": ["Tie\u00b7fer", "at\u00b7mend", "w\u00e4lzt", "der", "Di\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sich behaglich-schwer nach rechts.", "tokens": ["sich", "be\u00b7hag\u00b7lich\u00b7schwer", "nach", "rechts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Hand will sich verstecken", "tokens": ["Sei\u00b7ne", "Hand", "will", "sich", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "unter Kissen, unter Decken . . .", "tokens": ["un\u00b7ter", "Kis\u00b7sen", ",", "un\u00b7ter", "De\u00b7cken", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ging da einer \u2013?", "tokens": ["Ging", "da", "ei\u00b7ner", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Tr\u00e4ume, Schlaf und Ruhe schwinden.", "tokens": ["Tr\u00e4u\u00b7me", ",", "Schlaf", "und", "Ru\u00b7he", "schwin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und er kann sie nicht mehr finden . . .", "tokens": ["Und", "er", "kann", "sie", "nicht", "mehr", "fin\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Klappten T\u00fcren \u2013?", "tokens": ["Klapp\u00b7ten", "T\u00fc\u00b7ren", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Schl\u00e4ft er oder ist er wach \u2013?", "tokens": ["Schl\u00e4ft", "er", "o\u00b7der", "ist", "er", "wach", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "KON", "VAFIN", "PPER", "ADJD", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Aus den Fenstern, aus den W\u00e4nden", "tokens": ["Aus", "den", "Fens\u00b7tern", ",", "aus", "den", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "immer klingt es allerenden:", "tokens": ["im\u00b7mer", "klingt", "es", "al\u00b7le\u00b7ren\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "\u00bbfechenbach! Fechenbach!\u00ab", "tokens": ["\u00bb", "fe\u00b7chen\u00b7bach", "!", "Fe\u00b7chen\u00b7bach", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Aufgerichtet, unruhvollen", "tokens": ["Auf\u00b7ge\u00b7rich\u00b7tet", ",", "un\u00b7ruh\u00b7vol\u00b7len"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auges lauscht er in die Zeit.", "tokens": ["Au\u00b7ges", "lauscht", "er", "in", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stimmen, die dem Nichts entquollen,", "tokens": ["Stim\u00b7men", ",", "die", "dem", "Nichts", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "rufen aus der Dunkelheit:", "tokens": ["ru\u00b7fen", "aus", "der", "Dun\u00b7kel\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbw\u00e4hrend du auf bunten Messen", "tokens": ["\u00bb", "w\u00e4h\u00b7rend", "du", "auf", "bun\u00b7ten", "Mes\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "redetest, sa\u00df er vergessen", "tokens": ["re\u00b7de\u00b7test", ",", "sa\u00df", "er", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "in der Zelle!", "tokens": ["in", "der", "Zel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.8": {"text": "Legtest ab den Papagei-Eid:", "tokens": ["Leg\u00b7test", "ab", "den", "Pa\u00b7pa\u00b7gei\u00b7Eid", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Einigkeit und Recht und Freiheit . . .", "tokens": ["Ei\u00b7nig\u00b7keit", "und", "Recht", "und", "Frei\u00b7heit", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die Zelle \u2013?", "tokens": ["Und", "die", "Zel\u00b7le", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$(", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.11": {"text": "H\u00f6r sein Weinen tausendfach!", "tokens": ["H\u00f6r", "sein", "Wei\u00b7nen", "tau\u00b7send\u00b7fach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Mensch, das Recht ist in Bedr\u00e4ngnis!", "tokens": ["Mensch", ",", "das", "Recht", "ist", "in", "Be\u00b7dr\u00e4ng\u00b7nis", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Gib ihn frei aus dem Gef\u00e4ngnis \u2013!", "tokens": ["Gib", "ihn", "frei", "aus", "dem", "Ge\u00b7f\u00e4ng\u00b7nis", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Fechenbach!", "tokens": ["Fe\u00b7chen\u00b7bach", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Fechenbach!", "tokens": ["Fe\u00b7chen\u00b7bach", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Fechenbach!\u00ab", "tokens": ["Fe\u00b7chen\u00b7bach", "!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Aber er hatte immer, was das betraf,", "tokens": ["A\u00b7ber", "er", "hat\u00b7te", "im\u00b7mer", ",", "was", "das", "be\u00b7traf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "$,", "PWS", "PDS", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "eine gute Verdauung und guten Schlaf.", "tokens": ["ei\u00b7ne", "gu\u00b7te", "Ver\u00b7dau\u00b7ung", "und", "gu\u00b7ten", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Einer liegt nach gutem Mahle", "t