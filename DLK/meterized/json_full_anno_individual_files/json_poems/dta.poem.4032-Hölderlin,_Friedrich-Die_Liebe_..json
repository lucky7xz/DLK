{"dta.poem.4032": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr Freunde verge\u00dft, wenn ihr die Euern all',", "tokens": ["Wenn", "ihr", "Freun\u00b7de", "ver\u00b7ge\u00dft", ",", "wenn", "ihr", "die", "Eu\u00b7ern", "all'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PIS", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "O ihr Dankbaren, sie, euere Dichter schm\u00e4ht,", "tokens": ["O", "ihr", "Dank\u00b7ba\u00b7ren", ",", "sie", ",", "eu\u00b7e\u00b7re", "Dich\u00b7ter", "schm\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Gott vergeb' es, doch ehret", "tokens": ["Gott", "ver\u00b7geb'", "es", ",", "doch", "eh\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nur die Seele der Liebenden.", "tokens": ["Nur", "die", "See\u00b7le", "der", "Lie\u00b7ben\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Denn o saget, wo lebt menschliches Leben sonst,", "tokens": ["Denn", "o", "sa\u00b7get", ",", "wo", "lebt", "menschli\u00b7ches", "Le\u00b7ben", "sonst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "VVFIN", "$,", "PWAV", "VVFIN", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da die knechtische jetzt alles, die Sorge, zwingt?", "tokens": ["Da", "die", "knech\u00b7ti\u00b7sche", "jetzt", "al\u00b7les", ",", "die", "Sor\u00b7ge", ",", "zwingt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PIS", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Darum wandelt der Gott auch", "tokens": ["Da\u00b7rum", "wan\u00b7delt", "der", "Gott", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Sorglos \u00fcber dem Haupt uns l\u00e4ngst.", "tokens": ["Sorg\u00b7los", "\u00fc\u00b7ber", "dem", "Haupt", "uns", "l\u00e4ngst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Doch, wie immer das Jahr kalt und gesanglos ist,", "tokens": ["Doch", ",", "wie", "im\u00b7mer", "das", "Jahr", "kalt", "und", "ge\u00b7sang\u00b7los", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "ART", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zur beschiedenen Zeit, aber aus wei\u00dfem Feld", "tokens": ["Zur", "be\u00b7schie\u00b7de\u00b7nen", "Zeit", ",", "a\u00b7ber", "aus", "wei\u00b7\u00dfem", "Feld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gr\u00fcne Halme doch sprossen.", "tokens": ["Gr\u00fc\u00b7ne", "Hal\u00b7me", "doch", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Oft ein einsamer Vogel singt.", "tokens": ["Oft", "ein", "ein\u00b7sa\u00b7mer", "Vo\u00b7gel", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn sich m\u00e4hlig der Wald dehnet, der Strom", "tokens": ["Wenn", "sich", "m\u00e4h\u00b7lig", "der", "Wald", "deh\u00b7net", ",", "der", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PRF", "ADJD", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "sich regt.", "tokens": ["sich", "reg\u00b7t."], "token_info": ["word", "abbreviation"], "pos": ["PRF", "NE"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Schon die mildere Luft leise von Mittag weht", "tokens": ["Schon", "die", "mil\u00b7de\u00b7re", "Luft", "lei\u00b7se", "von", "Mit\u00b7tag", "weht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "APPR", "NN", "VVFIN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zur erlesenen Stunde,", "tokens": ["Zur", "er\u00b7le\u00b7se\u00b7nen", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "So ein Zeichen der sch\u00f6nern Zeit,", "tokens": ["So", "ein", "Zei\u00b7chen", "der", "sch\u00f6\u00b7nern", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Die wir glauben, erw\u00e4chst einzig gen\u00fcgsam nah,", "tokens": ["Die", "wir", "glau\u00b7ben", ",", "er\u00b7w\u00e4chst", "ein\u00b7zig", "ge\u00b7n\u00fcg\u00b7sam", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "ADJD", "ADJD", "ADJD", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Einzig edel und fromm \u00fcber dem ehernen,", "tokens": ["Ein\u00b7zig", "e\u00b7del", "und", "fromm", "\u00fc\u00b7ber", "dem", "e\u00b7her\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wilden Boden die Liebe,", "tokens": ["Wil\u00b7den", "Bo\u00b7den", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Gottes Tochter, von ihm allein.", "tokens": ["Got\u00b7tes", "Toch\u00b7ter", ",", "von", "ihm", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Sey gesegnet, o sey, himmlische Pflanze, mir", "tokens": ["Sey", "ge\u00b7seg\u00b7net", ",", "o", "sey", ",", "himm\u00b7li\u00b7sche", "Pflan\u00b7ze", ",", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "VVPP", "$,", "FM", "VAFIN", "$,", "ADJA", "NN", "$,", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Mit Gesange gepflegt, wenn des \u00e4therischen", "tokens": ["Mit", "Ge\u00b7san\u00b7ge", "ge\u00b7pflegt", ",", "wenn", "des", "\u00e4t\u00b7he\u00b7ri\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nektars Kr\u00e4fte Dich n\u00e4hren,", "tokens": ["Nek\u00b7tars", "Kr\u00e4f\u00b7te", "Dich", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und der sch\u00f6pf'rische Stral Dich reift.", "tokens": ["Und", "der", "sch\u00f6pf'\u00b7ri\u00b7sche", "Stral", "Dich", "reift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Wachs' und werde zum Wald! eine beseeltere,", "tokens": ["Wachs'", "und", "wer\u00b7de", "zum", "Wald", "!", "ei\u00b7ne", "be\u00b7seel\u00b7te\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VAFIN", "APPRART", "NN", "$.", "ART", "ADJA", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Voll entbl\u00fchende Welt! Sprache der Liebenden", "tokens": ["Voll", "ent\u00b7bl\u00fc\u00b7hen\u00b7de", "Welt", "!", "Spra\u00b7che", "der", "Lie\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$.", "NN", "ART", "NN"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sey die Sprache des Landes,", "tokens": ["Sey", "die", "Spra\u00b7che", "des", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ihre Seele der Laut des Volks!", "tokens": ["Ih\u00b7re", "See\u00b7le", "der", "Laut", "des", "Volks", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "APPR", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}