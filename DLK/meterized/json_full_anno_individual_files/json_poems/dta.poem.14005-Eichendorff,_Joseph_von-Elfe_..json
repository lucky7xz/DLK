{"dta.poem.14005": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Elfe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Freude, das sch\u00f6ne leichtgl\u00e4ubige Kind,", "tokens": ["Die", "Freu\u00b7de", ",", "das", "sch\u00f6\u00b7ne", "leicht\u00b7gl\u00e4u\u00b7bi\u00b7ge", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es wiegt sich in Abendwinden:", "tokens": ["Es", "wiegt", "sich", "in", "Ab\u00b7end\u00b7win\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo Silber auf Zweigen und B\u00fcschen rinnt,", "tokens": ["Wo", "Sil\u00b7ber", "auf", "Zwei\u00b7gen", "und", "B\u00fc\u00b7schen", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da wirst Du die Sch\u00f6nste finden!", "tokens": ["Da", "wirst", "Du", "die", "Sch\u00f6ns\u00b7te", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}