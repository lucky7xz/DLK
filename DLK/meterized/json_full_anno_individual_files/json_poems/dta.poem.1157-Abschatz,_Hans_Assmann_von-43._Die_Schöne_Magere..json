{"dta.poem.1157": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "43. Die Sch\u00f6ne Magere.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Durchlauchtigs Himmel-Bild/ gleich Jafpis und Crystallen/ ", "tokens": ["Durch\u00b7lauch\u00b7tigs", "Him\u00b7mel\u00b7Bild", "/", "gleich", "Jaf\u00b7pis", "und", "Crys\u00b7tal\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADV", "NE", "KON", "NN", "$("], "meter": "-+-+-+--+-++-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dir dringt mein k\u00fchner Blick bi\u00df zu dem Hertzen ein/", "tokens": ["Dir", "dringt", "mein", "k\u00fch\u00b7ner", "Blick", "bi\u00df", "zu", "dem", "Hert\u00b7zen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zehlt ieden Tropffen Blutt f\u00fcr Stern und Edelstein;", "tokens": ["Zehlt", "ie\u00b7den", "Tropf\u00b7fen", "Blutt", "f\u00fcr", "Stern", "und", "E\u00b7del\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir ist/ was irdisch ist/ des Fleisches Last entfallen.", "tokens": ["Dir", "ist", "/", "was", "ir\u00b7disch", "ist", "/", "des", "Flei\u00b7sches", "Last", "ent\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWS", "ADJD", "VAFIN", "$(", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimm Stimm und Federn an/ du gleichst den Nachtigallen/", "tokens": ["Nimm", "Stimm", "und", "Fe\u00b7dern", "an", "/", "du", "gleichst", "den", "Nach\u00b7ti\u00b7gal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "PTKVZ", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es kan dem ", "tokens": ["Es", "kan", "dem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Der z\u00e4rtsten Sch\u00f6nheit unent\u00e4dert Muster seyn:", "tokens": ["Der", "z\u00e4rts\u00b7ten", "Sch\u00f6n\u00b7heit", "un\u00b7en\u00b7t\u00e4\u00b7dert", "Mus\u00b7ter", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man sieht den reinen Geist auff leichten F\u00fcssen wallen.", "tokens": ["Man", "sieht", "den", "rei\u00b7nen", "Geist", "auff", "leich\u00b7ten", "F\u00fcs\u00b7sen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du gleichst bey Leben noch der schnellen G\u00f6tter Schaar:", "tokens": ["Du", "gleichst", "bey", "Le\u00b7ben", "noch", "der", "schnel\u00b7len", "G\u00f6t\u00b7ter", "Schaar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ohn Zweiffel war/ wie du/ der Blumen-Nimphen Paar/", "tokens": ["Ohn", "Zweif\u00b7fel", "war", "/", "wie", "du", "/", "der", "Blu\u00b7men\u00b7\u00b7N\u00b7im\u00b7phen", "Paar", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$(", "PWAV", "PPER", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Sonst h\u00e4tte sie der Wind nicht k\u00f6nnen so entf\u00fchren.", "tokens": ["Sonst", "h\u00e4t\u00b7te", "sie", "der", "Wind", "nicht", "k\u00f6n\u00b7nen", "so", "ent\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Syringe gleichte dir/ umsonst vom Pan geliebt/", "tokens": ["Sy\u00b7rin\u00b7ge", "gleich\u00b7te", "dir", "/", "um\u00b7sonst", "vom", "Pan", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$(", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie ihr verwandelt Rohr noch dessen Zeugnis giebt/", "tokens": ["Wie", "ihr", "ver\u00b7wan\u00b7delt", "Rohr", "noch", "des\u00b7sen", "Zeug\u00b7nis", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das er/ wie d\u00fcrr\u2019 es ist/ sich freuet zu ber\u00fchren.", "tokens": ["Das", "er", "/", "wie", "d\u00fcrr'", "es", "ist", "/", "sich", "freu\u00b7et", "zu", "be\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$(", "PWAV", "VMFIN", "PPER", "VAFIN", "$(", "PRF", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}