{"dta.poem.20111": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Von G\u00e4sten wimmelt Hof und Halle,", "tokens": ["Von", "G\u00e4s\u00b7ten", "wim\u00b7melt", "Hof", "und", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus K\u00fcch\u2019 und Keller l\u00e4rmt es laut,", "tokens": ["Aus", "K\u00fc\u00b7ch'", "und", "Kel\u00b7ler", "l\u00e4rmt", "es", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bei Gl\u00e4serklang und Liederschalle", "tokens": ["Bei", "Gl\u00e4\u00b7ser\u00b7klang", "und", "Lie\u00b7der\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trinkt man das Wohl der jungen Braut.", "tokens": ["Trinkt", "man", "das", "Wohl", "der", "jun\u00b7gen", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon an der Festestafel oben,", "tokens": ["Schon", "an", "der", "Fes\u00b7te\u00b7sta\u00b7fel", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gest\u00fctzt auf ihres Gatten Arm,", "tokens": ["Ge\u00b7st\u00fctzt", "auf", "ih\u00b7res", "Gat\u00b7ten", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat Sittah l\u00e4chelnd sich erhoben,", "tokens": ["Hat", "Sit\u00b7tah", "l\u00e4\u00b7chelnd", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und gr\u00fc\u00dft der G\u00e4ste lauten Schwarm; \u2014", "tokens": ["Und", "gr\u00fc\u00dft", "der", "G\u00e4s\u00b7te", "lau\u00b7ten", "Schwarm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da pl\u00f6tzlich schallen wilde T\u00f6ne", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "schal\u00b7len", "wil\u00b7de", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Im Hofe drunten am Portal,", "tokens": ["Im", "Ho\u00b7fe", "drun\u00b7ten", "am", "Por\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und Lieder der Zigeuners\u00f6hne", "tokens": ["Und", "Lie\u00b7der", "der", "Zi\u00b7geu\u00b7ner\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ziehn durch den hochzeitlichen Saal.", "tokens": ["Ziehn", "durch", "den", "hoch\u00b7zeit\u00b7li\u00b7chen", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Sie t\u00f6nen lauter schon \u2014 und wilder", "tokens": ["Sie", "t\u00f6\u00b7nen", "lau\u00b7ter", "schon", "und", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$(", "KON", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Saust in der Luft das Tambourin,", "tokens": ["Saust", "in", "der", "Luft", "das", "Tam\u00b7bo\u00b7u\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Da treten halbverge\u00dfne Bilder", "tokens": ["Da", "tre\u00b7ten", "halb\u00b7ver\u00b7ge\u00df\u00b7ne", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Auf\u2019s Neu vor Sittah\u2019s Seele hin.", "tokens": ["Auf's", "Neu", "vor", "Sit\u00b7tah's", "See\u00b7le", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sie ruht, wie sonst in tiefen Schluchten", "tokens": ["Sie", "ruht", ",", "wie", "sonst", "in", "tie\u00b7fen", "Schluch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und h\u00f6rt dem Waldesrauschen zu,", "tokens": ["Und", "h\u00f6rt", "dem", "Wal\u00b7des\u00b7rau\u00b7schen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sie blickt, auf\u2019s Neu, von Felsenbuchten", "tokens": ["Sie", "blickt", ",", "auf's", "Neu", ",", "von", "Fel\u00b7sen\u00b7buch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Auf Meeressturm und Meeresruh;", "tokens": ["Auf", "Mee\u00b7res\u00b7sturm", "und", "Mee\u00b7res\u00b7ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sie schaut der Abendr\u00f6the Streifen,", "tokens": ["Sie", "schaut", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "An denen einst ihr Auge hing,", "tokens": ["An", "de\u00b7nen", "einst", "ihr", "Au\u00b7ge", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und m\u00f6chte wieder danach greifen,", "tokens": ["Und", "m\u00f6ch\u00b7te", "wie\u00b7der", "da\u00b7nach", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wie Kinder nach dem Schmetterling.", "tokens": ["Wie", "Kin\u00b7der", "nach", "dem", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sie h\u00f6rt des Birkhuhns Kreischen wieder,", "tokens": ["Sie", "h\u00f6rt", "des", "Birk\u00b7huhns", "Krei\u00b7schen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Sie sieht das Irrlicht wieder gl\u00fchn,", "tokens": ["Sie", "sieht", "das", "Irr\u00b7licht", "wie\u00b7der", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Das l\u00e4ngs der Haide, auf und nieder,", "tokens": ["Das", "l\u00e4ngs", "der", "Hai\u00b7de", ",", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NE", "$,", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Unst\u00e4t wie sie, zu wandern schien;", "tokens": ["Un\u00b7st\u00e4t", "wie", "sie", ",", "zu", "wan\u00b7dern", "schien", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "$,", "PTKZU", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Sie m\u00f6chte wieder, wieder wandern", "tokens": ["Sie", "m\u00f6ch\u00b7te", "wie\u00b7der", ",", "wie\u00b7der", "wan\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "So weit die Himmel Gottes blaun,", "tokens": ["So", "weit", "die", "Him\u00b7mel", "Got\u00b7tes", "blaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Auf\u2019s Neu, von einem Tag zum andern,", "tokens": ["Auf's", "Neu", ",", "von", "ei\u00b7nem", "Tag", "zum", "an\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mit ihren Br\u00fcdern H\u00fctten baun. \u2014", "tokens": ["Mit", "ih\u00b7ren", "Br\u00fc\u00b7dern", "H\u00fct\u00b7ten", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Da, allgemach, erstirbt die Weise,", "tokens": ["Da", ",", "all\u00b7ge\u00b7mach", ",", "er\u00b7stirbt", "die", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und gl\u00fchend, ohne Blick und Wort,", "tokens": ["Und", "gl\u00fc\u00b7hend", ",", "oh\u00b7ne", "Blick", "und", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Schleicht Sittah aus dem Saal und leise", "tokens": ["Schleicht", "Sit\u00b7tah", "aus", "dem", "Saal", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "ART", "NN", "KON", "ADJD"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.36": {"text": "Sich von des Gatten Seite fort.", "tokens": ["Sich", "von", "des", "Gat\u00b7ten", "Sei\u00b7te", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}