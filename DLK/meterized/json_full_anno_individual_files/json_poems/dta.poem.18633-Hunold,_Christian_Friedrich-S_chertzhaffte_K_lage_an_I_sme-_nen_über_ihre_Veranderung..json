{"dta.poem.18633": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "S chertzhaffte  K lage an  I sme-  \n nen \u00fcber ihre Veranderung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Ismene f\u00fchrt bey mir den strengsten Orden ein/", "tokens": ["Is\u00b7me\u00b7ne", "f\u00fchrt", "bey", "mir", "den", "strengs\u00b7ten", "Or\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ich bin kein Catholic und mu\u00df die Fasten haben/", "tokens": ["Ich", "bin", "kein", "Cat\u00b7ho\u00b7lic", "und", "mu\u00df", "die", "Fas\u00b7ten", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NE", "KON", "VMFIN", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Fischen krieg ich nichts/ das Fleisch mu\u00df andre laben/", "tokens": ["Von", "Fi\u00b7schen", "krieg", "ich", "nichts", "/", "das", "Fleisch", "mu\u00df", "and\u00b7re", "la\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIS", "$(", "ART", "NN", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es wird vor mich der Tod in ihren Topffe seyn.", "tokens": ["Es", "wird", "vor", "mich", "der", "Tod", "in", "ih\u00b7ren", "Topf\u00b7fe", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wenn ihr nur der Tod nicht neues Leben giebt:", "tokens": ["Doch", "wenn", "ihr", "nur", "der", "Tod", "nicht", "neu\u00b7es", "Le\u00b7ben", "giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn wo die Keuschheit stirbt/ so gr\u00fcnt der Busch an Zweigen/", "tokens": ["Denn", "wo", "die", "Keuschheit", "stirbt", "/", "so", "gr\u00fcnt", "der", "Busch", "an", "Zwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$(", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die von der Fruchtbarkeit offt Widerwillen zeigen/", "tokens": ["Die", "von", "der", "Frucht\u00b7bar\u00b7keit", "offt", "Wi\u00b7der\u00b7wil\u00b7len", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch weil es andre letzt/ so macht es mich betr\u00fcbt.", "tokens": ["Doch", "weil", "es", "and\u00b7re", "letzt", "/", "so", "macht", "es", "mich", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADV", "$(", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich mu\u00df nur meine Kost an leeren Hilsen sehn/", "tokens": ["Ich", "mu\u00df", "nur", "mei\u00b7ne", "Kost", "an", "lee\u00b7ren", "Hil\u00b7sen", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie wenig wird dadurch ein hungriger erg\u00f6tzet/", "tokens": ["Wie", "we\u00b7nig", "wird", "da\u00b7durch", "ein", "hung\u00b7ri\u00b7ger", "er\u00b7g\u00f6t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PAV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was erst zu wohlfeil war/ wird nun so hoch gesch\u00e4tzet/", "tokens": ["Was", "erst", "zu", "wohl\u00b7feil", "war", "/", "wird", "nun", "so", "hoch", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKA", "ADJD", "VAFIN", "$(", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie soll ich nicht zuletzt in Schmertzen untergehn?", "tokens": ["Wie", "soll", "ich", "nicht", "zu\u00b7letzt", "in", "Schmert\u00b7zen", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach ja ich gr\u00e4me mich deswegen bi\u00df ins Grab/", "tokens": ["Ach", "ja", "ich", "gr\u00e4\u00b7me", "mich", "des\u00b7we\u00b7gen", "bi\u00df", "ins", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "VVFIN", "PRF", "PAV", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und werde den Verlust nach seinen Wehrt beweinen/", "tokens": ["Und", "wer\u00b7de", "den", "Ver\u00b7lust", "nach", "sei\u00b7nen", "Wehrt", "be\u00b7wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Drum weil ihr alles mu\u00df von mir ver\u00e4chtlich scheinen/", "tokens": ["Drum", "weil", "ihr", "al\u00b7les", "mu\u00df", "von", "mir", "ver\u00b7\u00e4cht\u00b7lich", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "PIS", "VMFIN", "APPR", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So borg' ich Welschland auch die ", "tokens": ["So", "bor\u00b7g'", "ich", "Wel\u00b7schland", "auch", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "ART"], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}