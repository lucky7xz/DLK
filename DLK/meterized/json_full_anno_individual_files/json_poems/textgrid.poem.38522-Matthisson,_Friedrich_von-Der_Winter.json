{"textgrid.poem.38522": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis zur fr\u00f6hlichen Lenzesauferstehung,", "tokens": ["Bis", "zur", "fr\u00f6h\u00b7li\u00b7chen", "Len\u00b7ze\u00b7sau\u00b7fer\u00b7ste\u00b7hung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ruhn die farbigen Rasenbl\u00fcmlein alle,", "tokens": ["Ruhn", "die", "far\u00b7bi\u00b7gen", "Ra\u00b7sen\u00b7bl\u00fcm\u00b7lein", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und die duftenden Kr\u00e4uter dieser Wiese,", "tokens": ["Und", "die", "duf\u00b7ten\u00b7den", "Kr\u00e4u\u00b7ter", "die\u00b7ser", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ach! im starrenden, kalten Wintergrabe,", "tokens": ["Ach", "!", "im", "star\u00b7ren\u00b7den", ",", "kal\u00b7ten", "Win\u00b7ter\u00b7gra\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Von hellblinkenden Floken \u00fcberflimmert!", "tokens": ["Von", "hell\u00b7blin\u00b7ken\u00b7den", "Flo\u00b7ken", "\u00fc\u00b7berf\u00b7lim\u00b7mert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Aehnlich Todtengerippen, stehn die B\u00e4ume,", "tokens": ["A\u00b7ehn\u00b7lich", "Tod\u00b7ten\u00b7ge\u00b7rip\u00b7pen", ",", "stehn", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Ihres s\u00e4uselnden Bl\u00e4tterschmuks entkleidet;", "tokens": ["Ih\u00b7res", "s\u00e4u\u00b7seln\u00b7den", "Bl\u00e4t\u00b7ter\u00b7schmuks", "ent\u00b7klei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Wo, geh\u00fcllet in gr\u00fcne Zweigbeschattung,", "tokens": ["Wo", ",", "ge\u00b7h\u00fcl\u00b7let", "in", "gr\u00fc\u00b7ne", "Zweig\u00b7be\u00b7schat\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Oft die fl\u00f6tende S\u00e4ngerin der Mainacht", "tokens": ["Oft", "die", "fl\u00f6\u00b7ten\u00b7de", "S\u00e4n\u00b7ge\u00b7rin", "der", "Mai\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ihre schmelzenden Zauberlieder t\u00f6nte!", "tokens": ["Ih\u00b7re", "schmel\u00b7zen\u00b7den", "Zau\u00b7berl\u00b7ie\u00b7der", "t\u00f6n\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Wes die J\u00fcnglinge sich und alle M\u00e4dchen,", "tokens": ["Wes", "die", "J\u00fcng\u00b7lin\u00b7ge", "sich", "und", "al\u00b7le", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PRF", "KON", "PIAT", "NN", "$,"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Auf den Blumengefilden, weiland freuten,", "tokens": ["Auf", "den", "Blu\u00b7men\u00b7ge\u00b7fil\u00b7den", ",", "wei\u00b7land", "freu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Hat, verheerender Winternord! dein Odem", "tokens": ["Hat", ",", "ver\u00b7hee\u00b7ren\u00b7der", "Win\u00b7ter\u00b7nord", "!", "dein", "O\u00b7dem"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Von den frostigen Fluren weggew\u00fctet!", "tokens": ["Von", "den", "fros\u00b7ti\u00b7gen", "Flu\u00b7ren", "weg\u00b7ge\u00b7w\u00fc\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Jedes d\u00e4mmernde Zweigdach f\u00fcr die Liebe,", "tokens": ["Je\u00b7des", "d\u00e4m\u00b7mern\u00b7de", "Zweig\u00b7dach", "f\u00fcr", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Jede schattende Laube f\u00fcr das Kelchglas,", "tokens": ["Je\u00b7de", "schat\u00b7ten\u00b7de", "Lau\u00b7be", "f\u00fcr", "das", "Kelchg\u00b7las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Hast in \u00f6dige W\u00fcste du gewandelt!", "tokens": ["Hast", "in", "\u00f6\u00b7di\u00b7ge", "W\u00fcs\u00b7te", "du", "ge\u00b7wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Unbek\u00fcmmert der tausend M\u00e4dchenthr\u00e4nen,", "tokens": ["Un\u00b7be\u00b7k\u00fcm\u00b7mert", "der", "tau\u00b7send", "M\u00e4d\u00b7chen\u00b7thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "CARD", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Unbek\u00fcmmert der bangen J\u00fcnglingsseufzer!", "tokens": ["Un\u00b7be\u00b7k\u00fcm\u00b7mert", "der", "ban\u00b7gen", "J\u00fcng\u00b7lings\u00b7seuf\u00b7zer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "O des m\u00fcrrischen Freudentilgers! selbst des", "tokens": ["O", "des", "m\u00fcr\u00b7ri\u00b7schen", "Freu\u00b7den\u00b7til\u00b7gers", "!", "selbst", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$.", "ADV", "ART"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Mir so heiligen Pl\u00e4zchens nicht zu schonen,", "tokens": ["Mir", "so", "hei\u00b7li\u00b7gen", "Pl\u00e4z\u00b7chens", "nicht", "zu", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.rel