{"dta.poem.7756": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol Hirten seid ihr all, und wisset, jeder werde", "tokens": ["Wol", "Hir\u00b7ten", "seid", "ihr", "all", ",", "und", "wis\u00b7set", ",", "je\u00b7der", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "PPER", "PIAT", "$,", "KON", "VVFIN", "$,", "PIS", "VAFIN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Mir geben Rechenschaft von sich und seiner Herde.", "tokens": ["Mir", "ge\u00b7ben", "Re\u00b7chen\u00b7schaft", "von", "sich", "und", "sei\u00b7ner", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "NN", "APPR", "PRF", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du K\u00f6nig bist ein Hirt, der Volksherd' angestammt,", "tokens": ["Du", "K\u00f6\u00b7nig", "bist", "ein", "Hirt", ",", "der", "Volks\u00b7herd'", "an\u00b7ge\u00b7stammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibst mir Rechenschaft von deinem Hirtenamt.", "tokens": ["Und", "gibst", "mir", "Re\u00b7chen\u00b7schaft", "von", "dei\u00b7nem", "Hir\u00b7ten\u00b7amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du Richter bist ein Hirt des Rechtes in dem Lande,", "tokens": ["Du", "Rich\u00b7ter", "bist", "ein", "Hirt", "des", "Rech\u00b7tes", "in", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibst mir Rechenschaft von deinem Hirtenstande.", "tokens": ["Und", "gibst", "mir", "Re\u00b7chen\u00b7schaft", "von", "dei\u00b7nem", "Hir\u00b7ten\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du Priester bist ein Hirt in meines Stalles H\u00fcrde,", "tokens": ["Du", "Pries\u00b7ter", "bist", "ein", "Hirt", "in", "mei\u00b7nes", "Stal\u00b7les", "H\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibst mir Rechenschaft von deiner Hirtenw\u00fcrde.", "tokens": ["Und", "gibst", "mir", "Re\u00b7chen\u00b7schaft", "von", "dei\u00b7ner", "Hir\u00b7ten\u00b7w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du Lehrer bist ein Hirt in Zucht und Unterricht,", "tokens": ["Du", "Leh\u00b7rer", "bist", "ein", "Hirt", "in", "Zucht", "und", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibst mir Rechenschaft von deiner Hirtenpflicht.", "tokens": ["Und", "gibst", "mir", "Re\u00b7chen\u00b7schaft", "von", "dei\u00b7ner", "Hir\u00b7ten\u00b7pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du Krieger bist ein Hirt, und wachst f\u00fcr Schutz und Ehre,", "tokens": ["Du", "Krie\u00b7ger", "bist", "ein", "Hirt", ",", "und", "wachst", "f\u00fcr", "Schutz", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du gibst mir Rechenschaft von deiner Hirtenwehre.", "tokens": ["Du", "gibst", "mir", "Re\u00b7chen\u00b7schaft", "von", "dei\u00b7ner", "Hir\u00b7ten\u00b7weh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du B\u00fcrger bist ein Hirt im anvertrauten Gut,", "tokens": ["Du", "B\u00fcr\u00b7ger", "bist", "ein", "Hirt", "im", "an\u00b7ver\u00b7trau\u00b7ten", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibst mir Rechenschaft von deiner Hirtenhut.", "tokens": ["Und", "gibst", "mir", "Re\u00b7chen\u00b7schaft", "von", "dei\u00b7ner", "Hir\u00b7ten\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du Vater bist ein Hirt, f\u00fcr Weib und Kind erlesen,", "tokens": ["Du", "Va\u00b7ter", "bist", "ein", "Hirt", ",", "f\u00fcr", "Weib", "und", "Kind", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibst mir Rechenschaft von deinem Hirtenwesen.", "tokens": ["Und", "gibst", "mir", "Re\u00b7chen\u00b7schaft", "von", "dei\u00b7nem", "Hir\u00b7ten\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Du Diener bist ein Hirt f\u00fcr deines Herren Habe,", "tokens": ["Du", "Die\u00b7ner", "bist", "ein", "Hirt", "f\u00fcr", "dei\u00b7nes", "Her\u00b7ren", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibst mir Rechenschaft von deinem Hirtenstabe.", "tokens": ["Und", "gibst", "mir", "Re\u00b7chen\u00b7schaft", "von", "dei\u00b7nem", "Hir\u00b7ten\u00b7sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct