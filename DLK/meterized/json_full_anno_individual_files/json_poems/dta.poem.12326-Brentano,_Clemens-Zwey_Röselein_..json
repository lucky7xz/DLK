{"dta.poem.12326": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Zwey R\u00f6selein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Knabe. Geh ich zum Br\u00fcnnelein,               ", "tokens": ["Kna\u00b7be", ".", "Geh", "ich", "zum", "Br\u00fcn\u00b7nel\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Trink aber nicht,", "tokens": ["Trink", "a\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Such ich mein Sch\u00e4tzelein,", "tokens": ["Such", "ich", "mein", "Sch\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Finds aber nicht.", "tokens": ["Finds", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Setz ich mich so allein", "tokens": ["Setz", "ich", "mich", "so", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aufs gr\u00fcne Gras,", "tokens": ["Aufs", "gr\u00fc\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Fallen zwei R\u00f6selein", "tokens": ["Fal\u00b7len", "zwei", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NN", "CARD", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mir in den Scho\u00df.", "tokens": ["Mir", "in", "den", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Diese zwei R\u00f6selein", "tokens": ["Die\u00b7se", "zwei", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "CARD", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Gelten mir nicht,", "tokens": ["Gel\u00b7ten", "mir", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ists nicht mein Sch\u00e4tzelein,", "tokens": ["Ists", "nicht", "mein", "Sch\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die sie mir bricht?", "tokens": ["Die", "sie", "mir", "bricht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Diese zwei R\u00f6selein", "tokens": ["Die\u00b7se", "zwei", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "CARD", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sind rosenroth,", "tokens": ["Sind", "ro\u00b7sen\u00b7roth", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Lebt noch mein Sch\u00e4tzelein,", "tokens": ["Lebt", "noch", "mein", "Sch\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Oder ists todt.", "tokens": ["O\u00b7der", "ists", "todt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wend ich mein Aeugelein", "tokens": ["Wend", "ich", "mein", "A\u00b7e\u00b7u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rum und um her,", "tokens": ["Rum", "und", "um", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "APPR", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Seh ich mein Sch\u00e4tzelein", "tokens": ["Seh", "ich", "mein", "Sch\u00e4t\u00b7zel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Beim andern stehn.", "tokens": ["Beim", "an\u00b7dern", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wirft ihn mit R\u00f6selein,", "tokens": ["Wirft", "ihn", "mit", "R\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Treffen mich thut,", "tokens": ["Tref\u00b7fen", "mich", "thut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Meint sie w\u00e4r ganz allein,", "tokens": ["Meint", "sie", "w\u00e4r", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das thut kein gut.", "tokens": ["Das", "thut", "kein", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "W\u00e4rst du mein Sch\u00e4tzelein,", "tokens": ["W\u00e4rst", "du", "mein", "Sch\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4rst du mir gut?", "tokens": ["W\u00e4rst", "du", "mir", "gut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Steck die zwei R\u00f6selein", "tokens": ["Steck", "die", "zwei", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "CARD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir auf den Hut.", "tokens": ["Mir", "auf", "den", "Hut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "M\u00e4dchen. Wirst doch nicht reisen fort,               ", "tokens": ["M\u00e4d\u00b7chen", ".", "Wirst", "doch", "nicht", "rei\u00b7sen", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ADV", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Hast ja noch Zeit.", "tokens": ["Hast", "ja", "noch", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Knabe. Ja ich will reisen fort,               ", "tokens": ["Kna\u00b7be", ".", "Ja", "ich", "will", "rei\u00b7sen", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "PPER", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mein Weg ist weit.", "tokens": ["Mein", "Weg", "ist", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hin, wo ihr treue Lieb", "tokens": ["Hin", ",", "wo", "ihr", "treu\u00b7e", "Lieb"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Kein M\u00e4gdlein bricht.", "tokens": ["Kein", "M\u00e4gd\u00b7lein", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "M\u00e4dchen. Schatz nimm zu Hau\u00df vor Lieb,               ", "tokens": ["M\u00e4d\u00b7chen", ".", "Schatz", "nimm", "zu", "Hau\u00df", "vor", "Lieb", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hin findst du nicht.", "tokens": ["Hin", "findst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "R\u00f6\u00dflein am Strauche bl\u00fchn", "tokens": ["R\u00f6\u00df\u00b7lein", "am", "Strau\u00b7che", "bl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVINF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ewig doch nicht,", "tokens": ["E\u00b7wig", "doch", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Lieb ist so lang nur gr\u00fcn,", "tokens": ["Lieb", "ist", "so", "lang", "nur", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bis man sie bricht.", "tokens": ["Bis", "man", "sie", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Nimm die zwei R\u00f6selein", "tokens": ["Nimm", "die", "zwei", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "CARD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf deinen Hut,", "tokens": ["Auf", "dei\u00b7nen", "Hut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ewig beinander sein", "tokens": ["E\u00b7wig", "bei\u00b7nan\u00b7der", "sein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Thut auch kein gut.", "tokens": ["Thut", "auch", "kein", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "ADJD", "$."], "meter": "---+", "measure": "unknown.measure.single"}}, "stanza.12": {"line.1": {"text": "Wenn die zwei R\u00f6selein", "tokens": ["Wenn", "die", "zwei", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "CARD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht mehr sind roth,", "tokens": ["Nicht", "mehr", "sind", "roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Werf sie in Flu\u00df hinein,", "tokens": ["Werf", "sie", "in", "Flu\u00df", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denk ich w\u00e4r todt.", "tokens": ["Denk", "ich", "w\u00e4r", "todt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Knabe. Bist du todt alzumahl,               ", "tokens": ["Kna\u00b7be", ".", "Bist", "du", "todt", "al\u00b7zu\u00b7mahl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Thut mirs nicht leid,", "tokens": ["Thut", "mirs", "nicht", "leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Untreu findt \u00fcberall,", "tokens": ["Un\u00b7treu", "findt", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wen sie erfreut.", "tokens": ["Wen", "sie", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}