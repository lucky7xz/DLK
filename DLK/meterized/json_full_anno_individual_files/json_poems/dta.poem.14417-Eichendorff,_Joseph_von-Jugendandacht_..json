{"dta.poem.14417": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie in einer Blume himmelblauen", "tokens": ["Wie", "in", "ei\u00b7ner", "Blu\u00b7me", "him\u00b7mel\u00b7blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Grund, wo schlummernd tr\u00e4umen stille Regenbogen,", "tokens": ["Grund", ",", "wo", "schlum\u00b7mernd", "tr\u00e4u\u00b7men", "stil\u00b7le", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ist mein Leben ein unendlich Schauen,", "tokens": ["Ist", "mein", "Le\u00b7ben", "ein", "un\u00b7end\u00b7lich", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Klar durch's ganze Herz Ein s\u00fc\u00dfes Bild gezogen.", "tokens": ["Klar", "durch's", "gan\u00b7ze", "Herz", "Ein", "s\u00fc\u00b7\u00dfes", "Bild", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}