{"dta.poem.21848": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ein grosser Sturmwind kahm geflogen", "tokens": ["Ein", "gros\u00b7ser", "Sturm\u00b7wind", "kahm", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bey mir kahm Amor eingezogen", "tokens": ["bey", "mir", "kahm", "A\u00b7mor", "ein\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und st\u00fcrmete viel h\u00e4rter an", "tokens": ["und", "st\u00fcr\u00b7me\u00b7te", "viel", "h\u00e4r\u00b7ter", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "h\u00e4tt\u2019 ich ihm damahls Plazz gegeben/", "tokens": ["h\u00e4tt'", "ich", "ihm", "da\u00b7mahls", "Plazz", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! da\u00df ich mich nicht schikken kan", "tokens": ["Ach", "!", "da\u00df", "ich", "mich", "nicht", "schik\u00b7ken", "kan"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}