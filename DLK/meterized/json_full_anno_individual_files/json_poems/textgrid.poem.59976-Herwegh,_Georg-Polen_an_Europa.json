{"textgrid.poem.59976": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Polen an Europa", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der heil'ge Krieg ist neu entglommen,", "tokens": ["Der", "heil'\u00b7ge", "Krieg", "ist", "neu", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die S\u00f6hne Polens werden wach,", "tokens": ["Die", "S\u00f6h\u00b7ne", "Po\u00b7lens", "wer\u00b7den", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir haben unser Schwert genommen", "tokens": ["Wir", "ha\u00b7ben", "un\u00b7ser", "Schwert", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach f\u00fcnfzehn Jahren tiefer Schmach.", "tokens": ["Nach", "f\u00fcnf\u00b7zehn", "Jah\u00b7ren", "tie\u00b7fer", "Schmach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An dich, du stumme Zeugin unsrer Klage", "tokens": ["An", "dich", ",", "du", "stum\u00b7me", "Zeu\u00b7gin", "uns\u00b7rer", "Kla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und unsrer namenlosen Qual,", "tokens": ["Und", "uns\u00b7rer", "na\u00b7men\u00b7lo\u00b7sen", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An dich, Europa, richten wir die Frage:", "tokens": ["An", "dich", ",", "Eu\u00b7ro\u00b7pa", ",", "rich\u00b7ten", "wir", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NE", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verl\u00e4\u00dft du uns zum zweitenmal?", "tokens": ["Ver\u00b7l\u00e4\u00dft", "du", "uns", "zum", "zwei\u00b7ten\u00b7mal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist's nicht ein Kampf f\u00fcr deine Sache?", "tokens": ["Ist's", "nicht", "ein", "Kampf", "f\u00fcr", "dei\u00b7ne", "Sa\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kampf, von jedem Flecken rein?", "tokens": ["Ein", "Kampf", ",", "von", "je\u00b7dem", "Fle\u00b7cken", "rein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf! Polens Adler will der Rache", "tokens": ["Auf", "!", "Po\u00b7lens", "Ad\u00b7ler", "will", "der", "Ra\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$.", "NN", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gebenedeiter Engel sein.", "tokens": ["Ge\u00b7be\u00b7ne\u00b7dei\u00b7ter", "En\u00b7gel", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Saat ist reif, es rauschen unsre Sensen,", "tokens": ["Die", "Saat", "ist", "reif", ",", "es", "rau\u00b7schen", "uns\u00b7re", "Sen\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wir schwingen auch f\u00fcr dich den Stahl:", "tokens": ["Wir", "schwin\u00b7gen", "auch", "f\u00fcr", "dich", "den", "Stahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Hoffnung sieh in unsern Augen gl\u00e4nzen \u2013", "tokens": ["Die", "Hoff\u00b7nung", "sieh", "in", "un\u00b7sern", "Au\u00b7gen", "gl\u00e4n\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verla\u00df uns nicht zum zweitenmal!", "tokens": ["Ver\u00b7la\u00df", "uns", "nicht", "zum", "zwei\u00b7ten\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du liegst an alter Schuld erkranket \u2013,", "tokens": ["Du", "liegst", "an", "al\u00b7ter", "Schuld", "er\u00b7kran\u00b7ket", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Europa, o ents\u00fchne dich!", "tokens": ["Eu\u00b7ro\u00b7pa", ",", "o", "ent\u00b7s\u00fch\u00b7ne", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schnell, solang die Waage schwanket,", "tokens": ["Und", "schnell", ",", "so\u00b7lang", "die", "Waa\u00b7ge", "schwan\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wirf noch dein Herz hinein f\u00fcr mich.", "tokens": ["Wirf", "noch", "dein", "Herz", "hin\u00b7ein", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Zaudern w\u00e4re dreifach ein Verbrechen,", "tokens": ["Dein", "Zau\u00b7dern", "w\u00e4\u00b7re", "drei\u00b7fach", "ein", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn dreifach ist der Feinde Zahl;", "tokens": ["Denn", "drei\u00b7fach", "ist", "der", "Fein\u00b7de", "Zahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr dich und mich ein dreifach Joch zu brechen,", "tokens": ["F\u00fcr", "dich", "und", "mich", "ein", "drei\u00b7fach", "Joch", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verla\u00df mich nicht zum zweitenmal.", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", "zum", "zwei\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein wildes Meer von Aufruhrflammen,", "tokens": ["Ein", "wil\u00b7des", "Meer", "von", "Auf\u00b7ruhr\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zorn der ganzen Welt vereint,", "tokens": ["Der", "Zorn", "der", "gan\u00b7zen", "Welt", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schlag \u00fcber seinem Haupt zusammen", "tokens": ["Schlag", "\u00fc\u00b7ber", "sei\u00b7nem", "Haupt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tr\u00fcmmre nieder unsern Feind!", "tokens": ["Und", "tr\u00fcmm\u00b7re", "nie\u00b7der", "un\u00b7sern", "Feind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Deutschland! sei zwischen uns ein Bundeszeichen,", "tokens": ["Deutschland", "!", "sei", "zwi\u00b7schen", "uns", "ein", "Bun\u00b7des\u00b7zei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der Freiheit loderndes Signal!", "tokens": ["Der", "Frei\u00b7heit", "lo\u00b7dern\u00b7des", "Sig\u00b7nal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch Polens Aar tr\u00e4gt einen Kranz von Eichen:", "tokens": ["Auch", "Po\u00b7lens", "Aar", "tr\u00e4gt", "ei\u00b7nen", "Kranz", "von", "Ei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verla\u00df mich nicht zum zweitenmal.", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", "zum", "zwei\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf, Preu\u00dfen, sch\u00fcttle deine Ketten!", "tokens": ["Auf", ",", "Preu\u00b7\u00dfen", ",", "sch\u00fctt\u00b7le", "dei\u00b7ne", "Ket\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erk\u00e4mpf dein Recht, der Tag ist da!", "tokens": ["Er\u00b7k\u00e4mpf", "dein", "Recht", ",", "der", "Tag", "ist", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es gilt ja mich und euch zu retten \u2013", "tokens": ["Es", "gilt", "ja", "mich", "und", "euch", "zu", "ret\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "KON", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf, Ungarn! auf, Italia!", "tokens": ["Auf", ",", "Un\u00b7garn", "!", "auf", ",", "I\u00b7ta\u00b7lia", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "$,", "NE", "$.", "PTKVZ", "$,", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Galliens Hahn, sprich, bist du blind geworden", "tokens": ["O", "Gal\u00b7li\u00b7ens", "Hahn", ",", "sprich", ",", "bist", "du", "blind", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$,", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "VAPP"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und ahnst du nicht den Morgenstrahl?", "tokens": ["Und", "ahnst", "du", "nicht", "den", "Mor\u00b7gen\u00b7strahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie nahn, sie w\u00fcten, die Barbarenhorden \u2013", "tokens": ["Sie", "nahn", ",", "sie", "w\u00fc\u00b7ten", ",", "die", "Bar\u00b7ba\u00b7ren\u00b7hor\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Verla\u00dft uns nicht zum zweitenmal!", "tokens": ["Ver\u00b7la\u00dft", "uns", "nicht", "zum", "zwei\u00b7ten\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKN