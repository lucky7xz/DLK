{"textgrid.poem.48832": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "57. Zur Zeit seiner Versto\u00dfung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kaufman, der sein Gut nur einem Schiffe traut,", "tokens": ["Ein", "Kauf\u00b7man", ",", "der", "sein", "Gut", "nur", "ei\u00b7nem", "Schif\u00b7fe", "traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist hochgef\u00e4rlich dran, in dem es bald kan kommen,", "tokens": ["ist", "hoch\u00b7ge\u00b7f\u00e4r\u00b7lich", "dran", ",", "in", "dem", "es", "bald", "kan", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df ihm auf einen Sto\u00df sein Ganzes wird genommen.", "tokens": ["da\u00df", "ihm", "auf", "ei\u00b7nen", "Sto\u00df", "sein", "Gan\u00b7zes", "wird", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der fehlt, der allzuviel auf ein Gel\u00fccke traut.", "tokens": ["Der", "fehlt", ",", "der", "all\u00b7zu\u00b7viel", "auf", "ein", "Ge\u00b7l\u00fc\u00b7cke", "traut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gedenk' ich nun an mich, so schauret mir die Haut.", "tokens": ["Ge\u00b7denk'", "ich", "nun", "an", "mich", ",", "so", "schau\u00b7ret", "mir", "die", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Schiff, das ist entzwei, mein Gut ist weggeschwommen.", "tokens": ["Mein", "Schiff", ",", "das", "ist", "ent\u00b7zwei", ",", "mein", "Gut", "ist", "weg\u00b7ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nichts mehr, das ist mein Rest, das machet kurze Summen.", "tokens": ["Nichts", "mehr", ",", "das", "ist", "mein", "Rest", ",", "das", "ma\u00b7chet", "kur\u00b7ze", "Sum\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich habe M\u00fch' und Angst, ein ander meine Braut.", "tokens": ["Ich", "ha\u00b7be", "M\u00fch'", "und", "Angst", ",", "ein", "an\u00b7der", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,", "ART", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich Ungl\u00fcckseliger! Mein Herze wird zerri\u00dfen,", "tokens": ["Ich", "Un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ger", "!", "Mein", "Her\u00b7ze", "wird", "zer\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mein Sinn ist ohne sich. Mein Geist zeucht von mir aus,", "tokens": ["mein", "Sinn", "ist", "oh\u00b7ne", "sich", ".", "Mein", "Geist", "zeucht", "von", "mir", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PRF", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mein Alles wird nun Nichts. Was wird doch endlich drau\u00df?", "tokens": ["mein", "Al\u00b7les", "wird", "nun", "Nichts", ".", "Was", "wird", "doch", "end\u00b7lich", "drau\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "VAFIN", "ADV", "PIS", "$.", "PWS", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "W\u00e4r' eins doch \u00fcbrig noch, so wolt' ich Alles mi\u00dfen.", "tokens": ["W\u00e4r'", "eins", "doch", "\u00fcb\u00b7rig", "noch", ",", "so", "wolt'", "ich", "Al\u00b7les", "mi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "ADV", "$,", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text"