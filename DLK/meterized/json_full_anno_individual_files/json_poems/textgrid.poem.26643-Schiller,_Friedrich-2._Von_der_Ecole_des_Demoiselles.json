{"textgrid.poem.26643": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Von der Ecole des Demoiselles", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Elysische Gef\u00fchle dr\u00e4ngen", "tokens": ["E\u00b7ly\u00b7si\u00b7sche", "Ge\u00b7f\u00fch\u00b7le", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herzens Saiten zu Ges\u00e4ngen,", "tokens": ["Des", "Her\u00b7zens", "Sai\u00b7ten", "zu", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein teurer Name weckte sie. \u2013", "tokens": ["Ein", "teu\u00b7rer", "Na\u00b7me", "weck\u00b7te", "sie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4gt nicht der Kinder Herz mit k\u00fchnern Schl\u00e4gen", "tokens": ["Schl\u00e4gt", "nicht", "der", "Kin\u00b7der", "Herz", "mit", "k\u00fch\u00b7nern", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der sanften Mutter Freudenfest entgegen", "tokens": ["Der", "sanf\u00b7ten", "Mut\u00b7ter", "Freu\u00b7den\u00b7fest", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schmilzt dahin in Wonnemelodie?", "tokens": ["Und", "schmilzt", "da\u00b7hin", "in", "Won\u00b7ne\u00b7me\u00b7lo\u00b7die", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie sollten wir jetzt f\u00fchllos schweigen,", "tokens": ["Wie", "soll\u00b7ten", "wir", "jetzt", "f\u00fchl\u00b7los", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da tausend Taten uns bezeugen,", "tokens": ["Da", "tau\u00b7send", "Ta\u00b7ten", "uns", "be\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da jeder Mund \u2013 da jedes Auge spricht; \u2013", "tokens": ["Da", "je\u00b7der", "Mund", "\u2013", "da", "je\u00b7des", "Au\u00b7ge", "spricht", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "ADV", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ist uns Franziska Mutter nicht?", "tokens": ["Ist", "uns", "Fran\u00b7zis\u00b7ka", "Mut\u00b7ter", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erlauben Sie dem kindlichen Entz\u00fccken,", "tokens": ["Er\u00b7lau\u00b7ben", "Sie", "dem", "kind\u00b7li\u00b7chen", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich Ihnen heute scheu zu nahn,", "tokens": ["Sich", "Ih\u00b7nen", "heu\u00b7te", "scheu", "zu", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "ADV", "ADJD", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O sehen Sie mit m\u00fctterlichen Blicken,", "tokens": ["O", "se\u00b7hen", "Sie", "mit", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was, unsre innige Verehrung auszudr\u00fccken,", "tokens": ["Was", ",", "uns\u00b7re", "in\u00b7ni\u00b7ge", "Ver\u00b7eh\u00b7rung", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PPOSAT", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir Ihnen darzubringen wagen, an!", "tokens": ["Wir", "Ih\u00b7nen", "dar\u00b7zu\u00b7brin\u00b7gen", "wa\u00b7gen", ",", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPER", "PAV", "VVINF", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erlauben Sie der sch\u00fcchternen Empfindung,", "tokens": ["Er\u00b7lau\u00b7ben", "Sie", "der", "sch\u00fcch\u00b7ter\u00b7nen", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr Sie, der M\u00fctter w\u00fcrdigste, zu gl\u00fchn,", "tokens": ["F\u00fcr", "Sie", ",", "der", "M\u00fct\u00b7ter", "w\u00fcr\u00b7digs\u00b7te", ",", "zu", "gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Erlauben Sie die k\u00fchne, stolze Wendung \u2013", "tokens": ["Er\u00b7lau\u00b7ben", "Sie", "die", "k\u00fch\u00b7ne", ",", "stol\u00b7ze", "Wen\u00b7dung", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn heute, heut dem Dank sich zu entziehn,", "tokens": ["Denn", "heu\u00b7te", ",", "heut", "dem", "Dank", "sich", "zu", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "W\u00e4r Frevel, w\u00e4r die str\u00e4flichste Verblendung!", "tokens": ["W\u00e4r", "Fre\u00b7vel", ",", "w\u00e4r", "die", "str\u00e4f\u00b7lichs\u00b7te", "Ver\u00b7blen\u00b7dung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn Dankbarkeit, die aus dem Herzen flie\u00dfet,", "tokens": ["Wenn", "Dank\u00b7bar\u00b7keit", ",", "die", "aus", "dem", "Her\u00b7zen", "flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn der Verspruch, stets auf der Tugend Pfad zu gehn,", "tokens": ["Wenn", "der", "Ver\u00b7spruch", ",", "stets", "auf", "der", "Tu\u00b7gend", "Pfad", "zu", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wenn Tr\u00e4nen, die die sanfte R\u00fchrung gie\u00dfet,", "tokens": ["Wenn", "Tr\u00e4\u00b7nen", ",", "die", "die", "sanf\u00b7te", "R\u00fch\u00b7rung", "gie\u00b7\u00dfet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn W\u00fcnsche, die empor zum Himmel flehn,", "tokens": ["Wenn", "W\u00fcn\u00b7sche", ",", "die", "em\u00b7por", "zum", "Him\u00b7mel", "flehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PTKVZ", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O wenn der Seelen feurigstes Empfinden", "tokens": ["O", "wenn", "der", "See\u00b7len", "feu\u00b7rigs\u00b7tes", "Emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Huld der besten Mutter lohnen k\u00f6nnten,", "tokens": ["Die", "Huld", "der", "bes\u00b7ten", "Mut\u00b7ter", "loh\u00b7nen", "k\u00f6nn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie ganz sollt unser Wesen nur Empfindung sein!", "tokens": ["Wie", "ganz", "sollt", "un\u00b7ser", "We\u00b7sen", "nur", "Emp\u00b7fin\u00b7dung", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPOSAT", "NN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nie sollten unsre Tr\u00e4nen, nie versiegen,", "tokens": ["Nie", "soll\u00b7ten", "uns\u00b7re", "Tr\u00e4\u00b7nen", ",", "nie", "ver\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zum Himmel sollten ewig unsre W\u00fcnsche fliegen,", "tokens": ["Zum", "Him\u00b7mel", "soll\u00b7ten", "e\u00b7wig", "uns\u00b7re", "W\u00fcn\u00b7sche", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Franzisken wollten wir ein ganzes Leben weihn!", "tokens": ["Fran\u00b7zis\u00b7ken", "woll\u00b7ten", "wir", "ein", "gan\u00b7zes", "Le\u00b7ben", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wenn auch das Gef\u00fchl, das unser Herz durchflossen,", "tokens": ["Doch", "wenn", "auch", "das", "Ge\u00b7f\u00fchl", ",", "das", "un\u00b7ser", "Herz", "durch\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei aller Liebe reichlichem Genu\u00df,", "tokens": ["Bei", "al\u00b7ler", "Lie\u00b7be", "reich\u00b7li\u00b7chem", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Womit Sie, Edelste! uns \u00fcbergossen,", "tokens": ["Wo\u00b7mit", "Sie", ",", "E\u00b7dels\u00b7te", "!", "uns", "\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NN", "$.", "PPER", "VVPP", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Err\u00f6ten und erlahmen mu\u00df \u2013", "tokens": ["Er\u00b7r\u00f6\u00b7ten", "und", "er\u00b7lah\u00b7men", "mu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So hebt uns doch das selige Vertrauen:", "tokens": ["So", "hebt", "uns", "doch", "das", "se\u00b7li\u00b7ge", "Ver\u00b7trau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Franziska wird mit gnadevollem Blick", "tokens": ["Fran\u00b7zis\u00b7ka", "wird", "mit", "gna\u00b7de\u00b7vol\u00b7lem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf ihrer T\u00f6chter schwaches Opfer schauen \u2013", "tokens": ["Auf", "ih\u00b7rer", "T\u00f6ch\u00b7ter", "schwa\u00b7ches", "Op\u00b7fer", "schau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Franziska st\u00f6\u00dft die Herzen nie zur\u00fcck!", "tokens": ["Fran\u00b7zis\u00b7ka", "st\u00f6\u00dft", "die", "Her\u00b7zen", "nie", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.9": {"text": "Und feuervoller wird der Vorsatz uns beleben,", "tokens": ["Und", "feu\u00b7er\u00b7vol\u00b7ler", "wird", "der", "Vor\u00b7satz", "uns", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem Meisterbild der Tugend nachzustreben!", "tokens": ["Dem", "Meis\u00b7ter\u00b7bild", "der", "Tu\u00b7gend", "nach\u00b7zu\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Elysische Gef\u00fchle dr\u00e4ngen", "tokens": ["E\u00b7ly\u00b7si\u00b7sche", "Ge\u00b7f\u00fch\u00b7le", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herzens Saiten zu Ges\u00e4ngen,", "tokens": ["Des", "Her\u00b7zens", "Sai\u00b7ten", "zu", "Ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein teurer Name weckte sie. \u2013", "tokens": ["Ein", "teu\u00b7rer", "Na\u00b7me", "weck\u00b7te", "sie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "