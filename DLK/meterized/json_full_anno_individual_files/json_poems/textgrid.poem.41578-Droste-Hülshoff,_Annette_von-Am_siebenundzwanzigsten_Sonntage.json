+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie \u00f6fters brach aus l\u00e4ngst entweihtem Mund", "tokens": ["Wie", "\u00f6f\u00b7ters", "brach", "aus", "l\u00e4ngst", "ent\u00b7weih\u00b7tem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schmerzenslaut, der alles kann vers\u00f6hnen!", "tokens": ["Ein", "Schmer\u00b7zens\u00b7laut", ",", "der", "al\u00b7les", "kann", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O, nur wer stand in gl\u00fcher W\u00fcstenei,", "tokens": ["O", ",", "nur", "wer", "stand", "in", "gl\u00fc\u00b7her", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PWS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der wei\u00df des gr\u00fcnen Blattes Wert zu sch\u00e4tzen,", "tokens": ["Der", "wei\u00df", "des", "gr\u00fc\u00b7nen", "Blat\u00b7tes", "Wert", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wessen Ohr kein Luftzug durfte letzen,", "tokens": ["Und", "wes\u00b7sen", "Ohr", "kein", "Luft\u00b7zug", "durf\u00b7te", "let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nu