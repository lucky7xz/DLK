, "und", "at\u00b7met", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und kann es dennoch einen Stamm enthalten,", "tokens": ["Und", "kann", "es", "den\u00b7noch", "ei\u00b7nen", "Stamm", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der herrlich einst die Zweige mag entfalten,", "tokens": ["Der", "herr\u00b7lich", "einst", "die", "Zwei\u00b7ge", "mag", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo das Gev\u00f6gel jubelt unterm Licht.", "tokens": ["Wo", "das", "Ge\u00b7v\u00f6\u00b7gel", "ju\u00b7belt", "un\u00b7term", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Sei Menschenurteil in Unwissenheit", "tokens": ["Sei", "Men\u00b7schen\u00b7ur\u00b7teil", "in", "Un\u00b7wis\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hart wie ein Stein, du Herr, erkennst das Winden", "tokens": ["Hart", "wie", "ein", "Stein", ",", "du", "Herr", ",", "er\u00b7kennst", "das", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "PPER", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Seele, und wie unter M\u00f6rdern schreit", "tokens": ["Der", "See\u00b7le", ",", "und", "wie", "un\u00b7ter", "M\u00f6r\u00b7dern", "schreit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "KOKOM", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu dir ein Seufzer, der sich selbst nicht finden", "tokens": ["Zu", "dir", "ein", "Seuf\u00b7zer", ",", "der", "sich", "selbst", "nicht", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und nennen kann. Kein Feuer brennt so hei\u00df", "tokens": ["Und", "nen\u00b7nen", "kann", ".", "Kein", "Feu\u00b7er", "brennt", "so", "hei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "$.", "PIAT", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als was sich w\u00fchlen mu\u00df durch Grund und Steine,", "tokens": ["Als", "was", "sich", "w\u00fch\u00b7len", "mu\u00df", "durch", "Grund", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "PRF", "VVINF", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von allen Quellen rauschender rinnt keine", "tokens": ["Von", "al\u00b7len", "Quel\u00b7len", "rau\u00b7schen\u00b7der", "rinnt", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVFIN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"tex