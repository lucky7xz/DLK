{"textgrid.poem.41578": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am siebenundzwanzigsten Sonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief, tief ein K\u00f6rnlein schl\u00e4ft in mancher Brust,", "tokens": ["Tief", ",", "tief", "ein", "K\u00f6rn\u00b7lein", "schl\u00e4ft", "in", "man\u00b7cher", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch Herr, du siehst es und du magst es segnen.", "tokens": ["Doch", "Herr", ",", "du", "siehst", "es", "und", "du", "magst", "es", "seg\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPER", "VVFIN", "PPER", "KON", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O schau auf jene die, sich unbewu\u00dft,", "tokens": ["O", "schau", "auf", "je\u00b7ne", "die", ",", "sich", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PDAT", "ART", "$,", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht f\u00fchlen deiner Gnadenwolke Regnen,", "tokens": ["Nicht", "f\u00fch\u00b7len", "dei\u00b7ner", "Gna\u00b7den\u00b7wol\u00b7ke", "Reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die um sich steigen lassen deinen Tau;", "tokens": ["Die", "um", "sich", "stei\u00b7gen", "las\u00b7sen", "dei\u00b7nen", "Tau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nachtwandler, dumpf gebannt in Traumes Leben,", "tokens": ["Nacht\u00b7wand\u00b7ler", ",", "dumpf", "ge\u00b7bannt", "in", "Trau\u00b7mes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Umwandeln Turmes Zinne sonder Beben,", "tokens": ["Um\u00b7wan\u00b7deln", "Tur\u00b7mes", "Zin\u00b7ne", "son\u00b7der", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht zuckend nur mit der geschlo\u00dfnen Brau'.", "tokens": ["Nicht", "zu\u00b7ckend", "nur", "mit", "der", "ge\u00b7schlo\u00df\u00b7nen", "Brau'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich bin erwacht, ob auch zu tiefer Schmach;", "tokens": ["Ich", "bin", "er\u00b7wacht", ",", "ob", "auch", "zu", "tie\u00b7fer", "Schmach", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So will ich heut nicht an mein Elend denken,", "tokens": ["So", "will", "ich", "heut", "nicht", "an", "mein", "E\u00b7lend", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will, ach, das einzige, was ich vermag,", "tokens": ["Will", ",", "ach", ",", "das", "ein\u00b7zi\u00b7ge", ",", "was", "ich", "ver\u00b7mag", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ITJ", "$,", "ART", "ADJA", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein zitterndes Gebet den Armen schenken;", "tokens": ["Ein", "zit\u00b7tern\u00b7des", "Ge\u00b7bet", "den", "Ar\u00b7men", "schen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob nur ein kraftlos halbgebrochner Hauch,", "tokens": ["Ob", "nur", "ein", "kraft\u00b7los", "halb\u00b7ge\u00b7broch\u00b7ner", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der dennoch mag die rechten Wege finden,", "tokens": ["Der", "den\u00b7noch", "mag", "die", "rech\u00b7ten", "We\u00b7ge", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und mu\u00df er sich zu deinem Throne winden", "tokens": ["Und", "mu\u00df", "er", "sich", "zu", "dei\u00b7nem", "Thro\u00b7ne", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie sich zum \u00c4ther w\u00e4lzet Nebelrauch.", "tokens": ["Wie", "sich", "zum", "\u00c4\u00b7ther", "w\u00e4l\u00b7zet", "Ne\u00b7bel\u00b7rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du Milder, wei\u00dft aus allem Erdendunst", "tokens": ["Du", "Mil\u00b7der", ",", "wei\u00dft", "aus", "al\u00b7lem", "Er\u00b7den\u00b7dunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "VVFIN", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den warmen Lebensodem wohl zu scheiden,", "tokens": ["Den", "war\u00b7men", "Le\u00b7ben\u00b7so\u00b7dem", "wohl", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gerechter du und doch die h\u00f6chste Gunst,", "tokens": ["Ge\u00b7rech\u00b7ter", "du", "und", "doch", "die", "h\u00f6chs\u00b7te", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Sonne scheinet \u00fcber Moor und Heiden,", "tokens": ["Des", "Son\u00b7ne", "schei\u00b7net", "\u00fc\u00b7ber", "Moor", "und", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O kr\u00e4ft'ge deinen Strahl, da\u00df er entgl\u00fcht", "tokens": ["O", "kr\u00e4ft'\u00b7ge", "dei\u00b7nen", "Strahl", ",", "da\u00df", "er", "ent\u00b7gl\u00fcht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die langverj\u00e4hrte Rinde mag durchdringen;", "tokens": ["Die", "lang\u00b7ver\u00b7j\u00e4hr\u00b7te", "Rin\u00b7de", "mag", "durch\u00b7drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mach des erstarrten Blutes Quellen springen,", "tokens": ["Mach", "des", "er\u00b7starr\u00b7ten", "Blu\u00b7tes", "Quel\u00b7len", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auftauen das erfrorne Augenlid.", "tokens": ["Auf\u00b7tau\u00b7en", "das", "er\u00b7fror\u00b7ne", "Au\u00b7gen\u00b7lid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie oft sah ich in schier vereistem Grund", "tokens": ["Wie", "oft", "sah", "ich", "in", "schier", "ver\u00b7eis\u00b7tem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sich leise noch das Samenk\u00f6rnlein dehnen,", "tokens": ["Sich", "lei\u00b7se", "noch", "das", "Sa\u00b7men\u00b7k\u00f6rn\u00b7lein", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie \u00f6fters brach aus l\u00e4ngst entweihtem Mund", "tokens": ["Wie", "\u00f6f\u00b7ters", "brach", "aus", "l\u00e4ngst", "ent\u00b7weih\u00b7tem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schmerzenslaut, der alles kann vers\u00f6hnen!", "tokens": ["Ein", "Schmer\u00b7zens\u00b7laut", ",", "der", "al\u00b7les", "kann", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O, nur wer stand in gl\u00fcher W\u00fcstenei,", "tokens": ["O", ",", "nur", "wer", "stand", "in", "gl\u00fc\u00b7her", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PWS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der wei\u00df des gr\u00fcnen Blattes Wert zu sch\u00e4tzen,", "tokens": ["Der", "wei\u00df", "des", "gr\u00fc\u00b7nen", "Blat\u00b7tes", "Wert", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wessen Ohr kein Luftzug durfte letzen,", "tokens": ["Und", "wes\u00b7sen", "Ohr", "kein", "Luft\u00b7zug", "durf\u00b7te", "let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur der vernimmt den halberstickten Schrei.", "tokens": ["Nur", "der", "ver\u00b7nimmt", "den", "hal\u00b7ber\u00b7stick\u00b7ten", "Schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mit meinem Schaden hab' ich es gelernt,", "tokens": ["Mit", "mei\u00b7nem", "Scha\u00b7den", "hab'", "ich", "es", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df nur der Himmel darf die S\u00fcnde w\u00e4gen,", "tokens": ["Da\u00df", "nur", "der", "Him\u00b7mel", "darf", "die", "S\u00fcn\u00b7de", "w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Menschenhand, sie halte sich entfernt,", "tokens": ["O", "Men\u00b7schen\u00b7hand", ",", "sie", "hal\u00b7te", "sich", "ent\u00b7fernt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die nur das Leben z\u00e4hlt nach Pulses Schl\u00e4gen.", "tokens": ["Die", "nur", "das", "Le\u00b7ben", "z\u00e4hlt", "nach", "Pul\u00b7ses", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Lebt doch das Samenkorn und atmet nicht,", "tokens": ["Lebt", "doch", "das", "Sa\u00b7men\u00b7korn", "und", "at\u00b7met", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und kann es dennoch einen Stamm enthalten,", "tokens": ["Und", "kann", "es", "den\u00b7noch", "ei\u00b7nen", "Stamm", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der herrlich einst die Zweige mag entfalten,", "tokens": ["Der", "herr\u00b7lich", "einst", "die", "Zwei\u00b7ge", "mag", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo das Gev\u00f6gel jubelt unterm Licht.", "tokens": ["Wo", "das", "Ge\u00b7v\u00f6\u00b7gel", "ju\u00b7belt", "un\u00b7term", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sei Menschenurteil in Unwissenheit", "tokens": ["Sei", "Men\u00b7schen\u00b7ur\u00b7teil", "in", "Un\u00b7wis\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hart wie ein Stein, du Herr, erkennst das Winden", "tokens": ["Hart", "wie", "ein", "Stein", ",", "du", "Herr", ",", "er\u00b7kennst", "das", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "PPER", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Seele, und wie unter M\u00f6rdern schreit", "tokens": ["Der", "See\u00b7le", ",", "und", "wie", "un\u00b7ter", "M\u00f6r\u00b7dern", "schreit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "KOKOM", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu dir ein Seufzer, der sich selbst nicht finden", "tokens": ["Zu", "dir", "ein", "Seuf\u00b7zer", ",", "der", "sich", "selbst", "nicht", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und nennen kann. Kein Feuer brennt so hei\u00df", "tokens": ["Und", "nen\u00b7nen", "kann", ".", "Kein", "Feu\u00b7er", "brennt", "so", "hei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "$.", "PIAT", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als was sich w\u00fchlen mu\u00df durch Grund und Steine,", "tokens": ["Als", "was", "sich", "w\u00fch\u00b7len", "mu\u00df", "durch", "Grund", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "PRF", "VVINF", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von allen Quellen rauschender rinnt keine", "tokens": ["Von", "al\u00b7len", "Quel\u00b7len", "rau\u00b7schen\u00b7der", "rinnt", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVFIN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als die sich h\u00fclflos windet unterm Eis.