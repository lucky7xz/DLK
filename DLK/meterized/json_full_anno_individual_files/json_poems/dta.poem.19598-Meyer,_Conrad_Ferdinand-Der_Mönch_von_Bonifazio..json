{"dta.poem.19598": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der M\u00f6nch von Bonifazio.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ecorsen, l\u00f6st des Portes Ketten! Jede Hoffnung ist verschwunden!", "tokens": ["\u201e", "cor\u00b7sen", ",", "l\u00f6st", "des", "Por\u00b7tes", "Ket\u00b7ten", "!", "Je\u00b7de", "Hoff\u00b7nung", "ist", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$.", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nirgend weht ein rettend Segel! Gebt euch! Pfleget eure Wunden!", "tokens": ["Nir\u00b7gend", "weht", "ein", "ret\u00b7tend", "Se\u00b7gel", "!", "Gebt", "euch", "!", "Pfle\u00b7get", "eu\u00b7re", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$.", "VVIMP", "PPER", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Genua, euer hat's vergessen! Sp\u00e4het aus von eurem Riffe!", "tokens": ["Ge\u00b7nua", ",", "eu\u00b7er", "hat's", "ver\u00b7ges\u00b7sen", "!", "Sp\u00e4\u00b7het", "aus", "von", "eu\u00b7rem", "Rif\u00b7fe", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "VAFIN", "VVPP", "$.", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-++-+-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Sucht im Meere! Sch\u00e4rft die Augen! Nirgend, nirgend", "tokens": ["Sucht", "im", "Mee\u00b7re", "!", "Sch\u00e4rft", "die", "Au\u00b7gen", "!", "Nir\u00b7gend", ",", "nir\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "APPRART", "NN", "$.", "VVFIN", "ART", "NN", "$.", "VVPP", "$,", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Eure Kinder h\u00f6r' ich wimmern, eure Fraun, die hungermatten,", "tokens": ["Eu\u00b7re", "Kin\u00b7der", "h\u00f6r'", "ich", "wim\u00b7mern", ",", "eu\u00b7re", "Fraun", ",", "die", "hun\u00b7ger\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Blicken hohl wie Nachtgespenster und ihr selber wankt wie Schatten!\u201c", "tokens": ["Bli\u00b7cken", "hohl", "wie", "Nacht\u00b7ge\u00b7spens\u00b7ter", "und", "ihr", "sel\u00b7ber", "wankt", "wie", "Schat\u00b7ten", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "KOKOM", "NN", "KON", "PPER", "ADV", "VVFIN", "KOKOM", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Vom Verdeck des Schiffes ruft's empor zu Bonifazio's Walle", "tokens": ["Vom", "Ver\u00b7deck", "des", "Schif\u00b7fes", "ruft's", "em\u00b7por", "zu", "Bo\u00b7ni\u00b7fa\u00b7zio's", "Wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "APPR", "NE", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "K\u00f6nig Alfons milden Sinnes, aber droben schweigen Alle.", "tokens": ["K\u00f6\u00b7nig", "Al\u00b7fons", "mil\u00b7den", "Sin\u00b7nes", ",", "a\u00b7ber", "dro\u00b7ben", "schwei\u00b7gen", "Al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "ADV", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Nimmer w\u00fcrden sich dem Dr\u00e4nger diese tapfern Corsen geben,", "tokens": ["Nim\u00b7mer", "w\u00fcr\u00b7den", "sich", "dem", "Dr\u00e4n\u00b7ger", "die\u00b7se", "tap\u00b7fern", "Cor\u00b7sen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN", "PDAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "G\u00f6lt' es nur das eigne, g\u00f6lt' es nicht der Knaben junges Leben!", "tokens": ["G\u00f6lt'", "es", "nur", "das", "eig\u00b7ne", ",", "g\u00f6lt'", "es", "nicht", "der", "Kna\u00b7ben", "jun\u00b7ges", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Finster vor sich niederstarrend, treten fl\u00fcsternd sie zusammen \u2014", "tokens": ["Fins\u00b7ter", "vor", "sich", "nie\u00b7ders\u00b7tar\u00b7rend", ",", "tre\u00b7ten", "fl\u00fcs\u00b7ternd", "sie", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVPP", "$,", "VVFIN", "ADJD", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eines M\u00f6nchs emp\u00f6rte Augen schie\u00dfen Blitze, schleudern Flammen:", "tokens": ["Ei\u00b7nes", "M\u00f6nchs", "em\u00b7p\u00f6r\u00b7te", "Au\u00b7gen", "schie\u00b7\u00dfen", "Blit\u00b7ze", ",", "schleu\u00b7dern", "Flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "\u201efeige Hunde! Keine Corsen! In die H\u00f6lle der Verr\u00e4ther!\u201c", "tokens": ["\u201e", "fei\u00b7ge", "Hun\u00b7de", "!", "Kei\u00b7ne", "Cor\u00b7sen", "!", "In", "die", "H\u00f6l\u00b7le", "der", "Ver\u00b7r\u00e4\u00b7ther", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "PIAT", "NN", "$.", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u201eschweige M\u00f6nch! Wir haben Herzen. Wir sind Gatten, wir", "tokens": ["\u201e", "schwei\u00b7ge", "M\u00f6nch", "!", "Wir", "ha\u00b7ben", "Her\u00b7zen", ".", "Wir", "sind", "Gat\u00b7ten", ",", "wir"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADJA", "NN", "$.", "PPER", "VAFIN", "NN", "$.", "PPER", "VAFIN", "NN", "$,", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.8": {"line.1": {"text": "Auf dem preisgegebnen Felsen kniet der M\u00f6nch in wildem Harme:", "tokens": ["Auf", "dem", "preis\u00b7ge\u00b7geb\u00b7nen", "Fel\u00b7sen", "kniet", "der", "M\u00f6nch", "in", "wil\u00b7dem", "Har\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u201eleihe, Gott, mir Deine H\u00e4nde! Gieb mir Deine starken Arme!", "tokens": ["\u201e", "lei\u00b7he", ",", "Gott", ",", "mir", "Dei\u00b7ne", "H\u00e4n\u00b7de", "!", "Gieb", "mir", "Dei\u00b7ne", "star\u00b7ken", "Ar\u00b7me", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "PPER", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Heute komm' ich Lohn zu fordern. Alles gab ich. Nichts geblieben", "tokens": ["Heu\u00b7te", "komm'", "ich", "Lohn", "zu", "for\u00b7dern", ".", "Al\u00b7les", "gab", "ich", ".", "Nichts", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$.", "PIS", "VVFIN", "PPER", "$.", "PIS", "VVPP"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ist mir au\u00dfer meinem Felsen. Aber etwas mu\u00df ich lieben.", "tokens": ["Ist", "mir", "au\u00b7\u00dfer", "mei\u00b7nem", "Fel\u00b7sen", ".", "A\u00b7ber", "et\u00b7was", "mu\u00df", "ich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "KON", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Gott, Du kannst mit Deinen Kr\u00e4ften eines Menschen Kr\u00e4fte", "tokens": ["Gott", ",", "Du", "kannst", "mit", "Dei\u00b7nen", "Kr\u00e4f\u00b7ten", "ei\u00b7nes", "Men\u00b7schen", "Kr\u00e4f\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Was Du thatst f\u00fcr Deine Juden, darfst Du keinem Corsen weigern!", "tokens": ["Was", "Du", "thatst", "f\u00fcr", "Dei\u00b7ne", "Ju\u00b7den", ",", "darfst", "Du", "kei\u00b7nem", "Cor\u00b7sen", "wei\u00b7gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Genua's Schiffe will ich suchen! Will sie bei den Schn\u00e4beln fassen!", "tokens": ["Ge\u00b7nu\u00b7a's", "Schif\u00b7fe", "will", "ich", "su\u00b7chen", "!", "Will", "sie", "bei", "den", "Schn\u00e4\u00b7beln", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Spannen will ich weite Segel und sie nicht ermatten lassen!\u201c", "tokens": ["Span\u00b7nen", "will", "ich", "wei\u00b7te", "Se\u00b7gel", "und", "sie", "nicht", "er\u00b7mat\u00b7ten", "las\u00b7sen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "KON", "PPER", "PTKNEG", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.12": {"line.1": {"text": "Alle seine Muskeln schwellen, alle seine Pulse beben,", "tokens": ["Al\u00b7le", "sei\u00b7ne", "Mus\u00b7keln", "schwel\u00b7len", ",", "al\u00b7le", "sei\u00b7ne", "Pul\u00b7se", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF", "$,", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schiffe durch das Meer zu schleppen, Segel aus der Flut zu heben.", "tokens": ["Schif\u00b7fe", "durch", "das", "Meer", "zu", "schlep\u00b7pen", ",", "Se\u00b7gel", "aus", "der", "Flut", "zu", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.13": {"line.1": {"text": "Aufgesprungen, \u00fcberwindend Raum und Zeit mit seinem Gotte", "tokens": ["Auf\u00b7ge\u00b7sprun\u00b7gen", ",", "\u00fc\u00b7berw\u00b7in\u00b7dend", "Raum", "und", "Zeit", "mit", "sei\u00b7nem", "Got\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVPP", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Deutet er ins Meer gewaltig: \u201eDort! ich sehe dort die Flotte!\u201c", "tokens": ["Deu\u00b7tet", "er", "ins", "Meer", "ge\u00b7wal\u00b7tig", ":", "\u201e", "Dort", "!", "ich", "se\u00b7he", "dort", "die", "Flot\u00b7te", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "$.", "$(", "ADV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.14": {"line.1": {"text": "Aber keine Segel blinken aus des Meeres farb'ger Weite,", "tokens": ["A\u00b7ber", "kei\u00b7ne", "Se\u00b7gel", "blin\u00b7ken", "aus", "des", "Mee\u00b7res", "fa\u00b7rb'\u00b7ger", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Unbev\u00f6lkert flutet eine schrankenlose Wasserbreite.", "tokens": ["Un\u00b7be\u00b7v\u00f6l\u00b7kert", "flu\u00b7tet", "ei\u00b7ne", "schran\u00b7ken\u00b7lo\u00b7se", "Was\u00b7ser\u00b7brei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.15": {"line.1": {"text": "Nur die Sonne wandert h\u00f6her, ihre Strahlen brennen w\u00e4rmer.", "tokens": ["Nur", "die", "Son\u00b7ne", "wan\u00b7dert", "h\u00f6\u00b7her", ",", "ih\u00b7re", "Strah\u00b7len", "bren\u00b7nen", "w\u00e4r\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nichts als Meer und nichts als Himmel. Alfons l\u00e4chelt: \u201eArmer", "tokens": ["Nichts", "als", "Meer", "und", "nichts", "als", "Him\u00b7mel", ".", "Al\u00b7fons", "l\u00e4\u00b7chelt", ":", "\u201e", "Ar\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["PIS", "KOKOM", "NN", "KON", "PIS", "KOKOM", "NN", "$.", "NE", "VVFIN", "$.", "$(", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.16": {"line.1": {"text": "Dort! Am Saum des Meers das P\u00fcnktchen ... Sichtbar kaum ...", "tokens": ["Dort", "!", "Am", "Saum", "des", "Meers", "das", "P\u00fcnkt\u00b7chen", "...", "Sicht\u00b7bar", "kaum", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "APPRART", "NN", "ART", "NN", "ART", "NN", "$(", "NE", "ADV", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Punkt und jetzt ein viert' und f\u00fcnfter und ein sechster in der Mitte!", "tokens": ["Punkt", "und", "jetzt", "ein", "viert'", "und", "f\u00fcnf\u00b7ter", "und", "ein", "sechs\u00b7ter", "in", "der", "Mit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ART", "PIAT", "KON", "ADJA", "KON", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.17": {"line.1": {"text": "Winde blasen, Wellen sto\u00dfen. Meer und Himmel sind im Bunde.", "tokens": ["Win\u00b7de", "bla\u00b7sen", ",", "Wel\u00b7len", "sto\u00b7\u00dfen", ".", "Meer", "und", "Him\u00b7mel", "sind", "im", "Bun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$.", "NN", "KON", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Segel, immer neue Segel steigen aus dem blauen Grunde.", "tokens": ["Se\u00b7gel", ",", "im\u00b7mer", "neu\u00b7e", "Se\u00b7gel", "stei\u00b7gen", "aus", "dem", "blau\u00b7en", "Grun\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanz