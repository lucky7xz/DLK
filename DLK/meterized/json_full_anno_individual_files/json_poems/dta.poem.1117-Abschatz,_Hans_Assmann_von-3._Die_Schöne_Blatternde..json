{"dta.poem.1117": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "3. Die Sch\u00f6ne Blatternde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Perlen/ die ihr seyd vom Eiter-Thau empfangen/ ", "tokens": ["Ihr", "Per\u00b7len", "/", "die", "ihr", "seyd", "vom", "Ei\u00b7ter\u00b7\u00b7T\u00b7hau", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von innerlicher Hitz\u2019 erh\u00f6ht und ausgekocht!", "tokens": ["Von", "in\u00b7ner\u00b7li\u00b7cher", "Hitz'", "er\u00b7h\u00f6ht", "und", "aus\u00b7ge\u00b7kocht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr feuchten Sternen/ wer von Milch die Strasse sucht/", "tokens": ["Ihr", "feuch\u00b7ten", "Ster\u00b7nen", "/", "wer", "von", "Milch", "die", "Stras\u00b7se", "sucht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PWS", "APPR", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sonst am Himmel gl\u00e4nzt/ find sie auff diesen Wangen.", "tokens": ["Die", "sonst", "am", "Him\u00b7mel", "gl\u00e4nzt", "/", "find", "sie", "auff", "die\u00b7sen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$(", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Cupido hat allhier ein St\u00fcckwerck angefangen/", "tokens": ["Cu\u00b7pi\u00b7do", "hat", "all\u00b7hier", "ein", "St\u00fcck\u00b7werck", "an\u00b7ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das zarte Fell bedeckt/ das Ros\u2019 und Purpur pocht/", "tokens": ["Das", "zar\u00b7te", "Fell", "be\u00b7deckt", "/", "das", "Ros'", "und", "Pur\u00b7pur", "pocht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie wenn der Wolcken Schleyr zu Trost erdurster Frucht", "tokens": ["Wie", "wenn", "der", "Wol\u00b7cken", "Schleyr", "zu", "Trost", "er\u00b7durs\u00b7ter", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", "NN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Im heissen Sommer wird der Sonnen vorgehangen.", "tokens": ["Im", "heis\u00b7sen", "Som\u00b7mer", "wird", "der", "Son\u00b7nen", "vor\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr Buhler seyd getrost/ und legt den Kummer hin/", "tokens": ["Ihr", "Buh\u00b7ler", "seyd", "ge\u00b7trost", "/", "und", "legt", "den", "Kum\u00b7mer", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df ihrer Liljen Pracht die F\u00e4ulni\u00df wird verletzen:", "tokens": ["Da\u00df", "ih\u00b7rer", "Lil\u00b7jen", "Pracht", "die", "F\u00e4ul\u00b7ni\u00df", "wird", "ver\u00b7let\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie werden freudiger auff diesen Regen bl\u00fchn.", "tokens": ["Sie", "wer\u00b7den", "freu\u00b7di\u00b7ger", "auff", "die\u00b7sen", "Re\u00b7gen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Pflegt die gescheide Welt der Steine Schmuck zu sch\u00e4tzen/", "tokens": ["Pflegt", "die", "ge\u00b7schei\u00b7de", "Welt", "der", "Stei\u00b7ne", "Schmuck", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das zarte Muschel-Kind aus tieffer See zu ziehn/", "tokens": ["Das", "zar\u00b7te", "Mu\u00b7schel\u00b7Kind", "aus", "tief\u00b7fer", "See", "zu", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hier zeuget die Natur Opal/ Perl\u2019 und Rubin.", "tokens": ["Hier", "zeu\u00b7get", "die", "Na\u00b7tur", "O\u00b7pal", "/", "Perl'", "und", "Ru\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$(", "NN", "KON", "NE", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}