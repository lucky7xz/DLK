{"dta.poem.2881": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kanst, wenn etwa dir ein Gro\u00dfes ist gelungen,", "tokens": ["Du", "kanst", ",", "wenn", "et\u00b7wa", "dir", "ein", "Gro\u00b7\u00dfes", "ist", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ADV", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die angestrengte Kraft ein Hohes hat erschwungen,", "tokens": ["Die", "an\u00b7ge\u00b7streng\u00b7te", "Kraft", "ein", "Ho\u00b7hes", "hat", "er\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dir nicht deswegen nun nachgeben auszuruhn,", "tokens": ["Dir", "nicht", "des\u00b7we\u00b7gen", "nun", "nach\u00b7ge\u00b7ben", "aus\u00b7zu\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PAV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir nachsehn gar daf\u00fcr was Schlechteres zu thun.", "tokens": ["Dir", "nach\u00b7sehn", "gar", "da\u00b7f\u00fcr", "was", "Schlech\u00b7te\u00b7res", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "PWS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dir auf legt jede Pflichterf\u00fcllung neues Joch,", "tokens": ["Dir", "auf", "legt", "je\u00b7de", "Pflich\u00b7ter\u00b7f\u00fcl\u00b7lung", "neu\u00b7es", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "VVFIN", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu leisten immer das, und immer mehr nur noch.", "tokens": ["Zu", "leis\u00b7ten", "im\u00b7mer", "das", ",", "und", "im\u00b7mer", "mehr", "nur", "noch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "PDS", "$,", "KON", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht eigenm\u00e4chtig kanst du dir den Freibrief schreiben,", "tokens": ["Nicht", "ei\u00b7gen\u00b7m\u00e4ch\u00b7tig", "kanst", "du", "dir", "den", "Frei\u00b7brief", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Gottes Knecht, du mu\u00dft in deiner Knechtschaft bleiben.", "tokens": ["O", "Got\u00b7tes", "Knecht", ",", "du", "mu\u00dft", "in", "dei\u00b7ner", "Knecht\u00b7schaft", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}