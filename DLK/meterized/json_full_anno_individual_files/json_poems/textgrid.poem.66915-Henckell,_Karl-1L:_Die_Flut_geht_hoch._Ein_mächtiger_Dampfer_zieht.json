{"textgrid.poem.66915": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Flut geht hoch. Ein m\u00e4chtiger Dampfer zieht", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flut geht hoch. Ein m\u00e4chtiger Dampfer zieht", "tokens": ["Die", "Flut", "geht", "hoch", ".", "Ein", "m\u00e4ch\u00b7ti\u00b7ger", "Damp\u00b7fer", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur Welt der Zukunft, die mein Auge sieht.", "tokens": ["Zur", "Welt", "der", "Zu\u00b7kunft", ",", "die", "mein", "Au\u00b7ge", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Flut geht hoch ...", "tokens": ["Die", "Flut", "geht", "hoch", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Einst ward ein schwaches Boot", "tokens": ["Einst", "ward", "ein", "schwa\u00b7ches", "Boot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gebaut von wenigen im Land der Not.", "tokens": ["Ge\u00b7baut", "von", "we\u00b7ni\u00b7gen", "im", "Land", "der", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Land der Not lag \u00f6de, sonnenleer,", "tokens": ["Das", "Land", "der", "Not", "lag", "\u00f6\u00b7de", ",", "son\u00b7nen\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mammon hie\u00df Herrscher dort, sein Fluch hing schwer.", "tokens": ["Mam\u00b7mon", "hie\u00df", "Herr\u00b7scher", "dort", ",", "sein", "Fluch", "hing", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Unwissend eignen Elends, eigner Kraft,", "tokens": ["Un\u00b7wis\u00b7send", "eig\u00b7nen", "E\u00b7lends", ",", "eig\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Verkam das Volk. War ausgepre\u00dft der Saft,", "tokens": ["Ver\u00b7kam", "das", "Volk", ".", "War", "aus\u00b7ge\u00b7pre\u00dft", "der", "Saft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VAFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das arme Blut der Kinder, M\u00e4nner, Fraun,", "tokens": ["Das", "ar\u00b7me", "Blut", "der", "Kin\u00b7der", ",", "M\u00e4n\u00b7ner", ",", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Warf man die Menschen weg. Im D\u00e4mmergraun", "tokens": ["Warf", "man", "die", "Men\u00b7schen", "weg", ".", "Im", "D\u00e4m\u00b7mer\u00b7graun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PIS", "ART", "NN", "PTKVZ", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Gewohnten Wahns, mit scheu geducktem Sinn,", "tokens": ["Ge\u00b7wohn\u00b7ten", "Wahns", ",", "mit", "scheu", "ge\u00b7duck\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Schlich der Heloten stumme Herde hin.", "tokens": ["Schlich", "der", "He\u00b7lo\u00b7ten", "stum\u00b7me", "Her\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.14": {"text": "Fern, weltenfern dem Leben, wo das Licht", "tokens": ["Fern", ",", "wel\u00b7ten\u00b7fern", "dem", "Le\u00b7ben", ",", "wo", "das", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Menschenw\u00fcrde durch die Notnacht bricht,", "tokens": ["Der", "Men\u00b7schen\u00b7w\u00fcr\u00b7de", "durch", "die", "Not\u00b7nacht", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Verbannt von allem, was das Atmen lohnt,", "tokens": ["Ver\u00b7bannt", "von", "al\u00b7lem", ",", "was", "das", "At\u00b7men", "lohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Lohnsklaven haben fremden Herrn gefront,", "tokens": ["Lohns\u00b7kla\u00b7ven", "ha\u00b7ben", "frem\u00b7den", "Herrn", "ge\u00b7front", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und jeder sorgte f\u00fcr sein nacktes Los,", "tokens": ["Und", "je\u00b7der", "sorg\u00b7te", "f\u00fcr", "sein", "nack\u00b7tes", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Verwehtem Flugsand gleich am Meeresscho\u00df.", "tokens": ["Ver\u00b7weh\u00b7tem", "Flug\u00b7sand", "gleich", "am", "Mee\u00b7res\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.21": {"text": "Da, wie das Unrecht berghoch sich gestaut,", "tokens": ["Da", ",", "wie", "das", "Un\u00b7recht", "berg\u00b7hoch", "sich", "ge\u00b7staut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Zuckt durch die Tiefen ein verworrner Laut.", "tokens": ["Zuckt", "durch", "die", "Tie\u00b7fen", "ein", "ver\u00b7worr\u00b7ner", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Wer wei\u00df, von wannen jener ihn vernahm?", "tokens": ["Wer", "wei\u00df", ",", "von", "wan\u00b7nen", "je\u00b7ner", "ihn", "ver\u00b7nahm", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "ADV", "PDAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wer wei\u00df, wie er zuerst zu diesem kam?", "tokens": ["Wer", "wei\u00df", ",", "wie", "er", "zu\u00b7erst", "zu", "die\u00b7sem", "kam", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPER", "ADV", "APPR", "PDAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Zeugt' ihn die Zeit aus frechem \u00dcberma\u00df", "tokens": ["Zeugt'", "ihn", "die", "Zeit", "aus", "fre\u00b7chem", "\u00dc\u00b7berm\u00b7a\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der G\u00fctergier, die den Erzeuger fra\u00df?", "tokens": ["Der", "G\u00fc\u00b7ter\u00b7gier", ",", "die", "den", "Er\u00b7zeu\u00b7ger", "fra\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Erst murrt es dumpf, dann rang ein Drohn sich frei:", "tokens": ["Erst", "murrt", "es", "dumpf", ",", "dann", "rang", "ein", "Drohn", "sich", "frei", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "\u00bbweh, Welt, dein Bau mu\u00df st\u00fcrzen!\u00ab gellt der Schrei.", "tokens": ["\u00bb", "weh", ",", "Welt", ",", "dein", "Bau", "mu\u00df", "st\u00fcr\u00b7zen", "!", "\u00ab", "gellt", "der", "Schrei", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$.", "$(", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.30": {"text": "Doch M\u00e4nner, drin die Menschheit m\u00e4chtig war,", "tokens": ["Doch", "M\u00e4n\u00b7ner", ",", "drin", "die", "Menschheit", "m\u00e4ch\u00b7tig", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.31": {"text": "Sie traten vor und trotzten der Gefahr.", "tokens": ["Sie", "tra\u00b7ten", "vor", "und", "trotz\u00b7ten", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der Forscher, der Geschichte schrieb und schuf,", "tokens": ["Der", "For\u00b7scher", ",", "der", "Ge\u00b7schich\u00b7te", "schrieb", "und", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Warf in die Arbeitswelt den Losungsruf:", "tokens": ["Warf", "in", "die", "Ar\u00b7beits\u00b7welt", "den", "Lo\u00b7sungs\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.34": {"text": "\u00bbvereinigt euch!\u00ab Der jugendfeurige Held", "tokens": ["\u00bb", "ver\u00b7ei\u00b7nigt", "euch", "!", "\u00ab", "Der", "ju\u00b7gend\u00b7feu\u00b7ri\u00b7ge", "Held"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.35": {"text": "St\u00fcrmt mit dem Schwert der Wissenschaft ins Feld.", "tokens": ["St\u00fcrmt", "mit", "dem", "Schwert", "der", "Wis\u00b7sen\u00b7schaft", "ins", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Des Volkes Sohn und F\u00fchrer, k\u00fchn und treu,", "tokens": ["Des", "Vol\u00b7kes", "Sohn", "und", "F\u00fch\u00b7rer", ",", "k\u00fchn", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Trug bis zum Tod die Fahne ...", "tokens": ["Trug", "bis", "zum", "Tod", "die", "Fah\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Da ward neu", "tokens": ["Da", "ward", "neu"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Geboren in dem Geist der Arbeit Sinn,", "tokens": ["Ge\u00b7bo\u00b7ren", "in", "dem", "Geist", "der", "Ar\u00b7beit", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Sie wagt's und w\u00e4chst zur Weltbefreierin.", "tokens": ["Sie", "wagt's", "und", "w\u00e4chst", "zur", "Welt\u00b7be\u00b7frei\u00b7e\u00b7rin", "."], "token_info": ["word", "word", "wor