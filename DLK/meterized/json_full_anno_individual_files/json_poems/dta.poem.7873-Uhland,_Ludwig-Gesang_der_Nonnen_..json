{"dta.poem.7873": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Nonnen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Erhebet euch mit heil\u2019gem Triebe,", "tokens": ["Er\u00b7he\u00b7bet", "euch", "mit", "heil'\u00b7gem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr frommen Schwestern, himmelan,", "tokens": ["Ihr", "from\u00b7men", "Schwes\u00b7tern", ",", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schwebt auf bl\u00fch\u2019nder Wolkenbahn!", "tokens": ["Und", "schwebt", "auf", "bl\u00fch'n\u00b7der", "Wol\u00b7ken\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da leuchtet uns die reinste Sonne,", "tokens": ["Da", "leuch\u00b7tet", "uns", "die", "reins\u00b7te", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da singen wir in Fr\u00fchlingswonne", "tokens": ["Da", "sin\u00b7gen", "wir", "in", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Lied von dir, du ew\u2019ge Liebe!", "tokens": ["Ein", "Lied", "von", "dir", ",", "du", "ew'\u00b7ge", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob welken alle zarte Bl\u00fcthen", "tokens": ["Ob", "wel\u00b7ken", "al\u00b7le", "zar\u00b7te", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem Genu\u00df der ird\u2019schen Glut:", "tokens": ["Von", "dem", "Ge\u00b7nu\u00df", "der", "ird'\u00b7schen", "Glut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein ewig Jugendblut", "tokens": ["Du", "bist", "ein", "e\u00b7wig", "Ju\u00b7gend\u00b7blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsrer Busen st\u00e4te F\u00fclle,", "tokens": ["Und", "uns\u00b7rer", "Bu\u00b7sen", "st\u00e4\u00b7te", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ew\u2019ge Flamme, die wir stille", "tokens": ["Die", "ew'\u00b7ge", "Flam\u00b7me", ",", "die", "wir", "stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am Altar und im Herzen h\u00fcten.", "tokens": ["Am", "Al\u00b7tar", "und", "im", "Her\u00b7zen", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}