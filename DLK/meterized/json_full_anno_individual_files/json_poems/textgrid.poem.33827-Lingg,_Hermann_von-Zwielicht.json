{"textgrid.poem.33827": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Zwielicht", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie d\u00fcster, wie in Sterbgewanden,", "tokens": ["Wie", "d\u00fcs\u00b7ter", ",", "wie", "in", "Sterb\u00b7ge\u00b7wan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im D\u00e4mmergrau die Berge stehn!", "tokens": ["Im", "D\u00e4m\u00b7mer\u00b7grau", "die", "Ber\u00b7ge", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So dunkel sind und unverstanden,", "tokens": ["So", "dun\u00b7kel", "sind", "und", "un\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die tiefer als die Andern sehn.", "tokens": ["Die", "tie\u00b7fer", "als", "die", "An\u00b7dern", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Menge f\u00e4hrt nur hocherschrocken", "tokens": ["Die", "Men\u00b7ge", "f\u00e4hrt", "nur", "ho\u00b7cher\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus ihrem Alltagstaumel auf,", "tokens": ["Aus", "ih\u00b7rem", "All\u00b7tags\u00b7tau\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn pl\u00f6tzlich ihr ger\u00e4t ins Stocken", "tokens": ["Wenn", "pl\u00f6tz\u00b7lich", "ihr", "ge\u00b7r\u00e4t", "ins", "Sto\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Dinge hergebrachter Lauf.", "tokens": ["Der", "Din\u00b7ge", "her\u00b7ge\u00b7brach\u00b7ter", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dann ist der Schleier weggerissen,", "tokens": ["Dann", "ist", "der", "Schlei\u00b7er", "weg\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der ihr die Tiefen barg, erwacht", "tokens": ["Der", "ihr", "die", "Tie\u00b7fen", "barg", ",", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein unbekanntes Weltgewissen,", "tokens": ["Ein", "un\u00b7be\u00b7kann\u00b7tes", "Welt\u00b7ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das sie vor sich erbeben macht.", "tokens": ["Das", "sie", "vor", "sich", "er\u00b7be\u00b7ben", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambi