{"textgrid.poem.60046": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn, vom gestirnten Himmel weit,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn, vom gestirnten Himmel weit,", "tokens": ["Wenn", ",", "vom", "ge\u00b7stirn\u00b7ten", "Him\u00b7mel", "weit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich ungef\u00e4hr zur Abendzeit,", "tokens": ["Sich", "un\u00b7ge\u00b7f\u00e4hr", "zur", "A\u00b7bend\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In grober Luft ein kleiner Dunst entz\u00fcndet,", "tokens": ["In", "gro\u00b7ber", "Luft", "ein", "klei\u00b7ner", "Dunst", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alsobald verschwindet,", "tokens": ["Und", "al\u00b7so\u00b7bald", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann sieht der Astronom auf seiner Warte kaum", "tokens": ["Dann", "sieht", "der", "Ast\u00b7ro\u00b7nom", "auf", "sei\u00b7ner", "War\u00b7te", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der D\u00fcnste Spiel im niedern Raum;", "tokens": ["Der", "D\u00fcns\u00b7te", "Spiel", "im", "nie\u00b7dern", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er blickt in Gegenden von ewig reinem Licht;", "tokens": ["Er", "blickt", "in", "Ge\u00b7gen\u00b7den", "von", "e\u00b7wig", "rei\u00b7nem", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dies Flackerwerk geh\u00f6rt an seinen Himmel nicht;", "tokens": ["Dies", "Fla\u00b7cker\u00b7werk", "ge\u00b7h\u00f6rt", "an", "sei\u00b7nen", "Him\u00b7mel", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein der P\u00f6bel glaubt auf Erden,", "tokens": ["Al\u00b7lein", "der", "P\u00f6\u00b7bel", "glaubt", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es putze sich ein Stern, um gl\u00e4nzender zu werden.", "tokens": ["Es", "put\u00b7ze", "sich", "ein", "Stern", ",", "um", "gl\u00e4n\u00b7zen\u00b7der", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn, vom gestirnten Himmel weit,", "tokens": ["Wenn"