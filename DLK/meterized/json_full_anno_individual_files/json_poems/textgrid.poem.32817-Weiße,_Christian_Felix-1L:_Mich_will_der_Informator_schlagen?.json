{"textgrid.poem.32817": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich will der Informator schlagen?", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich will der Informator schlagen?", "tokens": ["Mich", "will", "der", "In\u00b7for\u00b7ma\u00b7tor", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, nein, das geht nicht weiter an:", "tokens": ["Nein", ",", "nein", ",", "das", "geht", "nicht", "wei\u00b7ter", "an", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PDS", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Knabe must ich es ertragen,", "tokens": ["Als", "Kna\u00b7be", "must", "ich", "es", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch jetzt bin ich schon halb ein Mann.", "tokens": ["Doch", "jetzt", "bin", "ich", "schon", "halb", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist er nicht kl\u00fcger die\u00df zu wissen?", "tokens": ["Ist", "er", "nicht", "kl\u00fc\u00b7ger", "die\u00df", "zu", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00f6r er nur, was Hannchen spricht;", "tokens": ["So", "h\u00f6r", "er", "nur", ",", "was", "Hann\u00b7chen", "spricht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich die kleine N\u00e4rrin k\u00fcssen:", "tokens": ["Will", "ich", "die", "klei\u00b7ne", "N\u00e4r\u00b7rin", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So spricht sie, geh, dein Bart, der sticht.", "tokens": ["So", "spricht", "sie", ",", "geh", ",", "dein", "Bart", ",", "der", "sticht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich will der Informator schlagen?", "tokens": ["Mich", "will", "der", "In\u00b7for\u00b7ma\u00b7tor", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, nein, das geht nicht weiter an:", "tokens": ["Nein", ",", "nein", ",", "das", "geht", "nicht", "wei\u00b7ter", "an", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PDS", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Knabe must ich es ertragen,