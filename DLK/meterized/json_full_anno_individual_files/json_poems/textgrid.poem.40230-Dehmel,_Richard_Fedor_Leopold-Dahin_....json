{"textgrid.poem.40230": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Dahin ...", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber Ru\u00dflands Leichenangesicht", "tokens": ["Ue\u00b7ber", "Ru\u00df\u00b7lands", "Lei\u00b7che\u00b7nan\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "faltet hoch die Nacht die blassen H\u00e4nde;", "tokens": ["fal\u00b7tet", "hoch", "die", "Nacht", "die", "blas\u00b7sen", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "auf das stumme, wei\u00dfe, kalte Antlitz", "tokens": ["auf", "das", "stum\u00b7me", ",", "wei\u00b7\u00dfe", ",", "kal\u00b7te", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "hohlen Auges starrt die Nacht und lauscht.", "tokens": ["hoh\u00b7len", "Au\u00b7ges", "starrt", "die", "Nacht", "und", "lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Gellend hallt ein Gel\u00e4ute.", "tokens": ["Gel\u00b7lend", "hallt", "ein", "Ge\u00b7l\u00e4u\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Dumpf ein Stampfen von Hufen \u2013 ein Schlitten kreischt,", "tokens": ["Dumpf", "ein", "Stamp\u00b7fen", "von", "Hu\u00b7fen", "\u2013", "ein", "Schlit\u00b7ten", "kreischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "fahl flatternder Reif \u2013 der Schlitten fliegt;", "tokens": ["fahl", "flat\u00b7tern\u00b7der", "Reif", "\u2013", "der", "Schlit\u00b7ten", "fliegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "es dampfen die Pferde, die Peitsche pfeift,", "tokens": ["es", "damp\u00b7fen", "die", "Pfer\u00b7de", ",", "die", "Peit\u00b7sche", "pfeift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "stiebende Furchen die Kufe pfl\u00fcgt;", "tokens": ["stie\u00b7ben\u00b7de", "Fur\u00b7chen", "die", "Ku\u00b7fe", "pfl\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "flimmernd zittern die Birken.", "tokens": ["flim\u00b7mernd", "zit\u00b7tern", "die", "Bir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "\u00bbknecht, was h\u00f6rtest du von \u2013 ", "tokens": ["\u00bb", "knecht", ",", "was", "h\u00f6r\u00b7test", "du", "von", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "APPR", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bang aufschreckend der Bauer horcht erschauernd,", "tokens": ["Bang", "auf\u00b7schre\u00b7ckend", "der", "Bau\u00b7er", "horcht", "er\u00b7schau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "wie da hinter ihm das steinern bleiche", "tokens": ["wie", "da", "hin\u00b7ter", "ihm", "das", "stei\u00b7nern", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPER", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Antlitz mit den harten Lippen", "tokens": ["Ant\u00b7litz", "mit", "den", "har\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Laute so voll Trauer spricht.", "tokens": ["Lau\u00b7te", "so", "voll", "Trau\u00b7er", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "R\u00fcckw\u00e4rts horcht der Alte \u2013 hockt und starrt,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "horcht", "der", "Al\u00b7te", "\u2013", "hockt", "und", "starrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "starrt und staunt mit frommer Furchtgeberde:", "tokens": ["starrt", "und", "staunt", "mit", "from\u00b7mer", "Furcht\u00b7ge\u00b7ber\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "dr\u00fcben weit im Osten aus der Erde", "tokens": ["dr\u00fc\u00b7ben", "weit", "im", "Os\u00b7ten", "aus", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "drohend taucht die rotgeballte", "tokens": ["dro\u00b7hend", "taucht", "die", "rot\u00b7ge\u00b7ball\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mondfaust in die \u00f6de Nacht, \u2013", "tokens": ["Mond\u00b7faust", "in", "die", "\u00f6\u00b7de", "Nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "d\u00fcster wie von Blut die lange Stra\u00dfe glimmt,", "tokens": ["d\u00fcs\u00b7ter", "wie", "von", "Blut", "die", "lan\u00b7ge", "Stra\u00b7\u00dfe", "glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "wie von Blut die blanken Birken perlen,", "tokens": ["wie", "von", "Blut", "die", "blan\u00b7ken", "Bir\u00b7ken", "per\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wie von Blut umtropft das Haupt im Schlitten thront, \u2013", "tokens": ["wie", "von", "Blut", "um\u00b7tropft", "das", "Haupt", "im", "Schlit\u00b7ten", "thront", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "\u00bbknecht, was ", "tokens": ["\u00bb", "knecht", ",", "was"], "token_info": ["punct", "word", "punct", "word"], "pos": ["$(", "VVFIN", "$,", "PWS"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Klagend schrillt das Gel\u00e4ute.", "tokens": ["Kla\u00b7gend", "schrillt", "das", "Ge\u00b7l\u00e4u\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Auf zuckt der Bauer, die Peitsche klatscht,", "tokens": ["Auf", "zuckt", "der", "Bau\u00b7er", ",", "die", "Peit\u00b7sche", "klatscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die Glocke schreit, hohl rauscht's im Schnee, \u2013", "tokens": ["die", "Glo\u00b7cke", "schreit", ",", "hohl", "rauscht's", "im", "Schnee", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und schwer nun, feiervoll und sacht,", "tokens": ["und", "schwer", "nun", ",", "fei\u00b7er\u00b7voll", "und", "sacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie uralt Lied so dumpf und weh,", "tokens": ["wie", "ur\u00b7alt", "Lied", "so", "dumpf", "und", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ADV", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "kommt Wort um Wort gezogen:", "tokens": ["kommt", "Wort", "um", "Wort", "ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbgro\u00df am Himmel stand die schwarze Wolke,", "tokens": ["\u00bb", "gro\u00df", "am", "Him\u00b7mel", "stand", "die", "schwar\u00b7ze", "Wol\u00b7ke", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "fressen wollte sie den heil'gen Mond, \u2013", "tokens": ["fres\u00b7sen", "woll\u00b7te", "sie", "den", "heil'\u00b7gen", "Mond", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVINF", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und zerstoben ist die schwarze Wolke.", "tokens": ["und", "zer\u00b7sto\u00b7ben", "ist", "die", "schwar\u00b7ze", "Wol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Volk, was weinst du?", "tokens": ["Volk", ",", "was", "weinst", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Stolz der kalte ", "tokens": ["Stolz", "der", "kal\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "fressen sollte sie die stillen Sterne, \u2013", "tokens": ["fres\u00b7sen", "soll\u00b7te", "sie", "die", "stil\u00b7len", "Ster\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVINF", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Und es war ein gro\u00dfes schwarzes Heer,", "tokens": ["Und", "es", "war", "ein", "gro\u00b7\u00dfes", "schwar\u00b7zes", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und es war ein stolzer kalter Kaiser, \u2013", "tokens": ["und", "es", "war", "ein", "stol\u00b7zer", "kal\u00b7ter", "Kai\u00b7ser", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "aber unser M\u00fctterchen das heil'ge Ru\u00dfland", "tokens": ["a\u00b7ber", "un\u00b7ser", "M\u00fct\u00b7ter\u00b7chen", "das", "heil'\u00b7ge", "Ru\u00df\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "hat viel tausend tausend stille warme Herzen,", "tokens": ["hat", "viel", "tau\u00b7send", "tau\u00b7send", "stil\u00b7le", "war\u00b7me", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word