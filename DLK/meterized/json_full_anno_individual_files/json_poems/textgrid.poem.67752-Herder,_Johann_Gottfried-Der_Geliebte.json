{"textgrid.poem.67752": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Geliebte", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den ich mir zum Freund erw\u00e4hle,", "tokens": ["Den", "ich", "mir", "zum", "Freund", "er\u00b7w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll von m\u00e4nnlich edler Seele", "tokens": ["Soll", "von", "m\u00e4nn\u00b7lich", "ed\u00b7ler", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von offner Stirne sein.", "tokens": ["Und", "von", "off\u00b7ner", "Stir\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiser Anstand, Witz im Scherze", "tokens": ["Wei\u00b7ser", "An\u00b7stand", ",", "Witz", "im", "Scher\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "R\u00fchrt mein Herze,", "tokens": ["R\u00fchrt", "mein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nicht die Schale Punsch allein.", "tokens": ["Nicht", "die", "Scha\u00b7le", "Pun\u00b7sch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "ADV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Stutzer taugen nur zum Spielen,", "tokens": ["Stut\u00b7zer", "tau\u00b7gen", "nur", "zum", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Greise nur, sich abzuk\u00fchlen,", "tokens": ["Grei\u00b7se", "nur", ",", "sich", "ab\u00b7zu\u00b7k\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PRF", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4dchen nur zur Fr\u00fchlingslust.", "tokens": ["M\u00e4d\u00b7chen", "nur", "zur", "Fr\u00fch\u00b7lings\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du, o Freundschaft, sollst vor Allen", "tokens": ["Du", ",", "o", "Freund\u00b7schaft", ",", "sollst", "vor", "Al\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VMFIN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir gefallen;", "tokens": ["Mir", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Du entz\u00fcckest Haupt und Brust.", "tokens": ["Du", "ent\u00b7z\u00fc\u00b7ckest", "Haupt", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich in Dir sanft zerflie\u00dfe", "tokens": ["Wenn", "ich", "in", "Dir", "sanft", "zer\u00b7flie\u00b7\u00dfe"], "token_info": ["word",