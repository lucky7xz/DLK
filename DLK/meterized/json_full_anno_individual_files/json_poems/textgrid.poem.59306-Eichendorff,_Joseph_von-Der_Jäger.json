{"textgrid.poem.59306": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Segeln der W\u00fcnsche durch luftige H\u00f6h!", "tokens": ["Was", "Se\u00b7geln", "der", "W\u00fcn\u00b7sche", "durch", "luf\u00b7ti\u00b7ge", "H\u00f6h", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Was bildendes Tr\u00e4umen im bl\u00fchenden Klee!", "tokens": ["Was", "bil\u00b7den\u00b7des", "Tr\u00e4u\u00b7men", "im", "bl\u00fc\u00b7hen\u00b7den", "Klee", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was Hoffen und Bangen, was Schmachten, was Weh!", "tokens": ["Was", "Hof\u00b7fen", "und", "Ban\u00b7gen", ",", "was", "Schmach\u00b7ten", ",", "was", "Weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "$,", "PWS", "NN", "$,", "PWS", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und rauscht nicht die Erde in Bl\u00fcten und Duft?", "tokens": ["Und", "rauscht", "nicht", "die", "Er\u00b7de", "in", "Bl\u00fc\u00b7ten", "und", "Duft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und schreitet nicht H\u00f6rnerklang k\u00fchn durch die Luft?