{"textgrid.poem.50844": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Modernster Faust", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein ganzer Held! Den Mantel umgeschlagen", "tokens": ["Ich", "bin", "ein", "gan\u00b7zer", "Held", "!", "Den", "Man\u00b7tel", "um\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u2013 Romantisch schwarzer Samt ergl\u00e4nzt an Kleid und Kragen \u2013", "tokens": ["\u2013", "Ro\u00b7man\u00b7tisch", "schwar\u00b7zer", "Samt", "er\u00b7gl\u00e4nzt", "an", "Kleid", "und", "Kra\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "St\u00fcrm ich dahin in eitlem Wahn!", "tokens": ["St\u00fcrm", "ich", "da\u00b7hin", "in", "eit\u00b7lem", "Wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob Samt? ob nur Kattun? es war ein langes Zanken", "tokens": ["Ob", "Samt", "?", "ob", "nur", "Kat\u00b7tun", "?", "es", "war", "ein", "lan\u00b7ges", "Zan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$.", "KOUS", "ADV", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit meinem M\u00fctterlein; doch fest und ohne Wanken", "tokens": ["Mit", "mei\u00b7nem", "M\u00fct\u00b7ter\u00b7lein", ";", "doch", "fest", "und", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erstritt ich Samt, und niemand sieht mir's an.", "tokens": ["Er\u00b7stritt", "ich", "Samt", ",", "und", "nie\u00b7mand", "sieht", "mir's", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "PIS", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Leichtsinnig, hohen Muts mach ich die Morgenrunde;", "tokens": ["Leicht\u00b7sin\u00b7nig", ",", "ho\u00b7hen", "Muts", "mach", "ich", "die", "Mor\u00b7gen\u00b7run\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Wintersonne scheint, Cigarro brennt im Munde,", "tokens": ["Die", "Win\u00b7ter\u00b7son\u00b7ne", "scheint", ",", "Ci\u00b7gar\u00b7ro", "brennt", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den ich dem Kaufmann schuldig bin;", "tokens": ["Den", "ich", "dem", "Kauf\u00b7mann", "schul\u00b7dig", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wintersonne scheint, kalt ist ihr Silberflimmer,", "tokens": ["Die", "Win\u00b7ter\u00b7son\u00b7ne", "scheint", ",", "kalt", "ist", "ihr", "Sil\u00b7ber\u00b7flim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und kalt ist mir das Herz, kalt meiner Augen Schimmer", "tokens": ["Und", "kalt", "ist", "mir", "das", "Herz", ",", "kalt", "mei\u00b7ner", "Au\u00b7gen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "ADJD", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und tr\u00fcb, befangen immerhin.", "tokens": ["Und", "tr\u00fcb", ",", "be\u00b7fan\u00b7gen", "im\u00b7mer\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da treff ich einen Freund auf meiner irren Bahn,", "tokens": ["Da", "treff", "ich", "ei\u00b7nen", "Freund", "auf", "mei\u00b7ner", "ir\u00b7ren", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir halten mit Geklatsch ein halbes St\u00fcndchen an;", "tokens": ["Wir", "hal\u00b7ten", "mit", "Ge\u00b7klatsch", "ein", "hal\u00b7bes", "St\u00fcnd\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wenn zwei alte Hexen schelten,", "tokens": ["Wie", "wenn", "zwei", "al\u00b7te", "He\u00b7xen", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bricht von Bosheit nun und Neid ein ganzer Chor", "tokens": ["So", "bricht", "von", "Bos\u00b7heit", "nun", "und", "Neid", "ein", "gan\u00b7zer", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von Zoten, schlechtem Witz und Ha\u00df aus uns hervor,", "tokens": ["Von", "Zo\u00b7ten", ",", "schlech\u00b7tem", "Witz", "und", "Ha\u00df", "aus", "uns", "her\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df mir versch\u00e4mt die eignen Ohren gellten.", "tokens": ["Da\u00df", "mir", "ver\u00b7sch\u00e4mt", "die", "eig\u00b7nen", "Oh\u00b7ren", "gell\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da kommt ein Handwerksbursch, bleich, mit zerri\u00dfnen Sohlen,", "tokens": ["Da", "kommt", "ein", "Hand\u00b7werks\u00b7bursch", ",", "bleich", ",", "mit", "zer\u00b7ri\u00df\u00b7nen", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "M\u00fctz in der Hand, geduckt, ein G\u00e4blein sich zu holen;", "tokens": ["M\u00fctz", "in", "der", "Hand", ",", "ge\u00b7duckt", ",", "ein", "G\u00e4\u00b7blein", "sich", "zu", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVPP", "$,", "ART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit einem Kreuzer w\u00e4r ihm wohlgetan.", "tokens": ["Mit", "ei\u00b7nem", "Kreu\u00b7zer", "w\u00e4r", "ihm", "wohl\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch weil ich diesen nicht in leerer Tasche trage", "tokens": ["Doch", "weil", "ich", "die\u00b7sen", "nicht", "in", "lee\u00b7rer", "Ta\u00b7sche", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDS", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und doch nicht freundlich ihm es zu gestehen wage,", "tokens": ["Und", "doch", "nicht", "freund\u00b7lich", "ihm", "es", "zu", "ge\u00b7ste\u00b7hen", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fahr ich ihn rauh abweisend an.", "tokens": ["Fahr", "ich", "ihn", "rauh", "ab\u00b7wei\u00b7send", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ob mir das k\u00fchle Herz in rascher Scham ergl\u00fcht,", "tokens": ["Ob", "mir", "das", "k\u00fch\u00b7le", "Herz", "in", "ra\u00b7scher", "Scham", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob auch ein blut'ger Schnitt mir durch die Seele zieht:", "tokens": ["Ob", "auch", "ein", "blut'\u00b7ger", "Schnitt", "mir", "durch", "die", "See\u00b7le", "zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht es nicht in meinen Blicken;", "tokens": ["Man", "sieht", "es", "nicht", "in", "mei\u00b7nen", "Bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich habe ja gelernt, mit h\u00f6hnisch leichtem Spiel", "tokens": ["Ich", "ha\u00b7be", "ja", "ge\u00b7lernt", ",", "mit", "h\u00f6h\u00b7nisch", "leich\u00b7tem", "Spiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den halberfrornen Lenz, das innere Gef\u00fchl,", "tokens": ["Den", "hal\u00b7ber\u00b7fror\u00b7nen", "Lenz", ",", "das", "in\u00b7ne\u00b7re", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn es erbl\u00fchen will, zu unterdr\u00fccken!", "tokens": ["Wenn", "es", "er\u00b7bl\u00fc\u00b7hen", "will", ",", "zu", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O ich war treu, wie Gold, begeistert, klar und offen;", "tokens": ["O", "ich", "war", "treu", ",", "wie", "Gold", ",", "be\u00b7geis\u00b7tert", ",", "klar", "und", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ADJD", "$,", "PWAV", "NN", "$,", "VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Blatt ums andre fiel von meinem gr\u00fcnen Hoffen,", "tokens": ["Ein", "Blatt", "ums", "and\u00b7re", "fiel", "von", "mei\u00b7nem", "gr\u00fc\u00b7nen", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und taube N\u00fcsse tauscht ich ein!", "tokens": ["Und", "tau\u00b7be", "N\u00fcs\u00b7se", "tauscht", "ich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schmach \u00fcber dich, o Welt! du hast mich ganz beladen", "tokens": ["Schmach", "\u00fc\u00b7ber", "dich", ",", "o", "Welt", "!", "du", "hast", "mich", "ganz", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "FM", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit deinem Schlamm und Staub! O k\u00f6nnt ich rein mich baden", "tokens": ["Mit", "dei\u00b7nem", "Schlamm", "und", "Staub", "!", "O", "k\u00f6nnt", "ich", "rein", "mich", "ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$.", "NE", "VVFIN", "PPER", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im wilden Meer, sollt's auch ein Sterben sein!", "tokens": ["Im", "wil\u00b7den", "Meer", ",", "sollt's", "auch", "ein", "Ster\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VMFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Kokett ist dies Gedicht, Naivet\u00e4t erlogen", "tokens": ["Ko\u00b7kett", "ist", "dies", "Ge\u00b7dicht", ",", "Nai\u00b7ve\u00b7t\u00e4t", "er\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PDS", "NN", "$,", "NN", "VVINF"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und nur das Schn\u00f6de wahr! Ich hab euch arg betrogen,", "tokens": ["Und", "nur", "das", "Schn\u00f6\u00b7de", "wahr", "!", "Ich", "hab", "euch", "arg", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn zwei geworden sind mir Herz und Mund!", "tokens": ["Denn", "zwei", "ge\u00b7wor\u00b7den", "sind", "mir", "Herz", "und", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "VAPP", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin ganz euer Bild: selbsts\u00fcchtig, falsch und eitel", "tokens": ["Ich", "bin", "ganz", "eu\u00b7er", "Bild", ":", "selbst\u00b7s\u00fcch\u00b7tig", ",", "falsch", "und", "ei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und unklar in mir selbst; vom Fu\u00dfe bis zur Scheitel", "tokens": ["Und", "un\u00b7klar", "in", "mir", "selbst", ";", "vom", "Fu\u00b7\u00dfe", "bis", "zur", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER", "ADV", "$.", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein europ\u00e4isch schlechter Hund!", "tokens": ["Ein", "eu\u00b7ro\u00b7p\u00e4\u00b7isch", "schlech\u00b7ter", "Hund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich bin ein ganzer Held! Den Mantel umgeschlagen", "tokens": ["Ich", "bin", "ein", "gan\u00b7zer", "Held", "!", "Den", "Man\u00b7tel", "um\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u2013 Romantisch schwarzer Samt ergl\u00e4nzt an Kleid und Kragen \u2013", "tokens": ["\u2013", "Ro\u00b7man\u00b7tisch", "schwar\u00b7zer", "Samt", "er\u00b7gl\u00e4nzt", "an", "Kleid", "und", "Kra\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "St\u00fcrm ich dahin in eitlem Wahn!", "tokens": ["St\u00fcrm", "ich", "da\u00b7hin", "in", "eit\u00b7lem", "Wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob Samt? ob nur Kattun? es war ein langes Zanken", "tokens": ["Ob", "Samt", "?", "ob", "nur", "Kat\u00b7tun", "?", "es", "war", "ein", "lan\u00b7ges", "Zan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$.", "KOUS", "ADV", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit meinem M\u00fctterlein; doch fest und ohne Wanken", "tokens": ["Mit", "mei\u00b7nem", "M\u00fct\u00b7ter\u00b7lein", ";", "doch", "fest", "und", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erstritt ich Samt, und niemand sieht mir's an.", "tokens": ["Er\u00b7stritt", "ich", "Samt", ",", "und", "nie\u00b7mand", "sieht", "mir's", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "PIS", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Leichtsinnig, hohen Muts mach ich die Morgenrunde;", "tokens": ["Leicht\u00b7sin\u00b7nig", ",", "ho\u00b7hen", "Muts", "mach", "ich", "die", "Mor\u00b7gen\u00b7run\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Wintersonne scheint, Cigarro brennt im Munde,", "tokens": ["Die", "Win\u00b7ter\u00b7son\u00b7ne", "scheint", ",", "Ci\u00b7gar\u00b7ro", "brennt", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den ich dem Kaufmann schuldig bin;", "tokens": ["Den", "ich", "dem", "Kauf\u00b7mann", "schul\u00b7dig", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wintersonne scheint, kalt ist ihr Silberflimmer,", "tokens": ["Die", "Win\u00b7ter\u00b7son\u00b7ne", "scheint", ",", "kalt", "ist", "ihr", "Sil\u00b7ber\u00b7flim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und kalt ist mir das Herz, kalt meiner Augen Schimmer", "tokens": ["Und", "kalt", "ist", "mir", "das", "Herz", ",", "kalt", "mei\u00b7ner", "Au\u00b7gen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "ADJD", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und tr\u00fcb, befangen immerhin.", "tokens": ["Und", "tr\u00fcb", ",", "be\u00b7fan\u00b7gen", "im\u00b7mer\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da treff ich einen Freund auf meiner irren Bahn,", "tokens": ["Da", "treff", "ich", "ei\u00b7nen", "Freund", "auf", "mei\u00b7ner", "ir\u00b7ren", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir halten mit Geklatsch ein halbes St\u00fcndchen an;", "tokens": ["Wir", "hal\u00b7ten", "mit", "Ge\u00b7klatsch", "ein", "hal\u00b7bes", "St\u00fcnd\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wenn zwei alte Hexen schelten,", "tokens": ["Wie", "wenn", "zwei", "al\u00b7te", "He\u00b7xen", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bricht von Bosheit nun und Neid ein ganzer Chor", "tokens": ["So", "bricht", "von", "Bos\u00b7heit", "nun", "und", "Neid", "ein", "gan\u00b7zer", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von Zoten, schlechtem Witz und Ha\u00df aus uns hervor,", "tokens": ["Von", "Zo\u00b7ten", ",", "schlech\u00b7tem", "Witz", "und", "Ha\u00df", "aus", "uns", "her\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df mir versch\u00e4mt die eignen Ohren gellten.", "tokens": ["Da\u00df", "mir", "ver\u00b7sch\u00e4mt", "die", "eig\u00b7nen", "Oh\u00b7ren", "gell\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Da kommt ein Handwerksbursch, bleich, mit zerri\u00dfnen Sohlen,", "tokens": ["Da", "kommt", "ein", "Hand\u00b7werks\u00b7bursch", ",", "bleich", ",", "mit", "zer\u00b7ri\u00df\u00b7nen", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "M\u00fctz in der Hand, geduckt, ein G\u00e4blein sich zu holen;", "tokens": ["M\u00fctz", "in", "der", "Hand", ",", "ge\u00b7duckt", ",", "ein", "G\u00e4\u00b7blein", "sich", "zu", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVPP", "$,", "ART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit einem Kreuzer w\u00e4r ihm wohlgetan.", "tokens": ["Mit", "ei\u00b7nem", "Kreu\u00b7zer", "w\u00e4r", "ihm", "wohl\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch weil ich diesen nicht in leerer Tasche trage", "tokens": ["Doch", "weil", "ich", "die\u00b7sen", "nicht", "in", "lee\u00b7rer", "Ta\u00b7sche", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDS", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und doch nicht freundlich ihm es zu gestehen wage,", "tokens": ["Und", "doch", "nicht", "freund\u00b7lich", "ihm", "es", "zu", "ge\u00b7ste\u00b7hen", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fahr ich ihn rauh abweisend an.", "tokens": ["Fahr", "ich", "ihn", "rauh", "ab\u00b7wei\u00b7send", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ob mir das k\u00fchle Herz in rascher Scham ergl\u00fcht,", "tokens": ["Ob", "mir", "das", "k\u00fch\u00b7le", "Herz", "in", "ra\u00b7scher", "Scham", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob auch ein blut'ger Schnitt mir durch die Seele zieht:", "tokens": ["Ob", "auch", "ein", "blut'\u00b7ger", "Schnitt", "mir", "durch", "die", "See\u00b7le", "zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht es nicht in meinen Blicken;", "tokens": ["Man", "sieht", "es", "nicht", "in", "mei\u00b7nen", "Bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich habe ja gelernt, mit h\u00f6hnisch leichtem Spiel", "tokens": ["Ich", "ha\u00b7be", "ja", "ge\u00b7lernt", ",", "mit", "h\u00f6h\u00b7nisch", "leich\u00b7tem", "Spiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den halberfrornen Lenz, das innere Gef\u00fchl,", "tokens": ["Den", "hal\u00b7ber\u00b7fror\u00b7nen", "Lenz", ",", "das", "in\u00b7ne\u00b7re", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn es erbl\u00fchen will, zu unterdr\u00fccken!", "tokens": ["Wenn", "es", "er\u00b7bl\u00fc\u00b7hen", "will", ",", "zu", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "O ich war treu, wie Gold, begeistert, klar und offen;", "tokens": ["O", "ich", "war", "treu", ",", "wie", "Gold", ",", "be\u00b7geis\u00b7tert", ",", "klar", "und", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ADJD", "$,", "PWAV", "NN", "$,", "VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Blatt ums andre fiel von meinem gr\u00fcnen Hoffen,", "tokens": ["Ein", "Blatt", "ums", "and\u00b7re", "fiel", "von", "mei\u00b7nem", "gr\u00fc\u00b7nen", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und taube N\u00fcsse tauscht ich ein!", "tokens": ["Und", "tau\u00b7be", "N\u00fcs\u00b7se", "tauscht", "ich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schmach \u00fcber dich, o Welt! du hast mich ganz beladen", "tokens": ["Schmach", "\u00fc\u00b7ber", "dich", ",", "o", "Welt", "!", "du", "hast", "mich", "ganz", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "FM", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit deinem Schlamm und Staub! O k\u00f6nnt ich rein mich baden", "tokens": ["Mit", "dei\u00b7nem", "Schlamm", "und", "Staub", "!", "O", "k\u00f6nnt", "ich", "rein", "mich", "ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$.", "NE", "VVFIN", "PPER", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im wilden Meer, sollt's auch ein Sterben sein!", "tokens": ["Im", "wil\u00b7den", "Meer", ",", "sollt's", "auch", "ein", "Ster\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VMFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Kokett ist dies Gedicht, Naivet\u00e4t erlogen", "tokens": ["Ko\u00b7kett", "ist", "dies", "Ge\u00b7dicht", ",", "Nai\u00b7ve\u00b7t\u00e4t", "er\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PDS", "NN", "$,", "NN", "VVINF"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und nur das Schn\u00f6de wahr! Ich hab euch arg betrogen,", "tokens": ["Und", "nur", "das", "Schn\u00f6\u00b7de", "wahr", "!", "Ich", "hab", "euch", "arg", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn zwei geworden sind mir Herz und Mund!", "tokens": ["Denn", "zwei", "ge\u00b7wor\u00b7den", "sind", "mir", "Herz", "und", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "VAPP", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin ganz euer Bild: selbsts\u00fcchtig, falsch und eitel", "tokens": ["Ich", "bin", "ganz", "eu\u00b7er", "Bild", ":", "selbst\u00b7s\u00fcch\u00b7tig", ",", "falsch", "und", "ei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und unklar in mir selbst; vom Fu\u00dfe bis zur Scheitel", "tokens": ["Und", "un\u00b7klar", "in", "mir", "selbst", ";", "vom", "Fu\u00b7\u00dfe", "bis", "zur", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER", "ADV", "$.", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein europ\u00e4isch schlechter Hund!", "tokens": ["Ein", "eu\u00b7ro\u00b7p\u00e4\u00b7isch", "schlech\u00b7ter", "Hund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}