{"dta.poem.14443": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Du am Felsenhange stand'st alleine,", "tokens": ["Wenn", "Du", "am", "Fel\u00b7sen\u00b7han\u00b7ge", "stan\u00b7d'st", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unten im Walde V\u00f6gel seltsam sangen", "tokens": ["Un\u00b7ten", "im", "Wal\u00b7de", "V\u00f6\u00b7gel", "selt\u00b7sam", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "NN", "ADJD", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und H\u00f6rner aus der Ferne irrend klangen,", "tokens": ["Und", "H\u00f6r\u00b7ner", "aus", "der", "Fer\u00b7ne", "ir\u00b7rend", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ob die Heimath dr\u00fcben nach Dir weine,", "tokens": ["Als", "ob", "die", "Hei\u00b7math", "dr\u00fc\u00b7ben", "nach", "Dir", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}