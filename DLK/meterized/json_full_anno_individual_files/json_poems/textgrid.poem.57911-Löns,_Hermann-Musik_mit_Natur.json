{"textgrid.poem.57911": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Musik mit Natur", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird noch nicht genug getutet", "tokens": ["Es", "wird", "noch", "nicht", "ge\u00b7nug", "ge\u00b7tu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In unserer lieben Leinestadt,", "tokens": ["In", "un\u00b7se\u00b7rer", "lie\u00b7ben", "Lei\u00b7ne\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Noch immer der Georgengarten", "tokens": ["Noch", "im\u00b7mer", "der", "Ge\u00b7or\u00b7gen\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Milit\u00e4rkonzerte hat.", "tokens": ["Nicht", "Mi\u00b7li\u00b7t\u00e4r\u00b7kon\u00b7zer\u00b7te", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort singen blo\u00df die Nachtigallen", "tokens": ["Dort", "sin\u00b7gen", "blo\u00df", "die", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Amsel, Drossel, Star und Fink,", "tokens": ["Und", "Am\u00b7sel", ",", "Dros\u00b7sel", ",", "Star", "und", "Fink", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist doch nichts, denn wieviel feiner", "tokens": ["Das", "ist", "doch", "nichts", ",", "denn", "wie\u00b7viel", "fei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "$,", "KON", "PWAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Klingt's Schnedderengteng und Tschingdingding.", "tokens": ["Klingt's", "Schned\u00b7de\u00b7reng\u00b7teng", "und", "Tsching\u00b7ding\u00b7ding", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Piepm\u00e4tze hat ja jedes Bierdorf,", "tokens": ["Pie\u00b7pm\u00e4t\u00b7ze", "hat", "ja", "je\u00b7des", "Bier\u00b7dorf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Gro\u00dfstadt aber ist verw\u00f6hnt,", "tokens": ["Die", "Gro\u00df\u00b7stadt", "a\u00b7ber", "ist", "ver\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns kann nur ein Konzert begeistern,", "tokens": ["Uns", "kann", "nur", "ein", "Kon\u00b7zert", "be\u00b7geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ord'ntlich knallt und klingt und dr\u00f6hnt.", "tokens": ["Das", "ord'\u00b7nt\u00b7lich", "knallt", "und", "klingt", "und", "dr\u00f6hnt", "."], "token_info": ["word", "word", "word"