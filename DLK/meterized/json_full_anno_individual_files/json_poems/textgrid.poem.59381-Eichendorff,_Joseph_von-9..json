{"textgrid.poem.59381": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wendet z\u00fcrnend sich von mir die Eine,", "tokens": ["Es", "wen\u00b7det", "z\u00fcr\u00b7nend", "sich", "von", "mir", "die", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versenkt die Ferne mit den Wunderlichtern.", "tokens": ["Ver\u00b7senkt", "die", "Fer\u00b7ne", "mit", "den", "Wun\u00b7der\u00b7lich\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es stockt der Tanz \u2013 ich stehe pl\u00f6tzlich n\u00fcchtern,", "tokens": ["Es", "stockt", "der", "Tanz", "\u2013", "ich", "ste\u00b7he", "pl\u00f6tz\u00b7lich", "n\u00fcch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Musik l\u00e4\u00dft treulos mich so ganz alleine.", "tokens": ["Mu\u00b7sik", "l\u00e4\u00dft", "treu\u00b7los", "mich", "so", "ganz", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da spricht der Abgrund dunkel: Bist nun meine;", "tokens": ["Da", "spricht", "der", "Ab\u00b7grund", 