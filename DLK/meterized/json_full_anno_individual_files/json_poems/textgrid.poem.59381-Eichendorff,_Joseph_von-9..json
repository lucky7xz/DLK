, "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kann sich r\u00fchren nicht, von Frost bezwungen.", "tokens": ["Und", "kann", "sich", "r\u00fch\u00b7ren", "nicht", ",", "von", "Frost", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVFIN", "PTKNEG", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Versteinert schweigen mu\u00df der Wehmut Welle,", "tokens": ["Ver\u00b7stei\u00b7nert", "schwei\u00b7gen", "mu\u00df", "der", "Weh\u00b7mut", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVINF", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie willig auch, schm\u00f6lz ihn ein w\u00e4rmend Auge,", "tokens": ["Wie", "wil\u00b7lig", "auch", ",", "schm\u00f6lz", "ihn", "ein", "w\u00e4r\u00b7mend", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "$,", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kristall zerflie\u00dfen wollt als Tr\u00e4nenquelle.", "tokens": ["Kris\u00b7tall", "zer\u00b7flie\u00b7\u00dfen", "wollt", "als", "Tr\u00e4\u00b7nen\u00b7quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es wendet z\u00fcrnend sich von mir die Eine,", "tokens": ["Es", "wen\u00b7det", "z\u00fcr\u00b7nend", "sich", "von", "mir", "die", "Ei\u00b7ne", ","], "token_info": ["word", "word"