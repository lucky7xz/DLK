{"dta.poem.21768": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich auf meiner Liebsten Mund\n(ach sanfte Ruhstat!) br\u00fcnstig lage\nund meiner Schmerzen herbe Plage\nihr t\u00e4ht' au\u00df ganzem Herzen kund/\nwie\n\nGeharnschter Venus", "tokens": ["Als", "ich", "auf", "mei\u00b7ner", "Liebs\u00b7ten", "Mund", "(", "ach", "sanf\u00b7te", "Ruh\u00b7stat", "!", ")", "br\u00fcns\u00b7tig", "la\u00b7ge", "und", "mei\u00b7ner", "Schmer\u00b7zen", "her\u00b7be", "Pla\u00b7ge", "ihr", "t\u00e4ht'", "au\u00df", "gan\u00b7zem", "Her\u00b7zen", "kund", "/", "wie", "Ge\u00b7harnschter", "Ve\u00b7nus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN", "$(", "XY", "ADJA", "NN", "$.", "$(", "ADJD", "VVFIN", "KON", "PPOSAT", "NN", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$(", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+--+-+-+-+--+-+-+-+--+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "wie ich so oft um ihrentwegen", "tokens": ["wie", "ich", "so", "oft", "um", "ih\u00b7rent\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ruh-trost-und Sinnen-ohn gelegen.", "tokens": ["Ruh\u00b7trost\u00b7\u00b7und", "Sin\u00b7nen\u00b7ohn", "ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein (sprach sie) Herzgen/ sage doch:", "tokens": ["Mein", "(", "sprach", "sie", ")", "Herz\u00b7gen", "/", "sa\u00b7ge", "doch", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "VVFIN", "PPER", "$(", "NN", "$(", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zu welcher Zeit du bist entbronnen/", "tokens": ["zu", "wel\u00b7cher", "Zeit", "du", "bist", "ent\u00b7bron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und wodurch du mich lieb gewonnen:", "tokens": ["und", "wo\u00b7durch", "du", "mich", "lieb", "ge\u00b7won\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich mich recht entsinne noch/", "tokens": ["Wo", "ich", "mich", "recht", "ent\u00b7sin\u00b7ne", "noch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "hastu/ auch gar f\u00fcr wenig Wochen/", "tokens": ["has\u00b7tu", "/", "auch", "gar", "f\u00fcr", "we\u00b7nig", "Wo\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "ADV", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "kalt", "tokens": ["kalt"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Da ich doch/ als zum ersten mahl", "tokens": ["Da", "ich", "doch", "/", "als", "zum", "ers\u00b7ten", "mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$(", "KOKOM", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich dich nur obenhin erblikket/", "tokens": ["ich", "dich", "nur", "o\u00b7ben\u00b7hin", "er\u00b7blik\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "durch deine Freyheit blieb bestrikket.", "tokens": ["durch", "dei\u00b7ne", "Frey\u00b7heit", "blieb", "be\u00b7strik\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "di\u00df war nur meine gr\u00f6ste Qvaal/", "tokens": ["di\u00df", "war", "nur", "mei\u00b7ne", "gr\u00f6s\u00b7te", "Qvaal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die auch die G\u00f6tter kan betr\u00fcben/", "tokens": ["die", "auch", "die", "G\u00f6t\u00b7ter", "kan", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "dich sonder Gegen-Liebe lieben.", "tokens": ["dich", "son\u00b7der", "Ge\u00b7gen\u00b7Lie\u00b7be", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gott wei\u00df/ wie mir zu muhte war", "tokens": ["Gott", "wei\u00df", "/", "wie", "mir", "zu", "muh\u00b7te", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "PWAV", "PPER", "PTKZU", "VVFIN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auf die so unverhoffte Frage/", "tokens": ["auf", "die", "so", "un\u00b7ver\u00b7hoff\u00b7te", "Fra\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "vermischt von Zorn/ Verwei\u00df und Klage", "tokens": ["ver\u00b7mischt", "von", "Zorn", "/", "Ver\u00b7wei\u00df", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die meinen Undank machten klar!", "tokens": ["die", "mei\u00b7nen", "Un\u00b7dank", "mach\u00b7ten", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schaam/ so ich daher empfunde/", "tokens": ["Die", "Schaam", "/", "so", "ich", "da\u00b7her", "emp\u00b7fun\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "nahm Red\u2019 und Antwort meinem Munde.", "tokens": ["nahm", "Red'", "und", "Ant\u00b7wort", "mei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich ward verstarret/ kalt/ erbla\u00dft/", "tokens": ["Ich", "ward", "ver\u00b7star\u00b7ret", "/", "kalt", "/", "er\u00b7bla\u00dft", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "ADJD", "$(", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie/ dem die Seele kaum sich reget:", "tokens": ["wie", "/", "dem", "die", "See\u00b7le", "kaum", "sich", "re\u00b7get", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "ART", "ART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "bi\u00df/ au\u00df Erbarmn\u00fc\u00df sie beweget", "tokens": ["bi\u00df", "/", "au\u00df", "Er\u00b7barm\u00b7n\u00fc\u00df", "sie", "be\u00b7we\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$(", "APPR", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mich in die schlanken Arme fasst\u2019/", "tokens": ["mich", "in", "die", "schlan\u00b7ken", "Ar\u00b7me", "fasst'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! da ward mir gemach das Leben/", "tokens": ["Ach", "!", "da", "ward", "mir", "ge\u00b7mach", "das", "Le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Krast/ Geist und W\u00e4rme wieder geben.", "tokens": ["Krast", "/", "Geist", "und", "W\u00e4r\u00b7me", "wie\u00b7der", "ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Jm k\u00fcssen fing sie an-noch mehr", "tokens": ["Jm", "k\u00fcs\u00b7sen", "fing", "sie", "an\u00b7noch", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "VVINF", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mich bey der Fakkel-zubeschweeren", "tokens": ["mich", "bey", "der", "Fak\u00b7kel\u00b7zu\u00b7be\u00b7schwee\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die unser\u2019 Herzen kan versehren:", "tokens": ["die", "un\u00b7ser'", "Her\u00b7zen", "kan", "ver\u00b7seh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sag an (bistu mir gut) wann ehr", "tokens": ["Sag", "an", "(", "bis\u00b7tu", "mir", "gut", ")", "wann", "ehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "$(", "APPR", "PPER", "ADJD", "$(", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "du angefangen mich zu lieben/", "tokens": ["du", "an\u00b7ge\u00b7fan\u00b7gen", "mich", "zu", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und wa\u00df darzu dich erst getrieben.", "tokens": ["und", "wa\u00df", "dar\u00b7zu", "dich", "erst", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Du weist es wie auf jener Fluht/", "tokens": ["Du", "weist", "es", "wie", "auf", "je\u00b7ner", "Fluht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "von kalter Norden luft gestanden/", "tokens": ["von", "kal\u00b7ter", "Nor\u00b7den", "luft", "ge\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ich lag in deiner Arme Banden.", "tokens": ["ich", "lag", "in", "dei\u00b7ner", "Ar\u00b7me", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie ich dich von dem Wagen nahm", "tokens": ["Wie", "ich", "dich", "von", "dem", "Wa\u00b7gen", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und k\u00fc\u00dfte die gefrorne Wangen:", "tokens": ["und", "k\u00fc\u00df\u00b7te", "die", "ge\u00b7fror\u00b7ne", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald hat mein Herze Gluht gefangen.", "tokens": ["Bald", "hat", "mein", "Her\u00b7ze", "Gluht", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Feuer/ so au\u00df K\u00e4lte kahm", "tokens": ["Das", "Feu\u00b7er", "/", "so", "au\u00df", "K\u00e4l\u00b7te", "kahm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ob meines Lebens Rest zusammen.", "tokens": ["ob", "mei\u00b7nes", "Le\u00b7bens", "Rest", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun (sagt sie) hat ein kalter Ku\u00df", "tokens": ["Nun", "(", "sagt", "sie", ")", "hat", "ein", "kal\u00b7ter", "Ku\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "VVFIN", "PPER", "$(", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dich bracht in Feuer/ Hizz\u2019 und Leiden;", "tokens": ["dich", "bracht", "in", "Feu\u00b7er", "/", "Hiz\u00b7z'", "und", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "NE", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wei\u00df ich/ da\u00df K\u00fchlung/ Lust und Freuden", "tokens": ["wei\u00df", "ich", "/", "da\u00df", "K\u00fch\u00b7lung", "/", "Lust", "und", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein Warmer dir erwekken mu\u00df.", "tokens": ["ein", "War\u00b7mer", "dir", "er\u00b7wek\u00b7ken", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hat sie mir so viel erteilet/", "tokens": ["Der", "hat", "sie", "mir", "so", "viel", "er\u00b7tei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}