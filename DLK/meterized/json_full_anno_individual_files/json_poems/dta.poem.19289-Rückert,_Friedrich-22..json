{"dta.poem.19289": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du denkest fort und fort, dein Denken ist ein Schaffen,", "tokens": ["Du", "den\u00b7kest", "fort", "und", "fort", ",", "dein", "Den\u00b7ken", "ist", "ein", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine Denkkraft hat zu f\u00fcrchten kein Erschlaffen.", "tokens": ["Und", "dei\u00b7ne", "Denk\u00b7kraft", "hat", "zu", "f\u00fcrch\u00b7ten", "kein", "Er\u00b7schlaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was du einmal gedacht, das kanst du nie vergessen;", "tokens": ["Was", "du", "ein\u00b7mal", "ge\u00b7dacht", ",", "das", "kanst", "du", "nie", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du geschaffen, stets erinnerst du dich dessen.", "tokens": ["Was", "du", "ge\u00b7schaf\u00b7fen", ",", "stets", "e\u00b7rin\u00b7nerst", "du", "dich", "des\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PRF", "PDS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Indem du meiner dich erinnerst, hast du mich", "tokens": ["In\u00b7dem", "du", "mei\u00b7ner", "dich", "e\u00b7rin\u00b7nerst", ",", "hast", "du", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Innern ewig, und im Innern hab' ich dich.", "tokens": ["Im", "In\u00b7nern", "e\u00b7wig", ",", "und", "im", "In\u00b7nern", "hab'", "ich", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "KON", "APPRART", "NN", "VAFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vergi\u00df mich, Welt! ich wei\u00df, da\u00df Er sich mein erinnert;", "tokens": ["Ver\u00b7gi\u00df", "mich", ",", "Welt", "!", "ich", "wei\u00df", ",", "da\u00df", "Er", "sich", "mein", "e\u00b7rin\u00b7nert", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sterb' ich au\u00dfen dir, leb' ich in ihm verinnert.", "tokens": ["Und", "sterb'", "ich", "au\u00b7\u00dfen", "dir", ",", "leb'", "ich", "in", "ihm", "ver\u00b7in\u00b7nert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "$,", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}}}}