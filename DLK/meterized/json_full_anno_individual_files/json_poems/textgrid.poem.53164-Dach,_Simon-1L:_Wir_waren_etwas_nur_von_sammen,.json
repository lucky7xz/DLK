{"textgrid.poem.53164": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir waren etwas nur von sammen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir waren etwas nur von sammen,", "tokens": ["Wir", "wa\u00b7ren", "et\u00b7was", "nur", "von", "sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rosett', und du bist umbgewandt,", "tokens": ["Ro\u00b7sett'", ",", "und", "du", "bist", "umb\u00b7ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So heg' ich auch nun andre Flammen,", "tokens": ["So", "heg'", "ich", "auch", "nun", "and\u00b7re", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich dein' Vntrew hab' erkandt.", "tokens": ["Weil", "ich", "dein'", "Vnt\u00b7rew", "hab'", "er\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wil mich nimmer mehr vertrawen", "tokens": ["Ich", "wil", "mich", "nim\u00b7mer", "mehr", "ver\u00b7tra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So leicht gesinnter Freundlichheit,", "tokens": [