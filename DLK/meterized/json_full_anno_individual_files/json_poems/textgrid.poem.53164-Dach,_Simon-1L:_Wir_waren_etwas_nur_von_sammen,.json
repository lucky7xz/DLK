i\u00b7do", "!", "wer", "dir", "mehr", "wird", "tra\u00b7wen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Den treffe Fluch und Hertzeleid!", "tokens": ["Den", "tref\u00b7fe", "Fluch", "und", "Hert\u00b7ze\u00b7leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir werden, falsche Sch\u00e4ffrin schawen,", "tokens": ["Wir", "wer\u00b7den", ",", "fal\u00b7sche", "Sch\u00e4f\u00b7frin", "scha\u00b7wen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wehn dies zum ersten noch gerewt.", "tokens": ["Wehn", "dies", "zum", "ers\u00b7ten", "noch", "ge\u00b7rewt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "APPRART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der meinen Platz mir weg gekrieget,", "tokens": ["Der", "mei\u00b7nen", "Platz", "mir", "weg", "ge\u00b7krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kan dich nicht lieben so, wie ich,", "tokens": ["Kan", "dich", "nicht", "lie\u00b7ben", "so", ",", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd die ich ietzt lieb', \u00