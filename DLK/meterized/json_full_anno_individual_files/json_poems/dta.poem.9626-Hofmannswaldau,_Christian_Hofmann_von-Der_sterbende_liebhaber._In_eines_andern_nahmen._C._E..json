{"dta.poem.9626": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der sterbende liebhaber. In eines  \n andern nahmen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein hertze bricht mir schon: es zittern meine glieder:", "tokens": ["Mein", "hert\u00b7ze", "bricht", "mir", "schon", ":", "es", "zit\u00b7tern", "mei\u00b7ne", "glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die augen sind umw\u00f6lckt mit tr\u00fcber dunckelheit:", "tokens": ["Die", "au\u00b7gen", "sind", "um\u00b7w\u00f6lckt", "mit", "tr\u00fc\u00b7ber", "dun\u00b7ckel\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bringt meinen sarg herbey: reicht her das sterbe-kleid:", "tokens": ["Bringt", "mei\u00b7nen", "sarg", "her\u00b7bey", ":", "reicht", "her", "das", "ster\u00b7be\u00b7kleid", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "PTKVZ", "$.", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Stimmt/ eulen/ euren thon: hebt an die trauer-lieder:", "tokens": ["Stimmt", "/", "eu\u00b7len", "/", "eu\u00b7ren", "thon", ":", "hebt", "an", "die", "trau\u00b7e\u00b7rlie\u00b7der", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVINF", "$(", "PPOSAT", "NN", "$.", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erscheine blasser todt/ setz bey den sarg dich nieder/", "tokens": ["Er\u00b7schei\u00b7ne", "blas\u00b7ser", "todt", "/", "setz", "bey", "den", "sarg", "dich", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "$(", "VVFIN", "APPR", "ART", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du solt der marschall seyn von meinem grab-geleit:", "tokens": ["Du", "solt", "der", "mar\u00b7schall", "seyn", "von", "mei\u00b7nem", "grab\u00b7ge\u00b7leit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Folgt geister/ paar bey paar/ es ist schon hohe zeit:", "tokens": ["Folgt", "geis\u00b7ter", "/", "paar", "bey", "paar", "/", "es", "ist", "schon", "ho\u00b7he", "zeit", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$(", "PIAT", "APPR", "PIAT", "$(", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Folg\u2019 angst als schwester mir/ folgt schreck und rach als br\u00fcder:", "tokens": ["Fol\u00b7g'", "angst", "als", "schwes\u00b7ter", "mir", "/", "folgt", "schreck", "und", "rach", "als", "br\u00fc\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KOKOM", "ADJD", "PPER", "$(", "VVFIN", "ADJD", "KON", "VVFIN", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Vesuv und Aetna leucht statt fackeln mir zur seiten;", "tokens": ["Ve\u00b7suv", "und", "A\u00b7et\u00b7na", "leucht", "statt", "fa\u00b7ckeln", "mir", "zur", "sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Dein brausen/ nord-wind/ soll die todten-glocke l\u00e4uten.", "tokens": ["Dein", "brau\u00b7sen", "/", "nord\u00b7wind", "/", "soll", "die", "tod\u00b7ten\u00b7glo\u00b7cke", "l\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$(", "NE", "$(", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du aber/ armes hertz/ sey du der leichen-stein;", "tokens": ["Du", "a\u00b7ber", "/", "ar\u00b7mes", "hertz", "/", "sey", "du", "der", "lei\u00b7chen\u00b7stein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADJA", "NN", "$(", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00df jeden wandersmann die bittre grabschrifft lesen:", "tokens": ["La\u00df", "je\u00b7den", "wan\u00b7ders\u00b7mann", "die", "bitt\u00b7re", "grab\u00b7schrifft", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Charlottens grausamkeit ri\u00df hier mein leben ein;", "tokens": ["Char\u00b7lot\u00b7tens", "grau\u00b7sam\u00b7keit", "ri\u00df", "hier", "mein", "le\u00b7ben", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich aber bin ihr treu bi\u00df in den todt gewesen.", "tokens": ["Ich", "a\u00b7ber", "bin", "ihr", "treu", "bi\u00df", "in", "den", "todt", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "APPR", "ART", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}