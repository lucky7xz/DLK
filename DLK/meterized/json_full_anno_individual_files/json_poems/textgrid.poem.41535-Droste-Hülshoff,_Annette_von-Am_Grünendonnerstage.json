{"textgrid.poem.41535": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Gr\u00fcnendonnerstage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Wundernacht, ich gr\u00fc\u00dfe!", "tokens": ["O", "Wun\u00b7der\u00b7nacht", ",", "ich", "gr\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herr Jesus w\u00e4scht die F\u00fc\u00dfe,", "tokens": ["Herr", "Je\u00b7sus", "w\u00e4scht", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Luft ganz stille stand,", "tokens": ["Die", "Luft", "ganz", "stil\u00b7le", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Man h\u00f6rt den Atem hallen", "tokens": ["Man", "h\u00f6rt", "den", "A\u00b7tem", "hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wie die Tropfen fallen", "tokens": ["Und", "wie", "die", "Trop\u00b7fen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von seiner heil'gen Hand.", "tokens": ["Von", "sei\u00b7ner", "heil'\u00b7gen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da Jesus sich tut beugen,", "tokens": ["Da", "Je\u00b7sus", "sich", "tut", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins tiefe Meer sich neigen", "tokens": ["Ins", "tie\u00b7fe", "Meer", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl Inseln diesem Gru\u00df.", "tokens": ["Wohl", "In\u00b7seln", "die\u00b7sem", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist er so tief gestiegen,", "tokens": ["Ist", "er", "so", "tief", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00df ich ewig liegen", "tokens": ["So", "mu\u00df", "ich", "e\u00b7wig", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor meines N\u00e4chsten Fu\u00df.", "tokens": ["Vor", "mei\u00b7nes", "N\u00e4chs\u00b7ten", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Herr, ob sich gleich bet\u00f6ret", "tokens": ["Herr", ",", "ob", "sich", "gleich", "be\u00b7t\u00f6\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PRF", "ADV", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Seele mein emp\u00f6ret", "tokens": ["Die", "See\u00b7le", "mein", "em\u00b7p\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor aller Niedrigkeit,", "tokens": ["Vor", "al\u00b7ler", "Nied\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich vielmehr mein Leben", "tokens": ["Da\u00df", "ich", "viel\u00b7mehr", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In Qualen aufzugeben", "tokens": ["In", "Qua\u00b7len", "auf\u00b7zu\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVIZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr deinen Ruhm bereit:", "tokens": ["F\u00fcr", "dei\u00b7nen", "Ruhm", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So gib, da\u00df ich nicht klage,", "tokens": ["So", "gib", ",", "da\u00df", "ich", "nicht", "kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du in meine Tage", "tokens": ["Wenn", "du", "in", "mei\u00b7ne", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hast alle Schmach gebannt,", "tokens": ["Hast", "al\u00b7le", "Schmach", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df brennen meine Wunden,", "tokens": ["La\u00df", "bren\u00b7nen", "mei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So du mich stark befunden", "tokens": ["So", "du", "mich", "stark", "be\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu solchem harten Stand!", "tokens": ["Zu", "sol\u00b7chem", "har\u00b7ten", "Stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Gott, ich kann nicht bergen,", "tokens": ["O", "Gott", ",", "ich", "kann", "nicht", "ber\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie angst mir vor den Schergen,", "tokens": ["Wie", "angst", "mir", "vor", "den", "Scher\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die du vielleicht gesandt,", "tokens": ["Die", "du", "viel\u00b7leicht", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In Krankheit oder Gr\u00e4men", "tokens": ["In", "Krank\u00b7heit", "o\u00b7der", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sinne mir zu nehmen", "tokens": ["Die", "Sin\u00b7ne", "mir", "zu", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu t\u00f6ten den Verstand!", "tokens": ["Zu", "t\u00f6\u00b7ten", "den", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es ist mir oft zu Sinnen,", "tokens": ["Es", "ist", "mir", "oft", "zu", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wolle schon beginnen", "tokens": ["Als", "wol\u00b7le", "schon", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein schweres Strafgericht,", "tokens": ["Dein", "schwe\u00b7res", "Straf\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als d\u00e4mmre eine Wolke,", "tokens": ["Als", "d\u00e4mm\u00b7re", "ei\u00b7ne", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch unbewu\u00dft dem Volke,", "tokens": ["Doch", "un\u00b7be\u00b7wu\u00dft", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um meines Geistes Licht.", "tokens": ["Um", "mei\u00b7nes", "Geis\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch wie die Schmerzen schwinden,", "tokens": ["Doch", "wie", "die", "Schmer\u00b7zen", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mein Gehirn entz\u00fcnden,", "tokens": ["Die", "mein", "Ge\u00b7hirn", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So flieht der Nebelduft,", "tokens": ["So", "flieht", "der", "Ne\u00b7bel\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mit geheimem Gl\u00fchen", "tokens": ["Und", "mit", "ge\u00b7hei\u00b7mem", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchl' ich mich neu umziehen", "tokens": ["F\u00fchl'", "ich", "mich", "neu", "um\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "VVIZU"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die frische starke Luft.", "tokens": ["Die", "fri\u00b7sche", "star\u00b7ke", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mein Jesu, darf ich w\u00e4hlen,", "tokens": ["Mein", "Je\u00b7su", ",", "darf", "ich", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich will mich lieber qu\u00e4len", "tokens": ["Ich", "will", "mich", "lie\u00b7ber", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In aller Schmach und Leid,", "tokens": ["In", "al\u00b7ler", "Schmach", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als da\u00df mir so benommen,", "tokens": ["Als", "da\u00df", "mir", "so", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ob auch zu meinem Frommen,", "tokens": ["Ob", "auch", "zu", "mei\u00b7nem", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Menschenherrlichkeit.", "tokens": ["Die", "Men\u00b7schen\u00b7herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch ist er so vergiftet,", "tokens": ["Doch", "ist", "er", "so", "ver\u00b7gif\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df es Vernichtung stiftet,", "tokens": ["Da\u00df", "es", "Ver\u00b7nich\u00b7tung", "stif\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er mein Herz umfleu\u00dft,", "tokens": ["Wenn", "er", "mein", "Herz", "um\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So la\u00df mich ihn verlieren,", "tokens": ["So", "la\u00df", "mich", "ihn", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Seele heimzuf\u00fchren,", "tokens": ["Die", "See\u00b7le", "heim\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den reichbegabten Geist.", "tokens": ["Den", "reich\u00b7be\u00b7gab\u00b7ten", "Geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Hast du es denn beschlossen,", "tokens": ["Hast", "du", "es", "denn", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich soll ausgegossen", "tokens": ["Da\u00df", "ich", "soll", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein tot Gew\u00e4sser stehn", "tokens": ["Ein", "tot", "Ge\u00b7w\u00e4s\u00b7ser", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr dieses ganze Leben,", "tokens": ["F\u00fcr", "die\u00b7ses", "gan\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So will ich denn mit Beben", "tokens": ["So", "will", "ich", "denn", "mit", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An deine Pr\u00fcfung gehn.", "tokens": ["An", "dei\u00b7ne", "Pr\u00fc\u00b7fung", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O Wundernacht, ich gr\u00fc\u00dfe!", "tokens": ["O", "Wun\u00b7der\u00b7nacht", ",", "ich", "gr\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herr Jesus w\u00e4scht die F\u00fc\u00dfe,", "tokens": ["Herr", "Je\u00b7sus", "w\u00e4scht", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Luft ganz stille stand,", "tokens": ["Die", "Luft", "ganz", "stil\u00b7le", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Man h\u00f6rt den Atem hallen", "tokens": ["Man", "h\u00f6rt", "den", "A\u00b7tem", "hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wie die Tropfen fallen", "tokens": ["Und", "wie", "die", "Trop\u00b7fen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von seiner heil'gen Hand.", "tokens": ["Von", "sei\u00b7ner", "heil'\u00b7gen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da Jesus sich tut beugen,", "tokens": ["Da", "Je\u00b7sus", "sich", "tut", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins tiefe Meer sich neigen", "tokens": ["Ins", "tie\u00b7fe", "Meer", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl Inseln diesem Gru\u00df.", "tokens": ["Wohl", "In\u00b7seln", "die\u00b7sem", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist er so tief gestiegen,", "tokens": ["Ist", "er", "so", "tief", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00df ich ewig liegen", "tokens": ["So", "mu\u00df", "ich", "e\u00b7wig", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor meines N\u00e4chsten Fu\u00df.", "tokens": ["Vor", "mei\u00b7nes", "N\u00e4chs\u00b7ten", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Herr, ob sich gleich bet\u00f6ret", "tokens": ["Herr", ",", "ob", "sich", "gleich", "be\u00b7t\u00f6\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PRF", "ADV", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Seele mein emp\u00f6ret", "tokens": ["Die", "See\u00b7le", "mein", "em\u00b7p\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor aller Niedrigkeit,", "tokens": ["Vor", "al\u00b7ler", "Nied\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich vielmehr mein Leben", "tokens": ["Da\u00df", "ich", "viel\u00b7mehr", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In Qualen aufzugeben", "tokens": ["In", "Qua\u00b7len", "auf\u00b7zu\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVIZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr deinen Ruhm bereit:", "tokens": ["F\u00fcr", "dei\u00b7nen", "Ruhm", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "So gib, da\u00df ich nicht klage,", "tokens": ["So", "gib", ",", "da\u00df", "ich", "nicht", "kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du in meine Tage", "tokens": ["Wenn", "du", "in", "mei\u00b7ne", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hast alle Schmach gebannt,", "tokens": ["Hast", "al\u00b7le", "Schmach", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df brennen meine Wunden,", "tokens": ["La\u00df", "bren\u00b7nen", "mei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So du mich stark befunden", "tokens": ["So", "du", "mich", "stark", "be\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu solchem harten Stand!", "tokens": ["Zu", "sol\u00b7chem", "har\u00b7ten", "Stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "O Gott, ich kann nicht bergen,", "tokens": ["O", "Gott", ",", "ich", "kann", "nicht", "ber\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie angst mir vor den Schergen,", "tokens": ["Wie", "angst", "mir", "vor", "den", "Scher\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die du vielleicht gesandt,", "tokens": ["Die", "du", "viel\u00b7leicht", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In Krankheit oder Gr\u00e4men", "tokens": ["In", "Krank\u00b7heit", "o\u00b7der", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sinne mir zu nehmen", "tokens": ["Die", "Sin\u00b7ne", "mir", "zu", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu t\u00f6ten den Verstand!", "tokens": ["Zu", "t\u00f6\u00b7ten", "den", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Es ist mir oft zu Sinnen,", "tokens": ["Es", "ist", "mir", "oft", "zu", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wolle schon beginnen", "tokens": ["Als", "wol\u00b7le", "schon", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein schweres Strafgericht,", "tokens": ["Dein", "schwe\u00b7res", "Straf\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als d\u00e4mmre eine Wolke,", "tokens": ["Als", "d\u00e4mm\u00b7re", "ei\u00b7ne", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch unbewu\u00dft dem Volke,", "tokens": ["Doch", "un\u00b7be\u00b7wu\u00dft", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um meines Geistes Licht.", "tokens": ["Um", "mei\u00b7nes", "Geis\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Doch wie die Schmerzen schwinden,", "tokens": ["Doch", "wie", "die", "Schmer\u00b7zen", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mein Gehirn entz\u00fcnden,", "tokens": ["Die", "mein", "Ge\u00b7hirn", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So flieht der Nebelduft,", "tokens": ["So", "flieht", "der", "Ne\u00b7bel\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mit geheimem Gl\u00fchen", "tokens": ["Und", "mit", "ge\u00b7hei\u00b7mem", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchl' ich mich neu umziehen", "tokens": ["F\u00fchl'", "ich", "mich", "neu", "um\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "VVIZU"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die frische starke Luft.", "tokens": ["Die", "fri\u00b7sche", "star\u00b7ke", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Mein Jesu, darf ich w\u00e4hlen,", "tokens": ["Mein", "Je\u00b7su", ",", "darf", "ich", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich will mich lieber qu\u00e4len", "tokens": ["Ich", "will", "mich", "lie\u00b7ber", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In aller Schmach und Leid,", "tokens": ["In", "al\u00b7ler", "Schmach", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als da\u00df mir so benommen,", "tokens": ["Als", "da\u00df", "mir", "so", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ob auch zu meinem Frommen,", "tokens": ["Ob", "auch", "zu", "mei\u00b7nem", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Menschenherrlichkeit.", "tokens": ["Die", "Men\u00b7schen\u00b7herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Doch ist er so vergiftet,", "tokens": ["Doch", "ist", "er", "so", "ver\u00b7gif\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df es Vernichtung stiftet,", "tokens": ["Da\u00df", "es", "Ver\u00b7nich\u00b7tung", "stif\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er mein Herz umfleu\u00dft,", "tokens": ["Wenn", "er", "mein", "Herz", "um\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So la\u00df mich ihn verlieren,", "tokens": ["So", "la\u00df", "mich", "ihn", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Seele heimzuf\u00fchren,", "tokens": ["Die", "See\u00b7le", "heim\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den reichbegabten Geist.", "tokens": ["Den", "reich\u00b7be\u00b7gab\u00b7ten", "Geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Hast du es denn beschlossen,", "tokens": ["Hast", "du", "es", "denn", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich soll ausgegossen", "tokens": ["Da\u00df", "ich", "soll", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein tot Gew\u00e4sser stehn", "tokens": ["Ein", "tot", "Ge\u00b7w\u00e4s\u00b7ser", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr dieses ganze Leben,", "tokens": ["F\u00fcr", "die\u00b7ses", "gan\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So will ich denn mit Beben", "tokens": ["So", "will", "ich", "denn", "mit", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An deine Pr\u00fcfung gehn.", "tokens": ["An", "dei\u00b7ne", "Pr\u00fc\u00b7fung", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}