{"dta.poem.18712": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Als wenn das auf Namen ruhte!", "tokens": ["Als", "wenn", "das", "auf", "Na\u00b7men", "ruh\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PDS", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was sich schweigend nur entfaltet.", "tokens": ["Was", "sich", "schwei\u00b7gend", "nur", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieb\u2019 ich doch das sch\u00f6ne Gute", "tokens": ["Lieb'", "ich", "doch", "das", "sch\u00f6\u00b7ne", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie es sich aus Gott gestaltet.", "tokens": ["Wie", "es", "sich", "aus", "Gott", "ge\u00b7stal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jemand lieb\u2019 ich, das ist n\u00f6thig,", "tokens": ["Je\u00b7mand", "lieb'", "ich", ",", "das", "ist", "n\u00f6\u00b7thig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemand hass\u2019 ich; soll ich hassen;", "tokens": ["Nie\u00b7mand", "hass'", "ich", ";", "soll", "ich", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch dazu bin ich erb\u00f6tig,", "tokens": ["Auch", "da\u00b7zu", "bin", "ich", "er\u00b7b\u00f6\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hasse gleich in ganzen Massen.", "tokens": ["Has\u00b7se", "gleich", "in", "gan\u00b7zen", "Mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst sie aber n\u00e4her kennen,", "tokens": ["Willst", "sie", "a\u00b7ber", "n\u00e4\u00b7her", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich auf\u2019s Rechte, sich auf\u2019s Schlechte,", "tokens": ["Sich", "auf's", "Rech\u00b7te", ",", "sich", "auf's", "Schlech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$,", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was sie ganz f\u00fcrtrefflich nennen", "tokens": ["Was", "sie", "ganz", "f\u00fcr\u00b7treff\u00b7lich", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist wahrscheinlich nicht das Rechte.", "tokens": ["Ist", "wahr\u00b7schein\u00b7lich", "nicht", "das", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn das Rechte zu ergreifen", "tokens": ["Denn", "das", "Rech\u00b7te", "zu", "er\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Muss man aus dem Grunde leben,", "tokens": ["Muss", "man", "aus", "dem", "Grun\u00b7de", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und saalbadrisch auszuschweifen", "tokens": ["Und", "saal\u00b7ba\u00b7drisch", "aus\u00b7zu\u00b7schwei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "D\u00fcnket mich ein seicht Bestreben.", "tokens": ["D\u00fcn\u00b7ket", "mich", "ein", "seicht", "Be\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl! Herr Knitterer er kann sich", "tokens": ["Wohl", "!", "Herr", "Knit\u00b7te\u00b7rer", "er", "kann", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "NN", "NN", "PPER", "VMFIN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Zersplitterer vereinen,", "tokens": ["Mit", "Zer\u00b7split\u00b7te\u00b7rer", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Verwitterer alsdann sich", "tokens": ["Und", "Ver\u00b7wit\u00b7te\u00b7rer", "als\u00b7dann", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "PRF"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Allenfalls der beste scheinen.", "tokens": ["Al\u00b7len\u00b7falls", "der", "bes\u00b7te", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dass nur immer in Erneuung", "tokens": ["Dass", "nur", "im\u00b7mer", "in", "Er\u00b7neu\u00b7ung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder t\u00e4glich neues h\u00f6re,", "tokens": ["Je\u00b7der", "t\u00e4g\u00b7lich", "neu\u00b7es", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zugleich auch die Zerstreuung", "tokens": ["Und", "zu\u00b7gleich", "auch", "die", "Zer\u00b7streu\u00b7ung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeden in sich selbst zerst\u00f6re.", "tokens": ["Je\u00b7den", "in", "sich", "selbst", "zer\u00b7st\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dies der Landsmann w\u00fcnscht und liebet,", "tokens": ["Dies", "der", "Lands\u00b7mann", "w\u00fcnscht", "und", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag er Deutsch mag Teutsch sich schreiben,", "tokens": ["Mag", "er", "Deutsch", "mag", "Teutsch", "sich", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VMFIN", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Lied nur heimlich piepet:", "tokens": ["Und", "das", "Lied", "nur", "heim\u00b7lich", "pie\u00b7pet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Also war es und wird bleiben.", "tokens": ["Al\u00b7so", "war", "es", "und", "wird", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}