{"textgrid.poem.55864": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Abend in Sk\u00e5ne", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Park ist hoch. Und wie aus einem Haus", "tokens": ["Der", "Park", "ist", "hoch", ".", "Und", "wie", "aus", "ei\u00b7nem", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "tret ich aus seiner D\u00e4mmerung heraus", "tokens": ["tret", "ich", "aus", "sei\u00b7ner", "D\u00e4m\u00b7me\u00b7rung", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in Ebene und Abend. In den Wind,", "tokens": ["in", "E\u00b7be\u00b7ne", "und", "A\u00b7bend", ".", "In", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "denselben Wind, den auch die Wolken f\u00fchlen,", "tokens": ["den\u00b7sel\u00b7ben", "Wind", ",", "den", "auch", "die", "Wol\u00b7ken", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "die hellen Fl\u00fcsse und die Fl\u00fcgelm\u00fchlen,", "tokens": ["die", "hel\u00b7len", "Fl\u00fcs\u00b7se", "und", "die", "Fl\u00fc\u00b7gel\u00b7m\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die langsam mahlend stehn am Himmelsrand.", "tokens": ["die", "lang\u00b7sam", "mah\u00b7lend", "stehn", "am", "Him\u00b7mels\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jetzt bin auch ich ein Ding in seiner Hand,", "tokens": ["Jetzt", "bin", "auch", "ich", "ein", "Ding", "in", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "das kleinste unter diesen Himmeln. \u2013 Schau:", "tokens": ["das", "kleins\u00b7te", "un\u00b7ter", "die\u00b7sen", "Him\u00b7meln", ".", "\u2013", "Schau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PDAT", "NN", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ist das Ein Himmel?:", "tokens": ["Ist", "das", "Ein", "Him\u00b7mel", "?", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$.", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Selig lichtes Blau,", "tokens": ["Se\u00b7lig", "lich\u00b7tes", "Blau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "in das sich immer reinere Wolken dr\u00e4ngen,", "tokens": ["in", "das", "sich", "im\u00b7mer", "rei\u00b7ne\u00b7re", "Wol\u00b7ken", "dr\u00e4n\u00b7gen", ","], "token_info": ["word",