{"dta.poem.3305": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Freude.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ergetzt euch, Freunde, weil ihr k\u00f6nnt!", "tokens": ["Er\u00b7getzt", "euch", ",", "Freun\u00b7de", ",", "weil", "ihr", "k\u00f6nnt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Sterblichen ist nicht verg\u00f6nnt,", "tokens": ["Den", "Sterb\u00b7li\u00b7chen", "ist", "nicht", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Leiden immer frey zu bleiben.", "tokens": ["Von", "Lei\u00b7den", "im\u00b7mer", "frey", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vernunft wird \u00f6fters ohne Frucht", "tokens": ["Ver\u00b7nunft", "wird", "\u00f6f\u00b7ters", "oh\u00b7ne", "Frucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich wider schwarzen Unmuth str\u00e4uben:", "tokens": ["Sich", "wi\u00b7der", "schwar\u00b7zen", "Un\u00b7muth", "str\u00e4u\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ly\u00e4us weis ihn zu bet\u00e4uben,", "tokens": ["Ly\u00b7\u00e4us", "weis", "ihn", "zu", "be\u00b7t\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und singt ihn sieghaft in die Flucht.", "tokens": ["Und", "singt", "ihn", "sieg\u00b7haft", "in", "die", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lernt, wie sich finstrer Unverstand,", "tokens": ["Lernt", ",", "wie", "sich", "finst\u00b7rer", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verh\u00fcllt in trauriges Gewand,", "tokens": ["Ver\u00b7h\u00fcllt", "in", "trau\u00b7ri\u00b7ges", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von wahrer Weisheit unterscheide,", "tokens": ["Von", "wah\u00b7rer", "Weis\u00b7heit", "un\u00b7ter\u00b7schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mit entw\u00f6lkter Stirne gl\u00e4nzt,", "tokens": ["Die", "mit", "ent\u00b7w\u00f6lk\u00b7ter", "Stir\u00b7ne", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in der Wollust leichtem Kleide,", "tokens": ["Und", "in", "der", "Wol\u00b7lust", "leich\u00b7tem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sie, im Schoose sanfter Freude,", "tokens": ["Wie", "sie", ",", "im", "Schoo\u00b7se", "sanf\u00b7ter", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch oft mit Rosen sich bekr\u00e4nzt.", "tokens": ["Auch", "oft", "mit", "Ro\u00b7sen", "sich", "be\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O segnet ieden Augenblick,", "tokens": ["O", "seg\u00b7net", "ie\u00b7den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ihr ein unverg\u00e4lltes Gl\u00fcck", "tokens": ["Da", "ihr", "ein", "un\u00b7ver\u00b7g\u00e4ll\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In s\u00fcsser Freundschaft Armen schmecket:", "tokens": ["In", "s\u00fcs\u00b7ser", "Freund\u00b7schaft", "Ar\u00b7men", "schme\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da Bacchus euch mit Ephe\u00fc kr\u00f6nt,", "tokens": ["Da", "Bac\u00b7chus", "euch", "mit", "E\u00b7phe\u00fc", "kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Witz und attisch lachen wecket;", "tokens": ["Und", "Witz", "und", "at\u00b7tisch", "la\u00b7chen", "we\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und muntrer Scherz, der Narren schrecket,", "tokens": ["Und", "mun\u00b7trer", "Scherz", ",", "der", "Nar\u00b7ren", "schre\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Narren und ihr Gl\u00fcck verh\u00f6hnt.", "tokens": ["Die", "Nar\u00b7ren", "und", "ihr", "Gl\u00fcck", "ver\u00b7h\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch h\u00f6rt ihr, was die Wahrheit spricht?", "tokens": ["Doch", "h\u00f6rt", "ihr", ",", "was", "die", "Wahr\u00b7heit", "spricht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verw\u00f6hnt, verw\u00f6hnt die Seele nicht", "tokens": ["Ver\u00b7w\u00f6hnt", ",", "ver\u00b7w\u00f6hnt", "die", "See\u00b7le", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu rauschenden Erg\u00f6tzlichkeiten,", "tokens": ["Zu", "rau\u00b7schen\u00b7den", "Er\u00b7g\u00f6tz\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die, wann der Geist sie lieb gewinnt,", "tokens": ["Die", ",", "wann", "der", "Geist", "sie", "lieb", "ge\u00b7winnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Rosen unter D\u00f6rner leiten;", "tokens": ["Von", "Ro\u00b7sen", "un\u00b7ter", "D\u00f6r\u00b7ner", "lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kein Vergn\u00fcgen aller Zeiten,", "tokens": ["Und", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "al\u00b7ler", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur Augenblicke reizend sind.", "tokens": ["Nur", "Au\u00b7gen\u00b7bli\u00b7cke", "rei\u00b7zend", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Weisheit richtet meinen Sinn", "tokens": ["Die", "Weis\u00b7heit", "rich\u00b7tet", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dauerndes Vergn\u00fcgen hin,", "tokens": ["Auf", "dau\u00b7ern\u00b7des", "Ver\u00b7gn\u00fc\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das aus der Seele selbst entspringet.", "tokens": ["Das", "aus", "der", "See\u00b7le", "selbst", "ent\u00b7sprin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschmack und Wahrheit! ihr entz\u00fcckt,", "tokens": ["Ge\u00b7schmack", "und", "Wahr\u00b7heit", "!", "ihr", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch wann kein Saitenspiel erklinget:", "tokens": ["Auch", "wann", "kein", "Sai\u00b7ten\u00b7spiel", "er\u00b7klin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch wann mein Mund nicht lacht und singet,", "tokens": ["Auch", "wann", "mein", "Mund", "nicht", "lacht", "und", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bin ich in euerm Arm begl\u00fcckt.", "tokens": ["Bin", "ich", "in", "eu\u00b7erm", "Arm", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Anmuth pr\u00e4chtiger Natur", "tokens": ["Die", "An\u00b7muth", "pr\u00e4ch\u00b7ti\u00b7ger", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergn\u00fcgt mich auf bebl\u00fchmter Flur,", "tokens": ["Ver\u00b7gn\u00fcgt", "mich", "auf", "be\u00b7bl\u00fchm\u00b7ter", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf H\u00fcgeln und im dunkeln Hayne.", "tokens": ["Auf", "H\u00fc\u00b7geln", "und", "im", "dun\u00b7keln", "Hay\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich jauchz\u2019 an stiller Musen Brust", "tokens": ["Ich", "jauch\u00b7z'", "an", "stil\u00b7ler", "Mu\u00b7sen", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So fr\u00f6hlig, als bey Cyperns Weine:", "tokens": ["So", "fr\u00f6h\u00b7lig", ",", "als", "bey", "Cy\u00b7perns", "Wei\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja wenn ich Thoren einsam scheine,", "tokens": ["Ja", "wenn", "ich", "Tho\u00b7ren", "ein\u00b7sam", "schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vertraut sich mir die reinste Lust.", "tokens": ["Ver\u00b7traut", "sich", "mir", "die", "reins\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So lockend jene Freude lacht,", "tokens": ["So", "lo\u00b7ckend", "je\u00b7ne", "Freu\u00b7de", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nur die Sinne trunken macht,", "tokens": ["Die", "nur", "die", "Sin\u00b7ne", "trun\u00b7ken", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So nah ist sie dem Ueberdrusse.", "tokens": ["So", "nah", "ist", "sie", "dem", "Ue\u00b7berd\u00b7rus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wollust, vom Geschmack ern\u00e4hrt,", "tokens": ["Die", "Wol\u00b7lust", ",", "vom", "Ge\u00b7schmack", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stirbt unter dummem Ueberflusse:", "tokens": ["Stirbt", "un\u00b7ter", "dum\u00b7mem", "Ue\u00b7ber\u00b7flus\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie bleibt bey sparsamem Genusse", "tokens": ["Sie", "bleibt", "bey", "spar\u00b7sa\u00b7mem", "Ge\u00b7nus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weit l\u00e4nger sch\u00f6n und liebenswerth.", "tokens": ["Weit", "l\u00e4n\u00b7ger", "sch\u00f6n", "und", "lie\u00b7bens\u00b7werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du Tochter wilder Trunkenheit!", "tokens": ["Du", "Toch\u00b7ter", "wil\u00b7der", "Trun\u00b7ken\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fleuch, ungestalte Fr\u00f6hligkeit,", "tokens": ["Fleuch", ",", "un\u00b7ge\u00b7stal\u00b7te", "Fr\u00f6h\u00b7lig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rase nur bey bl\u00f6den Reichen!", "tokens": ["Und", "ra\u00b7se", "nur", "bey", "bl\u00f6\u00b7den", "Rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie m\u00f6gen durch entweihten Wein", "tokens": ["Sie", "m\u00f6\u00b7gen", "durch", "ent\u00b7weih\u00b7ten", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sanften Grazien verscheuchen!", "tokens": ["Die", "sanf\u00b7ten", "Gra\u00b7zi\u00b7en", "ver\u00b7scheu\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie, Bacchus! m\u00f6gen Thieren gleichen:", "tokens": ["Sie", ",", "Bac\u00b7chus", "!", "m\u00f6\u00b7gen", "Thie\u00b7ren", "glei\u00b7chen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$.", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Uns Freunde! lass\u2019 er Menschen seyn.", "tokens": ["Uns", "Freun\u00b7de", "!", "lass'", "er", "Men\u00b7schen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "VVFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}