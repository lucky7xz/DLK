{"dta.poem.14269": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Fl\u00fcgelro\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aurora will sich heben,", "tokens": ["Au\u00b7ro\u00b7ra", "will", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du schl\u00e4gst die Augen auf,", "tokens": ["Du", "schl\u00e4gst", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wonniges Erbeben,", "tokens": ["O", "won\u00b7ni\u00b7ges", "Er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O sch\u00f6ner Lebenslauf! \u2014", "tokens": ["O", "sch\u00f6\u00b7ner", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}