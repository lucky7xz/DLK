{"dta.poem.111": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber das Bildni\u00df einer Sch\u00f6-  \n nen in der  Tabaquier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Bey diesem Wunder-Bild da fehlet nur der Geist/", "tokens": ["Bey", "die\u00b7sem", "Wun\u00b7der\u00b7Bild", "da", "feh\u00b7let", "nur", "der", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht pr\u00e4chtige Gestalt/ und das was Sch\u00f6nheit heist/", "tokens": ["Nicht", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7stalt", "/", "und", "das", "was", "Sch\u00f6n\u00b7heit", "heist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$(", "KON", "PDS", "PRELS", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das milcherne Gesicht/ die Seiden-gleiche Wangen", "tokens": ["Das", "mil\u00b7cher\u00b7ne", "Ge\u00b7sicht", "/", "die", "Sei\u00b7den\u00b7glei\u00b7che", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieht man voll Lieblichkeit im stoltzen Schmucke prangen.", "tokens": ["Sieht", "man", "voll", "Lieb\u00b7lich\u00b7keit", "im", "stolt\u00b7zen", "Schmu\u00b7cke", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Augen sind ein Pech gesetzet in dem Schnee/", "tokens": ["Die", "Au\u00b7gen", "sind", "ein", "Pech", "ge\u00b7set\u00b7zet", "in", "dem", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Busens sch\u00f6ne Frucht die quillet in die H\u00f6h;", "tokens": ["Des", "Bu\u00b7sens", "sch\u00f6\u00b7ne", "Frucht", "die", "quil\u00b7let", "in", "die", "H\u00f6h", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch g\u00f6nnet uns die Hand nur halb die sch\u00f6nen Fr\u00fcchte/", "tokens": ["Doch", "g\u00f6n\u00b7net", "uns", "die", "Hand", "nur", "halb", "die", "sch\u00f6\u00b7nen", "Fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zieht das Wunder-Land uns meist aus dem Gesichte/", "tokens": ["Und", "zieht", "das", "Wun\u00b7der\u00b7Land", "uns", "meist", "aus", "dem", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Marmor \u00fcbersteigt/ und Hertzen brennen macht", "tokens": ["Das", "Mar\u00b7mor", "\u00fc\u00b7bers\u00b7teigt", "/", "und", "Hert\u00b7zen", "bren\u00b7nen", "macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$(", "KON", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn offtmahls hat ein Bild ein grosses Feur gebracht.", "tokens": ["Denn", "offt\u00b7mahls", "hat", "ein", "Bild", "ein", "gros\u00b7ses", "Feur", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kan dieses die ", "tokens": ["Kan", "die\u00b7ses", "die"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PDAT", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.12": {"text": "So wird das rechte Bild bald unser Bande lachen.", "tokens": ["So", "wird", "das", "rech\u00b7te", "Bild", "bald", "un\u00b7ser", "Ban\u00b7de", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Augen werden seyn zwey Lichter voller Feur/", "tokens": ["Die", "Au\u00b7gen", "wer\u00b7den", "seyn", "zwey", "Lich\u00b7ter", "vol\u00b7ler", "Feur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "CARD", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Flammen-reicher Ort/ ein blitzend Ungehenr;", "tokens": ["Ein", "Flam\u00b7men\u00b7rei\u00b7cher", "Ort", "/", "ein", "blit\u00b7zend", "Un\u00b7ge\u00b7henr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Br\u00fcste werden an den Feuer-Bergen siegen/", "tokens": ["Die", "Br\u00fcs\u00b7te", "wer\u00b7den", "an", "den", "Feu\u00b7er\u00b7Ber\u00b7gen", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Worinn ", "tokens": ["Wo\u00b7rinn"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Und fehlt hier schon der Geist/ heist es ein todtes Bild", "tokens": ["Und", "fehlt", "hier", "schon", "der", "Geist", "/", "heist", "es", "ein", "tod\u00b7tes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$(", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So ist doch diese Pracht in kein Gedicht verh\u00fcllt:", "tokens": ["So", "ist", "doch", "die\u00b7se", "Pracht", "in", "kein", "Ge\u00b7dicht", "ver\u00b7h\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn das ", "tokens": ["Denn", "das"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Und einer sch\u00f6nen Stadt ist es znm Schmuck gegeben.", "tokens": ["Und", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Stadt", "ist", "es", "znm", "Schmuck", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kan man schon ihre Pracht und Sch\u00f6nheit nicht besehn/", "tokens": ["Kan", "man", "schon", "ih\u00b7re", "Pracht", "und", "Sch\u00f6n\u00b7heit", "nicht", "be\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dieweil es Gl\u00fcck und Zeit allhter nicht l\u00e4st geschehn/", "tokens": ["Die\u00b7weil", "es", "Gl\u00fcck", "und", "Zeit", "allh\u00b7ter", "nicht", "l\u00e4st", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "ADV", "PTKNEG", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So will ich doch dis Bild mit h\u00f6chster Demuth gr\u00fcssen", "tokens": ["So", "will", "ich", "doch", "dis", "Bild", "mit", "h\u00f6chs\u00b7ter", "De\u00b7muth", "gr\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PDS", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wie ein Heiligthum in dem Beh\u00e4ltni\u00df k\u00fcssen.", "tokens": ["Und", "wie", "ein", "Hei\u00b7lig\u00b7thum", "in", "dem", "Be\u00b7h\u00e4lt\u00b7ni\u00df", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}