{"textgrid.poem.43332": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "Vierter Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und doch \u2013 leichtgl\u00e4ubiges Gef\u00fchl! \u2013", "tokens": ["Und", "doch", "\u2013", "leicht\u00b7gl\u00e4u\u00b7bi\u00b7ges", "Ge\u00b7f\u00fchl", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "ADJA", "NN", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ist alles die\u00df mehr als ein Gaukelspiel?", "tokens": ["Ist", "al\u00b7les", "die\u00df", "mehr", "als", "ein", "Gau\u00b7kel\u00b7spiel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PDS", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann die\u00df die St\u00e4tte seyn, wo wir", "tokens": ["Kann", "die\u00df", "die", "St\u00e4t\u00b7te", "seyn", ",", "wo", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PDS", "ART", "NN", "VAINF", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Thal des Schweigens flohn? Kaum glaub ich dir!", "tokens": ["Ins", "Thal", "des", "Schwei\u00b7gens", "flohn", "?", "Kaum", "glaub", "ich", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie reizend, wie bezaubernd lacht", "tokens": ["Wie", "rei\u00b7zend", ",", "wie", "be\u00b7zau\u00b7bernd", "lacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "PWAV", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die heitre Gegend! wie voll sanfter Pracht!", "tokens": ["Die", "heit\u00b7re", "Ge\u00b7gend", "!", "wie", "voll", "sanf\u00b7ter", "Pracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWAV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In sch\u00f6nrer Majest\u00e4t, in reiferm Strahle", "tokens": ["In", "sch\u00f6n\u00b7rer", "Ma\u00b7jes\u00b7t\u00e4t", ",", "in", "rei\u00b7ferm", "Strah\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gl\u00e4nzt diese Sonne! Milder flie\u00dft vom Thale", "tokens": ["Gl\u00e4nzt", "die\u00b7se", "Son\u00b7ne", "!", "Mil\u00b7der", "flie\u00dft", "vom", "Tha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "$.", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mir fremder Bl\u00fcthen Fr\u00fchlings-Duft;", "tokens": ["Mir", "frem\u00b7der", "Bl\u00fc\u00b7then", "Fr\u00fch\u00b7lings\u00b7Duft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und Balsamgeister str\u00f6hmen durch die Luft,", "tokens": ["Und", "Bal\u00b7sam\u00b7geis\u00b7ter", "str\u00f6h\u00b7men", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Un\u00fcbersehlich malt die Blumen-Flur", "tokens": ["Un\u00b7\u00fc\u00b7ber\u00b7seh\u00b7lich", "malt", "die", "Blu\u00b7men\u00b7Flur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sich meinem Aug, und die Natur", "tokens": ["Sich", "mei\u00b7nem", "Aug", ",", "und", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ist rings umher ein Garten! \u2013 Welcher Gott", "tokens": ["Ist", "rings", "um\u00b7her", "ein", "Gar\u00b7ten", "!", "\u2013", "Wel\u00b7cher", "Gott"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "PTKVZ", "ART", "NN", "$.", "$(", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Schmiegt eine Wildni\u00df unter das Gebot", "tokens": ["Schmiegt", "ei\u00b7ne", "Wild\u00b7ni\u00df", "un\u00b7ter", "das", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Sch\u00f6nheit, Ordnung, Fruchtbarkeit?", "tokens": ["Der", "Sch\u00f6n\u00b7heit", ",", "Ord\u00b7nung", ",", "Frucht\u00b7bar\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wer ists, der W\u00fcsteneyn gebeut,", "tokens": ["Wer", "ists", ",", "der", "W\u00fcs\u00b7te\u00b7neyn", "ge\u00b7beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sich in entfernter Sonnen Glut zu tauchen,", "tokens": ["Sich", "in", "ent\u00b7fern\u00b7ter", "Son\u00b7nen", "Glut", "zu", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und unbekannte Spezereyn zu hauchen? \u2013", "tokens": ["Und", "un\u00b7be\u00b7kann\u00b7te", "Spe\u00b7ze\u00b7reyn", "zu", "hau\u00b7chen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ha! nicht also, im festlichen Gewand,", "tokens": ["Ha", "!", "nicht", "al\u00b7so", ",", "im", "fest\u00b7li\u00b7chen", "Ge\u00b7wand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PTKNEG", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Gr\u00fc\u00dft ich dich einst, mein m\u00fctterliches Land!", "tokens": ["Gr\u00fc\u00dft", "ich", "dich", "einst", ",", "mein", "m\u00fct\u00b7ter\u00b7li\u00b7ches", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.21": {"text": "Unfreundlich, ungeschm\u00fcckt, und rauh und w\u00fcste,", "tokens": ["Un\u00b7freund\u00b7lich", ",", "un\u00b7ge\u00b7schm\u00fcckt", ",", "und", "rauh", "und", "w\u00fcs\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Im tr\u00fcben Dunkel schauerte die K\u00fcste;", "tokens": ["Im", "tr\u00fc\u00b7ben", "Dun\u00b7kel", "schau\u00b7er\u00b7te", "die", "K\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Kein Himmel leuchtete mild durch den Hayn;", "tokens": ["Kein", "Him\u00b7mel", "leuch\u00b7te\u00b7te", "mild", "durch", "den", "Hayn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Kein Tag der Aehren lud zu Freuden ein;", "tokens": ["Kein", "Tag", "der", "A\u00b7eh\u00b7ren", "lud", "zu", "Freu\u00b7den", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "In H\u00f6len lauschte Graun und Meuterey,", "tokens": ["In", "H\u00f6\u00b7len", "lauschte", "Graun", "und", "Meu\u00b7te\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Und was am Ufer scholl, war Kriegsgeschrey.", "tokens": ["Und", "was", "am", "U\u00b7fer", "scholl", ",", "war", "Kriegs\u00b7ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "ADJD", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Das Weib der Ehe trat mit Helm und Speer,", "tokens": ["Das", "Weib", "der", "E\u00b7he", "trat", "mit", "Helm", "und", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und neben ihr, von blutger R\u00fcstung schwer,", "tokens": ["Und", "ne\u00b7ben", "ihr", ",", "von", "blut\u00b7ger", "R\u00fcs\u00b7tung", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Die bl\u00fchnde Tochter f\u00fcrchterlich einher \u2013", "tokens": ["Die", "bl\u00fchn\u00b7de", "Toch\u00b7ter", "f\u00fcrch\u00b7ter\u00b7lich", "ein\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "O wie weit anmuthsvoller schreitet,", "tokens": ["O", "wie", "weit", "an\u00b7muths\u00b7vol\u00b7ler", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Von acht geliebten Kindern hold begleitet,", "tokens": ["Von", "acht", "ge\u00b7lieb\u00b7ten", "Kin\u00b7dern", "hold", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dort jene Mutter durch den Schattengang,", "tokens": ["Dort", "je\u00b7ne", "Mut\u00b7ter", "durch", "den", "Schat\u00b7ten\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "In dessen Hecken friedlicher Gesang", "tokens": ["In", "des\u00b7sen", "He\u00b7cken", "fried\u00b7li\u00b7cher", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ert\u00f6nt, wo goldnes Obst um sie entsprang!", "tokens": ["Er\u00b7t\u00f6nt", ",", "wo", "gold\u00b7nes", "Obst", "um", "sie", "ent\u00b7sprang", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Auf Rasen hingelehnt, im Auge Himmel,", "tokens": ["Auf", "Ra\u00b7sen", "hin\u00b7ge\u00b7lehnt", ",", "im", "Au\u00b7ge", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Erwartet das weithallende Gewimmel", "tokens": ["Er\u00b7war\u00b7tet", "das", "weit\u00b7hal\u00b7len\u00b7de", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Der frohe Vater, der mit reger Hand", "tokens": ["Der", "fro\u00b7he", "Va\u00b7ter", ",", "der", "mit", "re\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "In die veredelte Natur entbrannt,", "tokens": ["In", "die", "ver\u00b7e\u00b7del\u00b7te", "Na\u00b7tur", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Die m\u00e4chtge Feuerharfe schl\u00e4gt,", "tokens": ["Die", "m\u00e4cht\u00b7ge", "Feu\u00b7er\u00b7har\u00b7fe", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Da\u00df ihren Schall der H\u00fcgel und das Meer", "tokens": ["Da\u00df", "ih\u00b7ren", "Schall", "der", "H\u00fc\u00b7gel", "und", "das", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Und n\u00e4her wallender Wolken Heer", "tokens": ["Und", "n\u00e4\u00b7her", "wal\u00b7len\u00b7der", "Wol\u00b7ken", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Empor zum Tanz der Sph\u00e4ren tr\u00e4gt!", "tokens": ["Em\u00b7por", "zum", "Tanz", "der", "Sph\u00e4\u00b7ren", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Da\u00df sie den Staub der Urn erregt,", "tokens": ["Da\u00df", "sie", "den", "Staub", "der", "Urn", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Und Geister-Welten um sich her bewegt!", "tokens": ["Und", "Geis\u00b7ter\u00b7Wel\u00b7ten", "um", "sich", "her", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PRF", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Auch mich! auch mich! \u2013 \u00bb", "tokens": ["Auch", "mich", "!", "auch", "mich", "!", "\u2013", "\u00bb"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "PPER", "$.", "ADV", "PPER", "$.", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Wer ist der Gott, den deine Saite singt?", "tokens": ["Wer", "ist", "der", "Gott", ",", "den", "dei\u00b7ne", "Sai\u00b7te", "singt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Wer, dessen Schaur mich Bebenden durchdringt!", "tokens": ["Wer", ",", "des\u00b7sen", "Schaur", "mich", "Be\u00b7ben\u00b7den", "durch\u00b7dringt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PRELAT", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.48": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.2": {"line.1": {"text": "Und doch \u2013 leichtgl\u00e4ubiges Gef\u00fchl! \u2013", "tokens": ["Und", "doch", "\u2013", "leicht\u00b7gl\u00e4u\u00b7bi\u00b7ges", "Ge\u00b7f\u00fchl", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "ADJA", "NN", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ist alles die\u00df mehr als ein Gaukelspiel?", "tokens": ["Ist", "al\u00b7les", "die\u00df", "mehr", "als", "ein", "Gau\u00b7kel\u00b7spiel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PDS", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann di