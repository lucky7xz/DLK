{"textgrid.poem.60179": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Freund! die l\u00e4chelnde Rose", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freund! die l\u00e4chelnde Rose", "tokens": ["O", "Freund", "!", "die", "l\u00e4\u00b7cheln\u00b7de", "Ro\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weit aufgeschlossen \u2013 \u2013 sie stirbt", "tokens": ["Weit", "auf\u00b7ge\u00b7schlos\u00b7sen", "\u2013", "\u2013", "sie", "stirbt"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$(", "$(", "PPER", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und streut die welkenden Bl\u00e4tter", "tokens": ["Und", "streut", "die", "wel\u00b7ken\u00b7den", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hin auf ihr m\u00fctterlich Land.", "tokens": ["Hin", "auf", "ihr", "m\u00fct\u00b7ter\u00b7lich", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Mit krummen rauschendem Hiebe", "tokens": ["Mit", "krum\u00b7men", "rau\u00b7schen\u00b7dem", "Hie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00e4hrt jetzt die Sense daher.", "tokens": ["F\u00e4hrt", "jetzt", "die", "Sen\u00b7se", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "So m\u00e4ht im schrecklichen Schlachtfeld", "tokens": ["So", "m\u00e4ht", "im", "schreck\u00b7li\u00b7chen", "Schlacht\u00b7feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die muthigen Krieger, der Tod!", "tokens": ["Die", "mut\u00b7hi\u00b7gen", "Krie\u00b7ger", ",", "der", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Dort liegt in sengender Sonne,", "tokens": ["Dort", "liegt", "in", "sen\u00b7gen\u00b7der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Stolz des bl\u00fchenden Thals,", "tokens": ["Der", "Stolz", "des", "bl\u00fc\u00b7hen\u00b7den", "Thals", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf der sich Bienen verweilten:", "tokens": ["Auf", "der", "sich", "Bie\u00b7nen", "ver\u00b7weil\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sie tritt im Winter der Stier", "tokens": ["Sie", "tritt", "im", "Win\u00b7ter", "der", "Stier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Zu satt, mit stampfendem Fusse", "tokens": ["Zu", "satt", ",", "mit", "stamp\u00b7fen\u00b7dem", "Fus\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In ein ver\u00e4chtliches Grab.", "tokens": ["In", "ein", "ver\u00b7\u00e4cht\u00b7li\u00b7ches", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So liegt bey fressenden W\u00fcrmern", "tokens": ["So", "liegt", "bey", "fres\u00b7sen\u00b7den", "W\u00fcr\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der angebetete Reiz", "tokens": ["Der", "an\u00b7ge\u00b7be\u00b7te\u00b7te", "Reiz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Von irgend einer, die himmlisch", "tokens": ["Von", "ir\u00b7gend", "ei\u00b7ner", ",", "die", "himm\u00b7lisch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "ART", "$,", "PRELS", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gieng unter den Menschen umher.", "tokens": ["Gieng", "un\u00b7ter", "den", "Men\u00b7schen", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie starb; der grauen Verwesung", "tokens": ["Sie", "starb", ";", "der", "grau\u00b7en", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu theurer, k\u00f6stlicher Raub!", "tokens": ["Zu", "theu\u00b7rer", ",", "k\u00f6st\u00b7li\u00b7cher", "Raub", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Apollens singende S\u00f6hne,", "tokens": ["A\u00b7pol\u00b7lens", "sin\u00b7gen\u00b7de", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und Mavors drohendes Volk,", "tokens": ["Und", "Ma\u00b7vors", "dro\u00b7hen\u00b7des", "Volk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Sterne z\u00e4hlende Weisen,", "tokens": ["Die", "Ster\u00b7ne", "z\u00e4h\u00b7len\u00b7de", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie alle m\u00fcssen dahin.", "tokens": ["Sie", "al\u00b7le", "m\u00fcs\u00b7sen", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Bekannt mit allen Olympern", "tokens": ["Be\u00b7kannt", "mit", "al\u00b7len", "O\u00b7lym\u00b7pern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Gieng in die ewige Nacht", "tokens": ["Gieng", "in", "die", "e\u00b7wi\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der tagentbehrende Milton,", "tokens": ["Der", "ta\u00b7gent\u00b7beh\u00b7ren\u00b7de", "Mil\u00b7ton", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und lie\u00df uns seinen Gesang.", "tokens": ["Und", "lie\u00df", "uns", "sei\u00b7nen", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Hoch auf die Sitze der F\u00fcrsten", "tokens": ["Hoch", "auf", "die", "Sit\u00b7ze", "der", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Greift der langarmige Tod.", "tokens": ["Greift", "der", "lang\u00b7ar\u00b7mi\u00b7ge", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Dem K\u00f6nig nahm er den Bruder", "tokens": ["Dem", "K\u00f6\u00b7nig", "nahm", "er", "den", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und dir entri\u00df er den Freund.", "tokens": ["Und", "dir", "ent\u00b7ri\u00df", "er", "den", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Er rei\u00dft vom Herzen des Lieblings", "tokens": ["Er", "rei\u00dft", "vom", "Her\u00b7zen", "des", "Lieb\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den besten Menschen mit sich!", "tokens": ["Den", "bes\u00b7ten", "Men\u00b7schen", "mit", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleim seufzet mitten im Gastmahl:", "tokens": ["Gleim", "seuf\u00b7zet", "mit\u00b7ten", "im", "Gast\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbauch also ri\u00df mir der Tod", "tokens": ["\u00bb", "auch", "al\u00b7so", "ri\u00df", "mir", "der", "Tod"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Vom Innersten meiner Empfindung", "tokens": ["Vom", "In\u00b7ners\u00b7ten", "mei\u00b7ner", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einst meinen g\u00f6ttlichen Kleist!\u00ab", "tokens": ["Einst", "mei\u00b7nen", "g\u00f6tt\u00b7li\u00b7chen", "Kleist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O Sulzer! nenn ihn nicht dreymal,", "tokens": ["O", "Sul\u00b7zer", "!", "nenn", "ihn", "nicht", "drey\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Herz verblutet sich sonst.", "tokens": ["Sein", "Herz", "ver\u00b7blu\u00b7tet", "sich", "sonst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Nicht sch\u00fctzt die kniende Andacht", "tokens": ["Nicht", "sch\u00fctzt", "die", "kni\u00b7en\u00b7de", "An\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und nicht der Fr\u00f6mmigkeit Schild.", "tokens": ["Und", "nicht", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nichts fragt die schneidende Parce", "tokens": ["Nichts", "fragt", "die", "schnei\u00b7den\u00b7de", "Par\u00b7ce"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach Tugend oder Verdienst.", "tokens": ["Nach", "Tu\u00b7gend", "o\u00b7der", "Ver\u00b7dienst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Der Tod mit strengen Befehlen", "tokens": ["Der", "Tod", "mit", "stren\u00b7gen", "Be\u00b7feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "K\u00f6mmt schnell und \u00fcbet sie aus.", "tokens": ["K\u00f6mmt", "schnell", "und", "\u00fc\u00b7bet", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Von dem gesammleten Golde", "tokens": ["Von", "dem", "ge\u00b7samm\u00b7le\u00b7ten", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Folgt ihn der Wuchrer und klagt.", "tokens": ["Folgt", "ihn", "der", "Wuch\u00b7rer", "und", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Die Sterblichen f\u00fcrchten ihn alle.", "tokens": ["Die", "Sterb\u00b7li\u00b7chen", "f\u00fcrch\u00b7ten", "ihn", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihn flieht der keuchende Grei\u00df,", "tokens": ["Ihn", "flieht", "der", "keu\u00b7chen\u00b7de", "Grei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Alt und nicht weiser geworden \u2013", "tokens": ["Alt", "und", "nicht", "wei\u00b7ser", "ge\u00b7wor\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PTKNEG", "ADJD", "VAPP", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ihn scheut an Ketten der Sclav!", "tokens": ["Ihn", "scheut", "an", "Ket\u00b7ten", "der", "Sclav", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Doch wenn sie alle ihn f\u00fcrchten,", "tokens": ["Doch", "wenn", "sie", "al\u00b7le", "ihn", "f\u00fcrch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lachst du dem bl\u00f6ckenden Zahn", "tokens": ["Lachst", "du", "dem", "bl\u00f6\u00b7cken\u00b7den", "Zahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit gr\u00f6sserem Stolze entgegen,", "tokens": ["Mit", "gr\u00f6s\u00b7se\u00b7rem", "Stol\u00b7ze", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als, mit dem Becher voll Gift,", "tokens": ["Als", ",", "mit", "dem", "Be\u00b7cher", "voll", "Gift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.15": {"line.1": {"text": "Der freudenhoffende Heyde,", "tokens": ["Der", "freu\u00b7den\u00b7hof\u00b7fen\u00b7de", "Hey\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Angesichte des Volks,", "tokens": ["Im", "An\u00b7ge\u00b7sich\u00b7te", "des", "Volks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das seine Tugend verkannte.", "tokens": ["Das", "sei\u00b7ne", "Tu\u00b7gend", "ver\u00b7kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "