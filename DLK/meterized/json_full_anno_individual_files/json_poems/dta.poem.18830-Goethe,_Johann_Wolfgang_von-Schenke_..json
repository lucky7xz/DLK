{"dta.poem.18830": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Schenke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Nennen dich den grossen Dichter,", "tokens": ["Nen\u00b7nen", "dich", "den", "gros\u00b7sen", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dich auf dem Markte zeigest;", "tokens": ["Wenn", "dich", "auf", "dem", "Mark\u00b7te", "zei\u00b7gest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gerne h\u00f6r\u2019 ich wenn du singest", "tokens": ["Ger\u00b7ne", "h\u00f6r'", "ich", "wenn", "du", "sin\u00b7gest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich horche wenn du schweigest.", "tokens": ["Und", "ich", "hor\u00b7che", "wenn", "du", "schwei\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch ich liebe dich noch lieber,", "tokens": ["Doch", "ich", "lie\u00b7be", "dich", "noch", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du k\u00fcssest zum Erinnern;", "tokens": ["Wenn", "du", "k\u00fcs\u00b7sest", "zum", "E\u00b7rin\u00b7nern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Worte gehn vor\u00fcber", "tokens": ["Denn", "die", "Wor\u00b7te", "gehn", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Kuss der bleibt im Innern.", "tokens": ["Und", "der", "Kuss", "der", "bleibt", "im", "In\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reim auf Reim will was bedeuten,", "tokens": ["Reim", "auf", "Reim", "will", "was", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Besser ist es viel zu denken.", "tokens": ["Bes\u00b7ser", "ist", "es", "viel", "zu", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe du den andern Leuten", "tokens": ["Sin\u00b7ge", "du", "den", "an\u00b7dern", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verstumme mit dem Schenken.", "tokens": ["Und", "ver\u00b7stum\u00b7me", "mit", "dem", "Schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}