{"textgrid.poem.31652": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "24. Morgen-Gedanken", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf / auf! heb dich aus den Pflaumen: Nimm die Feder in die Hand.", "tokens": ["Auf", "/", "auf", "!", "heb", "dich", "aus", "den", "Pflau\u00b7men", ":", "Nimm", "die", "Fe\u00b7der", "in", "die", "Hand", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "PTKVZ", "$.", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "NE", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Schwing dich von der Eitelkeit / GOTT dein' Opffer-Gab zu bringen.", "tokens": ["Schwing", "dich", "von", "der", "Ei\u00b7tel\u00b7keit", "/", "GoTT", "dein'", "Opf\u00b7fer\u00b7Gab", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$(", "NE", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Gott prei\u00df-loben / ehren / danken / ist ja dein Beruff und Stand.", "tokens": ["Gott", "prei\u00df\u00b7lo\u00b7ben", "/", "eh\u00b7ren", "/", "dan\u00b7ken", "/", "ist", "ja", "dein", "Be\u00b7ruff", "und", "Stand", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "VVINF", "$(", "VVINF", "$(", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "La\u00df die lebend' Harpf / dein Herz / sch\u00f6n von Gottes Wundern klingen /", "tokens": ["La\u00df", "die", "le\u00b7bend'", "Harpf", "/", "dein", "Herz", "/", "sch\u00f6n", "von", "Got\u00b7tes", "Wun\u00b7dern", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "ADJD", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-++-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.5": {"text": "Gott / mein Sch\u00f6ffer / Heiland / Tr\u00f6ster / h\u00f6chste Drev und Einigkeit!", "tokens": ["Gott", "/", "mein", "Sch\u00f6f\u00b7fer", "/", "Hei\u00b7land", "/", "Tr\u00f6s\u00b7ter", "/", "h\u00f6chs\u00b7te", "Drev", "und", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "$(", "NN", "$(", "NN", "$(", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "sey gepreist vor alle Gnaden / mir erzeiget Lebens-Zeit.", "tokens": ["sey", "ge\u00b7preist", "vor", "al\u00b7le", "Gna\u00b7den", "/", "mir", "er\u00b7zei\u00b7get", "Le\u00b7bens\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PIAT", "NN", "$(", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "K\u00f6nt' ich allen Meeres-Sand / alle Stern / zu Zungen machen /", "tokens": ["K\u00f6nt'", "ich", "al\u00b7len", "Mee\u00b7res\u00b7Sand", "/", "al\u00b7le", "Stern", "/", "zu", "Zun\u00b7gen", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "$(", "PIAT", "NN", "$(", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "aller W\u00e4lder Haar das Laub / alle Zahl befreyte Sachen;", "tokens": ["al\u00b7ler", "W\u00e4l\u00b7der", "Haar", "das", "Laub", "/", "al\u00b7le", "Zahl", "be\u00b7frey\u00b7te", "Sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "ART", "NN", "$(", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "wann mir Mensch-und Engel h\u00fclffen: k\u00f6nt ich deines Lobes Prei\u00df", "tokens": ["wann", "mir", "Men\u00b7schund", "En\u00b7gel", "h\u00fclf\u00b7fen", ":", "k\u00f6nt", "ich", "dei\u00b7nes", "Lo\u00b7bes", "Prei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NE", "NE", "VVINF", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "nicht den ringsten Theil aussprechen. Dieses nur allein ich wei\u00df /", "tokens": ["nicht", "den", "rings\u00b7ten", "Theil", "aus\u00b7spre\u00b7chen", ".", "Die\u00b7ses", "nur", "al\u00b7lein", "ich", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVINF", "$.", "PDS", "ADV", "ADV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "da\u00df die Vnaussprechlichkeit / dessen Allgr\u00f6\u00df etwas zeiget.", "tokens": ["da\u00df", "die", "Vn\u00b7aus\u00b7sprech\u00b7lich\u00b7keit", "/", "des\u00b7sen", "All\u00b7gr\u00f6\u00df", "et\u00b7was", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "PDS", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Dieses lobt dich auf das h\u00f6chst / das in Lieb verzuckt still schweiget.", "tokens": ["Die\u00b7ses", "lobt", "dich", "auf", "das", "h\u00f6chst", "/", "das", "in", "Lieb", "ver\u00b7zuckt", "still", "schwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADV", "$(", "ART", "APPR", "NN", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf / auf! heb dich aus den Pflaumen: Nimm die Feder in die Hand.", "tokens": ["Auf", "/", "auf", "!", "heb", "dich", "aus", "den", "Pflau\u00b7men", ":", "Nimm", "die", "Fe\u00b7der", "in", "die", "Hand", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "PTKVZ", "$.", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "NE", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Schwing dich von der Eitelkeit / GOTT dein' Opffer-Gab zu bringen.", "tokens": ["Schwing", "dich", "von", "der", "Ei\u00b7tel\u00b7keit", "/", "GoTT", "dein'", "Opf\u00b7fer\u00b7Gab", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$(", "NE", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Gott prei\u00df-loben / ehren / danken / ist ja dein Beruff und Stand.", "tokens": ["Gott", "prei\u00df\u00b7lo\u00b7ben", "/", "eh\u00b7ren", "/", "dan\u00b7ken", "/", "ist", "ja", "dein", "Be\u00b7ruff", "und", "Stand", "."], "token_