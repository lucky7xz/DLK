{"textgrid.poem.48701": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. An Christianen und Sigismunden Ilgen, Gebr\u00fcder, \u00fcber Ableben ihres Vatern, Schwester und Schwagern", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du br\u00fcderliches Paar und meiner Freundschaft Zier,", "tokens": ["Du", "br\u00fc\u00b7der\u00b7li\u00b7ches", "Paar", "und", "mei\u00b7ner", "Freund\u00b7schaft", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "halt' an und sieh dich um, ob irgends aller Enden", "tokens": ["halt'", "an", "und", "sieh", "dich", "um", ",", "ob", "ir\u00b7gends", "al\u00b7ler", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "PTKVZ", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "noch was zu finden sei, das deine Qual kan wenden.", "tokens": ["noch", "was", "zu", "fin\u00b7den", "sei", ",", "das", "dei\u00b7ne", "Qual", "kan", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Halt' an und sieh dich um. - Ich sehe, klagst du mir,", "tokens": ["Halt'", "an", "und", "sieh", "dich", "um", ".", "Ich", "se\u00b7he", ",", "klagst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVIMP", "PPER", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "ich sehe nichts f\u00fcr mich. - Ja, recht! Ich gl\u00e4ub' es dir.", "tokens": ["ich", "se\u00b7he", "nichts", "f\u00fcr", "mich", ".", "Ja", ",", "recht", "!", "Ich", "gl\u00e4ub'", "es", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "$.", "$(", "PTKANT", "$,", "ADJD", "$.", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Was soll denn dieses Ach, di\u00df Ringen mit den H\u00e4nden?", "tokens": ["Was", "soll", "denn", "die\u00b7ses", "Ach", ",", "di\u00df", "Rin\u00b7gen", "mit", "den", "H\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PDAT", "NN", "$,", "PDS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ists di\u00df nun, das du siehst, das dir soll H\u00fclfe senden?", "tokens": ["Ists", "di\u00df", "nun", ",", "das", "du", "siehst", ",", "das", "dir", "soll", "H\u00fcl\u00b7fe", "sen\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O nein! Drum la\u00df es nach und nim ein Anders f\u00fcr.", "tokens": ["O", "nein", "!", "Drum", "la\u00df", "es", "nach", "und", "nim", "ein", "An\u00b7ders", "f\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PAV", "VVFIN", "PPER", "APPR", "KON", "VVIMP", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was aber? Gieb dich drein. Das Leid zwar wird gleich gro\u00df,", "tokens": ["Was", "a\u00b7ber", "?", "Gieb", "dich", "drein", ".", "Das", "Leid", "zwar", "wird", "gleich", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "VVIMP", "PPER", "PTKVZ", "$.", "ART", "NN", "ADV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "je Liebers uns was f\u00e4llt. Ich sag' auch nicht so blo\u00df,", "tokens": ["je", "Lie\u00b7bers", "uns", "was", "f\u00e4llt", ".", "Ich", "sag'", "auch", "nicht", "so", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df man um Freunde nicht geb\u00fcrlich trauren solle.", "tokens": ["da\u00df", "man", "um", "Freun\u00b7de", "nicht", "ge\u00b7b\u00fcr\u00b7lich", "trau\u00b7ren", "sol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was aber? Gieb dich drein. Entschlage dich der Angst", "tokens": ["Was", "a\u00b7ber", "?", "Gieb", "dich", "drein", ".", "Ent\u00b7schla\u00b7ge", 