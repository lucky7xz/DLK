{"textgrid.poem.33218": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2284.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlan, ich will meinen Lieben ein lied singen von meinem Anverwandten.", "tokens": ["Wo\u00b7hlan", ",", "ich", "will", "mei\u00b7nen", "Lie\u00b7ben", "ein", "lied", "sin\u00b7gen", "von", "mei\u00b7nem", "An\u00b7ver\u00b7wand\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PPOSAT", "ADJA", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+++--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein herz dichtet ein feines lied.", "tokens": ["Mein", "herz", "dich\u00b7tet", "ein", "fei\u00b7nes", "lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Wie man auf t\u00e4flein an der wand so hinsetzt mancherley verstand etc.", "tokens": ["Wie", "man", "auf", "t\u00e4\u00b7flein", "an", "der", "wand", "so", "hin\u00b7setzt", "man\u00b7cher\u00b7ley", "ver\u00b7stand", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PIS", "APPR", "NN", "APPR", "ART", "VVFIN", "ADV", "ADV", "PIS", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Doch die sacramentlichkeiten gehen viele tausend weiten \u00fcber allen text der lieder.", "tokens": ["Doch", "die", "sa\u00b7cra\u00b7ment\u00b7lich\u00b7kei\u00b7ten", "ge\u00b7hen", "vie\u00b7le", "tau\u00b7send", "wei\u00b7ten", "\u00fc\u00b7ber", "al\u00b7len", "text", "der", "lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "CARD", "ADJA", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Er liebet gerechtigkeit;", "tokens": ["Er", "lie\u00b7bet", "ge\u00b7rech\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Christi blut und gerechtigkeit etc.", "tokens": ["Chris\u00b7ti", "blut", "und", "ge\u00b7rech\u00b7tig\u00b7keit", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NE", "NN", "KON", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "darum hatt Gott", "tokens": ["da\u00b7rum", "hatt", "Gott"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VAFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Die heilige Dreyeinigkeit.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Drey\u00b7ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "den Gott dieses Chors", "tokens": ["den", "Gott", "die\u00b7ses", "Chors"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Einger j\u00fcngfr\u00e4ulicher Mann etc.", "tokens": ["Ein\u00b7ger", "j\u00fcng\u00b7fr\u00e4u\u00b7li\u00b7cher", "Mann", "et\u00b7c."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PIAT", "ADJA", "NN", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "mit dem \u00f6le der fr\u00f6lichkeit gesalbet", "tokens": ["mit", "dem", "\u00f6\u00b7le", "der", "fr\u00f6\u00b7lich\u00b7keit", "ge\u00b7sal\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Da denkt man an den breden, der Jesu haupt und fu\u00df durchtropft bis auf den boden.", "tokens": ["Da", "denkt", "man", "an", "den", "bre\u00b7den", ",", "der", "Je\u00b7su", "haupt", "und", "fu\u00df", "durch\u00b7tropft", "bis", "auf", "den", "bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "ADJA", "$,", "ART", "NE", "NN", "KON", "PTKVZ", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "f\u00fcr seine genossenschaft.", "tokens": ["f\u00fcr", "sei\u00b7ne", "ge\u00b7nos\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Komt sie seinem Leichlein nah, so wird sie Jesushaft.", "tokens": ["Komt", "sie", "sei\u00b7nem", "Leich\u00b7lein", "nah", ",", "so", "wird", "sie", "Je\u00b7sus\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.15": {"line.1": {"text": "Seine kleider riechen wie lauter myrrhen, aloes und cassia,", "tokens": ["Sei\u00b7ne", "klei\u00b7der", "rie\u00b7chen", "wie", "lau\u00b7ter", "myr\u00b7rhen", ",", "a\u00b7loes", "und", "cas\u00b7sia", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVINF", "KOKOM", "PIAT", "NN", "$,", "ADJA", "KON", "NE", "$,"], "meter": "+-+-+--+-+--+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.16": {"line.1": {"text": "So grabes-d\u00fcfte witterlich.", "tokens": ["So", "gra\u00b7bes\u00b7d\u00fcf\u00b7te", "wit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "wenn Er aus den elfenbeinern pall\u00e4sten daher trit in seiner sch\u00f6nen pracht.", "tokens": ["wenn", "Er", "aus", "den", "el\u00b7fen\u00b7bei\u00b7nern", "pal\u00b7l\u00e4s\u00b7ten", "da\u00b7her", "trit", "in", "sei\u00b7ner", "sch\u00f6\u00b7nen", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "PAV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "----+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.18": {"line.1": {"text": "In seinem schmuk gehen die k\u00f6nigs-t\u00f6chter.", "tokens": ["In", "sei\u00b7nem", "schmuk", "ge\u00b7hen", "die", "k\u00f6\u00b7nigs\u00b7t\u00f6ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.19": {"line.1": {"text": "Ist eine ", "tokens": ["Ist", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.20": {"line.1": {"text": "Die braut steht zu seiner rechten", "tokens": ["Die", "braut", "steht", "zu", "sei\u00b7ner", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "Wo du heraus gegraben bist, da dein Heiland verschieden ist.", "tokens": ["Wo", "du", "he\u00b7raus", "ge\u00b7gra\u00b7ben", "bist", ",", "da", "dein", "Hei\u00b7land", "ver\u00b7schie\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APZR", "VVPP", "VAFIN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.22": {"line.1": {"text": "Das ist ihre lust, so viel mir bewu\u00dft.", "tokens": ["Das", "ist", "ih\u00b7re", "lust", ",", "so", "viel", "mir", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.23": {"line.1": {"text": "und neige dein ohr;", "tokens": ["und", "nei\u00b7ge", "dein", "ohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.24": {"line.1": {"text": "Zu h\u00f6ren was der liebe ", "tokens": ["Zu", "h\u00f6\u00b7ren", "was", "der", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PWS", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "vergi\u00df deines volks und deines vaters hauses:", "tokens": ["ver\u00b7gi\u00df", "dei\u00b7nes", "volks", "und", "dei\u00b7nes", "va\u00b7ters", "hau\u00b7ses", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.26": {"line.1": {"text": "Da, wo des Mannes blut den boden duftig machte, vergrabet allen muth, der unlammhaftig dachte.", "tokens": ["Da", ",", "wo", "des", "Man\u00b7nes", "blut", "den", "bo\u00b7den", "duf\u00b7tig", "mach\u00b7te", ",", "ver\u00b7gra\u00b7bet", "al\u00b7len", "muth", ",", "der", "un\u00b7lamm\u00b7haf\u00b7tig", "dach\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NN", "ART", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.27": {"line.1": {"text": "so wird der K\u00f6nig lust an deiner sch\u00f6ne haben.", "tokens": ["so", "wird", "der", "K\u00f6\u00b7nig", "lust", "an", "dei\u00b7ner", "sch\u00f6\u00b7ne", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.28": {"line.1": {"text": "Der matte blik des Herrn vom haus, sieht dir zu deinen augen raus.", "tokens": ["Der", "mat\u00b7te", "blik", "des", "Herrn", "vom", "haus", ",", "sieht", "dir", "zu", "dei\u00b7nen", "au\u00b7gen", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.29": {"line.1": {"text": "Die tochter Zor wird mit geschenken da seyn.", "tokens": ["Die", "toch\u00b7ter", "Zor", "wird", "mit", "ge\u00b7schen\u00b7ken", "da", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "VVPP", "ADV", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.30": {"line.1": {"text": "Denn in was schn\u00f6den orden sie auferzogen worden, du sahst doch auf die s\u00fcnderin.", "tokens": ["Denn", "in", "was", "schn\u00f6\u00b7den", "or\u00b7den", "sie", "auf\u00b7er\u00b7zo\u00b7gen", "wor\u00b7den", ",", "du", "sahst", "doch", "auf", "die", "s\u00fcn\u00b7de\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ADJA", "NN", "PPER", "VVPP", "VAPP", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.31": {"line.1": {"text": "Selige Kyria, tochter des Abi-Ja, Ve Ruach Elohah, schwester des Jehovah.", "tokens": ["Se\u00b7li\u00b7ge", "Ky\u00b7ria", ",", "toch\u00b7ter", "des", "A\u00b7bi\u00b7\u00b7Ja", ",", "Ve", "Ruach", "E\u00b7lo\u00b7hah", ",", "schwes\u00b7ter", "des", "Je\u00b7ho\u00b7vah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "ART", "NN", "$,", "NE", "NE", "NE", "$,", "ADJD", "ART", "NN", "$."], "meter": "+--+-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}}, "stanza.32": {"line.1": {"text": "ist ganz herrlich inwendig.", "tokens": ["ist", "ganz", "herr\u00b7lich", "in\u00b7wen\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.33": {"line.1": {"text": "Ein viertel erd-schwamm von natur, drey viertel Jesushaft.", "tokens": ["Ein", "vier\u00b7tel", "erd\u00b7schwamm", "von", "na\u00b7tur", ",", "drey", "vier\u00b7tel", "Je\u00b7sus\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "CARD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.34": {"line.1": {"text": "Ich gl\u00e4ube einen Reihen von Gott des Vaters und des Geists ehrw\u00fcrdigen geschweyen.", "tokens": ["Ich", "gl\u00e4u\u00b7be", "ei\u00b7nen", "Rei\u00b7hen", "von", "Gott", "des", "Va\u00b7ters", "und", "des", "Geists", "ehr\u00b7w\u00fcr\u00b7di\u00b7gen", "ge\u00b7schwe\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ART", "NN", "KON", "ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.35": {"line.1": {"text": "Das volk, das Jesu marter treibt, wird nach und nach dir einverleibt.", "tokens": ["Das", "volk", ",", "das", "Je\u00b7su", "mar\u00b7ter", "treibt", ",", "wird", "nach", "und", "nach", "dir", "ein\u00b7ver\u00b7leibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,", "VAFIN", "APPR", "KON", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.36": {"line.1": {"text": "die wirst du zu f\u00fcrsten setzen", "tokens": ["die", "wirst", "du", "zu", "f\u00fcrs\u00b7ten", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PTKZU", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.37": {"line.1": {"text": "Alle \u00f6rter, darauf eure fu\u00dfsole trit, sollen euer seyn.", "tokens": ["Al\u00b7le", "\u00f6r\u00b7ter", ",", "da\u00b7rauf", "eu\u00b7re", "fu\u00df\u00b7so\u00b7le", "trit", ",", "sol\u00b7len", "eu\u00b7er", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,", "VMFIN", "PPOSAT", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.38": {"line.1": {"text": "in aller welt.", "tokens": ["in", "al\u00b7ler", "welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.39": {"line.1": {"text": "Die tag und nacht kein schlaf bef\u00e4llt, f\u00fchrn deinen wagen durch die welt.", "tokens": ["Die", "tag", "und", "nacht", "kein", "schlaf", "be\u00b7f\u00e4llt", ",", "f\u00fchrn", "dei\u00b7nen", "wa\u00b7gen", "durch", "die", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.40": {"line.1": {"text": "Ich will deines namens gedenken", "tokens": ["Ich", "will", "dei\u00b7nes", "na\u00b7mens", "ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.41": {"line.1": {"text": "von kind zu kindes kind.", "tokens": ["von", "kind", "zu", "kin\u00b7des", "kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.42": {"line.1": {"text": "Darum werden dir alle National-Ch\u00f6re danken", "tokens": ["Da\u00b7rum", "wer\u00b7den", "dir", "al\u0