{"dta.poem.7960": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Oeder Fr\u00fchling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl denk\u2019 ich jener sel\u2019gen Jugendtr\u00e4ume,", "tokens": ["Wohl", "denk'", "ich", "je\u00b7ner", "sel'\u00b7gen", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Obschon sich die Gef\u00fchle mir versagen,", "tokens": ["Ob\u00b7schon", "sich", "die", "Ge\u00b7f\u00fch\u00b7le", "mir", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann in den ersten, milden Fr\u00fchlingstagen", "tokens": ["Wann", "in", "den", "ers\u00b7ten", ",", "mil\u00b7den", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Busen sich mir dr\u00e4ngten volle Keime.", "tokens": ["Im", "Bu\u00b7sen", "sich", "mir", "dr\u00e4ng\u00b7ten", "vol\u00b7le", "Kei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Ahnung lockte mich in ferne R\u00e4ume,", "tokens": ["Die", "Ah\u00b7nung", "lock\u00b7te", "mich", "in", "fer\u00b7ne", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann wo ein Laut des Lenzes angeschlagen;", "tokens": ["Wann", "wo", "ein", "Laut", "des", "Len\u00b7zes", "an\u00b7ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAV", "ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Hoffnung wollte sich zum Lichte wagen,", "tokens": ["Die", "Hoff\u00b7nung", "woll\u00b7te", "sich", "zum", "Lich\u00b7te", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie aus den Knospen frisches Gr\u00fcn der B\u00e4ume.", "tokens": ["Wie", "aus", "den", "Knos\u00b7pen", "fri\u00b7sches", "Gr\u00fcn", "der", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch nun, da ich das H\u00f6chste j\u00fcngst genossen,", "tokens": ["Doch", "nun", ",", "da", "ich", "das", "H\u00f6chs\u00b7te", "j\u00fcngst", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gerissen aus dem innigsten Vereine,", "tokens": ["Ge\u00b7ris\u00b7sen", "aus", "dem", "in\u00b7nigs\u00b7ten", "Ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vom reichsten Paradiese kaum versto\u00dfen:", "tokens": ["Vom", "reichs\u00b7ten", "Pa\u00b7ra\u00b7die\u00b7se", "kaum", "ver\u00b7sto\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was sollen nun mir halbergr\u00fcnte Triften,", "tokens": ["Was", "sol\u00b7len", "nun", "mir", "hal\u00b7ber\u00b7gr\u00fcn\u00b7te", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einsamer Amselschlag im todten Haine,", "tokens": ["Ein\u00b7sa\u00b7mer", "Am\u00b7sel\u00b7schlag", "im", "tod\u00b7ten", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein armes Veilchen, noch so s\u00fc\u00df von D\u00fcften?", "tokens": ["Ein", "ar\u00b7mes", "Veil\u00b7chen", ",", "noch", "so", "s\u00fc\u00df", "von", "D\u00fcf\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}