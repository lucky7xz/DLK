{"dta.poem.6554": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch kan \u00f6ffters selbst mit sich nicht einig seyn", "tokens": ["Ein", "Mensch", "kan", "\u00f6ff\u00b7ters", "selbst", "mit", "sich", "nicht", "ei\u00b7nig", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPR", "PRF", "PTKNEG", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey Farben, die er sieht. Ein Krancker, der allein,", "tokens": ["Bey", "Far\u00b7ben", ",", "die", "er", "sieht", ".", "Ein", "Kran\u00b7cker", ",", "der", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entfernt von Lerm und Licht, in grosser Mattigkeit", "tokens": ["Ent\u00b7fernt", "von", "Lerm", "und", "Licht", ",", "in", "gros\u00b7ser", "Mat\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf seinem Bette liegt, (hebt er einmal", "tokens": ["Auf", "sei\u00b7nem", "Bet\u00b7te", "liegt", ",", "(", "hebt", "er", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "$(", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Augen-Lieder auf) ist durch den kleinsten Strahl", "tokens": ["Die", "Au\u00b7gen\u00b7Lie\u00b7der", "auf", ")", "ist", "durch", "den", "kleins\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "$(", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Geblendet und verletzet", "tokens": ["Ge\u00b7blen\u00b7det", "und", "ver\u00b7let\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch Farben, die ihn sonst, benebst dem Licht, erg\u00f6tzet.", "tokens": ["Durch", "Far\u00b7ben", ",", "die", "ihn", "sonst", ",", "be\u00b7nebst", "dem", "Licht", ",", "er\u00b7g\u00f6t\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}