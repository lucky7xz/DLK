rd", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$,", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht daneben ein.", "tokens": ["Viel\u00b7leicht", "da\u00b7ne\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Du, komm herab zu dieser Gruft,", "tokens": ["Du", ",", "komm", "her\u00b7ab", "zu", "die\u00b7ser", "Gruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stirb allhier, wie ich:", "tokens": ["Und", "stirb", "all\u00b7hier", ",", "wie", "ich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da droben in der deutschen Luft,", "tokens": ["Da", "dro\u00b7ben", "in", "der", "deut\u00b7schen", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ist nicht Raum f\u00fcr dich!", "tokens": ["Da", "ist", "nicht", "Raum", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}