{"dta.poem.2762": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen Sch\u00f6neste/ die meinen Geist erquickt/", "tokens": ["Will\u00b7kom\u00b7men", "Sch\u00f6\u00b7nes\u00b7te", "/", "die", "mei\u00b7nen", "Geist", "er\u00b7quickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die meine Seele durch ihren Glantz entz\u00fcckt/", "tokens": ["Die", "mei\u00b7ne", "See\u00b7le", "durch", "ih\u00b7ren", "Glantz", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Willkommen Schatz/ mein ander Leben/", "tokens": ["Will\u00b7kom\u00b7men", "Schatz", "/", "mein", "an\u00b7der", "Le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ich mich gantz und gar ergeben.", "tokens": ["Der", "ich", "mich", "gantz", "und", "gar", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Stern aller Freuden/ sch\u00f6neste Perlemuth/", "tokens": ["Stern", "al\u00b7ler", "Freu\u00b7den", "/", "sch\u00f6\u00b7nes\u00b7te", "Per\u00b7le\u00b7muth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$(", "ADJA", "NN", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wenn deiner Augen blitzende Liebes Glut", "tokens": ["Wenn", "dei\u00b7ner", "Au\u00b7gen", "blit\u00b7zen\u00b7de", "Lie\u00b7bes", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bestrahlet mein entbrandtes Hertze", "tokens": ["Be\u00b7strah\u00b7let", "mein", "ent\u00b7brand\u00b7tes", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So acht ich nicht der Sonnen Kertze.", "tokens": ["So", "acht", "ich", "nicht", "der", "Son\u00b7nen", "Kert\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn dein Gesicht weiset den Himmel mir/", "tokens": ["Denn", "dein", "Ge\u00b7sicht", "wei\u00b7set", "den", "Him\u00b7mel", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie der bebl\u00fcmet wird von der Sternen Zier;", "tokens": ["Wie", "der", "be\u00b7bl\u00fc\u00b7met", "wird", "von", "der", "Ster\u00b7nen", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVFIN", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So streu\u2019a auch deine Fackeln funcken/", "tokens": ["So", "streu'a", "auch", "dei\u00b7ne", "Fa\u00b7ckeln", "fun\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die in die Seele mir gesuncken.", "tokens": ["Die", "in", "die", "See\u00b7le", "mir", "ge\u00b7sun\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ich nur dencke/ was ich nur red\u2019 und thu/", "tokens": ["Was", "ich", "nur", "den\u00b7cke", "/", "was", "ich", "nur", "red'", "und", "thu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$(", "PWS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das auserw\u00e4hlte Perlemuth das bist du/", "tokens": ["Das", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", "Per\u00b7le\u00b7muth", "das", "bist", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "VAFIN", "PPER", "$("], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Ich geh/ ich steh/ ich schlaf\u2019/ ich wache/", "tokens": ["Ich", "geh", "/", "ich", "steh", "/", "ich", "schlaf'", "/", "ich", "wa\u00b7che", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bleibst du doch der Zweck der Sache.", "tokens": ["So", "bleibst", "du", "doch", "der", "Zweck", "der", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Holdreiche G\u00f6ttin/ die meinen Geist bezwingt/", "tokens": ["Hold\u00b7rei\u00b7che", "G\u00f6t\u00b7tin", "/", "die", "mei\u00b7nen", "Geist", "be\u00b7zwingt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und die das Siegsfahn \u00fcber mein Leben schwingt/", "tokens": ["Und", "die", "das", "Siegs\u00b7fahn", "\u00fc\u00b7ber", "mein", "Le\u00b7ben", "schwingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mich d\u00fcnckt da\u00df alle Seeligkeiten/", "tokens": ["Mich", "d\u00fcnckt", "da\u00df", "al\u00b7le", "See\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinem Eintritt dich begleiten.", "tokens": ["Mit", "dei\u00b7nem", "Ein\u00b7tritt", "dich", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gast dessen gleichen mir nicht der Erden Krei\u00df", "tokens": ["Gast", "des\u00b7sen", "glei\u00b7chen", "mir", "nicht", "der", "Er\u00b7den", "Krei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In allen Enden einst zu gew\u00e4hren wei\u00df/", "tokens": ["In", "al\u00b7len", "En\u00b7den", "einst", "zu", "ge\u00b7w\u00e4h\u00b7ren", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was nicht die gantze Welt kan schicken/", "tokens": ["Was", "nicht", "die", "gant\u00b7ze", "Welt", "kan", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dem kan Oel\u00dfe mich begl\u00fccken.", "tokens": ["Mit", "dem", "kan", "O\u00b7el\u00b7\u00dfe", "mich", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wie aus des Himmels Zimmern die Morgenr\u00f6th", "tokens": ["Wie", "aus", "des", "Him\u00b7mels", "Zim\u00b7mern", "die", "Mor\u00b7gen\u00b7r\u00f6th"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jm h\u00f6chsten Purpur gleich eine F\u00fcrstin geht/", "tokens": ["Jm", "h\u00f6chs\u00b7ten", "Pur\u00b7pur", "gleich", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So ist mit nicht geringerm Lichte", "tokens": ["So", "ist", "mit", "nicht", "ge\u00b7rin\u00b7germ", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umbgeben/ Schatz/ dein Angesichte.", "tokens": ["U\u00b7mbge\u00b7ben", "/", "Schatz", "/", "dein", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$(", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Du tr\u00e4gest8 Rosen/ gleichwie dein Name heist", "tokens": ["Du", "tr\u00e4\u00b7gest8", "Ro\u00b7sen", "/", "gleich\u00b7wie", "dein", "Na\u00b7me", "heist"], "token_info": ["word", "number_compound", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und theuren Perlen gleichet dein edler Geist/", "tokens": ["Und", "theu\u00b7ren", "Per\u00b7len", "glei\u00b7chet", "dein", "ed\u00b7ler", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich lasse Geld und G\u00fcter fahren/", "tokens": ["Ich", "las\u00b7se", "Geld", "und", "G\u00fc\u00b7ter", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bleibst die beste von den Waaren.", "tokens": ["Du", "bleibst", "die", "bes\u00b7te", "von", "den", "Waa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da\u00df mir dein Anblick schencket die h\u00f6chste Lust/", "tokens": ["Da\u00df", "mir", "dein", "An\u00b7blick", "schen\u00b7cket", "die", "h\u00f6chs\u00b7te", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df deine Worte gleichfals der Liebekost/", "tokens": ["Da\u00df", "dei\u00b7ne", "Wor\u00b7te", "gleich\u00b7fals", "der", "Lie\u00b7be\u00b7kost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df dein Ku\u00df kan die Seele weiden/", "tokens": ["Da\u00df", "dein", "Ku\u00df", "kan", "die", "See\u00b7le", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sind nur ein Vorbild gr\u00f6ssrer Freuden.", "tokens": ["Sind", "nur", "ein", "Vor\u00b7bild", "gr\u00f6ss\u00b7rer", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ach Sonne renne/ k\u00fcrtze der Tage Rest/", "tokens": ["Ach", "Son\u00b7ne", "ren\u00b7ne", "/", "k\u00fcrt\u00b7ze", "der", "Ta\u00b7ge", "Rest", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "$(", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bi\u00df da\u00df erscheinet unser Verm\u00e4hlungs-Fest/", "tokens": ["Bi\u00df", "da\u00df", "er\u00b7schei\u00b7net", "un\u00b7ser", "Ver\u00b7m\u00e4h\u00b7lungs\u00b7Fest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df wir den besten Zweck erzielen/", "tokens": ["Da\u00df", "wir", "den", "bes\u00b7ten", "Zweck", "er\u00b7zie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsre Glut zusammen spielen/", "tokens": ["Und", "uns\u00b7re", "Glut", "zu\u00b7sam\u00b7men", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Augen Sterne/ der Wangen Rosen-Glut/", "tokens": ["Der", "Au\u00b7gen", "Ster\u00b7ne", "/", "der", "Wan\u00b7gen", "Ro\u00b7sen\u00b7Glut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Schnee des Halses/ der Lippen Purpur-Blut/", "tokens": ["Der", "Schnee", "des", "Hal\u00b7ses", "/", "der", "Lip\u00b7pen", "Pur\u00b7pur\u00b7Blut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Br\u00fcste rund-geschwollne H\u00fcgel", "tokens": ["Der", "Br\u00fcs\u00b7te", "run\u00b7dge\u00b7schwoll\u00b7ne", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verbleiben unsrer Liebe Sigel.", "tokens": ["Ver\u00b7blei\u00b7ben", "uns\u00b7rer", "Lie\u00b7be", "Si\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "So hat die Venus nicht den Adonerfrischt/", "tokens": ["So", "hat", "die", "Ve\u00b7nus", "nicht", "den", "A\u00b7do\u00b7ner\u00b7frischt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn sie vergn\u00fcget haben im Wald getischt/", "tokens": ["Wenn", "sie", "ver\u00b7gn\u00fc\u00b7get", "ha\u00b7ben", "im", "Wald", "ge\u00b7tischt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als deine Gegenwart mich tr\u00f6stet.", "tokens": ["Als", "dei\u00b7ne", "Ge\u00b7gen\u00b7wart", "mich", "tr\u00f6s\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir das Liebes-Manna r\u00f6stet.", "tokens": ["Und", "mir", "das", "Lie\u00b7bes\u00b7Man\u00b7na", "r\u00f6s\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}