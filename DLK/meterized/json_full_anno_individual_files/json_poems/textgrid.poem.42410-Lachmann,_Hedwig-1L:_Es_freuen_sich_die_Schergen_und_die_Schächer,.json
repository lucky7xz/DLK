{"textgrid.poem.42410": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es freuen sich die Schergen und die Sch\u00e4cher,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es freuen sich die Schergen und die Sch\u00e4cher,", "tokens": ["Es", "freu\u00b7en", "sich", "die", "Scher\u00b7gen", "und", "die", "Sch\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass man die Unschuld peinigt und verh\u00f6hnt,", "tokens": ["Dass", "man", "die", "Un\u00b7schuld", "pei\u00b7nigt", "und", "ver\u00b7h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gebunden steht das Opfer, dran ein frecher", "tokens": ["Ge\u00b7bun\u00b7den", "steht", "das", "Op\u00b7fer", ",", "dran", "ein", "fre\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PAV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tyrannend\u00fcnkel seiner Willk\u00fcr fr\u00f6nt.", "tokens": ["Ty\u00b7ran\u00b7nen\u00b7d\u00fcn\u00b7kel", "sei\u00b7ner", "Will\u00b7k\u00fcr", "fr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So muss zu Fluch und ewigem Verderben", "tokens": ["So", "muss", "zu", "Fluch", "und", "e\u00b7wi\u00b7gem", "Ver\u00b7der\u00b7ben"], "token_info": ["word