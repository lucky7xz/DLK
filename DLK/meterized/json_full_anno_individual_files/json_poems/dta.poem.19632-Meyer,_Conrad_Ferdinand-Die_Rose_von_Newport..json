", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Schmal und gespenstig im Spiegel des Elends", "tokens": ["Schmal", "und", "ge\u00b7spens\u00b7tig", "im", "Spie\u00b7gel", "des", "E\u00b7lends"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.15": {"text": "Sieht er das eigene Antlitz und schaudert.", "tokens": ["Sieht", "er", "das", "ei\u00b7ge\u00b7ne", "Ant\u00b7litz", "und", "schau\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Morgen erz\u00e4hlen die Linden das M\u00e4rchen", "tokens": ["Mor\u00b7gen", "er\u00b7z\u00e4h\u00b7len", "die", "Lin\u00b7den", "das", "M\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NE", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Von dem enthaupteten K\u00f6nig in England.", "tokens": ["Von", "dem", "ent\u00b7haup\u00b7te\u00b7ten", "K\u00f6\u00b7nig", "in", "En\u00b7gland", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}