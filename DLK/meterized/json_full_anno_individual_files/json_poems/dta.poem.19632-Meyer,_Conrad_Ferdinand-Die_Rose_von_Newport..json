{"dta.poem.19632": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose von Newport.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Sprengende Reiter und flatternde Bl\u00fcthen,", "tokens": ["Spren\u00b7gen\u00b7de", "Rei\u00b7ter", "und", "flat\u00b7tern\u00b7de", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Einer voraus mit gescheitelten Locken \u2014", "tokens": ["Ei\u00b7ner", "vo\u00b7raus", "mit", "ge\u00b7schei\u00b7tel\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ist es der Lenz auf gefl\u00fcgeltem Renner?", "tokens": ["Ist", "es", "der", "Lenz", "auf", "ge\u00b7fl\u00fc\u00b7gel\u00b7tem", "Ren\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Karl ist's, der J\u00fcngling, der Erbe von England,", "tokens": ["Karl", "ist's", ",", "der", "J\u00fcng\u00b7ling", ",", "der", "Er\u00b7be", "von", "En\u00b7gland", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Und die sich n\u00e4hern in goldener Mailuft,", "tokens": ["Und", "die", "sich", "n\u00e4\u00b7hern", "in", "gol\u00b7de\u00b7ner", "Mai\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--++", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das sind die Giebel und Thore von Newport,", "tokens": ["Das", "sind", "die", "Gie\u00b7bel", "und", "Tho\u00b7re", "von", "New\u00b7port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dr\u00fcber das Wappen der Stadt: eine Rose!", "tokens": ["Dr\u00fc\u00b7ber", "das", "Wap\u00b7pen", "der", "Stadt", ":", "ei\u00b7ne", "Ro\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Jubelnde Gassen und jubelnde Wimpel", "tokens": ["Ju\u00b7beln\u00b7de", "Gas\u00b7sen", "und", "ju\u00b7beln\u00b7de", "Wim\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Und ein von treibender Jugend geschwelltes,", "tokens": ["Und", "ein", "von", "trei\u00b7ben\u00b7der", "Ju\u00b7gend", "ge\u00b7schwell\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Jubelndes Herz in dem Busen des Stuart ...", "tokens": ["Ju\u00b7beln\u00b7des", "Herz", "in", "dem", "Bu\u00b7sen", "des", "Stu\u00b7art", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Unter den bl\u00fchenden Linden des Marktes", "tokens": ["Un\u00b7ter", "den", "bl\u00fc\u00b7hen\u00b7den", "Lin\u00b7den", "des", "Mark\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Schreitet ein Reigen von bl\u00fch'nden Gestalten", "tokens": ["Schrei\u00b7tet", "ein", "Rei\u00b7gen", "von", "bl\u00fch'n\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.13": {"text": "Und eine Sch\u00f6nste mit herzlichem Beben", "tokens": ["Und", "ei\u00b7ne", "Sch\u00f6ns\u00b7te", "mit", "herz\u00b7li\u00b7chem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Bietet dem Prinzen die Rose von Newport:", "tokens": ["Bie\u00b7tet", "dem", "Prin\u00b7zen", "die", "Ro\u00b7se", "von", "New\u00b7port", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "NE", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.15": {"text": "\u201eseliges Gestern und Morgen und Heute,", "tokens": ["\u201e", "se\u00b7li\u00b7ges", "Ge\u00b7stern", "und", "Mor\u00b7gen", "und", "Heu\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "----+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Herr, Dir die Rose von Newport bedeute!\u201c", "tokens": ["Herr", ",", "Dir", "die", "Ro\u00b7se", "von", "New\u00b7port", "be\u00b7deu\u00b7te", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "APPR", "NE", "VVFIN", "$.", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Morgen erz\u00e4hlen die Linden das M\u00e4rchen", "tokens": ["Mor\u00b7gen", "er\u00b7z\u00e4h\u00b7len", "die", "Lin\u00b7den", "das", "M\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NE", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Von der entbl\u00e4tterten Rose von Newport.", "tokens": ["Von", "der", "ent\u00b7bl\u00e4t\u00b7ter\u00b7ten", "Ro\u00b7se", "von", "New\u00b7port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Sprengende Reiter und wirbelnde Flocken,", "tokens": ["Spren\u00b7gen\u00b7de", "Rei\u00b7ter", "und", "wir\u00b7beln\u00b7de", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Einer voraus mit verwilderten Haaren \u2014", "tokens": ["Ei\u00b7ner", "vo\u00b7raus", "mit", "ver\u00b7wil\u00b7der\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Ist es der Winter, der finstre Geselle?", "tokens": ["Ist", "es", "der", "Win\u00b7ter", ",", "der", "finst\u00b7re", "Ge\u00b7sel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Karl ist's, der Fl\u00fcchtling, der K\u00f6nig von England.", "tokens": ["Karl", "ist's", ",", "der", "Fl\u00fccht\u00b7ling", ",", "der", "K\u00f6\u00b7nig", "von", "En\u00b7gland", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "NE", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Seit er das Blut seines Volkes vergossen,", "tokens": ["Seit", "er", "das", "Blut", "sei\u00b7nes", "Vol\u00b7kes", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Reitet er neben zerschmetterndem Abgrund ...", "tokens": ["Rei\u00b7tet", "er", "ne\u00b7ben", "zer\u00b7schmet\u00b7tern\u00b7dem", "Ab\u00b7grund", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und die sich n\u00e4hern in wei\u00dfem Gest\u00f6ber,", "tokens": ["Und", "die", "sich", "n\u00e4\u00b7hern", "in", "wei\u00b7\u00dfem", "Ge\u00b7st\u00f6\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das sind die Giebel und Thore von Newport,", "tokens": ["Das", "sind", "die", "Gie\u00b7bel", "und", "Tho\u00b7re", "von", "New\u00b7port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dr\u00fcber das Wappen der Stadt: eine Rose!", "tokens": ["Dr\u00fc\u00b7ber", "das", "Wap\u00b7pen", "der", "Stadt", ":", "ei\u00b7ne", "Ro\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Nirgend ein Jubel und nirgend ein Wimpel,", "tokens": ["Nir\u00b7gend", "ein", "Ju\u00b7bel", "und", "nir\u00b7gend", "ein", "Wim\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Polternde H\u00e4mmer und kreischende Feilen \u2014", "tokens": ["Pol\u00b7tern\u00b7de", "H\u00e4m\u00b7mer", "und", "krei\u00b7schen\u00b7de", "Fei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Und ein von eisernen F\u00e4usten gepresstes,", "tokens": ["Und", "ein", "von", "ei\u00b7ser\u00b7nen", "F\u00e4us\u00b7ten", "ge\u00b7press\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Aechzendes Herz in dem Busen des Stuart ...", "tokens": ["A\u00b7ech\u00b7zen\u00b7des", "Herz", "in", "dem", "Bu\u00b7sen", "des", "Stu\u00b7art", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Unter den frierenden Linden des Marktes", "tokens": ["Un\u00b7ter", "den", "frie\u0