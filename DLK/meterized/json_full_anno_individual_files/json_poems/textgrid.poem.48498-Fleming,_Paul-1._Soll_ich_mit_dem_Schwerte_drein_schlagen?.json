{"textgrid.poem.48498": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1. Soll ich mit dem Schwerte drein schlagen?", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und du bist Petrus Art, mein Sinn! Wenn man dich dr\u00e4nget,", "tokens": ["Und", "du", "bist", "Pet\u00b7rus", "Art", ",", "mein", "Sinn", "!", "Wenn", "man", "dich", "dr\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "FM", "NN", "$,", "PPOSAT", "NN", "$.", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so fragst du zornig stracks: wie soll ich schlagen drein?", "tokens": ["so", "fragst", "du", "zor\u00b7nig", "stracks", ":", "wie", "soll", "ich", "schla\u00b7gen", "drein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$.", "PWAV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nein, nein! Das gehet nicht. Steh' aus, was Gott verh\u00e4nget!", "tokens": ["Nein", ",", "nein", "!", "Das", "ge\u00b7het", "nicht", ".", "Steh'", "aus", ",", "was", "Gott", "ver\u00b7h\u00e4n\u00b7get", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$.", "PDS", "VVFIN", "PTKNEG", "$.", "NN", "PTKVZ", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hat sich wol ger\u00e4cht, wer kan ged\u00fcltig sein.", "tokens": ["Der", "h