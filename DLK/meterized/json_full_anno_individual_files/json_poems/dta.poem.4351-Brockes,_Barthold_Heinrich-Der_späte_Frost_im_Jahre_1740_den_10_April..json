{"dta.poem.4351": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der sp\u00e4te Frost im Jahre 1740  \n den 10 April.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es suchen auf den \u00f6den Gr\u00fcnden,", "tokens": ["Es", "su\u00b7chen", "auf", "den", "\u00f6\u00b7den", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sonst so lucker, itzt so hart,", "tokens": ["Die", "sonst", "so", "lu\u00b7cker", ",", "itzt", "so", "hart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die armen Sch\u00e4fgen selbst, erstarrt,", "tokens": ["Die", "ar\u00b7men", "Sch\u00e4f\u00b7gen", "selbst", ",", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umsonst, ein Spierchen Gras zu finden.", "tokens": ["Um\u00b7sonst", ",", "ein", "Spier\u00b7chen", "Gras", "zu", "fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr lautes Schreyn, ihr \u00e4ngstlichs Blecken", "tokens": ["Ihr", "lau\u00b7tes", "Schreyn", ",", "ihr", "\u00e4ngst\u00b7lichs", "Ble\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Scheint Gram und Hunger zu entdecken.", "tokens": ["Scheint", "Gram", "und", "Hun\u00b7ger", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie f\u00fchlen ihr schon nah Verderben,", "tokens": ["Sie", "f\u00fch\u00b7len", "ihr", "schon", "nah", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von ihrer n\u00f6ht\u2019gen Kost beraubt.", "tokens": ["Von", "ih\u00b7rer", "n\u00f6ht'\u00b7gen", "Kost", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hier senkt ein zartes Lamm sein Haupt;", "tokens": ["Hier", "senkt", "ein", "zar\u00b7tes", "Lamm", "sein", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dort sieht man alte Schafe sterben.", "tokens": ["Dort", "sieht", "man", "al\u00b7te", "Scha\u00b7fe", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der arme Landmann kratzt sein Haar,", "tokens": ["Der", "ar\u00b7me", "Land\u00b7mann", "kratzt", "sein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und scheut noch gr\u00f6ssere Gefahr;", "tokens": ["Und", "scheut", "noch", "gr\u00f6s\u00b7se\u00b7re", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Indem er durch den Frost so gar", "tokens": ["In\u00b7dem", "er", "durch", "den", "Frost", "so", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sein Winter-Korn verdorben glaubt.", "tokens": ["Sein", "Win\u00b7ter\u00b7Korn", "ver\u00b7dor\u00b7ben", "glaubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Es ist nunmehro Zeit zu pfl\u00fcgen;", "tokens": ["Es", "ist", "nun\u00b7meh\u00b7ro", "Zeit", "zu", "pfl\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Allein er kann, trotz seinem Flei\u00df,", "tokens": ["Al\u00b7lein", "er", "kann", ",", "trotz", "sei\u00b7nem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "F\u00fcr das annoch vorhandne Eis", "tokens": ["F\u00fcr", "das", "an\u00b7noch", "vor\u00b7hand\u00b7ne", "Eis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Den Pflug nicht in die Erde kriegen.", "tokens": ["Den", "Pflug", "nicht", "in", "die", "Er\u00b7de", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Er denkt annoch ans vor\u2019ge Jahr,", "tokens": ["Er", "denkt", "an\u00b7noch", "ans", "vor'\u00b7ge", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wo auch, beym seltnen Licht der Sonnen,", "tokens": ["Wo", "auch", ",", "beym", "selt\u00b7nen", "Licht", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Da es stets kalt und windig war,", "tokens": ["Da", "es", "stets", "kalt", "und", "win\u00b7dig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Er auch nur wenig Korn gewonnen.", "tokens": ["Er", "auch", "nur", "we\u00b7nig", "Korn", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Er hoffet, mit fast m\u00fcden Sorgen,", "tokens": ["Er", "hof\u00b7fet", ",", "mit", "fast", "m\u00fc\u00b7den", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Von jedem Heut zum andern Morgen,", "tokens": ["Von", "je\u00b7dem", "Heut", "zum", "an\u00b7dern", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Von einer zu der andern Nacht,", "tokens": ["Von", "ei\u00b7ner", "zu", "der", "an\u00b7dern", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da\u00df sich die kalten Winde legen,", "tokens": ["Da\u00df", "sich", "die", "kal\u00b7ten", "Win\u00b7de", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Schnee, Eis und Reif entfernen m\u00f6gen,", "tokens": ["Schnee", ",", "Eis", "und", "Reif", "ent\u00b7fer\u00b7nen", "m\u00f6\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Auch da\u00df der Sonnen-Strahlen Pracht", "tokens": ["Auch", "da\u00df", "der", "Son\u00b7nen\u00b7Strah\u00b7len", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Sich mit der lauen Luft verbinde,", "tokens": ["Sich", "mit", "der", "lau\u00b7en", "Luft", "ver\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und ihre kalte Theil\u2019 entz\u00fcnde.", "tokens": ["Und", "ih\u00b7re", "kal\u00b7te", "Theil'", "ent\u00b7z\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Allein es ist sein \u00e4ngstlich Hoffen", "tokens": ["Al\u00b7lein", "es", "ist", "sein", "\u00e4ngst\u00b7lich", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Noch bis daher nicht eingetroffen.", "tokens": ["Noch", "bis", "da\u00b7her", "nicht", "ein\u00b7ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}