{"textgrid.poem.40340": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Mors", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine rote Feuerlilie schreitet", "tokens": ["Ei\u00b7ne", "ro\u00b7te", "Feu\u00b7er\u00b7li\u00b7lie", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "riesig durch die Weltennacht.", "tokens": ["rie\u00b7sig", "durch", "die", "Wel\u00b7ten\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Sonne bis zum Sirius breitet", "tokens": ["Von", "der", "Son\u00b7ne", "bis", "zum", "Si\u00b7rius", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "sich ihr Scharlachkelch. Der Schacht", "tokens": ["sich", "ihr", "Schar\u00b7lach\u00b7kelch", ".", "Der", "Schacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "des gez\u00e4hnten Schlundes kocht von Gluten,", "tokens": ["des", "ge\u00b7z\u00e4hn\u00b7ten", "Schlun\u00b7des", "kocht", "von", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "d\u00fcster flammt des Randes Zackenfirne;", "tokens": ["d\u00fcs\u00b7ter", "flammt", "des", "Ran\u00b7des", "Za\u00b7cken\u00b7fir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "um die wirbelnden Gestirne", "tokens": ["um", "die", "wir\u00b7beln\u00b7den", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "schlingt sie hungrig ihre Samenruten.", "tokens": ["schlingt", "sie", "hung\u00b7rig", "ih\u00b7re", "Sa\u00b7men\u00b7ru\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Grell aufz\u00fcngelnd schl\u00fcrft sie die getrennten", "tokens": ["Grell", "auf\u00b7z\u00fcn\u00b7gelnd", "schl\u00fcrft", "sie", "die", "ge\u00b7trenn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "VVFIN", "PPER", "ART", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Welten gierig in den br\u00fcnstigen Schoo\u00df;", "tokens": ["Wel\u00b7ten", "gie\u00b7rig", "in", "den", "br\u00fcns\u00b7ti\u00b7gen", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "aus den schwarzen Firmamenten", "tokens": ["aus", "den", "schwar\u00b7zen", "Fir\u00b7ma\u00b7men\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ringen Sonne, Sirius sich los.", "tokens": ["rin\u00b7gen", "Son\u00b7ne", ",", "Si\u00b7rius", "sich", "los", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NE", "PRF", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Lodernd sehn sie die Unendlichkeiten", "tokens": ["Lo\u00b7dernd", "sehn", "sie", "die", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "ihrer alten Sehnsucht \u00fcberbr\u00fcckt;", "tokens": ["ih\u00b7rer", "al\u00b7ten", "Sehn\u00b7sucht", "\u00fc\u00b7berb\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "aus den Angeln wanken sie verz\u00fcckt,", "tokens": ["aus", "den", "An\u00b7geln", "wan\u00b7ken", "sie", "ver\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "zu einander st\u00fcrzen die befreiten.", "tokens": ["zu", "ein\u00b7an\u00b7der", "st\u00fcr\u00b7zen", "die", "be\u00b7frei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Taumelnd folgen, brodeln, gl\u00fchen", "tokens": ["Tau\u00b7melnd", "fol\u00b7gen", ",", "bro\u00b7deln", ",", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ringsum die Trabantenl\u00fcfte;", "tokens": ["ring\u00b7sum", "die", "Tra\u00b7ban\u00b7ten\u00b7l\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "aus der brennenden Lilie spr\u00fchen", "tokens": ["aus", "der", "bren\u00b7nen\u00b7den", "Li\u00b7lie", "spr\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lavast\u00fcrme durch die Himmelsgr\u00fcfte.", "tokens": ["La\u00b7vas\u00b7t\u00fcr\u00b7me", "durch", "die", "Him\u00b7mels\u00b7gr\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Auf der Erde rast ihr Licht als Mord;", "tokens": ["Auf", "der", "Er\u00b7de", "rast", "ihr", "Licht", "als", "Mord", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "sengend fri\u00dft es W\u00e4lder, Str\u00f6me, Quellen,", "tokens": ["sen\u00b7gend", "fri\u00dft", "es", "W\u00e4l\u00b7der", ",", "Str\u00f6\u00b7me", ",", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Asche trieft aus blendenden Wolkenh\u00f6llen,", "tokens": ["A\u00b7sche", "trieft", "aus", "blen\u00b7den\u00b7den", "Wol\u00b7ken\u00b7h\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.8": {"text": "alle Kreatur verdorrt.", "tokens": ["al\u00b7le", "Kre\u00b7a\u00b7tur", "ver\u00b7dorrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur ein Brautpaar will noch f\u00fchlend enden,", "tokens": ["Nur", "ein", "Braut\u00b7paar", "will", "noch", "f\u00fch\u00b7lend", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "keuchend, schon erblindet beide;", "tokens": ["keu\u00b7chend", ",", "schon", "er\u00b7blin\u00b7det", "bei\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mit den hei\u00dfen Liebesh\u00e4nden", "tokens": ["mit", "den", "hei\u00b7\u00dfen", "Lie\u00b7bes\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "tastet er an ihrem Kleide.", "tokens": ["tas\u00b7tet", "er", "an", "ih\u00b7rem", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber in der Nacht der Seele", "tokens": ["A\u00b7ber", "in", "der", "Nacht", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wird der wilde Durst zur Wut:", "tokens": ["wird", "der", "wil\u00b7de", "Durst", "zur", "Wut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "tastend wittert er ihr Blut,", "tokens": ["tas\u00b7tend", "wit\u00b7tert", "er", "ihr", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "bei\u00dft er, schl\u00fcrft er sich in ihre Kehle.", "tokens": ["bei\u00dft", "er", ",", "schl\u00fcrft", "er", "sich", "in", "ih\u00b7re", "Keh\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Alles saugt der gro\u00dfe Flammenschlund.", "tokens": ["Al\u00b7les", "saugt", "der", "gro\u00b7\u00dfe", "Flam\u00b7men\u00b7schlund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Kreisend will er \u00fcbersch\u00e4umen.", "tokens": ["Krei\u00b7send", "will", "er", "\u00fc\u00b7bersc\u00b7h\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rissig klafft der zuckende Muttermund,", "tokens": ["Ris\u00b7sig", "klafft", "der", "zu\u00b7cken\u00b7de", "Mut\u00b7ter\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "D\u00e4mpfe bersten, Feuerpollen s\u00e4umen", "tokens": ["D\u00e4mp\u00b7fe", "bers\u00b7ten", ",", "Feu\u00b7er\u00b7pol\u00b7len", "s\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "den zerfetzten Riesenbl\u00fctenrand:", "tokens": ["den", "zer\u00b7fetz\u00b7ten", "Rie\u00b7sen\u00b7bl\u00fc\u00b7ten\u00b7rand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Strahlend quillt sie aus dem morgenroten", "tokens": ["Strah\u00b7lend", "quillt", "sie", "aus", "dem", "mor\u00b7gen\u00b7ro\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "furchtbar'n Siriusliebestodesbrand.", "tokens": ["furcht\u00b7ba\u00b7r'n", "Si\u00b7rius\u00b7lie\u00b7be\u00b7sto\u00b7des\u00b7brand", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.7": {"line.1": {"text": "Dahin also sehnt sich alles fort,", "tokens": ["Da\u00b7hin", "al\u00b7so", "sehnt", "sich", "al\u00b7les", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PRF", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "lin