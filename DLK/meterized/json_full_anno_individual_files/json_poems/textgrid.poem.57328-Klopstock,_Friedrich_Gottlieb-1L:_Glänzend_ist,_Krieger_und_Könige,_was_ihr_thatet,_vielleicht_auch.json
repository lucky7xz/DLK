{"textgrid.poem.57328": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00e4nzend ist, Krieger und K\u00f6nige, was ihr thatet, vielleicht auch", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00e4nzend ist, Krieger und K\u00f6nige, was ihr thatet, vielleicht auch", "tokens": ["Gl\u00e4n\u00b7zend", "ist", ",", "Krie\u00b7ger", "und", "K\u00f6\u00b7ni\u00b7ge", ",", "was", "ihr", "tha\u00b7tet", ",", "viel\u00b7leicht", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "NN", "KON", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADV"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Edel, o Wunder! so gar.", "tokens": ["E\u00b7del", ",", "o", "Wun\u00b7der", "!", "so", "gar", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$.", "ADV", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Was es denn sey; es steiget gewiss zu dem Enkel hinunter:", "tokens": ["Was", "es", "denn", "sey", ";", "es", "stei\u00b7get", "ge\u00b7wiss", "zu", "dem", "En\u00b7kel", "hin\u00b7un\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aber in welcher Gestalt?", "tokens": ["A\u00b7ber", "in", "wel\u00b7cher", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PWAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Etwa in der, die es hatte, da ihr es thatet? In jeder", "tokens": ["Et\u00b7wa", "in", "der", ",", "die", "es", "hat\u00b7te", ",", "da", "ihr", "es", "tha\u00b7tet", "?", "In", "je\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "$,", "PRELS", "PPER", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$.", "APPR", "PIAT"], "meter": "+---+-+--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Andern, in dieser nur nicht!", "tokens": ["An\u00b7dern", ",", "in", "die\u00b7ser", "nur", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "APPR", "PDAT", "ADV", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Von der Geschichte verfehlt, bald hoch zu der Wolke gehoben,", "tokens": ["Von", "der", "Ge\u00b7schich\u00b7te", "ver\u00b7fehlt", ",", "bald", "hoch", "zu", "der", "Wol\u00b7ke", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Bald gesenkt in den Staub;", "tokens": ["Bald", "ge\u00b7senkt", "in", "den", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Mit der Fabel Verwandlung beynah gebildet, zum Drachen", "tokens": ["Mit", "der", "Fa\u00b7bel", "Ver\u00b7wand\u00b7lung", "bey\u00b7nah", "ge\u00b7bil\u00b7det", ",", "zum", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "VVPP", "$,", "APPRART", "NN"], "meter": "--+--+----+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Kadmus, der Drache zum Gott.", "tokens": ["Kad\u00b7mus", ",", "der", "Dra\u00b7che", "zum", "Gott", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Und nun setzen die Richter sich hin, und richten den Schatten,", "tokens": ["Und", "nun", "set\u00b7zen", "die", "Rich\u00b7ter", "sich", "hin", ",", "und", "rich\u00b7ten", "den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Weiser Entscheidungen voll,", "tokens": ["Wei\u00b7ser", "Ent\u00b7schei\u00b7dun\u00b7gen", "voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Alles, nachdem bey dem glimmernden Docht der Erz\u00e4hlende dunkel,", "tokens": ["Al\u00b7les", ",", "nach\u00b7dem", "bey", "dem", "glim\u00b7mern\u00b7den", "Docht", "der", "Er\u00b7z\u00e4h\u00b7len\u00b7de", "dun\u00b7kel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Oder dunkler es sah.", "tokens": ["O\u00b7der", "dunk\u00b7ler", "es", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Arme Krieger und K\u00f6nige, das ist also der Nachruhm,", "tokens": ["Ar\u00b7me", "Krie\u00b7ger", "und", "K\u00f6\u00b7ni\u00b7ge", ",", "das", "ist", "al\u00b7so", "der", "Nach\u00b7ruhm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "PDS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Der euch schlafen nicht liess?", "tokens": ["Der", "euch", "schla\u00b7fen", "nicht", "liess", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Euch verbot, an der Wissenschaft erfrischenden Quelle", "tokens": ["Euch", "ver\u00b7bot", ",", "an", "der", "Wis\u00b7sen\u00b7schaft", "er\u00b7fri\u00b7schen\u00b7den", "Quel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Auch nur am Abend zu ruhn?", "tokens": ["Auch", "nur", "am", "A\u00b7bend", "zu", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Unerquickte, so halten die Rhadamantchen der Nachwelt", "tokens": ["Un\u00b7er\u00b7quick\u00b7te", ",", "so", "hal\u00b7ten", "die", "Rha\u00b7da\u00b7mant\u00b7chen", "der", "Nach\u00b7welt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "\u00dcber euch ihr Gericht?", "tokens": ["\u00dc\u00b7ber", "euch", "ihr", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Gl\u00fccklicher fiel sein Loos dem Dichter. Was er uns nachliess,", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "fiel", "sein", "Loos", "dem", "Dich\u00b7ter.", "Was", "er", "uns", "nach\u00b7liess", ","], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Bleibet stets, was es war.", "tokens": ["Blei\u00b7bet", "stets", ",", "was", "es", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.23": {"text": "\u00dcber ihn waltet sie nicht, die Geschichte; da spielt die Verwandlung", "tokens": ["\u00dc\u00b7ber", "ihn", "wal\u00b7tet", "sie", "nicht", ",", "die", "Ge\u00b7schich\u00b7te", ";", "da", "spielt", "die", "Ver\u00b7wand\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Nicht, wie mit Thaten sie spielt.", "tokens": ["Nicht", ",", "wie", "mit", "Tha\u00b7ten", "sie", "spielt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Richter sehn die Fehle des Werks, die Sch\u00f6nheit: allein mehr,", "tokens": ["Rich\u00b7ter", "sehn", "die", "Feh\u00b7le", "des", "Werks", ",", "die", "Sch\u00f6n\u00b7heit", ":", "al\u00b7lein", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$.", "ADV", "ADV", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Andere nicht, denn es har.", "tokens": ["An\u00b7de\u00b7re", "nicht", ",", "denn", "es", "har", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$,", "KON", "PPER", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Richtelnde k\u00f6nnens mit Tadel best\u00e4uben, und Lobe; doch diess auch", "tokens": ["Rich\u00b7teln\u00b7de", "k\u00f6n\u00b7nens", "mit", "Ta\u00b7del", "be\u00b7st\u00e4u\u00b7ben", ",", "und", "Lo\u00b7be", ";", "doch", "diess", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "NN", "VVPP", "$,", "KON", "NN", "$.", "ADV", "PDS", "ADV"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.28": {"text": "K\u00f6nnen die w\u00e4hnenden nur.", "tokens": ["K\u00f6n\u00b7nen", "die", "w\u00e4h\u00b7nen\u00b7den", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "Andere kommen dann auch, und st\u00e4uben ab: und es stehet", "tokens": ["An\u00b7de\u00b7re", "kom\u00b7men", "dann", "auch", ",", "und", "st\u00e4u\u00b7ben", "ab", ":", "und", "es", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$,", "KON", "VVFIN", "PTKVZ", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Wieder da, wie es sprang", "tokens": ["Wie\u00b7der", "da", ",", "wie", "es", "sprang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Aus des Geb\u00e4renden Stirn, ger\u00fcstet mit der \u00c4gide,", "tokens": ["Aus", "des", "Ge\u00b7b\u00e4\u00b7ren\u00b7den", "Stirn", ",", "ge\u00b7r\u00fcs\u00b7tet", "mit", "der", "\u00c4\u00b7gi\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Oder mit Kr\u00e4nzen geschm\u00fcckt.", "tokens": ["O\u00b7der", "mit", "Kr\u00e4n\u00b7zen", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.33": {"text": "Gl\u00fccklicher fiel dem Dichter sein Loos. Er wohnt an der Quelle,", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "fiel", "dem", "Dich\u00b7ter", "sein", "Loos", ".", "Er", "wohnt", "an", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Trinkt sie mit feurigem Durst,", "tokens": ["Trinkt", "sie", "mit", "feu\u00b7ri\u00b7gem", "Durst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "Sch\u00f6pfet dem Schnitter daraus, und bringt die labende Schale", "tokens": ["Sch\u00f6p\u00b7fet", "dem", "Schnit\u00b7ter", "da\u00b7raus", ",", "und", "bringt", "die", "la\u00b7ben\u00b7de", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Ihm in das Sonnengefild:", "tokens": ["Ihm", "in", "das", "Son\u00b7nen\u00b7ge\u00b7fild", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Oder leitet ihm zu in der Ulme Schatten die K\u00fchlung,", "tokens": ["O\u00b7der", "lei\u00b7tet", "ihm", "zu", "in", "der", "Ul\u00b7me", "Schat\u00b7ten", "die", "K\u00fch\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "Und vom Weste beweht.", "tokens": ["Und", "vom", "Wes\u00b7te", "be\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Gl\u00e4nzend ist, Krieger und K\u00f6nige, was ihr thatet, vielleicht auch", "tokens": ["Gl\u00e4n\u00b7zend", "ist", ",", "Krie\u00b7ger", "und", "K\u00f6\u00b7ni\u00b7ge", ",", "was", "ihr", "tha\u00b7tet", ",", "viel\u00b7leicht", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "NN", "KON", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADV"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Edel, o Wunder! so gar.", "tokens": ["E\u00b7del", ",", "o", "Wun\u00b7der", "!", "so", "gar", "."], "token_info": ["word", 