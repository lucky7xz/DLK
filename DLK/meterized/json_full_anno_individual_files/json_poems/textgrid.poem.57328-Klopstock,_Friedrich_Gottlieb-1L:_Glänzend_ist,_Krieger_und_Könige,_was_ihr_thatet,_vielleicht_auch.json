"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Alles, nachdem bey dem glimmernden Docht der Erz\u00e4hlende dunkel,", "tokens": ["Al\u00b7les", ",", "nach\u00b7dem", "bey", "dem", "glim\u00b7mern\u00b7den", "Docht", "der", "Er\u00b7z\u00e4h\u00b7len\u00b7de", "dun\u00b7kel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Oder dunkler es sah.", "tokens": ["O\u00b7der", "dunk\u00b7ler", "es", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Arme Krieger und K\u00f6nige, das ist also der Nachruhm,", "tokens": ["Ar\u00b7me", "Krie\u00b7ger", "und", "K\u00f6\u00b7ni\u00b7ge", ",", "das", "ist", "al\u00b7so", "der", "Nach\u00b7ruhm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "PDS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Der euch schlafen nicht liess?", "tokens": ["Der", "euch", "schla\u00b7fen", "nicht", "liess", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Euch verbot, an der Wissenschaft erfrischenden Quelle", "tokens": ["Euch", "ver\u00b7bot", ",", "an", "der", "Wis\u00b7sen\u00b7schaft", "er\u00b7fri\u00b7schen\u00b7den", "Quel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "mete