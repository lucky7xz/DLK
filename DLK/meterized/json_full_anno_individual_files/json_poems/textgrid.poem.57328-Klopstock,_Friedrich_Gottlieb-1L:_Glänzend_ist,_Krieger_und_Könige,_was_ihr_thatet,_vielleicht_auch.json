", "sie", "nicht", ",", "die", "Ge\u00b7schich\u00b7te", ";", "da", "spielt", "die", "Ver\u00b7wand\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Nicht, wie mit Thaten sie spielt.", "tokens": ["Nicht", ",", "wie", "mit", "Tha\u00b7ten", "sie", "spielt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Richter sehn die Fehle des Werks, die Sch\u00f6nheit: allein mehr,", "tokens": ["Rich\u00b7ter", "sehn", "die", "Feh\u00b7le", "des", "Werks", ",", "die", "Sch\u00f6n\u00b7heit", ":", "al\u00b7lein", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$.", "ADV", "ADV", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Andere nicht, denn es har.", "tokens": ["An\u00b7de\u00b7re", "nicht", ",", "denn", "es", "har", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$,", "KON", "PPER", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Richtelnde k\u00f6nnens mit Tadel best\u00e4uben, und Lobe; doch diess auch", "tokens": ["Rich\u00b7teln\u00b7de", "k\u00f6n\u00b7nens", "mit", "Ta\u00b7del", "be\u00b7st\u00e4u\u00b7ben", ",", "und", "Lo\u00b7be", ";", "doch", "diess", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "NN", "VVPP", "$,", "K