{"dta.poem.21255": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Ecloga.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198486", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verla\u00df einmal Elpin die Mauren, das Gedr\u00e4nge,", "tokens": ["Ver\u00b7la\u00df", "ein\u00b7mal", "El\u00b7pin", "die", "Mau\u00b7ren", ",", "das", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NE", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die eingesperrte Luft, des Sonnen-Lichtes Enge,", "tokens": ["Die", "ein\u00b7ge\u00b7sperr\u00b7te", "Luft", ",", "des", "Son\u00b7nen\u00b7Lich\u00b7tes", "En\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo die bem\u00fchte Kunst des Sch\u00f6pfers Werck verdringt,", "tokens": ["Wo", "die", "be\u00b7m\u00fch\u00b7te", "Kunst", "des", "Sch\u00f6p\u00b7fers", "Werck", "ver\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die Natur verkehrt in Mi\u00dfgestalten zwingt:", "tokens": ["Und", "die", "Na\u00b7tur", "ver\u00b7kehrt", "in", "Mi\u00df\u00b7ge\u00b7stal\u00b7ten", "zwingt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Komm auf das weite Land und schau\u2019, und h\u00f6r\u2019, und f\u00fchle-", "tokens": ["Komm", "auf", "das", "wei\u00b7te", "Land", "und", "schau'", ",", "und", "h\u00f6r'", ",", "und", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und rieche der Natur jungfr\u00e4ulich-edle Spiele,", "tokens": ["Und", "rie\u00b7che", "der", "Na\u00b7tur", "jung\u00b7fr\u00e4u\u00b7lich\u00b7ed\u00b7le", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die sanft, gelind und rein, in die geruhge Brust", "tokens": ["Die", "sanft", ",", "ge\u00b7lind", "und", "rein", ",", "in", "die", "ge\u00b7ruh\u00b7ge", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gesunde Regung f\u00fchrt, und ungemischte Lust;", "tokens": ["Ge\u00b7sun\u00b7de", "Re\u00b7gung", "f\u00fchrt", ",", "und", "un\u00b7ge\u00b7mischte", "Lust", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Wovon ich j\u00fcngst geh\u00f6rt den Sch\u00e4fer Hylas singen,", "tokens": ["Wo\u00b7von", "ich", "j\u00fcngst", "ge\u00b7h\u00f6rt", "den", "Sch\u00e4\u00b7fer", "Hy\u00b7las", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ART", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der an die Lindemag die Musen pflegt zu bringen.", "tokens": ["Der", "an", "die", "Lin\u00b7de\u00b7mag", "die", "Mu\u00b7sen", "pflegt", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jhr Gr\u00fcnde, fieng er an, ihr Matten, Busch, und Feld,", "tokens": ["Ihr", "Gr\u00fcn\u00b7de", ",", "fi\u00b7eng", "er", "an", ",", "ihr", "Mat\u00b7ten", ",", "Busch", ",", "und", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O Land, das alle Lust der Sinnen in sich h\u00e4lt;", "tokens": ["O", "Land", ",", "das", "al\u00b7le", "Lust", "der", "Sin\u00b7nen", "in", "sich", "h\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PIAT", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Winter hat zweymal verderbet euer Gl\u00e4nzen,", "tokens": ["Der", "Win\u00b7ter", "hat", "zwey\u00b7mal", "ver\u00b7der\u00b7bet", "eu\u00b7er", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zweymal sich euer Schoo\u00df verneut im frischen Lenzen,", "tokens": ["Zwey\u00b7mal", "sich", "eu\u00b7er", "Schoo\u00df", "ver\u00b7neut", "im", "fri\u00b7schen", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Seit da\u00df ich euch gemi\u00dft, wiewohl ich mit dem Leib", "tokens": ["Seit", "da\u00df", "ich", "euch", "ge\u00b7mi\u00dft", ",", "wie\u00b7wohl", "ich", "mit", "dem", "Leib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVPP", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey euch vorhanden war, seitdem ein sch\u00f6nes Weib,", "tokens": ["Bey", "euch", "vor\u00b7han\u00b7den", "war", ",", "seit\u00b7dem", "ein", "sch\u00f6\u00b7nes", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Seit Emma mich gehabt, und mich euch gantz entzogen.", "tokens": ["Seit", "Em\u00b7ma", "mich", "ge\u00b7habt", ",", "und", "mich", "euch", "gantz", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "VAPP", "$,", "KON", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Eur Glanz, und eure Lust war weg, sie war verflogen,", "tokens": ["Eur", "Glanz", ",", "und", "eu\u00b7re", "Lust", "war", "weg", ",", "sie", "war", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil ich ihr nicht geno\u00df; ich war in dieser Zeit", "tokens": ["Weil", "ich", "ihr", "nicht", "ge\u00b7no\u00df", ";", "ich", "war", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht euer, und nicht mein, von euch und mir sehr weit,", "tokens": ["Nicht", "eu\u00b7er", ",", "und", "nicht", "mein", ",", "von", "euch", "und", "mir", "sehr", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "$,", "KON", "PTKNEG", "PPOSAT", "$,", "APPR", "PPER", "KON", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So lang\u2019 auf Emmens Stirn das Jungfernkr\u00e4nzgen bl\u00fchte,", "tokens": ["So", "lang'", "auf", "Em\u00b7mens", "Stirn", "das", "Jung\u00b7fern\u00b7kr\u00e4nz\u00b7gen", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Scham, der Unschuld Bild, auf ihren Wangen gl\u00fchte;", "tokens": ["Und", "Scham", ",", "der", "Un\u00b7schuld", "Bild", ",", "auf", "ih\u00b7ren", "Wan\u00b7gen", "gl\u00fch\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So lang\u2019 ihr leichter Schwur bey mir Geh\u00f6re fand,", "tokens": ["So", "lang'", "ihr", "leich\u00b7ter", "Schwur", "bey", "mir", "Ge\u00b7h\u00f6\u00b7re", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJD", "VVFIN", "APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie h\u00e4tte mir ihr Hertz alleine zugewandt:", "tokens": ["Sie", "h\u00e4t\u00b7te", "mir", "ihr", "Hertz", "al\u00b7lei\u00b7ne", "zu\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nun da nicht meine Hand, wo bleibet ihr Gewissen,", "tokens": ["Nun", "da", "nicht", "mei\u00b7ne", "Hand", ",", "wo", "blei\u00b7bet", "ihr", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Seit Aegons wilde Hand das Kr\u00e4nzgen ihr zerrissen,", "tokens": ["Seit", "A\u00b7e\u00b7gons", "wil\u00b7de", "Hand", "das", "Kr\u00e4nz\u00b7gen", "ihr", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Den G\u00fcrtel aufgel\u00f6\u00dft, so bin ich wieder mein,", "tokens": ["Den", "G\u00fcr\u00b7tel", "auf\u00b7ge\u00b7l\u00f6\u00dft", ",", "so", "bin", "ich", "wie\u00b7der", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So bin ich wieder eur, ihr Matten, Feld und Hain!", "tokens": ["So", "bin", "ich", "wie\u00b7der", "eur", ",", "ihr", "Mat\u00b7ten", ",", "Feld", "und", "Hain", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gesicht, Geh\u00f6r, Geruch, Gef\u00fchle sind entbunden,", "tokens": ["Ge\u00b7sicht", ",", "Ge\u00b7h\u00f6r", ",", "Ge\u00b7ruch", ",", "Ge\u00b7f\u00fch\u00b7le", "sind", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Liebe schwartze Kunst zerst\u00f6rt und \u00fcberwunden.", "tokens": ["Der", "Lie\u00b7be", "schwart\u00b7ze", "Kunst", "zer\u00b7st\u00f6rt", "und", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Empfange dann, o Land, mich in den k\u00fchlen Raum", "tokens": ["Emp\u00b7fan\u00b7ge", "dann", ",", "o", "Land", ",", "mich", "in", "den", "k\u00fch\u00b7len", "Raum"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "FM", "NN", "$,", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schattenreichen Grund, wo Tann- und Fichten-Baum,", "tokens": ["Und", "schat\u00b7ten\u00b7rei\u00b7chen", "Grund", ",", "wo", "Tann", "und", "Fich\u00b7ten\u00b7Baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PWAV", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit dichtverschr\u00e4ncktem Ast, sich fest umschliessend gatten,", "tokens": ["Mit", "dicht\u00b7ver\u00b7schr\u00e4nck\u00b7tem", "Ast", ",", "sich", "fest", "um\u00b7schlies\u00b7send", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRF", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus freundlicher Begier in dem vereinten Schatten", "tokens": ["Aus", "freund\u00b7li\u00b7cher", "Be\u00b7gier", "in", "dem", "ver\u00b7ein\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Gast, der sie besucht, mit K\u00fchlung zu erfreun;", "tokens": ["Den", "Gast", ",", "der", "sie", "be\u00b7sucht", ",", "mit", "K\u00fch\u00b7lung", "zu", "er\u00b7freun", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Sonn\u2019 erhizte mich, doch mehr der Emma Schein,", "tokens": ["Die", "Sonn'", "er\u00b7hiz\u00b7te", "mich", ",", "doch", "mehr", "der", "Em\u00b7ma", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "ADV", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allhier ist K\u00fchlung gnug die Glieder zu erlaben,", "tokens": ["All\u00b7hier", "ist", "K\u00fch\u00b7lung", "gnug", "die", "Glie\u00b7der", "zu", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die in der Liebes-Glut sich ausgezehret haben.", "tokens": ["Die", "in", "der", "Lie\u00b7bes\u00b7Glut", "sich", "aus\u00b7ge\u00b7zeh\u00b7ret", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}