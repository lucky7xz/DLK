{"dta.poem.16122": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "176.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen Tugenden ist Scham genannt mit Recht", "tokens": ["Von", "al\u00b7len", "Tu\u00b7gen\u00b7den", "ist", "Scham", "ge\u00b7nannt", "mit", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Mutter, keine hat so bl\u00fchend ein Geschlecht.", "tokens": ["Die", "Mut\u00b7ter", ",", "kei\u00b7ne", "hat", "so", "bl\u00fc\u00b7hend", "ein", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Tugendmutter, Sohn, sie ehre, wie du ehrst", "tokens": ["Die", "Tu\u00b7gend\u00b7mut\u00b7ter", ",", "Sohn", ",", "sie", "eh\u00b7re", ",", "wie", "du", "ehrst"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eigne Mutter, der du nie den R\u00fccken kehrst.", "tokens": ["Die", "eig\u00b7ne", "Mut\u00b7ter", ",", "der", "du", "nie", "den", "R\u00fc\u00b7cken", "kehrst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Solange du sie hast vor Augen, lieber Sohn,", "tokens": ["So\u00b7lan\u00b7ge", "du", "sie", "hast", "vor", "Au\u00b7gen", ",", "lie\u00b7ber", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VAFIN", "APPR", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du unw\u00fcrdigen Versuchungen entflohn.", "tokens": ["Bist", "du", "un\u00b7w\u00fcr\u00b7di\u00b7gen", "Ver\u00b7su\u00b7chun\u00b7gen", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}