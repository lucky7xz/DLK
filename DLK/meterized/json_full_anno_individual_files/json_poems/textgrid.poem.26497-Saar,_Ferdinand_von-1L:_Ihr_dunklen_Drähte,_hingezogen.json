{"textgrid.poem.26497": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr dunklen Dr\u00e4hte, hingezogen", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr dunklen Dr\u00e4hte, hingezogen", "tokens": ["Ihr", "dunk\u00b7len", "Dr\u00e4h\u00b7te", ",", "hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So weit mein Aug' zur Ferne schweift,", "tokens": ["So", "weit", "mein", "Aug'", "zur", "Fer\u00b7ne", "schweift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie t\u00f6nt ihr, wenn der L\u00fcfte Wogen", "tokens": ["Wie", "t\u00f6nt", "ihr", ",", "wenn", "der", "L\u00fcf\u00b7te", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In euch so wie in Saiten greift!", "tokens": ["In", "euch", "so", "wie", "in", "Sai\u00b7ten", "greift", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "KOKOM", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O welch' ein seltsam leises Klingen,", "tokens": ["O", "welch'", "ein", "selt\u00b7sam", "lei\u00b7ses", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"},