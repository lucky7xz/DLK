{"dta.poem.9928": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob ich gleich zu hause bin/ wei\u00df ich doch was sonst geschiehet/", "tokens": ["Ob", "ich", "gleich", "zu", "hau\u00b7se", "bin", "/", "wei\u00df", "ich", "doch", "was", "sonst", "ge\u00b7schie\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "$(", "VVFIN", "PPER", "ADV", "PWS", "ADV", "VVPP", "$("], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was viel hundert meile\u0303 weit vorgeht/ man durch mich ersihet/", "tokens": ["Was", "viel", "hun\u00b7dert", "meil\u1ebd", "weit", "vor\u00b7geht", "/", "man", "durch", "mich", "er\u00b7si\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "CARD", "ADV", "ADJD", "VVFIN", "$(", "PIS", "APPR", "PPER", "VVFIN", "$("], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vielen den gef\u00e4llt mein reden/ wenigen giebt es verdru\u00df:", "tokens": ["Vie\u00b7len", "den", "ge\u00b7f\u00e4llt", "mein", "re\u00b7den", "/", "we\u00b7ni\u00b7gen", "giebt", "es", "ver\u00b7dru\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PPOSAT", "VVINF", "$(", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ich darf di\u00df nur unterscheiden/ was man sag- und schweigen", "tokens": ["Ich", "darf", "di\u00df", "nur", "un\u00b7ter\u00b7schei\u00b7den", "/", "was", "man", "sag", "und", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDS", "ADV", "VVINF", "$(", "PWS", "PIS", "TRUNC", "KON", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "mu\u00df.", "tokens": ["mu\u00df", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}}}}}