{"dta.poem.21627": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Die welke Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df, du wirst auch sie vergessen,", "tokens": ["Ge\u00b7wi\u00df", ",", "du", "wirst", "auch", "sie", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ihn, der dir die Rose gab \u2014", "tokens": ["Wie", "ihn", ",", "der", "dir", "die", "Ro\u00b7se", "gab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die arme kleine Leiche ruhet", "tokens": ["Die", "ar\u00b7me", "klei\u00b7ne", "Lei\u00b7che", "ru\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann still in dieses Buches Grab.", "tokens": ["Dann", "still", "in", "die\u00b7ses", "Bu\u00b7ches", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch lockt es dich durch diese Bl\u00e4tter,", "tokens": ["Doch", "lockt", "es", "dich", "durch", "die\u00b7se", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch deinen Friedhof hinzugehn,", "tokens": ["Durch", "dei\u00b7nen", "Fried\u00b7hof", "hin\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df, dann wird die Blumenleiche", "tokens": ["Ge\u00b7wi\u00df", ",", "dann", "wird", "die", "Blu\u00b7men\u00b7lei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Duft und Jugend auferstehn;", "tokens": ["In", "Duft", "und", "Ju\u00b7gend", "auf\u00b7er\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann wirst du weinend ihn erblicken", "tokens": ["Dann", "wirst", "du", "wei\u00b7nend", "ihn", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sie dir einst beim Scheiden gab,", "tokens": ["Der", "sie", "dir", "einst", "beim", "Schei\u00b7den", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leise, leise wirst du beten", "tokens": ["Und", "lei\u00b7se", ",", "lei\u00b7se", "wirst", "du", "be\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als knietest du an seinem Grab.", "tokens": ["Als", "knie\u00b7test", "du", "an", "sei\u00b7nem", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}