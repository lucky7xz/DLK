{"dta.poem.21839": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  Das kranke Buschgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Buschgen henkt' ihr H\u00e4uptchen nieder", "tokens": ["Buschgen", "henkt'", "ihr", "H\u00e4upt\u00b7chen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und lie\u00df herbe Seuffzer gehn/", "tokens": ["und", "lie\u00df", "her\u00b7be", "Seuff\u00b7zer", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "die sonst muntern Augen-lieder", "tokens": ["die", "sonst", "mun\u00b7tern", "Au\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hatte sie halb offen stehn/", "tokens": ["hat\u00b7te", "sie", "halb", "of\u00b7fen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wie sie die verzukkten mahlen", "tokens": ["wie", "sie", "die", "ver\u00b7zukk\u00b7ten", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "in Dionen Saalen.", "tokens": ["in", "Di\u00b7o\u00b7nen", "Saa\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der gehemmte Pul\u00df der H\u00e4nde", "tokens": ["Der", "ge\u00b7hemm\u00b7te", "Pul\u00df", "der", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie/ wenn einer nah\u2019 am Ende", "tokens": ["wie", "/", "wenn", "ei\u00b7ner", "nah'", "am", "En\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$(", "KOUS", "PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "kaum das Herze r\u00fchren kan.", "tokens": ["kaum", "das", "Her\u00b7ze", "r\u00fch\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhrer Wangen Rosen blichen", "tokens": ["Ih\u00b7rer", "Wan\u00b7gen", "Ro\u00b7sen", "bli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geist und w\u00e4rme wichen.", "tokens": ["Geist", "und", "w\u00e4r\u00b7me", "wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Seban hatte sich geleget", "tokens": ["Se\u00b7ban", "hat\u00b7te", "sich", "ge\u00b7le\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "auff der liebsten Doris Mund", "tokens": ["auff", "der", "liebs\u00b7ten", "Do\u00b7ris", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und/ wie ein Verliebter pfleget/", "tokens": ["und", "/", "wie", "ein", "Ver\u00b7lieb\u00b7ter", "pfle\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "als/ dehm alles war vergunnt/", "tokens": ["als", "/", "dehm", "al\u00b7les", "war", "ver\u00b7gunnt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PRELS", "PIS", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "durfft\u2019 er sich mit tausend Schm\u00e4zzen", "tokens": ["durfft'", "er", "sich", "mit", "tau\u00b7send", "Schm\u00e4z\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich vermerkte bald die Kreide/", "tokens": ["Ich", "ver\u00b7merk\u00b7te", "bald", "die", "Krei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df di\u00df treu-verliebte Paar/", "tokens": ["da\u00df", "di\u00df", "treu\u00b7ver\u00b7lieb\u00b7te", "Paar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zu des Buschgen stillem Leide", "tokens": ["zu", "des", "Buschgen", "stil\u00b7lem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "einig nur der Anla\u00df war/", "tokens": ["ei\u00b7nig", "nur", "der", "An\u00b7la\u00df", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "drum wolt\u2019 ich sie gleicher massen", "tokens": ["drum", "wolt'", "ich", "sie", "glei\u00b7cher", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "k\u00fcssend auch umfassen", "tokens": ["k\u00fcs\u00b7send", "auch", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Bald ward ihr Gesichte helle/", "tokens": ["Bald", "ward", "ihr", "Ge\u00b7sich\u00b7te", "hel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJA", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "r\u00f6tlich ihrer Wangen Saal/", "tokens": ["r\u00f6t\u00b7lich", "ih\u00b7rer", "Wan\u00b7gen", "Saal", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Muht und Leben kam zur Stelle", "tokens": ["Muht", "und", "Le\u00b7ben", "kam", "zur", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "doch erseuffzt sie noch einmahl/", "tokens": ["doch", "er\u00b7seuffzt", "sie", "noch", "ein\u00b7mahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "dieses machte/ da\u00df ich fragte", "tokens": ["die\u00b7ses", "mach\u00b7te", "/", "da\u00df", "ich", "frag\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was sie heimlich plagte.", "tokens": ["was", "sie", "heim\u00b7lich", "plag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Nichts nicht (sprach sie) mich betr\u00fcbet/", "tokens": ["Nichts", "nicht", "(", "sprach", "sie", ")", "mich", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$(", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df ich nicht zu Hause bin.", "tokens": ["da\u00df", "ich", "nicht", "zu", "Hau\u00b7se", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Mutter/ die mich liebet/", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "/", "die", "mich", "lie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kr\u00e4nket sich in ihrem Sinn/", "tokens": ["kr\u00e4n\u00b7ket", "sich", "in", "ih\u00b7rem", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn allein ich ohn begleiten", "tokens": ["wenn", "al\u00b7lein", "ich", "ohn", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "geh bey sp\u00e4ten Zeiten.", "tokens": ["geh", "bey", "sp\u00e4\u00b7ten", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "T\u00f6hricht m\u00fcst\u2019 ich sein gewesen/", "tokens": ["T\u00f6h\u00b7richt", "m\u00fcst'", "ich", "sein", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "VAPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn ich nicht errahten solt\u2019", "tokens": ["wenn", "ich", "nicht", "er\u00b7rah\u00b7ten", "solt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ihre Krankheit und genesen/", "tokens": ["ih\u00b7re", "Krank\u00b7heit", "und", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVPP", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "und was sie von mir gewolt/", "tokens": ["und", "was", "sie", "von", "mir", "ge\u00b7wolt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch verbarg ich di\u00df mein wissen", "tokens": ["Doch", "ver\u00b7barg", "ich", "di\u00df", "mein", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDS", "PPOSAT", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mit geh\u00e4ufften K\u00fcssen.", "tokens": ["mit", "ge\u00b7h\u00e4uff\u00b7ten", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Unter diesen Liebes-Freuden", "tokens": ["Un\u00b7ter", "die\u00b7sen", "Lie\u00b7bes\u00b7Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "fing ich sachtlich zu ihr an:", "tokens": ["fing", "ich", "sacht\u00b7lich", "zu", "ihr", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nes Kind/ ach! da\u00df uns beyden:", "tokens": ["Sch\u00f6\u00b7nes", "Kind", "/", "ach", "!", "da\u00df", "uns", "bey\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "XY", "$.", "KOUS", "PPER", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "gleiche Lust nicht werden kan", "tokens": ["glei\u00b7che", "Lust", "nicht", "wer\u00b7den", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKNEG", "VAINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die den zwey Verliebten heute", "tokens": ["die", "den", "zwey", "Ver\u00b7lieb\u00b7ten", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "CARD", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Venus schenkt zur Beute.", "tokens": ["Ve\u00b7nus", "schenkt", "zur", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn es Gottes Wille w\u00e4re", "tokens": ["Wenn", "es", "Got\u00b7tes", "Wil\u00b7le", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "w\u00fcrde bald gemacht der Kauff/", "tokens": ["w\u00fcr\u00b7de", "bald", "ge\u00b7macht", "der", "Kauff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ich ensage keiner Ehre", "tokens": ["ich", "en\u00b7sa\u00b7ge", "kei\u00b7ner", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "gab sie mir zur Antwort drauff.", "tokens": ["gab", "sie", "mir", "zur", "Ant\u00b7wort", "drauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so ward di\u00df scheinsam Lieben", "tokens": ["Und", "so", "ward", "di\u00df", "schein\u00b7sam", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PDS", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "k\u00fcssend unterschrieben.", "tokens": ["k\u00fcs\u00b7send", "un\u00b7ter\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Doch/ was kunnte das verschlagen/", "tokens": ["Doch", "/", "was", "kunn\u00b7te", "das", "ver\u00b7schla\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "VMFIN", "PDS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcssen leschet nicht genug.", "tokens": ["K\u00fcs\u00b7sen", "le\u00b7schet", "nicht", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhre Schwermuht abzutragen", "tokens": ["Ih\u00b7re", "Schwer\u00b7muht", "ab\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "war auch hier nicht Zeit noch Fug/", "tokens": ["war", "auch", "hier", "nicht", "Zeit", "noch", "Fug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "NN", "ADV", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "weil man bey der Lichten brandte", "tokens": ["weil", "man", "bey", "der", "Lich\u00b7ten", "brand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "uns zu sehr erkandte.", "tokens": ["uns", "zu", "sehr", "er\u00b7kand\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Magd/ wo bleibstu doch so lange", "tokens": ["Magd", "/", "wo", "bleibs\u00b7tu", "doch", "so", "lan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PWAV", "VVFIN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm/ und z\u00fcnd die Fakkel an/", "tokens": ["Komm", "/", "und", "z\u00fcnd", "die", "Fak\u00b7kel", "an", "/"], "token_info": ["word", "punct", "word",