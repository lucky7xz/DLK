{"textgrid.poem.47185": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war der Mann, dein Vater, du mein Kind;", "tokens": ["Ich", "war", "der", "Mann", ",", "dein", "Va\u00b7ter", ",", "du", "mein", "Kind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich gebot, gehorchtest du geschwind,", "tokens": ["Wo", "ich", "ge\u00b7bot", ",", "ge\u00b7horch\u00b7test", "du", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du hingest ab vom Winke dieser Hand,", "tokens": ["Du", "hin\u00b7gest", "ab", "vom", "Win\u00b7ke", "die\u00b7ser", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ganz unm\u00f6glich war dir Widerstand.", "tokens": ["Und", "ganz", "un\u00b7m\u00f6g\u00b7lich", "war", "dir", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun bist du meiner v\u00e4terlichen Macht", "tokens": ["Nun", "bist", "du", "mei\u00b7ner", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Macht"], "token_info": ["word", "word", "word", "word", "w