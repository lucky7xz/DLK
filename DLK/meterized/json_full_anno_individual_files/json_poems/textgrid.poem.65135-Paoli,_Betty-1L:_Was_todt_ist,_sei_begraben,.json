 ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Bund, um den wir wissen,", "tokens": ["Der", "Bund", ",", "um", "den", "wir", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch steht er fest wie Erz!", "tokens": ["Noch", "steht", "er", "fest", "wie", "Erz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gebrochen und zerrissen", "tokens": ["Ge\u00b7bro\u00b7chen", "und", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist nichts als nur mein Herz!", "tokens": ["Ist", "nichts", "als", "nur", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was todt ist, sei begraben,", "tokens": ["Was", "todt", "ist", ",", "sei", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verlor'nes sei dahin!", "tokens": ["Ver\u00b7lor'\u00b7nes", "sei", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir bleibt, mich d'ran zu laben,", "tokens": ["Mir", "bleibt", ",", "mich", "d'\u00b7ran", "zu", "la\u00b7ben", ","], "token_info": ["word", "word"