{"textgrid.poem.65135": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was todt ist, sei begraben,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was todt ist, sei begraben,", "tokens": ["Was", "todt", "ist", ",", "sei", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verlor'nes sei dahin!", "tokens": ["Ver\u00b7lor'\u00b7nes", "sei", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir bleibt, mich d'ran zu laben,", "tokens": ["Mir", "bleibt", ",", "mich", "d'\u00b7ran", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRF", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein ewiger Gewinn!", "tokens": ["Ein", "e\u00b7wi\u00b7ger", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Frei schwingt sich mein Gedanke", "tokens": ["Frei", "schwingt", "sich", "mein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu dir, du meine Welt!", "tokens": ["Zu", "dir", ",", "du", "mei\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und spottet jeder Schranke,", "tokens": ["Und", "spot\u00b7tet", "je\u00b7der", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die zwischen uns sich stellt!", "tokens": ["Die", "zwi\u00b7schen", "uns", "sich", "stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "L\u00fcgt auch die Welt des Scheines,", "tokens": ["L\u00fcgt", "auch", "die", "Welt", "des", "Schei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verloren h\u00e4tt' ich dich,", "tokens": ["Ver\u00b7lo\u00b7ren", "h\u00e4tt'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind ein ewig Eines,", "tokens": ["Wir", "sind", "ein", "e\u00b7wig", "Ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier gilt kein Du und Ich!", "tokens": ["Hier", "gilt", "kein", "Du", "und", "Ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier ward nach eig'nem Sinne", "tokens": ["Hier", "ward", "nach", "eig'\u00b7nem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gepr\u00fcft nicht und gew\u00e4hlt,", "tokens": ["Ge\u00b7pr\u00fcft", "nicht", "und", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KON", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Uns haben vom Beginne,", "tokens": ["Uns", "ha\u00b7ben", "vom", "Be\u00b7gin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gott und Natur verm\u00e4hlt!", "tokens": ["Gott", "und", "Na\u00b7tur", "ver\u00b7m\u00e4hlt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Das ist's, was stets auf's Neue", "tokens": ["Das", "ist's", ",", "was", "stets", "auf's", "Neu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entfacht den alten Brand,", "tokens": ["Ent\u00b7facht", "den", "al\u00b7ten", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was, trotz gebroch'ner Treue,", "tokens": ["Was", ",", "trotz", "ge\u00b7bro\u00b7ch'\u00b7ner", "Treu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Noch Seel' an Seele bannt,", "tokens": ["Noch", "Seel'", "an", "See\u00b7le", "bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Obsiegend