{"dta.poem.1129": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "15. Die Sch\u00f6ne Zerrissene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es mag sich wer da will in stoltzen Purpur kleiden/ ", "tokens": ["Es", "mag", "sich", "wer", "da", "will", "in", "stolt\u00b7zen", "Pur\u00b7pur", "klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PWS", "ADV", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In theurer W\u00fcrme Grab lebendig h\u00fcllen ein:", "tokens": ["In", "theu\u00b7rer", "W\u00fcr\u00b7me", "Grab", "le\u00b7ben\u00b7dig", "h\u00fcl\u00b7len", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du sch\u00f6ne Bettlerin kanst nackend sch\u00f6ner seyn/", "tokens": ["Du", "sch\u00f6\u00b7ne", "Bett\u00b7le\u00b7rin", "kanst", "na\u00b7ckend", "sch\u00f6\u00b7ner", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "ADJD", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Darffst fremder Hoffart Schmuck im wenigsten beneiden.", "tokens": ["Darffst", "frem\u00b7der", "Hof\u00b7fart", "Schmuck", "im", "we\u00b7nigs\u00b7ten", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "NN", "APPRART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Die Sonne/ wenn sich izt die dicken Wolcken scheiden/", "tokens": ["Die", "Son\u00b7ne", "/", "wenn", "sich", "izt", "die", "di\u00b7cken", "Wol\u00b7cken", "schei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Giebt durch zuri\u00dfnen Flor den angenehmen Schein;", "tokens": ["Giebt", "durch", "zu\u00b7ri\u00df\u00b7nen", "Flor", "den", "an\u00b7ge\u00b7neh\u00b7men", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hier kan die brennende Begier am Helffenbein", "tokens": ["Hier", "kan", "die", "bren\u00b7nen\u00b7de", "Be\u00b7gier", "am", "Helf\u00b7fen\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Glieder hin und her mit Lust die Augen weyden.", "tokens": ["Der", "Glie\u00b7der", "hin", "und", "her", "mit", "Lust", "die", "Au\u00b7gen", "wey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADV", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Lust-Hau\u00df deiner Brust/ der Garten deiner Scho\u00df/", "tokens": ["Das", "Lust\u00b7Hau\u00df", "dei\u00b7ner", "Brust", "/", "der", "Gar\u00b7ten", "dei\u00b7ner", "Scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit Lumpen nur umschr\u00e4nckt/ umz\u00e4unt mit Spinnen-We-", "tokens": ["Mit", "Lum\u00b7pen", "nur", "um\u00b7schr\u00e4nckt", "/", "um\u00b7z\u00e4unt", "mit", "Spin\u00b7nen\u00b7We"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "$(", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ben/ ", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Giebt uns den reichen Schmuck von tausend Blumen blo\u00df.", "tokens": ["Giebt", "uns", "den", "rei\u00b7chen", "Schmuck", "von", "tau\u00b7send", "Blu\u00b7men", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So pflagen sich in Streit die K\u00e4mpffer zu begeben/", "tokens": ["So", "pfla\u00b7gen", "sich", "in", "Streit", "die", "K\u00e4mpf\u00b7fer", "zu", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "(di\u00df Kleid geht auch der Lieb\u2019 am besten an die Hand/)", "tokens": ["(", "di\u00df", "Kleid", "geht", "auch", "der", "Lieb'", "am", "bes\u00b7ten", "an", "die", "Hand", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "NN", "VVFIN", "ADV", "ART", "NN", "PTKA", "ADJD", "APPR", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "An Sieges-Kr\u00e4utzen reich und d\u00fcrfftig an Gewand.", "tokens": ["An", "Sie\u00b7ges\u00b7Kr\u00e4ut\u00b7zen", "reich", "und", "d\u00fcr\u00b7ff\u00b7tig", "an", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}