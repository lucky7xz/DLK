{"dta.poem.4388": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An   S. T.  \n  Herrn F. von Hagedorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geist, Den jeder, doch ich mehr, als Jhn jeder ehret,", "tokens": ["Geist", ",", "Den", "je\u00b7der", ",", "doch", "ich", "mehr", ",", "als", "Jhn", "je\u00b7der", "eh\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "PIS", "$,", "KON", "PPER", "ADV", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ehre.", "tokens": ["eh\u00b7re", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Freund, Dem ein so redlichs Herz die gelehrte Brust be-", "tokens": ["Freund", ",", "Dem", "ein", "so", "red\u00b7lichs", "Herz", "die", "ge\u00b7lehr\u00b7te", "Brust", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PDS", "ART", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "lebt,", "tokens": ["lebt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Da\u00df Dich dessen Eigenschaft mehr, als jener noch, erhebt,", "tokens": ["Da\u00df", "Dich", "des\u00b7sen", "Ei\u00b7gen\u00b7schaft", "mehr", ",", "als", "je\u00b7ner", "noch", ",", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "ADV", "$,", "KOUS", "PDS", "ADV", "$,", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Welches, zeigtest Du es nicht, sonst kaum zu begreifen", "tokens": ["Wel\u00b7ches", ",", "zeig\u00b7test", "Du", "es", "nicht", ",", "sonst", "kaum", "zu", "be\u00b7grei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "w\u00e4re.", "tokens": ["w\u00e4\u00b7re", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Unsers unnachahmbar\u2019n ", "tokens": ["Un\u00b7sers", "un\u00b7nach\u00b7ahm\u00b7ba\u00b7r'n"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "St\u00fcck,Ein alter Frauen-Kopf.", "tokens": ["St\u00fcck", ",", "Ein", "al\u00b7ter", "Frau\u00b7en\u00b7Kopf", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Das Dein Bruder schildern lie\u00df, send ich Dir hiebey", "tokens": ["Das", "Dein", "Bru\u00b7der", "schil\u00b7dern", "lie\u00df", ",", "send", "ich", "Dir", "hie\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "zur\u00fcck.", "tokens": ["zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Wenn Du Jhm es \u00fcberschickest, meld Jhm unbeschwehrt", "tokens": ["Wenn", "Du", "Jhm", "es", "\u00fc\u00b7bersc\u00b7hi\u00b7ckest", ",", "meld", "Jhm", "un\u00b7be\u00b7schwehrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "dabey:", "tokens": ["da\u00b7bey", ":"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Da\u00df, in meinem Geist, von Jhm ein Jhm \u00e4hnlich treflichs", "tokens": ["Da\u00df", ",", "in", "mei\u00b7nem", "Geist", ",", "von", "Jhm", "ein", "Jhm", "\u00e4hn\u00b7lich", "tref\u00b7lichs"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPER", "ART", "PPER", "ADJD", "ADJA"], "meter": "+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.15": {"text": "Bild,", "tokens": ["Bild", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Durch Sein sonderbar Verdienst, kr\u00e4ftig abgeschildert", "tokens": ["Durch", "Sein", "son\u00b7der\u00b7bar", "Ver\u00b7dienst", ",", "kr\u00e4f\u00b7tig", "ab\u00b7ge\u00b7schil\u00b7dert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "sey.", "tokens": ["sey", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.18": {"text": "In dem Schildereyen-Saal, den Er Selber angef\u00fcllt", "tokens": ["In", "dem", "Schil\u00b7de\u00b7reyen\u00b7Saal", ",", "den", "Er", "Sel\u00b7ber", "an\u00b7ge\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "NN", "VVPP"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Mit so manchem Farben-Wunder, die uns Welsch- und", "tokens": ["Mit", "so", "man\u00b7chem", "Fa\u00b7rben\u00b7Wun\u00b7der", ",", "die", "uns", "Wel\u00b7sch", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "TRUNC", "KON"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "Holland schenket,", "tokens": ["Hol\u00b7land", "schen\u00b7ket", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Seh ich Jhn im Geist spatzieren, wie Er Seine Blicke", "tokens": ["Seh", "ich", "Jhn", "im", "Geist", "spat\u00b7zie\u00b7ren", ",", "wie", "Er", "Sei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.22": {"text": "lenket", "tokens": ["len\u00b7ket"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Auf der K\u00fcnstler kluge Werke, doch noch mehr auf die", "tokens": ["Auf", "der", "K\u00fcnst\u00b7ler", "klu\u00b7ge", "Wer\u00b7ke", ",", "doch", "noch", "mehr", "auf", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "APPR", "ART"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Natur,", "tokens": ["Na\u00b7tur", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Als der Wunder Wunder-Quell. K\u00f6nnt\u2019 Euch Euer", "tokens": ["Als", "der", "Wun\u00b7der", "Wun\u00b7der\u00b7Quell", ".", "K\u00f6nnt'", "Euch", "Eu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "$.", "VMFIN", "PPER", "PPOSAT"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Vater sehen,", "tokens": ["Va\u00b7ter", "se\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Dessen Treue D\u00e4nnemark noch bem\u00fcht ist zu erh\u00f6hen,", "tokens": ["Des\u00b7sen", "Treu\u00b7e", "D\u00e4n\u00b7ne\u00b7mark", "noch", "be\u00b7m\u00fcht", "ist", "zu", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NE", "ADV", "VVPP", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie das edle Paar der S\u00f6hne Jhn, durch Jhr Verdienst,", "tokens": ["Wie", "das", "ed\u00b7le", "Paar", "der", "S\u00f6h\u00b7ne", "Jhn", ",", "durch", "Ihr", "Ver\u00b7dienst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "noch ehrt;", "tokens": ["noch", "ehrt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "W\u00fcrde, glaub ich, Sein Vergn\u00fcgen, w\u00e4r es m\u00f6glich,", "tokens": ["W\u00fcr\u00b7de", ",", "glaub", "ich", ",", "Sein", "Ver\u00b7gn\u00fc\u00b7gen", ",", "w\u00e4r", "es", "m\u00f6g\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "noch vermehrt.", "tokens": ["noch", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "F\u00fcge Deinem w\u00fcrd\u2019gen Bruder dieses noch hinzu, und", "tokens": ["F\u00fc\u00b7ge", "Dei\u00b7nem", "w\u00fcrd'\u00b7gen", "Bru\u00b7der", "die\u00b7ses", "noch", "hin\u00b7zu", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PDAT", "ADV", "PTKVZ", "$,", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "setze:", "tokens": ["set\u00b7ze", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df ich darinn Seinen Stand, und Sein Gl\u00fcck vollkom-", "tokens": ["Da\u00df", "ich", "da\u00b7rinn", "Sei\u00b7nen", "Stand", ",", "und", "Sein", "Gl\u00fcck", "voll\u00b7kom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "men sch\u00e4tze,", "tokens": ["men", "sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und Jhn fast darum beneide; da Er dem an Huld und", "tokens": ["Und", "Jhn", "fast", "da\u00b7rum", "be\u00b7nei\u00b7de", ";", "da", "Er", "dem", "an", "Huld", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "PAV", "VVFIN", "$.", "KOUS", "PPER", "ART", "APPR", "NN", "KON"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Macht", "tokens": ["Macht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "So vollkommnen K\u00f6nig dient, da\u00df Er auch die kluge", "tokens": ["So", "voll\u00b7komm\u00b7nen", "K\u00f6\u00b7nig", "dient", ",", "da\u00df", "Er", "auch", "die", "klu\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Die Verdienste, das Erhabne, das Gef\u00e4llige, die G\u00fcte,", "tokens": ["Die", "Ver\u00b7diens\u00b7te", ",", "das", "Er\u00b7hab\u00b7ne", ",", "das", "Ge\u00b7f\u00e4l\u00b7li\u00b7ge", ",", "die", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Den durchdringenden Verstand, das liebreizende Gem\u00fchte,", "tokens": ["Den", "durch\u00b7drin\u00b7gen\u00b7den", "Ver\u00b7stand", ",", "das", "lieb\u00b7rei\u00b7zen\u00b7de", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-++---+-", "measure": "unknown.measure.septa"}, "line.11": {"text": "Dessen, Den die Tugend selber l\u00e4ngstens ehrt und lieb", "tokens": ["Des\u00b7sen", ",", "Den", "die", "Tu\u00b7gend", "sel\u00b7ber", "l\u00e4ngs\u00b7tens", "ehrt", "und", "lieb"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "ART", "ART", "NN", "ADV", "ADV", "VVFIN", "KON", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "gewann,", "tokens": ["ge\u00b7wann", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "In dem grossen Grafen ", "tokens": ["In", "dem", "gros\u00b7sen", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}