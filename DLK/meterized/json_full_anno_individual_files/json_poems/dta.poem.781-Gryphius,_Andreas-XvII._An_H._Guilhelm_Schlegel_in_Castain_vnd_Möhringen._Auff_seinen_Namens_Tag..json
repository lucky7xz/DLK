{"dta.poem.781": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XvII.  \n An H.  Guilhelm  Schlegel/ in Castain vnd  \n M\u00f6hringen. Auff seinen Namens Tag.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der/ welchen Euch ein Hertz/ vnd eine Trew verbunden/ ", "tokens": ["Der", "/", "wel\u00b7chen", "Euch", "ein", "Hertz", "/", "vnd", "ei\u00b7ne", "Trew", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAT", "PPER", "ART", "NN", "$(", "KON", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den jhr zwar vnerkennt/ doch mehr denn hoch gelibt/", "tokens": ["Den", "jhr", "zwar", "vner\u00b7kennt", "/", "doch", "mehr", "denn", "hoch", "ge\u00b7libt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$(", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Gibt/ weil bey grawen frost Euch/ Franckreich Blumen", "tokens": ["Gibt", "/", "weil", "bey", "gra\u00b7wen", "frost", "Euch", "/", "Fran\u00b7ck\u00b7reich", "Blu\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$(", "KOUS", "APPR", "NE", "VVFIN", "PPER", "$(", "NE", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "gibt/", "tokens": ["gibt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Was weniger anitzt/ al\u00df Blumen wird gefunden.", "tokens": ["Was", "we\u00b7ni\u00b7ger", "a\u00b7nitzt", "/", "al\u00df", "Blu\u00b7men", "wird", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "$(", "KOUS", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die pracht der alten welt/ ist mit der welt verschwunden:", "tokens": ["Die", "pracht", "der", "al\u00b7ten", "welt", "/", "ist", "mit", "der", "welt", "ver\u00b7schwun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hoheit wird durch fall vnd grimmen sturm betr\u00fcbt", "tokens": ["Die", "Ho\u00b7heit", "wird", "durch", "fall", "vnd", "grim\u00b7men", "sturm", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "VVFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Zeit hat gantze Reich/ al\u00df Asch\u2019 in wind gesiebt/", "tokens": ["Die", "Zeit", "hat", "gant\u00b7ze", "Reich", "/", "al\u00df", "Asch'", "in", "wind", "ge\u00b7siebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$(", "KOUS", "NN", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was tausend Jahre baw\u2019n/ vergeht in wenig stunden.", "tokens": ["Was", "tau\u00b7send", "Jah\u00b7re", "ba\u00b7w'n", "/", "ver\u00b7geht", "in", "we\u00b7nig", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "VVINF", "$(", "VVFIN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Nichts ist ohn seinen todt/ nur trewe gunst besteht", "tokens": ["Nichts", "ist", "ohn", "sei\u00b7nen", "todt", "/", "nur", "tre\u00b7we", "gunst", "be\u00b7steht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "ADJD", "$(", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Pergamus in glutt/ vnd Rom in staub vergeht/", "tokens": ["Wenn", "Per\u00b7ga\u00b7mus", "in", "glutt", "/", "vnd", "Rom", "in", "staub", "ver\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJD", "$(", "KON", "NE", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Staub der Tyber last/ das gauckelspiel der winde.", "tokens": ["In", "Staub", "der", "Ty\u00b7ber", "last", "/", "das", "gau\u00b7ckel\u00b7spiel", "der", "win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$(", "PDS", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie kennt was ewig ist/ vnd pocht der gr\u00e4ber noth/", "tokens": ["Sie", "kennt", "was", "e\u00b7wig", "ist", "/", "vnd", "pocht", "der", "gr\u00e4\u00b7ber", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADJD", "VAFIN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht Menschen/ G\u00f6ttern gleich/ sie macht den Todt ohn", "tokens": ["Macht", "Men\u00b7schen", "/", "G\u00f6t\u00b7tern", "gleich", "/", "sie", "macht", "den", "Todt", "ohn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$(", "NN", "ADV", "$(", "PPER", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "todt.", "tokens": ["todt", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Durch sie lebt was nur lebt: die ists mit der ich binde.", "tokens": ["Durch", "sie", "lebt", "was", "nur", "lebt", ":", "die", "ists", "mit", "der", "ich", "bin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PWS", "ADV", "VVFIN", "$.", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}