{"textgrid.poem.48716": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Als ihn einer seiner vertrautesten Freunde angebunden hatte an denselben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was werd' ich weiter tun? Ich soll, ich mu\u00df, ich will", "tokens": ["Was", "werd'", "ich", "wei\u00b7ter", "tun", "?", "Ich", "soll", ",", "ich", "mu\u00df", ",", "ich", "will"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVINF", "$.", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von dir gefangen sein, o meiner Freunde Leben.", "tokens": ["von", "dir", "ge\u00b7fan\u00b7gen", "sein", ",", "o", "mei\u00b7ner", "Freun\u00b7de", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$,", "FM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kn\u00fcpf zu, hier hast du sie, ich will sie willig geben,", "tokens": ["Kn\u00fcpf", "zu", ",", "hier", "hast", "du", "sie", ",", "ich", "will", "sie", "wil\u00b7lig", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PPER", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die unverz\u00fcckte Hand. Apollo, la\u00df dein Spiel,", "tokens": ["die", "un\u00b7ver\u00b7z\u00fcck\u00b7te", "Hand", ".", "A\u00b7pol\u00b7lo", ",", "la\u00df", "dein", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "$,", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Thalia, deinen Klang, der mir erst wolgefiel,", "tokens": ["Tha\u00b7lia", ",", "dei\u00b7nen", "Klang", ",", "der", "mir", "erst", "wol\u00b7ge\u00b7fiel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "itzt klingen mehr als vor, ich selbst will mit anheben", "tokens": ["itzt", "klin\u00b7gen", "mehr", "als", "vor", ",", "ich", "selbst", "will", "mit", "an\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "PTKVZ", "$,", "PPER", "ADV", "VMFIN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und mein Triumflied tun. Ly\u00e4us, Trost der Reben,", "tokens": ["und", "mein", "Tri\u00b7um\u00b7flied", "tun", ".", "Ly\u00b7\u00e4us", ",", "Trost", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$.", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "und Flora, Zier der Zeit, folgt uns, hier ist mein Ziel.", "tokens": ["und", "Flo\u00b7ra", ",", "Zier", "der", "Zeit", ",", "folgt", "uns", ",", "hier", "ist", "mein", "Ziel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr Gratien geht vor. Komm, Sais, Lust der Sitten,", "tokens": ["Ihr", "Gra\u00b7ti\u00b7en", "geht", "vor", ".", "Komm", ",", "Sais", ",", "Lust", "der", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "VVFIN", "$,", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und Nais, Schmuck der Bach. So ich euch kan erbitten,", "tokens": ["und", "Nais", ",", "Schmuck", "der", "Bach", ".", "So", "ich", "euch", "kan", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "NN", "ART", "NN", "$.", "ADV", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "kn\u00fcpft diesen, der mich kn\u00fcpft, und f\u00fchrt ihn in der Mitten.", "tokens": ["kn\u00fcpft", "die\u00b7sen", ",", "der", "mich", "kn\u00fcpft", ",", "und", "f\u00fchrt", "ihn", "in", "der", "Mit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "F\u00fchrt ihn hin, wie ihr wi\u00dft, in die begr\u00fcnten Gr\u00fcnde.", "tokens": ["F\u00fchrt", "ihn", "hin", ",", "wie", "ihr", "wi\u00dft", ",", "in", "die", "be\u00b7gr\u00fcn\u00b7ten", "Gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "wo