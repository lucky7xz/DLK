{"textgrid.poem.65316": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Blumensprache", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor ihrem Fenster stehn viel Nelkent\u00f6pfe,", "tokens": ["Vor", "ih\u00b7rem", "Fens\u00b7ter", "stehn", "viel", "Nel\u00b7ken\u00b7t\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und will sie, da\u00df zu ihr hinein ich schl\u00fcpfe,", "tokens": ["Und", "will", "sie", ",", "da\u00df", "zu", "ihr", "hin\u00b7ein", "ich", "schl\u00fcp\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "KOUS", "APPR", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirft sie herab zwei purpurrothe Kn\u00f6spchen.", "tokens": ["Wirft", "sie", "her\u00b7ab", "zwei", "pur\u00b7pur\u00b7ro\u00b7the", "Kn\u00f6spc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die purpurrothen Kn\u00f6spchen wollen sagen:", "tokens": ["Die", "pur\u00b7pur\u00b7ro\u00b7then", "Kn\u00f6spc\u00b7hen", "wol\u00b7len", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zwei purpurrothe Lippen sind dein eigen:", "tokens": ["Zwei", "pur\u00b7pur\u00b7ro\u00b7the", "Lip\u00b7pen", "sind", "dein", "ei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Komm, komm, und k\u00fcsse sie zu tausend Malen!", "tokens": ["Komm", ",", "komm", ",", "und", "k\u00fcs\u00b7se", "sie", "zu", "tau\u00b7send", "Ma\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich komme schon, will ihnen K\u00fcsse geben,", "tokens": ["Ich", "kom\u00b7me", "schon", ",", "will", "ih\u00b7nen", "K\u00fcs\u00b7se", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehr, als die vollsten Nelken Bl\u00e4tter haben,", "tokens": ["Mehr", ",", "als", "die", "volls\u00b7ten", "Nel\u00b7ken", "Bl\u00e4t\u00b7ter", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mehr, als Neiderblicke mich umsp\u00e4hen.", "tokens": ["Und", "mehr", ",", "als", "Nei\u00b7der\u00b7bli\u00b7cke", "mich", "um\u00b7sp\u00e4\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vor ihrem Fenster stehn viel Nelkent\u00f6pfe,", "tokens": ["Vor", "ih\u00b7rem", "Fens\u00b7ter", "stehn", "viel", "Nel\u00b7ken\u00b7