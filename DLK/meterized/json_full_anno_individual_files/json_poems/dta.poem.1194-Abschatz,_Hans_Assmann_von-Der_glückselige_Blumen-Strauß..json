{"dta.poem.1194": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der gl\u00fcckselige Blumen-Strau\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Amor selbst brach diese Blumen/ wo Aurora sammlet ein", "tokens": ["A\u00b7mor", "selbst", "brach", "die\u00b7se", "Blu\u00b7men", "/", "wo", "Au\u00b7ro\u00b7ra", "samm\u00b7let", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PDAT", "NN", "$(", "PWAV", "NE", "VVFIN", "ART"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ihre N\u00e4glein/ ihre Rosen/ die bey fr\u00fchem Tages-Schein", "tokens": ["Ih\u00b7re", "N\u00e4g\u00b7lein", "/", "ih\u00b7re", "Ro\u00b7sen", "/", "die", "bey", "fr\u00fc\u00b7hem", "Ta\u00b7ges\u00b7Schein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "An dem blauen Himmel gl\u00e4ntzen/", "tokens": ["An", "dem", "blau\u00b7en", "Him\u00b7mel", "gl\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr sch\u00f6nes Haubt bekr\u00e4ntzen.", "tokens": ["Und", "ihr", "sch\u00f6\u00b7nes", "Haubt", "be\u00b7kr\u00b7\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ne Blumen/ Prei\u00df der G\u00e4rten/ welche Florens Hand ge-", "tokens": ["Sch\u00f6\u00b7ne", "Blu\u00b7men", "/", "Prei\u00df", "der", "G\u00e4r\u00b7ten", "/", "wel\u00b7che", "Flo\u00b7rens", "Hand", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "NN", "ART", "NN", "$(", "PWAT", "NN", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Da\u00df sie von so sch\u00f6nen H\u00e4nden solten werden anger\u00fchrt/", "tokens": ["Da\u00df", "sie", "von", "so", "sch\u00f6\u00b7nen", "H\u00e4n\u00b7den", "sol\u00b7ten", "wer\u00b7den", "an\u00b7ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADJA", "NN", "VMFIN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie begl\u00fcckt seyd ihr f\u00fcr allen", "tokens": ["Wie", "be\u00b7gl\u00fcckt", "seyd", "ihr", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VAFIN", "PPER", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Amaranthen zu gefallen?", "tokens": ["A\u00b7ma\u00b7ran\u00b7then", "zu", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar eur Glantz wird m\u00fcssen sterben in der Rimphe sch\u00f6nen", "tokens": ["Zwar", "eur", "Glantz", "wird", "m\u00fcs\u00b7sen", "ster\u00b7ben", "in", "der", "Rim\u00b7phe", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VMFIN", "VVINF", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Aber tausend Hertzen w\u00fcnschten ihnen derogleichen Stand/", "tokens": ["A\u00b7ber", "tau\u00b7send", "Hert\u00b7zen", "w\u00fcnschten", "ih\u00b7nen", "de\u00b7ro\u00b7glei\u00b7chen", "Stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+--+--++-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "W\u00fcrden willig Geist und Leben", "tokens": ["W\u00fcr\u00b7den", "wil\u00b7lig", "Geist", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr zum treuen Opffer geben.", "tokens": ["Ihr", "zum", "treu\u00b7en", "Opf\u00b7fer", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "War nicht di\u00df ein sch\u00f6ner Garten/ der euch erst das Leben gab?", "tokens": ["War", "nicht", "di\u00df", "ein", "sch\u00f6\u00b7ner", "Gar\u00b7ten", "/", "der", "euch", "erst", "das", "Le\u00b7ben", "gab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PDS", "ART", "ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Werden nicht die sch\u00f6nsten Finger dieser Welt euch Bahr und", "tokens": ["Wer\u00b7den", "nicht", "die", "sch\u00f6ns\u00b7ten", "Fin\u00b7ger", "die\u00b7ser", "Welt", "euch", "Bahr", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "PDAT", "NN", "PPER", "NE", "KON"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer will nicht/ wie ihr/ verderben/", "tokens": ["Wer", "will", "nicht", "/", "wie", "ihr", "/", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "$(", "PWAV", "PPER", "$(", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so sch\u00f6nen Todes sterben!", "tokens": ["Und", "so", "sch\u00f6\u00b7nen", "To\u00b7des", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}