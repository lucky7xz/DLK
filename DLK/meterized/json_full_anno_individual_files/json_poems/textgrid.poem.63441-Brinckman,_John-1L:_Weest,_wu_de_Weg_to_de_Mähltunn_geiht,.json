"wen", "nich", "up'n", "Pen\u00b7ning", "s\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "NE", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "kriggt nie keen Dalers in sin Fick.", "tokens": ["kriggt", "nie", "ke\u00b7en", "Da\u00b7lers", "in", "sin", "Fick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wu geiht de Weg to S\u00fcnndagsrauh?", "tokens": ["Wu", "geiht", "de", "Weg", "to", "S\u00fcnn\u00b7dags\u00b7rauh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man \u00fcmmer nah de Warkeldag',", "tokens": ["Man", "\u00fcm\u00b7mer", "nah", "de", "War\u00b7keldag'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "NE", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "de Warkst\u00e4d d\u00f6rch un't Ackerfeld \u2013", "tokens": ["de", "Wark\u00b7st\u00e4d", "d\u00f6rch", "un't", "A\u00b7cker\u00b7feld", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "denn is f\u00f6r'n S\u00fcnndag gor keen Frag.", "tokens": ["denn", "is", "f\u00f6r'n", "S\u00fcnn\u00b7dag", "gor", "ke\u00b7en", "Frag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "VVFIN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "S\u00fcnnabens is he nich wid af;"