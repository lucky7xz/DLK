{"textgrid.poem.63441": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weest, wu de Weg to de M\u00e4hltunn geiht,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weest, wu de Weg to de M\u00e4hltunn geiht,", "tokens": ["Weest", ",", "wu", "de", "Weg", "to", "de", "M\u00e4hl\u00b7tunn", "geiht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.da", "FM.da", "FM.da", "FM.da", "FM.da", "FM.da", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "to sonn vull Tunn? Hak du man drist", "tokens": ["to", "sonn", "vull", "Tunn", "?", "Hak", "du", "man", "drist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.en", "FM.en", "FM.en", "FM.en", "$.", "NE", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "v\u00f6r D\u00e4u un Dag' bet in de Nacht,", "tokens": ["v\u00f6r", "D\u00e4u", "un", "Dag'", "bet", "in", "de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bet Stirn an Stirn sick baben wist.", "tokens": ["bet", "Stirn", "an", "Stirn", "sick", "ba\u00b7ben", "wist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kik di nich \u00fcm un bliw nich stahn,", "tokens": ["Kik", "di", "nich", "\u00fcm", "un", "bliw", "nich", "stahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "