{"dta.poem.13072": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum h\u00f6rte man den Rath, so fiel der ganze Krei\u00df", "tokens": ["Kaum", "h\u00f6r\u00b7te", "man", "den", "Rath", ",", "so", "fiel", "der", "gan\u00b7ze", "Krei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Wohlgefallen bey. Mithin wandt sich der", "tokens": ["Mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "bey", ".", "Mi\u00b7thin", "wandt", "sich", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PRF", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Grei\u00df,", "tokens": ["Grei\u00df", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und warff den frohen Blick bedachtsam durch die", "tokens": ["Und", "warff", "den", "fro\u00b7hen", "Blick", "be\u00b7dacht\u00b7sam", "durch", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Reihen,", "tokens": ["Rei\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Zu sehn ob einige davon anwesend seyen.", "tokens": ["Zu", "sehn", "ob", "ei\u00b7ni\u00b7ge", "da\u00b7von", "an\u00b7we\u00b7send", "se\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOUS", "PIS", "PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Es nahten aber sich die K\u00fcnste gleich dem Rath,", "tokens": ["Es", "nah\u00b7ten", "a\u00b7ber", "sich", "die", "K\u00fcns\u00b7te", "gleich", "dem", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von denen Eine schon ihm an die Seite trat.", "tokens": ["Von", "de\u00b7nen", "Ei\u00b7ne", "schon", "ihm", "an", "die", "Sei\u00b7te", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}