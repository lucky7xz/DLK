{"textgrid.poem.39841": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Irrtum", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fandest mich um alte Liebe klagen,", "tokens": ["Du", "fan\u00b7dest", "mich", "um", "al\u00b7te", "Lie\u00b7be", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein freundlich Auge sah mich tr\u00f6stend an!", "tokens": ["Dein", "freund\u00b7lich", "Au\u00b7ge", "sah", "mich", "tr\u00f6s\u00b7tend", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie bald genas ich von dem finstern Wahn", "tokens": ["Wie", "bald", "ge\u00b7nas", "ich", "von", "dem", "fins\u00b7tern", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sah in ", "tokens": ["Und", "sah", "in"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6rte dich so milde Worte sagen,", "tokens": ["Ich", "h\u00f6r\u00b7te", "dich", "so", "mil\u00b7de", "Wor\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah mitleidig meinem Schmerz d