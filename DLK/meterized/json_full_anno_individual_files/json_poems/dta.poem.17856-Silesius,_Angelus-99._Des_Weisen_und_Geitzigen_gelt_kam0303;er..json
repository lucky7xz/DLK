{"dta.poem.17856": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "99.  Des Weisen und Geitzigen gelt kam0303;er.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wei\u00df ist kl\u00fcglich reich; er hat das Gelt im kasten/", "tokens": ["Der", "Wei\u00df", "ist", "kl\u00fcg\u00b7lich", "reich", ";", "er", "hat", "das", "Gelt", "im", "kas\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geitzhals im gem\u00fcth/ drumb l\u00e4sts ihn niemahls", "tokens": ["Der", "Geitz\u00b7hals", "im", "ge\u00b7m\u00fcth", "/", "drumb", "l\u00e4sts", "ihn", "nie\u00b7mahls"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$(", "PAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "rasten.", "tokens": ["ras\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}