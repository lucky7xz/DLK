{"textgrid.poem.61126": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3. Das Kraut Vergessenheit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Mutter mir gesagt, dort hinter jenem Berge,", "tokens": ["Es", "hat", "die", "Mut\u00b7ter", "mir", "ge\u00b7sagt", ",", "dort", "hin\u00b7ter", "je\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Der Wolken um den Gipfel hat und Nebel um die Wurzel,", "tokens": ["Der", "Wol\u00b7ken", "um", "den", "Gip\u00b7fel", "hat", "und", "Ne\u00b7bel", "um", "die", "Wur\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Dort w\u00e4chst das Kraut Vergessenheit, dort w\u00e4chst es in den Schluchten.", "tokens": ["Dort", "w\u00e4chst", "das", "Kraut", "Ver\u00b7ges\u00b7sen\u00b7heit", ",", "dort", "w\u00e4chst", "es", "in", "den", "Schluch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "O w\u00fc\u00dft' ich nur den Pfad dahin, drei Tage wollt' ich wandern", "tokens": ["O", "w\u00fc\u00dft'", "ich", "nur", "den", "Pfad", "da\u00b7hin", ",", "drei", "Ta\u00b7ge", "wollt'", "ich", 