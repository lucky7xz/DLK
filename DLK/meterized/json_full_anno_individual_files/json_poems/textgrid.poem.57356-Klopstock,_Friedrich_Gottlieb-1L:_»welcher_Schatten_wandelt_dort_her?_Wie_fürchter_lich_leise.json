word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "\u00bbwarte! Noch untersuch' ich. Ich hab' es ergr\u00fcndet! Die G\u00f6ttin", "tokens": ["\u00bb", "war\u00b7te", "!", "Noch", "un\u00b7ter\u00b7such'", "ich", ".", "Ich", "hab'", "es", "er\u00b7gr\u00fcn\u00b7det", "!", "Die", "G\u00f6t\u00b7tin"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "$.", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Rache verdient den sch\u00f6nsten Altar!", "tokens": ["Ra\u00b7che", "ver\u00b7dient", "den", "sch\u00f6ns\u00b7ten", "Al\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.31": {"text": "Dieser Schatten, der uns von neuem nahet, und jetzo", "tokens": ["Die\u00b7ser", "Schat\u00b7ten", ",", "der", "uns", "von", "neu\u00b7em", "na\u00b7het", ",", "und", "jet\u00b7zo"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "VVFIN", "$,", "KON", "ADV"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Gar mit der Hand auf die Wunden uns zeigt,", "tokens": ["Gar", "mit", "der", "Hand", "auf", "die", "Wun\u00b7den", "uns", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.33": {"text": "Ist das todte Gesetz. Wir waren's, die's mordeten! Ich war's,", "tokens": ["Ist", "das", "tod\u00b7te", "Ge\u00b7setz", ".", "Wir", "wa\u00b7ren's", ",", "die's", "mor\u00b7de\u00b7ten", "!", "Ich", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "$,", "PDS", "VMFIN", "$.", "PPER", "VAFIN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.34": {"text": "Welche die meisten Wunden ihm grub;", "tokens": ["Wel\u00b7che", "die", "meis\u00b7ten", "Wun\u00b7den", "ihm", "grub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.35": {"text": "Theilt ihr unter euch, du, und Tribuna, die \u00fcbrigen, Ich bin's,", "tokens": ["Theilt", "ihr", "un\u00b7ter", "euch", ",", "du", ",", "und", "Tri\u00b7bu\u00b7na", ",", "die", "\u00fcb\u00b7ri\u00b7gen", ",", "Ich", "bin's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PPER", "$,", "KON", "NN", "$,", "ART", "ADJA", "$,", "PPER", "VAFIN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.36": {"text": "Die's nicht bereut! Ich n\u00e4hme den Dolch", "tokens": ["Die's", "nicht", "be\u00b7reut", "!", "Ich", "n\u00e4h\u00b7me", "den", "Dolch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Wieder; kehrte der Todte zur\u00fcck. Bey Marat! ich bahnte", "tokens": ["Wie\u00b7der", ";", "kehr\u00b7te", "der", "Tod\u00b7te", "zu\u00b7r\u00fcck", ".", "Bey", "Ma\u00b7rat", "!", "ich", "bahn\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$.", "APPR", "NE", "$.", "PPER", "VVFIN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Mir noch Einmal den blutigen Weg", "tokens": ["Mir", "noch", "Ein\u00b7mal", "den", "blu\u00b7ti\u00b7gen", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.39": {"text": "Zu dem Altare der Herschaft, und ach zu der Rach' Altare!\u00ab", "tokens": ["Zu", "dem", "Al\u00b7ta\u00b7re", "der", "Her\u00b7schaft", ",", "und", "ach", "zu", "der", "Rach'", "Al\u00b7ta\u00b7re", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Und die Hundertk\u00f6pfige schwieg.", "tokens": ["Und", "die", "Hun\u00b7dert\u00b7k\u00f6p\u00b7fi\u00b7ge", "schwieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Aber vom Rhodan her erhub ein Sausen sich, wurde", "tokens": ["A\u00b7ber", "vom", "Rho\u00b7dan", "her", "er\u00b7hub", "ein", "Sau\u00b7sen", "sich", ",", "wur\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPRART", "NE", "APZR", "VVFIN", "ART", "NN", "PRF", "$,", "VAFIN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Sturm, von der R\u00fcckkehr sprach's in dem Sturm!", "tokens": ["Sturm", ",", "von", "der", "R\u00fcck\u00b7kehr", "sprach's", "in", "dem", "Sturm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.43": {"text": "Und die Dolch' entfielen dem Schatten; Galliens Roma", "tokens": ["Und", "die", "Dolch'", "ent\u00b7fie\u00b7len", "dem", "Schat\u00b7ten", ";", "Gal\u00b7li\u00b7ens", "Ro\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$.", "NE", "NE"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Stutzte, das Ungeheuer entfloh.", "tokens": ["Stutz\u00b7te", ",", "das", "Un\u00b7ge\u00b7heu\u00b7er", "ent\u00b7floh", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}