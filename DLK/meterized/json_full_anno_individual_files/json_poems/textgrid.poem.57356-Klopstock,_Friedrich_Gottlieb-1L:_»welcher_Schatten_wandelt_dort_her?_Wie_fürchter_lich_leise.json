{"textgrid.poem.57356": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwelcher Schatten wandelt dort her? Wie f\u00fcrchter lich leise", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwelcher Schatten wandelt dort her? Wie f\u00fcrchter lich leise", "tokens": ["\u00bb", "wel\u00b7cher", "Schat\u00b7ten", "wan\u00b7delt", "dort", "her", "?", "Wie", "f\u00fcrch\u00b7ter", "lich", "lei\u00b7se"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAT", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "PWAV", "ADJD", "ADJD", "ADJD"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Tritt er! hat noch die Dolch' in der Brust!", "tokens": ["Tritt", "er", "!", "hat", "noch", "die", "Dolch'", "in", "der", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ah Tribuna, kennest du ihn? Es bef\u00e4llt mich, je mehr er", "tokens": ["Ah", "Tri\u00b7bu\u00b7na", ",", "ken\u00b7nest", "du", "ihn", "?", "Es", "be\u00b7f\u00e4llt", "mich", ",", "je", "mehr", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "PPER", "$,", "ADV", "ADV", "PPER"], "meter": "-+-+-+--+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mir sich naht, je b\u00e4ngeres Graun!\u00ab", "tokens": ["Mir", "sich", "naht", ",", "je", "b\u00e4n\u00b7ge\u00b7res", "Graun", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PRF", "VVFIN", "$,", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und dich schreckt ein Gespenst, dich Herscherin unter den St\u00e4dten,", "tokens": ["Und", "dich", "schreckt", "ein", "Ge\u00b7spenst", ",", "dich", "Her\u00b7sche\u00b7rin", "un\u00b7ter", "den", "St\u00e4d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Dich, die Roma des gallischen Reichs?", "tokens": ["Dich", ",", "die", "Ro\u00b7ma", "des", "gal\u00b7li\u00b7schen", "Reichs", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "\u00bbantwort! wer ist der Schatten? Er komt stets n\u00e4her, noch n\u00e4her!", "tokens": ["\u00bb", "ant\u00b7wort", "!", "wer", "ist", "der", "Schat\u00b7ten", "?", "Er", "komt", "stets", "n\u00e4\u00b7her", ",", "noch", "n\u00e4\u00b7her", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.8": {"text": "Z\u00e4hl die Dolche! mir dunkelt der Blick.\u00ab", "tokens": ["Z\u00e4hl", "die", "Dol\u00b7che", "!", "mir", "dun\u00b7kelt", "der", "Blick", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Ha, was geht der Schemen mich an? was, ob Dolch' ihn entleibten?", "tokens": ["Ha", ",", "was", "geht", "der", "Sche\u00b7men", "mich", "an", "?", "was", ",", "ob", "Dolch'", "ihn", "ent\u00b7leib\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "PWS", "$,", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Wenn man todt ist, wandert man weg,", "tokens": ["Wenn", "man", "todt", "ist", ",", "wan\u00b7dert", "man", "weg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Schattet. Nun weisst du alles. Mich k\u00fcmmern andere Dinge,", "tokens": ["Schat\u00b7tet", ".", "Nun", "weisst", "du", "al\u00b7les", ".", "Mich", "k\u00fcm\u00b7mern", "an\u00b7de\u00b7re", "Din\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPER", "PIS", "$.", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Herschen, und herschen das ist mein Genuss!", "tokens": ["Her\u00b7schen", ",", "und", "her\u00b7schen", "das", "ist", "mein", "Ge\u00b7nuss", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Davon wach' ich, und tr\u00e4um' ich! Die Stellvertreter des Volkes", "tokens": ["Da\u00b7von", "wach'", "ich", ",", "und", "tr\u00e4um'", "ich", "!", "Die", "Stell\u00b7ver\u00b7tre\u00b7ter", "des", "Vol\u00b7kes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "$.", "ART", "NN", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Kommen, gehorsamen, knieen vor mir.", "tokens": ["Kom\u00b7men", ",", "ge\u00b7hor\u00b7sa\u00b7men", ",", "kni\u00b7e\u00b7en", "vor", "mir", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Wer der kr\u00fcmste mir kniet, ich belohn' ihn, erh\u00f6h' zu der W\u00fcrd' ihn", "tokens": ["Wer", "der", "kr\u00fcms\u00b7te", "mir", "kniet", ",", "ich", "be\u00b7lohn'", "ihn", ",", "er\u00b7h\u00f6h'", "zu", "der", "W\u00fcrd'", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Stellvertreter des P\u00f6bels zu seyn.", "tokens": ["Stell\u00b7ver\u00b7tre\u00b7ter", "des", "P\u00f6\u00b7bels", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "\u00bbaber wer ist der Schatten? Schon lang' entfloh ich, wofern er", "tokens": ["\u00bb", "a\u00b7ber", "wer", "ist", "der", "Schat\u00b7ten", "?", "Schon", "lang'", "ent\u00b7floh", "ich", ",", "wo\u00b7fern", "er"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KON", "PWS", "VAFIN", "ART", "NN", "$.", "ADV", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Sich nicht wandt', und ins dunklere trat.\u00ab", "tokens": ["Sich", "nicht", "wandt'", ",", "und", "ins", "dunk\u00b7le\u00b7re", "trat", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "PTKNEG", "VVFIN", "$,", "KON", "APPRART", "ADJA", "VVFIN", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Frag' es Klubiofuria, weil du einmal nicht rastest,", "tokens": ["Frag'", "es", "Klu\u00b7bi\u00b7o\u00b7fu\u00b7ria", ",", "weil", "du", "ein\u00b7mal", "nicht", "ras\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NE", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Bis du des Spukes Namen vernimst.", "tokens": ["Bis", "du", "des", "Spu\u00b7kes", "Na\u00b7men", "ver\u00b7nimst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "\u00bbwarte! Ich untersuche. Verdienet die G\u00f6ttin Herschaft,", "tokens": ["\u00bb", "war\u00b7te", "!", "Ich", "un\u00b7ter\u00b7su\u00b7che", ".", "Ver\u00b7die\u00b7net", "die", "G\u00f6t\u00b7tin", "Her\u00b7schaft", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "PPER", "VVFIN", "$.", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-----+---", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Oder die G\u00f6ttin Rache verdient", "tokens": ["O\u00b7der", "die", "G\u00f6t\u00b7tin", "Ra\u00b7che", "ver\u00b7dient"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVPP"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Sie den sch\u00f6nsten Altar?\u00ab \u00bbDu hundertk\u00f6pfiges, hundert", "tokens": ["Sie", "den", "sch\u00f6ns\u00b7ten", "Al\u00b7tar", "?", "\u00ab", "\u00bb", "Du", "hun\u00b7dert\u00b7k\u00f6p\u00b7fi\u00b7ges", ",", "hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "$.", "$(", "$(", "PPER", "ADJA", "$,", "CARD"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Armiges Ungeheuer, und doch", "tokens": ["Ar\u00b7mi\u00b7ges", "Un\u00b7ge\u00b7heu\u00b7er", ",", "und", "doch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Nur ein\u00e4ugiges, mir, der Roma des gallischen Reiches,", "tokens": ["Nur", "ein\u00b7\u00e4u\u00b7gi\u00b7ges", ",", "mir", ",", "der", "Ro\u00b7ma", "des", "gal\u00b7li\u00b7schen", "Rei\u00b7ches", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PPER", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Mir gebeutst du zu warten? Wer ist,", "tokens": ["Mir", "ge\u00b7beutst", "du", "zu", "war\u00b7ten", "?", "Wer", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$.", "PWS", "VAFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.27": {"text": "Rede, wer ist der Schatten, der wieder nahet, und jetzo", "tokens": ["Re\u00b7de", ",", "wer", "ist", "der", "Schat\u00b7ten", ",", "der", "wie\u00b7der", "na\u00b7het", ",", "und", "jet\u00b7zo"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KON", "ADV"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Gar mit der Hand auf die Wunden, mir zeigt?\u00ab", "tokens": ["Gar", "mit", "der", "Hand", "auf", "die", "Wun\u00b7den", ",", "mir", "zeigt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "\u00bbwarte! Noch untersuch' ich. Ich hab' es ergr\u00fcndet! Die G\u00f6ttin", "tokens": ["\u00bb", "war\u00b7te", "!", "Noch", "un\u00b7ter\u00b7such'", "ich", ".", "Ich", "hab'", "es", "er\u00b7gr\u00fcn\u00b7det", "!", "Die", "G\u00f6t\u00b7tin"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "$.", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Rache verdient den sch\u00f6nsten Altar!", "tokens": ["Ra\u00b7che", "ver\u00b7dient", "den", "sch\u00f6ns\u00b7ten", "Al\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.31": {"text": "Dieser Schatten, der uns von neuem nahet, und jetzo", "tokens": ["Die\u00b7ser", "Schat\u00b7ten", ",", "der", "uns", "von", "neu\u00b7em", "na\u00b7het", ",", "und", "jet\u00b7zo"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "VVFIN", "$,", "KON", "ADV"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Gar mit der Hand auf die Wunden uns zeigt,", "tokens": ["Gar", "mit", "der", "Hand", "auf", "die", "Wun\u00b7den", "uns", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.33": {"text": "Ist das todte Gesetz. Wir waren's, die's mordeten! Ich war's,", "tokens": ["Ist", "das", "tod\u00b7te", "Ge\u00b7setz", ".", "Wir", "wa\u00b7ren's", ",", "die's", "mor\u00b7de\u00b7ten", "!", "Ich", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "$,", "PDS", "VMFIN", "$.", "PPER", "VAFIN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.34": {"text": "Welche die meisten Wunden ihm grub;", "tokens": ["Wel\u00b7che", "die", "meis\u00b7ten", "Wun\u00b7den", "ihm", "grub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.35": {"text": "Theilt ihr unter euch, du, und Tribuna, die \u00fcbrigen, Ich bin's,", "tokens": ["Theilt", "ihr", "un\u00b7ter", "euch", ",", "du", ",", "und", "Tri\u00b7bu\u00b7na", ",", "die", "\u00fcb\u00b7ri\u00b7gen", ",", "Ich", "bin's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PPER", "$,", "KON", "NN", "$,", "ART", "ADJA", "$,", "PPER", "VAFIN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.36": {"text": "Die's nicht bereut! Ich n\u00e4hme den Dolch", "tokens": ["Die's", "nicht", "be\u00b7reut", "!", "Ich", "n\u00e4h\u00b7me", "den", "Dolch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Wieder; kehrte der Todte zur\u00fcck. Bey Marat! ich bahnte", "tokens": ["Wie\u00b7der", ";", "kehr\u00b7te", "der", "Tod\u00b7te", "zu\u00b7r\u00fcck", ".", "Bey", "Ma\u00b7rat", "!", "ich", "bahn\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$.", "APPR", "NE", "$.", "PPER", "VVFIN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Mir noch Einmal den blutigen Weg", "tokens": ["Mir", "noch", "Ein\u00b7mal", "den", "blu\u00b7ti\u00b7gen", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.39": {"text": "Zu dem Altare der Herschaft, und ach zu der Rach' Altare!\u00ab", "tokens": ["Zu", "dem", "Al\u00b7ta\u00b7re", "der", "Her\u00b7schaft", ",", "und", "ach", "zu", "der", "Rach'", "Al\u00b7ta\u00b7re", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Und die Hundertk\u00f6pfige schwieg.", "tokens": ["Und", "die", "Hun\u00b7dert\u00b7k\u00f6p\u00b7fi\u00b7ge", "schwieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Aber vom Rhodan her erhub ein Sausen sich, wurde", "tokens": ["A\u00b7ber", "vom", "Rho\u00b7dan", "her", "er\u00b7hub", "ein", "Sau\u00b7sen", "sich", ",", "wur\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPRART", "NE", "APZR", "VVFIN", "ART", "NN", "PRF", "$,", "VAFIN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Sturm, von der R\u00fcckkehr sprach's in dem Sturm!", "tokens": ["Sturm", ",", "von", "der", "R\u00fcck\u00b7kehr", "sprach's", "in", "dem", "Sturm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.43": {"text": "Und die Dolch' entfielen dem Schatten; Galliens Roma", "tokens": ["Und", "die", "Dolch'", "ent\u00b7fie\u00b7len", "dem", "Schat\u00b7ten", ";", "Gal\u00b7li\u00b7ens", "Ro\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$.", "NE", "NE"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Stutzte, das Ungeheuer entfloh.", "tokens": ["Stutz\u00b7te", ",", "das", "Un\u00b7ge\u00b7heu\u00b7er", "ent\u00b7floh", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "\u00bbwelcher Schatten wandelt dort her? Wie f\u00fcrchter lich leise", "tokens": ["\u00bb", "wel\u00b7cher", "Schat\u00b7ten", "wan\u00b7delt", "dort", "her", "?", "Wie", "f\u00fcrch\u00b7ter", "lich", "lei\u00b7se"], "token_info": ["punct", "word", "w