{"dta.poem.1053": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Also kan der F\u00fcrsten gunst/", "tokens": ["Al\u00b7so", "kan", "der", "F\u00fcrs\u00b7ten", "gunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wan sie die Ph", "tokens": ["Wan", "sie", "die", "Ph"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Verg\u00fcldet/ mit s\u00fcsser kunst", "tokens": ["Ver\u00b7g\u00fcl\u00b7det", "/", "mit", "s\u00fcs\u00b7ser", "kunst"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$(", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jhr ewiges lob au\u00dfspraitten:", "tokens": ["Ihr", "e\u00b7wi\u00b7ges", "lob", "au\u00df\u00b7sprait\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vnd der Donderende Got/", "tokens": ["Vnd", "der", "Don\u00b7de\u00b7ren\u00b7de", "Got", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu widerstehen dem Tod/", "tokens": ["Zu", "wi\u00b7der\u00b7ste\u00b7hen", "dem", "Tod", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Gab das gold den Potentaten/", "tokens": ["Gab", "das", "gold", "den", "Po\u00b7ten\u00b7ta\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Damit sie den G\u00f6ttern gleich", "tokens": ["Da\u00b7mit", "sie", "den", "G\u00f6t\u00b7tern", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch seiner T\u00f6chtern wolthaten", "tokens": ["Durch", "sei\u00b7ner", "T\u00f6ch\u00b7tern", "wolt\u00b7ha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Nicht k\u00e4men in Plutons reich", "tokens": ["Nicht", "k\u00e4\u00b7men", "in", "Plu\u00b7tons", "reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "APPR", "NE", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Wie sonst gemeine soldaten.", "tokens": ["Wie", "sonst", "ge\u00b7mei\u00b7ne", "sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "VMFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}