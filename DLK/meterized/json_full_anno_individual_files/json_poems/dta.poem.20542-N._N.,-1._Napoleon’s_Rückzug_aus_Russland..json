{"dta.poem.20542": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "1.  \n  Napoleon\u2019s R\u00fcckzug aus Russland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Eine feste Burg ist unser Gott,", "tokens": ["Ei\u00b7ne", "fes\u00b7te", "Burg", "ist", "un\u00b7ser", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die beste Wehr und Waffen.", "tokens": ["Die", "bes\u00b7te", "Wehr", "und", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Frankenkaiser kam zu Spott,", "tokens": ["Der", "Fran\u00b7ken\u00b7kai\u00b7ser", "kam", "zu", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts half sein ems\u2019ges Schaffen.", "tokens": ["Nichts", "half", "sein", "ems'\u00b7ges", "Schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es hat auf Nordlands Feld", "tokens": ["Es", "hat", "auf", "Nord\u00b7lands", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gott selbst ihn hingestellt.", "tokens": ["Gott", "selbst", "ihn", "hin\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort stand er kahl und na\u00df,", "tokens": ["Dort", "stand", "er", "kahl", "und", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Fror und Freund Boreas", "tokens": ["Fror", "und", "Freund", "Bo\u00b7re\u00b7as"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Gab st\u00fcrmend ihm\u2019s Geleite.", "tokens": ["Gab", "st\u00fcr\u00b7mend", "ihm's", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In Moskau zog der Welschen Heer", "tokens": ["In", "Mos\u00b7kau", "zog", "der", "Wel\u00b7schen", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinein von allen Seiten.", "tokens": ["Hin\u00b7ein", "von", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Czaarenkremmel fand es leer", "tokens": ["Den", "Czaa\u00b7ren\u00b7krem\u00b7mel", "fand", "es", "leer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Koch- und K\u00fcfer-Leuten.", "tokens": ["Von", "Koch", "und", "K\u00fc\u00b7fer\u00b7Leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Werthlos wird Demantpracht,", "tokens": ["Wert\u00b7hlos", "wird", "De\u00b7mant\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wo Durst und Hunger wacht.", "tokens": ["Wo", "Durst", "und", "Hun\u00b7ger", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus ries\u2019gem grauen Thor", "tokens": ["Aus", "ries'\u00b7gem", "grau\u00b7en", "Thor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Seht wirbelnd dort empor", "tokens": ["Seht", "wir\u00b7belnd", "dort", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die Flammens\u00e4ule steigen!", "tokens": ["Die", "Flam\u00b7men\u00b7s\u00e4u\u00b7le", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Beresina hemmt den Weg,", "tokens": ["Die", "Be\u00b7re\u00b7si\u00b7na", "hemmt", "den", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Den Tausend heimw\u00e4rts wollen.", "tokens": ["Den", "Tau\u00b7send", "heim\u00b7w\u00e4rts", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADV", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der tapfre ", "tokens": ["Der", "tapf\u00b7re"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Trotz Sturm, trotz Eisesschollen;", "tokens": ["Trotz", "Sturm", ",", "trotz", "Ei\u00b7ses\u00b7schol\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es dr\u00e4ngt in Hast und Wuth", "tokens": ["Es", "dr\u00e4ngt", "in", "Hast", "und", "Wuth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Tro\u00df gen Br\u00fcck\u2019 und Fluth.", "tokens": ["Der", "Tro\u00df", "gen", "Br\u00fcck", "und", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Manch\u2019 Kranker st\u00fcrzt hinab,", "tokens": ["Man\u00b7ch'", "Kran\u00b7ker", "st\u00fcrzt", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jhn gr\u00fc\u00dft das k\u00fchle Grab.", "tokens": ["Jhn", "gr\u00fc\u00dft", "das", "k\u00fch\u00b7le", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Hoch th\u00fcrmen sich die Leichen!", "tokens": ["Hoch", "th\u00fcr\u00b7men", "sich", "die", "Lei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zerstoben war das Flitterwerk,", "tokens": ["Zer\u00b7sto\u00b7ben", "war", "das", "Flit\u00b7ter\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Staub war es entnommen.", "tokens": ["Dem", "Staub", "war", "es", "ent\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das k\u00fchnste Heer, voll Geist und St\u00e4rk\u2019,", "tokens": ["Das", "k\u00fchns\u00b7te", "Heer", ",", "voll", "Geist", "und", "St\u00e4rk'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sank hin, im Frost verklommen.", "tokens": ["Sank", "hin", ",", "im", "Frost", "ver\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gott wollt\u2019s. \u2014 Der Zweifler zwar", "tokens": ["Gott", "wollt'", "s.", "Der", "Zweif\u00b7ler", "zwar"], "token_info": ["word", "word", "abbreviation", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "VVIMP", "$(", "ART", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "W\u00e4hnt: Nichts sei wunderbar!", "tokens": ["W\u00e4hnt", ":", "Nichts", "sei", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Merk\u2019 denn: was Ha\u00df vollf\u00fchrt,", "tokens": ["Merk'", "denn", ":", "was", "Ha\u00df", "voll\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PWS", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Stirbt, wird nicht statuirt. \u2014", "tokens": ["Stirbt", ",", "wird", "nicht", "sta\u00b7tu\u00b7irt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ewig nur lebt die Liebe!", "tokens": ["E\u00b7wig", "nur", "lebt", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}