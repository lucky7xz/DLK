{"textgrid.poem.49161": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Gottseeliger Herzen Vergn\u00fcgung", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dennoch halt' ich's, Herr, mit dir;", "tokens": ["Den\u00b7noch", "halt'", "ich's", ",", "Herr", ",", "mit", "dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehe deine Feind herf\u00fcr,", "tokens": ["Zie\u00b7he", "dei\u00b7ne", "Feind", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass' sie gleich den Cedern stehen,", "tokens": ["Lass'", "sie", "gleich", "den", "Ce\u00b7dern", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00df im vollen Ehrenschein,", "tokens": ["Gro\u00df", "im", "vol\u00b7len", "Eh\u00b7ren\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reich an aller Wollust sein,", "tokens": ["Reich", "an", "al\u00b7ler", "Wol\u00b7lust", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00e4chtig selbst dein Volk zu schm\u00e4hen.", "tokens": ["M\u00e4ch\u00b7tig", "selbst", "dein", "Volk", "zu", "schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deinen Kindern, o mein Licht,", "tokens": ["Dei\u00b7nen", "Kin\u00b7dern", ",", "o", "mein", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebster Vater, dient dies nicht.", "tokens": ["Liebs\u00b7ter", "Va\u00b7ter", ",", "dient", "dies", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PDS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Begehren, Wunsch und Will',", "tokens": ["Mein", "Be\u00b7geh\u00b7ren", ",", "Wunsch", "und", "Will'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Seele H\u00fcll' und F\u00fcll',", "tokens": ["Mei\u00b7ner", "See\u00b7le", "H\u00fcll'", "und", "F\u00fcll'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Herzens Trost und Leben,", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "Trost", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Sinnen Lust und Ruh',", "tokens": ["Mei\u00b7ner", "Sin\u00b7nen", "Lust", "und", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das allein, mein Gott, bist du,", "tokens": ["Das", "al\u00b7lein", ",", "mein", "Gott", ",", "bist", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Du der mir den Geist gegeben", "tokens": ["Du", "der", "mir", "den", "Geist", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPER", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dein unvergleichlichs Bild", "tokens": ["Und", "dein", "un\u00b7ver\u00b7gleich\u00b7lichs", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In demselben hast verh\u00fcllt.", "tokens": ["In", "dem\u00b7sel\u00b7ben", "hast", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also hat die Ewigkeit", "tokens": ["Al\u00b7so", "hat", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in mein Gebein verkleidt", "tokens": ["Sich", "in", "mein", "Ge\u00b7bein", "ver\u00b7kleidt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich zum begl\u00fcckten Erben", "tokens": ["Und", "mich", "zum", "be\u00b7gl\u00fcck\u00b7ten", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Sternenreichs bestellt,", "tokens": ["Dei\u00b7nes", "Ster\u00b7nen\u00b7reichs", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was soll mir hierbei die Welt,", "tokens": ["Was", "soll", "mir", "hier\u00b7bei", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die ganz lieget im Verderben", "tokens": ["Die", "ganz", "lie\u00b7get", "im", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, je mehr sie uns erquickt,", "tokens": ["Und", ",", "je", "mehr", "sie", "uns", "er\u00b7quickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Desto mehr zur H\u00f6lle dr\u00fcckt.", "tokens": ["Des\u00b7to", "mehr", "zur", "H\u00f6l\u00b7le", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Lieber kr\u00e4nk', mein Gott, mich hier,", "tokens": ["Lie\u00b7ber", "kr\u00e4nk'", ",", "mein", "Gott", ",", "mich", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn so wachs' ich auf in dir;", "tokens": ["Denn", "so", "wachs'", "ich", "auf", "in", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass' mir Saft und Kraft vergehen,", "tokens": ["Lass'", "mir", "Saft", "und", "Kraft", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also schmeckt mir deine G\u00fct';", "tokens": ["Al\u00b7so", "schmeckt", "mir", "dei\u00b7ne", "G\u00fct'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll mir wol sein am Gem\u00fcth,", "tokens": ["Soll", "mir", "wol", "sein", "am", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VAINF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann dem Fleisch nicht gut geschehen,", "tokens": ["Kann", "dem", "Fleisch", "nicht", "gut", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wo du bestellt dein Haus,", "tokens": ["Und", "wo", "du", "be\u00b7stellt", "dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mu\u00df zuerst die Erd' heraus.", "tokens": ["Mu\u00df", "zu\u00b7erst", "die", "Erd'", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00fcsses Elend, sch\u00f6ne Pein,", "tokens": ["S\u00fcs\u00b7ses", "E\u00b7lend", ",", "sch\u00f6\u00b7ne", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir dort mein Heilsvolk sein!", "tokens": ["Die", "mir", "dort", "mein", "Heils\u00b7volk", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Theurer Kampf, erw\u00fcnschtes Leiden,", "tokens": ["Theu\u00b7rer", "Kampf", ",", "er\u00b7w\u00fcnschtes", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Welche meine Ritterkron'!", "tokens": ["Wel\u00b7che", "mei\u00b7ne", "Rit\u00b7ter\u00b7kron'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herrlich' Armuth, lieber Hohn,", "tokens": ["Herr\u00b7lich'", "Ar\u00b7muth", ",", "lie\u00b7ber", "Hohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Himmel mir bescheiden!", "tokens": ["Die", "den", "Him\u00b7mel", "mir", "be\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr, ein Wechsel, den die Welt", "tokens": ["Herr", ",", "ein", "Wech\u00b7sel", ",", "den", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Nur f\u00fcr eine M\u00e4re h\u00e4lt.", "tokens": ["Nur", "f\u00fcr", "ei\u00b7ne", "M\u00e4\u00b7re", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lass' sie, Gott, bei ihrem Gut,", "tokens": ["Lass'", "sie", ",", "Gott", ",", "bei", "ih\u00b7rem", "Gut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lass' es, dem es sanfte thut,", "tokens": ["Lass'", "es", ",", "dem", "es", "sanf\u00b7te", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also geht es bei der Erden.", "tokens": ["Al\u00b7so", "geht", "es", "bei", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich will gern hier Nichtes sein,", "tokens": ["Ich", "will", "gern", "hier", "Nich\u00b7tes", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PIS", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Um da\u00df du, o Alles, mein,", "tokens": ["Um", "da\u00df", "du", ",", "o", "Al\u00b7les", ",", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "KOUS", "PPER", "$,", "FM", "PIS", "$,", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach mein Alles, m\u00f6gest werden,", "tokens": ["Ach", "mein", "Al\u00b7les", ",", "m\u00f6\u00b7gest", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "PIS", "$,", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und da\u00df mich dein Richtertag", "tokens": ["Und", "da\u00df", "mich", "dein", "Rich\u00b7ter\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Desto mehr erfreuen mag.", "tokens": ["Des\u00b7to", "mehr", "er\u00b7freu\u00b7en", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Dennoch halt' ich's, Herr, mit dir;", "tokens": ["Den\u00b7noch", "halt'", "ich's", ",", "Herr", ",", "mit", "dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehe deine Feind herf\u00fcr,", "tokens": ["Zie\u00b7he", "dei\u00b7ne", "Feind", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass' sie gleich den Cedern stehen,", "tokens": ["Lass'", "sie", "gleich", "den", "Ce\u00b7dern", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00df im vollen Ehrenschein,", "tokens": ["Gro\u00df", "im", "vol\u00b7len", "Eh\u00b7ren\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reich an aller Wollust sein,", "tokens": ["Reich", "an", "al\u00b7ler", "Wol\u00b7lust", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00e4chtig selbst dein Volk zu schm\u00e4hen.", "tokens": ["M\u00e4ch\u00b7tig", "selbst", "dein", "Volk", "zu", "schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deinen Kindern, o mein Licht,", "tokens": ["Dei\u00b7nen", "Kin\u00b7dern", ",", "o", "mein", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebster Vater, dient dies nicht.", "tokens": ["Liebs\u00b7ter", "Va\u00b7ter", ",", "dient", "dies", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PDS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mein Begehren, Wunsch und Will',", "tokens": ["Mein", "Be\u00b7geh\u00b7ren", ",", "Wunsch", "und", "Will'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Seele H\u00fcll' und F\u00fcll',", "tokens": ["Mei\u00b7ner", "See\u00b7le", "H\u00fcll'", "und", "F\u00fcll'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Herzens Trost und Leben,", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "Trost", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Sinnen Lust und Ruh',", "tokens": ["Mei\u00b7ner", "Sin\u00b7nen", "Lust", "und", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das allein, mein Gott, bist du,", "tokens": ["Das", "al\u00b7lein", ",", "mein", "Gott", ",", "bist", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Du der mir den Geist gegeben", "tokens": ["Du", "der", "mir", "den", "Geist", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPER", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dein unvergleichlichs Bild", "tokens": ["Und", "dein", "un\u00b7ver\u00b7gleich\u00b7lichs", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In demselben hast verh\u00fcllt.", "tokens": ["In", "dem\u00b7sel\u00b7ben", "hast", "ver\u00b7h\u00fcllt", "."], "