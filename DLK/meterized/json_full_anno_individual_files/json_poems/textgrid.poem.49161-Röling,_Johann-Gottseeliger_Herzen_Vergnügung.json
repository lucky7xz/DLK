", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Desto mehr zur H\u00f6lle dr\u00fcckt.", "tokens": ["Des\u00b7to", "mehr", "zur", "H\u00f6l\u00b7le", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.10": {"line.1": {"text": "Lieber kr\u00e4nk', mein Gott, mich hier,", "tokens": ["Lie\u00b7ber", "kr\u00e4nk'", ",", "mein", "Gott", ",", "mich", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn so wachs' ich auf in dir;", "tokens": ["Denn", "so", "wachs'", "ich", "auf", "in", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass' mir Saft und Kraft vergehen,", "tokens": ["Lass'", "mir", "Saft", "und", "Kraft", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also schmeckt mir deine G\u00fct';", "tokens": ["Al\u00b7so", "schmeckt", "mir", "dei\u00b7ne", "G\u00fct'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll mir wol sein am Gem\u00fcth,", "tokens": ["Soll", "mir", "wol", "sein", "am", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VAINF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann dem Fleisch nicht gut geschehen,", "tokens": ["Kann", "dem", "Fleisch", "nicht", "gut", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wo du bestellt dein Haus,", "tokens": ["Und", "wo", "du", "be\u00b7stellt", "dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mu\u00df zuerst die Erd' heraus.", "tokens": ["Mu\u00df", "zu\u00b7erst", "die", "Erd'", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "S\u00fcsses Elend, sch\u00f6ne Pein,", "tokens": ["S\u00fcs\u00b7ses", "E\u00b7lend", ",", "sch\u00f6\u00b7ne", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir dort mein Heilsvolk sein!", "tokens": ["Die", "mir", "dort", "mein", "Heils\u00b7volk", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Theurer Kampf, erw\u00fcnschtes Leiden,", "tokens": ["Theu\u00b7rer", "Kampf", ",", "er\u00b7w\u00fcnschtes", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Welche meine Ritterkron'!", "tokens": ["Wel\u00b7che", "mei\u00b7ne", "Rit\u00b7ter\u00b7kron'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herrlich' Armuth, lieber Hohn,", "tokens": ["Herr\u00b7lich'", "Ar\u00b7muth", ",", "lie\u00b7ber", "Hohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Himmel mir bescheiden!", "tokens": ["Die", "den", "Him\u00b7mel", "mir", "be\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr, ein Wechsel, den die Welt", "tokens": ["Herr", ",", "ein", "Wech\u00b7sel", ",", "den", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Nur f\u00fcr eine M\u00e4re h\u00e4lt.", "tokens": ["Nur", "f\u00fcr", "ei\u00b7ne", "M\u00e4\u00b7re", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Lass' sie, Gott, bei ihrem Gut,", "tokens": ["Lass'", "sie", ",", "Gott", ",", "bei", "ih\u00b7rem", "Gut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lass' es, dem es sanfte thut,", "tokens": ["Lass'", "es", ",", "dem", "es", "sanf\