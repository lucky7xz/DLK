{"dta.poem.9711": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von weibern.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein schmeichler r\u00fchmt ein weib/ sie sey ein himmelreich;", "tokens": ["Ein", "schmeich\u00b7ler", "r\u00fchmt", "ein", "weib", "/", "sie", "sey", "ein", "him\u00b7mel\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$(", "PPER", "VAFIN", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wer die warheit liebt/ der wird dar\u00fcber lachen/", "tokens": ["Doch", "wer", "die", "war\u00b7heit", "liebt", "/", "der", "wird", "da\u00b7r\u00fc\u00b7ber", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$(", "ART", "VAFIN", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hingegen umbgekehrt den klugen ausspruch machen:", "tokens": ["Hin\u00b7ge\u00b7gen", "umb\u00b7ge\u00b7kehrt", "den", "klu\u00b7gen", "aus\u00b7spruch", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein weib ist eine grufft und selbst der h\u00f6llen gleich.", "tokens": ["Ein", "weib", "ist", "ei\u00b7ne", "grufft", "und", "selbst", "der", "h\u00f6l\u00b7len", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum mu\u00df die seele sich vor dieser grufft bewahren/", "tokens": ["Drum", "mu\u00df", "die", "see\u00b7le", "sich", "vor", "die\u00b7ser", "grufft", "be\u00b7wah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "VVFIN", "PRF", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer wolte th\u00f6richt seyn und in die h\u00f6lle fahren?", "tokens": ["Wer", "wol\u00b7te", "th\u00f6\u00b7richt", "seyn", "und", "in", "die", "h\u00f6l\u00b7le", "fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "VAINF", "KON", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}