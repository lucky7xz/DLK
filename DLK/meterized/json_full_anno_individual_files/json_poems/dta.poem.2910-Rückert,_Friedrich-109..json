{"dta.poem.2910": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm nur, du bist ein Knecht, und sei ein flei\u00dfig treuer!", "tokens": ["Komm", "nur", ",", "du", "bist", "ein", "Knecht", ",", "und", "sei", "ein", "flei\u00b7\u00dfig", "treu\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "ART", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bestell den Acker, streu die Saat und f\u00fcll die Scheuer.", "tokens": ["Be\u00b7stell", "den", "A\u00b7cker", ",", "streu", "die", "Saat", "und", "f\u00fcll", "die", "Scheu\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du thust es durch den Herrn, du thust es f\u00fcr den Herrn,", "tokens": ["Du", "thust", "es", "durch", "den", "Herrn", ",", "du", "thust", "es", "f\u00fcr", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieses sei dein Lohn, da\u00df du es thuest gern.", "tokens": ["Und", "die\u00b7ses", "sei", "dein", "Lohn", ",", "da\u00df", "du", "es", "thu\u00b7e