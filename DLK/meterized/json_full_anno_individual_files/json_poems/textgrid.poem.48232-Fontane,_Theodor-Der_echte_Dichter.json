{"textgrid.poem.48232": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der echte Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Dichter, ein echter, der Lyrik betreibt,", "tokens": ["Ein", "Dich\u00b7ter", ",", "ein", "ech\u00b7ter", ",", "der", "Ly\u00b7rik", "be\u00b7treibt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit einer K\u00f6chin ist er beweibt,", "tokens": ["Mit", "ei\u00b7ner", "K\u00f6\u00b7chin", "ist", "er", "be\u00b7weibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Seine Kinder sind schmuddlig und unerzogen,", "tokens": ["Sei\u00b7ne", "Kin\u00b7der", "sind", "schmudd\u00b7lig", "und", "un\u00b7er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Kommt der Mietszettelmann, so wird t\u00fcchtig gelogen,", "tokens": ["Kommt", "der", "Miets\u00b7zet\u00b7tel\u00b7mann", ",", "so", "wird", "t\u00fcch\u00b7tig", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Gelogen, gemogelt wird \u00fcberhaupt viel,", "tokens": ["Ge\u00b7lo\u00b7gen", ",", "ge\u00b7mo\u00b7gelt", "wird", "\u00fc\u00b7ber\u00b7haupt", "viel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "\u00bbfabulieren\u00ab ist ja Zweck und Ziel.", "tokens": ["\u00bb", "fa\u00b7bu\u00b7lie\u00b7ren", "\u00ab", "ist", "ja", "Zweck", "und", "Ziel", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$(", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und ist er gek\u00e4mmt und gewaschen zuzeiten,", "tokens": ["Und", "ist", "er", "ge\u00b7k\u00e4mmt", "und", "ge\u00b7wa\u00b7schen", "zu\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "KON", "ADJA", "VVIZU", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So schafft das nur Verlegenheiten,", "tokens": ["So", "schafft", "das", "nur", "Ver\u00b7le\u00b7gen\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist er gar ohne Wechsel und Schulden", "tokens": ["Und", "ist", "er", "gar", "oh\u00b7ne", "Wech\u00b7sel", "und", "Schul\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und empf\u00e4ngt er pro Zeile 'nen halben Gulden", "tokens": ["Und", "emp\u00b7f\u00e4ngt", "er", "pro", "Zei\u00b7le", "'nen", "hal\u00b7ben", "Gul\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Oder pendeln ihm Orden am Frack hin und her,", "tokens": ["O\u00b7der", "pen\u00b7deln", "ihm", "Or\u00b7den", "am", "Frack", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "So ist er gar kein Dichter mehr,", "tokens": ["So", "ist", "er", "gar", "kein", "Dich\u00b7ter", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eines echten Dichters eigenste Welt", "tokens": ["Ei\u00b7nes", "ech\u00b7ten", "Dich\u00b7ters", "ei\u00b7gens\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Ist der Himmel und \u2013 ein Zigeunerzelt.", "tokens": ["Ist", "der", "Him\u00b7mel", "und", "\u2013", "ein", "Zi\u00b7geu\u00b7ner\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "$(", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ein Dichter, ein echter, der Lyrik betreibt,", "tokens": ["Ein", "Dich\u00b7ter", ",", "ein", "ech\u00b7ter", ",", "der", "Ly\u00b7rik", "be\u00b7treibt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit einer K\u00f6chi