{"dta.poem.8651": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die Melancholie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Du geleitest mich durch's Leben,", "tokens": ["Du", "ge\u00b7lei\u00b7test", "mich", "durch's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinnende Melancholie!", "tokens": ["Sin\u00b7nen\u00b7de", "Me\u00b7lan\u00b7cho\u00b7lie", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag mein Stern sich strahlend heben,", "tokens": ["Mag", "mein", "Stern", "sich", "strah\u00b7lend", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag er sinken \u2014 weichest nie!", "tokens": ["Mag", "er", "sin\u00b7ken", "wei\u00b7chest", "nie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$(", "VVFIN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00fchrst mich oft in Felsenkl\u00fcfte,", "tokens": ["F\u00fchrst", "mich", "oft", "in", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Adler einsam haust,", "tokens": ["Wo", "der", "Ad\u00b7ler", "ein\u00b7sam", "haust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tannen ragen in die L\u00fcfte,", "tokens": ["Tan\u00b7nen", "ra\u00b7gen", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Waldstrom donnernd braust.", "tokens": ["Und", "der", "Wald\u00b7strom", "don\u00b7nernd", "braust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Meiner Todten dann gedenk' ich,", "tokens": ["Mei\u00b7ner", "Tod\u00b7ten", "dann", "ge\u00b7denk'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wild hervor die Thr\u00e4ne bricht,", "tokens": ["Wild", "her\u00b7vor", "die", "Thr\u00e4\u00b7ne", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und an deinen Busen senk' ich", "tokens": ["Und", "an", "dei\u00b7nen", "Bu\u00b7sen", "senk'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein umnachtet Angesicht.", "tokens": ["Mein", "um\u00b7nach\u00b7tet", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}