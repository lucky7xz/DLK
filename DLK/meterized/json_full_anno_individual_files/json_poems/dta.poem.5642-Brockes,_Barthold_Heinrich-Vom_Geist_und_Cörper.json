{"dta.poem.5642": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieselbigen nun stellt sie ihr", "tokens": ["Die\u00b7sel\u00b7bi\u00b7gen", "nun", "stellt", "sie", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedehnt, beweglich, theilbar f\u00fcr;", "tokens": ["Ge\u00b7dehnt", ",", "be\u00b7weg\u00b7lich", ",", "theil\u00b7bar", "f\u00fcr", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da denn, da\u00df sie verschiedlich sind vom Geist,", "tokens": ["Da", "denn", ",", "da\u00df", "sie", "ver\u00b7schied\u00b7lich", "sind", "vom", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und folglich c\u00f6rperlich, sich deutlich weis\u2019t.", "tokens": ["Und", "folg\u00b7lich", "c\u00f6r\u00b7per\u00b7lich", ",", "sich", "deut\u00b7lich", "weis'", "t."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "ADJD", "$,", "PRF", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den C\u00f6rper r\u00fchren sie durch c\u00f6rperliche Z\u00fcge.", "tokens": ["Den", "C\u00f6r\u00b7per", "r\u00fch\u00b7ren", "sie", "durch", "c\u00f6r\u00b7per\u00b7li\u00b7che", "Z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Drey Nahmen machen sie uns insgemein bekannt:", "tokens": ["Drey", "Nah\u00b7men", "ma\u00b7chen", "sie", "uns", "ins\u00b7ge\u00b7mein", "be\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Stoff, C\u00f6rper und was Ausgespannt.", "tokens": ["Stoff", ",", "C\u00f6r\u00b7per", "und", "was", "Aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}