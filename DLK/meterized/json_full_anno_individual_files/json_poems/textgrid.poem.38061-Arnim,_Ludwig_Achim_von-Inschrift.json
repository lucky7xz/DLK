{"textgrid.poem.38061": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Inschrift", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r mich du arme Pilgerin,", "tokens": ["H\u00f6r", "mich", "du", "ar\u00b7me", "Pil\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die zu Wallfahrten hast den Sinn,", "tokens": ["Die", "zu", "Wall\u00b7fahr\u00b7ten", "hast", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Nicht wollest du vor\u00fcber gehen,", "tokens": ["Nicht", "wol\u00b7lest", "du", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey diesem Bilde bleibe stehen,", "tokens": ["Bey", "die\u00b7sem", "Bil\u00b7de", "blei\u00b7be", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erfrisch allhier die m\u00fcden F\u00fc\u00df,", "tokens": ["Er\u00b7frisch", "all\u00b7hier", "die", "m\u00fc\u00b7den", "F\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Maria hier die Mutter s\u00fc\u00df", "tokens": ["Ma\u00b7ria", "hier", "die", "Mut\u00b7ter", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ganz ruhig stehet und wartet,", "tokens": ["Ganz", "ru\u00b7hig", "ste\u00b7het", "und", "war\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ob du bist gut geartet.", "tokens": ["Ob", "du", "bist", "gut", "ge\u00b7ar\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hast du ihr nichts zu geben mehr,", "tokens": ["Hast", "du", "ihr", "nichts", "zu", "ge\u00b7ben", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PIS", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df ihr nur eine fromme Z\u00e4hr,", "tokens": ["La\u00df", "ihr", "nur", "ei\u00b7ne", "from\u00b7me", "Z\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Thu sie mit nassen Augen", "tokens": ["Thu", "sie", "mit", "nas\u00b7sen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ganz sinniglich anschauen,", "tokens": ["Ganz", "sin\u00b7nig\u00b7lich", "an\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ohn Zweifel wirds ihr lieber seyn,", "tokens": ["Ohn", "Zwei\u00b7fel", "wirds", "ihr", "lie\u00b7ber", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn Silber, Gold und Edelstein,", "tokens": ["Denn", "Sil\u00b7ber", ",", "Gold", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie wird die Treue haben,", "tokens": ["Sie", "wird", "die", "Treu\u00b7e", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich wieder zu begaben.", "tokens": ["Dich", "wie\u00b7der", "zu", "be\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00f6r mich du arme Pilgerin,", "tokens": ["H\u00f6r", "mich", "du", "ar\u00b7me", "Pil\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die zu Wallfahrten hast den Sinn,", "tokens": ["Die", "zu", "Wall\u00b7fahr\u00b7ten", "hast", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Nicht wollest du vor\u00fcber gehen,", "tokens": ["Nicht", "wol\u00b7lest", "du", "vor\u00b7\u00fc\u00b7ber", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["