{"textgrid.poem.31183": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Es bleibt sich gleich!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bleibt sich gleich!", "tokens": ["Es", "bleibt", "sich", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ob du ein sogenannter Gl\u00fcckspilz bist,", "tokens": ["Ob", "du", "ein", "so\u00b7ge\u00b7nann\u00b7ter", "Gl\u00fccks\u00b7pilz", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der bunte W\u00e4sche tr\u00e4gt, Coupons abschneidet", "tokens": ["Der", "bun\u00b7te", "W\u00e4\u00b7sche", "tr\u00e4gt", ",", "Cou\u00b7pons", "ab\u00b7schnei\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nur Havannahs zu f\u00fcnf Mark das St\u00fcck raucht,", "tokens": ["Und", "nur", "Ha\u00b7van\u00b7nahs", "zu", "f\u00fcnf", "Mark", "das", "St\u00fcck", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "APPR", "CARD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ob du am Rand der staubigen Chausee", "tokens": ["Ob", "du", "am", "Rand", "der", "stau\u00b7bi\u00b7gen", "Chau\u00b7see"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bl\u00f6dsinnig niederkniest und Steine klopfst,", "tokens": ["Bl\u00f6d\u00b7sin\u00b7nig", "nie\u00b7der\u00b7kniest", "und", "Stei\u00b7ne", "klopfst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es bleibt sich gleich! Nur deine Brille thut's.", "tokens": ["Es", "bleibt", "sich", "gleich", "!", "Nur", "dei\u00b7ne", "Bril\u00b7le", "thut'", "s."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$.", "ADV", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der hohle Zahn, der dem Idioten weh thut,", "tokens": ["Der", "hoh\u00b7le", "Zahn", ",", "der", "dem", "I\u00b7dio\u00b7ten", "weh", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Schmerzt auch den besten Mathematiker.", "tokens": ["Schmerzt", "auch", "den", "bes\u00b7ten", "Ma\u00b7the\u00b7ma\u00b7ti\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Und die Carriere, die der Leutnant X macht,", "tokens": ["Und", "die", "Car\u00b7rie\u00b7re", ",", "die", "der", "Leut\u00b7nant", "X", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "XY", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Ist grad so gl\u00e4nzend und verf\u00fchr