{"dta.poem.13497": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n hier zu vertr\u00e4umen", "tokens": ["Wie", "sch\u00f6n", "hier", "zu", "ver\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Nacht im stillen Wald,", "tokens": ["Die", "Nacht", "im", "stil\u00b7len", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn in den dunklen B\u00e4umen", "tokens": ["Wenn", "in", "den", "dunk\u00b7len", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das alte M\u00e4hrchen hallt.", "tokens": ["Das", "al\u00b7te", "M\u00e4hr\u00b7chen", "hallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Berg' im Mondesschimmer", "tokens": ["Die", "Ber\u00b7g'", "im", "Mon\u00b7des\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie in Gedanken stehn,", "tokens": ["Wie", "in", "Ge\u00b7dan\u00b7ken", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und durch verworrne Tr\u00fcmmer", "tokens": ["Und", "durch", "ver\u00b7worr\u00b7ne", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Quellen klagend gehn.", "tokens": ["Die", "Quel\u00b7len", "kla\u00b7gend", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn m\u00fcd ging auf den Matten", "tokens": ["Denn", "m\u00fcd", "ging", "auf", "den", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sch\u00f6nheit nun zur Ruh,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "nun", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es deckt mit k\u00fchlen Schatten", "tokens": ["Es", "deckt", "mit", "k\u00fch\u00b7len", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Nacht das Liebchen zu.", "tokens": ["Die", "Nacht", "das", "Lieb\u00b7chen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das ist das irre Klagen", "tokens": ["Das", "ist", "das", "ir\u00b7re", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In stiller Waldespracht,", "tokens": ["In", "stil\u00b7ler", "Wal\u00b7des\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Nachtigallen schlagen", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von ihr die ganze Nacht.", "tokens": ["Von", "ihr", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Stern' gehn auf und nieder\u2014", "tokens": ["Die", "Stern'", "gehn", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann kommst du, Morgenwind,", "tokens": ["Wann", "kommst", "du", ",", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hebst die Schatten wieder", "tokens": ["Und", "hebst", "die", "Schat\u00b7ten", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dem vertr\u00e4umten Kind?", "tokens": ["Von", "dem", "ver\u00b7tr\u00e4um\u00b7ten", "Kind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Schon r\u00fchrt sich's in den B\u00e4umen,", "tokens": ["Schon", "r\u00fchrt", "sich's", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lerche weckt sie bald \u2014", "tokens": ["Die", "Ler\u00b7che", "weckt", "sie", "bald"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So will ich treu vertr\u00e4umen", "tokens": ["So", "will", "ich", "treu", "ver\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Nacht im stillen Wald.", "tokens": ["Die", "Nacht", "im", "stil\u00b7len", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}