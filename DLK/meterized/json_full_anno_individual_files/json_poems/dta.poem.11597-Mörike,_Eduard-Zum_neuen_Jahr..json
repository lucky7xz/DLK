{"dta.poem.11597": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zum neuen Jahr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Engelein leise", "tokens": ["Ein", "En\u00b7ge\u00b7lein", "lei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit rosigen F\u00fc\u00dfen", "tokens": ["Mit", "ro\u00b7si\u00b7gen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Erde betritt:", "tokens": ["Die", "Er\u00b7de", "be\u00b7tritt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So nahte der Morgen.", "tokens": ["So", "nah\u00b7te", "der", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Jauchzt ihm, ihr Frommen,", "tokens": ["Jauchzt", "ihm", ",", "ihr", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein heilig Willkommen,", "tokens": ["Ein", "hei\u00b7lig", "Will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ein heilig Willkommen!", "tokens": ["Ein", "hei\u00b7lig", "Will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Herz, jauchze du mit!", "tokens": ["Herz", ",", "jauch\u00b7ze", "du", "mit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "In Ihm sey's begonnen,", "tokens": ["In", "Ihm", "sey's", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der Monde und Sonnen", "tokens": ["Der", "Mon\u00b7de", "und", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "An blauen Gezelten", "tokens": ["An", "blau\u00b7en", "Ge\u00b7zel\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des Himmels bewegt.", "tokens": ["Des", "Him\u00b7mels", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Du, Vater, du rathe!", "tokens": ["Du", ",", "Va\u00b7ter", ",", "du", "ra\u00b7the", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Lenke du und wende!", "tokens": ["Len\u00b7ke", "du", "und", "wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Herr, dir in die H\u00e4nde", "tokens": ["Herr", ",", "dir", "in", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sey Anfang und Ende,", "tokens": ["Sey", "An\u00b7fang", "und", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Sey Alles gelegt!", "tokens": ["Sey", "Al\u00b7les", "ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}