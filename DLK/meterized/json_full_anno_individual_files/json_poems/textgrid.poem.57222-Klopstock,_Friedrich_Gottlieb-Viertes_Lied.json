{"textgrid.poem.57222": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Viertes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Freunde fehlt noch, die ihr mich k\u00fcnftig liebt!", "tokens": ["Ihr", "Freun\u00b7de", "fehlt", "noch", ",", "die", "ihr", "mich", "k\u00fcnf\u00b7tig", "liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PRELS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo seyd ihr? Eile, s\u00e4ume nicht, sch\u00f6ne Zeit!", "tokens": ["Wo", "seyd", "ihr", "?", "Ei\u00b7le", ",", "s\u00e4u\u00b7me", "nicht", ",", "sch\u00f6\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "NN", "$,", "VVFIN", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Komt, auserkohrne, helle Stunden,", "tokens": ["Komt", ",", "au\u00b7ser\u00b7kohr\u00b7ne", ",", "hel\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich sie seh', und sie sanft umarme!", "tokens": ["Da", "ich", "sie", "seh'", ",", "und", "sie", "sanft", "um\u00b7ar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KON", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und du, o Freundin, die du mich lieben wirst,", "tokens": ["Und", "du", ",", "o", "Freun\u00b7din", ",", "die", "du", "mich", "lie\u00b7ben", "wirst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "PRELS", "PPER", "PRF", "VVINF", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo bist du? Dich sucht, Beste, mein einsames,", "tokens": ["Wo", "bist", "du", "?", "Dich", "sucht", ",", "Bes\u00b7te", ",", "mein", "ein\u00b7sa\u00b7mes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "PPER", "VVFIN", "$,", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein f\u00fchlend Herz, in dunkler Zukunft,", "tokens": ["Mein", "f\u00fch\u00b7lend", "Herz", ",", "in", "dunk\u00b7ler", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Labyrinthe der Nacht hin suchts dich!", "tokens": ["Durch", "La\u00b7by\u00b7rin\u00b7the", "der", "Nacht", "hin", "suchts", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "H\u00e4lt dich, o Freundin, etwa die z\u00e4rtlichste", "tokens": ["H\u00e4lt", "dich", ",", "o", "Freun\u00b7din", ",", "et\u00b7wa", "die", "z\u00e4rt\u00b7lichs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "ADV", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von allen Frauen m\u00fctterlich ungest\u00fcm;", "tokens": ["Von", "al\u00b7len", "Frau\u00b7en", "m\u00fct\u00b7ter\u00b7lich", "un\u00b7ge\u00b7st\u00fcm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl dir! auf ihrem Schoosse lernst du", "tokens": ["Wohl", "dir", "!", "auf", "ih\u00b7rem", "Schoos\u00b7se", "lernst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tugend und Liebe zugleich empfinden!", "tokens": ["Tu\u00b7gend", "und", "Lie\u00b7be", "zu\u00b7gleich", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Doch hat dir Blumenkr\u00e4nze des Fr\u00fchlings Hand", "tokens": ["Doch", "hat", "dir", "Blu\u00b7men\u00b7kr\u00e4n\u00b7ze", "des", "Fr\u00fch\u00b7lings", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gestreut, und ruhst du, wo er im Schatten weht;", "tokens": ["Ge\u00b7streut", ",", "und", "ruhst", "du", ",", "wo", "er", "im", "Schat\u00b7ten", "weht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So f\u00fchl auch dort sie! Dieses Auge,", "tokens": ["So", "f\u00fchl", "auch", "dort", "sie", "!", "Die\u00b7ses", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "$.", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach dein von Z\u00e4rtlichkeit volles Auge,", "tokens": ["Ach", "dein", "von", "Z\u00e4rt\u00b7lich\u00b7keit", "vol\u00b7les", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und der in Z\u00e4hren schwimmende s\u00fcsse Blick,", "tokens": ["Und", "der", "in", "Z\u00e4h\u00b7ren", "schwim\u00b7men\u00b7de", "s\u00fcs\u00b7se", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "(die ganze Seele bildet in ihm sich mir!", "tokens": ["(", "die", "gan\u00b7ze", "See\u00b7le", "bil\u00b7det", "in", "ihm", "sich", "mir", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PRF", "PPER", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr heller Ernst, ihr Flug zu denken,", "tokens": ["Ihr", "hel\u00b7ler", "Ernst", ",", "ihr", "Flug", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leichter als Tanz in dem West und sch\u00f6ner!)", "tokens": ["Leich\u00b7ter", "als", "Tanz", "in", "dem", "West", "und", "sch\u00f6\u00b7ner", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KOUS", "NN", "APPR", "ART", "NN", "KON", "ADJD", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Die Mine, voll des Guten, des Edlen voll,", "tokens": ["Die", "Mi\u00b7ne", ",", "voll", "des", "Gu\u00b7ten", ",", "des", "Ed\u00b7len", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Diess vor Empfindung bebende sanfte Herz!", "tokens": ["Diess", "vor", "Emp\u00b7fin\u00b7dung", "be\u00b7ben\u00b7de", "sanf\u00b7te", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Diess alles, o die einst mich liebet!", "tokens": ["Diess", "al\u00b7les", ",", "o", "die", "einst", "mich", "lie\u00b7bet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "FM", "ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dieses geliebte Phantom ist mein! du,", "tokens": ["Die\u00b7ses", "ge\u00b7lieb\u00b7te", "Phan\u00b7tom", "ist", "mein", "!", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VAFIN", "PPOSAT", "$.", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Du selber fehlst mir! Einsam und wehmuthsvoll", "tokens": ["Du", "sel\u00b7ber", "fehlst", "mir", "!", "Ein\u00b7sam", "und", "weh\u00b7muths\u00b7voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "ADJD", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und still und weinend irr' ich, und suche dich,", "tokens": ["Und", "still", "und", "wei\u00b7nend", "irr'", "ich", ",", "und", "su\u00b7che", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dich, Beste, die mich k\u00fcnftig liebet,", "tokens": ["Dich", ",", "Bes\u00b7te", ",", "die", "mich", "k\u00fcnf\u00b7tig", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach die mich liebt! und noch fern von mir ist!", "tokens": ["Ach", "die", "mich", "liebt", "!", "und", "noch", "fern", "von", "mir", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PRELS", "PPER", "VVFIN", "$.", "KON", "ADV", "ADJD", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ihr Freunde fehlt noch, die ihr mich k\u00fcnftig liebt!", "tokens": ["Ihr", "Freun\u00b7de", "fehlt", "noch", ",", "die", "ihr", "mich", "k\u00fcnf\u00b7tig", "liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PRELS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo seyd ihr? Eile, s\u00e4ume nicht, sch\u00f6ne Zeit!", "tokens": ["W