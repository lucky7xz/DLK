{"dta.poem.12433": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Adelnssucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mancher jetzund nach Adel strebt,               ", "tokens": ["Man\u00b7cher", "je\u00b7tzund", "nach", "A\u00b7del", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "H\u00e4tt er nicht Geld,", "tokens": ["H\u00e4tt", "er", "nicht", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "W\u00fcrd \u00f6fter um sich schauen,", "tokens": ["W\u00fcrd", "\u00f6f\u00b7ter", "um", "sich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gedenken wer sein Vater war,", "tokens": ["Ge\u00b7den\u00b7ken", "wer", "sein", "Va\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ders ganze Jahr", "tokens": ["Ders", "gan\u00b7ze", "Jahr"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den Acker muste bauen;", "tokens": ["Den", "A\u00b7cker", "mus\u00b7te", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der jetzund sich", "tokens": ["Der", "je\u00b7tzund", "sich"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So gar h\u00f6flich", "tokens": ["So", "gar", "h\u00f6f\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Beyn Leuten thut aufschm\u00fccken,", "tokens": ["Beyn", "Leu\u00b7ten", "thut", "auf\u00b7schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "H\u00e4lts nicht daf\u00fcr,", "tokens": ["H\u00e4lts", "nicht", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Als wenn man sp\u00fcr,", "tokens": ["Als", "wenn", "man", "sp\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Da\u00df er den Pflug kann zwicken.", "tokens": ["Da\u00df", "er", "den", "Pflug", "kann", "zwi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn er nun kommt zum Abendtanz,", "tokens": ["Wenn", "er", "nun", "kommt", "zum", "A\u00b7bend\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gilt sein Kranz", "tokens": ["So", "gilt", "sein", "Kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mehr denn der andern allen.", "tokens": ["Mehr", "denn", "der", "an\u00b7dern", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er kr\u00fcmmt sich fast nach Adelssitt,", "tokens": ["Er", "kr\u00fcmmt", "sich", "fast", "nach", "A\u00b7dels\u00b7sitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein geme\u00dfner Tritt", "tokens": ["Sein", "ge\u00b7me\u00df\u00b7ner", "Tritt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Thut ihm selbst wohlgefallen.", "tokens": ["Thut", "ihm", "selbst", "wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer h\u00e4tt vertraut,", "tokens": ["Wer", "h\u00e4tt", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Da\u00df solches Kraut", "tokens": ["Da\u00df", "sol\u00b7ches", "Kraut"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In D\u00f6rfern auch sollt wachsen?", "tokens": ["In", "D\u00f6r\u00b7fern", "auch", "sollt", "wach\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wenn er nur spricht,", "tokens": ["Wenn", "er", "nur", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Er ist erwischt,", "tokens": ["Er", "ist", "er\u00b7wischt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ist b\u00e4urisch ausgelassen.", "tokens": ["Ist", "b\u00e4u\u00b7risch", "aus\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weisheit die thut ihm viel zu leid,", "tokens": ["Weis\u00b7heit", "die", "thut", "ihm", "viel", "zu", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Giebt b\u00f6s Bescheid,", "tokens": ["Giebt", "b\u00f6s", "Be\u00b7scheid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn mans ihm nicht will glauben,", "tokens": ["Wenn", "mans", "ihm", "nicht", "will", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "D\u00fcnkt sich in aller Sach gescheit,", "tokens": ["D\u00fcnkt", "sich", "in", "al\u00b7ler", "Sach", "ge\u00b7scheit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch fehlts ihm weit,", "tokens": ["Doch", "fehlts", "ihm", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sieht aus wie saure Trauben.", "tokens": ["Sieht", "aus", "wie", "sau\u00b7re", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Spiegel-Glas,", "tokens": ["Im", "Spie\u00b7gel\u00b7Glas", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wird sehen das,", "tokens": ["Wird", "se\u00b7hen", "das", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "PDS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der Kittel ihn bas zieret,", "tokens": ["Der", "Kit\u00b7tel", "ihn", "bas", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Den seiden Waat,", "tokens": ["Den", "sei\u00b7den", "Waat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.11": {"text": "Den Adelsstaat,", "tokens": ["Den", "A\u00b7dels\u00b7staat", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Zu b\u00e4urisch Art verf\u00fchret.", "tokens": ["Zu", "b\u00e4u\u00b7risch", "Art", "ver\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}