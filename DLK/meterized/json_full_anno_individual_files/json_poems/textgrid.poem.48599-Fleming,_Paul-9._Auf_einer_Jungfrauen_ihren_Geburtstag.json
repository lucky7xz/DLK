 Lieder,", "tokens": ["von", "uns", "h\u00f6\u00b7rest", "neu\u00b7e", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ihr und dir und uns zur Zier!", "tokens": ["ihr", "und", "dir", "und", "uns", "zur", "Zier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "KON", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}