{"textgrid.poem.41035": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Das Silberhaus am Tegernsee", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du im wei\u00dfen Zauberhaus", "tokens": ["O", "du", "im", "wei\u00b7\u00dfen", "Zau\u00b7ber\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am gr\u00fcnen Tegernsee,", "tokens": ["Am", "gr\u00fc\u00b7nen", "Te\u00b7gern\u00b7see", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist, ich sag' es frei heraus,", "tokens": ["Du", "bist", ",", "ich", "sag'", "es", "frei", "he\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All dieser Wunder Fee.", "tokens": ["All", "die\u00b7ser", "Wun\u00b7der", "Fee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie eine Perle wei\u00df und rein", "tokens": ["Wie", "ei\u00b7ne", "Per\u00b7le", "wei\u00df", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auftaucht im gr\u00fcnen Meer,", "tokens": ["Auf\u00b7taucht", "im", "gr\u00fc\u00b7nen", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So blickt dein Haus im Silberschein", "tokens": ["So", "blickt", "dein", "Haus", "im", "Sil\u00b7ber\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus gr\u00fcnen Matten her.", "tokens": ["Aus", "gr\u00fc\u00b7nen", "Mat\u00b7ten", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sehns\u00fcchtig blickt der See empor", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "blickt", "der", "See", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ihm in stiller Nacht,", "tokens": ["Zu", "ihm", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn es in seinem Silberflor", "tokens": ["Wenn", "es", "in", "sei\u00b7nem", "Sil\u00b7ber\u00b7flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Mondschein niederlacht.", "tokens": ["Im", "Mond\u00b7schein", "nie\u00b7der\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Schwellen seiner Wellenbrust", "tokens": ["Das", "Schwel\u00b7len", "sei\u00b7ner", "Wel\u00b7len\u00b7brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tut seine Liebe kund,", "tokens": ["Tut", "sei\u00b7ne", "Lie\u00b7be", "kund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df wohl, wen er sucht voll Lust,", "tokens": ["Ich", "wei\u00df", "wohl", ",", "wen", "er", "sucht", "voll", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich Fee, in seinem Grund.", "tokens": ["Dich", "Fee", ",", "in", "sei\u00b7nem", "Grund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie er blickt zu dir empor,", "tokens": ["Und", "wie", "er", "blickt", "zu", "dir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4chst auch zu ihm dein Sinn,", "tokens": ["W\u00e4chst", "auch", "zu", "ihm", "dein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Tuch von himmelblauem Flor", "tokens": ["Ein", "Tuch", "von", "him\u00b7mel\u00b7blau\u00b7em", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt durch die Luft dich hin.", "tokens": ["Tr\u00e4gt", "durch", "die", "Luft", "dich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Da unten im kristallnen Haus,", "tokens": ["Da", "un\u00b7ten", "im", "kris\u00b7tall\u00b7nen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wohltun ganz begl\u00fcckt,", "tokens": ["Im", "Wohl\u00b7tun", "ganz", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Macht er mit dir die Wunder aus,", "tokens": ["Macht", "er", "mit", "dir", "die", "Wun\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die rings er aufw\u00e4rts schickt.", "tokens": ["Die", "rings", "er", "auf\u00b7w\u00e4rts", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er macht mit dir den heitern Tag,", "tokens": ["Er", "macht", "mit", "dir", "den", "hei\u00b7tern", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Berge Farbenpracht,", "tokens": ["Der", "Ber\u00b7ge", "Far\u00b7ben\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der V\u00f6gel und der Wellen Schlag,", "tokens": ["Der", "V\u00f6\u00b7gel", "und", "der", "Wel\u00b7len", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Mondschein durch die Nacht,", "tokens": ["Den", "Mond\u00b7schein", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Matten lichtes, saft'ges Gr\u00fcn,", "tokens": ["Der", "Mat\u00b7ten", "lich\u00b7tes", ",", "saft'\u00b7ges", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der W\u00e4lder Nachtgewand,", "tokens": ["Der", "W\u00e4l\u00b7der", "Nacht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der stillen H\u00fctten friedlich Bl\u00fchn", "tokens": ["Der", "stil\u00b7len", "H\u00fct\u00b7ten", "fried\u00b7lich", "Bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Bergen und am Strand,", "tokens": ["Auf", "Ber\u00b7gen", "und", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Den Gottesfrieden in der Brust", "tokens": ["Den", "Got\u00b7tes\u00b7frie\u00b7den", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Wandrers, der hier weilt,", "tokens": ["Des", "Wand\u00b7rers", ",", "der", "hier", "weilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er, vertieft in all die Lust,", "tokens": ["Da\u00df", "er", ",", "ver\u00b7tieft", "in", "all", "die", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mehr zur Heimat eilt.", "tokens": ["Nicht", "mehr", "zur", "Hei\u00b7mat", "eilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dies alles denkt und macht mit dir", "tokens": ["Dies", "al\u00b7les", "denkt", "und", "macht", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gute Seegeist aus,", "tokens": ["Der", "gu\u00b7te", "See\u00b7geist", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann schwebst du wieder fort von hier", "tokens": ["Dann", "schwebst", "du", "wie\u00b7der", "fort", "von", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gen Berg ins Silberhaus.", "tokens": ["Gen", "Berg", "ins", "Sil\u00b7ber\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wer dich dort sieht im blauen Kleid,", "tokens": ["Wer", "dich", "dort", "sieht", "im", "blau\u00b7en", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer h\u00f6ret den Gesang,", "tokens": ["Wer", "h\u00f6\u00b7ret", "den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der oft schon durch die Einsamkeit", "tokens": ["Der", "oft", "schon", "durch", "die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinab zu lauschen drang,", "tokens": ["Hin\u00b7ab", "zu", "lau\u00b7schen", "drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der ahnet, \u2013 spricht er auch nicht aus,", "tokens": ["Der", "ah\u00b7net", ",", "\u2013", "spricht", "er", "auch", "nicht", "aus", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "$(", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df eine Fee du bist, \u2013", "tokens": ["Da\u00df", "ei\u00b7ne", "Fee", "du", "bist", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN