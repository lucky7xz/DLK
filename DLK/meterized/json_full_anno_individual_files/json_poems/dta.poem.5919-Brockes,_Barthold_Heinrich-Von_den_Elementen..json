{"dta.poem.5919": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die kleinen K\u00fcgelchen, die fest und dicht,", "tokens": ["Die", "klei\u00b7nen", "K\u00fc\u00b7gel\u00b7chen", ",", "die", "fest", "und", "dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Die nimmer ihren Lauff verlieren,", "tokens": ["Die", "nim\u00b7mer", "ih\u00b7ren", "Lauff", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die sich nicht", "tokens": ["Und", "die", "sich", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "PRF", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Vermischen, ob sie sich gleich stets ber\u00fchren,", "tokens": ["Ver\u00b7mi\u00b7schen", ",", "ob", "sie", "sich", "gleich", "stets", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sind die, so in der Welt was fl\u00fcssig ist, formiren.", "tokens": ["Sind", "die", ",", "so", "in", "der", "Welt", "was", "fl\u00fcs\u00b7sig", "ist", ",", "for\u00b7mi\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "$,", "ADV", "APPR", "ART", "NN", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie r\u00fchren und bewegen sich gar bald", "tokens": ["Sie", "r\u00fch\u00b7ren", "und", "be\u00b7we\u00b7gen", "sich", "gar", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Durch ihre fl\u00fcchtige, bewegliche Gestallt;", "tokens": ["Durch", "ih\u00b7re", "fl\u00fcch\u00b7ti\u00b7ge", ",", "be\u00b7weg\u00b7li\u00b7che", "Ge\u00b7stallt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Daher in grossen Kreis ihr Gantzes hefftig rennt.", "tokens": ["Da\u00b7her", "in", "gros\u00b7sen", "Kreis", "ihr", "Gant\u00b7zes", "heff\u00b7tig", "rennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dies ist ", "tokens": ["Dies", "ist"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}}}}