{"dta.poem.20024": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sankt Alban .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es steht dem Land zum Gru\u00dfe", "tokens": ["Es", "steht", "dem", "Land", "zum", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Kreutz auf Berges H\u00f6h',", "tokens": ["Ein", "Kreutz", "auf", "Ber\u00b7ges", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leis' wallt zu seinem Fu\u00dfe", "tokens": ["Leis'", "wallt", "zu", "sei\u00b7nem", "Fu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein himmelblauer See.", "tokens": ["Ein", "him\u00b7mel\u00b7blau\u00b7er", "See", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel duft'ge Kr\u00e4uter bl\u00fchen", "tokens": ["Viel", "duft'\u00b7ge", "Kr\u00e4u\u00b7ter", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An dieses Wassers Rand,", "tokens": ["An", "die\u00b7ses", "Was\u00b7sers", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Viel fromme Pilger ziehen", "tokens": ["Viel", "from\u00b7me", "Pil\u00b7ger", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dahin vom fernen Land.", "tokens": ["Da\u00b7hin", "vom", "fer\u00b7nen", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl vor zw\u00f6lfhundert Jahren,", "tokens": ["Wohl", "vor", "zw\u00f6lf\u00b7hun\u00b7dert", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lag die\u00df Land gar wild,", "tokens": ["Da", "lag", "die\u00df", "Land", "gar", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wald mit Thiereschaaren,", "tokens": ["Der", "Wald", "mit", "Thie\u00b7re\u00b7schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der See mit Gift erf\u00fcllt:", "tokens": ["Der", "See", "mit", "Gift", "er\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die an des Kreutzes Stelle", "tokens": ["Die", "an", "des", "Kreut\u00b7zes", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein schlimmer Felsen war,", "tokens": ["Ein", "schlim\u00b7mer", "Fel\u00b7sen", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Stellend zur Lust der H\u00f6lle", "tokens": ["Stel\u00b7lend", "zur", "Lust", "der", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Des Satans Bildni\u00df dar.", "tokens": ["Des", "Sa\u00b7tans", "Bild\u00b7ni\u00df", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kalt, wie des Mondes Strahlen,", "tokens": ["Kalt", ",", "wie", "des", "Mon\u00b7des", "Strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Blickt' es in's Land hinein,", "tokens": ["Blickt'", "es", "in's", "Land", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Fluch den H\u00f6h'n und Thalen;", "tokens": ["Zum", "Fluch", "den", "H\u00f6h'n", "und", "Tha\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Statt Blumen wuchsen Stein',", "tokens": ["Statt", "Blu\u00b7men", "wuch\u00b7sen", "Stein'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Statt Menschen wurden Drachen,", "tokens": ["Statt", "Men\u00b7schen", "wur\u00b7den", "Dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Statt Fischlein Schlangen im See,", "tokens": ["Statt", "Fisc\u00b7hlein", "Schlan\u00b7gen", "im", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die H\u00f6lle sah's mit Lachen", "tokens": ["Die", "H\u00f6l\u00b7le", "sah's", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und pries das Bild der H\u00f6h'.", "tokens": ["Und", "pries", "das", "Bild", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da kam vom fernen Strande", "tokens": ["Da", "kam", "vom", "fer\u00b7nen", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sankt Alban, stark und k\u00fchn,", "tokens": ["Sankt", "Al\u00b7ban", ",", "stark", "und", "k\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu diesem wilden Lande,", "tokens": ["Zu", "die\u00b7sem", "wil\u00b7den", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu diesem Felsen hin.", "tokens": ["Zu", "die\u00b7sem", "Fel\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihn fa\u00dft' des Landes Jammer,", "tokens": ["Ihn", "fa\u00dft'", "des", "Lan\u00b7des", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er sprang zum Felsenwall,", "tokens": ["Er", "sprang", "zum", "Fel\u00b7sen\u00b7wall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zerschlug mit starkem Hammer", "tokens": ["Zer\u00b7schlug", "mit", "star\u00b7kem", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Bild, \u2014 es fiel mit Schall.", "tokens": ["Das", "Bild", ",", "es", "fiel", "mit", "Schall", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Dankvoll, da\u00df ihm's gelungen,", "tokens": ["Dank\u00b7voll", ",", "da\u00df", "ihm's", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kniet' er dort auf den H\u00f6h'n,", "tokens": ["Kniet'", "er", "dort", "auf", "den", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Fels, der war zersprungen,", "tokens": ["Der", "Fels", ",", "der", "war", "zer\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Kreuz daraus blieb steh'n.", "tokens": ["Ein", "Kreuz", "da\u00b7raus", "blieb", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "PAV", "VVFIN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie dasselbe blickte", "tokens": ["Und", "wie", "das\u00b7sel\u00b7be", "blick\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PDAT", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weit in das Land hinein:", "tokens": ["Weit", "in", "das", "Land", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man Ros' und Lilie pfl\u00fcckte", "tokens": ["Man", "Ros'", "und", "Li\u00b7lie", "pfl\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In lindem Maienschein.", "tokens": ["In", "lin\u00b7dem", "Mai\u00b7en\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da lagen in den Kl\u00fcften", "tokens": ["Da", "la\u00b7gen", "in", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erdr\u00fcckt die Drachen all,", "tokens": ["Er\u00b7dr\u00fcckt", "die", "Dra\u00b7chen", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sang in Blumend\u00fcften", "tokens": ["Da", "sang", "in", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So manche Nachtigall,", "tokens": ["So", "man\u00b7che", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel Fischlein, silberhelle,", "tokens": ["Viel", "Fisc\u00b7hlein", ",", "sil\u00b7ber\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Waren im See zu schau'n,", "tokens": ["Wa\u00b7ren", "im", "See", "zu", "schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und an Sankt Albans Stelle,", "tokens": ["Und", "an", "Sankt", "Al\u00b7bans", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da knieten zarte Fraun.", "tokens": ["Da", "knie\u00b7ten", "zar\u00b7te", "Fraun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}