{"textgrid.poem.33744": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie aus Erinnrung eigner Jugendf\u00e4hrte", "tokens": ["Wie", "aus", "E\u00b7rinn\u00b7rung", "eig\u00b7ner", "Ju\u00b7gend\u00b7f\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Taucht mir aus ferner Zeit die fromme Kunde,", "tokens": ["Taucht", "mir", "aus", "fer\u00b7ner", "Zeit", "die", "from\u00b7me", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Bild empor, o Vorzeit, lichtverkl\u00e4rte,", "tokens": ["Dein", "Bild", "em\u00b7por", ",", "o", "Vor\u00b7zeit", ",", "licht\u00b7ver\u00b7kl\u00e4r\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Menschheit-Morgenland und Morgenstunde!", "tokens": ["Du", "Menschheit\u00b7Mor\u00b7gen\u00b7land", "und", "Mor\u00b7gen\u00b7stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Treu wie das Jahr in seiner Sternenrunde", "tokens": ["Treu", "wie", "das", "Jahr", "in", "sei\u00b7ner", "Ster\u00b7nen\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", 