, "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Sie Gnade uns beweiset.", "tokens": ["Sie", "Gna\u00b7de", "uns", "be\u00b7wei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die K\u00f6nigin im Himmel ist", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "im", "Him\u00b7mel", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mutter des Sohnes Jesu Christ,", "tokens": ["Mut\u00b7ter", "des", "Soh\u00b7nes", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "In Schmerzen", "tokens": ["In", "Schmer\u00b7zen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Trost der Herzen.", "tokens": ["Trost", "der", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}