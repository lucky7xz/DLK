{"textgrid.poem.36374": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Bild des Menschlichen Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich neulich fr\u00fch am Morgen,", "tokens": ["Ich", "neu\u00b7lich", "fr\u00fch", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu edler Fr\u00fchlingszeit,", "tokens": ["Zu", "ed\u00b7ler", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War losgemacht von Sorgen,", "tokens": ["War", "los\u00b7ge\u00b7macht", "von", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von aller Last befreit;", "tokens": ["Von", "al\u00b7ler", "Last", "be\u00b7freit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sah ich in dem Garten", "tokens": ["Da", "sah", "ich", "in", "dem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erbl\u00fcht ein Bl\u00fcmlein zart,", "tokens": ["Er\u00b7bl\u00fcht", "ein", "Bl\u00fcm\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das wollt' ich gerne warten,", "tokens": ["Das", "wollt'", "ich", "ger\u00b7ne", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis es vollkommen ward.", "tokens": ["Bis", "es", "voll\u00b7kom\u00b7men", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Morgenr\u00f6t' verschwunde,", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6t'", "ver\u00b7schwun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil ihren Purpurschein", "tokens": ["Weil", "ih\u00b7ren", "Pur\u00b7pur\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der helle Tag umwunde", "tokens": ["Der", "hel\u00b7le", "Tag", "um\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit gro\u00dfer Klarheit sein.", "tokens": ["Mit", "gro\u00b7\u00dfer", "Klar\u00b7heit", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Sonn' mit sanften Strahlen", "tokens": ["Die", "Sonn'", "mit", "sanf\u00b7ten", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Bl\u00fcmlein \u00fcbergo\u00df,", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "\u00fc\u00b7ber\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "All' Bl\u00e4ttlein sie t\u00e4t malen,", "tokens": ["All'", "Bl\u00e4tt\u00b7lein", "sie", "t\u00e4t", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Tief in den Bl\u00fctenscho\u00df.", "tokens": ["Tief", "in", "den", "Bl\u00fc\u00b7ten\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Nun es gar lieblich blickte,", "tokens": ["Nun", "es", "gar", "lieb\u00b7lich", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und s\u00fc\u00df Geruch ausstreut,", "tokens": ["Und", "s\u00fc\u00df", "Ge\u00b7ruch", "aus\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Leiden es erquickte,", "tokens": ["Die", "Lei\u00b7den", "es", "er\u00b7quick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sterbenden erfreut.", "tokens": ["Die", "Ster\u00b7ben\u00b7den", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein L\u00fcftlein lind von Atem", "tokens": ["Ein", "L\u00fcft\u00b7lein", "lind", "von", "A\u00b7tem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "R\u00fchrt an das Bl\u00fcmelein,", "tokens": ["R\u00fchrt", "an", "das", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da schwebts wie an dem Faden", "tokens": ["Da", "schwebts", "wie", "an", "dem", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gebundnes V\u00f6gelein.", "tokens": ["Ge\u00b7bund\u00b7nes", "V\u00f6\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf feinem Stiel im Mute", "tokens": ["Auf", "fei\u00b7nem", "Stiel", "im", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich wendt' es hin und her,", "tokens": ["Sich", "wendt'", "es", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Kraft und Lebensblute", "tokens": ["Voll", "Kraft", "und", "Le\u00b7bens\u00b7blu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als k\u00e4m' der Tod nie mehr.", "tokens": ["Als", "k\u00e4m'", "der", "Tod", "nie", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Bl\u00fcmlein sch\u00f6n ohn' Ma\u00dfen", "tokens": ["O", "Bl\u00fcm\u00b7lein", "sch\u00f6n", "ohn'", "Ma\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bist du in deiner Zier;", "tokens": ["Bist", "du", "in", "dei\u00b7ner", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Will nun von dir nicht lassen", "tokens": ["Will", "nun", "von", "dir", "nicht", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis auf den Abend hier.", "tokens": ["Bis", "auf", "den", "A\u00b7bend", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ei, wer mag dann aussprechen", "tokens": ["Ei", ",", "wer", "mag", "dann", "aus\u00b7spre\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VMFIN", "ADV", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dein Sch\u00f6n und Lieblichkeit;", "tokens": ["Dein", "Sch\u00f6n", "und", "Lieb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dir wei\u00df kein Gebrechen,", "tokens": ["In", "dir", "wei\u00df", "kein", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bist voller Zierlichkeit.", "tokens": ["Bist", "vol\u00b7ler", "Zier\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja Salomon, der m\u00e4chtig,", "tokens": ["Ja", "Sa\u00b7lo\u00b7mon", ",", "der", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "NE", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War nicht so sch\u00f6n bekleid't,", "tokens": ["War", "nicht", "so", "sch\u00f6n", "be\u00b7kleid't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann er schon leuchtet pr\u00e4chtig", "tokens": ["Wann", "er", "schon", "leuch\u00b7tet", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Pomp und Herrlichkeit.", "tokens": ["In", "Pomp", "und", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Um dich die Bienlein summen", "tokens": ["Um", "dich", "die", "Bien\u00b7lein", "sum\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Honig sammeln ein,", "tokens": ["Und", "Ho\u00b7nig", "sam\u00b7meln", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu saugen sie da kummen", "tokens": ["Zu", "sau\u00b7gen", "sie", "da", "kum\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein' zarte W\u00e4ngelein.", "tokens": ["Dein'", "zar\u00b7te", "W\u00e4n\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Menschen Kind imgleichen", "tokens": ["Der", "Men\u00b7schen", "Kind", "im\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Lust dich schauen an;", "tokens": ["Mit", "Lust", "dich", "schau\u00b7en", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "All' Sch\u00f6nheit mu\u00df dir weichen,", "tokens": ["All'", "Sch\u00f6n\u00b7heit", "mu\u00df", "dir", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein freut sich jedermann.", "tokens": ["Dein", "freut", "sich", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PRF", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohl magst du nun stolzieren.", "tokens": ["Wohl", "magst", "du", "nun", "stol\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Gartensternelein,", "tokens": ["Du", "Gar\u00b7tens\u00b7ter\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00dft endlich doch verlieren", "tokens": ["Mu\u00dft", "end\u00b7lich", "doch", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "All' dein gef\u00e4rbten Schein;", "tokens": ["All'", "dein", "ge\u00b7f\u00e4rb\u00b7ten", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu bald wirst dich entf\u00e4rben,", "tokens": ["Zu", "bald", "wirst", "dich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gestalt wird rei\u00dfen ab;", "tokens": ["Ge\u00b7stalt", "wird", "rei\u00b7\u00dfen", "ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Noch heute mu\u00dft du sterben,", "tokens": ["Noch", "heu\u00b7te", "mu\u00dft", "du", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denk zeitlich nur zum Grab.", "tokens": ["Denk", "zeit\u00b7lich", "nur", "zum", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich zwar will dich nicht brechen,", "tokens": ["Ich", "zwar", "will", "dich", "nicht", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von mir wird's nicht geschehn,", "tokens": ["Von", "mir", "wird's", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Sonne wird dich stechen,", "tokens": ["Die", "Son\u00b7ne", "wird", "dich", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirst nicht mehr lange stehn.", "tokens": ["Wirst", "nicht", "mehr", "lan\u00b7ge", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, halt, soll's dann schon werden? \u2013", "tokens": ["Ach", ",", "halt", ",", "soll's", "dann", "schon", "wer\u00b7den", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "VMFIN", "ADV", "ADV", "VAINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verdoppelt schie\u00dft den Strahl", "tokens": ["Ver\u00b7dop\u00b7pelt", "schie\u00dft", "den", "Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Sonne her zur Erden", "tokens": ["Die", "Son\u00b7ne", "her", "zur", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "All' Glut und Feuers Qual.", "tokens": ["All'", "Glut", "und", "Feu\u00b7ers", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ach was will nun beginnen", "tokens": ["Ach", "was", "will", "nun", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So zartes Gartenblut,", "tokens": ["So", "zar\u00b7tes", "Gar\u00b7ten\u00b7blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Bl\u00e4ttlein gar entrinnen", "tokens": ["Die", "Bl\u00e4tt\u00b7lein", "gar", "ent\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von hei\u00dfer Sonnenglut.", "tokens": ["Von", "hei\u00b7\u00dfer", "Son\u00b7nen\u00b7glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da neigt es sich zur Stunde,", "tokens": ["Da", "neigt", "es", "sich", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das jetzt noch aufrecht stunde", "tokens": ["Das", "jetzt", "noch", "auf\u00b7recht", "stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Verwelkt und sinket hin,", "tokens": ["Ver\u00b7welkt", "und", "sin\u00b7ket", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit also stolzem Sinn.", "tokens": ["Mit", "al\u00b7so", "stol\u00b7zem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das Bl\u00fcmlein jung an Tagen,", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "jung", "an", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein H\u00e4uptlein niedersenkt,", "tokens": ["Sein", "H\u00e4upt\u00b7lein", "nie\u00b7der\u00b7senkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, ach, sollt' ich nicht klagen,", "tokens": ["Ach", ",", "ach", ",", "sollt'", "ich", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was mich im Herzen kr\u00e4nkt.", "tokens": ["Was", "mich", "im", "Her\u00b7zen", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O weh der kurzen Stunden,", "tokens": ["O", "weh", "der", "kur\u00b7zen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O weh, da schl\u00e4ft es ein,", "tokens": ["O", "weh", ",", "da", "schl\u00e4ft", "es", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt liegt es \u00fcberwunden", "tokens": ["Jetzt", "liegt", "es", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein zartes Bl\u00fcmelein.", "tokens": ["Mein", "zar\u00b7tes", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich neulich fr\u00fch am Morgen,", "tokens": ["Ich", "neu\u00b7lich", "fr\u00fch", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu edler Fr\u00fchlingszeit,", "tokens": ["Zu", "ed\u00b7ler", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War losgemacht von Sorgen,", "tokens": ["War", "los\u00b7ge\u00b7macht", "von", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von aller Last befreit;", "tokens": ["Von", "al\u00b7ler", "Last", "be\u00b7freit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sah ich in dem Garten", "tokens": ["Da", "sah", "ich", "in", "dem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erbl\u00fcht ein Bl\u00fcmlein zart,", "tokens": ["Er\u00b7bl\u00fcht", "ein", "Bl\u00fcm\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das wollt' ich gerne warten,", "tokens": ["Das", "wollt'", "ich", "ger\u00b7ne", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis es vollkommen ward.", "tokens": ["Bis", "es", "voll\u00b7kom\u00b7men", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Morgenr\u00f6t' verschwunde,", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6t'", "ver\u00b7schwun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil ihren Purpurschein", "tokens": ["Weil", "ih\u00b7ren", "Pur\u00b7pur\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der helle Tag umwunde", "tokens": ["Der", "hel\u00b7le", "Tag", "um\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit gro\u00dfer Klarheit sein.", "tokens": ["Mit", "gro\u00b7\u00dfer", "Klar\u00b7heit", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Sonn' mit sanften Strahlen", "tokens": ["Die", "Sonn'", "mit", "sanf\u00b7ten", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Bl\u00fcmlein \u00fcbergo\u00df,", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "\u00fc\u00b7ber\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "All' Bl\u00e4ttlein sie t\u00e4t malen,", "tokens": ["All'", "Bl\u00e4tt\u00b7lein", "sie", "t\u00e4t", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Tief in den Bl\u00fctenscho\u00df.", "tokens": ["Tief", "in", "den", "Bl\u00fc\u00b7ten\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Nun es gar lieblich blickte,", "tokens": ["Nun", "es", "gar", "lieb\u00b7lich", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und s\u00fc\u00df Geruch ausstreut,", "tokens": ["Und", "s\u00fc\u00df", "Ge\u00b7ruch", "aus\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Leiden es erquickte,", "tokens": ["Die", "Lei\u00b7den", "es", "er\u00b7quick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sterbenden erfreut.", "tokens": ["Die", "Ster\u00b7ben\u00b7den", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein L\u00fcftlein lind von Atem", "tokens": ["Ein", "L\u00fcft\u00b7lein", "lind", "von", "A\u00b7tem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "R\u00fchrt an das Bl\u00fcmelein,", "tokens": ["R\u00fchrt", "an", "das", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da schwebts wie an dem Faden", "tokens": ["Da", "schwebts", "wie", "an", "dem", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gebundnes V\u00f6gelein.", "tokens": ["Ge\u00b7bund\u00b7nes", "V\u00f6\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Auf feinem Stiel im Mute", "tokens": ["Auf", "fei\u00b7nem", "Stiel", "im", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich wendt' es hin und her,", "tokens": ["Sich", "wendt'", "es", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Kraft und Lebensblute", "tokens": ["Voll", "Kraft", "und", "Le\u00b7bens\u00b7blu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als k\u00e4m' der Tod nie mehr.", "tokens": ["Als", "k\u00e4m'", "der", "Tod", "nie", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Bl\u00fcmlein sch\u00f6n ohn' Ma\u00dfen", "tokens": ["O", "Bl\u00fcm\u00b7lein", "sch\u00f6n", "ohn'", "Ma\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bist du in deiner Zier;", "tokens": ["Bist", "du", "in", "dei\u00b7ner", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Will nun von dir nicht lassen", "tokens": ["Will", "nun", "von", "dir", "nicht", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis auf den Abend hier.", "tokens": ["Bis", "auf", "den", "A\u00b7bend", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ei, wer mag dann aussprechen", "tokens": ["Ei", ",", "wer", "mag", "dann", "aus\u00b7spre\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VMFIN", "ADV", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dein Sch\u00f6n und Lieblichkeit;", "tokens": ["Dein", "Sch\u00f6n", "und", "Lieb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dir wei\u00df kein Gebrechen,", "tokens": ["In", "dir", "wei\u00df", "kein", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bist voller Zierlichkeit.", "tokens": ["Bist", "vol\u00b7ler", "Zier\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja Salomon, der m\u00e4chtig,", "tokens": ["Ja", "Sa\u00b7lo\u00b7mon", ",", "der", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "NE", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War nicht so sch\u00f6n bekleid't,", "tokens": ["War", "nicht", "so", "sch\u00f6n", "be\u00b7kleid't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann er schon leuchtet pr\u00e4chtig", "tokens": ["Wann", "er", "schon", "leuch\u00b7tet", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Pomp und Herrlichkeit.", "tokens": ["In", "Pomp", "und", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Um dich die Bienlein summen", "tokens": ["Um", "dich", "die", "Bien\u00b7lein", "sum\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Honig sammeln ein,", "tokens": ["Und", "Ho\u00b7nig", "sam\u00b7meln", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", 