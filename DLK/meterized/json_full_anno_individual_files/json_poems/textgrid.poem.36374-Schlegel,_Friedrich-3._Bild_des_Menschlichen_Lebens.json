: {"text": "Du Gartensternelein,", "tokens": ["Du", "Gar\u00b7tens\u00b7ter\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00dft endlich doch verlieren", "tokens": ["Mu\u00dft", "end\u00b7lich", "doch", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "All' dein gef\u00e4rbten Schein;", "tokens": ["All'", "dein", "ge\u00b7f\u00e4rb\u00b7ten", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu bald wirst dich entf\u00e4rben,", "tokens": ["Zu", "bald", "wirst", "dich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gestalt wird rei\u00dfen ab;", "tokens": ["Ge\u00b7stalt", "wird", "rei\u00b7\u00dfen", "ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Noch heute mu\u00dft du sterben,", "tokens": ["Noch", "heu\u00b7te", "mu\u00dft", "du", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denk zeitlich nur zum Grab.", "tokens": ["Denk", "zeit\u00b7lich", "nur", "zum", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Ich zwar will dich nicht brechen,", "tokens": ["Ich", "zwar", "will", "dich", "nicht", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von mir wird's nicht geschehn,", "tokens": ["Von", "mir", "wird's", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Sonne wird dich stechen,", "tokens": ["Die", "Son\u00b7ne", "wird", "dich", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirst nicht mehr lange stehn.", "tokens": ["Wirst", "nicht", "mehr", "lan\u00b7ge", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, halt, soll's dann schon werden? \u2013", "tokens": ["Ach", ",", "halt", ",", "soll's", "dann", "schon", "wer\u00b7den", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "VMFIN", "ADV", "ADV", "VAINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verdoppelt schie\u00dft den Strahl", "tokens": ["Ver\u00b7dop\u00b7pelt", "schie\u00dft", "den", "Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Sonne her zur Erden", "tokens": ["Die", "Son\u00b7ne", "her", "zur", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic