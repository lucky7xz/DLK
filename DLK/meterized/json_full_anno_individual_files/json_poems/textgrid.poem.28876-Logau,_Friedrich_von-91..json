{"textgrid.poem.28876": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fccke richtet auff; das Gl\u00fccke st\u00f6sset nieder;", "tokens": ["Das", "Gl\u00fc\u00b7cke", "rich\u00b7tet", "auff", ";", "das", "Gl\u00fc\u00b7cke", "st\u00f6s\u00b7set", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O, Gl\u00fccke thut es nicht! nach dem sich stellt ein ieder,", "tokens": ["O", ",", "Gl\u00fc\u00b7cke", "thut", "es", "nicht", "!", "nach", "dem", "sich", "stellt", "ein", "ie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "APPR", "PRELS", "PRF", "VVFIN", "ART", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach deme stellt sich Gl\u00fcck. Ein Sinn, dem stets gef\u00e4llt,", "tokens": ["Nach", "de\u00b7me", "stellt", "sich", "Gl\u00fcck", ".", "Ein", "Sinn", ",", "dem", "stets", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PRF", "NN", "$.", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was Gott gef\u00e4llt, steht stets, weil Zuversicht ihn h\u00e4lt.", "tokens": ["Was", "Gott", "ge\u00b7f\u00e4llt", ",", "steht", "stets", ",", "weil", "Zu\u00b7ver\u00b7sicht", "ihn