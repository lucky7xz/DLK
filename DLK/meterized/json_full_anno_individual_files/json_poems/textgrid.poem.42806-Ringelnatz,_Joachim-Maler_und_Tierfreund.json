, "vom", "Schwein\u00b7fur\u00b7ter", "Gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "VVFIN", "APPRART", "NN", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Vergiftet am Boden, verreckt.", "tokens": ["Ver\u00b7gif\u00b7tet", "am", "Bo\u00b7den", ",", "ver\u00b7reckt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "In den Himmel hatte sich eine Fliege geklebt,", "tokens": ["In", "den", "Him\u00b7mel", "hat\u00b7te", "sich", "ei\u00b7ne", "Flie\u00b7ge", "ge\u00b7klebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und zwar mit dem R\u00fccken.", "tokens": ["Und", "zwar", "mit", "dem", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die strampelte, wie man, wenn man Gro\u00dfes erlebt,", "tokens": ["Die", "stram\u00b7pel\u00b7te", ",", "wie", "man", ",", "wenn", "man", "Gro\u00b7\u00dfes", "er\u00b7lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PIS", "$,", "KOUS", "PIS", "NN", "VVPP", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit den Beinen strampelt vor lauter Entz\u00fccken.", "tokens": ["Mit", "den", "Bei\u00b7nen", "stram\u00b7pelt", "vor", "lau\u00b7ter", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "wo