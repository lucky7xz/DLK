{"textgrid.poem.42806": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Maler und Tierfreund", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte eine Landschaft in \u00d6l gemalt,", "tokens": ["Ich", "hat\u00b7te", "ei\u00b7ne", "Land\u00b7schaft", "in", "\u00d6l", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sie gefiel mir sehr:", "tokens": ["Und", "sie", "ge\u00b7fiel", "mir", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein blauer Himmel, aus dem die Sonne wie Wonne strahlt,", "tokens": ["Ein", "blau\u00b7er", "Him\u00b7mel", ",", "aus", "dem", "die", "Son\u00b7ne", "wie", "Won\u00b7ne", "strahlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und darunter weites, ruhiges, gr\u00fcnes Meer.", "tokens": ["Und", "da\u00b7run\u00b7ter", "wei\u00b7tes", ",", "ru\u00b7hi\u00b7ges", ",", "gr\u00fc\u00b7nes", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "\u00bbeinsame Sehnsucht.\u00ab", "tokens": ["\u00bb", "ein\u00b7sa\u00b7me", "Sehn\u00b7sucht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Danach fuhr ich irgendwo hin,", "tokens": ["Da\u00b7nach", "fuhr", "ich", "ir\u00b7gend\u00b7wo", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Um einen kleinen Affen zu erwerben,", "tokens": ["Um", "ei\u00b7nen", "klei\u00b7nen", "Af\u00b7fen", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil ich ein Tierfreund bin.", "tokens": ["Weil", "ich", "ein", "Tier\u00b7freund", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aber was einem die Tiere nicht alles verderben.", "tokens": ["A\u00b7ber", "was", "ei\u00b7nem", "die", "Tie\u00b7re", "nicht", "al\u00b7les", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ART", "NN", "PTKNEG", "PIS", "VVFIN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}, "stanza.3": {"line.1": {"text": "Wieder zu Haus, stie\u00df ich aus einen Schrei,", "tokens": ["Wie\u00b7der", "zu", "Haus", ",", "stie\u00df", "ich", "aus", "ei\u00b7nen", "Schrei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn mein Bild war verhext.", "tokens": ["Denn", "mein", "Bild", "war", "ver\u00b7hext", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Erstens hatte mein Papagei", "tokens": ["Ers\u00b7tens", "hat\u00b7te", "mein", "Pa\u00b7pa\u00b7gei"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Etwas Groteskes ins Meer gekleckst,", "tokens": ["Et\u00b7was", "Gro\u00b7tes\u00b7kes", "ins", "Meer", "ge\u00b7kleckst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und das geradezu k\u00fcnstlerisch k\u00fchn.", "tokens": ["Und", "das", "ge\u00b7ra\u00b7de\u00b7zu", "k\u00fcnst\u00b7le\u00b7risch", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aber das Wasser selber war abgeleckt", "tokens": ["A\u00b7ber", "das", "Was\u00b7ser", "sel\u00b7ber", "war", "ab\u00b7ge\u00b7leckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "VVPP"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Von meinem Wolfshund. Der lag vom Schweinfurter Gr\u00fcn", "tokens": ["Von", "mei\u00b7nem", "Wolfs\u00b7hund", ".", "Der", "lag", "vom", "Schwein\u00b7fur\u00b7ter", "Gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "VVFIN", "APPRART", "NN", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Vergiftet am Boden, verreckt.", "tokens": ["Ver\u00b7gif\u00b7tet", "am", "Bo\u00b7den", ",", "ver\u00b7reckt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "In den Himmel hatte sich eine Fliege geklebt,", "tokens": ["In", "den", "Him\u00b7mel", "hat\u00b7te", "sich", "ei\u00b7ne", "Flie\u00b7ge", "ge\u00b7klebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und zwar mit dem R\u00fccken.", "tokens": ["Und", "zwar", "mit", "dem", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die strampelte, wie man, wenn man Gro\u00dfes erlebt,", "tokens": ["Die", "stram\u00b7pel\u00b7te", ",", "wie", "man", ",", "wenn", "man", "Gro\u00b7\u00dfes", "er\u00b7lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PIS", "$,", "KOUS", "PIS", "NN", "VVPP", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit den Beinen strampelt vor lauter Entz\u00fccken.", "tokens": ["Mit", "den", "Bei\u00b7nen", "stram\u00b7pelt", "vor", "lau\u00b7ter", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und offenbar nicht minder begl\u00fcckt", "tokens": ["Und", "of\u00b7fen\u00b7bar", "nicht", "min\u00b7der", "be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In ihrer N\u00e4he", "tokens": ["In", "ih\u00b7rer", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Hing auch mein Laubfrosch ans Bild angedr\u00fcckt", "tokens": ["Hing", "auch", "mein", "Laub\u00b7fro\u00b7sch", "ans", "Bild", "an\u00b7ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVPP"], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und tat so, als ob er die Fliege nicht s\u00e4he.", "tokens": ["Und", "tat", "so", ",", "als", "ob", "er", "die", "Flie\u00b7ge", "nicht", "s\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOKOM", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Da wollte mein Affe mit lautem Geschrei \u2013 \u2013 \u2013", "tokens": ["Da", "woll\u00b7te", "mein", "Af\u00b7fe", "mit", "lau\u00b7tem", "Ge\u00b7schrei", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$(", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch ich band ihn fest. Und l\u00e4chelte dann.", "tokens": ["Doch", "ich", "band", "ihn", "fest", ".", "Und", "l\u00e4\u00b7chel\u00b7te", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "VVFIN", "ADV", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie gut, da\u00df man bei der \u00d6lmalerei", "tokens": ["Wie", "gut", ",", "da\u00df", "man", "bei", "der", "\u00d6l\u00b7ma\u00b7le\u00b7rei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PIS", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Alles noch \u00fcbermalen kann.", "tokens": ["Al\u00b7les", "noch", "\u00fc\u00b7berm\u00b7a\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Mit Phantasie das Gegebne fixiert \u2013", "tokens": ["Mit", "Phan\u00b7ta\u00b7sie", "das", "Ge\u00b7geb\u00b7ne", "fi\u00b7xiert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Genie und Farbe und Lichter dick aufgetragen \u2013", "tokens": ["Ge\u00b7nie", "und", "Far\u00b7be", "und", "Lich\u00b7ter", "dick", "auf\u00b7ge\u00b7tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schwarz, Wei\u00df, Rot, Ocker mutig dar\u00fcber geschmiert \u2013 \u2013 \u2013", "tokens": ["Schwarz", ",", "Wei\u00df", ",", "Rot", ",", "O\u00b7cker", "mu\u00b7tig", "da\u00b7r\u00fc\u00b7ber", "ge\u00b7schmiert", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "NN", "$,", "NN", "ADJD", "PAV", "VVPP", "$(", "$(", "$("], "meter": "+-++-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein sch\u00f6nes Bild, mu\u00df ich selber sagen,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Bild", ",", "mu\u00df", "ich", "sel\u00b7ber", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbmein Selbstportr\u00e4t\u00ab.", "tokens": ["\u00bb", "mein", "Selbst\u00b7por\u00b7tr\u00e4t", "\u00ab", "."], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$(", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Ich hatte eine Landschaft in \u00d6l gemalt,", "tokens": ["Ich", "hat\u00b7te", "ei\u00b7ne", "Land\u00b7schaft", "in", "\u00d6l", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sie gefiel mir sehr:", "tokens": ["Und", "sie", "ge\u00b7fiel", "mir", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein blauer Himmel, aus dem die Sonne wie Wonne strahlt,", "tokens": ["Ein", "blau\u00b7er", "Him\u00b7mel", ",", "aus", "dem", "die", "Son\u00b7ne", "wie", "Won\u00b7ne", "strahlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und darunter weites, ruhiges, gr\u00fcnes Meer.", "tokens": ["Und", "da\u00b7run\u00b7ter", "wei\u00b7tes", ",", "ru\u00b7hi\u00b7ges", ",", "gr\u00fc\u