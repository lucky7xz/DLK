{"dta.poem.12013": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der stiehlt dir, was er leicht von dir geschenkt bek\u00e4me;", "tokens": ["Der", "stiehlt", "dir", ",", "was", "er", "leicht", "von", "dir", "ge\u00b7schenkt", "be\u00b7k\u00e4\u00b7me", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADJD", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es macht' ihm minder Lust, wenn er's nicht heimlich n\u00e4hme.", "tokens": ["Es", "macht'", "ihm", "min\u00b7der", "Lust", ",", "wenn", "er's", "nicht", "heim\u00b7lich", "n\u00e4h\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "$,", "KOUS", "PIS", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was willst du ihm die Lust, die dir nicht schadet, st\u00f6ren?", "tokens": ["Was", "willst", "du", "ihm", "die", "Lust", ",", "die", "dir", "nicht", "scha\u00b7det", ",", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist sein eigner Thor und meinet dich zu th\u00f6ren.", "tokens": ["Er", "ist", "sein", "eig\u00b7ner", "Thor", "und", "mei\u00b7net", "dich", "zu", "th\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dir stehlend, hat er nichts dir als den Dank gestohlen;", "tokens": ["Dir", "steh\u00b7lend", ",", "hat", "er", "nichts", "dir", "als", "den", "Dank", "ge\u00b7stoh\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "VAFIN", "PPER", "PIS", "PPER", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch beschenkt, h\u00e4tt' er sich ohne Dank empfohlen.", "tokens": ["Und", "auch", "be\u00b7schenkt", ",", "h\u00e4tt'", "er", "sich", "oh\u00b7ne", "Dank", "emp\u00b7foh\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VAFIN", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}