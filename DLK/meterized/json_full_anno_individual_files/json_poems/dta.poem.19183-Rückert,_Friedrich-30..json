{"dta.poem.19183": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh in die Welt hinaus mit allen deinen Sinnen,", "tokens": ["Geh", "in", "die", "Welt", "hin\u00b7aus", "mit", "al\u00b7len", "dei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APZR", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um Bienen-gleich ins Haus den Honig zu gewinnen.", "tokens": ["Um", "Bie\u00b7nen\u00b7gleich", "ins", "Haus", "den", "Ho\u00b7nig", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wohin du fliegen magst, da bist du eingeladen,", "tokens": ["Wo\u00b7hin", "du", "flie\u00b7gen", "magst", ",", "da", "bist", "du", "ein\u00b7ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und irre kanst du auch nicht gehn auf allen Pfaden.", "tokens": ["Und", "ir\u00b7re", "kanst", "du", "auch", "nicht", "gehn", "auf", "al\u00b7len", "Pfa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dich zieht von hier und dort, von jedem Duftversuch,", "tokens": ["Dich", "zieht", "von", "hier", "und", "dort", ",", "von", "je\u00b7dem", "Duft\u00b7ver\u00b7such", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "KON", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu deinen Zellen heim der s\u00fc\u00dfre Honigruch,", "tokens": ["Zu", "dei\u00b7nen", "Zel\u00b7len", "heim", "der", "s\u00fc\u00df\u00b7re", "Ho\u00b7ni\u00b7gruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Vorrath st\u00e4rker, als den drau\u00dfen du begehrst,", "tokens": ["Der", "Vor\u00b7rath", "st\u00e4r\u00b7ker", ",", "als", "den", "drau\u00b7\u00dfen", "du", "be\u00b7gehrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KOUS", "ART", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den aber du mit dem von au\u00dfen immer mehrst.", "tokens": ["Den", "a\u00b7ber", "du", "mit", "dem", "von", "au\u00b7\u00dfen", "im\u00b7mer", "mehrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "APPR", "ART", "APPR", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "M\u00fcde von Flug und Braus, kehr' in dich still zur\u00fcck,", "tokens": ["M\u00fc\u00b7de", "von", "Flug", "und", "Braus", ",", "kehr'", "in", "dich", "still", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "APPR", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ruh' in dir selber aus, so f\u00fchlst du h\u00f6chstes Gl\u00fcck,", "tokens": ["Ruh'", "in", "dir", "sel\u00b7ber", "aus", ",", "so", "f\u00fchlst", "du", "h\u00f6chs\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.6": {"line.1": {"text": "Wenn du dich, ohne zu versinken, ganz versenkest,", "tokens": ["Wenn", "du", "dich", ",", "oh\u00b7ne", "zu", "ver\u00b7sin\u00b7ken", ",", "ganz", "ver\u00b7sen\u00b7kest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "KOUI", "PTKZU", "VVINF", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Ausdichtest spiegelglatt, was du durchsichtig denkest.", "tokens": ["Aus\u00b7dich\u00b7test", "spie\u00b7gel\u00b7glatt", ",", "was", "du", "durch\u00b7sich\u00b7tig", "den\u00b7kest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.7": {"line.1": {"text": "Wie du ziehst von Natur den Athem aus und ein,", "tokens": ["Wie", "du", "ziehst", "von", "Na\u00b7tur", "den", "A\u00b7them", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lern' auch im Geiste nur au\u00dfen und innen seyn.", "tokens": ["Lern'", "auch", "im", "Geis\u00b7te", "nur", "au\u00b7\u00dfen", "und", "in\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ADV", "ADV", "KON", "ADV", "VAINF", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}