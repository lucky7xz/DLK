], "pos": ["KON", "ADV", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Augen meine Lust,", "tokens": ["Vor", "Au\u00b7gen", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In jeden Baum sie grab' ich,", "tokens": ["In", "je\u00b7den", "Baum", "sie", "grab'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es ist ein schlechter Trost,", "tokens": ["Es", "ist", "ein", "schlech\u00b7ter", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch keinen bessern hab' ich.", "tokens": ["Doch", "kei\u00b7nen", "bes\u00b7sern", "hab'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VAFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}