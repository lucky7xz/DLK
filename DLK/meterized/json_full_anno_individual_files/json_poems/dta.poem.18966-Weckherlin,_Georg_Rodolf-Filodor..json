{"dta.poem.18966": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Filodor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r/ wie der Widerhall/", "tokens": ["H\u00f6r", "/", "wie", "der", "Wi\u00b7der\u00b7hall", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "H\u00f6r/ wie die Nachtigall", "tokens": ["H\u00f6r", "/", "wie", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "(als die die Lieb auch fihlen)", "tokens": ["(", "als", "die", "die", "Lieb", "auch", "fih\u00b7len", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von deiner Sch\u00f6nheit spihlen", "tokens": ["Von", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "spih\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit zwitzerendem schall:", "tokens": ["Mit", "zwit\u00b7ze\u00b7ren\u00b7dem", "schall", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00f6r/ mit wie s\u00fcssem singen", "tokens": ["H\u00f6r", "/", "mit", "wie", "s\u00fcs\u00b7sem", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "APPR", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die pr\u00e4chtig-s\u00fcsse macht/", "tokens": ["Die", "pr\u00e4ch\u00b7tig\u00b7s\u00fcs\u00b7se", "macht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den m\u00e4chtig-s\u00fcssen pracht", "tokens": ["Den", "m\u00e4ch\u00b7tig\u00b7s\u00fcs\u00b7sen", "pracht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Lieb Sie vns f\u00fcrbringen!", "tokens": ["Der", "Lieb", "Sie", "vns", "f\u00fcr\u00b7brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}