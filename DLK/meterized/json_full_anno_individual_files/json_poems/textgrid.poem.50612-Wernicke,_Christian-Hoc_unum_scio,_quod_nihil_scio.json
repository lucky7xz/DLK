{"textgrid.poem.50612": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Hoc unum scio, quod nihil scio", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du denkst, da\u00df Jedermann aus deinen eiteln Schl\u00fcssen", "tokens": ["Du", "denkst", ",", "da\u00df", "Je\u00b7der\u00b7mann", "aus", "dei\u00b7nen", "ei\u00b7teln", "Schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und falscher Wissenschaft sehr gro\u00dfe Klugheit schleu\u00dft;", "tokens": ["Und", "fal\u00b7scher", "Wis\u00b7sen\u00b7schaft", "sehr", "gro\u00b7\u00dfe", "Klug\u00b7heit", "schleu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was n\u00fctzt es, wenn du gleich wei\u00dft, da\u00df du gar nichts", "tokens": ["Was", "n\u00fctzt", "es", ",", "wenn", "du", "gleich", "wei\u00dft", ",", "da\u00df", "du", "gar", "nichts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIS"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn du nicht wei\u00dft, da\u00df dies auch Andre von dir wissen!", "tokens": ["Wenn", "du", "nicht", "wei\u00dft", ",", "da\u00df", "dies", "auch", "And\u00b7re", "von", "dir", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "wor