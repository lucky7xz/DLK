{"dta.poem.7151": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So la\u00df denn itzt, o Ew\u2019ge G\u00fcte!", "tokens": ["So", "la\u00df", "denn", "itzt", ",", "o", "Ew'\u00b7ge", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Dancken Dir gef\u00e4llig seyn,", "tokens": ["Mein", "Dan\u00b7cken", "Dir", "ge\u00b7f\u00e4l\u00b7lig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ich, voll Andacht, Dir allein,", "tokens": ["Das", "ich", ",", "voll", "An\u00b7dacht", ",", "Dir", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "ADJD", "NN", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus inniglich-ger\u00fchretem Gem\u00fcthe,", "tokens": ["Aus", "in\u00b7nig\u00b7lich\u00b7ge\u00b7r\u00fch\u00b7re\u00b7tem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum Anfang dieses Jahrs, beflissen bin zu weih\u2019n!", "tokens": ["Zum", "An\u00b7fang", "die\u00b7ses", "Jahrs", ",", "be\u00b7flis\u00b7sen", "bin", "zu", "weih'n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}