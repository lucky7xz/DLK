{"dta.poem.8607": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Abendheimkehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Sein B\u00fcndel Holz am R\u00fccken bringt", "tokens": ["Sein", "B\u00fcn\u00b7del", "Holz", "am", "R\u00fc\u00b7cken", "bringt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Arme heimgetragen;", "tokens": ["Der", "Ar\u00b7me", "heim\u00b7ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der frohe Knecht die Gei\u00dfel schwingt", "tokens": ["Der", "fro\u00b7he", "Knecht", "die", "Gei\u00b7\u00dfel", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am erndtevollen Wagen.", "tokens": ["Am", "ernd\u00b7te\u00b7vol\u00b7len", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die milchbeladne Heerde wiegt", "tokens": ["Die", "milch\u00b7be\u00b7lad\u00b7ne", "Heer\u00b7de", "wiegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich in die trauten St\u00e4lle;", "tokens": ["Sich", "in", "die", "trau\u00b7ten", "St\u00e4l\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Scherz und Ku\u00df zur Dirne fliegt", "tokens": ["Mit", "Scherz", "und", "Ku\u00df", "zur", "Dir\u00b7ne", "fliegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der lustige Geselle.", "tokens": ["Der", "lus\u00b7ti\u00b7ge", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von Feld und Walde pfeift nach Haus", "tokens": ["Von", "Feld", "und", "Wal\u00b7de", "pfeift", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der J\u00e4ger dort, der rasche,", "tokens": ["Der", "J\u00e4\u00b7ger", "dort", ",", "der", "ra\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Has' und Wachtel guckt heraus,", "tokens": ["Und", "Has'", "und", "Wach\u00b7tel", "guckt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu prahlen, aus der Tasche.", "tokens": ["Zu", "prah\u00b7len", ",", "aus", "der", "Ta\u00b7sche", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Den Dichter sieht man aus der Nacht", "tokens": ["Den", "Dich\u00b7ter", "sieht", "man", "aus", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Eichen selig schwanken;", "tokens": ["Der", "Ei\u00b7chen", "se\u00b7lig", "schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er taumelt heim mit seiner Tracht", "tokens": ["Er", "tau\u00b7melt", "heim", "mit", "sei\u00b7ner", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unsterblicher Gedanken.", "tokens": ["U\u00b7nsterb\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}