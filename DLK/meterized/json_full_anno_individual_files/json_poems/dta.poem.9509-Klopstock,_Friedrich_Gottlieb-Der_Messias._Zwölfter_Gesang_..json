{"dta.poem.9509": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der Messias.  \n Zw\u00f6lfter Gesang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1769", "urn": "urn:nbn:de:kobv:b4-200905199341", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb' ist, und bang in ihren verborgensten Tiefen die Seele,", "tokens": ["Tr\u00fcb'", "ist", ",", "und", "bang", "in", "ih\u00b7ren", "ver\u00b7bor\u00b7gens\u00b7ten", "Tie\u00b7fen", "die", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wenn sie f\u00fcrchtet, da\u00df Gott sie aus ihrem himmlischen Erbe", "tokens": ["Wenn", "sie", "f\u00fcrch\u00b7tet", ",", "da\u00df", "Gott", "sie", "aus", "ih\u00b7rem", "himm\u00b7li\u00b7schen", "Er\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "NN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sto\u00dfen werde. Verirrt in dem Labyrinthe der Vorsicht", "tokens": ["Sto\u00b7\u00dfen", "wer\u00b7de", ".", "Ver\u00b7irrt", "in", "dem", "La\u00b7by\u00b7rin\u00b7the", "der", "Vor\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wenden sich weg von weiterem Forschen alle Gedanken;", "tokens": ["Wen\u00b7den", "sich", "weg", "von", "wei\u00b7te\u00b7rem", "For\u00b7schen", "al\u00b7le", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PRF", "ADV", "APPR", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Jede von ihren Empfindungen treffen die Fl\u00fcche vom Sina,", "tokens": ["Je\u00b7de", "von", "ih\u00b7ren", "Emp\u00b7fin\u00b7dun\u00b7gen", "tref\u00b7fen", "die", "Fl\u00fc\u00b7che", "vom", "Si\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NE", "$,"], "meter": "+--+-+-+-+--+-+-+", "measure": "iambic.octa.plus.invert"}, "line.6": {"text": "Und vom Ebal, noch mehr des hohen Golgatha Schrecken.", "tokens": ["Und", "vom", "E\u00b7bal", ",", "noch", "mehr", "des", "ho\u00b7hen", "Gol\u00b7ga\u00b7tha", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADV", "ADV", "ART", "ADJA", "NE", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ach! nun wird sie das wei\u00dfe Gewand der Sieger nicht kleiden!", "tokens": ["Ach", "!", "nun", "wird", "sie", "das", "wei\u00b7\u00dfe", "Ge\u00b7wand", "der", "Sie\u00b7ger", "nicht", "klei\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Jhr die Palme der Ueberwinder im Himmel nicht werden!", "tokens": ["Ihr", "die", "Pal\u00b7me", "der", "Ue\u00b7ber\u00b7win\u00b7der", "im", "Him\u00b7mel", "nicht", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "APPRART", "NN", "PTKNEG", "VAINF", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und die Krone nicht strahlen! Sie liegt zerschmettert im Staube,", "tokens": ["Und", "die", "Kro\u00b7ne", "nicht", "strah\u00b7len", "!", "Sie", "liegt", "zer\u00b7schmet\u00b7tert", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "$.", "PPER", "VVFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Und sie w\u00fcrde vergehn, wenn sie Ein Gedanke nicht hielte,", "tokens": ["Und", "sie", "w\u00fcr\u00b7de", "ver\u00b7gehn", ",", "wenn", "sie", "Ein", "Ge\u00b7dan\u00b7ke", "nicht", "hiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Er ihr Retter nicht w\u00e4r, ihr Engel vom Himmel gesendet,", "tokens": ["Er", "ihr", "Ret\u00b7ter", "nicht", "w\u00e4r", ",", "ihr", "En\u00b7gel", "vom", "Him\u00b7mel", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "VAFIN", "$,", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Dieser gro\u00dfe: Sich Gott in Allem zu unterwerfen!", "tokens": ["Die\u00b7ser", "gro\u00b7\u00dfe", ":", "Sich", "Gott", "in", "Al\u00b7lem", "zu", "un\u00b7ter\u00b7wer\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$.", "PRF", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "So voll Jammers, und so von jeder Hoffnung verlassen", "tokens": ["So", "voll", "Jam\u00b7mers", ",", "und", "so", "von", "je\u00b7der", "Hoff\u00b7nung", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "$,", "KON", "ADV", "APPR", "PIAT", "NN", "VVINF"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "War der kleine Haufe der Wenigen unter den Menschen,", "tokens": ["War", "der", "klei\u00b7ne", "Hau\u00b7fe", "der", "We\u00b7ni\u00b7gen", "un\u00b7ter", "den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Die den Vers\u00f6hner des Ewigen kannten, da ihn ihr Auge", "tokens": ["Die", "den", "Ver\u00b7s\u00f6h\u00b7ner", "des", "E\u00b7wi\u00b7gen", "kann\u00b7ten", ",", "da", "ihn", "ihr", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "ADJA", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.16": {"text": "Starr, und todt auf Golgatha sah, und um ihn nun alles", "tokens": ["Starr", ",", "und", "todt", "auf", "Gol\u00b7ga\u00b7tha", "sah", ",", "und", "um", "ihn", "nun", "al\u00b7les"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ADJD", "APPR", "NE", "VVFIN", "$,", "KON", "APPR", "PPER", "ADV", "PIS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Oed\u2019 und verstummt; und so war\u2019s der von Arimath\u00e4a,", "tokens": ["O\u00b7ed'", "und", "ver\u00b7stummt", ";", "und", "so", "wa\u00b7r's", "der", "von", "A\u00b7rim\u00b7at\u00b7h\u00e4a", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$.", "KON", "ADV", "VAFIN", "ART", "APPR", "NE", "$,"], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Er der Eine, da\u00df sie nicht ganz dem Jammer erlagen.", "tokens": ["Er", "der", "Ei\u00b7ne", ",", "da\u00df", "sie", "nicht", "ganz", "dem", "Jam\u00b7mer", "er\u00b7la\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}}}}