{"dta.poem.11978": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu einem Manne, dem sein Kind gestorben war,", "tokens": ["Zu", "ei\u00b7nem", "Man\u00b7ne", ",", "dem", "sein", "Kind", "ge\u00b7stor\u00b7ben", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem mit Trost umsonst sich m\u00fchte Freundeschaar,", "tokens": ["An", "dem", "mit", "Trost", "um\u00b7sonst", "sich", "m\u00fch\u00b7te", "Freun\u00b7de\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sprach einer so zuletzt: Ein K\u00f6nig hatte, laut", "tokens": ["Sprach", "ei\u00b7ner", "so", "zu\u00b7letzt", ":", "Ein", "K\u00f6\u00b7nig", "hat\u00b7te", ",", "laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADV", "ADV", "$.", "ART", "NN", "VAFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Glaubw\u00fcrd'ger Kunde, zur Verwahrung anvertraut", "tokens": ["Glaub\u00b7w\u00fcrd'\u00b7ger", "Kun\u00b7de", ",", "zur", "Ver\u00b7wah\u00b7rung", "an\u00b7ver\u00b7traut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Kleinod einem Mann, und ihm f\u00fcr alle Stunden", "tokens": ["Ein", "Klei\u00b7nod", "ei\u00b7nem", "Mann", ",", "und", "ihm", "f\u00fcr", "al\u00b7le", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufmerksamkeit darauf die strengste eingebunden;", "tokens": ["Auf\u00b7merk\u00b7sam\u00b7keit", "da\u00b7rauf", "die", "strengs\u00b7te", "ein\u00b7ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df es verdorben ihm nicht werde noch besch\u00e4digt,", "tokens": ["Da\u00df", "es", "ver\u00b7dor\u00b7ben", "ihm", "nicht", "wer\u00b7de", "noch", "be\u00b7sch\u00e4\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "PTKNEG", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis der Verantwortung die R\u00fcckgab' ihn erledigt.", "tokens": ["Bis", "der", "Ver\u00b7ant\u00b7wor\u00b7tung", "die", "R\u00fc\u00b7ck\u00b7gab'", "ihn", "er\u00b7le\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+---+-+--+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Da hatte vor Verlust, vor Schaden und Gefahren", "tokens": ["Da", "hat\u00b7te", "vor", "Ver\u00b7lust", ",", "vor", "Scha\u00b7den", "und", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er Sorgen Tag und Nacht das Kleinod zu bewahren.", "tokens": ["Er", "Sor\u00b7gen", "Tag", "und", "Nacht", "das", "Klei\u00b7nod", "zu", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und als der Eigner kam, und fordert' es zur\u00fcck,", "tokens": ["Und", "als", "der", "Eig\u00b7ner", "kam", ",", "und", "for\u00b7dert'", "es", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gab er mit Freuden es und hielt es f\u00fcr ein Gl\u00fcck.", "tokens": ["Gab", "er", "mit", "Freu\u00b7den", "es", "und", "hielt", "es", "f\u00fcr", "ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So bist gewesen du auch eines Kindes H\u00fcter,", "tokens": ["So", "bist", "ge\u00b7we\u00b7sen", "du", "auch", "ei\u00b7nes", "Kin\u00b7des", "H\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VAPP", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des theuersten von Gott uns anvertrauter G\u00fcter.", "tokens": ["Des", "theu\u00b7ers\u00b7ten", "von", "Gott", "uns", "an\u00b7ver\u00b7trau\u00b7ter", "G\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und da\u00df du unversehrt das Gut nun gabst zur\u00fcck,", "tokens": ["Und", "da\u00df", "du", "un\u00b7ver\u00b7sehrt", "das", "Gut", "nun", "gabst", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ART", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halt es f\u00fcr Ungl\u00fcck nicht, h\u00e4ltst du's auch nicht f\u00fcr Gl\u00fcck.", "tokens": ["Halt", "es", "f\u00fcr", "Un\u00b7gl\u00fcck", "nicht", ",", "h\u00e4ltst", "du's", "auch", "nicht", "f\u00fcr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "PTKNEG", "$,", "VVFIN", "PIS", "ADV", "PTKNEG", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}