{"dta.poem.2438": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der arme S\u00fcnder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Schau allerliebster Gott wie ich nun vor dir stehe/", "tokens": ["Schau", "al\u00b7ler\u00b7liebs\u00b7ter", "Gott", "wie", "ich", "nun", "vor", "dir", "ste\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KOKOM", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit gantz zerknirschtem Geist\u2019/ ach h\u00f6r mein Fle-", "tokens": ["Mit", "gantz", "zer\u00b7knirschtem", "Geist'", "/", "ach", "h\u00f6r", "mein", "Fle"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "hen an/", "tokens": ["hen", "an", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ich wei\u00df es zwar sehr wol da\u00df ich nicht wol gethan/", "tokens": ["Ich", "wei\u00df", "es", "zwar", "sehr", "wol", "da\u00df", "ich", "nicht", "wol", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da\u00df ich im Gebet auch S\u00fcnde noch begehe/", "tokens": ["Und", "da\u00df", "ich", "im", "Ge\u00b7bet", "auch", "S\u00fcn\u00b7de", "noch", "be\u00b7ge\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "ADV", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dr\u00fcm dekk es frommer Gott mit deiner Gnade zu/", "tokens": ["Dr\u00fcm", "dekk", "es", "from\u00b7mer", "Gott", "mit", "dei\u00b7ner", "Gna\u00b7de", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist doch alles S\u00fcnd\u2019 ich thu auch was ich thu.", "tokens": ["Es", "ist", "doch", "al\u00b7les", "S\u00fcnd'", "ich", "thu", "auch", "was", "ich", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "PPER", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}