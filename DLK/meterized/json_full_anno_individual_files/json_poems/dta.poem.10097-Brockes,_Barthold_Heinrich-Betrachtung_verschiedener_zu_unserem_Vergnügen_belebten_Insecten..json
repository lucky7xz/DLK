{"dta.poem.10097": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung verschiedener zu unserem  \n Vergn\u00fcgen belebten Insecten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man siehet ietzt fast \u00fcberall mit Hauffen,", "tokens": ["Man", "sie\u00b7het", "ietzt", "fast", "\u00fc\u00b7be\u00b7rall", "mit", "Hauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel bunte K\u00e4ferchen, gef\u00e4rbte kleine Fliegen,", "tokens": ["Viel", "bun\u00b7te", "K\u00e4\u00b7fer\u00b7chen", ",", "ge\u00b7f\u00e4rb\u00b7te", "klei\u00b7ne", "Flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Zu unsrer Augen-Lust, ein Leben kriegen,", "tokens": ["Zu", "uns\u00b7rer", "Au\u00b7gen\u00b7Lust", ",", "ein", "Le\u00b7ben", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in dem Gras\u2019, auf Kraut, auf Laub und Bluhmen", "tokens": ["Und", "in", "dem", "Gras'", ",", "auf", "Kraut", ",", "auf", "Laub", "und", "Bluh\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein GOTT! wenn ich die bunte Meng\u2019 erwege,", "tokens": ["Mein", "GoTT", "!", "wenn", "ich", "die", "bun\u00b7te", "Meng'", "er\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihrer Farben und Figur", "tokens": ["Und", "ih\u00b7rer", "Far\u00b7ben", "und", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bewunderns-werthe Zierlichkeit,", "tokens": ["Be\u00b7wun\u00b7derns\u00b7wert\u00b7he", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bewunderns-werthen Unterscheid,", "tokens": ["Be\u00b7wun\u00b7derns\u00b7wert\u00b7hen", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In stiller Muss\u2019 erweg\u2019 und \u00fcberlege,", "tokens": ["In", "stil\u00b7ler", "Muss'", "er\u00b7weg'", "und", "\u00fc\u00b7berl\u00b7e\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie schnell sie h\u00fcpffen, fliegen, rennen,", "tokens": ["Wie", "schnell", "sie", "h\u00fcpf\u00b7fen", ",", "flie\u00b7gen", ",", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie fertig sie sich regen k\u00f6nnen,", "tokens": ["Wie", "fer\u00b7tig", "sie", "sich", "re\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Ergetzet mich die spielende Natur.", "tokens": ["Er\u00b7get\u00b7zet", "mich", "die", "spie\u00b7len\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich freue mich: denn ich kann deutlich sehn,", "tokens": ["Ich", "freu\u00b7e", "mich", ":", "denn", "ich", "kann", "deut\u00b7lich", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da sie so mancherley, so zierlich und so sch\u00f6n,", "tokens": ["Da", "sie", "so", "man\u00b7cher\u00b7ley", ",", "so", "zier\u00b7lich", "und", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df die Natur sie dazu bilden wollen,", "tokens": ["Da\u00df", "die", "Na\u00b7tur", "sie", "da\u00b7zu", "bil\u00b7den", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PAV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df wir des Sch\u00f6pfers Wunder-Macht,", "tokens": ["Da\u00df", "wir", "des", "Sch\u00f6p\u00b7fers", "Wun\u00b7der\u00b7Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auch in derselben Farben-Pracht,", "tokens": ["Auch", "in", "der\u00b7sel\u00b7ben", "Fa\u00b7rben\u00b7Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In unsrer Lust betrachten sollen.", "tokens": ["In", "uns\u00b7rer", "Lust", "be\u00b7trach\u00b7ten", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer wird der Farben Meng\u2019 und ihre Sch\u00f6nheit nennen,", "tokens": ["Wer", "wird", "der", "Far\u00b7ben", "Meng'", "und", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NE", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erzehlen und beschreiben k\u00f6nnen,", "tokens": ["Er\u00b7zeh\u00b7len", "und", "be\u00b7schrei\u00b7ben", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit welcher die Natur die kleinen Thierchen schm\u00fcckt?", "tokens": ["Mit", "wel\u00b7cher", "die", "Na\u00b7tur", "die", "klei\u00b7nen", "Thier\u00b7chen", "schm\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie mancherley hab ich mit innigem Vergn\u00fcgen,", "tokens": ["Wie", "man\u00b7cher\u00b7ley", "hab", "ich", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Nur blo\u00df an Fliegen einst erblickt!", "tokens": ["Nur", "blo\u00df", "an", "Flie\u00b7gen", "einst", "er\u00b7blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Woran die Farben sich recht wunderbarlich f\u00fcgen,", "tokens": ["Wo\u00b7ran", "die", "Far\u00b7ben", "sich", "recht", "wun\u00b7der\u00b7bar\u00b7lich", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Braun, gelblich, r\u00f6thlich, schwartz und grau,", "tokens": ["Braun", ",", "gelb\u00b7lich", ",", "r\u00f6th\u00b7lich", ",", "schwartz", "und", "grau", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gr\u00fcn, roth, gelb, hell-und dunckel-blau,", "tokens": ["Gr\u00fcn", ",", "roth", ",", "gelb", ",", "hell\u00b7\u00b7und", "dun\u00b7ckel\u00b7blau", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Bald Gold mit gr\u00fcn, bald Gold mit roth, gemenget;", "tokens": ["Bald", "Gold", "mit", "gr\u00fcn", ",", "bald", "Gold", "mit", "roth", ",", "ge\u00b7men\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJD", "$,", "ADV", "NN", "APPR", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Bald ist der Fl\u00fcgel k\u00fcnstlichs Paar", "tokens": ["Bald", "ist", "der", "Fl\u00fc\u00b7gel", "k\u00fcnst\u00b7lichs", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie ein Crystall so wei\u00df, so klar;", "tokens": ["Wie", "ein", "Crys\u00b7tall", "so", "wei\u00df", ",", "so", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bald sind auch die gef\u00e4rbt und bunt gesprenget.", "tokens": ["Bald", "sind", "auch", "die", "ge\u00b7f\u00e4rbt", "und", "bunt", "ge\u00b7spren\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bald scheinet sich in ihrer Fl\u00fcgel Glantz", "tokens": ["Bald", "schei\u00b7net", "sich", "in", "ih\u00b7rer", "Fl\u00fc\u00b7gel", "Glantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der bunten Jris halber Crantz", "tokens": ["Der", "bun\u00b7ten", "Jris", "hal\u00b7ber", "Crantz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPO", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "In sch\u00f6n gemischten Schmuck zu bilden.", "tokens": ["In", "sch\u00f6n", "ge\u00b7mischten", "Schmuck", "zu", "bil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Bey diesem ist der Leib, bey dem die Fl\u00fcgel, g\u00fclden.", "tokens": ["Bey", "die\u00b7sem", "ist", "der", "Leib", ",", "bey", "dem", "die", "Fl\u00fc\u00b7gel", ",", "g\u00fcl\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Durchsichtig sind sie bald, bald wiederscheinend bunt;", "tokens": ["Durch\u00b7sich\u00b7tig", "sind", "sie", "bald", ",", "bald", "wie\u00b7der\u00b7schei\u00b7nend", "bunt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bald haben rothe blau-, bald gr\u00fcne rothe K\u00f6pfe;", "tokens": ["Bald", "ha\u00b7ben", "ro\u00b7the", "blau", ",", "bald", "gr\u00fc\u00b7ne", "ro\u00b7the", "K\u00f6p\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "TRUNC", "$,", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bald sind die K\u00f6pfchen platt, bald sind sie lang, bald rund:", "tokens": ["Bald", "sind", "die", "K\u00f6pf\u00b7chen", "platt", ",", "bald", "sind", "sie", "lang", ",", "bald", "rund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es zieren selbige bald kleine schwartze Z\u00f6pfe,", "tokens": ["Es", "zie\u00b7ren", "sel\u00b7bi\u00b7ge", "bald", "klei\u00b7ne", "schwart\u00b7ze", "Z\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bald H\u00f6rnerchen, die eingekerbt und bunt.", "tokens": ["Bald", "H\u00f6r\u00b7ner\u00b7chen", ",", "die", "ein\u00b7ge\u00b7kerbt", "und", "bunt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie lieblich sieht es aus, wann schlancke Grase-Metzen,", "tokens": ["Wie", "lieb\u00b7lich", "sieht", "es", "aus", ",", "wann", "schlan\u00b7cke", "Gra\u00b7se\u00b7Met\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die blauer noch, als ein T\u00fcrckis, gemahlet,", "tokens": ["Die", "blau\u00b7er", "noch", ",", "als", "ein", "T\u00fcr\u00b7ckis", ",", "ge\u00b7mah\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "KOUS", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf Bl\u00e4ttern, die Smaragd an gr\u00fcnem Glantze gleich,", "tokens": ["Auf", "Bl\u00e4t\u00b7tern", ",", "die", "Sma\u00b7ragd", "an", "gr\u00fc\u00b7nem", "Glant\u00b7ze", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf Bl\u00e4ttern, welche hier beschattet, dort bestrahlet,", "tokens": ["Auf", "Bl\u00e4t\u00b7tern", ",", "wel\u00b7che", "hier", "be\u00b7schat\u00b7tet", ",", "dort", "be\u00b7strah\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald sanfte schweben, bald sich setzen!", "tokens": ["Bald", "sanf\u00b7te", "schwe\u00b7ben", ",", "bald", "sich", "set\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVINF", "$,", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein sch\u00f6ner Schmeltz ist in der Welt,", "tokens": ["Kein", "sch\u00f6\u00b7ner", "Sch\u00b7meltz", "ist", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als den der blaue Glantz, vom schwartzen noch erhoben,", "tokens": ["Als", "den", "der", "blau\u00b7e", "Glantz", ",", "vom", "schwart\u00b7zen", "noch", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "pun