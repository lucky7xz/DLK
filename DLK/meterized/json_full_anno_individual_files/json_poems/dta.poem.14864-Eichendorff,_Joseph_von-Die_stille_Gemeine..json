{"dta.poem.14864": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und als die Nacht schritt leis daher,", "tokens": ["Und", "als", "die", "Nacht", "schritt", "leis", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hauptmann stand am Strande,", "tokens": ["Der", "Haupt\u00b7mann", "stand", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So still im Wald, so still das Meer,", "tokens": ["So", "still", "im", "Wald", ",", "so", "still", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur die Wachen riefen im Lande.", "tokens": ["Nur", "die", "Wa\u00b7chen", "rie\u00b7fen", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}