{"dta.poem.23324": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "St\u00e4ndisches.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eein jeder bleib' in seinem Stande,", "tokens": ["\u201e", "ein", "je\u00b7der", "bleib'", "in", "sei\u00b7nem", "Stan\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jeder denke nur an sich;", "tokens": ["Ein", "je\u00b7der", "den\u00b7ke", "nur", "an", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Das ist ein Segen unserm Lande,", "tokens": ["Das", "ist", "ein", "Se\u00b7gen", "un\u00b7serm", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das pa\u00dft sich gut f\u00fcr dich und mich.\u201c", "tokens": ["Das", "pa\u00dft", "sich", "gut", "f\u00fcr", "dich", "und", "mich", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "KON", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O weh, o weh, du schn\u00f6de Schande!", "tokens": ["O", "weh", ",", "o", "weh", ",", "du", "schn\u00f6\u00b7de", "Schan\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du teuflische Simplicitas!", "tokens": ["Du", "teuf\u00b7li\u00b7sche", "Sim\u00b7pli\u00b7ci\u00b7tas", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleibt jeder nur bei seinem Stande,", "tokens": ["Bleibt", "je\u00b7der", "nur", "bei", "sei\u00b7nem", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kommt zu Stande niemals was.", "tokens": ["So", "kommt", "zu", "Stan\u00b7de", "nie\u00b7mals", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}