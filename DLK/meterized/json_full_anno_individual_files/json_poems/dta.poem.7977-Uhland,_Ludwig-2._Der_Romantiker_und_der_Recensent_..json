{"dta.poem.7977": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Der Romantiker und der Recensent .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Welche Stimme, rauh und heischer!", "tokens": ["Wel\u00b7che", "Stim\u00b7me", ",", "rauh", "und", "hei\u00b7scher", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist das wohl der Baur Hornvilla?", "tokens": ["Ist", "das", "wohl", "der", "Baur", "Horn\u00b7vil\u00b7la", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "NN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ist es Klemens wohl, der Fleischer?", "tokens": ["Ist", "es", "Kle\u00b7mens", "wohl", ",", "der", "Flei\u00b7scher", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den Fenstern der Kamilla", "tokens": ["Von", "den", "Fens\u00b7tern", "der", "Ka\u00b7mil\u00b7la"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Heb dich weg, du alter Kreischer!", "tokens": ["Heb", "dich", "weg", ",", "du", "al\u00b7ter", "Krei\u00b7scher", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die krit\u2019sche Feder h\u00e4lt", "tokens": ["Was", "die", "krit'\u00b7sche", "Fe\u00b7der", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den Alpen bis zum Belt,", "tokens": ["Von", "den", "Al\u00b7pen", "bis", "zum", "Belt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "W\u00fcth\u2019 es doch zu Haus und sch\u00e4ume,", "tokens": ["W\u00fcth'", "es", "doch", "zu", "Haus", "und", "sch\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nur verschon\u2019 es Ihrer Tr\u00e4ume", "tokens": ["Nur", "ver\u00b7schon'", "es", "Ih\u00b7rer", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wundervolle M&#228;hrchenwelt!", "tokens": ["Wun\u00b7der\u00b7vol\u00b7le", "M", "&#228;", "hr\u00b7chen\u00b7welt", "!"], "token_info": ["word", "word", "XML_entity", "word", "punct"], "pos": ["NN", "NE", "$(", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}