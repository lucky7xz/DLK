{"textgrid.poem.56150": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchmt euch, ihr Richtenden, nicht der entbehrlichen Folter", "tokens": ["R\u00fchmt", "euch", ",", "ihr", "Rich\u00b7ten\u00b7den", ",", "nicht", "der", "ent\u00b7behr\u00b7li\u00b7chen", "Fol\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "und da\u00df das Eisen nicht l\u00e4nger an H\u00e4lsen sperrt.", "tokens": ["und", "da\u00df", "das", "Ei\u00b7sen", "nicht", "l\u00e4n\u00b7ger", "an", "H\u00e4l\u00b7sen", "sperrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Keins ist gesteigert, kein Herz \u2013, weil ein gewollter", "tokens": ["Keins", "ist", "ge\u00b7stei\u00b7gert", ",", "kein", "Herz", "\u2013", ",", "weil", "ein", "ge\u00b7woll\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PIS", "VAFIN", "VVPP", "$,", "PIAT", "NN", "$(", "$,", "KOUS", "ART", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Krampf der Milde euch zarter verzerrt.", "tokens": ["Krampf", "der", "Mil\u00b7de", "euch", "zar\u00b7ter", "ver\u00b7zerrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was es durch Zeiten bekam, das schenkt das Schafott", "tokens": ["Was", "es", "durch", "Zei\u00b7ten", "be\u00b7kam", ",", "das", "schenkt", "das", "Scha\u00b7fott"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "wieder zur\u00fcck, wie Kinder ihr Spielzeug vom vorig", "tokens": ["wie\u00b7der", "zu\u00b7r\u00fcck", ",", "wie", "Kin\u00b7der", "ihr", "Spiel\u00b7zeug", "vom", "vo\u00b7rig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "PWAV", "NN", "PPOSAT", "NN", "APPRART", "ADJD"], "meter": "+--+-+--++-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "alten Geburtstag. Ins reine, ins hohe, ins thorig", "tokens": ["al\u00b7ten", "Ge\u00b7burts\u00b7tag", ".", "Ins", "rei\u00b7ne", ",", "ins", "ho\u00b7he", ",", "ins", "tho\u00b7rig"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPRART", "ADJA", "$,", "APPRART", "ADJA", "$,", "APPRART", "ADJD"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "offene Herz tr\u00e4te er anders, der Gott", "tokens": ["of\u00b7fe\u00b7ne", "Herz", "tr\u00e4\u00b7te", "er", "an\u00b7ders", ",", "der", "Gott"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "NN"], "meter": "+--++--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "wirklicher Milde. Er k\u00e4me gewaltig und griffe", "tokens": ["wirk\u00b7li\u00b7cher", "Mil\u00b7de", ".", "Er", "k\u00e4\u00b7me", "ge\u00b7wal\u00b7tig", "und", "grif\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "strahlender um sich, wie G\u00f6ttliche sind.", "tokens": ["strah\u00b7len\u00b7der", "um", "sich", ",", "wie", "G\u00f6tt\u00b7li\u00b7che", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "$,", "PWAV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Weniger nicht, als die heimliche leise Gewahrung,", "tokens": ["We\u00b7ni\u00b7ger", "nicht", ",", "als", "die", "heim\u00b7li\u00b7che", "lei\u00b7se", "Ge\u00b7wah\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "KOUS", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "die uns im Innern schweigend gewinnt", "tokens": ["die", "uns", "im", "In\u00b7nern", "schwei\u00b7gend", "ge\u00b7winnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "wie ein still spielendes Kind aus unendlicher Paarung.", "tokens": ["wie", "ein", "still", "spie\u00b7len\u00b7des", "Kind", "aus", "un\u00b7end\u00b7li\u00b7cher", "Paa\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "---+--+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "R\u00fchmt euch, ihr Richtenden, nicht der entbehrlichen Folter", "tokens": ["R\u00fchmt", "euch", ",", "ihr", "Rich\u00b7ten\u00b7den", ",", "nicht", "der", "ent\u00b7behr\u00b7li\u00b7chen", "Fol\u00b7ter"], "token_info": ["word", "wor