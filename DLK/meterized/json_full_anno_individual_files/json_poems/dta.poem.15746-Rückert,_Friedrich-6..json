{"dta.poem.15746": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo der Gedanke fehlt, die unverwandte Richtung", "tokens": ["Wo", "der", "Ge\u00b7dan\u00b7ke", "fehlt", ",", "die", "un\u00b7ver\u00b7wand\u00b7te", "Rich\u00b7tung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf hochgestecktes Ziel, da ist ein Tand die Dichtung.", "tokens": ["Auf", "hoch\u00b7ge\u00b7steck\u00b7tes", "Ziel", ",", "da", "ist", "ein", "Tand", "die", "Dich\u00b7tung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Fantasienspiel der Kinderm\u00e4hrchenlieder", "tokens": ["Das", "Fan\u00b7ta\u00b7sien\u00b7spiel", "der", "Kin\u00b7der\u00b7m\u00e4hr\u00b7chen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist mit der Kindheit hin, und Niemand bringt sie wieder.", "tokens": ["Ist", "mit", "der", "Kind\u00b7heit", "hin", ",", "und", "Nie\u00b7mand", "bringt", "sie", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Statt Ammenkinderfrau sei nun Erzieherinn", "tokens": ["Statt", "Am\u00b7men\u00b7kin\u00b7der\u00b7frau", "sei", "nun", "Er\u00b7zie\u00b7he\u00b7rinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Muse dem Geschlecht zu h\u00f6herm Lebenssinn.", "tokens": ["Die", "Mu\u00b7se", "dem", "Ge\u00b7schlecht", "zu", "h\u00f6\u00b7herm", "Le\u00b7bens\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hinfort gen\u00fcgt nicht mehr anmuthig Klingendes,", "tokens": ["Hin\u00b7fort", "ge\u00b7n\u00fcgt", "nicht", "mehr", "an\u00b7mut\u00b7hig", "Klin\u00b7gen\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur Himmelringendes, Geschickbezwingendes.", "tokens": ["Nur", "Him\u00b7mel\u00b7rin\u00b7gen\u00b7des", ",", "Ge\u00b7schick\u00b7be\u00b7zwin\u00b7gen\u00b7des", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}