{"textgrid.poem.66934": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir sind erwacht, wir sind befreit", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind erwacht, wir sind befreit", "tokens": ["Wir", "sind", "er\u00b7wacht", ",", "wir", "sind", "be\u00b7freit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Todesnacht und -Qualen.", "tokens": ["Von", "To\u00b7des\u00b7nacht", "und", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vorbei, vorbei die grause Zeit!", "tokens": ["Vor\u00b7bei", ",", "vor\u00b7bei", "die", "grau\u00b7se", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Morgengipfel strahlen.", "tokens": ["Die", "Mor\u00b7gen\u00b7gip\u00b7fel", "strah\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Freiheit Sonne steigt empor,", "tokens": ["Der", "Frei\u00b7heit", "Son\u00b7ne", "steigt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir ziehn ihr k\u00fchn entgegen,", "tokens": ["Wir", "ziehn", "ihr", "k\u00fchn", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "A