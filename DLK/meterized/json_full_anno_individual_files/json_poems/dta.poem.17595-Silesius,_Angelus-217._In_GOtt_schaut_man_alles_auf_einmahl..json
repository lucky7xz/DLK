{"dta.poem.17595": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "217.  In GOtt schaut man alles auf  \n einmahl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Freund wann man Gott beschaut/ schaut man auf", "tokens": ["Freund", "wann", "man", "Gott", "be\u00b7schaut", "/", "schaut", "man", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PWAV", "PIS", "NN", "VVPP", "$(", "VVFIN", "PIS", "APPR"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "einmahl an/", "tokens": ["ein\u00b7mahl", "an", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Was man sonst ewig nicht ohn jhn durchschauen kan.", "tokens": ["Was", "man", "sonst", "e\u00b7wig", "nicht", "ohn", "jhn", "durch\u00b7schau\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADJD", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}