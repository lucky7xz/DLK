{"dta.poem.21880": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Wie w\u00fcrdestu dich stellen/", "tokens": ["Wie", "w\u00fcr\u00b7des\u00b7tu", "dich", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wenu/ Rose/ den Gesellen", "tokens": ["we\u00b7nu", "/", "Ro\u00b7se", "/", "den", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$(", "NE", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "du bey mir solltest treffen an?", "tokens": ["du", "bey", "mir", "soll\u00b7test", "tref\u00b7fen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du bist zu langsam kommen/", "tokens": ["du", "bist", "zu", "lang\u00b7sam", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du weist/ da\u00df Brunst nicht harren kan.", "tokens": ["Du", "weist", "/", "da\u00df", "Brunst", "nicht", "har\u00b7ren", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}