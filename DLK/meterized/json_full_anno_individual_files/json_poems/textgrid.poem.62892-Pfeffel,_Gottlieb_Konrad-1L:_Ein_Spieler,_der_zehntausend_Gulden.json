{"textgrid.poem.62892": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Spieler, der zehntausend Gulden", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Spieler, der zehntausend Gulden", "tokens": ["Ein", "Spie\u00b7ler", ",", "der", "zehn\u00b7tau\u00b7send", "Gul\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Parolieren durchgebracht,", "tokens": ["Mit", "Pa\u00b7ro\u00b7lie\u00b7ren", "durch\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf sein Ehrenwort noch Schulden", "tokens": ["Und", "auf", "sein", "Eh\u00b7ren\u00b7wort", "noch", "Schul\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr mehr als er besa\u00df gemacht,", "tokens": ["F\u00fcr", "mehr", "als", "er", "be\u00b7sa\u00df", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlich mit Gesang, wie ein Franzose,", "tokens": ["Schlich", "mit", "Ge\u00b7sang", ",", "wie", "ein", "Fran\u00b7zo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Der heimlich flucht, vom Caffehaus,", "tokens": ["Der", "heim\u00b7lich", "flucht", ",", "vom", "Caf\u00b7fe\u00b7haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und zog sein letztes Gut, die Dose,", "tokens": ["Und", "zog", "sein", "letz\u00b7tes", "Gut", ",", "die", "Do\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Grillen zu zerstreun, heraus.", "tokens": ["Die", "Gril\u00b7len", "zu", "zer\u00b7streun", ",", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da fleht um eine milde Steuer", "tokens": ["Da", "fleht", "um", "ei\u00b7ne", "mil\u00b7de", "Steu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Greis ihn an. La\u00df mich in Ruh,", "tokens": ["Ein", "Greis", "ihn", "an", ".", "La\u00df", "mich", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$.", "VVIMP", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach er, ich habe keinen Dreyer,", "tokens": ["Sprach", "er", ",", "ich", "ha\u00b7be", "kei\u00b7nen", "Drey\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Willst du Toback? so greife zu.", "tokens": ["Willst", "du", "To\u00b7back", "?", "so", "grei\u00b7fe", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "$.", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist nicht n\u00f6thig, da\u00df ich nie\u00dfe,", "tokens": ["Es", "ist", "nicht", "n\u00f6\u00b7thig", ",", "da\u00df", "ich", "nie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Versetzt der Greis mit schlauem Spott,", "tokens": ["Ver\u00b7setzt", "der", "Greis", "mit", "schlau\u00b7em", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man sagt mir t\u00e4glich ohne Prise,", "tokens": ["Man", "sagt", "mir", "t\u00e4g\u00b7lich", "oh\u00b7ne", "Pri\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur allzuh\u00e4ufig: Helf dir Gott!", "tokens": ["Nur", "all\u00b7zu\u00b7h\u00e4u\u00b7fig", ":", "Helf", "dir", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Spieler, der zehntausend Gulden", "tokens": ["Ein", "Spie\u00b7ler", ",", "der", "zehn\u00b7tau\u00b7send", "Gul\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "CARD", "NN"], "meter": "-+-+-+-+-", "measure