{"dta.poem.18452": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Letzte bitte eines diebes", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dort ward zum strick verdamt Marinckus ein Polack", "tokens": ["Dort", "ward", "zum", "strick", "ver\u00b7damt", "Ma\u00b7rin\u00b7ckus", "ein", "Po\u00b7lack"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "VVFIN", "NE", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der bat vor seinem end umb eine pfeif toback/", "tokens": ["Der", "bat", "vor", "sei\u00b7nem", "end", "umb", "ei\u00b7ne", "pfeif", "to\u00b7back", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ist/ sprach er gesund/ und kan die fl\u00fc\u00df vertreiben/", "tokens": ["Das", "ist", "/", "sprach", "er", "ge\u00b7sund", "/", "und", "kan", "die", "fl\u00fc\u00df", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "VVFIN", "PPER", "ADJD", "$(", "KON", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich werde doch die nacht im k\u00fchlen m\u00fcssen bleiben/", "tokens": ["Ich", "wer\u00b7de", "doch", "die", "nacht", "im", "k\u00fch\u00b7len", "m\u00fcs\u00b7sen", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN