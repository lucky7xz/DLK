{"dta.poem.18783": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt\u2019 ich irgend wohl Bedenken", "tokens": ["H\u00e4tt'", "ich", "ir\u00b7gend", "wohl", "Be\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bochara und Samarcand,", "tokens": ["Bo\u00b7cha\u00b7ra", "und", "Sa\u00b7mar\u00b7cand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "S\u00fcsses Liebchen, dir zu schenken?", "tokens": ["S\u00fcs\u00b7ses", "Lieb\u00b7chen", ",", "dir", "zu", "schen\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser St\u00e4dte Rausch und Tand.", "tokens": ["Die\u00b7ser", "St\u00e4d\u00b7te", "Rausch", "und", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber frag einmal den Kaiser,", "tokens": ["A\u00b7ber", "frag", "ein\u00b7mal", "den", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob er dir die St\u00e4dte giebt?", "tokens": ["Ob", "er", "dir", "die", "St\u00e4d\u00b7te", "giebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er ist herrlicher und weiser;", "tokens": ["Er", "ist", "herr\u00b7li\u00b7cher", "und", "wei\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch er weiss nicht wie man liebt.", "tokens": ["Doch", "er", "weiss", "nicht", "wie", "man", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PWAV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herrscher! zu dergleichen Gaben", "tokens": ["Herr\u00b7scher", "!", "zu", "derg\u00b7lei\u00b7chen", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmermehr bestimmst du dich!", "tokens": ["Nim\u00b7mer\u00b7mehr", "be\u00b7stimmst", "du", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solch ein M\u00e4dchen muss man haben", "tokens": ["Solch", "ein", "M\u00e4d\u00b7chen", "muss", "man", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "VMFIN", "PIS", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Bettler seyn wie ich.", "tokens": ["Und", "ein", "Bett\u00b7ler", "seyn", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAINF", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}