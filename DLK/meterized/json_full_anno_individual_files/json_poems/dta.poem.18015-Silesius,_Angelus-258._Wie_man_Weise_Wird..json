{"dta.poem.18015": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "258.  Wie man Weise Wird.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch wiltu Weise seyn/ wilt Gott und dich erkennen/", "tokens": ["Mensch", "wil\u00b7tu", "Wei\u00b7se", "seyn", "/", "wilt", "Gott", "und", "dich", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "VAINF", "$(", "VMFIN", "NN", "KON", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mustu vor in dir die Welt begihr verbrennen.", "tokens": ["So", "mus\u00b7tu", "vor", "in", "dir", "die", "Welt", "be\u00b7gihr", "ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "APPR", "PPER", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}