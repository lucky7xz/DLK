{"dta.poem.5367": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat auch heute meine Seele, durchs ", "tokens": ["Hat", "auch", "heu\u00b7te", "mei\u00b7ne", "See\u00b7le", ",", "durchs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,", "APPRART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und Loben", "tokens": ["und", "Lo\u00b7ben"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "F\u00fcr so Lehr\u2019, als Harmonie, zu dem Sch\u00f6pfer sich erhoben?", "tokens": ["F\u00fcr", "so", "Lehr'", ",", "als", "Har\u00b7mo\u00b7nie", ",", "zu", "dem", "Sch\u00f6p\u00b7fer", "sich", "er\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$,", "KOUS", "NN", "$,", "APPR", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Hat sie, wenn sie etwas lieblichs, wie vermuthlich ist, ", "tokens": ["Hat", "sie", ",", "wenn", "sie", "et\u00b7was", "lieb\u00b7lichs", ",", "wie", "ver\u00b7muth\u00b7lich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJA", "$,", "PWAV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "F\u00fcr des ", "tokens": ["F\u00fcr", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Danck! gesprochen?", "tokens": ["Danck", "!", "ge\u00b7spro\u00b7chen", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hat sie, wenn sich, sonder Pein, K\u00e4lt und Hitze wol gemischt,", "tokens": ["Hat", "sie", ",", "wenn", "sich", ",", "son\u00b7der", "Pein", ",", "K\u00e4lt", "und", "Hit\u00b7ze", "wol", "ge\u00b7mischt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PRF", "$,", "KON", "NN", "$,", "VVFIN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Ja noch andre sanfte Vorw\u00fcff\u2019 ihres C\u00f6rpers Haut erfrischt,", "tokens": ["Ja", "noch", "and\u00b7re", "sanf\u00b7te", "Vor\u00b7w\u00fcff'", "ih\u00b7res", "C\u00f6r\u00b7pers", "Haut", "er\u00b7frischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KON", "PIS", "VVFIN", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wol des Sch\u00f6pfers Huld ", "tokens": ["Wol", "des", "Sch\u00f6p\u00b7fers", "Huld"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "empfunden,", "tokens": ["emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Auch empfunden, da\u00df dem Geber sie davor zum Danck", "tokens": ["Auch", "emp\u00b7fun\u00b7den", ",", "da\u00df", "dem", "Ge\u00b7ber", "sie", "da\u00b7vor", "zum", "Danck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "KOUS", "ART", "NN", "PPER", "PAV", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "verbunden?", "tokens": ["ver\u00b7bun\u00b7den", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Hat sie GOTTES G\u00fct und Liebe, in der s\u00fcssen Kraft zu", "tokens": ["Hat", "sie", "GoT\u00b7TES", "G\u00fct", "und", "Lie\u00b7be", ",", "in", "der", "s\u00fcs\u00b7sen", "Kraft", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NE", "NN", "KON", "NN", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "schmecken?", "tokens": ["schme\u00b7cken", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Wenn sie Spreis und Tranck genossen, auch zu schmecken,", "tokens": ["Wenn", "sie", "Spreis", "und", "Tranck", "ge\u00b7nos\u00b7sen", ",", "auch", "zu", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "zu entdecken", "tokens": ["zu", "ent\u00b7de\u00b7cken"], "token_info": ["word", "word"], "pos": ["PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Und, mit Lust daf\u00fcr zu dancken, wie sie schuldig, sich bestrebt?", "tokens": ["Und", ",", "mit", "Lust", "da\u00b7f\u00fcr", "zu", "dan\u00b7cken", ",", "wie", "sie", "schul\u00b7dig", ",", "sich", "be\u00b7strebt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "PAV", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADJD", "$,", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Kurtz: Hast du zu GOTTES Ehren heut, und als ein", "tokens": ["Kurtz", ":", "Hast", "du", "zu", "GoT\u00b7TES", "Eh\u00b7ren", "heut", ",", "und", "als", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "VAFIN", "PPER", "APPR", "NE", "NN", "ADV", "$,", "KON", "KOUS", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Mensch, gelebt?", "tokens": ["Mensch", ",", "ge\u00b7lebt", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Hast du, mit vergn\u00fcgter Seele, wahrer Andacht heisse", "tokens": ["Hast", "du", ",", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "See\u00b7le", ",", "wah\u00b7rer", "An\u00b7dacht", "heis\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "Triebe,", "tokens": ["Trie\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Eine Demuths-volle Ehrfurcht, kindlich-br\u00fcnst\u2019ge Gegen-", "tokens": ["Ei\u00b7ne", "De\u00b7muths\u00b7vol\u00b7le", "Ehr\u00b7furcht", ",", "kind\u00b7lich\u00b7br\u00fcnst'\u00b7ge", "Ge\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.23": {"text": "Liebe", "tokens": ["Lie\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Gegen den allm\u00e4chtgen Vater, der dir so viel Guts ge-", "tokens": ["Ge\u00b7gen", "den", "all\u00b7m\u00e4cht\u00b7gen", "Va\u00b7ter", ",", "der", "dir", "so", "viel", "Guts", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "PIAT", "NN", "TRUNC"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.25": {"text": "schenckt,", "tokens": ["schenckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Der in deinen Leib und Geist solche F\u00e4higkeit gesenckt,", "tokens": ["Der", "in", "dei\u00b7nen", "Leib", "und", "Geist", "sol\u00b7che", "F\u00e4\u00b7hig\u00b7keit", "ge\u00b7senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "KON", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Heut\u2019 in stiller Lust gesp\u00fchrt? hast du einen regen Willen,", "tokens": ["Heut'", "in", "stil\u00b7ler", "Lust", "ge\u00b7sp\u00fchrt", "?", "hast", "du", "ei\u00b7nen", "re\u00b7gen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$.", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}, "line.28": {"text": "Eines solchen holden Gebers Wort und Willen zu erf\u00fcllen,", "tokens": ["Ei\u00b7nes", "sol\u00b7chen", "hol\u00b7den", "Ge\u00b7bers", "Wort", "und", "Wil\u00b7len", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIAT", "ADJA", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Deinen N\u00e4chsten recht zu lieben, heut in deiner Seel", "tokens": ["Dei\u00b7nen", "N\u00e4chs\u00b7ten", "recht", "zu", "lie\u00b7ben", ",", "heut", "in", "dei\u00b7ner", "Seel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.30": {"text": "empfunden?", "tokens": ["emp\u00b7fun\u00b7den", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Hast du inniglich gew\u00fcnscht da\u00df, von grobe Lastern rein,", "tokens": ["Hast", "du", "in\u00b7nig\u00b7lich", "ge\u00b7w\u00fcnscht", "da\u00df", ",", "von", "gro\u00b7be", "Las\u00b7tern", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "KOUS", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Du, f\u00fcr so viel Huld und Wolthat, deinem GOtt gef\u00e4llig seyn,", "tokens": ["Du", ",", "f\u00fcr", "so", "viel", "Huld", "und", "Wolt\u00b7hat", ",", "dei\u00b7nem", "Gott", "ge\u00b7f\u00e4l\u00b7lig", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}