{"textgrid.poem.62968": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von seinem milden Landesvater", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von seinem milden Landesvater", "tokens": ["Von", "sei\u00b7nem", "mil\u00b7den", "Lan\u00b7des\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Frohnen abgezehrt, lag Jost", "tokens": ["Durch", "Froh\u00b7nen", "ab\u00b7ge\u00b7zehrt", ",", "lag", "Jost"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf faulem Moos. Ein frommer Pater", "tokens": ["Auf", "fau\u00b7lem", "Moos", ".", "Ein", "from\u00b7mer", "Pa\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gab in dem letzten Kampf ihm Trost:", "tokens": ["Gab", "in", "dem", "letz\u00b7ten", "Kampf", "ihm", "Trost", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald, sprach er, wird euch Gott entbinden", "tokens": ["Bald", ",", "sprach", "er", ",", "wird", "euch", "Gott", "ent\u00b7bin\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Joch, das euch so hart gedr\u00fcckt:", "tokens": ["Vom", "Joch", ",", "das", "euch", "so", "hart", "ge\u00b7dr\u00fcckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Ruhe, die euch nie begl\u00fcckt,", "tokens": ["Die", "Ru\u00b7he", ",", "die", "euch", "nie", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Freund, werdet ihr im Himmel finden.", "tokens": ["Freund", ",", "wer\u00b7det", "ihr", "im", "Him\u00b7mel", "fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ach, Herr! rief Jost so dumpf und hohl", "tokens": ["Ach", ",", "Herr", "!", "rief", "Jost", "so", "dumpf", "und", "hohl"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$.", "VVFIN", "NE", "ADV", "ADJD", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie aus dem Grab, wer kann das wissen?", "tokens": ["Wie", "aus", "dem", "Grab", ",", "wer", "kann", "das", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PWS", "VMFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wir armen Bauren werden wohl", "tokens": ["Wir", "ar\u00b7men", "Bau\u00b7ren", "wer\u00b7den", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+",