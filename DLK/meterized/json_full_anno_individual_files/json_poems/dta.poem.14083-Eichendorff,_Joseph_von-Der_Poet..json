{"dta.poem.14083": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Poet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, darf ich bei Liebchen weilen:", "tokens": ["Ja", ",", "darf", "ich", "bei", "Lieb\u00b7chen", "wei\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchl' ich froh der Stunden Schwall", "tokens": ["F\u00fchl'", "ich", "froh", "der", "Stun\u00b7den", "Schwall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl melodischer enteilen", "tokens": ["Wohl", "me\u00b7lo\u00b7di\u00b7scher", "ent\u00b7ei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der sch\u00f6nste Sylbenfall,", "tokens": ["Als", "der", "sch\u00f6ns\u00b7te", "Syl\u00b7ben\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will ich singen, Lippen neigen", "tokens": ["Will", "ich", "sin\u00b7gen", ",", "Lip\u00b7pen", "nei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich auf mich, und leiden's nicht,", "tokens": ["Sich", "auf", "mich", ",", "und", "lei\u00b7den's", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "$,", "KON", "ADJA", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wie gerne mag ich schweigen,", "tokens": ["Und", "wie", "ger\u00b7ne", "mag", "ich", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Wird mein Leben zum Gedicht!", "tokens": ["Wird", "mein", "Le\u00b7ben", "zum", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}