{"textgrid.poem.34040": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Kuppelbeleuchtung zu Rom", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alter Sanct Peter, was seh' ich? Es ringelt die Schlange des Feuers", "tokens": ["Al\u00b7ter", "Sanct", "Pe\u00b7ter", ",", "was", "seh'", "ich", "?", "Es", "rin\u00b7gelt", "die", "Schlan\u00b7ge", "des", "Feu\u00b7ers"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NE", "$,", "PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Gl\u00fchend sich um dich herum, z\u00fcngelt noch \u00fcber das Haupt", "tokens": ["Gl\u00fc\u00b7hend", "sich", "um", "dich", "he\u00b7rum", ",", "z\u00fcn\u00b7gelt", "noch", "\u00fc\u00b7ber", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PRF", "APPR", "PPER", "PTKVZ", "$,", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+---+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dir hinaus und verscheucht den Mond, den frommen Versilb'rer", "tokens": ["Dir", "hin\u00b7aus", "und", "ver\u00b7scheucht", "den", "Mond", ",", "den", "from\u00b7men", "Ver\u00b7silb'\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deiner Kuppel, der w\u00e4hnt, da\u00df ihn die H\u00f6lle vertreibt.", "tokens": ["Dei\u00b7ner", "Kup\u00b7pel", ",", "der", "w\u00e4hnt", ",", "da\u00df", "ihn", "die", "H\u00f6l\u00b7le", "ver\u00b7treibt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Doch, ich irrte mich wohl! Du stehst nicht erschrocken, die Flamme", "tokens": ["Doch", ",", "ich", "irr\u00b7te", "mich", "wohl", "!", "Du", "stehst", "nicht", "er\u00b7schro\u00b7cken", ",", "die", "Flam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PTKNEG", "VVINF", "$,", "ART", "NN"], "meter": "--+-+-++--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Zittert, statt deiner, sie friert, gern auch entschl\u00fcpfte sie dir,", "tokens": ["Zit\u00b7tert", ",", "statt", "dei\u00b7ner", ",", "sie", "friert", ",", "gern", "auch", "ent\u00b7schl\u00fcpf\u00b7te", "sie", "dir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PPOSAT", "$,", "PPER", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Aber, du h\u00e4ltst sie, sie soll den Vorwitz b\u00fc\u00dfen, verwegen", "tokens": ["A\u00b7ber", ",", "du", "h\u00e4ltst", "sie", ",", "sie", "soll", "den", "Vor\u00b7witz", "b\u00fc\u00b7\u00dfen", ",", "ver\u00b7we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,", "VVFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Aufgekrochen zu sein an dem Giganten von Stein.", "tokens": ["Auf\u00b7ge\u00b7kro\u00b7chen", "zu", "sein", "an", "dem", "Gi\u00b7gan\u00b7ten", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Oder hat sich der j\u00fcngste der Blitze, der Wolke entwischend,", "tokens": ["O\u00b7der", "hat", "sich", "der", "j\u00fcngs\u00b7te", "der", "Blit\u00b7ze", ",", "der", "Wol\u00b7ke", "ent\u00b7wi\u00b7schend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ART", "ADJA", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Gar des Angriffs erk\u00fchnt, hast du den Nestling gepackt", "tokens": ["Gar", "des", "An\u00b7griffs", "er\u00b7k\u00fchnt", ",", "hast", "du", "den", "Nest\u00b7ling", "ge\u00b7packt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Und dich in ihn gewickelt, damit er verk\u00fcnde: Sanct Peter", "tokens": ["Und", "dich", "in", "ihn", "ge\u00b7wi\u00b7ckelt", ",", "da\u00b7mit", "er", "ver\u00b7k\u00fcn\u00b7de", ":", "Sanct", "Pe\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$.", "VVFIN", "NE"], "meter": "-+-+-+--+--+----", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Hat zwar das Ende der Welt, aber Nichts weiter zu scheu'n?", "tokens": ["Hat", "zwar", "das", "En\u00b7de", "der", "Welt", ",", "a\u00b7ber", "N