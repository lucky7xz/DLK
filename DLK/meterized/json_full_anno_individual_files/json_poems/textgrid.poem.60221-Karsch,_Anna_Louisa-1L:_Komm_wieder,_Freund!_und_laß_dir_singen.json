{"textgrid.poem.60221": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm wieder, Freund! und la\u00df dir singen", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm wieder, Freund! und la\u00df dir singen", "tokens": ["Komm", "wie\u00b7der", ",", "Freund", "!", "und", "la\u00df", "dir", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "NN", "$.", "KON", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald ernsthaft, bald im Scherz.", "tokens": ["Bald", "ernst\u00b7haft", ",", "bald", "im", "Scherz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Freundschaft gegen mich wirst du zur\u00fccke bringen;", "tokens": ["Die", "Freund\u00b7schaft", "ge\u00b7gen", "mich", "wirst", "du", "zu\u00b7r\u00fc\u00b7cke", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch nicht dein mitgenomnes Herz!", "tokens": ["Doch", "nicht", "dein", "mit\u00b7ge\u00b7nom\u00b7nes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir hat ein Gott im Traum erz\u00e4hlet:", "tokens": ["Mir", "hat", "ein", "Gott", "im", "Traum", "er\u00b7z\u00e4h\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In zwanzig M\u00e4dchens Augen sa\u00df", "tokens": ["In", "zwan\u00b7zig", "M\u00e4d\u00b7chens", "Au\u00b7gen", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Cytherens kleiner Sohn, und hat zwar oft gefehlet;", "tokens": ["Cy\u00b7the\u00b7rens", "klei\u00b7ner", "Sohn", ",", "und", "hat", "zwar", "oft", "ge\u00b7feh\u00b7let", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch als Palemon ganz verga\u00df,", "tokens": ["Doch", "als", "Pa\u00b7le\u00b7mon", "ganz", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df in der Welt ein Amor w\u00e4re,", "tokens": ["Da\u00df", "in", "der", "Welt", "ein", "A\u00b7mor", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Scho\u00df ihm, aus sch\u00f6ner Sternen Sph\u00e4re,", "tokens": ["Scho\u00df", "ihm", ",", "aus", "sch\u00f6\u00b7ner", "Ster\u00b7nen", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Ein Strahl in seine Brust. Nun f\u00fchlt er s\u00fcsse Pein,", "token