{"textgrid.poem.55679": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Ofen knistert lustig laut das Feuer,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Ofen knistert lustig laut das Feuer,", "tokens": ["Im", "O\u00b7fen", "knis\u00b7tert", "lus\u00b7tig", "laut", "das", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Phantastisch zucken Lichter hin und her,", "tokens": ["Phan\u00b7tas\u00b7tisch", "zu\u00b7cken", "Lich\u00b7ter", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ins Spiel der Flammen starrt' ich, weltvergessen,", "tokens": ["Ins", "Spiel", "der", "Flam\u00b7men", "starrt'", "ich", ",", "welt\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "PWAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich \u00fcberflutet der Gedanken Meer.", "tokens": ["Mich", "\u00fc\u00b7berf\u00b7lu\u00b7tet", "der", "Ge\u00b7dan\u00b7ken", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor\u00fcber zogen meiner Kindheit Tage,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zo\u00b7gen", "mei\u00b7ner", "Kind\u00b7heit", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So freud- und freundlos, wie bei Andern kaum,", "tokens": ["So", "freu\u00b7d", "und", "freund\u00b7los", ",", "wie", "bei", "An\u00b7dern", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "ADJD", "$,", "PWAV", "APPR", "ADJA", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein stumpfergebnes Tragen und Entsagen,", "tokens": ["Ein", "stump\u00b7fer\u00b7geb\u00b7nes", "Tra\u00b7gen", "und", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein sorgenloser, sonnenheller Traum \u2013 \u2013 \u2013", "tokens": ["Kein", "sor\u00b7gen\u00b7lo\u00b7ser", ",", "son\u00b7nen\u00b7hel\u00b7ler", "Traum", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "ADJA", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und halbzerdr\u00fcckt sich von den Wimpern l\u00f6ste", "tokens": ["Und", "halb\u00b7zer\u00b7dr\u00fcckt", "sich", "von", "den", "Wim\u00b7pern", "l\u00f6s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl eine Tr\u00e4ne nach der andern leis',", "tokens": ["Wohl", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "nach", "der", "an\u00b7dern", "leis'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00df nicht, ob Zornes- oder Sehnsuchtstr\u00e4nen \u2013", "tokens": ["Wei\u00df", "nicht", ",", "ob", "Zor\u00b7nes", "o\u00b7der", "Sehn\u00b7sucht\u00b7str\u00e4\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "TRUNC", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch bitter waren sie und brennend hei\u00df.", "tokens": ["Doch", "bit\u00b7ter", "wa\u00b7ren", "sie", "und", "bren\u00b7nend", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Ofen knistert lustig laut das Feuer,", "tokens": ["Im", "O\u00b7fen", "knis\u00b7tert", "lus\u00b7tig", "laut", "das", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Phantastisch zucken Lichter hin und her,", "tokens": ["Phan\u00b7tas\u00b7tisch", "zu\u00b7cken", "Lich\u00b7ter", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ins Spiel der Flammen starrt' ich, weltvergessen,", "tokens": ["Ins", "Spiel", "der", "Flam\u00b7men", "starrt'", "ich", ",", "welt\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "PWAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich \u00fcberflutet 