{"textgrid.poem.64339": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00e4ufle mir, s\u00fc\u00dfer Schlummer, in des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4ufle mir, s\u00fc\u00dfer Schlummer, in des Lebens", "tokens": ["Tr\u00e4uf\u00b7le", "mir", ",", "s\u00fc\u00b7\u00dfer", "Schlum\u00b7mer", ",", "in", "des", "Le\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Bl\u00fcte himmlisches Taues helle Tropfen!", "tokens": ["Bl\u00fc\u00b7te", "himm\u00b7li\u00b7sches", "Tau\u00b7es", "hel\u00b7le", "Trop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wehet, L\u00fcfte tagender Ahndung, wehet", "tokens": ["We\u00b7het", ",", "L\u00fcf\u00b7te", "ta\u00b7gen\u00b7der", "Ahn\u00b7dung", ",", "we\u00b7het"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "NN", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Freundlich und leise,", "tokens": ["Freund\u00b7lich", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Bis mir, im Strahlenglanz, der Zukunft Sonne", "tokens": ["Bis", "mir", ",", "im", "Strah\u00b7len\u00b7glanz", ",", "der", "Zu\u00b7kunft", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct