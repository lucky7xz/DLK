{"dta.poem.18196": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und singen dich noch immer an, o Lenz,", "tokens": ["Und", "sin\u00b7gen", "dich", "noch", "im\u00b7mer", "an", ",", "o", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch da dein Zauber nun auch mich bezwungen,", "tokens": ["Doch", "da", "dein", "Zau\u00b7ber", "nun", "auch", "mich", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Meld ich mich auch zur gro\u00dfen Concurrenz.", "tokens": ["Meld", "ich", "mich", "auch", "zur", "gro\u00b7\u00dfen", "Con\u00b7cur\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch f\u00fcrcht ich fast, ich bin dir zu prosaisch,", "tokens": ["Doch", "f\u00fcrcht", "ich", "fast", ",", "ich", "bin", "dir", "zu", "pro\u00b7sa\u00b7isch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Aus meinen Versen spr\u00fcht kein F\u00fcnkchen Geist;", "tokens": ["Aus", "mei\u00b7nen", "Ver\u00b7sen", "spr\u00fcht", "kein", "F\u00fcnk\u00b7chen", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und denk ich gar an deinen Dichter Kleist,", "tokens": ["Und", "denk", "ich", "gar", "an", "dei\u00b7nen", "Dich\u00b7ter", "Kleist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Klingt meine Sprache mir fast wie Hava\u00efsch.", "tokens": ["Klingt", "mei\u00b7ne", "Spra\u00b7che", "mir", "fast", "wie", "Ha\u00b7va\u00efsch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "KOKOM", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Kein Veilchenduft versetzt mich in Extase", "tokens": ["Kein", "Veil\u00b7chen\u00b7duft", "ver\u00b7setzt", "mich", "in", "Ex\u00b7ta\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn ach, ich bin ein Epigone nur;", "tokens": ["Denn", "ach", ",", "ich", "bin", "ein", "E\u00b7pi\u00b7go\u00b7ne", "nur", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nie trank ich Wein aus einem Wasserglase", "tokens": ["Nie", "trank", "ich", "Wein", "aus", "ei\u00b7nem", "Was\u00b7ser\u00b7gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und n\u00fcchtern bin ich bis zur Unnatur.", "tokens": ["Und", "n\u00fcch\u00b7tern", "bin", "ich", "bis", "zur", "Un\u00b7na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Tonfall meiner lyrischen Collegen", "tokens": ["Der", "Ton\u00b7fall", "mei\u00b7ner", "ly\u00b7ri\u00b7schen", "Col\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ist mir ein unverstandner Dialect,", "tokens": ["Ist", "mir", "ein", "un\u00b7ver\u00b7stand\u00b7ner", "Di\u00b7a\u00b7lect", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn meinen Reim hat die Kultur beleckt", "tokens": ["Denn", "mei\u00b7nen", "Reim", "hat", "die", "Kul\u00b7tur", "be\u00b7leckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und meine Muse wallt auf andern Wegen.", "tokens": ["Und", "mei\u00b7ne", "Mu\u00b7se", "wallt", "auf", "an\u00b7dern", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ins Waldversteck verirrt sie sich nur selten,", "tokens": ["Ins", "Wald\u00b7ver\u00b7steck", "ver\u00b7irrt", "sie", "sich", "nur", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die blaue Blume ist ihr l\u00e4ngst verbl\u00fcht;", "tokens": ["Die", "blau\u00b7e", "Blu\u00b7me", "ist", "ihr", "l\u00e4ngst", "ver\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch zieht die Ahnung neugeborner Welten", "tokens": ["Doch", "zieht", "die", "Ah\u00b7nung", "neu\u00b7ge\u00b7bor\u00b7ner", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr s\u00fc\u00dfer als ein M\u00e4rchen durchs Gem\u00fcth.", "tokens": ["Ihr", "s\u00fc\u00b7\u00dfer", "als", "ein", "M\u00e4r\u00b7chen", "durchs", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zur Armuth tritt sie hin und z\u00e4hlt die Groschen,", "tokens": ["Zur", "Ar\u00b7muth", "tritt", "sie", "hin", "und", "z\u00e4hlt", "die", "Gro\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr rothes Banner pflanzt sie in den Streit,", "tokens": ["Ihr", "ro\u00b7thes", "Ban\u00b7ner", "pflanzt", "sie", "in", "den", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An ihr Herz schl\u00e4gt das gro\u00dfe Herz der Zeit", "tokens": ["An", "ihr", "Herz", "schl\u00e4gt", "das", "gro\u00b7\u00dfe", "Herz", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.8": {"text": "Und aller Weltschmerz scheint ihr abgedroschen.", "tokens": ["Und", "al\u00b7ler", "Welt\u00b7schmerz", "scheint", "ihr", "ab\u00b7ge\u00b7dro\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch heute singt sie, was ihr l\u00e4ngst verboten,", "tokens": ["Doch", "heu\u00b7te", "singt", "sie", ",", "was", "ihr", "l\u00e4ngst", "ver\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir scheint, dein L\u00e4cheln hat sie mir behext,", "tokens": ["Mir", "scheint", ",", "dein", "L\u00e4\u00b7cheln", "hat", "sie", "mir", "be\u00b7hext", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unter deine altbekannten Noten", "tokens": ["Und", "un\u00b7ter", "dei\u00b7ne", "alt\u00b7be\u00b7kann\u00b7ten", "No\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schreibt sie begeistert einen neuen Text.", "tokens": ["Schreibt", "sie", "be\u00b7geis\u00b7tert", "ei\u00b7nen", "neu\u00b7en", "Text", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Flur ergr\u00fcnt und bl\u00e4ulich bl\u00fcht der Flieder,", "tokens": ["Die", "Flur", "er\u00b7gr\u00fcnt", "und", "bl\u00e4u\u00b7lich", "bl\u00fcht", "der", "Flie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich aber leire meine Lenzmusik", "tokens": ["Ich", "a\u00b7ber", "lei\u00b7re", "mei\u00b7ne", "Lenz\u00b7mu\u00b7sik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und lachend schon vernehm ich die Kritik:", "tokens": ["Und", "la\u00b7chend", "schon", "ver\u00b7nehm", "ich", "die", "Kri\u00b7tik", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das denkt und singt ja wie ein Seifensieder!", "tokens": ["Das", "denkt", "und", "singt", "ja", "wie", "ein", "Sei\u00b7fen\u00b7sie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}