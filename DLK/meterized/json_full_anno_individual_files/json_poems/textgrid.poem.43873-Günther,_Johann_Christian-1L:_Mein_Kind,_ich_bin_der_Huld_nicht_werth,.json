{"textgrid.poem.43873": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Kind, ich bin der Huld nicht werth,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, ich bin der Huld nicht werth,", "tokens": ["Mein", "Kind", ",", "ich", "bin", "der", "Huld", "nicht", "werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir von deiner Hand so h\u00e4ufig widerf\u00e4hrt.", "tokens": ["Die", "mir", "von", "dei\u00b7ner", "Hand", "so", "h\u00e4u\u00b7fig", "wi\u00b7der\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum z\u00fcrne nicht, wenn ich", "tokens": ["Drum", "z\u00fcr\u00b7ne", "nicht", ",", "wenn", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich in dies seltne Gl\u00fccke", "tokens": ["Mich", "in", "dies", "selt\u00b7ne", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PDS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht, wie ich sollte, schicke,", "tokens": ["Nicht", ",", "wie", "ich", "soll\u00b7te", ",", "schi\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und glaube sicherlich:", "tokens": ["Und", "glau\u00b7be", "si\u00b7cher\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "W\u00fcrdiget dein Gnadenstrahl", "tokens": ["W\u00fcr\u00b7di\u00b7get", "dein", "Gna\u00b7den\u00b7strahl"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Meine Lippen noch einmahl,", "tokens": ["Mei\u00b7ne", "Lip\u00b7pen", "noch", "ein\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Deinen sch\u00f6nen Mund zu k\u00fc\u00dfen,", "tokens": ["Dei\u00b7nen", "sch\u00f6\u00b7nen", "Mund", "zu", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "So werd ich f\u00fcrchten m\u00fc\u00dfen,", "tokens": ["So", "werd", "ich", "f\u00fcrch\u00b7ten", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da\u00df nicht die Wollust dieser Zeit", "tokens": ["Da\u00df", "nicht", "die", "Wol\u00b7lust", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Durch ihre S\u00fc\u00dfigkeit", "tokens": ["Durch", "ih\u00b7re", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "