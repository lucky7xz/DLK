{"textgrid.poem.24729": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wirklich, bist du zu schwach, dich der seligen Zeit zu erinnern?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wirklich, bist du zu schwach, dich der seligen Zeit zu erinnern?", "tokens": ["Wirk\u00b7lich", ",", "bist", "du", "zu", "schwach", ",", "dich", "der", "se\u00b7li\u00b7gen", "Zeit", "zu", "e\u00b7rin\u00b7nern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "PRF", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "\u00dcber dem dunkelnden Tal zogen die Sterne herauf,", "tokens": ["\u00dc\u00b7ber", "dem", "dun\u00b7keln\u00b7den", "Tal", "zo\u00b7gen", "die", "Ster\u00b7ne", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Wir aber standen im Schatten und bebten. Die riesige Ulme", "tokens": ["Wir", "a\u00b7ber", "stan\u00b7den", "im", "Schat\u00b7ten", "und", "beb\u00b7ten", ".", "Die", "rie\u00b7si\u00b7ge", "Ul\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sch\u00fcttelte sich wie im Traum, warf einen Schauer herab", "tokens": ["Sch\u00fct\u00b7tel\u00b7te", "sich", "wie", "im", "Traum", ",", "warf", "ei\u00b7nen", "Schau\u00b7er", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KOKOM", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "ADV"], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "L\u00e4rmender Tropfen ins Gras: Es war keine Stunde vergangen", "tokens": ["L\u00e4r\u00b7men\u00b7der", "Trop\u00b7fen", "ins", "Gras", ":", "Es", "war", "kei\u00b7ne", "Stun\u00b7de", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Seit jenem Regen! Und mir schien es unendliche Zeit.", "tokens": ["Seit", "je\u00b7nem", "Re\u00b7gen", "!", "Und", "mir", "schien", "es", "un\u00b7end\u00b7li\u00b7che", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Denn dem Erlebenden dehnt sich das Leben: es tuen sich lautlos", "tokens": ["Denn", "dem", "Er\u00b7le\u00b7ben\u00b7den", "dehnt", "sich", "das", "Le\u00b7ben", ":", "es", "tu\u00b7en", "sich", "laut\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$.", "PPER", "VVFIN", "PRF", "ADJD"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.8": {"text": "Kl\u00fcfte unendlichen Traums zwischen zwei Blicken ihm auf:", "tokens": ["Kl\u00fcf\u00b7te", "un\u00b7end\u00b7li\u00b7chen", "Traums", "zwi\u00b7schen", "zwei", "Bli\u00b7cken", "ihm", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "APPR", "CARD", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "In mich h\u00e4tt ich gesogen dein zwanzigj\u00e4hriges Dasein", "tokens": ["In", "mich", "h\u00e4tt", "ich", "ge\u00b7so\u00b7gen", "dein", "zwan\u00b7zig\u00b7j\u00e4h\u00b7ri\u00b7ges", "Da\u00b7sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "\u2013 War mir, indessen der Baum noch seine Tropfen behielt.", "tokens": ["\u2013", "War", "mir", ",", "in\u00b7des\u00b7sen", "der", "Baum", "noch", "sei\u00b7ne", "Trop\u00b7fen", "be\u00b7hielt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wirklich, bist du zu schwach, dich der seligen Zeit zu erinnern?", "tokens": ["Wirk\u00b7lich", ",", "bist", "du", "zu", "schwach", ",", "dich", "der", "se\u00b7li\u00b7gen", "Zeit", "zu", "e\u00b7rin\u00b7nern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "PRF", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "\u00dcber dem dunkelnden Tal zogen die Sterne herauf,", "tokens": ["\u00dc\u00b7ber", "dem", "dun\u00b7keln\u00b7den", "Tal", "zo\u00b7gen", "die", "Ster\u00b7ne", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", 