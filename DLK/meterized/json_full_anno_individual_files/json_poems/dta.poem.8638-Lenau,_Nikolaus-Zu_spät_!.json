{"dta.poem.8638": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Zu sp\u00e4t !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Schon hat der Lenz verbl\u00fcht und ausgesungen,", "tokens": ["Schon", "hat", "der", "Lenz", "ver\u00b7bl\u00fcht", "und", "aus\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die holden Tr\u00e4ume, seligen Gef\u00fchle", "tokens": ["Die", "hol\u00b7den", "Tr\u00e4u\u00b7me", ",", "se\u00b7li\u00b7gen", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erstarben in der bangen Sommerschw\u00fcle,", "tokens": ["Er\u00b7star\u00b7ben", "in", "der", "ban\u00b7gen", "Som\u00b7mer\u00b7schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit der das Thatenleben angedrungen.", "tokens": ["Mit", "der", "das", "Tha\u00b7ten\u00b7le\u00b7ben", "an\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u201edas Ro\u00df gespornt! die Wehre frisch geschwungen!\u201c", "tokens": ["\u201e", "das", "Ro\u00df", "ge\u00b7spornt", "!", "die", "Weh\u00b7re", "frisch", "ge\u00b7schwun\u00b7gen", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVPP", "$.", "ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So hei\u00dft es nun im hei\u00dfen Kampfgew\u00fchle,", "tokens": ["So", "hei\u00dft", "es", "nun", "im", "hei\u00b7\u00dfen", "Kampf\u00b7ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis mir der Sabbath f\u00e4chelt seine K\u00fchle,", "tokens": ["Bis", "mir", "der", "Sab\u00b7ba\u00b7th", "f\u00e4\u00b7chelt", "sei\u00b7ne", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wann m\u00fcden mich des Todes Arm umschlungen. \u2014", "tokens": ["Wann", "m\u00fc\u00b7den", "mich", "des", "To\u00b7des", "Arm", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "ART", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir war's versagt, in jenen Bl\u00fcthentagen,", "tokens": ["Mir", "wa\u00b7r's", "ver\u00b7sagt", ",", "in", "je\u00b7nen", "Bl\u00fc\u00b7then\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O M\u00e4dchen meiner Sehnsucht, dich zu finden\u201a", "tokens": ["O", "M\u00e4d\u00b7chen", "mei\u00b7ner", "Sehn\u00b7sucht", ",", "dich", "zu", "fin\u00b7den", "\u201a"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es suchten dich vergebens meine Klagen! \u2014", "tokens": ["Es", "such\u00b7ten", "dich", "ver\u00b7ge\u00b7bens", "mei\u00b7ne", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch taucht mir hier und dort aus Kampfeswogen", "tokens": ["Noch", "taucht", "mir", "hier", "und", "dort", "aus", "Kamp\u00b7fes\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Bild herauf, doch mu\u00df es wieder schwinden\u201a", "tokens": ["Dein", "Bild", "her\u00b7auf", ",", "doch", "mu\u00df", "es", "wie\u00b7der", "schwin\u00b7den", "\u201a"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald hat die Brandung es hinabgezogen.", "tokens": ["Bald", "hat", "die", "Bran\u00b7dung", "es", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}