}, "line.4": {"text": "Da wurde sie zum erstenmale rot!", "tokens": ["Da", "wur\u00b7de", "sie", "zum", "ers\u00b7ten\u00b7ma\u00b7le", "rot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Paradies war ihr versch\u00e4mt Gesicht;", "tokens": ["Ein", "Pa\u00b7ra\u00b7dies", "war", "ihr", "ver\u00b7sch\u00e4mt", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O Himmel, ich verge\u00df es nicht!", "tokens": ["O", "Him\u00b7mel", ",", "ich", "ver\u00b7ge\u00df", "es", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O was f\u00fcr gute Freunde waren", "tokens": ["O", "was", "f\u00fcr", "gu\u00b7te", "Freun\u00b7de", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Belind' und ich, in jenen Jahren,", "tokens": ["Be\u00b7lind'", "und", "ich", ",", "in", "je\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In welchen man, im Busen noch nicht hei\u00df,", "tokens": ["In", "wel\u00b7chen", "man", ",", "im", "Bu\u00b7sen", "noch", "nicht", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIS", "$,