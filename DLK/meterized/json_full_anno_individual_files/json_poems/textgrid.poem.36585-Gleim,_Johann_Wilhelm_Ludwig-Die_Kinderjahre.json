{"textgrid.poem.36585": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Kinderjahre", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O was f\u00fcr gute Freunde waren", "tokens": ["O", "was", "f\u00fcr", "gu\u00b7te", "Freun\u00b7de", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Belind' und ich, in jenen Jahren,", "tokens": ["Be\u00b7lind'", "und", "ich", ",", "in", "je\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In welchen man, im Busen noch nicht hei\u00df,", "tokens": ["In", "wel\u00b7chen", "man", ",", "im", "Bu\u00b7sen", "noch", "nicht", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIS", "$,", "APPRART", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom J\u00fcngling nichts, und nichts von Liebe wei\u00df.", "tokens": ["Vom", "J\u00fcng\u00b7ling", "nichts", ",", "und", "nichts", "von", "Lie\u00b7be", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "$,", "KON", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In welchen man nicht suchet, und nicht flieht,", "tokens": ["In", "wel\u00b7chen", "man", "nicht", "su\u00b7chet", ",", "und", "nicht", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIS", "PTKNEG", "VVFIN", "$,", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und froh ist, wenn man nur sich sieht!", "tokens": ["Und", "froh", "ist", ",", "wenn", "man", "nur", "sich", "sieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Viel Bl\u00fcmchen wurden da gefunden,", "tokens": ["Viel", "Bl\u00fcm\u00b7chen", "wur\u00b7den", "da", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Kr\u00e4nze wurden da gewunden,", "tokens": ["Viel", "Kr\u00e4n\u00b7ze", "wur\u00b7den", "da", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die setzten wir bei einem Wettelauf", "tokens": ["Die", "setz\u00b7ten", "wir", "bei", "ei\u00b7nem", "Wet\u00b7tel\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Uns scherzend dann einander auf.", "tokens": ["Uns", "scher\u00b7zend", "dann", "ein\u00b7an\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und war einmal ein T\u00e4nzchen, dann war ich", "tokens": ["Und", "war", "ein\u00b7mal", "ein", "T\u00e4nz\u00b7chen", ",", "dann", "war", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um sie herum, und sie um mich!", "tokens": ["Um", "sie", "he\u00b7rum", ",", "und", "sie", "um", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKVZ", "$,", "KON", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df es noch, wie wir mit N\u00fcssen", "tokens": ["Ich", "wei\u00df", "es", "noch", ",", "wie", "wir", "mit", "N\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00c4pfeln, uns einander schmissen!", "tokens": ["Und", "\u00c4p\u00b7feln", ",", "uns", "ein\u00b7an\u00b7der", "schmis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df es noch, wie, bei dem G\u00e4nsespiel,", "tokens": ["Ich", "wei\u00df", "es", "noch", ",", "wie", ",", "bei", "dem", "G\u00e4n\u00b7se\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bei ihr sa\u00df, und in den Brunnen fiel,", "tokens": ["Ich", "bei", "ihr", "sa\u00df", ",", "und", "in", "den", "Brun\u00b7nen", "fiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie sie sich betr\u00fcbte, wenn der Tod", "tokens": ["Und", "wie", "sie", "sich", "be\u00b7tr\u00fcb\u00b7te", ",", "wenn", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mir seine scharfe Sense bot!", "tokens": ["Mir", "sei\u00b7ne", "schar\u00b7fe", "Sen\u00b7se", "bot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df es noch, wie wir uns gr\u00fc\u00dfen,", "tokens": ["Ich", "wei\u00df", "es", "noch", ",", "wie", "wir", "uns", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und guten Morgen sagen lie\u00dfen;", "tokens": ["Und", "gu\u00b7ten", "Mor\u00b7gen", "sa\u00b7gen", "lie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df es noch, wie gern ich den Papa", "tokens": ["Ich", "wei\u00df", "es", "noch", ",", "wie", "gern", "ich", "den", "Pa\u00b7pa"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Beg