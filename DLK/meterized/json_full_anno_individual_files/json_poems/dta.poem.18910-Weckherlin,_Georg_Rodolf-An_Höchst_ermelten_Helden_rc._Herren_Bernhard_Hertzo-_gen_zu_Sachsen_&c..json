{"dta.poem.18910": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An H\u00f6chst ermelten Helden/  \n rc. Herren Bernhard Hertzo-  \n gen zu Sachsen   &c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Dv bist/ Welt-wehrter Held/ w\u00fcrdiglich hochge-", "tokens": ["Dv", "bist", "/", "Welt\u00b7wehr\u00b7ter", "Held", "/", "w\u00fcr\u00b7dig\u00b7lich", "hoch\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "$(", "NN", "NN", "$(", "ADJD", "TRUNC"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "achtet/", "tokens": ["ach\u00b7tet", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der wahren Dapferkeit ein wahrer Erb vn\u0303 Sohn:", "tokens": ["Der", "wah\u00b7ren", "Dap\u00b7fer\u00b7keit", "ein", "wah\u00b7rer", "Erb", "v\u00f1", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bist die blum/ der ruhm der Teutschen Nation/", "tokens": ["Du", "bist", "die", "blum", "/", "der", "ruhm", "der", "Teut\u00b7schen", "Na\u00b7tion", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "$(", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Nur von dem/ der Got selbs verachten darff/ ver-", "tokens": ["Nur", "von", "dem", "/", "der", "Got", "selbs", "ver\u00b7ach\u00b7ten", "darff", "/", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "$(", "ART", "NN", "ADV", "VVFIN", "VMFIN", "$(", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "achtet.", "tokens": ["ach\u00b7tet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Dein schwerd/ so wie der strahl vnd des tods s\u00e4gis", "tokens": ["Dein", "schwerd", "/", "so", "wie", "der", "strahl", "vnd", "des", "tods", "s\u00e4\u00b7gis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ADV", "KOKOM", "ART", "NN", "KON", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schlachtet", "tokens": ["schlach\u00b7tet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Was dir zu wider ist/ ist der Gotlosen lohn/", "tokens": ["Was", "dir", "zu", "wi\u00b7der", "ist", "/", "ist", "der", "Got\u00b7lo\u00b7sen", "lohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "PTKVZ", "VAFIN", "$(", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der angefochtnen trost/ vnd der vertribnen wohn/", "tokens": ["Der", "an\u00b7ge\u00b7focht\u00b7nen", "trost", "/", "vnd", "der", "ver\u00b7trib\u00b7nen", "wohn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja auch der pracht des Reichs/ das sunst au\u00df", "tokens": ["Ja", "auch", "der", "pracht", "des", "Reichs", "/", "das", "sunst", "au\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "ART", "NN", "$(", "PDS", "VVFIN", "PTKVZ"], "meter": "---+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "scham verschmachtet.", "tokens": ["scham", "ver\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wan C", "tokens": ["Wan", "C"], "token_info": ["word", "word"], "pos": ["PWAV", "NE"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Die Donaw vnd den Rhein so offt in scharlach", "tokens": ["Die", "Do\u00b7naw", "vnd", "den", "Rhein", "so", "offt", "in", "schar\u00b7lach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NE", "ADV", "ADV", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sliessen/", "tokens": ["slies\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vnd deinen gaist/ hertz/ mund so reich an ehr/", "tokens": ["Vnd", "dei\u00b7nen", "gaist", "/", "hertz", "/", "mund", "so", "reich", "an", "ehr", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "NN", "$(", "NN", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "macht/ lehr:", "tokens": ["macht", "/", "lehr", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "W\u00fcrd Er/ sich naigend bald zu seines Sigers f\u00fcssen/", "tokens": ["W\u00fcrd", "Er", "/", "sich", "nai\u00b7gend", "bald", "zu", "sei\u00b7nes", "Si\u00b7gers", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "PRF", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bekennen/ da\u00df dir/ held/ nichts Kayserliches mehr", "tokens": ["Be\u00b7ken\u00b7nen", "/", "da\u00df", "dir", "/", "held", "/", "nichts", "Kay\u00b7ser\u00b7li\u00b7ches", "mehr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "PPER", "$(", "VVFIN", "$(", "PIS", "ADJA", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ermanglet dan der Nam/ vnnd dich selbst C", "tokens": ["Er\u00b7mang\u00b7let", "dan", "der", "Nam", "/", "vnnd", "dich", "selbst", "C"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "KON", "PPER", "ADV", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "gr\u00fcssen.", "tokens": ["gr\u00fcs\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}