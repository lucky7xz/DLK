{"dta.poem.760": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlVI.  An die Freunde.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Gehabt euch alle wol! O Erden gute Nacht! ", "tokens": ["Ge\u00b7habt", "euch", "al\u00b7le", "wol", "!", "O", "Er\u00b7den", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "$.", "NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr Himmel ich vergeh! vmbsonst hat meine wunden.", "tokens": ["Ihr", "Him\u00b7mel", "ich", "ver\u00b7geh", "!", "vmbsonst", "hat", "mei\u00b7ne", "wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit so viel wehrtem flei\u00df ", "tokens": ["Mit", "so", "viel", "wehr\u00b7tem", "flei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Man hat vmbsonst an mich/ so liebe schreiben bracht!", "tokens": ["Man", "hat", "vmbsonst", "an", "mich", "/", "so", "lie\u00b7be", "schrei\u00b7ben", "bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "PPER", "$(", "ADV", "VVFIN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Uranie vmbsonst hab ich so viel gewacht! ", "tokens": ["U\u00b7ra\u00b7nie", "vmbsonst", "hab", "ich", "so", "viel", "ge\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Eugenie ich bin eh' als jhr meynt verschwunden. ", "tokens": ["Eu\u00b7ge\u00b7nie", "ich", "bin", "eh'", "als", "jhr", "meynt", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "ADV", "KOUS", "PPER", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die kalte Brust erstart/ der puls wirdt mehr nicht sunden:", "tokens": ["Die", "kal\u00b7te", "Brust", "er\u00b7start", "/", "der", "puls", "wirdt", "mehr", "nicht", "sun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "ART", "NE", "VAFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Augen brechen mir; der matte geist verschmacht. ", "tokens": ["Die", "Au\u00b7gen", "bre\u00b7chen", "mir", ";", "der", "mat\u00b7te", "geist", "ver\u00b7schmacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Soll ich mein Vaterlandt/ soll ich dich nicht mehr schawen.", "tokens": ["Soll", "ich", "mein", "Va\u00b7ter\u00b7landt", "/", "soll", "ich", "dich", "nicht", "mehr", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "$(", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Soll ich mein todtes pfand der frembden gruft vertrawen/", "tokens": ["Soll", "ich", "mein", "tod\u00b7tes", "pfand", "der", "fremb\u00b7den", "gruft", "ver\u00b7tra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Scheid ich Eugenie ohn ewren abschied ku\u00df: ", "tokens": ["Scheid", "ich", "Eu\u00b7ge\u00b7nie", "ohn", "ew\u00b7ren", "ab\u00b7schied", "ku\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NE", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Mein licht! jhr werdet mir die Augen nicht zudrucken:", "tokens": ["Mein", "licht", "!", "jhr", "wer\u00b7det", "mir", "die", "Au\u00b7gen", "nicht", "zu\u00b7dru\u00b7cken", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd mit ", "tokens": ["Vnd", "mit"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Myrten acht ich nicht: weil ich verwelcken mu\u00df.", "tokens": ["Der", "Myr\u00b7ten", "acht", "ich", "nicht", ":", "weil", "ich", "ver\u00b7wel\u00b7cken", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "PPER", "PTKNEG", "$.", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}