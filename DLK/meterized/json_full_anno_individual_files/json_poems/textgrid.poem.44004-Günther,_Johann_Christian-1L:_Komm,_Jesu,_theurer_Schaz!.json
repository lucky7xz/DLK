], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Ach, nimm dich meiner an", "tokens": ["Ach", ",", "nimm", "dich", "mei\u00b7ner", "an"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PPOSAT", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf der verwirrten Bahn;", "tokens": ["Auf", "der", "ver\u00b7wirr\u00b7ten", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchr mich vom H\u00f6llenwege", "tokens": ["F\u00fchr", "mich", "vom", "H\u00f6l\u00b7len\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu deinem Himmelsstege!", "tokens": ["Zu", "dei\u00b7nem", "Him\u00b7melss\u00b7te\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ich seufze nach Erbarmen,", "tokens": ["Ich", "seuf\u00b7ze", "nach", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nimm mich in deine Armen!", "tokens": ["Nimm", "mich", "in", "dei\u00b7ne", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Hilf mir die kurze Zeit", "tokens": ["Hilf", "mir", "die", "kur\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hier dieser Eitelkeit,", "tokens": ["Hier", "die\u00b7ser", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stets deines Vaters Willen", "tokens": ["Stets", "dei\u00b7nes", "Va\u00b7ters", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In allem recht erf\u00fcllen;", "tokens": ["In", "al\u00b7lem", "recht", "er\u00b7f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Las mich so leben wollen,", "tokens": ["Las", "mich", "so", "le\u00b7ben", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie wahre Christen sollen.", "tokens": ["Wie", "wah\u00b7re", "Chris\u00b7ten", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}