{"textgrid.poem.44004": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, Jesu, theurer Schaz!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Jesu, theurer Schaz!", "tokens": ["Komm", ",", "Je\u00b7su", ",", "theu\u00b7rer", "Schaz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Mein Geist der macht dir Plaz.", "tokens": ["Mein", "Geist", "der", "macht", "dir", "Plaz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier seufzet dein Erl\u00f6ster:", "tokens": ["Hier", "seuf\u00b7zet", "dein", "Er\u00b7l\u00f6s\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erh\u00f6re mich, mein Tr\u00f6ster;", "tokens": ["Er\u00b7h\u00f6\u00b7re", "mich", ",", "mein", "Tr\u00f6s\u00b7ter", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach, theure Gnadenkerze,", "tokens": ["Ach", ",", "theu\u00b7re", "Gna\u00b7den\u00b7ker\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bestrahle Seel und Herze!", "tokens": ["Be\u00b7strah\u00b7le", "Seel", "und", "Her\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erneur in mir den Bund,", "tokens": ["Er\u00b7neur", "in", "mir", "den", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da deines Dieners Mund", "tokens": ["Da", "dei\u00b7nes", "Die\u00b7ners", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich in dem theuren Bade", "tokens": ["Mich", "in", "dem", "theu\u00b7ren", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der s\u00fc\u00dfen Jesusgnade", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Je\u00b7sus\u00b7gna\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von S\u00fcnden losgezehlet", "tokens": ["Von", "S\u00fcn\u00b7den", "los\u00b7ge\u00b7zeh\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und dir mich anvermehlet.", "tokens": ["Und", "dir", "mich", "an\u00b7ver\u00b7meh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich habe zwar, mein Gott,", "tokens": ["Ich", "ha\u00b7be", "zwar", ",", "mein", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch l\u00e4ngst verdienten Tod", "tokens": ["Durch", "l\u00e4ngst", "ver\u00b7dien\u00b7ten", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Schon oft den Bund gebrochen,", "tokens": ["Schon", "oft", "den", "Bund", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So da\u00df des Herzens Pochen", "tokens": ["So", "da\u00df", "des", "Her\u00b7zens", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich heist mit Angst und Schr\u00f6cken", "tokens": ["Mich", "heist", "mit", "Angst", "und", "Schr\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor deinem Zorn verstecken.", "tokens": ["Vor", "dei\u00b7nem", "Zorn", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, aber wo soll mir", "tokens": ["Ach", ",", "a\u00b7ber", "wo", "soll", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "PWAV", "VMFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auch wohl ein Ort vor dir", "tokens": ["Auch", "wohl", "ein", "Ort", "vor", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gnug Schuz und H\u00fclfe geben?", "tokens": ["Gnug", "Schuz", "und", "H\u00fcl\u00b7fe", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer kan dir widerstreben,", "tokens": ["Wer", "kan", "dir", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dir, der du Held und Wafen,", "tokens": ["Dir", ",", "der", "du", "Held", "und", "Wa\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja alles hast geschafen?", "tokens": ["Ja", "al\u00b7les", "hast", "ge\u00b7scha\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich mu\u00df vor deiner Macht", "tokens": ["Ich", "mu\u00df", "vor", "dei\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In meiner S\u00fcndennacht", "tokens": ["In", "mei\u00b7ner", "S\u00fcn\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erzittern und erbeben;", "tokens": ["Er\u00b7zit\u00b7tern", "und", "er\u00b7be\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein schwer verschuldtes Leben", "tokens": ["Mein", "schwer", "ver\u00b7schuld\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vergehet im Gedr\u00e4nge", "tokens": ["Ver\u00b7ge\u00b7het", "im", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der gro\u00dfen S\u00fcndenmenge.", "tokens": ["Der", "gro\u00b7\u00dfen", "S\u00fcn\u00b7den\u00b7men\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach, ach, wo las ich mich?", "tokens": ["Ach", ",", "ach", ",", "wo", "las", "ich", "mich", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jedoch ich hof auf dich.", "tokens": ["Je\u00b7doch", "ich", "hof", "auf", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sagest, da\u00df vor allen", "tokens": ["Du", "sa\u00b7gest", ",", "da\u00df", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir der wird wohl gefallen,", "tokens": ["Dir", "der", "wird", "wohl", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Den bey zerknirschtem Herzen", "tokens": ["Den", "bey", "zer\u00b7knirschtem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Begangne S\u00fcnden schmerzen.", "tokens": ["Be\u00b7gang\u00b7ne", "S\u00fcn\u00b7den", "schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun, Jesu, mein Gewinn,", "tokens": ["Nun", ",", "Je\u00b7su", ",", "mein", "Ge\u00b7winn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nimm Herz und Seele hin.", "tokens": ["Nimm", "Herz", "und", "See\u00b7le", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du weist es zu ergr\u00fcnden:", "tokens": ["Du", "weist", "es", "zu", "er\u00b7gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erforsche, wie die S\u00fcnden", "tokens": ["Er\u00b7for\u00b7sche", ",", "wie", "die", "S\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dies mit dem schwersten Dr\u00fccken", "tokens": ["Dies", "mit", "dem", "schwers\u00b7ten", "Dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Reu und Leid verstricken.", "tokens": ["In", "Reu", "und", "Leid", "ver\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Komm, gieb mir deinen Geist,", "tokens": ["Komm", ",", "gieb", "mir", "dei\u00b7nen", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den, welchen du verheist", "tokens": ["Den", ",", "wel\u00b7chen", "du", "ver\u00b7heist"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAT", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Demselben auszutheilen,", "tokens": ["Dem\u00b7sel\u00b7ben", "aus\u00b7zu\u00b7thei\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer bey bedr\u00e4ngtem Heulen", "tokens": ["Wer", "bey", "be\u00b7dr\u00e4ng\u00b7tem", "Heu\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit rechten Herzensz\u00e4hren", "tokens": ["Mit", "rech\u00b7ten", "Her\u00b7zens\u00b7z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu dir sich wird bekehren.", "tokens": ["Zu", "dir", "sich", "wird", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ach, nimm dich meiner an", "tokens": ["Ach", ",", "nimm", "dich", "mei\u00b7ner", "an"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PPOSAT", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf der verwirrten Bahn;", "tokens": ["Auf", "der", "ver\u00b7wirr\u00b7ten", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchr mich vom H\u00f6llenwege", "tokens": ["F\u00fchr", "mich", "vom", "H\u00f6l\u00b7len\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu deinem Himmelsstege!", "tokens": ["Zu", "dei\u00b7nem", "Him\u00b7melss\u00b7te\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ich seufze nach Erbarmen,", "tokens": ["Ich", "seuf\u00b7ze", "nach", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nimm mich in deine Armen!", "tokens": ["Nimm", "mich", "in", "dei\u00b7ne", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Hilf mir die kurze Zeit", "tokens": ["Hilf", "mir", "die", "kur\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hier dieser Eitelkeit,", "tokens": ["Hier", "die\u00b7ser", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stets deines Vaters Willen", "tokens": ["Stets", "dei\u00b7nes", "Va\u00b7ters", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In allem recht erf\u00fcllen;", "tokens": ["In", "al\u00b7lem", "recht", "er\u00b7f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Las mich so leben wollen,", "tokens": ["Las", "mich", "so", "le\u00b7ben", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie wahre Christen sollen.", "tokens": ["Wie", "wah\u00b7re", "Chris\u00b7ten", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Komm, Jesu, theurer Schaz!", "tokens": ["Komm", ",", "Je\u00b7su", ",", "theu\u00b7rer", "Schaz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Mein Geist der macht dir Plaz.", "tokens": ["Mein", "Geist", "der", "macht", "dir", "Plaz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier seufzet dein Erl\u00f6ster:", "tokens": ["Hier", "seuf\u00b7zet", "dein", "Er\u00b7l\u00f6s\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erh\u00f6re mich, mein Tr\u00f6ster;", "tokens": ["Er\u00b7h\u00f6\u00b7re", "mich", ",", "mein", "Tr\u00f6s\u00b7ter", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "lin