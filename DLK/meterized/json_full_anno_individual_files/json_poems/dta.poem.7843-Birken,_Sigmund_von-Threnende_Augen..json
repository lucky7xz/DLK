{"dta.poem.7843": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Threnende Augen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Edle Hirtinn! deine Z\u00e4hren/", "tokens": ["Ed\u00b7le", "Hir\u00b7tinn", "!", "dei\u00b7ne", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die du oft pflegst zuverr\u00f6hren", "tokens": ["die", "du", "oft", "pflegst", "zu\u00b7ver\u00b7r\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "aus den lieben Aeugelein/", "tokens": ["aus", "den", "lie\u00b7ben", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "kan ich unberedt nit lassen/", "tokens": ["kan", "ich", "un\u00b7be\u00b7redt", "nit", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ich mu\u00df meine Feder fassen/", "tokens": ["ich", "mu\u00df", "mei\u00b7ne", "Fe\u00b7der", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sie in Threnen tunken ein.", "tokens": ["sie", "in", "Thre\u00b7nen", "tun\u00b7ken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Schwanenweisse Wangen/", "tokens": ["Dei\u00b7ne", "Schwa\u00b7nen\u00b7weis\u00b7se", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wann daran die Zehrlein hangen/", "tokens": ["wann", "da\u00b7ran", "die", "Ze\u00b7hrlein", "han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PAV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sind den Perlen Muscheln gleich,", "tokens": ["sind", "den", "Per\u00b7len", "Mu\u00b7scheln", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Perle! du l\u00e4sst Perlen rinnen:", "tokens": ["Per\u00b7le", "!", "du", "l\u00e4sst", "Per\u00b7len", "rin\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "fangt sie auf/ ihr Nereinnen!", "tokens": ["fangt", "sie", "auf", "/", "ihr", "Ner\u00b7ein\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "$(", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "solches Weinen machet reich.", "tokens": ["sol\u00b7ches", "Wei\u00b7nen", "ma\u00b7chet", "reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Diamanten/ die zerflossen/", "tokens": ["Di\u00b7a\u00b7man\u00b7ten", "/", "die", "zer\u00b7flos\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kommen Backen-ab geschossen.", "tokens": ["kom\u00b7men", "Ba\u00b7cken\u00b7ab", "ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Augen/ sihet man", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "/", "si\u00b7het", "man"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PIS"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "viel Krystallen-tropfen ballen.", "tokens": ["viel", "Kry\u00b7stal\u00b7len\u00b7trop\u00b7fen", "bal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! da\u00df ich nit dieses Wallen/", "tokens": ["Ach", "!", "da\u00df", "ich", "nit", "die\u00b7ses", "Wal\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PTKNEG", "PDAT", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "diese Threnen/ stopfen kan!", "tokens": ["die\u00b7se", "Thre\u00b7nen", "/", "stop\u00b7fen", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja/ sie fliessen mir zu Herzen:", "tokens": ["Ja", "/", "sie", "flies\u00b7sen", "mir", "zu", "Her\u00b7zen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die gewi\u00dflich nicht ohn Schmerzen", "tokens": ["die", "ge\u00b7wi\u00df\u00b7lich", "nicht", "ohn", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "aus dem Herzen fliessen dir.", "tokens": ["aus", "dem", "Her\u00b7zen", "flies\u00b7sen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sag doch/ mein! was f\u00fcr Ursachen", "tokens": ["Sag", "doch", "/", "mein", "!", "was", "f\u00fcr", "Ur\u00b7sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$(", "PPOSAT", "$.", "PWS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "m\u00f6gen dich wohl weinend machen?", "tokens": ["m\u00f6\u00b7gen", "dich", "wohl", "wei\u00b7nend", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich rah", "tokens": ["Ich", "rah"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Sind dann deine Aeuglein Bronnen?", "tokens": ["Sind", "dann", "dei\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", "Bron\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die man sehend h\u00e4lt vor Sonnen?", "tokens": ["die", "man", "se\u00b7hend", "h\u00e4lt", "vor", "Son\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feur und Wasser reimt sich nicht.", "tokens": ["Feur", "und", "Was\u00b7ser", "reimt", "sich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es wird strahlen nicht so helle/", "tokens": ["Es", "wird", "strah\u00b7len", "nicht", "so", "hel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "PTKNEG", "ADV", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wann es seyn will eine Quelle/", "tokens": ["wann", "es", "seyn", "will", "ei\u00b7ne", "Quel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAINF", "VMFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "deiner Augenliechter Liecht.", "tokens": ["dei\u00b7ner", "Au\u00b7gen\u00b7liech\u00b7ter", "Liecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6ne Sonnen! ihr solt scheinen/", "tokens": ["Sch\u00f6\u00b7ne", "Son\u00b7nen", "!", "ihr", "solt", "schei\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nicht betr\u00fcbte Threnen weinen.", "tokens": ["nicht", "be\u00b7tr\u00fcb\u00b7te", "Thre\u00b7nen", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Euer Himmel/ da ihr steht/", "tokens": ["Eu\u00b7er", "Him\u00b7mel", "/", "da", "ihr", "steht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "dieses himmlisch\u2019 Angesichte/", "tokens": ["die\u00b7ses", "himm\u00b7lisch'", "An\u00b7ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "siht viel sch\u00f6ner/ wann es liechte/", "tokens": ["siht", "viel", "sch\u00f6\u00b7ner", "/", "wann", "es", "liech\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "als wann es tr\u00fcb \u00fcbergeht.", "tokens": ["als", "wann", "es", "tr\u00fcb", "\u00fc\u00b7ber\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Zwar/ mein Urtheil beyzulegen:", "tokens": ["Zwar", "/", "mein", "Ur\u00b7theil", "bey\u00b7zu\u00b7le\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ihr seit Sonne/ seit auch Regen.", "tokens": ["ihr", "seit", "Son\u00b7ne", "/", "seit", "auch", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$(", "APPR", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ihr Herzen z\u00fcndet an/", "tokens": ["Weil", "ihr", "Her\u00b7zen", "z\u00fcn\u00b7det", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "liebe Aeuglein! mu\u00df ingleichen", "tokens": ["lie\u00b7be", "A\u00b7e\u00b7u\u00b7glein", "!", "mu\u00df", "in\u00b7glei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "VMFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "eure Quelle L\u00f6schung reichen/", "tokens": ["eu\u00b7re", "Quel\u00b7le", "L\u00f6\u00b7schung", "rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "giessen manchen frischen Thran.", "tokens": ["gies\u00b7sen", "man\u00b7chen", "fri\u00b7schen", "Thran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr beweinet itzt die Schmerzen/", "tokens": ["Ihr", "be\u00b7wei\u00b7net", "itzt", "die", "Schmer\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die einmal Verliebten Herzen", "tokens": ["die", "ein\u00b7mal", "Ver\u00b7lieb\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "eure Sch\u00f6nheit machen wird.", "tokens": ["eu\u00b7re", "Sch\u00f6n\u00b7heit", "ma\u00b7chen", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die viel Weinens noch soll machen/", "tokens": ["Die", "viel", "Wei\u00b7nens", "noch", "soll", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "weiner billig/ die ins Lachen", "tokens": ["wei\u00b7ner", "bil\u00b7lig", "/", "die", "ins", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$(", "ART", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "k\u00fcnftig eine Theurung f\u00fchrt.", "tokens": ["k\u00fcnf\u00b7tig", "ei\u00b7ne", "Theu\u00b7rung", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": " selbst die Liebe/ recht zusagen/", "tokens": ["selbst", "die", "Lie\u00b7be", "/", "recht", "zu\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hat hier Wohnung aufgeschlagen.", "tokens": ["hat", "hier", "Woh\u00b7nung", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herzen/ die der Augen Glut", "tokens": ["Her\u00b7zen", "/", "die", "der", "Au\u00b7gen", "Glut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "PRELS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "nicht zerschmelzet/ Stein und Eichen/", "tokens": ["nicht", "zer\u0