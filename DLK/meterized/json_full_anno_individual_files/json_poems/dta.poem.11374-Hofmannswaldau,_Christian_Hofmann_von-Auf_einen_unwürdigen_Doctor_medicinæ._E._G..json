{"dta.poem.11374": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen unw\u00fcrdigen   Doctor  \n medicin\u00e6.  \n E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Musen singt!", "tokens": ["Ihr", "Mu\u00b7sen", "singt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nun zieht Hans Taps die Doctor-hosen an;", "tokens": ["Nun", "zieht", "Hans", "Taps", "die", "Doc\u00b7tor\u00b7ho\u00b7sen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und macht erz\u00f6rnte minen,", "tokens": ["Und", "macht", "er\u00b7z\u00f6rn\u00b7te", "mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die patienten zu bedienen,", "tokens": ["Die", "pa\u00b7ti\u00b7en\u00b7ten", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wodurch er tod und teufel scheiden kan.", "tokens": ["Wo\u00b7durch", "er", "tod", "und", "teu\u00b7fel", "schei\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es zittern und beben des todes gesellen,", "tokens": ["Es", "zit\u00b7tern", "und", "be\u00b7ben", "des", "to\u00b7des", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und wollen sich selber die gr\u00e4ber bestellen.", "tokens": ["Und", "wol\u00b7len", "sich", "sel\u00b7ber", "die", "gr\u00e4\u00b7ber", "be\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr Musen singt!", "tokens": ["Ihr", "Mu\u00b7sen", "singt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du armer tod! so wirst du coujonirt!", "tokens": ["Du", "ar\u00b7mer", "tod", "!", "so", "wirst", "du", "cou\u00b7jo\u00b7nirt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gelt! solche pillen-helden,", "tokens": ["Gelt", "!", "sol\u00b7che", "pil\u00b7len\u00b7hel\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die k\u00f6nnen dir den abschied melden,", "tokens": ["Die", "k\u00f6n\u00b7nen", "dir", "den", "ab\u00b7schied", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der dich von uns in ferne grentzen f\u00fchrt.", "tokens": ["Der", "dich", "von", "uns", "in", "fer\u00b7ne", "grent\u00b7zen", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach tropffen! ach pulver! ach gl\u00fcckliche pillen!", "tokens": ["Ach", "tropf\u00b7fen", "!", "ach", "pul\u00b7ver", "!", "ach", "gl\u00fcck\u00b7li\u00b7che", "pil\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "XY", "NE", "$.", "XY", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ach! last uns die l\u00e4nder mit doctorn erf\u00fcllen!", "tokens": ["Ach", "!", "last", "uns", "die", "l\u00e4n\u00b7der", "mit", "doc\u00b7torn", "er\u00b7f\u00fcl\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ART", "ADJA", "APPR", "ADV", "VVINF", "$."], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Jhr Musen singt!", "tokens": ["Ihr", "Mu\u00b7sen", "singt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der lincke strumpff ist gl\u00fccklich angebracht;", "tokens": ["Der", "lin\u00b7cke", "strumpff", "ist", "gl\u00fcck\u00b7lich", "an\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Potz blicktz! wie knackt die seide", "tokens": ["Potz", "blicktz", "!", "wie", "knackt", "die", "sei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu grossem hertzeleide!", "tokens": ["Zu", "gros\u00b7sem", "hert\u00b7ze\u00b7lei\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die str\u00fcmpffe hat der teufel gar gemacht.", "tokens": ["Die", "str\u00fcmpf\u00b7fe", "hat", "der", "teu\u00b7fel", "gar", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch stille! sie sollen ihn selber verjagen,", "tokens": ["Doch", "stil\u00b7le", "!", "sie", "sol\u00b7len", "ihn", "sel\u00b7ber", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "So offt sie dem doctor ", "tokens": ["So", "offt", "sie", "dem", "doc\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jhr Musen singt!", "tokens": ["Ihr", "Mu\u00b7sen", "singt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der doctor kn\u00fcpfft die k\u00fcrmes-weste zu,", "tokens": ["Der", "doc\u00b7tor", "kn\u00fcpfft", "die", "k\u00fcr\u00b7mes\u00b7wes\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sieht schon mit belieben", "tokens": ["Und", "sieht", "schon", "mit", "be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den kn\u00f6pffen thaler angeschrieben.", "tokens": ["Den", "kn\u00f6pf\u00b7fen", "tha\u00b7ler", "an\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Herr Doctor! macht mir nicht zu grossen schmu!", "tokens": ["Herr", "Doc\u00b7tor", "!", "macht", "mir", "nicht", "zu", "gros\u00b7sen", "schmu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die hunde, so sonsten nicht Juden vertragen,", "tokens": ["Die", "hun\u00b7de", ",", "so", "sons\u00b7ten", "nicht", "Ju\u00b7den", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "ADV", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die m\u00f6chten euch irgend mit ihnen verjagen,", "tokens": ["Die", "m\u00f6ch\u00b7ten", "euch", "ir\u00b7gend", "mit", "ih\u00b7nen", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Jhr Musen singt!", "tokens": ["Ihr", "Mu\u00b7sen", "singt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nun nimmt Hanns Taps den spitzen-mantel um,", "tokens": ["Nun", "nimmt", "Hanns", "Taps", "den", "spit\u00b7zen\u00b7man\u00b7tel", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und k\u00f6mmt mit gravit\u00e4ten", "tokens": ["Und", "k\u00f6mmt", "mit", "gra\u00b7vi\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum doctor-schm\u00e4u\u00dfgen angetreten.", "tokens": ["Zum", "doc\u00b7tor\u00b7schm\u00b7\u00e4u\u00df\u00b7gen", "an\u00b7ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bey meiner seel! er ist f\u00fcrwahr nicht tumm.", "tokens": ["Bey", "mei\u00b7ner", "seel", "!", "er", "ist", "f\u00fcr\u00b7wahr", "nicht", "tumm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur letzlich, da hatt sich die \u2012 \u2012 ergossen:", "tokens": ["Nur", "letz\u00b7lich", ",", "da", "hatt", "sich", "die", "\u2012", "\u2012", "er\u00b7gos\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VAFIN", "PRF", "ART", "$(", "$(", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da ist ihm ein bi\u00dfgen gehirne verflossen.", "tokens": ["Da", "ist", "ihm", "ein", "bi\u00df\u00b7gen", "ge\u00b7hir\u00b7ne", "ver\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Jhr Musen singt!", "tokens": ["Ihr", "Mu\u00b7sen", "singt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der doctor kommt mit seinem gantzen staat.", "tokens": ["Der", "doc\u00b7tor", "kommt", "mit", "sei\u00b7nem", "gant\u00b7zen", "staat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun ist er abgebacken,", "tokens": ["Nun", "ist", "er", "ab\u00b7ge\u00b7ba\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hat mehr dignit\u00e4t im nacken,", "tokens": ["Und", "hat", "mehr", "dig\u00b7ni\u00b7t\u00e4t", "im", "na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als Hippocras jemahls besessen hat.", "tokens": ["Als", "Hip\u00b7po\u00b7cras", "je\u00b7mahls", "be\u00b7ses\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Jhr m\u00e4gdgen! ach! kommt doch aus fenster gelauffen!", "tokens": ["Ihr", "m\u00e4gd\u00b7gen", "!", "ach", "!", "kommt", "doch", "aus", "fens\u00b7ter", "ge\u00b7lauf\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "XY", "$.", "VVFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die doctor sind heute sehr wohlfeil zu kauffen.", "tokens": ["Die", "doc\u00b7tor", "sind", "heu\u00b7te", "sehr", "wohl\u00b7feil", "zu", "kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Jhr Musen singt!", "tokens": ["Ihr", "Mu\u00b7sen", "singt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nun ist der karrn einmahl in dr .. gef\u00fchrt:", "tokens": ["Nun", "ist", "der", "karrn", "ein\u00b7mahl", "in", "dr", "..", "ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "NE", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wird auch drinnen bleiben,", "tokens": ["Und", "wird", "auch", "drin\u00b7nen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bi\u00df man von hinten zu wird schreiben.", "tokens": ["Bi\u00df", "man", "von", "hin\u00b7ten", "zu", "wird", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADV", "APPR", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Indessen f\u00e4hrt, wer mit ducaten schmiert.", "tokens": ["In\u00b7des\u00b7sen", "f\u00e4hrt", ",", "wer", "mit", "du\u00b7ca\u00b7ten", "schmiert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jhr mesten! ihr gl\u00e4ser! ihr krausen und titten!", "tokens": ["Ihr", "mes\u00b7ten", "!", "ihr", "gl\u00e4\u00b7ser", "!", "ihr", "krau\u00b7sen", "und", "tit\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "$.", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Kommt alle zu eurem herr doctor geritten!", "tokens": ["Kommt", "al\u00b7le", "zu", "eu\u00b7rem", "herr", "doc\u00b7tor", "ge\u00b7rit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}