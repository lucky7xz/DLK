], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dem Zevs mit seinem Finger droht.", "tokens": ["Dem", "Zevs", "mit", "sei\u00b7nem", "Fin\u00b7ger", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich ging zur\u00fcck; o Freund! nun gl\u00fchte Purpurroth", "tokens": ["Ich", "ging", "zu\u00b7r\u00fcck", ";", "o", "Freund", "!", "nun", "gl\u00fch\u00b7te", "Pur\u00b7pur\u00b7roth"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "FM", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf meiner sonst so blassen Wange;", "tokens": ["Auf", "mei\u00b7ner", "sonst", "so", "blas\u00b7sen", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich gr\u00fc\u00dfte Lentulus und ihn", "tokens": ["Mich", "gr\u00fc\u00df\u00b7te", "Len\u00b7tu\u00b7lus", "und", "ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab ich verwirrt gedankt, ich taumelte, ich schien", "tokens": ["Hab", "ich", "ver\u00b7wirrt", "ge\u00b7dankt", ",", "ich", "tau\u00b7mel\u00b7te", ",", "ich", "schien"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADJD", "VVPP", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den trunknen Menschen gleich im Reden und im", "tokens": ["Den", "trunk\u00b7nen", "Men\u00b7schen", "gleich", "im", "Re\u00b7den", "und", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "KON", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gange;", "tokens": ["Gan\u00b7ge", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und dennoch schw\u00f6r ich dir beym heiligsten Gesange:", "tokens": ["Und", "den\u00b7noch", "schw\u00f6r", "ich", "dir", "beym", "hei\u00b7ligs\u00b7ten", "Ge\u00b7san\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wenn Friedrich mir von Cedernholz", "tokens": ["Wenn", "Fried\u00b7rich", "mir", "von", "Ce\u00b7dern\u00b7holz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Haus durch K\u00fcnstler bauen lie\u00dfe,", "tokens": ["Ein", "Haus", "durch", "K\u00fcnst\u00b7ler", "bau\u00b7en", "lie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch w\u00fcrde nicht dadurch der Sappho Seele stolz,", "tokens": ["Doch", "w\u00fcr\u00b7de", "nicht", "da\u00b7durch", "der", "Sap\u00b7pho", "See\u00b7le", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PAV", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn ihr ist nur die Freundschaft s\u00fc\u00dfe.", "tokens": ["Denn", "ihr", "ist", "nur", "die", "Freund\u00b7schaft", "s\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}