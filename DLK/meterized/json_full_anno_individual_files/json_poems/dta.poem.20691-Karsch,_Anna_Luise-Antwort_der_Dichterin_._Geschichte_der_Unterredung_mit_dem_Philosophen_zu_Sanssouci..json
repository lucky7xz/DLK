{"dta.poem.20691": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Antwort der Dichterin .  \n  Geschichte der Unterredung  \n mit  \n dem Philosophen zu Sanssouci.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, wenn mir vor dem Schritt zum Leben                 ", "tokens": ["Freund", ",", "wenn", "mir", "vor", "dem", "Schritt", "zum", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht von der g\u00fctigen Natur", "tokens": ["Nicht", "von", "der", "g\u00fc\u00b7ti\u00b7gen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon ein Befehl zur Demuth ward gegeben,", "tokens": ["Schon", "ein", "Be\u00b7fehl", "zur", "De\u00b7muth", "ward", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann w\u00fcrd ich kleine Creatur", "tokens": ["Dann", "w\u00fcrd", "ich", "klei\u00b7ne", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Mit innerm Stolz mich hoch erheben,", "tokens": ["Mit", "in\u00b7nerm", "Stolz", "mich", "hoch", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Dir erz\u00e4hlen, da\u00df in Friedrichs Marmor-Saal", "tokens": ["Und", "Dir", "er\u00b7z\u00e4h\u00b7len", ",", "da\u00df", "in", "Fried\u00b7richs", "Mar\u00b7mor\u00b7Saal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "KOUS", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein falticht Antlitz sich bespiegelt,", "tokens": ["Mein", "fal\u00b7ticht", "Ant\u00b7litz", "sich", "be\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und aus der Brust das Herz befl\u00fcgelt", "tokens": ["Und", "aus", "der", "Brust", "das", "Herz", "be\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auf meine Lippen trat, und meiner Worte Wahl", "tokens": ["Auf", "mei\u00b7ne", "Lip\u00b7pen", "trat", ",", "und", "mei\u00b7ner", "Wor\u00b7te", "Wahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und den Accent geregelt h\u00e4tte,", "tokens": ["Und", "den", "Ac\u00b7cent", "ge\u00b7re\u00b7gelt", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Indem der K\u00f6nig mit mir redte,", "tokens": ["In\u00b7dem", "der", "K\u00f6\u00b7nig", "mit", "mir", "red\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der gr\u00f6\u00dfre Redekunst besitzt,", "tokens": ["Der", "gr\u00f6\u00df\u00b7re", "Re\u00b7de\u00b7kunst", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als Marc Anton, der vor dem Volke", "tokens": ["Als", "Marc", "An\u00b7ton", ",", "der", "vor", "dem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Des C\u00e4sars M\u00f6rder bald verklaget, bald besch\u00fctzt.", "tokens": ["Des", "C\u00e4\u00b7sars", "M\u00f6r\u00b7der", "bald", "ver\u00b7kla\u00b7get", ",", "bald", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er kam, und \u00fcber Ihm in einer goldnen Wolke", "tokens": ["Er", "kam", ",", "und", "\u00fc\u00b7ber", "Ihm", "in", "ei\u00b7ner", "gold\u00b7nen", "Wol\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sah ich den schwebenden Apoll.", "tokens": ["Sah", "ich", "den", "schwe\u00b7ben\u00b7den", "A\u00b7poll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Er sprach, und in mein Ohr erscholl", "tokens": ["Er", "sprach", ",", "und", "in", "mein", "Ohr", "er\u00b7scholl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mit Seiner schnell gesprochnen Frage", "tokens": ["Mit", "Sei\u00b7ner", "schnell", "ge\u00b7sproch\u00b7nen", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der Donner Jupiters, und Seines Auges Blick", "tokens": ["Der", "Don\u00b7ner", "Ju\u00b7pi\u00b7ters", ",", "und", "Sei\u00b7nes", "Au\u00b7ges", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "War wie der Blitz am Erndtetage:", "tokens": ["War", "wie", "der", "Blitz", "am", "Ernd\u00b7te\u00b7ta\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Doch, Freund! ich staunte nicht zur\u00fcck.", "tokens": ["Doch", ",", "Freund", "!", "ich", "staun\u00b7te", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sagte, welcher Mann mich zeugte,", "tokens": ["Ich", "sag\u00b7te", ",", "wel\u00b7cher", "Mann", "mich", "zeug\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und welcher Staub mich niederbeugte:", "tokens": ["Und", "wel\u00b7cher", "Staub", "mich", "nie\u00b7der\u00b7beug\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mein Genie herauf gestrebt,", "tokens": ["Wie", "mein", "Ge\u00b7nie", "her\u00b7auf", "ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In welchem Dunkel ich der Jugend Zeit verlebt,", "tokens": ["In", "wel\u00b7chem", "Dun\u00b7kel", "ich", "der", "Ju\u00b7gend", "Zeit", "ver\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da\u00df ich nicht der Kunst geschriebne Regeln w\u00fc\u00dfte:", "tokens": ["Und", "da\u00df", "ich", "nicht", "der", "Kunst", "ge\u00b7schrieb\u00b7ne", "Re\u00b7geln", "w\u00fc\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da\u00df mein Liebling, der Plutarch,", "tokens": ["Und", "da\u00df", "mein", "Lieb\u00b7ling", ",", "der", "Plu\u00b7tarch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Oft einen finstern Blick von mir vertragen m\u00fc\u00dfte,", "tokens": ["Oft", "ei\u00b7nen", "fins\u00b7tern", "Blick", "von", "mir", "ver\u00b7tra\u00b7gen", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn in ihm s\u00e4nd ich nie den Sieger, den Monarch,", "tokens": ["Denn", "in", "ihm", "s\u00e4nd", "ich", "nie", "den", "Sie\u00b7ger", ",", "den", "Mon\u00b7arch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den Mensch und Philosoph vereinet,", "tokens": ["Den", "Mensch", "und", "Phi\u00b7lo\u00b7soph", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ob Alexander gleich gesieget und geweinet,", "tokens": ["Ob", "A\u00b7lex\u00b7an\u00b7der", "gleich", "ge\u00b7sie\u00b7get", "und", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und C\u00e4sar selbst zufrieden schien,", "tokens": ["Und", "C\u00e4\u00b7sar", "selbst", "zu\u00b7frie\u00b7den", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn er jedweden Tag bezeichnet mit Verschonen,", "tokens": ["Wenn", "er", "jed\u00b7we\u00b7den", "Tag", "be\u00b7zeich\u00b7net", "mit", "Ver\u00b7scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und einem Brutus selbst verziehn,", "tokens": ["Und", "ei\u00b7nem", "Bru\u00b7tus", "selbst", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der mit dem Dolch ihm sollte lohnen,", "tokens": ["Der", "mit", "dem", "Dolch", "ihm", "soll\u00b7te", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Doch f\u00e4nd ich auf der Griechen Thronen,", "tokens": ["Doch", "f\u00e4nd", "ich", "auf", "der", "Grie\u00b7chen", "Thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und auf der R\u00f6mer Kampfplatz nichts", "tokens": ["Und", "auf", "der", "R\u00f6\u00b7mer", "Kampf\u00b7platz", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vergleichendes mit dem, der Seines Angesichts", "tokens": ["Ver\u00b7glei\u00b7chen\u00b7des", "mit", "dem", ",", "der", "Sei\u00b7nes", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In Winterl\u00fcften nicht geschonet,", "tokens": ["In", "Win\u00b7ter\u00b7l\u00fcf\u00b7ten", "nicht", "ge\u00b7scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und wenn der Lenz gebl\u00fcht das Kriegeszelt bewohnet,", "tokens": ["Und", "wenn", "der", "Lenz", "ge\u00b7bl\u00fcht", "das", "Krie\u00b7ges\u00b7zelt", "be\u00b7woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von Freuden und vom Throne fern.", "tokens": ["Von", "Freu\u00b7den", "und", "vom", "Thro\u00b7ne", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und mehr den Vater als den Herrn", "tokens": ["Und", "mehr", "den", "Va\u00b7ter", "als", "den", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Zur\u00fcckgebracht aus so viel Schlachten.", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7bracht", "aus", "so", "viel", "Schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Er frug: wer lehrte dich Gesang?", "tokens": ["Er", "frug", ":", "wer", "lehr\u00b7te", "dich", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wer unterwies dich in Apollens Saytenzwang?", "tokens": ["Wer", "un\u00b7ter\u00b7wies", "dich", "in", "A\u00b7pol\u00b7lens", "Say\u00b7ten\u00b7zwang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Held! sprach ich, die Natur und Deine Siege", "tokens": ["Held", "!", "sprach", "ich", ",", "die", "Na\u00b7tur", "und", "Dei\u00b7ne", "Sie\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "machten", "tokens": ["mach\u00b7ten"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mich ohne Kunst zur Dichterin.", "tokens": ["Mich", "oh\u00b7ne", "Kunst", "zur", "Dich\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er l\u00e4chelte, und wollte wissen", "tokens": ["Er", "l\u00e4\u00b7chel\u00b7te", ",", "und", "woll\u00b7te", "wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Woher ich Nahrung n\u00e4hm; da sagt\u2019 ich: Freunde m\u00fcssen", "tokens": ["Wo\u00b7her", "ich", "Nah\u00b7rung", "n\u00e4hm", ";", "da", "sagt'", "ich", ":", "Freun\u00b7de", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$.", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mich n\u00e4hren, t\u00e4glich geh ich hin", "tokens": ["Mich", "n\u00e4h\u00b7ren", ",", "t\u00e4g\u00b7lich", "geh", "ich", "hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum niemals stolzen Stahl, der stets mich gerne siehet,", "tokens": ["Zum", "nie\u00b7mals", "stol\u00b7zen", "Stahl", ",", "der", "stets", "mich", "ger\u00b7ne", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "$,", "PRELS", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und eine zweyte S\u00e4ngerin", "tokens": ["Und", "ei\u00b7ne", "zwey\u00b7te", "S\u00e4n\u00b7ge\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In meiner Tochter Dir erziehet.", "tokens": ["In", "mei\u00b7ner", "Toch\u00b7ter", "Dir", "er\u00b7zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich sprach\u2019s, und Friedrichs Blick schien meinen", "tokens": ["Ich", "sprach's", ",", "und", "Fried\u00b7richs", "Blick", "schien", "mei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "NE", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Freund zu loben.", "tokens": ["Freund", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Nach meiner Wohnung frug er mich.", "tokens": ["Nach", "mei\u00b7ner", "Woh\u00b7nung", "frug", "er", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Monarch! sprach ich, die Sterne gr\u00e4nzen nachbarlich", "tokens": ["Mon\u00b7arch", "!", "sprach", "ich", ",", "die", "Ster\u00b7ne", "gr\u00e4n\u00b7zen", "nach\u00b7bar\u00b7lich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Mit meinem Winkel unterm Dache hoch erhoben.", "tokens": ["Mit", "mei\u00b7nem", "Win\u00b7kel", "un\u00b7term", "Da\u00b7che", "hoch", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn Du nicht z\u00fcrntest, w\u00fcrd\u2019 ich Dich", "tokens": ["Wenn", "Du", "nicht", "z\u00fcrn\u00b7test", ",", "w\u00fcrd'", "ich", "Dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "VAFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Kniebeugend bitten, da\u00df Du meine Kammer d\u00e4chtest,", "tokens": ["Knie\u00b7beu\u00b7gend", "bit\u00b7ten", ",", "da\u00df", "Du", "mei\u00b7ne", "Kam\u00b7mer", "d\u00e4ch\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "