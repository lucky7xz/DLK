{"dta.poem.19994": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frisch aufgebl\u00fchet stand die Heimat wieder,", "tokens": ["Frisch", "auf\u00b7ge\u00b7bl\u00fc\u00b7het", "stand", "die", "Hei\u00b7mat", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vers\u00f6hnt dich lieben Fl\u00fcchtling zu empfangen,", "tokens": ["Ver\u00b7s\u00f6hnt", "dich", "lie\u00b7ben", "Fl\u00fccht\u00b7ling", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus dunklem Gr\u00fcn mondhelle Bl\u00fcten drangen,", "tokens": ["Aus", "dunk\u00b7lem", "Gr\u00fcn", "mond\u00b7hel\u00b7le", "Bl\u00fc\u00b7ten", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den V\u00f6geln wuchs ein farbig neu Gefieder.", "tokens": ["Den", "V\u00f6\u00b7geln", "wuchs", "ein", "far\u00b7big", "neu", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus wolk'gen W\u00e4ldern t\u00f6nten ihre Lieder,", "tokens": ["Aus", "wol\u00b7k'\u00b7gen", "W\u00e4l\u00b7dern", "t\u00f6n\u00b7ten", "ih\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Thal, auf Bergen, Hirt und Hirtin sangen,", "tokens": ["Im", "Thal", ",", "auf", "Ber\u00b7gen", ",", "Hirt", "und", "Hir\u00b7tin", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war als senkt' in aller Farben Prangen", "tokens": ["Es", "war", "als", "senkt'", "in", "al\u00b7ler", "Far\u00b7ben", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der reiche Himmel sich zur Erde nieder.", "tokens": ["Der", "rei\u00b7che", "Him\u00b7mel", "sich", "zur", "Er\u00b7de", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Arme waren ausgereckt in Freude,", "tokens": ["Und", "Ar\u00b7me", "wa\u00b7ren", "aus\u00b7ge\u00b7reckt", "in", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Herzen schlugen sehnend dir entgegen,", "tokens": ["Und", "Her\u00b7zen", "schlu\u00b7gen", "seh\u00b7nend", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom rauhen Norden solltest du erwarmen.", "tokens": ["Vom", "rau\u00b7hen", "Nor\u00b7den", "soll\u00b7test", "du", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da nahm dich uns der Tod mit blassem Neide.", "tokens": ["Da", "nahm", "dich", "uns", "der", "Tod", "mit", "blas\u00b7sem", "Nei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun welke nur, du reicher Fr\u00fchlingssegen!", "tokens": ["Nun", "wel\u00b7ke", "nur", ",", "du", "rei\u00b7cher", "Fr\u00fch\u00b7lings\u00b7se\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nichts frommst du mehr mit deinem Schmuck uns Armen.", "tokens": ["Nichts", "frommst", "du", "mehr", "mit", "dei\u00b7nem", "Schmuck", "uns", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}