{"dta.poem.5394": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es vermehrt so gar der Schatten,", "tokens": ["Es", "ver\u00b7mehrt", "so", "gar", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den das Licht durch C\u00f6rper macht,", "tokens": ["Den", "das", "Licht", "durch", "C\u00f6r\u00b7per", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Denn wenn Schatten-Bilder sich", "tokens": ["Denn", "wenn", "Schat\u00b7ten\u00b7Bil\u00b7der", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "PRF"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Mit des Urbilds Bildern gatten;", "tokens": ["Mit", "des", "Ur\u00b7bilds", "Bil\u00b7dern", "gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stellt sich der ", "tokens": ["Stellt", "sich", "der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Unsern Augen doppelt f\u00fcr.", "tokens": ["Un\u00b7sern", "Au\u00b7gen", "dop\u00b7pelt", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Mehrt sich noch verwunderlich", "tokens": ["Mehrt", "sich", "noch", "ver\u00b7wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Durch der Schatten Dunckelheit.", "tokens": ["Durch", "der", "Schat\u00b7ten", "Dun\u00b7ckel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "In den W\u00e4ldern, auf den Matten,", "tokens": ["In", "den", "W\u00e4l\u00b7dern", ",", "auf", "den", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn wir alle Vorw\u00fcrff sehn;", "tokens": ["Wenn", "wir", "al\u00b7le", "Vor\u00b7w\u00fcrff", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Sind sie fast noch einst so sch\u00f6n", "tokens": ["Sind", "sie", "fast", "noch", "einst", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Durch die Nachbarschaft der Schatten,", "tokens": ["Durch", "die", "Nach\u00b7bar\u00b7schaft", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Die das Licht noch mehr erh\u00f6hn,", "tokens": ["Die", "das", "Licht", "noch", "mehr", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Und es auf verschiednen Stellen", "tokens": ["Und", "es", "auf", "ver\u00b7schied\u00b7nen", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Durch den Gegensatz erhellen,", "tokens": ["Durch", "den", "Ge\u00b7gen\u00b7satz", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die sie, durch beweglichs Schertzen,", "tokens": ["Die", "sie", ",", "durch", "be\u00b7weg\u00b7lichs", "Schert\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Nicht so, wie es scheinet, schw\u00e4rtzen,", "tokens": ["Nicht", "so", ",", "wie", "es", "schei\u00b7net", ",", "schw\u00e4rt\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und die Farben nicht vertreiben;", "tokens": ["Und", "die", "Far\u00b7ben", "nicht", "ver\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Da sie w\u00fcrcklich alle bleiben.", "tokens": ["Da", "sie", "w\u00fcrck\u00b7lich", "al\u00b7le", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ferner kann man in der Hitze,", "tokens": ["Fer\u00b7ner", "kann", "man", "in", "der", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wie die Schatten s\u00fc\u00df und n\u00fctze,", "tokens": ["Wie", "die", "Schat\u00b7ten", "s\u00fc\u00df", "und", "n\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Da sie uns so lieblich k\u00fchlen,", "tokens": ["Da", "sie", "uns", "so", "lieb\u00b7lich", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wenn man es erweget, f\u00fchlen.", "tokens": ["Wenn", "man", "es", "er\u00b7we\u00b7get", ",", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}