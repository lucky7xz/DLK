{"textgrid.poem.28680": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4uffig sauffen Schwei\u00df und Blut armer Christen-Leute,", "tokens": ["H\u00e4uf\u00b7fig", "sauf\u00b7fen", "Schwei\u00df", "und", "Blut", "ar\u00b7mer", "Chris\u00b7ten\u00b7Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dr\u00fcber f\u00fchren guten Mut, ist gebr\u00e4uchlich heute;", "tokens": ["Dr\u00fc\u00b7ber", "f\u00fch\u00b7ren", "gu\u00b7ten", "Mut", ",", "ist", "ge\u00b7br\u00e4uch\u00b7lich", "heu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "ADJA", "NN", "$,", "VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Was mu\u00df drinnen wol f\u00fcr Lust, f\u00fcr Vergn\u00fcgen stecken?", "tokens": ["Was", "mu\u00df", "drin\u00b7nen", "wol", "f\u00fcr", "Lust", ",", "f\u00fcr", "Ver\u00b7gn\u00fc\u00b7gen", "ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "APPR", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ey! zum Truncke diese Kost pflegt wie Saltz zu schmecken;", "tokens": ["Ey", "!", "zum", "Trun\u00b7cke", "die\u00b7se", "Kost", "pflegt", "wie", "Saltz", "zu", "schme\u00b7cken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN"