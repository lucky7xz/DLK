{"textgrid.poem.41770": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir werden, Herz an Herz, Gedichte leben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir werden, Herz an Herz, Gedichte leben,", "tokens": ["Wir", "wer\u00b7den", ",", "Herz", "an", "Herz", ",", "Ge\u00b7dich\u00b7te", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "APPR", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn dieser Trennung herbe Qual bezwungen!", "tokens": ["Wenn", "die\u00b7ser", "Tren\u00b7nung", "her\u00b7be", "Qual", "be\u00b7zwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So hofften wir, da wir im Leid gerungen,", "tokens": ["So", "hoff\u00b7ten", "wir", ",", "da", "wir", "im", "Leid", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns nur begr\u00fc\u00dft, getrennt von Eisenst\u00e4ben.", "tokens": ["Uns", "nur", "be\u00b7gr\u00fc\u00dft", ",", "ge\u00b7trennt", "von", "Ei\u00b7sen\u00b7st\u00e4\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sonette leben! o die Reime weben,", "tokens": ["So\u00b7net\u00b7te", "le\u00b7