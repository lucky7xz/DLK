Ja verpaschen!", "tokens": ["Ja", "ver\u00b7pa\u00b7schen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.18": {"line.1": {"text": "Ab'r sie worden ihm wed'r abgejagt,", "tokens": ["Ab'r", "sie", "wor\u00b7den", "ihm", "we\u00b7d'r", "ab\u00b7ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAPP", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und Kunz mit sinen Gesellen", "tokens": ["Und", "Kunz", "mit", "si\u00b7nen", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Uf Gr\u00fcnhain in unsers Herrn Abts Gewalt", "tokens": ["Uf", "Gr\u00fcn\u00b7hain", "in", "un\u00b7sers", "Herrn", "Ab\u00b7ts", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "PPOSAT", "NN", "NE", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gebracht, und darnach uf Zwicka gestallt,", "tokens": ["Ge\u00b7bracht", ",", "und", "dar\u00b7nach", "uf", "Zwi\u00b7cka", "ge\u00b7stallt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PAV", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und musten sich lahn prellen,", "tokens": ["Und", "mus\u00b7ten", "sich", "lahn", "prel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja lahn prellen!", "tokens": ["Ja", "lahn", "prel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.19": {"line.1": {"text": "Darvor fiel ab gar mancher Kopf,", "tokens": ["Dar\u00b7vor", "fiel", "ab", "gar", "man\u00b7cher", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner, der gefangen,", "tokens": ["Und", "kei\u00b7ner", ",", "der", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kam aus der Haft ganzbeinicht davon,", "tokens": ["Kam", "aus", "der", "Haft", "ganz\u00b7bei\u00b7nicht", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwert, Rad, Zang'n, Strick, die war'n ihr Lohn,", "tokens": ["Schwert", ",", "Rad", ",", "Zang'n", ",", "Strick", ",", "die", "wa\u00b7r'n", "ihr", "Lohn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Man sah die R\u00fcmper hangen,", "tokens": ["Man", "sah", "die", "R\u00fcm\u00b7per", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja hangen!", "tokens": ["Ja", "han\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.20": {"line.1": {"text": "So gehts, wer wider die Obrigkeit", "tokens": ["So", "gehts", ",", "wer", "wi\u00b7der", "die", "Ob\u00b7rig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWS", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich unbesonnen emp\u00f6ret,", "tokens": ["Sich", "un\u00b7be\u00b7son\u00b7nen", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer's nicht meint, schau an Kunzen,", "tokens": ["Wer's", "nicht", "meint", ",", "schau", "an", "Kun\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Syn Kop thut z' Freiberg noch 'runter schmunzen,", "tokens": ["Syn", "Kop", "thut", "z'", "Frei\u00b7berg", "noch", "'r\u00b7un\u00b7ter", "schmun\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "NE", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und jed'rmann davon lehret,", "tokens": ["Und", "je\u00b7d'r\u00b7mann", "da\u00b7von", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Ja lehret!", "tokens": ["Ja", "leh\u00b7ret", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}