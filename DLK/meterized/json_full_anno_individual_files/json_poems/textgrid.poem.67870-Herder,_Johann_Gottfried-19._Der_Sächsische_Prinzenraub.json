{"textgrid.poem.67870": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "19. Der S\u00e4chsische Prinzenraub", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir woll'n ein Liedel heben an,", "tokens": ["Wir", "woll'n", "ein", "Lie\u00b7del", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sich hat angespunnen,", "tokens": ["Was", "sich", "hat", "an\u00b7ge\u00b7spun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie's im Plei\u00dfnerland gar schlecht war b'stallt,", "tokens": ["Wie's", "im", "Plei\u00df\u00b7ner\u00b7land", "gar", "schlecht", "war", "stall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als den jungen F\u00fcrst'n geschah Gewalt,", "tokens": ["Als", "den", "jun\u00b7gen", "F\u00fcr\u00b7st'n", "ge\u00b7schah", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Durch Kunzen von Kauffungen,", "tokens": ["Durch", "Kun\u00b7zen", "von", "Kauf\u00b7fun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja Kauffungen!", "tokens": ["Ja", "Kauf\u00b7fun\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der Adler hat uf'n Fels gebaut", "tokens": ["Der", "Ad\u00b7ler", "hat", "uf'n", "Fels", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "NE", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sch\u00f6nes Nest mit Jungen;", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Nest", "mit", "Jun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie er einst war g'flogen aus,", "tokens": ["Und", "wie", "er", "einst", "war", "g'\u00b7flo\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Holt ein Geyr die Jungen heraus,", "tokens": ["Holt", "ein", "Geyr", "die", "Jun\u00b7gen", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Drauf ward's Nest leer gefungen,", "tokens": ["Drauf", "ward's", "Nest", "leer", "ge\u00b7fun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "ADJD", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ja gefungen!", "tokens": ["Ja", "ge\u00b7fun\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wo der Geyer auf'm Dache sizt,", "tokens": ["Wo", "der", "Ge\u00b7yer", "auf'm", "Da\u00b7che", "sizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da deihen die K\u00fcchlein selten,", "tokens": ["Da", "dei\u00b7hen", "die", "K\u00fcch\u00b7lein", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "'s war Werl! ein seltsam Narrenspiel.", "tokens": ["'s", "war", "Werl", "!", "ein", "selt\u00b7sam", "Nar\u00b7ren\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$.", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Welch'r F\u00fcrst sein'n R\u00e4then getraut so viel,", "tokens": ["Welch'r", "F\u00fcrst", "sein'n", "R\u00e4\u00b7then", "ge\u00b7traut", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPOSAT", "NN", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mu\u00df oft der Herr selbst entgelten,", "tokens": ["Mu\u00df", "oft", "der", "Herr", "selbst", "ent\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ja entgelten!", "tokens": ["Ja", "ent\u00b7gel\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Altenborg, du feine Stadt,", "tokens": ["Al\u00b7ten\u00b7borg", ",", "du", "fei\u00b7ne", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich th\u00e4t er mit Untreu meinen,", "tokens": ["Dich", "th\u00e4t", "er", "mit", "Un\u00b7treu", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da in dir war'n all' Hofleut voll,", "tokens": ["Da", "in", "dir", "wa\u00b7r'n", "all'", "Hof\u00b7leut", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kam Kunz mit Leitern und Buben toll,", "tokens": ["Kam", "Kunz", "mit", "Lei\u00b7tern", "und", "Bu\u00b7ben", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und holt die F\u00fcrsten so kleine,", "tokens": ["Und", "holt", "die", "F\u00fcrs\u00b7ten", "so", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ja so kleine!", "tokens": ["Ja", "so", "klei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Was bla\u00dft dich, Kunz, f\u00fcr Unlust an,", "tokens": ["Was", "bla\u00dft", "dich", ",", "Kunz", ",", "f\u00fcr", "Un\u00b7lust", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NE", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du ins Schlo\u00df 'nein steigest?", "tokens": ["Da\u00df", "du", "ins", "Schlo\u00df", "'n\u00b7ein", "stei\u00b7gest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und stielst die zarten Herren heraus,", "tokens": ["Und", "stielst", "die", "zar\u00b7ten", "Her\u00b7ren", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als der Kurf\u00fcrst eben nit war zu Haus,", "tokens": ["Als", "der", "Kur\u00b7f\u00fcrst", "e\u00b7ben", "nit", "war", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "VAFIN", "APPR", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die zarten F\u00fcrstenzweige,", "tokens": ["Die", "zar\u00b7ten", "F\u00fcrs\u00b7ten\u00b7zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja F\u00fcrstenzweige!", "tokens": ["Ja", "F\u00fcrs\u00b7ten\u00b7zwei\u00b7ge", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Es war wohl als ein Wunderding,", "tokens": ["Es", "war", "wohl", "als", "ein", "Wun\u00b7der\u00b7ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sich das Land beweget.", "tokens": ["Wie", "sich", "das", "Land", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was da uf'n Strassen waren f\u00fcr Leut,", "tokens": ["Was", "da", "uf'n", "Stras\u00b7sen", "wa\u00b7ren", "f\u00fcr", "Leut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die den R\u00e4ubern folgten nach in Zeit,", "tokens": ["Die", "den", "R\u00e4u\u00b7bern", "folg\u00b7ten", "nach", "in", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "All's wibbelt, kribbelt, sich beweget,", "tokens": ["All's", "wib\u00b7belt", ",", "krib\u00b7belt", ",", "sich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ja beweget!", "tokens": ["Ja", "be\u00b7we\u00b7get", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Im Walde dort ward Kunz ertappt,", "tokens": ["Im", "Wal\u00b7de", "dort", "ward", "Kunz", "er\u00b7tappt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wollt he Beeren naschen,", "tokens": ["Da", "wollt", "he", "Bee\u00b7ren", "na\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r he in der Hast facken fortgeretten,", "tokens": ["W\u00e4r", "he", "in", "der", "Hast", "fa\u00b7cken", "fort\u00b7ge\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPR", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Da\u00df 'm die K\u00f6hler nit geleppischt hetten,", "tokens": ["Da\u00df", "'m", "die", "K\u00f6h\u00b7ler", "nit", "ge\u00b7lep\u00b7pischt", "het\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "H\u00e4tt he sie kunnt verpaschen,", "tokens": ["H\u00e4tt", "he", "sie", "kunnt", "ver\u00b7pa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ja verpaschen!", "tokens": ["Ja", "ver\u00b7pa\u00b7schen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Ab'r sie worden ihm wed'r abgejagt,", "tokens": ["Ab'r", "sie", "wor\u00b7den", "ihm", "we\u00b7d'r", "ab\u00b7ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAPP", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und Kunz mit sinen Gesellen", "tokens": ["Und", "Kunz", "mit", "si\u00b7nen", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Uf Gr\u00fcnhain in unsers Herrn Abts Gewalt", "tokens": ["Uf", "Gr\u00fcn\u00b7hain", "in", "un\u00b7sers", "Herrn", "Ab\u00b7ts", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "PPOSAT", "NN", "NE", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gebracht, und darnach uf Zwicka gestallt,", "tokens": ["Ge\u00b7bracht", ",", "und", "dar\u00b7nach", "uf", "Zwi\u00b7cka", "ge\u00b7stallt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PAV", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und musten sich lahn prellen,", "tokens": ["Und", "mus\u00b7ten", "sich", "lahn", "prel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja lahn prellen!", "tokens": ["Ja", "lahn", "prel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Darvor fiel ab gar mancher Kopf,", "tokens": ["Dar\u00b7vor", "fiel", "ab", "gar", "man\u00b7cher", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner, der gefangen,", "tokens": ["Und", "kei\u00b7ner", ",", "der", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kam aus der Haft ganzbeinicht davon,", "tokens": ["Kam", "aus", "der", "Haft", "ganz\u00b7bei\u00b7nicht", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwert, Rad, Zang'n, Strick, die war'n ihr Lohn,", "tokens": ["Schwert", ",", "Rad", ",", "Zang'n", ",", "Strick", ",", "die", "wa\u00b7r'n", "ihr", "Lohn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Man sah die R\u00fcmper hangen,", "tokens": ["Man", "sah", "die", "R\u00fcm\u00b7per", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja hangen!", "tokens": ["Ja", "han\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "So gehts, wer wider die Obrigkeit", "tokens": ["So", "gehts", ",", "wer", "wi\u00b7der", "die", "Ob\u00b7rig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWS", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich unbesonnen emp\u00f6ret,", "tokens": ["Sich", "un\u00b7be\u00b7son\u00b7nen", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer's nicht meint, schau an Kunzen,", "tokens": ["Wer's", "nicht", "meint", ",", "schau", "an", "Kun\u00b7zen", ","], "token_info": ["word", "word",