{"dta.poem.21216": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Mittag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mittag, begleitet von f\u00e4chelnden", "tokens": ["Der", "Mit\u00b7tag", ",", "be\u00b7glei\u00b7tet", "von", "f\u00e4\u00b7cheln\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "NN"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Stunden,\nEr\u00f6fnet sein F\u00fcllhorn, mit Blumen um-\nwunden,\nUnd gie\u00dft es auf alles verschwenderisch aus.\nDie allgemeinen wohlth\u00e4tigen Feste\nErfrischen des K\u00f6nigs gew\u00f6lbte Pall\u00e4ste,\nSo wie des Landmanns umschattetes Haus.", "tokens": ["Stun\u00b7den", ",", "Er\u00b7\u00f6f\u00b7net", "sein", "F\u00fcll\u00b7horn", ",", "mit", "Blu\u00b7men", "um", "wun\u00b7den", ",", "Und", "gie\u00dft", "es", "auf", "al\u00b7les", "ver\u00b7schwen\u00b7de\u00b7risch", "aus", ".", "Die", "all\u00b7ge\u00b7mei\u00b7nen", "wohlt\u00b7h\u00e4\u00b7ti\u00b7gen", "Fes\u00b7te", "Er\u00b7fri\u00b7schen", "des", "K\u00f6\u00b7nigs", "ge\u00b7w\u00f6lb\u00b7te", "Pal\u00b7l\u00e4s\u00b7te", ",", "So", "wie", "des", "Land\u00b7manns", "um\u00b7schat\u00b7te\u00b7tes", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN", "TRUNC", "ADJA", "$,", "KON", "VVFIN", "PPER", "APPR", "PIS", "ADJD", "PTKVZ", "$.", "ART", "ADJA", "ADJA", "NN", "NN", "ART", "NN", "ADJA", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+--+--+--+-+-+--+--+--+--+--+-+--+--+--+--+", "measure": "dactylic.penta.plus"}}, "stanza.2": {"line.1": {"text": "Empfange mich, ehrw\u00fcrdger Eichenwald!", "tokens": ["Emp\u00b7fan\u00b7ge", "mich", ",", "ehr\u00b7w\u00fcrd\u00b7ger", "Ei\u00b7chen\u00b7wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Jetzt, da wir ganz vom Mittagsstral ermatten,", "tokens": ["Jetzt", ",", "da", "wir", "ganz", "vom", "Mit\u00b7tags\u00b7stral", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sucht die Betrachtung gern den stillen Aufenthalt", "tokens": ["Sucht", "die", "Be\u00b7trach\u00b7tung", "gern", "den", "stil\u00b7len", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "In deinem k\u00fchlen Schatten.", "tokens": ["In", "dei\u00b7nem", "k\u00fch\u00b7len", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der laute Bach rollt murmelnd in das Thal;", "tokens": ["Der", "lau\u00b7te", "Bach", "rollt", "mur\u00b7melnd", "in", "das", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Westwind w\u00e4lzet sich im Wipfel hoher Buchen,", "tokens": ["Der", "West\u00b7wind", "w\u00e4l\u00b7zet", "sich", "im", "Wip\u00b7fel", "ho\u00b7her", "Bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da Bienen ohne Zahl", "tokens": ["Da", "Bie\u00b7nen", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Von Blumen ihren Raub mit stetem Summen", "tokens": ["Von", "Blu\u00b7men", "ih\u00b7ren", "Raub", "mit", "ste\u00b7tem", "Sum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Heerde lagert sich im Klee,", "tokens": ["Die", "Heer\u00b7de", "la\u00b7gert", "sich", "im", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Inde\u00df der Hirt von einer luftgen H\u00f6h", "tokens": ["In\u00b7de\u00df", "der", "Hirt", "von", "ei\u00b7ner", "luft\u00b7gen", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Soin Horn ert\u00f6nen l\u00e4\u00dft! und, durch den West", "tokens": ["Soin", "Horn", "er\u00b7t\u00f6\u00b7nen", "l\u00e4\u00dft", "!", "und", ",", "durch", "den", "West"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "VVINF", "VVFIN", "$.", "KON", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den s\u00fc\u00dfen Lobgesang zur B\u00e4che Murmeln mischet.", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Lob\u00b7ge\u00b7sang", "zur", "B\u00e4\u00b7che", "Mur\u00b7meln", "mi\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O wie begl\u00fcckt ist der, den nie sein Herz verdammt!", "tokens": ["O", "wie", "be\u00b7gl\u00fcckt", "ist", "der", ",", "den", "nie", "sein", "Herz", "ver\u00b7dammt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVPP", "VAFIN", "ART", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und den kein leerer Stolz, kein Durst nach Gold", "tokens": ["Und", "den", "kein", "lee\u00b7rer", "Stolz", ",", "kein", "Durst", "nach", "Gold"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "ADJA", "NN", "$,", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der, wenn die ganze Welt in Lastern um ihn", "tokens": ["Der", ",", "wenn", "die", "gan\u00b7ze", "Welt", "in", "Las\u00b7tern", "um", "ihn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Sich kalt erh\u00e4lt; nach keinen W\u00fcrden rennet;", "tokens": ["Sich", "kalt", "er\u00b7h\u00e4lt", ";", "nach", "kei\u00b7nen", "W\u00fcr\u00b7den", "ren\u00b7net", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$.", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und, fern vom L\u00e4rm der falschheitsvollen Stadt,", "tokens": ["Und", ",", "fern", "vom", "L\u00e4rm", "der", "falschheits\u00b7vol\u00b7len", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Frey unter Linden ruht, die er gepflanzet hat.", "tokens": ["Frey", "un\u00b7ter", "Lin\u00b7den", "ruht", ",", "die", "er", "ge\u00b7pflan\u00b7zet", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nie kan man gr\u00f6\u00dfre Wollust f\u00fchlen,", "tokens": ["Nie", "kan", "man", "gr\u00f6\u00df\u00b7re", "Wol\u00b7lust", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Indem uns tausend L\u00fcfte k\u00fchlen,", "tokens": ["In\u00b7dem", "uns", "tau\u00b7send", "L\u00fcf\u00b7te", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als wenn ein dankbar Herz den Herrn der", "tokens": ["Als", "wenn", "ein", "dank\u00b7bar", "Herz", "den", "Herrn", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJD", "NN", "ART", "NN", "ART"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Der K\u00f6nig, dem der Wein aus g\u00fcldnen", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "dem", "der", "Wein", "aus", "g\u00fcld\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hirt, der aus der Quelle trinket,", "tokens": ["Der", "Hirt", ",", "der", "aus", "der", "Quel\u00b7le", "trin\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vergesse nie den Geber, der ihn n\u00e4hrt.", "tokens": ["Ver\u00b7ges\u00b7se", "nie", "den", "Ge\u00b7ber", ",", "der", "ihn", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}