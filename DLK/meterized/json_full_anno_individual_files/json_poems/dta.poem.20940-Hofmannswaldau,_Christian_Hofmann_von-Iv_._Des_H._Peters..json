{"dta.poem.20940": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .  \n Des H. Peters.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist der Grabestein/ so diesen Stein beschlaust/", "tokens": ["Hier", "ist", "der", "Gra\u00b7be\u00b7stein", "/", "so", "die\u00b7sen", "Stein", "be\u00b7schlaust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "ADV", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nicht mehr wie zuvor die Thr\u00e4nen von sich geust.", "tokens": ["Der", "nicht", "mehr", "wie", "zu\u00b7vor", "die", "Thr\u00e4\u00b7nen", "von", "sich", "geust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "KOKOM", "ADV", "ART", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch sol der todte Stein nicht auf das neue weinen.", "tokens": ["Doch", "sol", "der", "tod\u00b7te", "Stein", "nicht", "auf", "das", "neu\u00b7e", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So jaget Weib und Haan von diesen beyden Stei-", "tokens": ["So", "ja\u00b7get", "Weib", "und", "Haan", "von", "die\u00b7sen", "bey\u00b7den", "Stei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NE", "APPR", "PDAT", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "nen.", "tokens": ["nen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}