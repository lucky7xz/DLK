{"dta.poem.5931": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die Figuren nun, die ohne Zahl", "tokens": ["Durch", "die", "Fi\u00b7gu\u00b7ren", "nun", ",", "die", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In C\u00f6rper eingetheilt und unersch\u00f6pfflich seyn,", "tokens": ["In", "C\u00f6r\u00b7per", "ein\u00b7ge\u00b7theilt", "und", "un\u00b7er\u00b7sch\u00f6pf\u00b7flich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stellt sonder End\u2019 und allzumal", "tokens": ["Stellt", "son\u00b7der", "End'", "und", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ungez\u00e4hltes Heer von Aenderung sich ein.", "tokens": ["Ein", "un\u00b7ge\u00b7z\u00e4hl\u00b7tes", "Heer", "von", "A\u00b7en\u00b7de\u00b7rung", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aus diesen Mischungen jedoch", "tokens": ["Aus", "die\u00b7sen", "Misc\u00b7hun\u00b7gen", "je\u00b7doch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entspringt ein andrer Nutzen noch.", "tokens": ["Ent\u00b7springt", "ein", "an\u00b7drer", "Nut\u00b7zen", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die unterschiedliche Versammelung,", "tokens": ["Die", "un\u00b7ter\u00b7schied\u00b7li\u00b7che", "Ver\u00b7sam\u00b7me\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und gantz verschiedene Vereinigung,", "tokens": ["Und", "gantz", "ver\u00b7schie\u00b7de\u00b7ne", "Ver\u00b7ei\u00b7ni\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Die immer auf das neu sich f\u00fcgt und lencket,", "tokens": ["Die", "im\u00b7mer", "auf", "das", "neu", "sich", "f\u00fcgt", "und", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "ADJD", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Hat an sich nichts, das eingeschr\u00e4ncket.", "tokens": ["Hat", "an", "sich", "nichts", ",", "das", "ein\u00b7ge\u00b7schr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "PIS", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nur blo\u00df von einer Art Materie allein", "tokens": ["Nur", "blo\u00df", "von", "ei\u00b7ner", "Art", "Ma\u00b7te\u00b7rie", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "ADV"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Sind alle Dinge, so verhanden,", "tokens": ["Sind", "al\u00b7le", "Din\u00b7ge", ",", "so", "ver\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Hervorgekommen und entstanden;", "tokens": ["Her\u00b7vor\u00b7ge\u00b7kom\u00b7men", "und", "ent\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die all\u2019 an Aenderung ohn Ende seyn.", "tokens": ["Die", "all'", "an", "A\u00b7en\u00b7de\u00b7rung", "ohn", "En\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}