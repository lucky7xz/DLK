{"dta.poem.5907": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Damit nun unser Geist m\u00f6g einig' H\u00fclffe sp\u00fcren,", "tokens": ["Da\u00b7mit", "nun", "un\u00b7ser", "Geist", "m\u00f6g", "ei\u00b7nig'", "H\u00fclf\u00b7fe", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So will er zur Gebuhrt des weiten ", "tokens": ["So", "will", "er", "zur", "Ge\u00b7buhrt", "des", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von allen Vorwurff frey, bem\u00fch\u2019 man sich zu dencken:", "tokens": ["Von", "al\u00b7len", "Vor\u00b7wurff", "frey", ",", "be\u00b7m\u00fch'", "man", "sich", "zu", "den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "VVFIN", "PIS", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df die Materie von GOTT hervorgebracht,", "tokens": ["Da\u00df", "die", "Ma\u00b7te\u00b7rie", "von", "GoTT", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}