{"textgrid.poem.41467": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Dritte Erz\u00e4hlung", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Frauenlist, dem Eigensinn getreu,", "tokens": ["Der", "Frau\u00b7en\u00b7list", ",", "dem", "Ei\u00b7gen\u00b7sinn", "ge\u00b7treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Flieht Adelheid so gar der Ehe Schatten;", "tokens": ["Flieht", "A\u00b7del\u00b7heid", "so", "gar", "der", "E\u00b7he", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch liefert ihr und ihrer Glei\u00dfnerei", "tokens": ["Doch", "lie\u00b7fert", "ihr", "und", "ih\u00b7rer", "Glei\u00df\u00b7ne\u00b7rei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der zw\u00f6lfte Tag den sehnsuchtvollen Gatten.", "tokens": ["Der", "zw\u00f6lf\u00b7te", "Tag", "den", "sehn\u00b7sucht\u00b7vol\u00b7len", "Gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Fl\u00fcchtling selbst, den die Gewalt verbannt,", "tokens": ["Der", "Fl\u00fccht\u00b7ling", "selbst", ",", "den", "die", "Ge\u00b7walt", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erh\u00e4lt zwar oft der R\u00fcckkunft Recht und Gl\u00fccke;", "tokens": ["Er\u00b7h\u00e4lt", "zwar", "oft", "der", "R\u00fcck\u00b7kunft", "Recht", "und", "Gl\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch sieht er dann sein offnes Vaterland", "tokens": ["Doch", "sieht", "er", "dann", "sein", "off\u00b7nes", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit mindrer Lust, als Henrich ihre Blicke.", "tokens": ["Mit", "mind\u00b7rer", "Lust", ",", "als", "Hen\u00b7rich", "ih\u00b7re", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es kann die Welt, tr\u00e4gt er romanisch vor,", "tokens": ["Es", "kann", "die", "Welt", ",", "tr\u00e4gt", "er", "ro\u00b7ma\u00b7nisch", "vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Der Sonne nicht, der Sch\u00f6nen nicht entbehren:", "tokens": ["Der", "Son\u00b7ne", "nicht", ",", "der", "Sch\u00f6\u00b7nen", "nicht", "ent\u00b7beh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verh\u00fcllst denn du in einen Trauerflor,", "tokens": ["Ver\u00b7h\u00fcllst", "denn", "du", "in", "ei\u00b7nen", "Trau\u00b7er\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was w\u00fcrdig ist, sich Menschen aufzukl\u00e4ren?", "tokens": ["Was", "w\u00fcr\u00b7dig", "ist", ",", "sich", "Men\u00b7schen", "auf\u00b7zu\u00b7kl\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PRF", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das war galant, schlau wie ein Lobgedicht,", "tokens": ["Das", "war", "ga\u00b7lant", ",", "schlau", "wie", "ein", "Lob\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und f\u00fchrte bald zu ausgeschm\u00fcckten S\u00e4tzen:", "tokens": ["Und", "f\u00fchr\u00b7te", "bald", "zu", "aus\u00b7ge\u00b7schm\u00fcck\u00b7ten", "S\u00e4t\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Allein umsonst. Ihn irrt, ihm widerspricht", "tokens": ["Al\u00b7lein", "um\u00b7sonst", ".", "Ihn", "irrt", ",", "ihm", "wi\u00b7der\u00b7spricht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Z\