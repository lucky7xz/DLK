{"textgrid.poem.66628": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Morgenwanderung", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um f\u00fcnf Uhr fr\u00fch schritt ich dem Berg entgegen,", "tokens": ["Um", "f\u00fcnf", "Uhr", "fr\u00fch", "schritt", "ich", "dem", "Berg", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gaslaternen brannten Sonnenlicht,", "tokens": ["Die", "Gas\u00b7la\u00b7ter\u00b7nen", "brann\u00b7ten", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Osten streute seinen blanken Segen", "tokens": ["Der", "Os\u00b7ten", "streu\u00b7te", "sei\u00b7nen", "blan\u00b7ken", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den M\u00e4dchen an den Brunnen ins Gesicht.", "tokens": ["Den", "M\u00e4d\u00b7chen", "an", "den", "Brun\u00b7nen", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Trabe kamen nach der Stadt gefahren", "tokens": ["Im", "Tra\u00b7be", "ka\u00b7men", "nach", "der", "Stadt", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Milchleute, die schon nachts zu Wege waren.", "tokens": ["Milch\u00b7leu\u00b7te", ",", "die", "schon", "nachts", "zu", "We\u00b7ge", "wa\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da drau\u00dfen, wo sich's l\u00e4ndlich bald verlor,", "tokens": ["Da", "drau\u00b7\u00dfen", ",", "wo", "sich's", "l\u00e4nd\u00b7lich", "bald", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Stand Bursch und B\u00e4urin schon am Gartentor.", "tokens": ["Stand", "Bursch", "und", "B\u00e4u\u00b7rin", "schon", "am", "Gar\u00b7ten\u00b7tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht lange bin ich einsam fortgewandelt,", "tokens": ["Nicht", "lan\u00b7ge", "bin", "ich", "ein\u00b7sam", "fort\u00b7ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wiewohl der Pfad gleich oben waldw\u00e4rts bog.", "tokens": ["Wie\u00b7wohl", "der", "Pfad", "gleich", "o\u00b7ben", "wald\u00b7w\u00e4rts", "bog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "'s war Feiertag. Just Arm in Arm gebandelt", "tokens": ["'s", "war", "Fei\u00b7er\u00b7tag", ".", "Just", "Arm", "in", "Arm", "ge\u00b7ban\u00b7delt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$.", "NN", "NN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ein M\u00e4dchenpaar an mir vor\u00fcberzog.", "tokens": ["Ein", "M\u00e4d\u00b7chen\u00b7paar", "an", "mir", "vor\u00b7\u00fc\u00b7ber\u00b7zog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie trugen Riesenstr\u00e4u\u00dfe in den H\u00e4nden,", "tokens": ["Sie", "tru\u00b7gen", "Rie\u00b7sen\u00b7str\u00e4u\u00b7\u00dfe", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gepfl\u00fcckt an Wiesenbach und Talschluchtw\u00e4nden,", "tokens": ["Ge\u00b7pfl\u00fcckt", "an", "Wie\u00b7sen\u00b7bach", "und", "Tal\u00b7schlucht\u00b7w\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und sangen Lieder in die helle Flur", "tokens": ["Und", "san\u00b7gen", "Lie\u00b7der", "in", "die", "hel\u00b7le", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von Lieb' und Lust, von Heimat und Natur.", "tokens": ["Von", "Lieb'", "und", "Lust", ",", "von", "Hei\u00b7mat", "und", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbgr\u00fc\u00df Gott!\u00ab Sie schieden links, ich rechts zum Kamme,", "tokens": ["\u00bb", "gr\u00fc\u00df", "Gott", "!", "\u00ab", "Sie", "schie\u00b7den", "links", ",", "ich", "rechts", "zum", "Kam\u00b7me", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "$,", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ihrem Duo lauscht' ich noch von fern.", "tokens": ["Doch", "ih\u00b7rem", "Duo", "lauscht'", "ich", "noch", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das ist ein lieber Zug am Schweizerstamme,", "tokens": ["Das", "ist", "ein", "lie\u00b7ber", "Zug", "am", "Schwei\u00b7zer\u00b7stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Lust zum Lied. Sie haben's \u00bbgr\u00fcsli\u00ab gern.", "tokens": ["Die", "Lust", "zum", "Lied", ".", "Sie", "ha\u00b7ben's", "\u00bb", "gr\u00fcs\u00b7li", "\u00ab", "gern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "PPER", "VAFIN", "$(", "FM.la", "$(", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt war's verweht. Nun sang es aus den B\u00fcschen,", "tokens": ["Jetzt", "wa\u00b7r's", "ver\u00b7weht", ".", "Nun", "sang", "es", "aus", "den", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er rann noch links vom Wege ziemlich flach,", "tokens": ["Er", "rann", "noch", "links", "vom", "We\u00b7ge", "ziem\u00b7lich", "flach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ger\u00e4uschvoll scho\u00df er rechts, ein heftiger Bach.", "tokens": ["Ge\u00b7r\u00e4uschvoll", "scho\u00df", "er", "rechts", ",", "ein", "hef\u00b7ti\u00b7ger", "Bach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Kerzentannen spielten mit den Strahlen", "tokens": ["Die", "Ker\u00b7zen\u00b7tan\u00b7nen", "spiel\u00b7ten", "mit", "den", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der wei\u00dfen Sonne, die im Tau zersprang", "tokens": ["Der", "wei\u00b7\u00dfen", "Son\u00b7ne", ",", "die", "im", "Tau", "zer\u00b7sprang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Farben, wie sie keine K\u00fcnstler malen,", "tokens": ["Und", "Far\u00b7ben", ",", "wie", "sie", "kei\u00b7ne", "K\u00fcnst\u00b7ler", "ma\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Perlenketten um die Erlen schlang.", "tokens": ["In", "Per\u00b7len\u00b7ket\u00b7ten", "um", "die", "Er\u00b7len", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mutwillige Fl\u00fcgler schwirrten walddurchschweifend,", "tokens": ["Mut\u00b7wil\u00b7li\u00b7ge", "Fl\u00fcg\u00b7ler", "schwirr\u00b7ten", "wald\u00b7durch\u00b7schwei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "++--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Bald mir den Kopf, bald tief die Gr\u00e4ser streifend,", "tokens": ["Bald", "mir", "den", "Kopf", ",", "bald", "tief", "die", "Gr\u00e4\u00b7ser", "strei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf einmal aus dem Fichtendunkel schrie", "tokens": ["Auf", "ein\u00b7mal", "aus", "dem", "Fich\u00b7ten\u00b7dun\u00b7kel", "schrie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Kuckuck seine Traummonotonie.", "tokens": ["Der", "Ku\u00b7ckuck", "sei\u00b7ne", "Traum\u00b7mo\u00b7no\u00b7to\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Am letzten Abend hatt' es fest geregnet,", "tokens": ["Am", "letz\u00b7ten", "A\u00b7bend", "hatt'", "es", "fest", "ge\u00b7reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Dickichtwegen wurd' ich t\u00fcchtig na\u00df.", "tokens": ["Auf", "Di\u00b7ckicht\u00b7we\u00b7gen", "wurd'", "ich", "t\u00fcch\u00b7tig", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch hab' ich die Betr\u00e4uflung gern gesegnet,", "tokens": ["Doch", "hab'", "ich", "die", "Be\u00b7tr\u00e4uf\u00b7lung", "gern", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies Frischgef\u00fchl \u2013 o welche Wonne das!", "tokens": ["Dies", "Frischge\u00b7f\u00fchl", "\u2013", "o", "wel\u00b7che", "Won\u00b7ne", "das", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "FM", "PWAT", "NN", "ART", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "So in dem fetten Humus einzusinken,", "tokens": ["So", "in", "dem", "fet\u00b7ten", "Hu\u00b7mus", "ein\u00b7zu\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Fruchtbarkeit mit Sohlen aufzutrinken,", "tokens": ["Die", "Frucht\u00b7bar\u00b7keit", "mit", "Soh\u00b7len", "auf\u00b7zu\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu schl\u00fcrfen diesen feuchten Sonnenseim \u2013", "tokens": ["Zu", "schl\u00fcr\u00b7fen", "die\u00b7sen", "feuch\u00b7ten", "Son\u00b7nen\u00b7seim", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Hochgenu\u00df wie kein Poetenreim.", "tokens": ["Ein", "Hoch\u00b7ge\u00b7nu\u00df", "wie", "kein", "Poe\u00b7ten\u00b7reim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Vorm Adlisberger Forsthaus hielt ich Atzung", "tokens": ["Vorm", "Ad\u00b7lis\u00b7ber\u00b7ger", "Forst\u00b7haus", "hielt", "ich", "At\u00b7zung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "NN"], "meter": "++---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mit frischer Milch und saftigem Bauernbrot.", "tokens": ["Mit", "fri\u00b7scher", "Milch", "und", "saf\u00b7ti\u00b7gem", "Bau\u00b7ern\u00b7brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gering war noch der G\u00e4ste Fr\u00fchbesatzung;", "tokens": ["Ge\u00b7ring", "war", "noch", "der", "G\u00e4s\u00b7te", "Fr\u00fch\u00b7be\u00b7sat\u00b7zung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein st\u00e4dtisch Meisterlein mir \u00bbWohlsein\u00ab bot.", "tokens": ["Ein", "st\u00e4d\u00b7tisch", "Meis\u00b7ter\u00b7lein", "mir", "\u00bb", "Wohl\u00b7sein", "\u00ab", "bot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "$(", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein runzlig H\u00e4ufchen Arbeit, krumm von Sorgen,", "tokens": ["Ein", "runz\u00b7lig", "H\u00e4uf\u00b7chen", "Ar\u00b7beit", ",", "krumm", "von", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur froh f\u00fcr jetzt: \u00bbGell Sie? Ein pr\u00e4chtiger Morgen!", "tokens": ["Nur", "froh", "f\u00fcr", "jetzt", ":", "\u00bb", "Gell", "Sie", "?", "Ein", "pr\u00e4ch\u00b7ti\u00b7ger", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADV", "$.", "$(", "NN", "PPER", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wenn man das ganze Jahr sich plagt und m\u00fcht ...", "tokens": ["Wenn", "man", "das", "gan\u00b7ze", "Jahr", "sich", "plagt", "und", "m\u00fcht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "PRF", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "'s ist eine Freude, wie das w\u00e4chst und bl\u00fcht!\u00ab", "tokens": ["'s", "ist", "ei\u00b7ne", "Freu\u00b7de", ",", "wie", "das", "w\u00e4c