{"textgrid.poem.56117": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und fast ein M\u00e4dchen wars und ging hervor", "tokens": ["Und", "fast", "ein", "M\u00e4d\u00b7chen", "wars", "und", "ging", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus diesem einigen Gl\u00fcck von Sang und Leier", "tokens": ["aus", "die\u00b7sem", "ei\u00b7ni\u00b7gen", "Gl\u00fcck", "von", "Sang", "und", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und gl\u00e4nzte klar durch ihre Fr\u00fchlingsschleier", "tokens": ["und", "gl\u00e4nz\u00b7te", "klar", "durch", "ih\u00b7re", "Fr\u00fch\u00b7lings\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und machte sich ein Bett in meinem Ohr.", "tokens": ["und", "mach\u00b7te", "sich", "ein", "Bett", "in", "mei\u00b7nem", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und schlief in mir. Und alles war ihr Schlaf.", "tokens": ["Und", "schlief", "in", "mir", ".", "Und", "al\u00b7les", "war", "ihr", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$.", "KON", "PIS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die B\u00e4ume, die ich je bewundert, diese", "tokens": ["Die", "B\u00e4u\u00b7me", ",", "die", "ich", "je", "be\u00b7wun\u00b7dert", ",", "die\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "f\u00fchlbare Ferne, die gef\u00fchlte Wiese", "tokens": ["f\u00fchl\u00b7ba\u00b7re", "Fer\u00b7ne", ",", "die", "ge\u00b7f\u00fchl\u00b7te", "Wie\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "und jedes Staunen, das mich selbst betraf.", "tokens": ["und", "je\u00b7des", "Stau\u00b7nen", ",", "das", "mich", "selbst", "be\u00b7traf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie schlief die Welt. Singender Gott, wie hast", "tokens": ["Sie", "schlief", "die", "Welt", ".", "Sin\u00b7gen\u00b7der", "Gott", ",", "wie", "hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NN", "NN", "$,", "PWAV", "VAFIN"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "du sie vollendet, da\u00df sie nicht begehrte,", "tokens": ["du", "sie", "voll\u00b7en\u00b7det", ",", "da\u00df", "sie", "nicht", "be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "$,", "KOUS", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "erst wach zu sein? Sieh, sie erstand und schlief.", "tokens": ["erst", "wach", "zu", "sein", "?", "Sieh", ",", "sie", "er\u00b7stand", "und", "schlief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "$.", "NE", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo ist ihr Tod? O, wirst du dies Motiv", "tokens": ["Wo", "ist", "ihr", "Tod", "?", "O", ",", "wirst", "du", "dies", "Mo\u00b7tiv"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$.", "NE", "$,", "VAFIN", "PPER", "PDS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "erfinden noch, eh sich dein Lied verzehrte? \u2013", "tokens": ["er\u00b7fin\u00b7den", "noch", ",", "eh", "sich", "dein", "Lied", "ver\u00b7zehr\u00b7te", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wo sinkt sie hin aus mir? ... Ein M\u00e4dchen fast ...", "tokens": ["Wo", "sinkt", "sie", "hin", "aus", "mir", "?", "...", "Ein", "M\u00e4d\u00b7chen", "fast", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$.", "$(", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und fast ein M\u00e4dchen wars und ging hervor", "tokens": ["Und", "fast"