{"dta.poem.259": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An das liebens-w\u00fcrdige Frauen-Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Sch\u00f6nen freuet euch/", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", "freu\u00b7et", "euch", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr nach Kron und Palmen tracht/", "tokens": ["Die", "ihr", "nach", "Kron", "und", "Pal\u00b7men", "tracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat der Liebe-Reich", "tokens": ["Es", "hat", "der", "Lie\u00b7be\u00b7Reich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Sieg davon gebracht.", "tokens": ["Den", "Sieg", "da\u00b7von", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df nun die Welt", "tokens": ["Da\u00df", "nun", "die", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zu Fu\u00df euch f\u00e4llt", "tokens": ["Zu", "Fu\u00df", "euch", "f\u00e4llt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und spricht:", "tokens": ["Und", "spricht", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Man mu\u00df allein", "tokens": ["Man", "mu\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Euch Dienstbahr seyn/", "tokens": ["Euch", "Dienst\u00b7bahr", "seyn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAINF", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und eurem Wunder-Licht.", "tokens": ["Und", "eu\u00b7rem", "Wun\u00b7der\u00b7Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Facht eure Flammen auf/", "tokens": ["Facht", "eu\u00b7re", "Flam\u00b7men", "auf", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Augen la\u00dft voll Feuer seyn/", "tokens": ["Die", "Au\u00b7gen", "la\u00dft", "voll", "Feu\u00b7er", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schickt Blick\u2019 im vollen Lauff", "tokens": ["Schickt", "Blick'", "im", "vol\u00b7len", "Lauff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf unser Geister ein:", "tokens": ["Auf", "un\u00b7ser", "Geis\u00b7ter", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Br\u00fcste Pracht", "tokens": ["Der", "Br\u00fcs\u00b7te", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sey blo\u00df gemacht", "tokens": ["Sey", "blo\u00df", "ge\u00b7macht"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Anjetzt:", "tokens": ["An\u00b7jetzt", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Da\u00df Labsahl sey/", "tokens": ["Da\u00df", "Lab\u00b7sahl", "sey", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Das k\u00fchle Zwey", "tokens": ["Das", "k\u00fch\u00b7le", "Zwey"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Wenn eur Gesichte blitzt.", "tokens": ["Wenn", "eur", "Ge\u00b7sich\u00b7te", "blitzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00dft eure Artigkeit", "tokens": ["La\u00dft", "eu\u00b7re", "Ar\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In sch\u00f6nster Zier der Mienen sehn.", "tokens": ["In", "sch\u00f6ns\u00b7ter", "Zier", "der", "Mie\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihr mit rechte seyd", "tokens": ["Da\u00df", "ihr", "mit", "rech\u00b7te", "seyd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Galant und wunder-sch\u00f6n.", "tokens": ["Ga\u00b7lant", "und", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nehmt Hertzen ein", "tokens": ["Nehmt", "Hert\u00b7zen", "ein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da\u00df sie euch seyn", "tokens": ["Da\u00df", "sie", "euch", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Verpf\u00e4ndt:", "tokens": ["Ver\u00b7pf\u00e4ndt", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Denn eure Pracht", "tokens": ["Denn", "eu\u00b7re", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Gefangne macht", "tokens": ["Ge\u00b7fang\u00b7ne", "macht"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Wo man die Augen wendt.", "tokens": ["Wo", "man", "die", "Au\u00b7gen", "wendt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}