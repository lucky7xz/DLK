{"textgrid.poem.53398": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[ich sang vor vielen Jahren]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sang vor vielen Jahren:", "tokens": ["Ich", "sang", "vor", "vie\u00b7len", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df sterben, la\u00df hinfahren", "tokens": ["La\u00df", "ster\u00b7ben", ",", "la\u00df", "hin\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "VVIMP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was zeitig sterben kan,", "tokens": ["Was", "zei\u00b7tig", "ster\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Zeit ist so beschaffen,", "tokens": ["Die", "Zeit", "ist", "so", "be\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df die, so selig schlaffen,", "tokens": ["Da\u00df", "die", ",", "so", "se\u00b7lig", "schlaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erst wol sind umb und an.", "tokens": ["Erst", "wol", "sind", "umb", "und", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt klagen meine Seiten", "tokens": ["Jetzt", "kla\u00b7gen", "mei\u00b7ne", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gerechter als vorzeiten,", "tokens": ["Ge\u00b7rech\u00b7ter", "als", "vor\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun uns die Fluth bezwingt,", "tokens": ["Nun", "uns", "die", "Fluth", "be\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd an die Seele steiget,", "tokens": ["Vnd", "an", "die", "See\u00b7le", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun hie sich alles neiget", "tokens": ["Nun", "hie", "sich", "al\u00b7les", "nei\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PRF", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd mit dem Tode ringt.", "tokens": ["Vnd", "mit", "dem", "To\u00b7de", "ringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Sing' ich von jung erkalten?", "tokens": ["Sing'", "ich", "von", "jung", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man m\u00f6chte selig halten", "tokens": ["Man", "m\u00f6ch\u00b7te", "se\u00b7lig", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein denselben schier,", "tokens": ["Al\u00b7lein", "den\u00b7sel\u00b7ben", "schier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie Posidippus schreibet,", "tokens": ["Wie", "Po\u00b7si\u00b7dip\u00b7pus", "schrei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der ungesehen bleibet", "tokens": ["Der", "un\u00b7ge\u00b7se\u00b7hen", "blei\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von aller Sonnen Zier.", "tokens": ["Von", "al\u00b7ler", "Son\u00b7nen", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt m\u00fcssen wir bekennen,", "tokens": ["Jetzt", "m\u00fcs\u00b7sen", "wir", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie selig die zu nennen", "tokens": ["Wie", "se\u00b7lig", "die", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "PTKZU", "VVINF"], "meter": "-------", "measure": "unknown.measure.zero"}, "line.3": {"text": "So hie der Seuchen zwangk", "tokens": ["So", "hie", "der", "Seu\u00b7chen", "zwangk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gebracht in ihre Kammer,", "tokens": ["Ge\u00b7bracht", "in", "ih\u00b7re", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie h\u00f6ren keinen Jammer", "tokens": ["Sie", "h\u00f6\u00b7ren", "kei\u00b7nen", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und keiner Waffen Klangk.", "tokens": ["Und", "kei\u00b7ner", "Waf\u00b7fen", "Klangk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sagte fast die Seinen,", "tokens": ["Ich", "sag\u00b7te", "fast", "die", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PPOSS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sterben, itzt beweinen", "tokens": ["So", "ster\u00b7ben", ",", "itzt", "be\u00b7wei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sey Unbesonnenheit,", "tokens": ["Sey", "Un\u00b7be\u00b7son\u00b7nen\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich mu\u00df f\u00fcr Freuden lachen", "tokens": ["Ich", "mu\u00df", "f\u00fcr", "Freu\u00b7den", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Seh' ich ein Grab wo machen,", "tokens": ["Seh'", "ich", "ein", "Grab", "wo", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PWAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So b\u00f6s' ist diese Zeit.", "tokens": ["So", "b\u00f6s'", "ist", "die\u00b7se", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir bawen an den W\u00e4llen,", "tokens": ["Wir", "ba\u00b7wen", "an", "den", "W\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Darauff wir Krieger stellen,", "tokens": ["Dar\u00b7auff", "wir", "Krie\u00b7ger", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Feinde Wiederstand,", "tokens": ["Der", "Fein\u00b7de", "Wie\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer suchet nicht zu wachen", "tokens": ["Wer", "su\u00b7chet", "nicht", "zu", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd schickt die besten Sachen", "tokens": ["Vnd", "schickt", "die", "bes\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo in ein sicher Land?", "tokens": ["Wo", "in", "ein", "si\u00b7cher", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Wann ich ein' Insul w\u00fcste", "tokens": ["Wann", "ich", "ein'", "In\u00b7sul", "w\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In die kein Feind hin m\u00fcste,", "tokens": ["In", "die", "kein", "Feind", "hin", "m\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da Ruh w\u00e4r' \u00fcberall,", "tokens": ["Da", "Ruh", "w\u00e4r'", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie gern w\u00fcrd ich ohn weinen", "tokens": ["Wie", "gern", "w\u00fcrd", "ich", "ohn", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPR", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hie lassen alle Meinen,", "tokens": ["Hie", "las\u00b7sen", "al\u00b7le", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu fliehen f\u00fcr dem Fall.", "tokens": ["Zu", "flie\u00b7hen", "f\u00fcr", "dem", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Insul ist der Himmel,", "tokens": ["Die", "In\u00b7sul", "ist", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dahin kein Welt-get\u00fcmmel", "tokens": ["Da\u00b7hin", "kein", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Krieges-wetter reicht,", "tokens": ["Kein", "Krie\u00b7ge\u00b7swet\u00b7ter", "reicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dahin sich nichts als Leben", "tokens": ["Da\u00b7hin", "sich", "nichts", "als", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PRF", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd Vnschuld kan begeben,", "tokens": ["Vnd", "Vn\u00b7schuld", "kan", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da aller Tod verbleicht,", "tokens": ["Da", "al\u00b7ler", "Tod", "ver\u00b7bleicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da aller Streit sich endet,", "tokens": ["Da", "al\u00b7ler", "Streit", "sich", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Daselbst ist angelendet,", "tokens": ["Da\u00b7selbst", "ist", "an\u00b7ge\u00b7len\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun, Fraw, auch ewer Sohn,", "tokens": ["Nun", ",", "Fraw", ",", "auch", "e\u00b7wer", "Sohn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist zu der Ruh gekommen", "tokens": ["Ist", "zu", "der", "Ruh", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd tr\u00e4gt sampt allen Frommen", "tokens": ["Vnd", "tr\u00e4gt", "sampt", "al\u00b7len", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Glaubens wehrten Lohn.", "tokens": ["Des", "Glau\u00b7bens", "wehr\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wohnt in des Friedens-H\u00fctten,", "tokens": ["Wohnt", "in", "des", "Frie\u00b7dens\u00b7H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo keine Feinde w\u00fcten,", "tokens": ["Wo", "kei\u00b7ne", "Fein\u00b7de", "w\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Vater beygef\u00fcgt,", "tokens": ["Dem", "Va\u00b7ter", "bey\u00b7ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der in den guten Jahren", "tokens": ["Der", "in", "den", "gu\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bereit dahin gefahren", "tokens": ["Be\u00b7reit", "da\u00b7hin", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "PAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd unbesorget liegt.", "tokens": ["Vnd", "un\u00b7be\u00b7sor\u00b7get", "liegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gebt ma\u00df, Fraw, ewrem Leiden,", "tokens": ["Gebt", "ma\u00df", ",", "Fraw", ",", "ew\u00b7rem", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mi\u00dfg\u00f6nnt ihm nicht die Freuden:", "tokens": ["Mi\u00df\u00b7g\u00f6nnt", "ihm", "nicht", "die", "Freu\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd f\u00e4hrt er jung dahin,", "tokens": ["Vnd", "f\u00e4hrt", "er", "jung", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt-Sirenen werden", "tokens": ["Die", "Welt\u00b7Si\u00b7re\u00b7nen", "wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht mit dem Gifft der Erden", "tokens": ["Nicht", "mit", "dem", "Gifft", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Entz\u00fcnden seinen Sinn.", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "sei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Das Vngl\u00fcck dieser Zeiten,", "tokens": ["Das", "Vn\u00b7gl\u00fcck", "die\u00b7ser", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Damit wir itzt noch streiten,", "tokens": ["Da\u00b7mit", "wir", "itzt", "noch", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird nimmer ihn bestehn,", "tokens": ["Wird", "nim\u00b7mer", "ihn", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er darff nicht \u00e4ngstig schawen,", "tokens": ["Er", "darff", "nicht", "\u00e4ngs\u00b7tig", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie wir f\u00fcr Furcht und Grawen", "tokens": ["Wie", "wir", "f\u00fcr", "Furcht", "und", "Gra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur nicht gar untergehn.", "tokens": ["Nur", "nicht", "gar", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}