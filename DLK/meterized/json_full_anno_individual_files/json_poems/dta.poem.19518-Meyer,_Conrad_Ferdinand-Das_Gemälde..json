{"dta.poem.19518": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Gem\u00e4lde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb brennt der Schenke Kerzenlicht,", "tokens": ["Tr\u00fcb", "brennt", "der", "Schen\u00b7ke", "Ker\u00b7zen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wirthin junges Angesicht,", "tokens": ["Der", "Wirt\u00b7hin", "jun\u00b7ges", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erm\u00fcdet, schlummertrunken,", "tokens": ["Er\u00b7m\u00fc\u00b7det", ",", "schlum\u00b7mer\u00b7trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nickt auf die Brust gesunken,", "tokens": ["Nickt", "auf", "die", "Brust", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn schon ist Mitternacht vorbei.", "tokens": ["Denn", "schon", "ist", "Mit\u00b7ter\u00b7nacht", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Schiefertische spielen Zwei,", "tokens": ["Am", "Schie\u00b7fer\u00b7ti\u00b7sche", "spie\u00b7len", "Zwei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "CARD", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Die wei\u00dfen W\u00fcrfel schallen,", "tokens": ["Die", "wei\u00b7\u00dfen", "W\u00fcr\u00b7fel", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schlecht ist der Wurf gefallen \u2014", "tokens": ["Schlecht", "ist", "der", "Wurf", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein junges wildes Augenpaar", "tokens": ["Ein", "jun\u00b7ges", "wil\u00b7des", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Droht aus verworrnem Lockenhaar:", "tokens": ["Droht", "aus", "ver\u00b7worr\u00b7nem", "Lo\u00b7cken\u00b7haar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u201edas war mein letztes Silberst\u00fcck!", "tokens": ["\u201e", "das", "war", "mein", "letz\u00b7tes", "Sil\u00b7ber\u00b7st\u00fcck", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Doch wenden mu\u00df sich jetzt das Gl\u00fcck!", "tokens": ["Doch", "wen\u00b7den", "mu\u00df", "sich", "jetzt", "das", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Du, Alter, mu\u00dft mir borgen!", "tokens": ["Du", ",", "Al\u00b7ter", ",", "mu\u00dft", "mir", "bor\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wir spielen bis zum Morgen!\u201c", "tokens": ["Wir", "spie\u00b7len", "bis", "zum", "Mor\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Mit gr\u00fcnen Katzenaugen blitzt", "tokens": ["Mit", "gr\u00fc\u00b7nen", "Kat\u00b7zen\u00b7au\u00b7gen", "blitzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Alte, der im Dunkel sitzt:", "tokens": ["Der", "Al\u00b7te", ",", "der", "im", "Dun\u00b7kel", "sitzt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u201ela\u00df dich zu Bette legen,", "tokens": ["\u201e", "la\u00df", "dich", "zu", "Bet\u00b7te", "le\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.18": {"text": "Die Mutter spricht den Segen!\u201c", "tokens": ["Die", "Mut\u00b7ter", "spricht", "den", "Se\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Des Jungen Faust zerdr\u00fcckt das Glas", "tokens": ["Des", "Jun\u00b7gen", "Faust", "zer\u00b7dr\u00fcckt", "das", "Glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mit einem Fluch \u2014 \u201eKind, wei\u00dft du was?", "tokens": ["Mit", "ei\u00b7nem", "Fluch", "\u201e", "Kind", ",", "wei\u00dft", "du", "was", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "$(", "NN", "$,", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u201eein Schl\u00f6\u00dflein steht auf gr\u00fcnem Plan\u201c", "tokens": ["\u201e", "ein", "Schl\u00f6\u00df\u00b7lein", "steht", "auf", "gr\u00fc\u00b7nem", "Plan", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So f\u00e4ngt ein altes M\u00e4rchen an.", "tokens": ["So", "f\u00e4ngt", "ein", "al\u00b7tes", "M\u00e4r\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ich meine das im Walde,", "tokens": ["Ich", "mei\u00b7ne", "das", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Hier oben an der Halde.", "tokens": ["Hier", "o\u00b7ben", "an", "der", "Hal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Verschlossen sind die Fenster,", "tokens": ["Ver\u00b7schlos\u00b7sen", "sind", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Drin hausen nur Gespenster", "tokens": ["Drin", "hau\u00b7sen", "nur", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "F\u00fcr den der an Gespenster glaubt \u2014", "tokens": ["F\u00fcr", "den", "der", "an", "Ge\u00b7spens\u00b7ter", "glaubt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sobald das Jahr den Wald entlaubt,", "tokens": ["So\u00b7bald", "das", "Jahr", "den", "Wald", "ent\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Macht sich der Herr von hinnen", "tokens": ["Macht", "sich", "der", "Herr", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ART", "NN", "APPR", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.30": {"text": "Von diesen luft'gen Zinnen \u2014", "tokens": ["Von", "die\u00b7sen", "luft'\u00b7gen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Schwelgt in der Stadt im Marmorsaal", "tokens": ["Schwelgt", "in", "der", "Stadt", "im", "Mar\u00b7mor\u00b7saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und spielt bei lust'gem Kerzenstrahl.", "tokens": ["Und", "spielt", "bei", "lust'\u00b7gem", "Ker\u00b7zen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Kling, kling! Ich h\u00f6r' es klingen,", "tokens": ["Kling", ",", "kling", "!", "Ich", "h\u00f6r'", "es", "klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Wie goldne F\u00fcchse springen ...", "tokens": ["Wie", "gold\u00b7ne", "F\u00fcch\u00b7se", "sprin\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Dein Vater \u2014 ward mir recht gesagt? \u2014", "tokens": ["Dein", "Va\u00b7ter", "ward", "mir", "recht", "ge\u00b7sagt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "War P\u00e4chter und ist ausgejagt ...", "tokens": ["War", "P\u00e4ch\u00b7ter", "und", "ist", "aus\u00b7ge\u00b7jagt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Da wei\u00dft du droben ein und aus,", "tokens": ["Da", "wei\u00dft", "du", "dro\u00b7ben", "ein", "und", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Du kennst den Hund, du kennst das Haus \u2014", "tokens": ["Du", "kennst", "den", "Hund", ",", "du", "kennst", "das", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ich borgte mir mein Spielgeld frisch", "tokens": ["Ich", "borg\u00b7te", "mir", "mein", "Spiel\u00b7geld", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Von dieses reichen Mannes Tisch!", "tokens": ["Von", "die\u00b7ses", "rei\u00b7chen", "Man\u00b7nes", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Nimm was da liegt, nimm was da steht:", "tokens": ["Nimm", "was", "da", "liegt", ",", "nimm", "was", "da", "steht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "ADV", "VVFIN", "$,", "VVIMP", "PWS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ein Prunkgeschirr, ein Goldger\u00e4t,", "tokens": ["Ein", "Prunk\u00b7ge\u00b7schirr", ",", "ein", "Gold\u00b7ge\u00b7r\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Mir darfst du's gleich verhandeln,", "tokens": ["Mir", "darfst", "du's", "gleich", "ver\u00b7han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Ich kann's in M\u00fcnze wandeln.", "tokens": ["Ich", "kann's", "in", "M\u00fcn\u00b7ze", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Von selber \u00f6ffnet sich der Schrein,", "tokens": ["Von", "sel\u00b7ber", "\u00f6ff\u00b7net", "sich", "der", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Du m\u00fc\u00dftest nicht ein Schlosser sein ...\u201c", "tokens": ["Du", "m\u00fc\u00df\u00b7test", "nicht", "ein", "Schlos\u00b7ser", "sein", "...", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "PPOSAT", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Der Bursche lauscht mit dumpfem Hirn", "tokens": ["Der", "Bur\u00b7sche", "lauscht", "mit", "dum\u00b7pfem", "Hirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Dem h\u00f6llischen Gemunkel,", "tokens": ["Dem", "h\u00f6l\u00b7li\u00b7schen", "Ge\u00b7mun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Ein Schatten steht auf seiner Stirn,", "tokens": ["Ein", "Schat\u00b7ten", "steht", "auf", "sei\u00b7ner", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Ein Schatten tief und dunkel:", "tokens": ["Ein", "Schat\u00b7ten", "tief", "und", "dun\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Und wieder leis und l\u00fcstern", "tokens": ["Und", "wie\u00b7der", "leis", "und", "l\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Beginnt das grimme Fl\u00fcstern:", "tokens": ["Be\u00b7ginnt", "das", "grim\u00b7me", "Fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "\u201ekurt, sieh den Lauf der Welt dir an!", "tokens": ["\u201e", "kurt", ",", "sieh", "den", "Lauf", "der", "Welt", "dir", "an", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVIMP", "ART", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Was wohl gelingt, ist wohl gethan!", "tokens": ["Was", "wohl", "ge\u00b7lingt", ",", "ist", "wohl", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Betrachte dir die Thaten", "tokens": ["Be\u00b7trach\u00b7te", "dir", "die", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Der gro\u00dfen Diplomaten,", "tokens": ["Der", "gro\u00b7\u00dfen", "Dip\u00b7lo\u00b7ma\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "Die klugen Herrn verstehn den Pfiff,", "tokens": [