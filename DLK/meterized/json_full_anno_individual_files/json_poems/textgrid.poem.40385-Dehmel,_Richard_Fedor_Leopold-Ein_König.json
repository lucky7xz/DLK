{"textgrid.poem.40385": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein K\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein K\u00f6nig werden;", "tokens": ["Ich", "kann", "ein", "K\u00f6\u00b7nig", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nicht etwa bei uns, i wo!", "tokens": ["nicht", "et\u00b7wa", "bei", "uns", ",", "i", "wo", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "$,", "NE", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei uns, da mu\u00df man Kronprinz hei\u00dfen,", "tokens": ["Bei", "uns", ",", "da", "mu\u00df", "man", "Kron\u00b7prinz", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dann wird man's sowieso.", "tokens": ["dann", "wird", "man's", "so\u00b7wi\u00b7e\u00b7so", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich werd bei den Negern K\u00f6nig!", "tokens": ["Ich", "werd", "bei", "den", "Ne\u00b7gern", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die fragen nicht nach dem Taufschein,", "tokens": ["Die", "fra\u00b7gen", "nicht", "nach", "dem", "Tauf\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wenn man nur orndtlich regiert.", "tokens": ["wenn", "man", "nur", "orndt\u00b7lich", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Erst z\u00e4hm ich mir ein Dutzend L\u00f6wen,", "tokens": ["Erst", "z\u00e4hm", "ich", "mir", "ein", "Dut\u00b7zend", "L\u00f6\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dann komm ich ankutschiert,", "tokens": ["dann", "komm", "ich", "an\u00b7kut\u00b7schiert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "acht Zebras vorgespannt:", "tokens": ["acht", "Zeb\u00b7ras", "vor\u00b7ge\u00b7spannt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was lauft ihr weg wie die Affen?", "tokens": ["Was", "lauft", "ihr", "weg", "wie", "die", "Af\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Reich ist vogelfrei!", "tokens": ["Mein", "Reich", "ist", "vo\u00b7gel\u00b7frei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer stark ist, darf's erobern helfen;", "tokens": ["Wer", "stark", "ist", ",", "dar\u00b7f's", "er\u00b7o\u00b7bern", "hel\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PAV", "VVINF", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die Klugen sind stark f\u00fcr zwei!", "tokens": ["die", "Klu\u00b7gen", "sind", "stark", "f\u00fcr", "zwei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "CARD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Kommt, Kinder, dankt euerm Herrgott!", "tokens": ["Kommt", ",", "Kin\u00b7der", ",", "dankt", "eu\u00b7erm", "Herr\u00b7gott", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr habt einen K\u00f6nig und Priester,", "tokens": ["Ihr", "habt", "ei\u00b7nen", "K\u00f6\u00b7nig", "und", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "der braucht keinen Polsterthron,", "tokens": ["der", "braucht", "kei\u00b7nen", "Pols\u00b7ter\u00b7thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "keinen Feldherrn, Hofherrn, Minister", "tokens": ["kei\u00b7nen", "Feld\u00b7herrn", ",", "Hof\u00b7herrn", ",", "Mi\u00b7nis\u00b7ter"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN"], "m