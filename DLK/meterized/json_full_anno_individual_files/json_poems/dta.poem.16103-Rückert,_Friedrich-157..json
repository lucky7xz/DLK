{"dta.poem.16103": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "157.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An Sch\u00f6nes, Wahres hat uns oft ein Traum gemahnt,", "tokens": ["An", "Sch\u00f6\u00b7nes", ",", "Wah\u00b7res", "hat", "uns", "oft", "ein", "Traum", "ge\u00b7mahnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJA", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was nicht in seinem Schatz der wache Geist geahnt.", "tokens": ["Was", "nicht", "in", "sei\u00b7nem", "Schatz", "der", "wa\u00b7che", "Geist", "ge\u00b7ahnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch Falsches, H\u00e4ssliches auch hat er angedeutet,", "tokens": ["Doch", "Fal\u00b7sches", ",", "H\u00e4ss\u00b7li\u00b7ches", "auch", "hat", "er", "an\u00b7ge\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was im Gem\u00fcthe l\u00e4ngst wir glaubten ausgereutet.", "tokens": ["Was", "im", "Ge\u00b7m\u00fc\u00b7the", "l\u00e4ngst", "wir", "glaub\u00b7ten", "aus\u00b7ge\u00b7reu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "PPER", "VVFIN", "VVPP", "$."], "