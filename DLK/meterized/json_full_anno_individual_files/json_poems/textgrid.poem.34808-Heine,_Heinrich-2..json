{"textgrid.poem.34808": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ganges rauscht, mit klugen Augen schauen", "tokens": ["Der", "Gan\u00b7ges", "rauscht", ",", "mit", "klu\u00b7gen", "Au\u00b7gen", "schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Antilopen aus dem Laub, sie springen", "tokens": ["Die", "An\u00b7ti\u00b7lo\u00b7pen", "aus", "dem", "Laub", ",", "sie", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herbei mutwillig, ihre bunten Schwingen", "tokens": ["Her\u00b7bei", "mut\u00b7wil\u00b7lig", ",", "ih\u00b7re", "bun\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entfaltend, wandeln stolzgespreizte Pfauen.", "tokens": ["Ent\u00b7fal\u00b7tend", ",", "wan\u00b7deln", "stolz\u00b7ge\u00b7spreiz\u00b7te", "Pfau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Tief aus dem Herzen der bestrahlten Auen", "tokens": ["Tief", "aus", "dem", "Her\u00b7zen", "der", "be\u00b7strahl\u00b7ten", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART",