{"dta.poem.20306": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Solimen/ als sie roth/ er hinge-  \n gen im gesichte bleich ward.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da deine wangen sich mit keuschem purpur f\u00e4rben/", "tokens": ["Da", "dei\u00b7ne", "wan\u00b7gen", "sich", "mit", "keu\u00b7schem", "pur\u00b7pur", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So siehst du mein gesicht in bleicher angst verderben.", "tokens": ["So", "siehst", "du", "mein", "ge\u00b7sicht", "in", "blei\u00b7cher", "angst", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht wundre/ sch\u00f6nste/ dich ob diesem unterscheidt/", "tokens": ["Nicht", "wund\u00b7re", "/", "sch\u00f6ns\u00b7te", "/", "dich", "ob", "die\u00b7sem", "un\u00b7ter\u00b7scheidt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "VVFIN", "$(", "PPER", "KOUS", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Cupido hat sein feur gantz ungleich ausgestreut/", "tokens": ["Cu\u00b7pi\u00b7do", "hat", "sein", "feur", "gantz", "un\u00b7gleich", "aus\u00b7ge\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und/ wie ein ieder selbst an unsrer farbe sp\u00fcret/", "tokens": ["Und", "/", "wie", "ein", "ie\u00b7der", "selbst", "an", "uns\u00b7rer", "far\u00b7be", "sp\u00fc\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOKOM", "ART", "PIAT", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey dir die wangen nur/ bey mir das hertz ger\u00fchret.", "tokens": ["Bey", "dir", "die", "wan\u00b7gen", "nur", "/", "bey", "mir", "das", "hertz", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "$(", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}}}}