{"dta.poem.14231": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsame .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Morgens das fr\u00f6hliche Licht bricht ein,", "tokens": ["Wenn", "Mor\u00b7gens", "das", "fr\u00f6h\u00b7li\u00b7che", "Licht", "bricht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Tret' ich zum offenen Fensterlein,", "tokens": ["Tret'", "ich", "zum", "of\u00b7fe\u00b7nen", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drau\u00dfen geh'n lau die L\u00fcft' auf den Auen,", "tokens": ["Drau\u00b7\u00dfen", "geh'n", "lau", "die", "L\u00fcft'", "auf", "den", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Singen die Lerchen schon hoch im Blauen,", "tokens": ["Sin\u00b7gen", "die", "Ler\u00b7chen", "schon", "hoch", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Rauschen am Fenster die B\u00e4ume gar munter,", "tokens": ["Rau\u00b7schen", "am", "Fens\u00b7ter", "die", "B\u00e4u\u00b7me", "gar", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Zieh'n die Br\u00fcder in den Wald hinunter;", "tokens": ["Zieh'n", "die", "Br\u00fc\u00b7der", "in", "den", "Wald", "hin\u00b7un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und bei dem Sange und H\u00f6rnerklange", "tokens": ["Und", "bei", "dem", "San\u00b7ge", "und", "H\u00f6r\u00b7ner\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Wird mir immer so bange, bange.", "tokens": ["Wird", "mir", "im\u00b7mer", "so", "ban\u00b7ge", ",", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}