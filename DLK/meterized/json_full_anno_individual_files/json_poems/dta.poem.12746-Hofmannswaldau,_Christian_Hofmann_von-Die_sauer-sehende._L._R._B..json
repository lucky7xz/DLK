{"dta.poem.12746": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die sauer-sehende.  \n L. R. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was lachst du? da\u00df ihr maul so auf die seite steht,", "tokens": ["Was", "lachst", "du", "?", "da\u00df", "ihr", "maul", "so", "auf", "die", "sei\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lauter sauer-sehn ihr aus den minen geht;", "tokens": ["Und", "lau\u00b7ter", "sau\u00b7e\u00b7rsehn", "ihr", "aus", "den", "mi\u00b7nen", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O h\u00f6hne nicht zu fr\u00fch! wer will calender machen,", "tokens": ["O", "h\u00f6h\u00b7ne", "nicht", "zu", "fr\u00fch", "!", "wer", "will", "ca\u00b7len\u00b7der", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$.", "PWS", "VMFIN", "NE", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mu\u00df alle jubel-jahr nicht mehr als einmal lachen:", "tokens": ["Mu\u00df", "al\u00b7le", "ju\u00b7bel\u00b7jahr", "nicht", "mehr", "als", "ein\u00b7mal", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PTKNEG", "ADV", "KOKOM", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und weil man itzger zeit auf neue mode h\u00e4lt,", "tokens": ["Und", "weil", "man", "itz\u00b7ger", "zeit", "auf", "neu\u00b7e", "mo\u00b7de", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist dir ihr schlimmes maul zum muster vorgestellt.", "tokens": ["Ist", "dir", "ihr", "schlim\u00b7mes", "maul", "zum", "mus\u00b7ter", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein ieder narr der tr\u00e4gts, wies ihm gewachsen ist,", "tokens": ["Ein", "ie\u00b7der", "narr", "der", "tr\u00e4gts", ",", "wies", "ihm", "ge\u00b7wach\u00b7sen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "VVFIN", "$,", "VVFIN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Verkehre du es selbst, wo du so k\u00fcnstlich bist.", "tokens": ["Ver\u00b7keh\u00b7re", "du", "es", "selbst", ",", "wo", "du", "so", "k\u00fcnst\u00b7lich", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}