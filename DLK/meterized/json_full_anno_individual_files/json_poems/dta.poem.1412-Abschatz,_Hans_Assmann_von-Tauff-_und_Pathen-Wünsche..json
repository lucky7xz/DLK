{"dta.poem.1412": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn GOttes strenger Zorn ob angeerbten S\u00fcnden", "tokens": ["Wenn", "Got\u00b7tes", "stren\u00b7ger", "Zorn", "ob", "an\u00b7ge\u00b7erb\u00b7ten", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJA", "NN", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In heissem Grimm entbrandt/ erk\u00fchlen soll und schwin-", "tokens": ["In", "heis\u00b7sem", "Grimm", "ent\u00b7brandt", "/", "er\u00b7k\u00fch\u00b7len", "soll", "und", "schwin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVPP", "$(", "VVINF", "VMFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "So ist hierzu die edle Flutt/", "tokens": ["So", "ist", "hier\u00b7zu", "die", "ed\u00b7le", "Flutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns durch sein Wort/ geweyht im Bad der Tauffe gutt/", "tokens": ["Uns", "durch", "sein", "Wort", "/", "ge\u00b7weyht", "im", "Bad", "der", "Tauf\u00b7fe", "gutt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$(", "VVPP", "APPRART", "NE", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df/ Pathe/ wirst du auch zur Seligkeit empfinden!", "tokens": ["Di\u00df", "/", "Pa\u00b7the", "/", "wirst", "du", "auch", "zur", "Se\u00b7lig\u00b7keit", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "NE", "$(", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}