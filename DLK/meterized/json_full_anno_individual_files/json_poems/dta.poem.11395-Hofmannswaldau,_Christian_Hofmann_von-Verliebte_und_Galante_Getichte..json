{"dta.poem.11395": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte und Galante  \n Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Melinde, die gewi\u00df Dianen wenig weicht,", "tokens": ["Me\u00b7lin\u00b7de", ",", "die", "ge\u00b7wi\u00df", "Di\u00b7a\u00b7nen", "we\u00b7nig", "weicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Netzt ihrer h\u00e4nde schnee in den verliebten z\u00e4hren,", "tokens": ["Netzt", "ih\u00b7rer", "h\u00e4n\u00b7de", "schnee", "in", "den", "ver\u00b7lieb\u00b7ten", "z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die meiner angen quell ihr als ein opffer reicht,", "tokens": ["Die", "mei\u00b7ner", "an\u00b7gen", "quell", "ihr", "als", "ein", "opf\u00b7fer", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und will mit diesem na\u00df mich in ein wild verkehren.", "tokens": ["Und", "will", "mit", "die\u00b7sem", "na\u00df", "mich", "in", "ein", "wild", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PDAT", "VVFIN", "PRF", "APPR", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indessen ist gleichwohl ihr wille nicht geschehn:", "tokens": ["In\u00b7des\u00b7sen", "ist", "gleich\u00b7wohl", "ihr", "wil\u00b7le", "nicht", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vielleichte, weil ich sie noch niemahls nackt gesehn.", "tokens": ["Viel\u00b7leich\u00b7te", ",", "weil", "ich", "sie", "noch", "nie\u00b7mahls", "nackt", "ge\u00b7sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}