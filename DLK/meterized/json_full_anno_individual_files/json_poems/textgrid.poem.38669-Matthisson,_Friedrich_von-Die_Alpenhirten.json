{"textgrid.poem.38669": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Alpenhirten", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unten im Rauch' und Gerassel der St\u00e4dte, wie d\u00fcnkte des Hirten", "tokens": ["Un\u00b7ten", "im", "Rauch'", "und", "Ge\u00b7ras\u00b7sel", "der", "St\u00e4d\u00b7te", ",", "wie", "d\u00fcnk\u00b7te", "des", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "NN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Patriarchalischer Stand oft so beneidenswerth mir!", "tokens": ["Pat\u00b7ri\u00b7ar\u00b7cha\u00b7li\u00b7scher", "Stand", "oft", "so", "be\u00b7nei\u00b7dens\u00b7werth", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "ADV", "ADJD", "PPER", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.3": {"text": "Und ich erklimmte die Berge den Gl\u00fccklichen selber zu gr\u00fc\u00dfen,", "tokens": ["Und", "ich", "er\u00b7klimm\u00b7te", "die", "Ber\u00b7ge", "den", "Gl\u00fcck\u00b7li\u00b7chen", "sel\u00b7ber", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch da zerflossen wie Dunst pl\u00f6tzlich die Bilder des Wahns.", "tokens": ["Doch", "da", "zer\u00b7flos\u00b7sen", "wie", "Dunst", "pl\u00f6tz\u00b7lich", "die", "Bil\u00b7der", "des", "Wahns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "KOKOM", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wi\u00dft! auf den Triften der Alpen, treibt heerdengesegnet ein V\u00f6lkchen", "tokens": ["Wi\u00dft", "!", "auf", "den", "Trif\u00b7ten", "der", "Al\u00b7pen", ",", "treibt", "heer\u00b7den\u00b7ge\u00b7seg\u00b7net", "ein", "V\u00f6lk\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "W\u00fcnschend und f\u00fcrchtend wie wir, nimmer befriedigt sein Werk.", "tokens": ["W\u00fcn\u00b7schend", "und", "f\u00fcrch\u00b7tend", "wie", "wir", ",", "nim\u00b7mer", "be\u00b7frie\u00b7digt", "sein", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Du, der Gen\u00fcgsamkeit Blume! wo diesseits der Lethe dich finden?", "tokens": ["Du", ",", "der", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "Blu\u00b7me", "!", "wo", "dies\u00b7seits", "der", "Le\u00b7the", "dich", "fin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$.", "PWAV", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+---+-+--+-", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Ach! nur im Lande der Fee'n, unter dem singenden Baum.", "tokens": ["Ach", "!", "nur", "im", "Lan\u00b7de", "der", "Fee'n", ",", "un\u00b7ter", "dem", "sin\u00b7gen\u00b7den", "Baum