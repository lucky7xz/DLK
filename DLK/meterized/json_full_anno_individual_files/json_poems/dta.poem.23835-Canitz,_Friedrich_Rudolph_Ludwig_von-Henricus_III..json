{"dta.poem.23835": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Henricus III.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ungern Ubermuht/ der gar zu hoch gestiegen/", "tokens": ["Der", "Un\u00b7gern", "U\u00b7ber\u00b7muht", "/", "der", "gar", "zu", "hoch", "ge\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "ADV", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df doch der Majest\u00e4t des Reiches unterliegen/", "tokens": ["Mu\u00df", "doch", "der", "Ma\u00b7jes\u00b7t\u00e4t", "des", "Rei\u00b7ches", "un\u00b7ter\u00b7lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ich zu meiner Zeit noch unverletzt behielt;", "tokens": ["Die", "ich", "zu", "mei\u00b7ner", "Zeit", "noch", "un\u00b7ver\u00b7letzt", "be\u00b7hielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob gleich der P\u00e4bste Trug auf ihren Fall gezielt.", "tokens": ["Ob", "gleich", "der", "P\u00e4bs\u00b7te", "Trug", "auf", "ih\u00b7ren", "Fall", "ge\u00b7zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos"