{"dta.poem.8858": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Aber da\u00df du sch\u00f6n geblieben, wie du warst, das ist ge-", "tokens": ["A\u00b7ber", "da\u00df", "du", "sch\u00f6n", "ge\u00b7blie\u00b7ben", ",", "wie", "du", "warst", ",", "das", "ist", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP", "$,", "PWAV", "PPER", "VAFIN", "$,", "PDS", "VAFIN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Wenn im br\u00fcderlichen Zirkel andrer J\u00fcnglinge du stehst,", "tokens": ["Wenn", "im", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Zir\u00b7kel", "an\u00b7drer", "J\u00fcng\u00b7lin\u00b7ge", "du", "stehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+---+", "measure": "unknown.measure.septa"}, "line.3": {"text": "O so stehst du wie der Morgen zwischen Grau'n und", "tokens": ["O", "so", "stehst", "du", "wie", "der", "Mor\u00b7gen", "zwi\u00b7schen", "Grau'n", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "APPR", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Nur vergeb'ne M\u00fche war es, um zu retten mich vor", "tokens": ["Nur", "ver\u00b7ge\u00b7b'\u00b7ne", "M\u00fc\u00b7he", "war", "es", ",", "um", "zu", "ret\u00b7ten", "mich", "vor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "PRF", "APPR"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Da\u00df ich Andre sch\u00f6n zu finden \u00fcber Alles mich befli\u00df!", "tokens": ["Da\u00df", "ich", "And\u00b7re", "sch\u00f6n", "zu", "fin\u00b7den", "\u00fc\u00b7ber", "Al\u00b7les", "mich", "be\u00b7fli\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJD", "PTKZU", "VVINF", "APPR", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Doch in eines Stolzen Banden sich zu wissen, ist so", "tokens": ["Doch", "in", "ei\u00b7nes", "Stol\u00b7zen", "Ban\u00b7den", "sich", "zu", "wis\u00b7sen", ",", "ist", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PRF", "PTKZU", "VVINF", "$,", "VAFIN", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Da\u00df ich oft, ergrimmt und trotzig, in die falsche Kette", "tokens": ["Da\u00df", "ich", "oft", ",", "er\u00b7grimmt", "und", "trot\u00b7zig", ",", "in", "die", "fal\u00b7sche", "Ket\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "VVPP", "KON", "ADJD", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Grausam ist es, Trank und Speise meiner Lippe zu", "tokens": ["Grau\u00b7sam", "ist", "es", ",", "Trank", "und", "Spei\u00b7se", "mei\u00b7ner", "Lip\u00b7pe", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "NN", "KON", "NN", "PPOSAT", "NN", "PTKZU"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Und dabey mir Gl\u00fcck zu w\u00fcnschen, und zu sagen: Trink'", "tokens": ["Und", "da\u00b7bey", "mir", "Gl\u00fcck", "zu", "w\u00fcn\u00b7schen", ",", "und", "zu", "sa\u00b7gen", ":", "Trink'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PAV", "PPER", "NN", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$.", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}