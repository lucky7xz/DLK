{"dta.poem.6102": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt nun, die auf solche Art formiret,", "tokens": ["Die", "Welt", "nun", ",", "die", "auf", "sol\u00b7che", "Art", "for\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Folgt unver\u00e4nderlich dem vorgesetzten Gang.", "tokens": ["Folgt", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "dem", "vor\u00b7ge\u00b7setz\u00b7ten", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie bleibet, ob man gleich ein stetes Aendern sp\u00fcret.", "tokens": ["Sie", "blei\u00b7bet", ",", "ob", "man", "gleich", "ein", "ste\u00b7tes", "A\u00b7en\u00b7dern", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der unterschiedliche Zusammendrang", "tokens": ["Der", "un\u00b7ter\u00b7schied\u00b7li\u00b7che", "Zu\u00b7sam\u00b7men\u00b7drang"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der dreyen Elementen macht,", "tokens": ["Der", "drey\u00b7en", "E\u00b7le\u00b7men\u00b7ten", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Da\u00df so verschiedne Ding verschiedene Gestallten", "tokens": ["Da\u00df", "so", "ver\u00b7schied\u00b7ne", "Ding", "ver\u00b7schie\u00b7de\u00b7ne", "Ge\u00b7stall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bekommen haben und behalten.", "tokens": ["Be\u00b7kom\u00b7men", "ha\u00b7ben", "und", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das erste hat der hellen Sonnen-Pracht,", "tokens": ["Das", "ers\u00b7te", "hat", "der", "hel\u00b7len", "Son\u00b7nen\u00b7Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und das hellgl\u00e4ntzende Gestirn hervor gebracht.", "tokens": ["Und", "das", "hell\u00b7gl\u00e4nt\u00b7zen\u00b7de", "Ge\u00b7stirn", "her\u00b7vor", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "VVPP", "$."], "meter": "--++-+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Das zweyte, so den Raum des Himmels f\u00fcllt; beweget", "tokens": ["Das", "zwey\u00b7te", ",", "so", "den", "Raum", "des", "Him\u00b7mels", "f\u00fcllt", ";", "be\u00b7we\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$.", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Jrr-Gestirn, so es in seinem Schoosse tr\u00e4get.", "tokens": ["Die", "Jrr\u00b7Ge\u00b7stirn", ",", "so", "es", "in", "sei\u00b7nem", "Schoos\u00b7se", "tr\u00e4\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch die durchsicht\u2019ge Fluht von seinem hellen Blauen,", "tokens": ["Durch", "die", "durch\u00b7sicht'\u00b7ge", "Fluht", "von", "sei\u00b7nem", "hel\u00b7len", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die kleine K\u00fcgelchen, die so beweglich seyn;", "tokens": ["Die", "klei\u00b7ne", "K\u00fc\u00b7gel\u00b7chen", ",", "die", "so", "be\u00b7weg\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Kan man die Ordnungen, zusammt der Strahlen Schein", "tokens": ["Kan", "man", "die", "Ord\u00b7nun\u00b7gen", ",", "zu\u00b7sammt", "der", "Strah\u00b7len", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ART", "NN", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Von so verschiednen Sternen schauen.", "tokens": ["Von", "so", "ver\u00b7schied\u00b7nen", "Ster\u00b7nen", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das dritte, so gemischt von Theilen, die nicht gleich,", "tokens": ["Das", "drit\u00b7te", ",", "so", "ge\u00b7mischt", "von", "Thei\u00b7len", ",", "die", "nicht", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "VVPP", "APPR", "NN", "$,", "PRELS", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die an Ver\u00e4ndrungen fast unersch\u00f6pfflich reich,", "tokens": ["Die", "an", "Ver\u00b7\u00e4n\u00b7drun\u00b7gen", "fast", "un\u00b7er\u00b7sch\u00f6pf\u00b7flich", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.18": {"text": "Formirt den zarten Dufft", "tokens": ["For\u00b7mirt", "den", "zar\u00b7ten", "Dufft"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.19": {"text": "Der uns umgebenden und ausgespannten Lufft.", "tokens": ["Der", "uns", "um\u00b7ge\u00b7ben\u00b7den", "und", "aus\u00b7ge\u00b7spann\u00b7ten", "Lufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}