{"textgrid.poem.52281": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Rittermannshagen, dor was mal en Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Rittermannshagen, dor was mal en Mann,", "tokens": ["In", "Rit\u00b7ter\u00b7manns\u00b7ha\u00b7gen", ",", "dor", "was", "mal", "en", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "De l\u00e4d sick woll hen, \u00fcm tau starben,", "tokens": ["De", "l\u00e4d", "sick", "woll", "hen", ",", "\u00fcm", "tau", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "VVINF", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Un wil nu doch keiner wat mitnehmen kann,", "tokens": ["Un", "wil", "nu", "doch", "kei\u00b7ner", "wat", "mit\u00b7neh\u00b7men", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "So let hei sin Kinner dat arben.", "tokens": ["So", "let", "hei", "sin", "Kin\u00b7ner", "dat", "ar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So deilt hei sin Hus un sin Hof un sin Feld", "tokens": ["So", "deilt", "hei", "sin", "Hus", "un", "sin", "Hof", "un", "sin", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tau gliken Deil v\u00f6r sin D\u00f6chter.", "tokens": ["Tau", "gli\u00b7ken", "Deil", "v\u00f6r", "sin", "D\u00f6ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbok kriggt nu en jeder von jug glikes Geld", "tokens": ["\u00bb", "ok", "kriggt", "nu", "en", "je\u00b7der", "von", "jug", "gli\u00b7kes", "Geld"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "VVFIN", "ADV", "VVFIN", "PIS", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Un de H\u00e4lft' von den Goren\u00ab, so seggt er.", "tokens": ["Un", "de", "H\u00e4lft'", "von", "den", "Go\u00b7ren", "\u00ab", ",", "so", "seggt", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPR", "ART", "NN", "$(", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Un kum dat de Oll verstorben nu was", "tokens": ["Un", "kum", "dat", "de", "Oll", "ver\u00b7stor\u00b7ben", "nu", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NE", "NE", "VVFIN", "ADV", "PWS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Un was in Freden begraben,", "tokens": ["Un", "was", "in", "Fre\u00b7den", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Dunn rafften de beiden mit Hast un mit Ha\u00df", "tokens": ["Dunn", "raff\u00b7ten", "de", "bei\u00b7den", "mit", "Hast", "un", "mit", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "PIAT", "APPR", "NE", "FM", "APPR", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dat Arwdeil tausam, as de Raben.", "tokens": ["Dat", "Arw\u00b7deil", "tau\u00b7sam", ",", "as", "de", "Ra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dat Geld, dat w\u00fcrd deilt, un de Hofst\u00e4d dortau,", "tokens": ["Dat", "Geld", ",", "dat", "w\u00fcrd", "deilt", ",", "un", "de", "Hof\u00b7st\u00e4d", "dor\u00b7tau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Un kein' von de beid' was taufreden;", "tokens": ["Un", "kein'", "von", "de", "beid'", "was", "tauf\u00b7re\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "APPR", "NE", "APPR", "PRELS", "ADJA", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Un as sei sick deilten den Goren genau,", "tokens": ["Un", "as", "sei", "sick", "de\u00b7il\u00b7ten", "den", "Go\u00b7ren", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dunn heww'n sei sick gruglichen streden.", "tokens": ["Dunn", "hew\u00b7w'n", "sei", "sick", "grug\u00b7li\u00b7chen", "stre\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "In den middelsten Stig w\u00fcrd en Kirschbom sin,", "tokens": ["In", "den", "mid\u00b7dels\u00b7ten", "Stig", "w\u00fcrd", "en", "Kirschbom", "sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nich rechtsch un nich linksch stunn hei 'ranner.", "tokens": ["Nich", "rechtsch", "un", "nich", "link\u00b7sch", "stunn", "hei", "'ran\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "FM", "PTKNEG", "ADJD", "VVFIN", "NE", "NE", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u00bbdat's min!\u00ab s\u00e4d de \u00d6llst', \u00bbde Kirschbom is min!\u00ab", "tokens": ["\u00bb", "dat's", "min", "!", "\u00ab", "s\u00e4d", "de", "\u00d6llst'", ",", "\u00bb", "de", "Kirschbom", "is", "min", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "NE", "$.", "$(", "FM.la", "FM.la", "FM.la", "$,", "$(", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbdu b\u00fcst woll nich klauk\u00ab, s\u00e4d de anner.", "tokens": ["\u00bb", "du", "b\u00fcst", "woll", "nich", "klauk", "\u00ab", ",", "s\u00e4d", "de", "an\u00b7ner", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PTKNEG", "NE", "$(", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Un as nu de Kirschen rip wiren binah,", "tokens": ["Un", "as", "nu", "de", "Kir\u00b7schen", "rip", "wi\u00b7ren", "bi\u00b7nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+--++-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dunn wull ok de \u00d6llst sei sick austen.", "tokens": ["Dunn", "wull", "ok", "de", "\u00d6llst", "sei", "sick", "aus\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "\u00bbherut ut den Bom! Herunner! Ick slah!\u00ab", "tokens": ["\u00bb", "he\u00b7rut", "ut", "den", "Bom", "!", "He\u00b7run\u00b7ner", "!", "Ick", "slah", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "ART", "NN", "$.", "NN", "$.", "PPER", "VVFIN", "$.", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Rep de J\u00fcngst, \u00bbdat sall di wat hausten!\u00ab", "tokens": ["Rep", "de", "J\u00fcngst", ",", "\u00bb", "dat", "sall", "di", "wat", "haus\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "NE", "$,", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Sei sch\u00fcllen sick 'r\u00fcm, un sei fohrten tausam", "tokens": ["Sei", "sch\u00fcl\u00b7len", "sick", "'r\u00fcm", ",", "un", "sei", "fohr\u00b7ten", "tau\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "NE", "$,", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Un kratzten sick af de Gesichter,", "tokens": ["Un", "kratz\u00b7ten", "sick", "af", "de", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NE", "NE", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sei slogen sick krumm, un sei slogen sick lahm", "tokens": ["Sei", "slo\u00b7gen", "sick", "krumm", ",", "un", "sei", "slo\u00b7gen", "sick", "lahm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "ADJD", "ADJD", "$,", "FM", "FM", "FM", "FM", "FM"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Un lepen tauletzt nah den Richter.", "tokens": ["Un", "le\u00b7pen", "tau\u00b7letzt", "nah", "den", "Rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "De Kirschbom, de bl\u00e4uht, de Kirschbom, de drog,", "tokens": ["De", "Kirschbom", ",", "de", "bl\u00e4uht", ",", "de", "Kirschbom", ",", "de", "drog", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VVFIN", "$,", "NE", "NE", "$,", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Avvekaten, de kemen;", "tokens": ["De", "Av\u00b7ve\u00b7ka\u00b7ten", ",", "de", "ke\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dat Frugensvolk j\u00f6hrlich sick wedder slog,", "tokens": ["Dat", "Fru\u00b7gens\u00b7volk", "j\u00f6hr\u00b7lich", "sick", "wed\u00b7der", "slog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn kein von ehr wull sick bequemen.", "tokens": ["Denn", "kein", "von", "ehr", "wull", "sick", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "De Kirschbom, de bl\u00e4uht, de Kirschbom, de drog,", "tokens": ["De", "Kirschbom", ",", "de", "bl\u00e4uht", ",", "de", "Kirschbom", ",", "de", "drog", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VVFIN", "$,", "NE", "NE", "$,", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un j\u00f6hrlich gaww't en Getagel,", "tokens": ["Un", "j\u00f6hr\u00b7lich", "gaw\u00b7w't", "en", "Ge\u00b7ta\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "ADJA", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Un wil dat eine de annere slog,", "tokens": ["Un", "wil", "dat", "ei\u00b7ne", "de", "an\u00b7ne\u00b7re", "slog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Verteh