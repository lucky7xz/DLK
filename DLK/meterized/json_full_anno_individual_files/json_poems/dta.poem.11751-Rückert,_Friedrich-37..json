{"dta.poem.11751": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer etwas lernen will, der mu\u00df dazu drei Gaben,", "tokens": ["Wer", "et\u00b7was", "ler\u00b7nen", "will", ",", "der", "mu\u00df", "da\u00b7zu", "drei", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "$,", "ART", "VMFIN", "PAV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von obenher, aus sich, und auch von au\u00dfen haben.", "tokens": ["Von", "o\u00b7ben\u00b7her", ",", "aus", "sich", ",", "und", "auch", "von", "au\u00b7\u00dfen", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "APPR", "PRF", "$,", "KON", "ADV", "APPR", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die F\u00e4higkeit, die Lust und die Gelegenheit;", "tokens": ["Die", "F\u00e4\u00b7hig\u00b7keit", ",", "die", "Lust", "und", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die drei wo fehlen, kommt ein Lernender nicht weit.", "tokens": ["Die", "drei", "wo", "feh\u00b7len", ",", "kommt", "ein", "Ler\u00b7nen\u00b7der", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "PWAV", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum Lernen F\u00e4higkeit mu\u00df Gott dir selbst verleihen,", "tokens": ["Zum", "Ler\u00b7nen", "F\u00e4\u00b7hig\u00b7keit", "mu\u00df", "Gott", "dir", "selbst", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VMFIN", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil in fruchtbarem Grund Fruchtb\u00e4ume nur gedeihen.", "tokens": ["Weil", "in", "frucht\u00b7ba\u00b7rem", "Grund", "Frucht\u00b7b\u00e4u\u00b7me", "nur", "ge\u00b7dei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die F\u00e4higkeit ist todt, wo sie nicht wird zum Triebe;", "tokens": ["Die", "F\u00e4\u00b7hig\u00b7keit", "ist", "todt", ",", "wo", "sie", "nicht", "wird", "zum", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "PPER", "PTKNEG", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Lernen treiben mu\u00df dich eigne Lust und Liebe.", "tokens": ["Zum", "Ler\u00b7nen", "trei\u00b7ben", "mu\u00df", "dich", "eig\u00b7ne", "Lust", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "PRF", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dann mu\u00df Gelegenheit von au\u00dfen zum Besuch", "tokens": ["Dann", "mu\u00df", "Ge\u00b7le\u00b7gen\u00b7heit", "von", "au\u00b7\u00dfen", "zum", "Be\u00b7such"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "APPR", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir kommen in Gestalt von Lehrer oder Buch.", "tokens": ["Dir", "kom\u00b7men", "in", "Ge\u00b7stalt", "von", "Leh\u00b7rer", "o\u00b7der", "Buch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Fehlt in der N\u00e4he dir Gelegenheit zu lernen,", "tokens": ["Fehlt", "in", "der", "N\u00e4\u00b7he", "dir", "Ge\u00b7le\u00b7gen\u00b7heit", "zu", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Trieb zu lernen wird dich treiben in die Fernen.", "tokens": ["Der", "Trieb", "zu", "ler\u00b7nen", "wird", "dich", "trei\u00b7ben", "in", "die", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und jede F\u00e4higkeit ist selbst ihr eigner Trieb;", "tokens": ["Und", "je\u00b7de", "F\u00e4\u00b7hig\u00b7keit", "ist", "selbst", "ihr", "eig\u00b7ner", "Trieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und also sind sie Eins, die ich als drei beschrieb.", "tokens": ["Und", "al\u00b7so", "sind", "sie", "Eins", ",", "die", "ich", "als", "drei", "be\u00b7schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$,", "PRELS", "PPER", "KOKOM", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}