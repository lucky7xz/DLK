{"textgrid.poem.41158": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies war aus alter Zeit ein Weib,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies war aus alter Zeit ein Weib,", "tokens": ["Dies", "war", "aus", "al\u00b7ter", "Zeit", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch jetzt noch gibt es Frauen,", "tokens": ["Doch", "jetzt", "noch", "gibt", "es", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frauen, die emsig ihren Leib,", "tokens": ["Frau\u00b7en", ",", "die", "em\u00b7sig", "ih\u00b7ren", "Leib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch faul den Geist bebauen.", "tokens": ["Doch", "faul", "den", "Geist", "be\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie werdet, Eitle, ihr einmal", "tokens": ["Wie", "wer\u00b7det", ",", "Eit\u00b7le", ",", "ihr", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "$,", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach dem Tod aus Spiegeln blicken!", "tokens": ["Nach", "dem", "Tod", "aus", "Spie\u00b7geln", "bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In des aufgeblasenen Rocks Skandal,", "tokens": ["In", "des", "auf\u00b7ge\u00b7bla\u00b7se\u00b7nen", "Rocks", "Skan\u00b7dal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Den Putzhut in dem R\u00fccken.", "tokens": ["Den", "Putz\u00b7hut", "in", "dem", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Um euren Arm den Firlefanz", "tokens": ["Um", "eu\u00b7ren", "Arm", "den", "Fir\u00b7le\u00b7fanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von Spitzen, \u2013 Gott, welch Schauer!", "tokens": ["Von", "Spit\u00b7zen", ",", "\u2013", "Gott", ",", "welch", "Schau\u00b7er", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "$(", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Beginnet ihr den Totentanz", "tokens": ["Be\u00b7gin\u00b7net", "ihr", "den", "To\u00b7ten\u00b7tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So um die Kirchhofmauer.", "tokens": ["So", "um", "die", "Kirch\u00b7hof\u00b7mau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die M\u00e4nner, die in gleichem Wahn", "tokens": ["Die", "M\u00e4n\u00b7ner", ",", "die", "in", "glei\u00b7chem", "Wahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit euch, ihr Eitlen, stecken,", "tokens": ["Mit", "euch", ",", "ihr", "Eit\u00b7len", ",", "ste\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Mittanzen als Gerippe dann", "tokens": ["Mit\u00b7tan\u00b7zen", "als", "Ge\u00b7rip\u00b7pe", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOUS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In ihren l\u00e4pp'schen Fr\u00e4cken,", "tokens": ["In", "ih\u00b7ren", "l\u00e4pp'\u00b7schen", "Fr\u00e4\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Angstr\u00f6hren, da\u00df sich Gott erbarm'!", "tokens": ["Angs\u00b7tr\u00f6h\u00b7ren", ",", "da\u00df", "sich", "Gott", "er\u00b7barm'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Auf ihren K\u00f6pfen tragend,", "tokens": ["Auf", "ih\u00b7ren", "K\u00f6p\u00b7fen", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Oder Klapph\u00fcte unterm Arm,", "tokens": ["O\u00b7der", "Klapp\u00b7h\u00fc\u00b7te", "un\u00b7term", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.20": {"text": "Komplimentenr\u00e4der schlagend.", "tokens": ["Kom\u00b7pli\u00b7men\u00b7ten\u00b7r\u00e4\u00b7der", "schla\u00b7gend", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Stellt euch einmal die Engel vor", "tokens": ["Stellt", "euch", "ein\u00b7mal", "die", "En\u00b7gel", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "In H\u00fcten l\u00e4cherlich putzig,", "tokens": ["In", "H\u00fc\u00b7ten", "l\u00e4\u00b7cher\u00b7lich", "put\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Wie jetzt sie sind bei euch im Flor,", "tokens": ["Wie", "jetzt", "sie", "sind", "bei", "euch", "im", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Im Nacken sitzend stutzig.", "tokens": ["Im", "Na\u00b7cken", "sit\u00b7zend", "stut\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Seht sie in des Ballonrocks Schmach,", "tokens": ["Seht", "sie", "in", "des", "Bal\u00b7lon\u00b7rocks", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Wie euch, o Schauer! wallen,", "tokens": ["Wie", "euch", ",", "o", "Schau\u00b7er", "!", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "FM", "NE", "$.", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Gewi\u00df, ihr w\u00fcrdet sagen: ach!", "tokens": ["Ge\u00b7wi\u00df", ",", "ihr", "w\u00fcr\u00b7det", "sa\u00b7gen", ":", "ach", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVINF", "$.", "XY", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wie tief sind ", "tokens": ["Wie", "tief", "sind"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Ihr Fraun, die ihr die Eitelkeit", "tokens": ["Ihr", "Fraun", ",", "die", "ihr", "die", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Durch Demut \u00fcberwunden,", "tokens": ["Durch", "De\u00b7mut", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Euer Kopfputz sei ein T\u00fcchlein breit,", "tokens": ["Eu\u00b7er", "Kopf\u00b7putz", "sei", "ein", "T\u00fcch\u00b7lein", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "Um die blanke Stirn gebunden.", "tokens": ["Um", "die", "blan\u00b7ke", "Stirn", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Umh\u00fcllen m\u00f6ge euren Leib", "tokens": ["Um\u00b7h\u00fcl\u00b7len", "m\u00f6\u00b7ge", "eu\u00b7ren", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ein wei\u00dfes Kleid von Linnen,", "tokens": ["Ein", "wei\u00b7\u00dfes", "Kleid", "von", "Lin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Das k\u00f6nnt ihr selbst zum Zeitvertreib", "tokens": ["Das", "k\u00f6nnt", "ihr", "selbst", "zum", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Euch mit den T\u00f6chtern spinnen.", "tokens": ["Euch", "mit", "den", "T\u00f6ch\u00b7tern", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Die Seele bleibt, auf ", "tokens": ["Die", "See\u00b7le", "bleibt", ",", "auf"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.38": {"text": "Ihr Fraun, der Leib ist fl\u00fcchtig;", "tokens": ["Ihr", "Fraun", ",", "der", "Leib", "ist", "fl\u00fcch\u00b7tig", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Doch mancher, ach! ist ihre Haut", "tokens": ["Doch", "man\u00b7cher", ",", "ach", "!", "ist", "ih\u00b7re", "Haut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "ITJ", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Mehr als die Seele wichtig.", "tokens": ["Mehr", "als", "die", "See\u00b7le", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Die Seele, noch so sch\u00f6n umh\u00fcllt,", "tokens": ["Die", "See\u00b7le", ",", "noch", "so", "sch\u00f6n", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ist's eine w\u00fcste Seele,", "tokens": ["Ist's", "ei\u00b7ne", "w\u00fcs\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Die blicket einst als Schreckensbild", "tokens": ["Die", "bli\u00b7cket", "einst", "als", "Schre\u00b7ckens\u00b7bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Aus dem Spiegel ohne Fehle.", "tokens": ["Aus", "dem", "Spie\u00b7gel", "oh\u00b7ne", "Feh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Ihr aber, deren Seele licht,", "tokens": ["Ihr", "a\u00b7ber", ",", "de\u00b7ren", "See\u00b7le", "licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Dem\u00fct'ge, fromme Frauen!", "tokens": ["De\u00b7m\u00fct'\u00b7ge", ",", "from\u00b7me", "Frau\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Ihr werdet nach dem Tode nicht", "tokens": ["Ihr", "wer\u00b7det", "nach", "dem", "To\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Aus ird'schen Spiegeln schauen.", "tokens": ["Aus", "ird'\u00b7schen", "Spie\u00b7geln", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Ihr schwebet aus der Erde Nacht", "tokens": ["Ihr", "schwe\u00b7bet", "aus", "der", "Er\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Empor zur Himmelsklarheit,", "tokens": ["Em\u00b7por", "zur", "Him\u00b7mels\u00b7klar\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Schaut, was ihr hier geglaubt, gedacht,", "tokens": ["Schaut", ",", "was", "ihr", "hier", "ge\u00b7glaubt", ",", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Im Spiegel ew'ger Wahrheit.", "tokens": ["Im", "Spie\u00b7gel", "ew'\u00b7ger", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dies war aus alter Zeit ein Weib,", "tokens": ["Dies", "war", "aus", "al\u00b7ter", "Zeit", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch jetzt noch gibt es Frauen,", "tokens": ["Doch", "jetzt", "noch", "gibt", "es", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frauen, die emsig ihren Leib,", "tokens": ["Frau\u00b7en", ",", "die", "em\u00b7sig", "ih\u00b7ren", "Leib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch faul den Geist bebauen.", "tokens": ["Doch", "faul", "den", "Geist", "be\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie werdet, Eitle, ihr einmal", "tokens": ["Wie", "wer\u00b7det", ",", "Eit\u00b7le", ",", "ihr", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "$,", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach dem Tod aus Spiegeln blicken!", "tokens": ["Nach", "dem", "Tod", "aus", "Spie\u00b7geln", "bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In des aufgeb