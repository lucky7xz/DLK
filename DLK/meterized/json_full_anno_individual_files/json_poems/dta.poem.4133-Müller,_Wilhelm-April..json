{"dta.poem.4133": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "April.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wandl' ich in jeder Stunde Leib und Sinn:", "tokens": ["Wandl'", "ich", "in", "je\u00b7der", "Stun\u00b7de", "Leib", "und", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kaum wei\u00df ich selbst, wie ich beschaffen bin,", "tokens": ["Kaum", "wei\u00df", "ich", "selbst", ",", "wie", "ich", "be\u00b7schaf\u00b7fen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie sollen mich die fremden Leute fassen?", "tokens": ["Wie", "sol\u00b7len", "mich", "die", "frem\u00b7den", "Leu\u00b7te", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier werf' ich einen Schneeball durch die Gassen,", "tokens": ["Hier", "wer\u00b7f'", "ich", "ei\u00b7nen", "Schnee\u00b7ball", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Dort schweb' ich blau in jungen D\u00fcften hin,", "tokens": ["Dort", "schweb'", "ich", "blau", "in", "jun\u00b7gen", "D\u00fcf\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald streich' ich sanft der Sch\u00f6nen weiches Kinn,", "tokens": ["Bald", "streich'", "ich", "sanft", "der", "Sch\u00f6\u00b7nen", "wei\u00b7ches", "Kinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald sagen sie, ich w\u00e4re grob im Spa\u00dfen.", "tokens": ["Bald", "sa\u00b7gen", "sie", ",", "ich", "w\u00e4\u00b7re", "grob", "im", "Spa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gern wollt' ich dir noch Vieles von mir sagen,", "tokens": ["Gern", "wollt'", "ich", "dir", "noch", "Vie\u00b7les", "von", "mir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch dr\u00fcckt mich des Sonettes enges Band,", "tokens": ["Doch", "dr\u00fcckt", "mich", "des", "So\u00b7net\u00b7tes", "en\u00b7ges", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das mir die Muse um den Mund geschlagen.", "tokens": ["Das", "mir", "die", "Mu\u00b7se", "um", "den", "Mund", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie sprach: Ich kenne dich als ungezogen,", "tokens": ["Sie", "sprach", ":", "Ich", "ken\u00b7ne", "dich", "als", "un\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "KOUS", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und jener Herr hat in dem welschen Land", "tokens": ["Und", "je\u00b7ner", "Herr", "hat", "in", "dem", "wel\u00b7schen", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der besten Sitt' als Kavalier gepflogen.", "tokens": ["Der", "bes\u00b7ten", "Sitt'", "als", "Ka\u00b7va\u00b7lier", "ge\u00b7pflo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}