{"dta.poem.5946": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder C\u00f6rper wiederstehet", "tokens": ["Ein", "je\u00b7der", "C\u00f6r\u00b7per", "wie\u00b7ders\u00b7te\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dem C\u00f6rper, der ihm dr\u00fcckt. Indem nun alles voll,", "tokens": ["Dem", "C\u00f6r\u00b7per", ",", "der", "ihm", "dr\u00fcckt", ".", "In\u00b7dem", "nun", "al\u00b7les", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "KOUS", "ADV", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird der C\u00f6rper Trieb, da\u00df er nicht weiter gehet,", "tokens": ["So", "wird", "der", "C\u00f6r\u00b7per", "Trieb", ",", "da\u00df", "er", "nicht", "wei\u00b7ter", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von ihnen unter sich gehemmt. Jedennoch streben", "tokens": ["Von", "ih\u00b7nen", "un\u00b7ter", "sich", "ge\u00b7hemmt", ".", "Je\u00b7den\u00b7noch", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PRF", "VVPP", "$.", "ADV", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie alle, sich vom Mittel-Punct zu heben,", "tokens": ["Sie", "al\u00b7le", ",", "sich", "vom", "Mit\u00b7tel\u00b7Punct", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch halten, im geraden Lauff,", "tokens": ["Doch", "hal\u00b7ten", ",", "im", "ge\u00b7ra\u00b7den", "Lauff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie selbst durch ihren Druck einander auf;", "tokens": ["Sie", "selbst", "durch", "ih\u00b7ren", "Druck", "ein\u00b7an\u00b7der", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}