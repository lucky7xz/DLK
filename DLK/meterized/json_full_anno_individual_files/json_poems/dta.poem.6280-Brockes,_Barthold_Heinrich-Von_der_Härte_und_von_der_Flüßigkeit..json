{"dta.poem.6280": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn zum Exempel, wir im Glase Wasser sehn", "tokens": ["Wenn", "zum", "Ex\u00b7em\u00b7pel", ",", "wir", "im", "Gla\u00b7se", "Was\u00b7ser", "sehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "$,", "PPER", "APPRART", "NN", "NN", "VVINF"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gantz still und ohn Bewegung stehn;", "tokens": ["Gantz", "still", "und", "ohn", "Be\u00b7we\u00b7gung", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beliebe man nur frey viel tausend kleine Wellen,", "tokens": ["Be\u00b7lie\u00b7be", "man", "nur", "frey", "viel", "tau\u00b7send", "klei\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wodurch sich seine Masse theilt,", "tokens": ["Wo\u00b7durch", "sich", "sei\u00b7ne", "Mas\u00b7se", "theilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In selbigen sich vorzustellen,", "tokens": ["In", "sel\u00b7bi\u00b7gen", "sich", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PRF", "VVIZU", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Die zugerichtet sind, da\u00df jede weicht und eilt.", "tokens": ["Die", "zu\u00b7ge\u00b7rich\u00b7tet", "sind", ",", "da\u00df", "je\u00b7de", "weicht", "und", "eilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die kleinen glatten C\u00f6rper nun,", "tokens": ["Die", "klei\u00b7nen", "glat\u00b7ten", "C\u00f6r\u00b7per", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die nimmer ruhn,", "tokens": ["Die", "nim\u00b7mer", "ruhn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die lauffen fertig auf und nieder,", "tokens": ["Die", "lauf\u00b7fen", "fer\u00b7tig", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durchdringen sich, und wallen hin und wieder", "tokens": ["Durch\u00b7drin\u00b7gen", "sich", ",", "und", "wal\u00b7len", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "$,", "KON", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In langen V\u00f6gen fort. Sie stossen an den W\u00e4nden", "tokens": ["In", "lan\u00b7gen", "V\u00f6\u00b7gen", "fort", ".", "Sie", "stos\u00b7sen", "an", "den", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von ihrem Kercker stets, an allen Enden,", "tokens": ["Von", "ih\u00b7rem", "Ker\u00b7cker", "stets", ",", "an", "al\u00b7len", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und werden, ob das Aug\u2019 es gleich nicht sp\u00fchrt,", "tokens": ["Und", "wer\u00b7den", ",", "ob", "das", "Aug'", "es", "gleich", "nicht", "sp\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAINF", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Doch immer als ein Meer ger\u00fchrt.", "tokens": ["Doch", "im\u00b7mer", "als", "ein", "Meer", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}