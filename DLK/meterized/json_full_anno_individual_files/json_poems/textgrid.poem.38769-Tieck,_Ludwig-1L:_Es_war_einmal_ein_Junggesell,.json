{"textgrid.poem.38769": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war einmal ein Junggesell,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein Junggesell,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "Jung\u00b7ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der th\u00e4t hin fischen gehn,", "tokens": ["Der", "th\u00e4t", "hin", "fi\u00b7schen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wasser schienen klar und hell,", "tokens": ["Die", "Was\u00b7ser", "schie\u00b7nen", "klar", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne gar so sch\u00f6n,", "tokens": ["Die", "Son\u00b7ne", "gar", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er schaut wohl in die nasse Fluth,", "tokens": ["Er", "schaut", "wohl", "in", "die", "nas\u00b7se", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er denkt an sie und klagt und f\u00fchlt den Liebes-Muth.", "tokens": ["Er", "denkt", "an", "sie", "und", "klagt", "und", "f\u00fchlt", "den", "Lie\u00b7bes\u00b7Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und willst du mich mit Netzen stehlen?", "tokens": ["Und", "willst", "du", "mich", "mit", "Net\u00b7zen", "steh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So singt es aus dem Flu\u00df:", "tokens": ["So", "singt", "es", "aus", "dem", "Flu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Liebsten wollt' ich dich erw\u00e4hlen,", "tokens": ["Zum", "Liebs\u00b7ten", "wollt'", "ich", "dich", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm her, komm her zum Ku\u00df!", "tokens": ["Komm", "her", ",", "komm", "her", "zum", "Ku\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Er zieht das Netz mit gro\u00dfer Pein,", "tokens": ["Er", "zieht", "das", "Netz", "mit", "gro\u00b7\u00dfer", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schaut! da zappelt und lacht die Liebste drein.", "tokens": ["Und", "schaut", "!", "da", "zap\u00b7pelt", "und", "lacht", "die", "Liebs\u00b7te", "drein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Nackt f\u00e4llt sie ihm an seinen Mund,", "tokens": ["Nackt", "f\u00e4llt", "sie", "ihm", "an", "sei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und halst und druckt ihn sehr,", "tokens": ["Und", "halst", "und", "druckt", "ihn", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da war er froh und ganz gesund,", "tokens": ["Da", "war", "er", "froh", "und", "ganz", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klagte nimmer mehr,", "tokens": ["Und", "klag\u00b7te", "nim\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sankt Peter segnet ihm den Zug,", "tokens": ["Sankt", "Pe\u00b7ter", "seg\u00b7net", "ihm", "den", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er hat mi