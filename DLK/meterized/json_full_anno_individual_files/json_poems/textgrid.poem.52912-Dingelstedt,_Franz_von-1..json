{"textgrid.poem.52912": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allm\u00e4cht'ger Fr\u00fchling, deck mit deinen Ranken,", "tokens": ["All\u00b7m\u00e4cht'\u00b7ger", "Fr\u00fch\u00b7ling", ",", "deck", "mit", "dei\u00b7nen", "Ran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit deines Rasens Gr\u00fcn dies Tr\u00fcmmer zu,", "tokens": ["Mit", "dei\u00b7nes", "Ra\u00b7sens", "Gr\u00fcn", "dies", "Tr\u00fcm\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PDS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sing ein Volk von hoffnungslosen Kranken", "tokens": ["Und", "sing", "ein", "Volk", "von", "hoff\u00b7nungs\u00b7lo\u00b7sen", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch deine Nachtigallen s\u00fc\u00df zur Ruh'!", "tokens": ["Durch", "dei\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len", "s\u00fc\u00df", "zur", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vergeude nicht an andre deine Sch\u00e4tze,", "tokens": ["Ver\u00b7geu\u00b7de", "nicht", "an", "and\u00b7re", "dei\u00b7ne", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spar deinen Lebenshauch, hier tut er not;", "tokens": ["Spar", "dei\u00b7nen", "Le\u00b7bens\u00b7hauch", ",", "hier", "tut", "er", "not", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O komm und weine auf die w\u00fcsten Pl\u00e4tze,", "tokens": ["O", "komm", "und", "wei\u00b7ne", "auf", "die", "w\u00fcs\u00b7ten", "Pl\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Brand und Kampf und Pest und Mord gedroht!", "tokens": ["Wo", "Brand", "und", "Kampf", "und", "Pest", "und", "Mord", "ge\u00b7droht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier stand ein Haus, wo jetzt auf morschen St\u00e4ndern", "tokens": ["Hier", "stand", "ein", "Haus", ",", "wo", "jetzt", "auf", "mor\u00b7schen", "St\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Truggebild sich haltlos wiegt und streckt,", "tokens": ["Ein", "Trug\u00b7ge\u00b7bild", "sich", "halt\u00b7los", "wiegt", "und", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier bl\u00fchten Saaten, wo auf brachen L\u00e4ndern", "tokens": ["Hier", "bl\u00fch\u00b7ten", "Saa\u00b7ten", ",", "wo", "auf", "bra\u00b7chen", "L\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gestr\u00fcpp und Schlingkraut heut den Boden deckt.", "tokens": ["Ge\u00b7str\u00fcpp", "und", "Schling\u00b7kraut", "heut", "den", "Bo\u00b7den", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wes war die Hand, die unter sichre D\u00e4cher", "tokens": ["Wes", "war", "die", "Hand", ",", "die", "un\u00b7ter", "sich\u00b7re", "D\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zuerst die Fackel der Zerst\u00f6rung hielt,", "tokens": ["Zu\u00b7erst", "die", "Fa\u00b7ckel", "der", "Zer\u00b7st\u00f6\u00b7rung", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die in dem Innren friedlicher Gem\u00e4cher", "tokens": ["Die", "in", "dem", "Inn\u00b7ren", "fried\u00b7li\u00b7cher", "Ge\u00b7m\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf treue M\u00e4nner m\u00f6rderisch gezielt?", "tokens": ["Auf", "treu\u00b7e", "M\u00e4n\u00b7ner", "m\u00f6r\u00b7de\u00b7risch", "ge\u00b7zielt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer zog die St\u00fctzen eines sichren Lebens", "tokens": ["Wer", "zog", "die", "St\u00fct\u00b7zen", "ei\u00b7nes", "sich\u00b7ren", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Volke fort und brach der V\u00e4ter Eid?", "tokens": ["Dem", "Vol\u00b7ke", "fort", "und", "brach", "der", "V\u00e4\u00b7ter", "Eid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer schlug die Kraft des edlen Gegenstrebens,", "tokens": ["Wer", "schlug", "die", "Kraft", "des", "ed\u00b7len", "Ge\u00b7gen\u00b7stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Lug und Trug, durch Zwang und Drang und Leid?", "tokens": ["Durch", "Lug", "und", "Trug", ",", "durch", "Zwang", "und", "Drang", "und", "Leid", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Verbotne Fragen! ... Trage in der Stille,", "tokens": ["Ver\u00b7bot\u00b7ne", "Fra\u00b7gen", "!", "...", "Tra\u00b7ge", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was zu ertragen sich ein Volk entschlo\u00df;", "tokens": ["Was", "zu", "er\u00b7tra\u00b7gen", "sich", "ein", "Volk", "ent\u00b7schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unendliches vermag ein ehrner Wille,", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", "ver\u00b7mag", "ein", "ehr\u00b7ner", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ach! die Zeit tr\u00e4gt b\u00f6se Frucht im Scho\u00df.", "tokens": ["Und", "ach", "!", "die", "Zeit", "tr\u00e4gt", "b\u00f6\u00b7se", "Frucht", "im", "Scho\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "NN", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sieh, wie gebeugt die weisen H\u00e4upter alle,", "tokens": ["Sieh", ",", "wie", "ge\u00b7beugt", "die", "wei\u00b7sen", "H\u00e4up\u00b7ter", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVPP", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieh, wie zerrissen jede Kraft im Staat,", "tokens": ["Sieh", ",", "wie", "zer\u00b7ris\u00b7sen", "je\u00b7de", "Kraft", "im", "Staat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVPP", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es schwankt das Land, gleich einem irren Balle,", "tokens": ["Es", "schwankt", "das", "Land", ",", "gleich", "ei\u00b7nem", "ir\u00b7ren", "Bal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Pol zu Pol und wei\u00df sich keinen Rat.", "tokens": ["Von", "Pol", "zu", "Pol", "und", "wei\u00df", "sich", "kei\u00b7nen", "Rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Parteiung schleicht in seinem Heiligtume", "tokens": ["Par\u00b7tei\u00b7ung", "schleicht", "in", "sei\u00b7nem", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gef\u00e4hrlich um, Mut und Vertrauen wankt;", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "um", ",", "Mut", "und", "Ver\u00b7trau\u00b7en", "wankt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Weh, armes Volk, weh deinem alten Ruhme,", "tokens": ["Weh", ",", "ar\u00b7mes", "Volk", ",", "weh", "dei\u00b7nem", "al\u00b7ten", "Ruh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Herz ist hart getroffen und erkrankt.", "tokens": ["Dein", "Herz", "ist", "hart", "ge\u00b7trof\u00b7fen", "und", "er\u00b7krankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Aufw\u00e4rts die Blicke aus dem n\u00e4chsten Grauen", "tokens": ["Auf\u00b7w\u00e4rts", "die", "Bli\u00b7cke", "aus", "dem", "n\u00e4chs\u00b7ten", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Gegenwart; nicht ewig w\u00e4hrt die Nacht!", "tokens": ["Der", "Ge\u00b7gen\u00b7wart", ";", "nicht", "e\u00b7wig", "w\u00e4hrt", "die", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PTKNEG", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer wei\u00df, wie bald die Himmel wieder blauen?", "tokens": ["Wer", "wei\u00df", ",", "wie", "bald", "die", "Him\u00b7mel", "wie\u00b7der", "blau\u00b7en", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer wei\u00df, wie fr\u00fch ein deutsches Volk erwacht?", "tokens": ["Wer", "wei\u00df", ",", "wie", "fr\u00fch", "ein", "deut\u00b7sches", "Volk", "er\u00b7wacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Der Fr\u00fchling ist zur\u00fcck ins Land geflogen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "zu\u00b7r\u00fcck", "ins", "Land", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihn hemmte weder Maut noch Polizei,", "tokens": ["Ihn", "hemm\u00b7te", "we\u00b7der", "Maut", "noch", "Po\u00b7li\u00b7zei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Frei schreitet er einher und ruft den Wogen,", "tokens": ["Frei", "schrei\u00b7tet", "er", "ein\u00b7her", "und", "ruft", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den W\u00e4ldern zu, den Wiesen: Ihr seid frei!", "tokens": ["Den", "W\u00e4l\u00b7dern", "zu", ",", "den", "Wie\u00b7sen", ":", "Ihr", "seid", "frei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und tausend Stimmen, die im Chor erwidern,", "tokens": ["Und", "tau\u00b7send", "Stim\u00b7men", ",", "die", "im", "Chor", "er\u00b7wi\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tausend Kr\u00e4fte, die sich neu geregt;", "tokens": ["Und", "tau\u00b7send", "Kr\u00e4f\u00b7te", ",", "die", "sich", "neu", "ge\u00b7regt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rt nur, wie ihres Heeres schmucken Gliedern", "tokens": ["H\u00f6rt", "nur", ",", "wie", "ih\u00b7res", "Hee\u00b7res", "schmu\u00b7cken", "Glie\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Lerche mahnend die Reveille schl\u00e4gt!", "tokens": ["Die", "Ler\u00b7che", "mah\u00b7nend", "die", "Re\u00b7veil\u00b7le", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Getrost, getrost! Dein Fr\u00fchling auch wird kommen.", "tokens": ["Ge\u00b7trost", ",", "ge\u00b7trost", "!", "Dein", "Fr\u00fch\u00b7ling", "auch", "wird", "kom\u00b7men", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$.", "PPOSAT", "NN", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht, du ahnst es nicht, ist er schon nah;", "tokens": ["Viel\u00b7leicht", ",", "du", "ahnst", "es", "nicht", ",", "ist", "er", "schon", "nah", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "PT