{"dta.poem.23484": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "S Ein volck mit lieb/ vnd mit lob seine", "tokens": ["S", "Ein", "volck", "mit", "lieb", "/", "vnd", "mit", "lob", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ADJD", "$(", "KON", "APPR", "NN", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "brust", "tokens": ["brust"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Thut seine Embsigkeit erf\u00fcllen/", "tokens": ["Thut", "sei\u00b7ne", "Emb\u00b7sig\u00b7keit", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dan keines tags/ nein/ keiner stund verlust", "tokens": ["Dan", "kei\u00b7nes", "tags", "/", "nein", "/", "kei\u00b7ner", "stund", "ver\u00b7lust"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$(", "PTKANT", "$(", "PIS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hat je beth\u00f6ret seinen willen.", "tokens": ["Hat", "je", "be\u00b7th\u00f6\u00b7ret", "sei\u00b7nen", "wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es kan kein F\u00fcrst de\u0303 Staht seiner Landschaft/", "tokens": ["Es", "kan", "kein", "F\u00fcrst", "d\u1ebd", "Staht", "sei\u00b7ner", "Land\u00b7schaft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Vnd seines volcks gem\u00fcht vn\u0303 aigenschaft", "tokens": ["Vnd", "sei\u00b7nes", "volcks", "ge\u00b7m\u00fcht", "v\u00f1", "ai\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So wol sehen/ wissen/ bekehren/", "tokens": ["So", "wol", "se\u00b7hen", "/", "wis\u00b7sen", "/", "be\u00b7keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Vnd mit guten exempeln lehren:", "tokens": ["Vnd", "mit", "gu\u00b7ten", "ex\u00b7em\u00b7peln", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Kein F\u00fcrst kan so m\u00fchsam vnd wei\u00df", "tokens": ["Kein", "F\u00fcrst", "kan", "so", "m\u00fch\u00b7sam", "vnd", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Der nohtdurft nach die \u00c4mpter selbs be-", "tokens": ["Der", "noht\u00b7durft", "nach", "die", "\u00c4mp\u00b7ter", "selbs", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "setzen/", "tokens": ["set\u00b7zen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Die gesch\u00e4ften mit sorg vnd flei\u00df", "tokens": ["Die", "ge\u00b7sch\u00e4f\u00b7ten", "mit", "sorg", "vnd", "flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Verrichten bald/ vnd den verdienst erg\u00f6tze\u0303;", "tokens": ["Ver\u00b7rich\u00b7ten", "bald", "/", "vnd", "den", "ver\u00b7dienst", "er\u00b7g\u00f6tz\u1ebd", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Noch mit Ruh-verachtender hand", "tokens": ["Noch", "mit", "Ruh\u00b7ver\u00b7ach\u00b7ten\u00b7der", "hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Zujeder zeit das stewr so gewi\u00df halten/", "tokens": ["Zu\u00b7je\u00b7der", "zeit", "das", "stewr", "so", "ge\u00b7wi\u00df", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Vnd alles so embsig verwalten/", "tokens": ["Vnd", "al\u00b7les", "so", "emb\u00b7sig", "ver\u00b7wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADJD", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "Wie Er gantz gleich an m\u00fch/ flei\u00df vn\u0303 versta\u0303d.", "tokens": ["Wie", "Er", "gantz", "gleich", "an", "m\u00fch", "/", "flei\u00df", "v\u00f1", "ver\u00b7st\u00e3d", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPR", "ADJD", "$(", "FM.la", "FM.la", "FM.la", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}