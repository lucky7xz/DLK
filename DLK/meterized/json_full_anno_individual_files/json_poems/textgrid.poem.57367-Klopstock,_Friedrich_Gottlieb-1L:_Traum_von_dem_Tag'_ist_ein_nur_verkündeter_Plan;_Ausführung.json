 "pos": ["APPR", "PPOSAT", "NN", "$.", "NE", "VAFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Viel zu traurig verwandelt, gemacht aus den Rechten des Menschen", "tokens": ["Viel", "zu", "trau\u00b7rig", "ver\u00b7wan\u00b7delt", ",", "ge\u00b7macht", "aus", "den", "Rech\u00b7ten", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "VVPP", "$,", "VVPP", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Rechte des Kamul! (Der Gott", "tokens": ["Rech\u00b7te", "des", "Ka\u00b7mul", "!", "(", "Der", "Gott"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "D\u00fcrstete Menschenopfer) Zu diesem Barbarischen stimmet", "tokens": ["D\u00fcrs\u00b7te\u00b7te", "Men\u00b7schen\u00b7op\u00b7fer", ")", "Zu", "die\u00b7sem", "Bar\u00b7ba\u00b7ri\u00b7schen", "stim\u00b7met"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "APPR", "PDAT", "NN", "VVFIN"], "meter": "+--+-+----+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Nicht der Anmuth leisester Laut,", "tokens": ["Nicht", "der", "An\u00b7muth", "lei\u00b7ses\u00b7ter", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Nicht der Grazien; sie hat lang schon der Kam\u00fclottide", "tokens": ["Nicht", "der", "Gra\u00b7zi\u00b7en", ";", "sie", "hat", "lang", "schon", "der", "Ka\u00b7m\u00fc\u00b7lot\u00b7ti\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "ADV", "ART", "NN"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.38": {"text": "Angespien, und gepeitscht!", "tokens": ["An\u00b7ge\u00b7spi\u00b7en", ",", "und", "ge\u00b7peitscht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "\u00bbnun so schaue denn um, und starr' auf die Tr\u00fcmmern. Dort lag einst", "tokens": ["\u00bb", "nun", "so", "schau\u00b7e", "denn", "um", ",", "und", "starr'", "auf", "die", "Tr\u00fcm\u00b7mern", ".", "Dort", "lag", "einst"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "ADV"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.40": {"text": "Eine wimmelnde t\u00f6nende Stadt,", "tokens": ["Ei\u00b7ne", "wim\u00b7meln\u00b7de", "t\u00f6\u00b7nen\u00b7de", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.41": {"text": "Voll von B\u00fcrgergl\u00fcck. Die Pest kam. Die mit der Schaufel", "tokens": ["Voll", "von", "B\u00fcr\u00b7ger\u00b7gl\u00fcck", ".", "Die", "Pest", "kam", ".", "Die", "mit", "der", "Schau\u00b7fel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "$.", "ART", "NN", "VVFIN", "$.", "ART", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Raubten bald, und begruben nicht mehr.", "tokens": ["Raub\u00b7ten", "bald", ",", "und", "be\u00b7gru\u00b7ben", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "w