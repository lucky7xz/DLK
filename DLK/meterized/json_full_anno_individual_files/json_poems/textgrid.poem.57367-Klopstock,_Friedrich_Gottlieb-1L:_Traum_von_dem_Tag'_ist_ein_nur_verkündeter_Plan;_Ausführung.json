 ["NN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.18": {"text": "Schwindend, ein schleichend Gespenst?", "tokens": ["Schwin\u00b7dend", ",", "ein", "schlei\u00b7chend", "Ge\u00b7spenst", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJD", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Wurde Weises so ganz zu Th\u00f6richtem? Wurde die Menschheit", "tokens": ["Wur\u00b7de", "Wei\u00b7ses", "so", "ganz", "zu", "Th\u00f6\u00b7rich\u00b7tem", "?", "Wur\u00b7de", "die", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "ADV", "APPR", "NN", "$.", "VAFIN", "ART", "NN"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Jemals also entmenscht?", "tokens": ["Je\u00b7mals", "al\u00b7so", "ent\u00b7menscht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "\u00bbdrache ward der Gott, den um Heilung Hesperien anrief!", "tokens": ["\u00bb", "dra\u00b7che", "ward", "der", "Gott", ",", "den", "um", "Hei\u00b7lung", "Hes\u00b7pe\u00b7ri\u00b7en", "an\u00b7rief", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Jupiter Stier!\u00ab", "tokens": ["Ju\u00b7pi\u00b7ter", "Stier", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Jupiter war denn ein Gott; doch was war der Despot des Olympus?", "tokens": ["Ju\u00b7pi\u00b7ter", "war", "denn", "ein", "Gott", ";", "doch", "was", "war", "der", "Des\u00b7pot", "des", "O\u00b7lym\u00b7pus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "$.", "KON", "PWS", "VAFIN", "ART", "NN", "ART", "NE", "$."], "meter": "+--+--+--+--+-+--", "measure": "dactylic.tetra.plus"}, "line.24": {"text": "Ihm verderbte der Stier nicht zu viel.", "tokens": ["Ihm", "ver\u00b7derb\u00b7te", "der", "Stier", "nicht", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.25": {"text": "\u00bbsieh, dort weilet er, schaft der erfindende K\u00fcnstler. Er will nicht", "tokens": ["\u00bb", "sieh", ",", "dort", "wei\u00b7let", "er", ",", "schaft", "der", "er\u00b7fin\u00b7den\u00b7de", "K\u00fcnst\u00b7ler", ".", "Er", "will", "nicht"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "$,", "ADV", "VVFIN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PTKNEG"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Werden Parrasios, Angelo nicht:", "tokens": ["Wer\u00b7den", "Par\u00b7ra\u00b7si\u00b7os", ",", "An\u00b7ge\u00b7lo", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "NE", "PTKNEG", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.27": {"text": "Will Er Selber seyn! Das Meisterwerk ist vollendet!", "tokens": ["Will", "Er", "Sel\u00b7ber", "seyn", "!", "Das", "Meis\u00b7ter\u00b7werk", "ist", "voll\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", 