{"textgrid.poem.57367": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Traum von dem Tag' ist ein nur verk\u00fcndeter Plan; Ausf\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traum von dem Tag' ist ein nur verk\u00fcndeter Plan; Ausf\u00fchrung", "tokens": ["Traum", "von", "dem", "Tag'", "ist", "ein", "nur", "ver\u00b7k\u00fcn\u00b7de\u00b7ter", "Plan", ";", "Aus\u00b7f\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "ART", "ADV", "ADJA", "NN", "$.", "NN"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ist der erwachte, goldene Tag.", "tokens": ["Ist", "der", "er\u00b7wach\u00b7te", ",", "gol\u00b7de\u00b7ne", "Tag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schon begann f\u00fcr die Franken die Morgenr\u00f6the zu d\u00e4mmern,", "tokens": ["Schon", "be\u00b7gann", "f\u00fcr", "die", "Fran\u00b7ken", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "zu", "d\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wehete Schauer die Fr\u00fche; da ward", "tokens": ["We\u00b7he\u00b7te", "Schau\u00b7er", "die", "Fr\u00fc\u00b7he", ";", "da", "ward"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "ADV", "VAFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Selber der Grund des menschlichsten Plans zerst\u00f6ret! Von der Nacht", "tokens": ["Sel\u00b7ber", "der", "Grund", "des", "menschlichs\u00b7ten", "Plans", "zer\u00b7st\u00f6\u00b7ret", "!", "Von", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Rede, wer kann.", "tokens": ["Re\u00b7de", ",", "wer", "kann", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Steht mir, Bemerkende, bey, entdeckt: Ob jemals was gleich war", "tokens": ["Steht", "mir", ",", "Be\u00b7mer\u00b7ken\u00b7de", ",", "bey", ",", "ent\u00b7deckt", ":", "Ob", "je\u00b7mals", "was", "gleich", "war"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPR", "$,", "VVPP", "$.", "KOUS", "ADV", "PWS", "ADV", "VAFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Dem, das Schmach den Franken jetzt ist,", "tokens": ["Dem", ",", "das", "Schmach", "den", "Fran\u00b7ken", "jetzt", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "NN", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Seyn wird, und so, wie laut es auch preise die eherne Unscham,", "tokens": ["Seyn", "wird", ",", "und", "so", ",", "wie", "laut", "es", "auch", "prei\u00b7se", "die", "e\u00b7her\u00b7ne", "Un\u00b7scham", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "$,", "KON", "ADV", "$,", "PWAV", "ADJD", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Treiben auf immer im Strome der Zeit.", "tokens": ["Trei\u00b7ben", "auf", "im\u00b7mer", "im", "Stro\u00b7me", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Wenn die volle Gewissheit zeugt, und von Gr\u00f6sse der Unthat;", "tokens": ["Wenn", "die", "vol\u00b7le", "Ge\u00b7wiss\u00b7heit", "zeugt", ",", "und", "von", "Gr\u00f6s\u00b7se", "der", "Un\u00b7that", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "APPR", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+--++", "measure": "anapaest.di.plus"}, "line.12": {"text": "Ewigen diese Geschicht', und Gesang.", "tokens": ["E\u00b7wi\u00b7gen", "die\u00b7se", "Ge\u00b7schicht'", ",", "und", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "PDAT", "NN", "$,", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.13": {"text": "Sucht in der Welt der Fabel, Bemerkende, sucht in der wahren:", "tokens": ["Sucht", "in", "der", "Welt", "der", "Fa\u00b7bel", ",", "Be\u00b7mer\u00b7ken\u00b7de", ",", "sucht", "in", "der", "wah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Aber entdeckt!", "tokens": ["A\u00b7ber", "ent\u00b7deckt", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Ward vortrefliches je so ganz entheiligt? Erhabnes,", "tokens": ["Ward", "vor\u00b7tre\u00b7fli\u00b7ches", "je", "so", "ganz", "en\u00b7thei\u00b7ligt", "?", "Er\u00b7hab\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADV", "ADV", "ADV", "VVPP", "$.", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Sank es jemals so tief?", "tokens": ["Sank", "es", "je\u00b7mals", "so", "tief", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Schrumpfte so sehr die Sch\u00f6nheit ein, von der Eiterung todtbleich,", "tokens": ["Schrumpf\u00b7te", "so", "sehr", "die", "Sch\u00f6n\u00b7heit", "ein", ",", "von", "der", "Ei\u00b7te\u00b7rung", "todt\u00b7bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.18": {"text": "Schwindend, ein schleichend Gespenst?", "tokens": ["Schwin\u00b7dend", ",", "ein", "schlei\u00b7chend", "Ge\u00b7spenst", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJD", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Wurde Weises so ganz zu Th\u00f6richtem? Wurde die Menschheit", "tokens": ["Wur\u00b7de", "Wei\u00b7ses", "so", "ganz", "zu", "Th\u00f6\u00b7rich\u00b7tem", "?", "Wur\u00b7de", "die", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "ADV", "APPR", "NN", "$.", "VAFIN", "ART", "NN"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Jemals also entmenscht?", "tokens": ["Je\u00b7mals", "al\u00b7so", "ent\u00b7menscht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "\u00bbdrache ward der Gott, den um Heilung Hesperien anrief!", "tokens": ["\u00bb", "dra\u00b7che", "ward", "der", "Gott", ",", "den", "um", "Hei\u00b7lung", "Hes\u00b7pe\u00b7ri\u00b7en", "an\u00b7rief", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Jupiter Stier!\u00ab", "tokens": ["Ju\u00b7pi\u00b7ter", "Stier", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Jupiter war denn ein Gott; doch was war der Despot des Olympus?", "tokens": ["Ju\u00b7pi\u00b7ter", "war", "denn", "ein", "Gott", ";", "doch", "was", "war", "der", "Des\u00b7pot", "des", "O\u00b7lym\u00b7pus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "$.", "KON", "PWS", "VAFIN", "ART", "NN", "ART", "NE", "$."], "meter": "+--+--+--+--+-+--", "measure": "dactylic.tetra.plus"}, "line.24": {"text": "Ihm verderbte der Stier nicht zu viel.", "tokens": ["Ihm", "ver\u00b7derb\u00b7te", "der", "Stier", "nicht", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.25": {"text": "\u00bbsieh, dort weilet er, schaft der erfindende K\u00fcnstler. Er will nicht", "tokens": ["\u00bb", "sieh", ",", "dort", "wei\u00b7let", "er", ",", "schaft", "der", "er\u00b7fin\u00b7den\u00b7de", "K\u00fcnst\u00b7ler", ".", "Er", "will", "nicht"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "$,", "ADV", "VVFIN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PTKNEG"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Werden Parrasios, Angelo nicht:", "tokens": ["Wer\u00b7den", "Par\u00b7ra\u00b7si\u00b7os", ",", "An\u00b7ge\u00b7lo", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "NE", "PTKNEG", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.27": {"text": "Will Er Selber seyn! Das Meisterwerk ist vollendet!", "tokens": ["Will", "Er", "Sel\u00b7ber", "seyn", "!", "Das", "Meis\u00b7ter\u00b7werk", "ist", "voll\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.28": {"text": "Aber den gl\u00fchenden fasst", "tokens": ["A\u00b7ber", "den", "gl\u00fc\u00b7hen\u00b7den", "fasst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "Schnell was, wie Zaubergewalt; er bildet um, die Gestalten", "tokens": ["Schnell", "was", ",", "wie", "Zau\u00b7ber\u00b7ge\u00b7walt", ";", "er", "bil\u00b7det", "um", ",", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PWS", "$,", "PWAV", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Werden wilde Phantome des Wahns!\u00ab", "tokens": ["Wer\u00b7den", "wil\u00b7de", "Phan\u00b7to\u00b7me", "des", "Wahns", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Schweiget von dem, was die Kunst gebar; die Vergleichung ent\u00e4hnlicht", "tokens": ["Schwei\u00b7get", "von", "dem", ",", "was", "die", "Kunst", "ge\u00b7bar", ";", "die", "Ver\u00b7glei\u00b7chung", "ent\u00b7\u00e4hn\u00b7licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "$,", "PRELS", "ART", "NN", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Durch ihr Heiteres: Gallien hat", "tokens": ["Durch", "ihr", "Hei\u00b7te\u00b7res", ":", "Gal\u00b7li\u00b7en", "hat"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NE", "VAFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Viel zu traurig verwandelt, gemacht aus den Rechten des Menschen", "tokens": ["Viel", "zu", "trau\u00b7rig", "ver\u00b7wan\u00b7delt", ",", "ge\u00b7macht", "aus", "den", "Rech\u00b7ten", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "VVPP", "$,", "VVPP", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Rechte des Kamul! (Der Gott", "tokens": ["Rech\u00b7te", "des", "Ka\u00b7mul", "!", "(", "Der", "Gott"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "D\u00fcrstete Menschenopfer) Zu diesem Barbarischen stimmet", "tokens": ["D\u00fcrs\u00b7te\u00b7te", "Men\u00b7schen\u00b7op\u00b7fer", ")", "Zu", "die\u00b7sem", "Bar\u00b7ba\u00b7ri\u00b7schen", "stim\u00b7met"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "APPR", "PDAT", "NN", "VVFIN"], "meter": "+--+-+----+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Nicht der Anmuth leisester Laut,", "tokens": ["Nicht", "der", "An\u00b7muth", "lei\u00b7ses\u00b7ter", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Nicht der Grazien; sie hat lang schon der Kam\u00fclottide", "tokens": ["Nicht", "der", "Gra\u00b7zi\u00b7en", ";", "sie", "hat", "lang", "schon", "der", "Ka\u00b7m\u00fc\u00b7lot\u00b7ti\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "ADV", "ART", "NN"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.38": {"text": "Angespien, und gepeitscht!", "tokens": ["An\u00b7ge\u00b7spi\u00b7en", ",", "und", "ge\u00b7peitscht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "\u00bbnun so schaue denn um, und starr' auf die Tr\u00fcmmern. Dort lag einst", "tokens": ["\u00bb", "nun", "so", "schau\u00b7e", "denn", "um", ",", "und", "starr'", "auf", "die", "Tr\u00fcm\u00b7mern", ".", "Dort", "lag", "einst"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "ADV"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.40": {"text": "Eine wimmelnde t\u00f6nende Stadt,", "tokens": ["Ei\u00b7ne", "wim\u00b7meln\u00b7de", "t\u00f6\u00b7nen\u00b7de", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.41": {"text": "Voll von B\u00fcrgergl\u00fcck. Die Pest kam. Die mit der Schaufel", "tokens": ["Voll", "von", "B\u00fcr\u00b7ger\u00b7gl\u00fcck", ".", "Die", "Pest", "kam", ".", "Die", "mit", "der", "Schau\u00b7fel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "$.", "ART", "NN", "VVFIN", "$.", "ART", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Raubten bald, und begruben nicht mehr.", "tokens": ["Raub\u00b7ten", "bald", ",", "und", "be\u00b7gru\u00b7ben", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.43": {"text": "Da die raubten, nicht mehr begruben; flog der Belagrer", "tokens": ["Da", "die", "raub\u00b7ten", ",", "nicht", "mehr", "be\u00b7gru\u00b7ben", ";", "flog", "der", "Be\u00b7lag\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "PTKNEG", "ADV", "VVINF", "$.", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.44": {"text": "Gl\u00fchend Geschoss,", "tokens": ["Gl\u00fc\u00b7hend", "Ge\u00b7schoss", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.45": {"text": "Z\u00fcndete Haus und Thurm, und mit den gesunkenen Th\u00fcrmen", "tokens": ["Z\u00fcn\u00b7de\u00b7te", "Haus", "und", "Thurm", ",", "und", "mit", "den", "ge\u00b7sun\u00b7ke\u00b7nen", "Th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.46": {"text": "Schwieg das Todtengel\u00e4ut.", "tokens": ["Schwieg", "das", "Tod\u00b7ten\u00b7ge\u00b7l\u00e4ut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.47": {"text": "Da das Todtengel\u00e4ut verstumt war; st\u00fcrmten sie, w\u00fcrgten,", "tokens": ["Da", "das", "Tod\u00b7ten\u00b7ge\u00b7l\u00e4ut", "ver\u00b7stumt", "war", ";", "st\u00fcrm\u00b7ten", "sie", ",", "w\u00fcrg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$.", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.48": {"text": "Schnellere Seuche, Mutter und Kind!", "tokens": ["Schnel\u00b7le\u00b7re", "Seu\u00b7che", ",", "Mut\u00b7ter", "und", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.49": {"text": "Bruder bey Bruder, den Sohn, den Br\u00e4utigam, neben dem Vater;", "tokens": ["Bru\u00b7der", "bey", "Bru\u00b7der", ",", "den", "Sohn", ",", "den", "Br\u00e4u\u00b7ti\u00b7gam", ",", "ne\u00b7ben", "dem", "Va\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ART", "NN", "$,", "ART", "NE", "$,", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.50": {"text": "Aber langsam die Braut!\u00ab", "tokens": ["A\u00b7ber", "lang\u00b7sam", "die", "Braut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.51": {"text": "Weh die gl\u00fcckliche Stadt ist gewesen, die fr\u00f6hliche! Wendet", "tokens": ["Weh", "die", "gl\u00fcck\u00b7li\u00b7che", "Stadt", "ist", "ge\u00b7we\u00b7sen", ",", "die", "fr\u00f6h\u00b7li\u00b7che", "!", "Wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "VAPP", "$,", "ART", "ADJA", "$.", "VVFIN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.52": {"text": "Eure Thr\u00e4nen, und schaut nicht zur\u00fcck.", "tokens": ["Eu\u00b7re", "Thr\u00e4\u00b7nen", ",", "und", "schaut", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.53": {"text": "\u00bbach wir schauen immer noch hin, es gebricht uns an St\u00e4rke,", "tokens": ["\u00bb", "ach", "wir", "schau\u00b7en", "im\u00b7mer", "noch", "hin", ",", "es", "ge\u00b7bricht", "uns", "an", "St\u00e4r\u00b7ke", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.54": {"text": "Wegzuwenden den Blick.", "tokens": ["Weg\u00b7zu\u00b7wen\u00b7den", "den", "Blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.55": {"text": "Niedergest\u00fcrzt, vernichtet die Obergewalt des Gesetzes,", "tokens": ["Nie\u00b7der\u00b7ge\u00b7st\u00fcrzt", ",", "ver\u00b7nich\u00b7tet", "die", "O\u00b7ber\u00b7ge\u00b7walt", "des", "Ge\u00b7set\u00b7zes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.56": {"text": "Durch das herschende Schwert!", "tokens": ["Durch", "das", "her\u00b7schen\u00b7de", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.57": {"text": "Schreyender Widerspruch: Freystaat, und ein Staat, der auf Mord sich", "tokens": ["Schre\u00b7yen\u00b7der", "Wi\u00b7der\u00b7spruch", ":", "Frey\u00b7staat", ",", "und", "ein", "Staat", ",", "der", "auf", "Mord", "sich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "$,", "KON", "ART", "NN", "$,", "PRELS", "APPR", "NN", "PRF"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.58": {"text": "Gr\u00fcndet! Wer nent", "tokens": ["Gr\u00fcn\u00b7det", "!", "Wer", "nent"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "PWS", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.59": {"text": "Diess Republikgeripp mit Einem Namen? Entflieh nicht,", "tokens": ["Diess", "Re\u00b7pub\u00b7lik\u00b7ge\u00b7ripp", "mit", "Ei\u00b7nem", "Na\u00b7men", "?", "Ent\u00b7flieh", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$.", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.60": {"text": "Rede, nenne mit uns!", "tokens": ["Re\u00b7de", ",", "nen\u00b7ne", "mit", "uns", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.61": {"text": "Henkerstaat! sey der erste Nam', und der andere Sklaven-", "tokens": ["Hen\u00b7ker\u00b7staat", "!", "sey", "der", "ers\u00b7te", "Nam'", ",", "und", "der", "an\u00b7de\u00b7re", "Skla\u00b7ven"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "TRUNC"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.62": {"text": "Staat!\u00ab Der nennet auch, so verstumt.", "tokens": ["Staat", "!", "\u00ab", "Der", "nen\u00b7net", "auch", ",", "so", "ver\u00b7stumt", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PDS", "VVFIN", "ADV", "$,", "ADV", "VVPP", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Traum von dem Tag' ist ein nur verk\u00fcndeter Plan; Ausf\u00fchrung", "tokens": ["Traum", "von", "dem", "Tag'", "ist", "ein", "nur", "ver\u00b7k\u00fcn\u00b7de\u00b7ter", "Plan", ";", "Aus\u00b7f\u00fch\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "ART", "ADV", "ADJA", "NN", "$.", "NN"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ist der erwachte, goldene Tag.", "tokens": ["Ist", "der", "er\u00b7wach\u00b7te", ",", "gol\u00b7de\u00b7ne", "Tag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schon begann f\u00fcr die Franken die Morgenr\u00f6the zu d\u00e4mmern,", "tokens": ["Schon", "be\u00b7gann", "f\u00fcr", "die", "Fran\u00b7ken", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "zu", "d\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wehete Schauer die Fr\u00fche; da ward", "tokens": ["We\u00b7he\u00b7te", "Schau\u00b7er", "die", "Fr\u00fc\u00b7he", ";", "da", "ward"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "ADV", "VAFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Selber der Grund des menschlichsten Plans zerst\u00f6ret! Von der Nacht", "tokens": ["Sel\u00b7ber", "der", "Grund", "des", "menschlichs\u00b7ten", "Plans", "zer\u00b7st\u00f6\u00b7ret", "!", "Von", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Rede, wer kann.", "tokens": ["Re\u00b7de", ",", "wer", "kann", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Steht mir, Bemerkende, bey, entdeckt: Ob jemals was gleich war", "tokens": ["Steht", "mir", ",", "Be\u00b7mer\u00b7ken\u00b7de", ",", "bey", ",", "ent\u00b7deckt", ":", "Ob", "je\u00b7mals", "was", "gleich", "war"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPR", "$,", "VVPP", "$.", "KOUS", "ADV", "PWS", "ADV", "VAFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Dem, das Schmach den Franken jetzt ist,", "tokens": ["Dem", ",", "das", "Schmach", "den", "Fran\u00b7ken", "jetzt", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "NN", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Seyn wird, und so, wie laut es auch preise die eherne Unscham,", "tokens": ["Seyn", "wird", ",", "und", "so", ",", "wie", "laut", "es", "auch", "prei\u00b7se", "die", "e\u00b7her\u00b7ne", "Un\u00b7scham", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "$,", "KON", "ADV", "$,", "PWAV", "ADJD", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Treiben auf immer im Strome der Zeit.", "tokens": ["Trei\u00b7ben", "auf", "im\u00b7mer", "im", "Stro\u00b7me", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Wenn die volle Gewissheit zeugt, und von Gr\u00f6sse der Unthat;", "tokens": ["Wenn", "die", "vol\u00b7le", "Ge\u00b7wiss\u00b7heit", "zeugt", ",", "und", "von", "Gr\u00f6s\u00b7se", "der", "Un\u00b7that", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "APPR", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+--++", "measure": "anapaest.di.plus"}, "line.12": {"text": "Ewigen diese Geschicht', und Gesang.", "tokens": ["E\u00b7wi\u00b7gen", "die\u00b7se", "Ge\u00b7schicht'", ",", "und", "Ge\u00b7sang", 