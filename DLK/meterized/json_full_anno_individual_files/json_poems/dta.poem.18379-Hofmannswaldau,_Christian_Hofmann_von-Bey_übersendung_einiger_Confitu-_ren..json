{"dta.poem.18379": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey \u00fcbersendung einiger Confitu-  \n ren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer will dein armer knecht ein schlechtes opffer schicken/", "tokens": ["Hjer", "will", "dein", "ar\u00b7mer", "knecht", "ein", "schlech\u00b7tes", "opf\u00b7fer", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der selbst auf deinem mund ein opffer w\u00fcnscht zu seyn.", "tokens": ["Der", "selbst", "auf", "dei\u00b7nem", "mund", "ein", "opf\u00b7fer", "w\u00fcnscht", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bestrahlstu dieses pfand mit gunst geneigten blicken/", "tokens": ["Be\u00b7strahl\u00b7stu", "die\u00b7ses", "pfand", "mit", "gunst", "ge\u00b7neig\u00b7ten", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "APPR", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird der himmel selbst mein hertz mit lust bestreun.", "tokens": ["So", "wird", "der", "him\u00b7mel", "selbst", "mein", "hertz", "mit", "lust", "be\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nim es in gnaden an di\u00df kleine demuths zeichen/", "tokens": ["Nim", "es", "in", "gna\u00b7den", "an", "di\u00df", "klei\u00b7ne", "de\u00b7muths", "zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "APPR", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das zwar vor deinen mund/ mein licht/ gewidmet ist:", "tokens": ["Das", "zwar", "vor", "dei\u00b7nen", "mund", "/", "mein", "licht", "/", "ge\u00b7wid\u00b7met", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch mag kein zucker nicht dem s\u00fcssen nectar gleichen/", "tokens": ["Doch", "mag", "kein", "zu\u00b7cker", "nicht", "dem", "s\u00fcs\u00b7sen", "nec\u00b7tar", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der wie ein honig-thau auf deinen lippen fliest;", "tokens": ["Der", "wie", "ein", "ho\u00b7nig\u00b7\u00b7thau", "auf", "dei\u00b7nen", "lip\u00b7pen", "fliest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kein Amber kan so sehr den matten geist erquicken/", "tokens": ["Kein", "Am\u00b7ber", "kan", "so", "sehr", "den", "mat\u00b7ten", "geist", "er\u00b7qui\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als wohl von deinem mund ein hei\u00df entz\u00fcndter ku\u00df;", "tokens": ["Als", "wohl", "von", "dei\u00b7nem", "mund", "ein", "hei\u00df", "ent\u00b7z\u00fcnd\u00b7ter", "ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wann wird der himmel doch einst deinen knecht begl\u00fccken/", "tokens": ["Wann", "wird", "der", "him\u00b7mel", "doch", "einst", "dei\u00b7nen", "knecht", "be\u00b7gl\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df er auch schmecken darff den s\u00fcssen \u00fcberflu\u00df?", "tokens": ["Da\u00df", "er", "auch", "schme\u00b7cken", "darff", "den", "s\u00fcs\u00b7sen", "\u00fc\u00b7berf\u00b7lu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}