{"dta.poem.501": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er w\u00fcnscht den Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Leben liebt den Tod/ es scheuet kein Verderben", "tokens": ["Mein", "Le\u00b7ben", "liebt", "den", "Tod", "/", "es", "scheu\u00b7et", "kein", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was andern bitter schmeckt soll mir ein Honig seyn;", "tokens": ["Was", "an\u00b7dern", "bit\u00b7ter", "schmeckt", "soll", "mir", "ein", "Ho\u00b7nig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "VMFIN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir bringt das Sterben Lust nicht Angst beseelte Pein/", "tokens": ["Mir", "bringt", "das", "Ster\u00b7ben", "Lust", "nicht", "Angst", "be\u00b7seel\u00b7te", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKNEG", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn mich der Tod nur l\u00e4st in deinen Armen sterben.", "tokens": ["Wenn", "mich", "der", "Tod", "nur", "l\u00e4st", "in", "dei\u00b7nen", "Ar\u00b7men", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word"