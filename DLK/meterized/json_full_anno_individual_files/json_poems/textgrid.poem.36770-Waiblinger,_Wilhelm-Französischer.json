{"textgrid.poem.36770": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Franz\u00f6sischer", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hle den Stoff nur gut, er sei fein klassisch, denn einzig", "tokens": ["W\u00e4h\u00b7le", "den", "Stoff", "nur", "gut", ",", "er", "sei", "fein", "klas\u00b7sisch", ",", "denn", "ein\u00b7zig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "ADJD", "$,", "KON", "ADJD"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Ist es das Klassische nur, was mit der Kunst sich vertr\u00e4gt.", "tokens": ["Ist", "es", "das", "Klas\u00b7si\u00b7sche", "nur", ",", "was", "mit", "der", "Kunst", "sich", "ver\u00b7tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "R\u00f6mer und Griechen und Mythologie sind klassische Quellen,", "tokens": ["R\u00f6\u00b7mer", "und", "Grie\u00b7chen", "und", "My\u00b7tho\u00b7lo\u00b7gie", "sind", "klas\u00b7si\u00b7sche", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber verstehe mich, da\u00df du den Effekt nicht vergi\u00dft.", "tokens": ["A\u00b7ber", "ver\u00b7ste\u00b7he", "mich", ",", "da\u00df", "du", "den", "Ef\u00b7fekt", "nicht", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Ungew\u00f6hnlich sei Licht und Reflex, Halbschatten und Schatten,", "tokens": ["Un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "sei", "Licht", "und", "Re\u00b7flex", ",", "Halb\u00b7schat\u00b7ten", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Denn das Gew\u00f6hnliche bleibt einmal f\u00fcr immer gemein.", "tokens": ["Denn", "das", "Ge\u00b7w\u00f6hn\u00b7li\u00b7che", "bleibt", "ein\u00b7mal", "f\u00fcr", "im\u00b7mer", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "ADV", "ADJD", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Was die Antike betrifft, so ahme den Reiz und die Wollust,", "tokens": ["Was", "die", "An\u00b7ti\u00b7ke", "be\u00b7tr\u00b7ifft", ",", "so", "ah\u00b7me", "den", "Reiz", "und", "die", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Ahme die sinnliche Form, aber die K\u00e4lte nicht nach.", "tokens": ["Ah\u00b7me", "die", "sinn\u00b7li\u00b7che", "Form", ",", "a\u00b7ber", "die", "K\u00e4l\u00b7te", "nicht", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Gieb der Venus ein \u00fcppig Gelock, und k\u00fcnstliche Blumen,", "tokens": ["Gieb", "der", "Ve\u00b7nus", "ein", "\u00fcp\u00b7pig", "Ge\u00b7lock", ",", "und", "k\u00fcnst\u00b7li\u00b7che", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "ADJD", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Und so l\u00fcstern sie kann, liege die Schmachtende da.", "tokens": ["Und", "so", "l\u00fcs\u00b7tern", "sie", "kann", ",", "lie\u00b7ge", "die", "Schmach\u00b7ten\u00b7de", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VMFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Bildest du Helden, so bilde sie mir in rasender Stellung,", "tokens": ["Bil\u00b7dest", "du", "Hel\u00b7den", ",", "so", "bil\u00b7de", "sie", "mir", "in", "ra\u00b7sen\u00b7der", "Stel\u00b7lung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Nimm das tragische Spiel gro\u00dfer Acteurs zum Modell.", "tokens": ["Nimm", "das", "tra\u00b7gi\u00b7sche", "Spiel", "gro\u00b7\u00dfer", "Ac\u00b7teurs", "zum", "Mo\u00b7dell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.13": {"text": "Componirest du C\u00e4sars Tod, so denke, du malest", "tokens": ["Com\u00b7po\u00b7ni\u00b7rest", "du", "C\u00e4\u00b7sars", "Tod", ",", "so", "den\u00b7ke", ",", "du", "ma\u00b7lest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "NE", "NN", "$,", "ADV", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+++", "measure": "unknown.measure.octa.plus"}, "line.14": {"text": "Furien, und da\u00df der Blick stier nur und f\u00fcrchterlich ist!", "tokens": ["Fu\u00b7ri\u00b7en", ",", "und", "da\u00df", "der", "Blick", "stier", "nur", "und", "f\u00fcrch\u00b7ter\u00b7lich", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "KOUS", "ART", "NN", "ADV", "ADV", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.15": {"text": "Nur nichts an Farben gespart, du hast auf Augen zu wirken,", "tokens": ["Nur", "nichts", "an", "Far\u00b7ben", "ge\u00b7spart", ",", "du", "hast", "auf", "Au\u00b7gen", "zu", "wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "NN", "VVPP", "$,", "PPER", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Und auf die Menge, was gehn Sinn und Verstand dich denn an?", "tokens": ["Und", "auf", "die", "Men\u00b7ge", ",", "was", "gehn", "Sinn", "und", "Ver\u00b7stand", "dich", "denn", "an", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWS", "VVFIN", "NN", "KON", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.