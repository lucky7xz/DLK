{"dta.poem.7002": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Diejenigen, die dies stets von sich h\u00f6ren lassen,", "tokens": ["Die\u00b7je\u00b7ni\u00b7gen", ",", "die", "dies", "stets", "von", "sich", "h\u00f6\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PDS", "ADV", "APPR", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als k\u00f6nnten sie unm\u00f6glich fassen,", "tokens": ["Als", "k\u00f6nn\u00b7ten", "sie", "un\u00b7m\u00f6g\u00b7lich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie doch ein Wesen sey, und w\u00fcrck\u2019, und doch dabey", "tokens": ["Wie", "doch", "ein", "We\u00b7sen", "sey", ",", "und", "w\u00fcrck'", ",", "und", "doch", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "$,", "KON", "VAFIN", "$,", "KON", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein C\u00f6rper sey;", "tokens": ["Kein", "C\u00f6r\u00b7per", "sey", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Begreiffen die, wie es doch kan geschehen,", "tokens": ["Be\u00b7greif\u00b7fen", "die", ",", "wie", "es", "doch", "kan", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PWAV", "PPER", "ADV", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df C\u00f6rper w\u00fcrcken und verstehen?", "tokens": ["Da\u00df", "C\u00f6r\u00b7per", "w\u00fcr\u00b7cken", "und", "ver\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist ihnen der Bewegungs-Grund,", "tokens": ["Ist", "ih\u00b7nen", "der", "Be\u00b7we\u00b7gungs\u00b7Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der C\u00f6rpern eingegeben, kund?", "tokens": ["Der", "C\u00f6r\u00b7pern", "ein\u00b7ge\u00b7ge\u00b7ben", ",", "kund", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist ihnen der Verstand,", "tokens": ["Ist", "ih\u00b7nen", "der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der sie erleuchtet, recht bekannt?", "tokens": ["Der", "sie", "er\u00b7leuch\u00b7tet", ",", "recht", "be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}