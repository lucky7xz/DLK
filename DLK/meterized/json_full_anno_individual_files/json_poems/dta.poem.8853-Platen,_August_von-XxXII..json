{"dta.poem.8853": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.71", "sv:0.28"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Kam bey jedem Schritte fast in's Stocken sonst;", "tokens": ["Kam", "bey", "je\u00b7dem", "Schrit\u00b7te", "fast", "in's", "Sto\u00b7cken", "sonst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sie, die nun im Aether ihre Schwinge wiegt,", "tokens": ["Sie", ",", "die", "nun", "im", "A\u00b7e\u00b7ther", "ih\u00b7re", "Schwin\u00b7ge", "wiegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Lie\u00df in tausend Netze sich verlocken sonst;", "tokens": ["Lie\u00df", "in", "tau\u00b7send", "Net\u00b7ze", "sich", "ver\u00b7lo\u00b7cken", "sonst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "PRF", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Sie, die nun die Hydra der Begier erlegt,", "tokens": ["Sie", ",", "die", "nun", "die", "Hyd\u00b7ra", "der", "Be\u00b7gier", "er\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Sa\u00df in Weiberr\u00f6cken vor dem Rocken sonst;", "tokens": ["Sa\u00df", "in", "Wei\u00b7ber\u00b7r\u00f6\u00b7cken", "vor", "dem", "Ro\u00b7cken", "sonst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Gegen\u00fcber einem Angesicht wie deins", "tokens": ["Ge\u00b7gen\u00b7\u00fc\u00b7ber", "ei\u00b7nem", "An\u00b7ge\u00b7sicht", "wie", "deins"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KOKOM", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "War ich nicht so frostig, nicht so trocken sonst;", "tokens": ["War", "ich", "nicht", "so", "fros\u00b7tig", ",", "nicht", "so", "tro\u00b7cken", "sonst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "PTKNEG", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Aber neu verf\u00fchren wirst du mein Gem\u00fcth,", "tokens": ["A\u00b7ber", "neu", "ver\u00b7f\u00fch\u00b7ren", "wirst", "du", "mein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Denn was wollen anders deine Locken sonst?", "tokens": ["Denn", "was", "wol\u00b7len", "an\u00b7ders", "dei\u00b7ne", "Lo\u00b7cken", "sonst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}