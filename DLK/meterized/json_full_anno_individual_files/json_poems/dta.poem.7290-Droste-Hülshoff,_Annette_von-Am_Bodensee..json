{"dta.poem.7290": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Bodensee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber Gel\u00e4nde, matt gedehnt,", "tokens": ["Ue\u00b7ber", "Ge\u00b7l\u00e4n\u00b7de", ",", "matt", "ge\u00b7dehnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hat Nebelhauch sich wimmelnd gelegt,", "tokens": ["Hat", "Ne\u00b7bel\u00b7hauch", "sich", "wim\u00b7melnd", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "M\u00fcde, m\u00fcde die Luft am Strande st\u00f6hnt,", "tokens": ["M\u00fc\u00b7de", ",", "m\u00fc\u00b7de", "die", "Luft", "am", "Stran\u00b7de", "st\u00f6hnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wie ein Ro\u00df, das den schlafenden Reiter tr\u00e4gt;", "tokens": ["Wie", "ein", "Ro\u00df", ",", "das", "den", "schla\u00b7fen\u00b7den", "Rei\u00b7ter", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Im Fischerhause kein L\u00e4mpchen brennt,", "tokens": ["Im", "Fi\u00b7scher\u00b7hau\u00b7se", "kein", "L\u00e4mp\u00b7chen", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Im \u00f6den Thurme kein Heimchen schrillt,", "tokens": ["Im", "\u00f6\u00b7den", "Thur\u00b7me", "kein", "Heim\u00b7chen", "schrillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nur langsam rollend der Pulsschlag schwillt", "tokens": ["Nur", "lang\u00b7sam", "rol\u00b7lend", "der", "Puls\u00b7schlag", "schwillt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In dem zitternden Element.", "tokens": ["In", "dem", "zit\u00b7tern\u00b7den", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6r' es w\u00fchlen am feuchten Strand,", "tokens": ["Ich", "h\u00f6r'", "es", "w\u00fch\u00b7len", "am", "feuch\u00b7ten", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir unter'm Fu\u00dfe es w\u00fchlen fort,", "tokens": ["Mir", "un\u00b7ter'm", "Fu\u00b7\u00dfe", "es", "w\u00fch\u00b7len", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Kiesel knistern, es rauscht der Sand,", "tokens": ["Die", "Kie\u00b7sel", "knis\u00b7tern", ",", "es", "rauscht", "der", "Sand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Stein an Stein entbr\u00f6ckelt dem Bord.", "tokens": ["Und", "Stein", "an", "Stein", "ent\u00b7br\u00f6\u00b7ckelt", "dem", "Bord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "An meiner Sohle zerf\u00e4hrt der Schaum,", "tokens": ["An", "mei\u00b7ner", "Soh\u00b7le", "zer\u00b7f\u00e4hrt", "der", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Eine Stimme klaget im hohlen Grund,", "tokens": ["Ei\u00b7ne", "Stim\u00b7me", "kla\u00b7get", "im", "hoh\u00b7len", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ged\u00e4mpft, mit halbgeschlossenem Mund,", "tokens": ["Ge\u00b7d\u00e4mpft", ",", "mit", "halb\u00b7ge\u00b7schlos\u00b7se\u00b7nem", "Mund", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wie des grollenden Wetters Traum.", "tokens": ["Wie", "des", "grol\u00b7len\u00b7den", "Wet\u00b7ters", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich beuge mich lauschend am Thurme her,", "tokens": ["Ich", "beu\u00b7ge", "mich", "lau\u00b7schend", "am", "Thur\u00b7me", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Spr\u00fchregenflitter f\u00e4hrt in die H\u00f6h',", "tokens": ["Spr\u00fch\u00b7re\u00b7gen\u00b7flit\u00b7ter", "f\u00e4hrt", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ha, meine Locke ist feucht und schwer!", "tokens": ["Ha", ",", "mei\u00b7ne", "Lo\u00b7cke", "ist", "feucht", "und", "schwer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Was treibst du denn, unruhiger See?", "tokens": ["Was", "treibst", "du", "denn", ",", "un\u00b7ru\u00b7hi\u00b7ger", "See", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kann dir der heilige Schlaf nicht nahn?", "tokens": ["Kann", "dir", "der", "hei\u00b7li\u00b7ge", "Schlaf", "nicht", "nahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch nein, du schl\u00e4fst, ich seh' es genau,", "tokens": ["Doch", "nein", ",", "du", "schl\u00e4fst", ",", "ich", "seh'", "es", "ge\u00b7nau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Dein Auge decket die Wimper grau,", "tokens": ["Dein", "Au\u00b7ge", "de\u00b7cket", "die", "Wim\u00b7per", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Am Ufer schlummert der Kahn.", "tokens": ["Am", "U\u00b7fer", "schlum\u00b7mert", "der", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Hast du so Vieles, so Vieles erlebt,", "tokens": ["Hast", "du", "so", "Vie\u00b7les", ",", "so", "Vie\u00b7les", "er\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "$,", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df dir im Traume es kehren mu\u00df,", "tokens": ["Da\u00df", "dir", "im", "Trau\u00b7me", "es", "keh\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df deine glei\u00dfende Nerv' erbebt,", "tokens": ["Da\u00df", "dei\u00b7ne", "glei\u00b7\u00dfen\u00b7de", "Ner\u00b7v'", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Naht ihr am Strand eines Menschen Fu\u00df?", "tokens": ["Naht", "ihr", "am", "Strand", "ei\u00b7nes", "Men\u00b7schen", "Fu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dahin, dahin! die einst so gesund,", "tokens": ["Da\u00b7hin", ",", "da\u00b7hin", "!", "die", "einst", "so", "ge\u00b7sund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "$.", "ART", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "So reich und m\u00e4chtig, so arm und klein,", "tokens": ["So", "reich", "und", "m\u00e4ch\u00b7tig", ",", "so", "arm", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und nur ihr fl\u00fcchtiger Spiegelschein", "tokens": ["Und", "nur", "ihr", "fl\u00fcch\u00b7ti\u00b7ger", "Spie\u00b7gel\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Liegt zerflossen auf deinem Grund.", "tokens": ["Liegt", "zer\u00b7flos\u00b7sen", "auf", "dei\u00b7nem", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Der Ritter, so aus der Burg hervor", "tokens": ["Der", "Rit\u00b7ter", ",", "so", "aus", "der", "Burg", "her\u00b7vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Hange trabte in aller Fr\u00fch;", "tokens": ["Vom", "Han\u00b7ge", "trab\u00b7te", "in", "al\u00b7ler", "Fr\u00fch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u2014 Jetzt nickt die Esche vom grauen Thor,", "tokens": ["Jetzt", "nickt", "die", "E\u00b7sche", "vom", "grau\u00b7en", "Thor", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Am Zwinger zeichnet die Mylady. \u2014", "tokens": ["Am", "Zwin\u00b7ger", "zeich\u00b7net", "die", "My\u00b7la\u00b7dy", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das arme M\u00fctterlein, das gebleicht", "tokens": ["Das", "ar\u00b7me", "M\u00fct\u00b7ter\u00b7lein", ",", "das", "ge\u00b7bleicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sein Leichenhemde den Strand entlang,", "tokens": ["Sein", "Lei\u00b7chen\u00b7hem\u00b7de", "den", "Strand", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der Kranke, der seinen letzten Gang", "tokens": ["Der", "Kran\u00b7ke", ",", "der", "sei\u00b7nen", "letz\u00b7ten", "Gang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "An deinem Borde gekeucht;", "tokens": ["An", "dei\u00b7nem", "Bor\u00b7de", "ge\u00b7keucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Das spielende Kind, das neckend hier", "tokens": ["Das", "spie\u00b7len\u00b7de", "Kind", ",", "das", "ne\u00b7ckend", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Schneckenh\u00e4uschen geschleudert hat,", "tokens": ["Sein", "Schne\u00b7cken\u00b7h\u00e4usc\u00b7hen", "ge\u00b7schleu\u00b7dert", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die gl\u00fchende Braut, die l\u00e4chelnd dir", "tokens": ["Die", "gl\u00fc\u00b7hen\u00b7de", "Braut", ",", "die", "l\u00e4\u00b7chelnd", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von der Ringelblume gab Blatt um Blatt;", "tokens": ["Von", "der", "Rin\u00b7gel\u00b7blu\u00b7me", "gab", "Blatt", "um", "Blatt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "--+-+-++-+", "measure": "anapaest.init"}, "line.5": {"text": "Der S\u00e4nger, der mit trunkenem Aug'", "tokens": ["Der", "S\u00e4n\u00b7ger", ",", "der", "mit", "trun\u00b7ke\u00b7nem", "Aug'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Das Metrum gepl\u00e4tschert in deiner Flut,", "tokens": ["Das", "Me\u00b7trum", "ge\u00b7pl\u00e4t\u00b7schert", "in", "dei\u00b7ner", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der Pilger, so am Gesteine geruht,", "tokens": ["Der", "Pil\u00b7ger", ",", "so", "am", "Ge\u00b7stei\u00b7ne", "ge\u00b7ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sie Alle dahin wie Rauch!", "tokens": ["Sie", "Al\u00b7le", "da\u00b7hin", "wie", "Rauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PAV", "KOKOM", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Bist du so fromm, alte Wasserfey,", "tokens": ["Bist", "du", "so", "fromm", ",", "al\u00b7te", "Was\u00b7ser\u00b7fey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00e4ltst nur umschlungen, l\u00e4\u00dft nimmer los?", "tokens": ["H\u00e4ltst", "nur", "um\u00b7schlun\u00b7gen", ",", "l\u00e4\u00dft", "nim\u00b7mer", "los", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$,", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3