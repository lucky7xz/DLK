{"textgrid.poem.59516": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An die Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine Tr\u00e4ne, Hermann, f\u00fcr dein Volk?", "tokens": ["Kei\u00b7ne", "Tr\u00e4\u00b7ne", ",", "Her\u00b7mann", ",", "f\u00fcr", "dein", "Volk", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Keine Tr\u00e4ne? und die Schande brennet,", "tokens": ["Kei\u00b7ne", "Tr\u00e4\u00b7ne", "?", "und", "die", "Schan\u00b7de", "bren\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Feind gebietet, wo die Freien", "tokens": ["Und", "der", "Feind", "ge\u00b7bie\u00b7tet", ",", "wo", "die", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Siegten und fielen!", "tokens": ["Sieg\u00b7ten", "und", "fie\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Keine Stimme laut, wo Luther sprach?", "tokens": ["Kei\u00b7ne", "Stim\u00b7me", "laut", ",", "wo", "Lu\u00b7ther", "sprach", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Alle Donner, die der Himmel sendet,", "tokens": ["Al\u00b7le", "Don\u00b7ner", ",", "die", "der", "Him\u00b7mel", "sen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sollten rufen: Volk, erwache! feiges,", "tokens": ["Soll\u00b7ten", "ru\u00b7fen", ":", "Volk", ",", "er\u00b7wa\u00b7che", "!", "fei\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "VVINF", "$.", "NN", "$,", "VVFIN", "$.", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Greife zum Schwerte!", "tokens": ["Grei\u00b7fe", "zum", "Schwer\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Rache! Rache! Hei\u00dfen, blut'gen Tod", "tokens": ["Ra\u00b7che", "!", "Ra\u00b7che", "!", "Hei\u00b7\u00dfen", ",", "blut'\u00b7gen", "Tod"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sklavenf\u00fcrsten und dem Knecht, der fliehet!", "tokens": ["Skla\u00b7ven\u00b7f\u00fcrs\u00b7ten", "und", "dem", "Knecht", ",", "der", "flie\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00e4nnerwort gef\u00fcrchtet und gepriesen", "tokens": ["M\u00e4n\u00b7ner\u00b7wort", "ge\u00b7f\u00fcrch\u00b7tet", "und", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00e4nnliche Tugend!", "tokens": ["M\u00e4nn\u00b7li\u00b7che", "Tu\u00b7gend", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach wohin? Wo Winkelried erlag,", "tokens": ["Ach", "wo\u00b7hin", "?", "Wo", "Win\u00b7kel\u00b7ried", "er\u00b7lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "$.", "PWAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wilhelm schlug und Ruyter tapfer siegte,", "tokens": ["Wil\u00b7helm", "schlug", "und", "Ruy\u00b7ter", "tap\u00b7fer", "sieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf den h\u00f6chsten Alpen, in den tiefsten", "tokens": ["Auf", "den", "h\u00f6chs\u00b7ten", "Al\u00b7pen", ",", "in", "den", "tiefs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "S\u00fcmpfen ist Knechtschaft.", "tokens": ["S\u00fcmp\u00b7fen", "ist", "Knecht\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Auch du, Hermann, auch du, k\u00fchnes Volk?", "tokens": ["Auch", "du", ",", "Her\u00b7mann", ",", "auch", "du", ",", "k\u00fch\u00b7nes", "Volk", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NE", "$,", "ADV", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Auf! erwache! sch\u00fcttle deine Ketten,", "tokens": ["Auf", "!", "er\u00b7wa\u00b7che", "!", "sch\u00fctt\u00b7le", "dei\u00b7ne", "Ket\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df die Schmach die Welt vernehme, bald auch", "tokens": ["Da\u00df", "die", "Schmach", "die", "Welt", "ver\u00b7neh\u00b7me", ",", "bald", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ADV", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Blutige Rache.", "tokens": ["Blu\u00b7ti\u00b7ge", "Ra\u00b7che", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Lieder helfen hier und M\u00e4ler nicht.", "tokens": ["Lie\u00b7der", "hel\u00b7fen", "hier", "und", "M\u00e4\u00b7ler", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4ler? Tief im Herzen sei das Denkmal,", "tokens": ["M\u00e4\u00b7ler", "?", "Tief", "im", "Her\u00b7zen", "sei", "das", "Denk\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "An dem Turm der selbstgebornen Tugend", "tokens": ["An", "dem", "Turm", "der", "selbst\u00b7ge\u00b7bor\u00b7nen", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hebe dich, J\u00fcngling!", "tokens": ["He\u00b7be", "dich", ",", "J\u00fcng\u00b7ling", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Und vorangeworfen k\u00fchn die Brust!", "tokens": ["Und", "vor\u00b7an\u00b7ge\u00b7wor\u00b7fen", "k\u00fchn", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und empor das Auge zu dem Himmel!", "tokens": ["Und", "em\u00b7por", "das", "Au\u00b7ge", "zu", "dem", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hoch die Fahnen! Hoch zum Himmel! H\u00f6her", "tokens": ["Hoch", "die", "Fah\u00b7nen", "!", "Hoch", "zum", "Him\u00b7mel", "!", "H\u00f6\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "ART", "NN", "$.", "ADJD", "APPRART", "NN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Flammende Herzen!", "tokens": ["Flam\u00b7men\u00b7de", "Her\u00b7zen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Tod, du s\u00fc\u00dfer, f\u00fcr das Vaterland!", "tokens": ["Tod", ",", "du", "s\u00fc\u00b7\u00dfer", ",", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJD", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfer als der Brautgru\u00df, als das Lallen", "tokens": ["S\u00fc\u00b7\u00dfer", "als", "der", "Braut\u00b7gru\u00df", ",", "als", "das", "Lal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf dem Mutterscho\u00df des ersten Kindes,", "tokens": ["Auf", "dem", "Mut\u00b7ter\u00b7scho\u00df", "des", "ers\u00b7ten", "Kin\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sei mir willkommen!", "tokens": ["Sei", "mir", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Was das Lied nicht l\u00f6set, l\u00f6st das Schwert \u2013", "tokens": ["Was", "das", "Lied", "nicht", "l\u00f6\u00b7set", ",", "l\u00f6st", "das", "Schwert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Blinkend Heil, umg\u00fcrte meine H\u00fcften!", "tokens": ["Blin\u00b7kend", "Heil", ",", "um\u00b7g\u00fcr\u00b7te", "mei\u00b7ne", "H\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Von der Schande kannst du Tapfre retten,", "tokens": ["Von", "der", "Schan\u00b7de", "kannst", "du", "Tapf\u00b7re", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zierde der Tapfern.", "tokens": ["Zier\u00b7de", "der", "Tap\u00b7fern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Keine Tr\u00e4ne, Hermann, f\u00fcr dein Volk?", "tokens": ["Kei\u00b7ne", "Tr\u00e4\u00b7ne", ",", "Her\u00b7mann", ",", "f\u00fcr", "dein", "Volk", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Keine Tr\u00e4ne? und die Schande brennet,", "tokens": ["Kei\u00b7ne", "Tr\u00e4\u00b7ne", "?", "und", "die", "Schan\u00b7de", "bren\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Feind gebietet, wo die Freien", "tokens": ["Und", "der", "Feind", "ge\u00b7bie\u00b7tet", ",", "wo", "die", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Siegten und fielen!", "tokens": ["Sieg\u00b7ten", "und", "fie\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Keine Stimme laut, wo Luther sprach?", "tokens": ["Kei\u00b7ne", "Stim\u00b7me", "laut", ",", "wo", "Lu\u00b7ther", "sprach", "?"], "token_info": ["word", "