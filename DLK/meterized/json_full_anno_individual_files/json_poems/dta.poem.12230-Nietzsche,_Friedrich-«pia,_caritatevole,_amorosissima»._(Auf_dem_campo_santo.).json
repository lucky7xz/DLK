{"dta.poem.12230": {"metadata": {"author": {"name": "Nietzsche, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "\u00abpia, caritatevole, amorosissima\u00bb.  \n (Auf dem campo santo.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-30172-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O M\u00e4dchen, das dem Lamme", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "das", "dem", "Lam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "             Das zarte Fellchen kraut,", "tokens": ["Das", "zar\u00b7te", "Fell\u00b7chen", "kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "             Dem Beides, Licht und Flamme,", "tokens": ["Dem", "Bei\u00b7des", ",", "Licht", "und", "Flam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "             Aus beiden Augen schaut,", "tokens": ["Aus", "bei\u00b7den", "Au\u00b7gen", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "         Du lieblich Ding zum Scherzen,", "tokens": ["Du", "lieb\u00b7lich", "Ding", "zum", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "         Du Liebling weit und nah,", "tokens": ["Du", "Lieb\u00b7ling", "weit", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "             So fromm, so mild von Herzen,", "tokens": ["So", "fromm", ",", "so", "mild", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "             Was riss so fr\u00fch die Kette?", "tokens": ["Was", "riss", "so", "fr\u00fch", "die", "Ket\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "         Wer hat dein Herz betr\u00fcbt?", "tokens": ["Wer", "hat", "dein", "Herz", "be\u00b7tr\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "         Und liebtest du, wer h\u00e4tte", "tokens": ["Und", "lieb\u00b7test", "du", ",", "wer", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "             Dich nicht genug geliebt? \u2014", "tokens": ["Dich", "nicht", "ge\u00b7nug", "ge\u00b7liebt", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "             Du schweigst \u2014 doch sind die Thr\u00e4nen", "tokens": ["Du", "schweigst", "doch", "sind", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ADV", "VAFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "             Den milden Augen nah: \u2014", "tokens": ["Den", "mil\u00b7den", "Au\u00b7gen", "nah", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "         Du schwiegst \u2014 und starbst vor Sehnen,", "tokens": ["Du", "schwiegst", "und", "starbst", "vor", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}