{"dta.poem.19565": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "La\u00df scharren deiner Rosse Huf!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Geh nicht, die Gott f\u00fcr mich erschuf!", "tokens": ["Geh", "nicht", ",", "die", "Gott", "f\u00fcr", "mich", "er\u00b7schuf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df scharren deiner Rosse Huf", "tokens": ["La\u00df", "schar\u00b7ren", "dei\u00b7ner", "Ros\u00b7se", "Huf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Reiseruf!", "tokens": ["Den", "Rei\u00b7se\u00b7ruf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du willst von meinem Herde fliehn?", "tokens": ["Du", "willst", "von", "mei\u00b7nem", "Her\u00b7de", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00dft ja nicht, wohin, wohin", "tokens": ["Und", "wei\u00dft", "ja", "nicht", ",", "wo\u00b7hin", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich deine Rosse ziehn!", "tokens": ["Dich", "dei\u00b7ne", "Ros\u00b7se", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Stunde rinnt! Das Leben jagt!", "tokens": ["Die", "Stun\u00b7de", "rinnt", "!", "Das", "Le\u00b7ben", "jagt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir haben uns noch nichts gesagt \u2014", "tokens": ["Wir", "ha\u00b7ben", "uns", "noch", "nichts", "ge\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib bis es tagt!", "tokens": ["Bleib", "bis", "es", "tagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du darfst aus meinen Armen fliehn?", "tokens": ["Du", "darfst", "aus", "mei\u00b7nen", "Ar\u00b7men", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00dft ja nicht, wohin, wohin", "tokens": ["Und", "wei\u00dft", "ja", "nicht", ",", "wo\u00b7hin", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich deine Rosse ziehn ...", "tokens": ["Dich", "dei\u00b7ne", "Ros\u00b7se", "ziehn", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}