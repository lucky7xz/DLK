{"dta.poem.1033": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die Teutsche Musa.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sagt was jhr wolt/ Es kan nicht sein/", "tokens": ["Sagt", "was", "jhr", "wolt", "/", "Es", "kan", "nicht", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "PTKNEG", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd solt es Euch zumahl verdriessen.", "tokens": ["Vnd", "solt", "es", "Euch", "zu\u00b7mahl", "ver\u00b7dries\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin jetz Jhr/ vnd Sie ist mein/", "tokens": ["Ich", "bin", "jetz", "Ihr", "/", "vnd", "Sie", "ist", "mein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "$(", "KON", "PPER", "VAFIN", "PPOSAT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will Sie auch f\u00fcr Euch all begr\u00fcessen", "tokens": ["Will", "Sie", "auch", "f\u00fcr", "Euch", "all", "be\u00b7gr\u00fces\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit einem Lied wahr/ hell vnd rein.", "tokens": ["Mit", "ei\u00b7nem", "Lied", "wahr", "/", "hell", "vnd", "rein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$(", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}