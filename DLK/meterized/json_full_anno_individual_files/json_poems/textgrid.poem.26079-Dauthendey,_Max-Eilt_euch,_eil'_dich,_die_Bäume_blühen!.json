{"textgrid.poem.26079": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Eilt euch, eil' dich, die B\u00e4ume bl\u00fchen!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eilt euch, eil' dich, die B\u00e4ume bl\u00fchen!", "tokens": ["Eilt", "euch", ",", "eil'", "dich", ",", "die", "B\u00e4u\u00b7me", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Voll Liebesblicke die B\u00e4ume stehen;", "tokens": ["Voll", "Lie\u00b7bes\u00b7bli\u00b7cke", "die", "B\u00e4u\u00b7me", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Eh' du hingesehen, will's schon vergehen.", "tokens": ["Eh'", "du", "hin\u00b7ge\u00b7se\u00b7hen", ",", "will's", "schon", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.2": {"line.1": {"text": "Komm zu den hellen verliebten B\u00e4umen,", "tokens": ["Komm", "zu", "den", "hel\u00b7len", "ver\u00b7lieb\u00b7ten", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die alle Wege jetzt hochzeitlich s\u00e4umen!", "tokens": ["Die", "al\u00b7le", "We\u00b7ge", "jetzt", "hoch\u00b7zeit\u00b7lich", "s\u00e4u\u00