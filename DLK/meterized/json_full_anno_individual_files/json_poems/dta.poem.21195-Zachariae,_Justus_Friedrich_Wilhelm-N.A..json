{"dta.poem.21195": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, fromme Wanderer! Betrachtet diesen Berg", "tokens": ["Ja", ",", "from\u00b7me", "Wan\u00b7de\u00b7rer", "!", "Be\u00b7trach\u00b7tet", "die\u00b7sen", "Berg"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$.", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit heiligem Vergn\u00fcgen", "tokens": ["Mit", "hei\u00b7li\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr, als die pr\u00e4chtigste der stolzen Pyramiden,", "tokens": ["Mehr", ",", "als", "die", "pr\u00e4ch\u00b7tigs\u00b7te", "der", "stol\u00b7zen", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die seiner F\u00fcrsten Aschenkr\u00fcgen", "tokens": ["Die", "sei\u00b7ner", "F\u00fcrs\u00b7ten", "A\u00b7schen\u00b7kr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aegypten aufgeth\u00fcrmt.", "tokens": ["A\u00b7e\u00b7gyp\u00b7ten", "auf\u00b7ge\u00b7th\u00fcrmt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu Ehren dessen, welcher hier verschieden,", "tokens": ["Zu", "Eh\u00b7ren", "des\u00b7sen", ",", "wel\u00b7cher", "hier", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDS", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Steht Golgatha,", "tokens": ["Steht", "Gol\u00b7ga\u00b7tha", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Selbst von Ungl\u00e4ubigen beschirmt,", "tokens": ["Selbst", "von", "Un\u00b7gl\u00e4u\u00b7bi\u00b7gen", "be\u00b7schirmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Zum gro\u00dfen Denkmaal seines Todes da.", "tokens": ["Zum", "gro\u00b7\u00dfen", "Denk\u00b7maal", "sei\u00b7nes", "To\u00b7des", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Jhr werdet zwar f\u00fcr eure S\u00fcnden", "tokens": ["Ihr", "wer\u00b7det", "zwar", "f\u00fcr", "eu\u00b7re", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Durch diese Wallfahrt nicht Vergebung finden,", "tokens": ["Durch", "die\u00b7se", "Wall\u00b7fahrt", "nicht", "Ver\u00b7ge\u00b7bung", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Wenn wahre Bu\u00dfe nicht", "tokens": ["Wenn", "wah\u00b7re", "Bu\u00b7\u00dfe", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "F\u00fcr euch zum Gottmensch spricht;", "tokens": ["F\u00fcr", "euch", "zum", "Gott\u00b7mensch", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Doch kommet ihr mit tiefgebeugter Seele,", "tokens": ["Doch", "kom\u00b7met", "ihr", "mit", "tief\u00b7ge\u00b7beug\u00b7ter", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nicht gleich den stolzen Frommen,", "tokens": ["Nicht", "gleich", "den", "stol\u00b7zen", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Zu seiner heilgen Grabesh\u00f6hle,", "tokens": ["Zu", "sei\u00b7ner", "heil\u00b7gen", "Gra\u00b7bes\u00b7h\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und seyd ihr durch der wahren Andacht Geist", "tokens": ["Und", "seyd", "ihr", "durch", "der", "wah\u00b7ren", "An\u00b7dacht", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Hieher gereist;", "tokens": ["Hie\u00b7her", "ge\u00b7reist", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "So seyd mir tausendtausendmal willkommen.", "tokens": ["So", "seyd", "mir", "tau\u00b7send\u00b7tau\u00b7send\u00b7mal", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}