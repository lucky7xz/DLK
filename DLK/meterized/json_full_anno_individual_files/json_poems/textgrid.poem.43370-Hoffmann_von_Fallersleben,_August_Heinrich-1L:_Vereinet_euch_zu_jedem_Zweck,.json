{"textgrid.poem.43370": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vereinet euch zu jedem Zweck,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vereinet euch zu jedem Zweck,", "tokens": ["Ver\u00b7ei\u00b7net", "euch", "zu", "je\u00b7dem", "Zweck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr jeden Quark und jeden Dreck!", "tokens": ["F\u00fcr", "je\u00b7den", "Quark", "und", "je\u00b7den", "Dreck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur denkt dabei, wie gut es ist,", "tokens": ["Nur", "denkt", "da\u00b7bei", ",", "wie", "gut", "es", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man auch uns nicht ganz vergisst.", "tokens": ["Wenn", "man", "auch", "uns", "nicht", "ganz", "ver\u00b7gisst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt denkt ihr schon an's liebe Vieh,", "tokens": ["Jetzt", "denkt", "ihr", "schon", "an's", "lie\u00b7be", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iamb