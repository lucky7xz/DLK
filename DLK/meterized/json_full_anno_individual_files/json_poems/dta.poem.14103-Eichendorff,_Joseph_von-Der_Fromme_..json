{"dta.poem.14103": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fromme .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, Liebchen, denn am Meeresstrande!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Lieb\u00b7chen", ",", "denn", "am", "Mee\u00b7res\u00b7stran\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie rauschen lockend da an's Herz die Wellen", "tokens": ["Wie", "rau\u00b7schen", "lo\u00b7ckend", "da", "an's", "Herz", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und tiefe Sehnsucht will die Seele schwellen,", "tokens": ["Und", "tie\u00b7fe", "Sehn\u00b7sucht", "will", "die", "See\u00b7le", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn and're tr\u00e4ge schlafen auf dem Lande.", "tokens": ["Wenn", "an\u00b7d'\u00b7re", "tr\u00e4\u00b7ge", "schla\u00b7fen", "auf", "dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}