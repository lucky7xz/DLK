": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Umsonst mag ihm die Klugheit warnend stets", "tokens": ["Um\u00b7sonst", "mag", "ihm", "die", "Klug\u00b7heit", "war\u00b7nend", "stets"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVPP", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zur Seite geh'n. Es wird sich nie verl\u00e4ugnen,", "tokens": ["Zur", "Sei\u00b7te", "geh'", "n.", "Es", "wird", "sich", "nie", "ver\u00b7l\u00e4ug\u00b7nen", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PPER", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wird immer wahr und nur es selber seyn.", "tokens": ["Wird", "im\u00b7mer", "wahr", "und", "nur", "es", "sel\u00b7ber", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADV", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Als spr\u00e4chest Du vor zehentausend Zeugen,", "tokens": ["Als", "spr\u00e4\u00b7chest", "Du", "vor", "ze\u00b7hen\u00b7tau\u00b7send", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Als h\u00f6rten Dir mit allen ihren Ohren", "tokens": ["Als", "h\u00f6r\u00b7ten", "Dir", "mit", "al\u00b7len", "ih\u00b7ren", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Des Himmels Milliarden schweigend zu,", "tokens": ["Des", "Him\u00b7mels", "Mil\u00b7li\u00b7ar\u00b7den", "schwei\u00b7gend", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "So sprichst Du, wahres Herz, mit jedem Einen,", "tokens": ["So", "sprichst", "Du", ",", "wah\u00b7res", "Herz", ",", "mit", "je\u00b7dem", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ihn treibe Lieb' und herzliches Vertrauen,", "tokens": ["Ihn", "trei\u00b7be", "Lieb'", "und", "herz\u00b7li\u00b7ches", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ihn f\u00fchre Mi\u00dftrau'n Dir und Schalkheit zu,", "tokens": ["Ihn", "f\u00fch\u00b7re", "Mi\u00df\u00b7trau'n", "Dir", "und", "Schalk\u00b7heit", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Dein Wort ist wie Du selbst, ist nur Du selber,", "tokens": ["Dein", "Wort", "ist", "wie", "Du", "selbst", ",", "ist", "nur", "Du", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "PPER", "ADV", "$,", "VAFIN", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Dein ganzes Du, Dein inniges Empfinden.", "tokens": ["Dein", "gan\u00b7zes", "Du", ",", "Dein", "in\u00b7ni\u00b7ges", "Emp\u