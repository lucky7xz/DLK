{"textgrid.poem.39341": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "1L: O reines Herz! Was gleichet deiner W\u00fcrde,", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O reines Herz! Was gleichet deiner W\u00fcrde,", "tokens": ["O", "rei\u00b7nes", "Herz", "!", "Was", "glei\u00b7chet", "dei\u00b7ner", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bist Du und bist Du stets der Wahrheit Spiegel,", "tokens": ["Bist", "Du", "und", "bist", "Du", "stets", "der", "Wahr\u00b7heit", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein offner Sinn f\u00fcr alle Wirklichkeiten,", "tokens": ["Ein", "off\u00b7ner", "Sinn", "f\u00fcr", "al\u00b7le", "Wirk\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wiederhall von jeglicher Erfahrung!", "tokens": ["Ein", "Wie\u00b7der\u00b7hall", "von", "jeg\u00b7li\u00b7cher", "Er\u00b7fah\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So prallt der Stral von hellgeschliffner Fl\u00e4che", "tokens": ["So", "prallt", "der", "Stral", "von", "hell\u00b7ge\u00b7schliff\u00b7ner", "Fl\u00