{"textgrid.poem.47263": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem Wandersmann geh\u00f6rt die Welt", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Wandersmann geh\u00f6rt die Welt", "tokens": ["Dem", "Wan\u00b7ders\u00b7mann", "ge\u00b7h\u00f6rt", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In allen ihren Weiten,", "tokens": ["In", "al\u00b7len", "ih\u00b7ren", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil er kann \u00fcber Thal und Feld", "tokens": ["Weil", "er", "kann", "\u00fc\u00b7ber", "Thal", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "So wohlgemut hinschreiten.", "tokens": ["So", "wohl\u00b7ge\u00b7mut", "hin\u00b7schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Felder sind wohl angebaut", "tokens": ["Die", "Fel\u00b7der", "sind", "wohl", "an\u00b7ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr andre und von andern;", "tokens": ["F\u00fcr", "and\u00b7re", "und", "von", "an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihm aber, der sie sich beschaut,", "tokens": ["Ihm", "a\u00b7ber", ",", "der", "sie", "sich", "be\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geh\u00f6ren sie jetzt beim Wandern.", "tokens": ["Ge\u00b7h\u00f6\u00b7ren", "sie", "jetzt", "beim", "Wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Durch Wiesen schl\u00e4ngelt sich ein Pfad", "tokens": ["Durch", "Wie\u00b7sen", "schl\u00e4n\u00b7gelt", "sich", "ein", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie zwischen Blumenbeeten.", "tokens": ["Wie", "zwi\u00b7schen", "Blu\u00b7men\u00b7bee\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df nicht, wessen Fu\u00df ihn trat;", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wes\u00b7sen", "Fu\u00df", "ihn", "trat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist f\u00fcr mich getreten.", "tokens": ["Er", "ist", "f\u00fcr", "mich", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und neben in das Gras hinein,", "tokens": ["Und", "ne\u00b7ben", "in", "das", "Gras", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo sie wohl Futter holen,", "tokens": ["Wo", "sie", "wohl", "Fut\u00b7ter", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Gr\u00fcn ist auch beim Wandern mein,", "tokens": ["Das", "Gr\u00fcn", "ist", "auch", "beim", "Wan\u00b7dern", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Teppich f\u00fcr meine Sohlen.", "tokens": ["Ein", "Tep\u00b7pich", "f\u00fcr", "mei\u00b7ne", "Soh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Baum, der hier am Wege steht,", "tokens": ["Der", "Baum", ",", "der", "hier", "am", "We\u00b7ge", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wem mag er Frucht erstatten?", "tokens": ["Wem", "mag", "er", "Frucht", "er\u00b7stat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch weil mein Weg vor\u00fcbergeht,", "tokens": ["Doch", "weil", "mein", "Weg", "vor\u00b7\u00fc\u00b7ber\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gibt er mir den Schatten.", "tokens": ["So", "gibt", "er", "mir", "den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben ihn hieher gesetzt", "tokens": ["Sie", "ha\u00b7ben", "ihn", "hie\u00b7her", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl nicht zu meinem Frommen;", "tokens": ["Wohl", "nicht", "zu", "mei\u00b7nem", "From\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich aber glaube, da\u00df er jetzt", "tokens": ["Ich", "a\u00b7ber", "glau\u00b7be", ",", "da\u00df", "er", "jetzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sei eigens f\u00fcr mich gekommen.", "tokens": ["Sei", "ei\u00b7gens", "f\u00fcr", "mich", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Bach, der mir entgegenrauscht,", "tokens": ["Der", "Bach", ",", "der", "mir", "ent\u00b7ge\u00b7gen\u00b7rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt her, mich zu begr\u00fc\u00dfen,", "tokens": ["Kommt", "her", ",", "mich", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Reden, die er mit mir tauscht,", "tokens": ["Durch", "Re\u00b7den", ",", "die", "er", "mit", "mir", "tauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gang mir zu vers\u00fc\u00dfen.", "tokens": ["Den", "Gang", "mir", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn ich seiner m\u00fcde bin,", "tokens": ["Und", "wenn", "ich", "sei\u00b7ner", "m\u00fc\u00b7de", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er wartet auf mein Winken,", "tokens": ["Er", "war\u00b7tet", "auf", "mein", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gleich wendet er sich zur Rechten hin,", "tokens": ["Gleich", "wen\u00b7det", "er", "sich", "zur", "Rech\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und ich zieh' fort zur Linken.", "tokens": ["Und", "ich", "zieh'", "fort", "zur", "Lin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die L\u00fcfte sind mir dienstbar auch,", "tokens": ["Die", "L\u00fcf\u00b7te", "sind", "mir", "dienst\u00b7bar", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir im R\u00fccken wehen,", "tokens": ["Die", "mir", "im", "R\u00fc\u00b7cken", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wollen doch mit ihrem Hauch", "tokens": ["Sie", "wol\u00b7len", "doch", "mit", "ih\u00b7rem", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich f\u00f6rdern nur im Gehen.", "tokens": ["Mich", "f\u00f6r\u00b7dern", "nur", "im", "Ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und die ins Angesicht mich k\u00fc\u00dft,", "tokens": ["Und", "die", "ins", "An\u00b7ge\u00b7sicht", "mich", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie will mir auch nicht schaden:", "tokens": ["Sie", "will", "mir", "auch", "nicht", "scha\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es ist die Ferne, die mich gr\u00fc\u00dft,", "tokens": ["Es", "ist", "die", "Fer\u00b7ne", ",", "die", "mich", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu sich mich einzuladen.", "tokens": ["Zu", "sich", "mich", "ein\u00b7zu\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Regen und der Sonnenschein", "tokens": ["Der", "Re\u00b7gen", "und", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind meine zwei Gesellen,", "tokens": ["Sind", "mei\u00b7ne", "zwei", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die, einer hinterm andern drein,", "tokens": ["Die", ",", "ei\u00b7ner", "hin\u00b7term", "an\u00b7dern", "drein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "APPRART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Abwechselnd ein sich stellen.", "tokens": ["Ab\u00b7wech\u00b7selnd", "ein", "sich", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Regen l\u00f6scht der Stra\u00dfe Staub,", "tokens": ["Der", "Re\u00b7gen", "l\u00f6scht", "der", "Stra\u00b7\u00dfe", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sonne macht sie trocken;", "tokens": ["Die", "Son\u00b7ne", "macht", "sie", "tro\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Daneben wollen Gras und Laub", "tokens": ["Da\u00b7ne\u00b7ben", "wol\u00b7len", "Gras", "und", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie aus dem Boden locken.", "tokens": ["Sie", "aus", "dem", "Bo\u00b7den", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und spannt in ihrem Wechselspiel", "tokens": ["Und", "spannt", "in", "ih\u00b7rem", "Wech\u00b7sel\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich aus ein Regenbogen,", "tokens": ["Sich", "aus", "ein", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm' ich, entgegen meinem Ziel,", "tokens": ["Komm'", "ich", ",", "ent\u00b7ge\u00b7gen", "mei\u00b7nem", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darunter hergezogen.", "tokens": ["Da\u00b7run\u00b7ter", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Bogen ist f\u00fcr mich gespannt,", "tokens": ["Der", "Bo\u00b7gen", "ist", "f\u00fcr", "mich", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ich darunter walle;", "tokens": ["Weil", "ich", "da\u00b7run\u00b7ter", "wal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu Tr\u00e4gern sind die Berg' ernannt,", "tokens": ["Zu", "Tr\u00e4\u00b7gern", "sind", "die", "Ber\u00b7g'", "er\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Da\u00df er auf mich nicht falle.", "tokens": ["Da\u00df", "er", "auf", "mich", "nicht", "fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Und wo ein Dorf entgegentritt,", "tokens": ["Und", "wo", "ein", "Dorf", "ent\u00b7ge\u00b7gen\u00b7tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da h\u00f6r' ich Glocken l\u00e4uten.", "tokens": ["Da", "h\u00f6r'", "ich", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie meinen selber mich damit,", "tokens": ["Sie", "mei\u00b7nen", "sel\u00b7ber", "mich", "da\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was k\u00f6nnt' es sonst bedeuten?", "tokens": ["Was", "k\u00f6nnt'", "es", "sonst", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie l\u00e4uten etwan einer Braut,", "tokens": ["Sie", "l\u00e4u\u00b7ten", "et\u00b7wan", "ei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Vielleicht auch einem Toten;", "tokens": ["Viel\u00b7leicht", "auch", "ei\u00b7nem", "To\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich aber deut' auf mich den Laut:", "tokens": ["Ich", "a\u00b7ber", "deut'", "auf", "mich", "den", "Laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Gru\u00df wird mir geboten.", "tokens": ["Ein", "Gru\u00df", "wird", "mir", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So zieh' ich im Triumphgesang", "tokens": ["So", "zieh'", "ich", "im", "Tri\u00b7umph\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Entlang die lange Stra\u00dfe;", "tokens": ["Ent\u00b7lang", "die", "lan\u00b7ge", "Stra\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nie wird mir um etwas bang',", "tokens": ["Und", "nie", "wird", "mir", "um", "et\u00b7was", "bang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich im R\u00fccken lasse.", "tokens": ["Das", "ich", "im", "R\u00fc\u00b7cken", "las\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie eines hinter mir entweicht,", "tokens": ["Wie", "ei\u00b7nes", "hin\u00b7ter", "mir", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kommt gleich her das andre;", "tokens": ["So", "kommt", "gleich", "her", "das", "and\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und nie hab' ich das End' erreicht", "tokens": ["Und", "nie", "hab'", "ich", "das", "End'", "er\u00b7reicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Welt, soweit ich wandre.", "tokens": ["Der", "Welt", ",", "so\u00b7weit", "ich", "wand\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dem Wandersmann geh\u00f6rt die Welt", "tokens": ["Dem", "Wan\u00b7ders\u00b7mann", "ge\u00b7h\u00f6rt", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In allen ihren Weiten,", "tokens": ["In", "al\u00b7len", "ih\u00b7ren", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil er kann \u00fcber Thal und Feld", "tokens": ["Weil", "er", "kann", "\u00fc\u00b7ber", "Thal", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "So wohlgemut hinschreiten.", "tokens": ["So", "wohl\u00b7ge\u00b7mut", "hin\u00b7schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Felder sind wohl angebaut", "tokens": ["Die", "Fel\u00b7der", "sind", "wohl", "an\u00b7ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr andre und von andern;", "tokens": ["F\u00fcr", "and\u00b7re", "und", "von", "an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihm aber, der sie sich beschaut,", "tokens": ["Ihm", "a\u00b7ber", ",", "der", "sie", "sich", "be\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geh\u00f6ren sie jetzt beim Wandern.", "tokens": ["Ge\u00b7h\u00f6\u00b7ren", "sie", "jetzt", "beim", "Wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Durch Wiesen schl\u00e4ngelt sich ein Pfad", "tokens": ["Durch", "Wie\u00b7sen", "schl\u00e4n\u00b7gelt", "sich", "ein", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie zwischen Blumenbeeten.", "tokens": ["Wie", "zwi\u00b7schen", "Blu\u00b7men\u00b7bee\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df nicht, wessen Fu\u00df ihn trat;", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wes\u00b7sen", "Fu\u00df", "ihn", "trat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist f\u00fcr mich getreten.", "tokens": ["Er", "ist", "f\u00fcr", "mich", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und neben in das Gras hinein,", "tokens": ["Und", "ne\u00b7ben", "in", "das", "Gras", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo sie wohl Futter holen,", "tokens": ["Wo", "sie", "wohl", "Fut\u00b7ter", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Gr\u00fcn ist auch beim Wandern mein,", "tokens": ["Das", "Gr\u00fcn", "ist", "auch", "beim", "Wan\u00b7dern", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Teppich f\u00fcr meine Sohlen.", "tokens": ["Ein", "Tep\u00b7pich", "f\u00fcr", "mei\u00b7ne", "Soh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Der Baum, der hier am Wege steht,", "tokens": ["Der", "Baum", ",", "der", "hier", "am", "We\u00b7ge", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wem mag er Frucht erstatten?", "tokens": ["Wem", "mag", "er", "Frucht", "er\u00b7stat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch weil mein Weg vor\u00fcbergeht,", "tokens": ["Doch", "weil", "mein", "Weg", "vor\u00b7\u00fc\u00b7ber\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gibt er mir den Schatten.", "tokens": ["So", "gibt", "er", "mir", "den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben ihn hieher gesetzt", "tokens": ["Sie", "ha\u00b7ben", "ihn", "hie\u00b7her", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl nicht zu meinem Frommen;", "tokens": ["Wohl", "nicht", "zu", "mei\u00b7nem", "From\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich aber glaube, da\u00df er jetzt", "tokens": ["Ich", "a\u00b7ber", "glau\u00b7be", ",", "da\u00df", "er", "jetzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sei eigens f\u00fcr mich gekommen.", "tokens": ["Sei", "ei\u00b7gens", "f\u00fcr", "mich", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Der Bach, der mir entgegenrauscht,", "tokens": ["Der", "Bach", ",", "der", "mir", "ent\u00b7ge\u00b7gen\u00b7rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt her, mich zu begr\u00fc\u00dfen,", "tokens": ["Kommt", "her", ",", "mich", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Reden, die er mit mir tauscht,", "tokens": ["Durch", "Re\u00b7den", ",", "die", "er", "mit", "mir", "tauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gang mir zu vers\u00fc\u00dfen.", "tokens": ["Den", "Gang", "mir", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn ich seiner m\u00fcde bin,", "tokens": ["Und", "wenn", "ich", "sei\u00b7ner", "m\u00fc\u00b7de", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er wartet auf mein Winken,", "tokens": ["Er", "war\u00b7tet", "auf", "mein", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gleich wendet er sich zur Rechten hin,", "tokens": ["Gleich", "wen\u00b7det", "er", "sich", "zur", "Rech\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und ich zieh' fort zur Linken.", "tokens": ["Und", "ich", "zieh'", "fort", "zur", "Lin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die L\u00fcfte sind mir dienstbar auch,", "tokens": ["Die", "L\u00fcf\u00b7te", "sind", "mir", "dienst\u00b7bar", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir im R\u00fccken wehen,", "tokens": ["Die", "mir", "im", "R\u00fc\u00b7cken", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wollen doch mit ihrem Hauch", "tokens": ["Sie", "wol\u00b7len", "doch", "mit", "ih\u00b7rem", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich f\u00f6rdern nur im Gehen.", "tokens": ["Mich", "f\u00f6r\u00b7dern", "nur", "im", "Ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und die ins Angesicht mich k\u00fc\u00dft,", "tokens": ["Und", "die", "ins", "An\u00b7ge\u00b7sicht", "mich", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie will mir auch nicht schaden:", "tokens": ["Sie", "will", "mir", "auch", "nicht", "scha\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es ist die Ferne, die mich gr\u00fc\u00dft,", "tokens": ["Es", "ist", "die", "Fer\u00b7ne", ",", "die", "mich", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu sich mich einzuladen.", "tokens": ["Zu", "sich", "mich", "ein\u00b7zu\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Der Regen und der Sonnenschein", "tokens": ["Der", "Re\u00b7gen", "und", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind meine zwei Gesellen,", "tokens": ["Sind", "mei\u00b7ne", "zwei", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die, einer hinterm andern drein,", "tokens": ["Die", ",", "ei\u00b7ner", "hin\u00b7term", "an\u00b7dern", "drein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "APPRART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Abwechselnd ein sich stellen.", "tokens": ["Ab\u00b7wech\u00b7selnd", "ein", "sich", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Regen l\u00f6scht der Stra\u00dfe Staub,", "tokens": ["Der", "Re\u00b7gen", "l\u00f6scht", "der", "Stra\u00b7\u00dfe", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sonne macht sie trocken;", "tokens": ["Die", "Son\u00b7ne", "macht", "sie", "tro\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Daneben wollen Gras und Laub", "tokens": ["Da\u00b7ne\u00b7ben", "wol\u00b7len", "Gras", "und", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie aus dem Boden locken.", "tokens": ["Sie", "aus", "dem", "Bo\u00b7den", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und spannt in ihrem Wechselspiel", "tokens": ["Und", "spannt", "in", "ih\u00b7rem", "Wech\u00b7sel\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich aus ein Regenbogen,", "tokens": ["Sich", "aus", "ein", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm' ich, entgegen meinem Ziel,", "tokens": ["Komm'", "ich", ",", "ent\u00b7ge\u00b7gen", "mei\u00b7nem", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darunter hergezogen.", "tokens": ["Da\u00b7run\u00b7ter", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Bogen ist f\u00fcr mich gespannt,", "tokens": ["Der", "Bo\u00b7gen", "ist", "f\u00fcr", "mich", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ich darunter walle;", "tokens": ["Weil", "ich", "da\u00b7run\u00b7ter", "wal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu Tr\u00e4gern sind die Berg' ernannt,", "tokens": ["Zu", "Tr\u00e4\u00b7gern", "sind", "die", "Ber\u00b7g'", "er\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Da\u00df er auf mich nicht falle.", "tokens": ["Da\u00df", "er", "auf", "mich", "nicht", "fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.17": {"line.1": {"text": "Und wo ein Dorf entgegentritt,", "tokens": ["Und", "wo", "ein", "Dorf", "ent\u00b7ge\u00b7gen\u00b7tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da h\u00f6r' ich Glocken l\u00e4uten.", "tokens": ["Da", "h\u00f6r'", "ich", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie meinen selber mich damit,", "tokens": ["Sie", "mei\u00b7nen", "sel\u00b7ber", "mich", "da\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was k\u00f6nnt' es sonst bedeuten?", "tokens": ["Was", "k\u00f6nnt'", "es", "sonst", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie l\u00e4uten etwan einer Braut,", "tokens": ["Sie", "l\u00e4u\u00b7ten", "et\u00b7wan", "ei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Vielleicht auch einem Toten;", "tokens": ["Viel\u00b7leicht", "auch", "ei\u00b7nem", "To\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich aber deut' auf mich den Laut:", "tokens": ["Ich", "a\u00b7ber", "deut'", "auf", "mich", "den", "Laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Gru\u00df wird mir geboten.", "tokens": ["Ein", "Gru\u00df", "wird", "mir", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "So zieh' ich im Triumphgesang", "tokens": ["So", "zieh'", "ich", "im", "Tri\u00b7umph\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Entlang die lange Stra\u00dfe;", "tokens": ["Ent\u00b7lang", "die", "lan\u00b7ge", "Stra\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nie wird mir um etwas bang',", "tokens": ["Und", "nie", "wird", "mir", "um", "et\u00b7was", "bang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich im R\u00fccken lasse.", "tokens": ["Das", "ich", "im", "R\u00fc\u00b7cken", "las\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie eines hinter mir entweicht,", "tokens": ["Wie", "ei\u00b7nes", "hin\u00b7ter", "mir", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kommt gleich her das andre;", "tokens": ["So", "kommt", "gleich", "her", "das", "and\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und nie hab' ich das End' erreicht", "tokens": ["Und", "nie", "hab'", "ich", "das", "End'", "er\u00b7reicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Welt, soweit ich wandre.", "tokens": ["Der", "Welt", ",", "so\u00b7weit", "ich", "wand\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}