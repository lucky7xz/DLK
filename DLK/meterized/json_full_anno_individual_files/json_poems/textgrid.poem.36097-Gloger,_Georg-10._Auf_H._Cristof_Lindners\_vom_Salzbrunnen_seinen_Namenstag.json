{"textgrid.poem.36097": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "10. Auf H. Cristof Lindners\\ vom Salzbrunnen seinen Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann nicht wie ich wil. Apollo z\u00fcrnt mit mir", "tokens": ["Ich", "kann", "nicht", "wie", "ich", "wil", ".", "A\u00b7pol\u00b7lo", "z\u00fcrnt", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "PWAV", "PPER", "VMFIN", "$.", "NE", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd sieht mich sawer an, so da\u00df ich heute dir,", "tokens": ["Vnd", "sieht", "mich", "sa\u00b7wer", "an", ",", "so", "da\u00df", "ich", "heu\u00b7te", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ADV", "KOUS", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie schuldig ich auch bin, nichts Artiges kan singen.", "tokens": ["Wie", "schul\u00b7dig", "ich", "auch", "bin", ",", "nichts", "Ar\u00b7ti\u00b7ges", "kan", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "PIS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Saiten sind zu schlaff. Die Leyer wil nicht klingen.", "tokens": ["Die", "Sai\u00b7ten", "sind", "zu", "schlaff", ".", "Die", "Le\u00b7yer", "wil", "nicht", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$.", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist mir ganz verstimmt. Kein Wirbel wil nicht stehn;", "tokens": ["Es", "ist", "mir", "ganz", "ver\u00b7stimmt", ".", "Kein", "Wir\u00b7bel", "wil", "nicht", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PIAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Sinnen Uhrwerk stockt, vnd wil nicht richtig gehn.", "tokens": ["Der", "Sin\u00b7nen", "Uhr\u00b7werk", "stockt", ",", "vnd", "wil", "nicht", "rich\u00b7tig", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "KON", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Geistes Flug wird Bley, kann nicht bergan mehr steigen.", "tokens": ["Des", "Geis\u00b7tes", "Flug", "wird", "Bley", ",", "kann", "nicht", "ber\u00b7gan", "mehr", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,", "VMFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So mu\u00df ich deinen Tag mit lauter Stilleschweigen", "tokens": ["So", "mu\u00df", "ich", "dei\u00b7nen", "Tag", "mit", "lau\u00b7ter", "Stil\u00b7le\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd nichts nicht thun begehen. So pflegt es mir zu gehn;", "tokens": ["Vnd", "nichts", "nicht", "thun", "be\u00b7ge\u00b7hen", ".", "So", "pflegt", "es", "mir", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKNEG", "VVINF", "VVINF", "$.", "ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Denn, wenn zuweilen ich am besten soll bestehn,", "tokens": ["Denn", ",", "wenn", "zu\u00b7wei\u00b7len", "ich", "am", "bes\u00b7ten", "soll", "be\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "PPER", "PTKA", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da wil es ganz nicht fort. Nichts meld ich von dem Kr\u00e4nken,", "tokens": ["Da", "wil", "es", "ganz", "nicht", "fort", ".", "Nichts", "meld", "ich", "von", "dem", "Kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$.", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df ich dich nicht, wie Brauch, mit etwas soll beschenken,", "tokens": ["Da\u00df", "ich", "dich", "nicht", ",", "wie", "Brauch", ",", "mit", "et\u00b7was", "soll", "be\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "$,", "PWAV", "NN", "$,", "APPR", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das deiner w\u00fcrdig ist. Es mangelt nichts als That;", "tokens": ["Das", "dei\u00b7ner", "w\u00fcr\u00b7dig", "ist", ".", "Es", "man\u00b7gelt", "nichts", "als", "That", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "An W\u00fcndschen bin ich reich, vnd habe Vorrat satt.", "tokens": ["An", "W\u00fcnd\u00b7schen", "bin", "ich", "reich", ",", "vnd", "ha\u00b7be", "Vor\u00b7rat", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,", "KON", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch denk' ich, wenn ich dir dasjenige nur gebe,", "tokens": ["Doch", "denk'", "ich", ",", "wenn", "ich", "dir", "das\u00b7je\u00b7ni\u00b7ge", "nur", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "PDS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So viel mein Reichthumb tr\u00e4gt, von dem ich selbst auch lebe,", "tokens": ["So", "viel", "mein", "Reicht\u00b7humb", "tr\u00e4gt", ",", "von", "dem", "ich", "selbst", "auch", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So sey es alles gut, nimst du den Willen an,", "tokens": ["So", "sey", "es", "al\u00b7les", "gut", ",", "nimst", "du", "den", "Wil\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So hat di\u00df mein Pr\u00e4sent das Seine wol gethan,", "tokens": ["So", "hat", "di\u00df", "mein", "Pr\u00e4\u00b7sent", "das", "Sei\u00b7ne", "wol", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "PPOSAT", "NN", "ART", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.19": {"text": "Bey mir vnd auch bey dir, was n\u00fctzt es, da\u00df ichs sage,", "tokens": ["Bey", "mir", "vnd", "auch", "bey", "dir", ",", "was", "n\u00fctzt", "es", ",", "da\u00df", "ichs", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ADV", "APPR", "PPER", "$,", "PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.20": {"text": "Da\u00df ich das H\u00f6chste noch in meinem Herzen trage?", "tokens": ["Da\u00df", "ich", "das", "H\u00f6chs\u00b7te", "noch", "in", "mei\u00b7nem", "Her\u00b7zen", "tra\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich kann nicht wie ich wil. Apollo z\u00fcrnt mit mir", "tokens": ["Ich", "kann", "nicht", "wie", "ich", "wil", ".", "A\u00b7pol\u00b7lo", "z\u00fcrnt", "mit", "mir"], "t