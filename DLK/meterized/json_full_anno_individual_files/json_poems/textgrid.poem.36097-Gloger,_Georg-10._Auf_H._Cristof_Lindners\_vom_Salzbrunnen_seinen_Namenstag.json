ol", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "PPOSAT", "NN", "ART", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.19": {"text": "Bey mir vnd auch bey dir, was n\u00fctzt es, da\u00df ichs sage,", "tokens": ["Bey", "mir", "vnd", "auch", "bey", "dir", ",", "was", "n\u00fctzt", "es", ",", "da\u00df", "ichs", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ADV", "APPR", "PPER", "$,", "PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.20": {"text": "Da\u00df ich das H\u00f6chste noch in meinem Herzen trage?", "tokens": ["Da\u00df", "ich", "das", "H\u00f6chs\u00b7te", "noch", "in", "mei\u00b7nem", "Her\u00b7zen", "tra\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}