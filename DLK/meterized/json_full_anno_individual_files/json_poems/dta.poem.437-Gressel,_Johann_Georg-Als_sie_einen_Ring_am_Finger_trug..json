{"dta.poem.437": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie einen Ring am Finger trug.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wie/ tr\u00e4gt die Marmor Hand Gold und auch Demant-stein/", "tokens": ["Wie", "/", "tr\u00e4gt", "die", "Mar\u00b7mor", "Hand", "Gold", "und", "auch", "De\u00b7mant\u00b7stein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VVFIN", "ART", "NN", "NN", "NN", "KON", "ADV", "NN", "$("], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da ihre Artigkeit dieselben \u00fcberwieget?", "tokens": ["Da", "ih\u00b7re", "Ar\u00b7tig\u00b7keit", "die\u00b7sel\u00b7ben", "\u00fc\u00b7berw\u00b7ie\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PDAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch still! du hast mein Hertz durch einen Blick besieget/", "tokens": ["Doch", "still", "!", "du", "hast", "mein", "Hertz", "durch", "ei\u00b7nen", "Blick", "be\u00b7sie\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun soll auch meine Treu so vest wie Demant seyn.", "tokens": ["Nun", "soll", "auch", "mei\u00b7ne", "Treu", "so", "vest", "wie", "De\u00b7mant", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word