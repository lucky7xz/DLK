{"dta.poem.11693": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Selbstgest\u00e4ndniss.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Und weil die andern ausblieben sind,", "tokens": ["Und", "weil", "die", "an\u00b7dern", "aus\u00b7blie\u00b7ben", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was wei\u00df ich wie viel, die Sechs oder Sieben,", "tokens": ["Was", "wei\u00df", "ich", "wie", "viel", ",", "die", "Sechs", "o\u00b7der", "Sie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KOKOM", "ADV", "$,", "PRELS", "CARD", "KON", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So ist eben Alles an mir h\u00e4ngen blieben;", "tokens": ["So", "ist", "e\u00b7ben", "Al\u00b7les", "an", "mir", "h\u00e4n\u00b7gen", "blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ich hab' m\u00fcssen die Liebe, die Treue, die G\u00fcte", "tokens": ["Ich", "hab'", "m\u00fcs\u00b7sen", "die", "Lie\u00b7be", ",", "die", "Treu\u00b7e", ",", "die", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VMFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "F\u00fcr ein ganz halb Dutzend allein aufessen,", "tokens": ["F\u00fcr", "ein", "ganz", "halb", "Dut\u00b7zend", "al\u00b7lein", "au\u00b7fes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+---+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ich will's mein Lebtag nicht vergessen.", "tokens": ["Ich", "will's", "mein", "Leb\u00b7tag", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es h\u00e4tte mir aber noch wohl m\u00f6gen frommen,", "tokens": ["Es", "h\u00e4t\u00b7te", "mir", "a\u00b7ber", "noch", "wohl", "m\u00f6\u00b7gen", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "H\u00e4tt' ich nur auch Schl\u00e4g' f\u00fcr Sechse bekommen.", "tokens": ["H\u00e4tt'", "ich", "nur", "auch", "Schl\u00e4g'", "f\u00fcr", "Sech\u00b7se", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}}}}