": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zween kleine B\u00e4che murmelnd fliessen.", "tokens": ["Zween", "klei\u00b7ne", "B\u00e4\u00b7che", "mur\u00b7melnd", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie setzten sich an einen gr\u00fcnen H\u00fcgel,", "tokens": ["Sie", "setz\u00b7ten", "sich", "an", "ei\u00b7nen", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Moo\u00df und Rohr die Schoo\u00df, ein Wald den R\u00fccken, deckte,", "tokens": ["Dem", "Moo\u00df", "und", "Rohr", "die", "Schoo\u00df", ",", "ein", "Wald", "den", "R\u00fc\u00b7cken", ",", "deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und der den fetten Fu\u00df ins klare Wasser streckte;", "tokens": ["Und", "der", "den", "fet\u00b7ten", "Fu\u00df", "ins", "kla\u00b7re", "Was\u00b7ser", "streck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie sah'n die Silber-reine Fluht,", "tokens": ["Sie", "sah'n", "die", "Sil\u00b7ber\u00b7rei\u00b7ne", "Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter