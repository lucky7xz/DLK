{"textgrid.poem.35075": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Meine Engel", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df im lieben, trauten St\u00fcbchen,", "tokens": ["Ich", "sa\u00df", "im", "lie\u00b7ben", ",", "trau\u00b7ten", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Grad als der Tag dem Abend wich.", "tokens": ["Grad", "als", "der", "Tag", "dem", "A\u00b7bend", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein kleines, s\u00fc\u00dfes Herzensb\u00fcbchen", "tokens": ["Mein", "klei\u00b7nes", ",", "s\u00fc\u00b7\u00dfes", "Her\u00b7zens\u00b7b\u00fcb\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlang seine Aermchen warm um mich.", "tokens": ["Schlang", "sei\u00b7ne", "A\u00b7erm\u00b7chen", "warm", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.5": {"text": "Da strich, nicht etwa von der Sonne,", "tokens": ["Da", "strich", ",", "nicht", "et\u00b7wa", "von", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An uns vorbei ein lichter Schein,", "tokens": ["An", "uns", "vor\u00b7bei", "ein", "lich\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ich gedachte voller Wonne:", "tokens": ["Und", "ich", "ge\u00b7dach\u00b7te", "vol\u00b7ler", "Won\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbdas wird des Kindes Engel sein!\u00ab", "tokens": ["\u00bb", "das", "wird", "des", "Kin\u00b7des", "En\u00b7gel", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "NE", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wachte an dem Krankenlager.", "tokens": ["Ich", "wach\u00b7te", "an", "dem", "Kran\u00b7ken\u00b7la\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es war so d\u00fcster in dem Raum!", "tokens": ["Es", "war", "so", "d\u00fcs\u00b7ter", "in", "dem", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Leidenden Gesicht so hager;", "tokens": ["Der", "Lei\u00b7den\u00b7den", "Ge\u00b7sicht", "so", "ha\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man unterschied die Z\u00fcge kaum.", "tokens": ["Man", "un\u00b7ter\u00b7schied", "die", "Z\u00fc\u00b7ge", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir beteten; da pl\u00f6tzlich legte", "tokens": ["Wir", "be\u00b7te\u00b7ten", ";", "da", "pl\u00f6tz\u00b7lich", "leg\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich um ihr Haupt ein lichter Schein,", "tokens": ["Sich", "um", "ihr", "Haupt", "ein", "lich\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der den Gedanken in mir regte:", "tokens": ["Der", "den", "Ge\u00b7dan\u00b7ken", "in", "mir", "reg\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbdas wird der Kranken Engel sein!\u00ab", "tokens": ["\u00bb", "das", "wird", "der", "Kran\u00b7ken", "En\u00b7gel", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "NE", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er stand vor mir im halben Dunkel,", "tokens": ["Er", "stand", "vor", "mir", "im", "hal\u00b7ben", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Klinge in der Faust bereit;", "tokens": ["Die", "Klin\u00b7ge", "in", "der", "Faust", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Augs verr\u00e4therisch Gefunkel", "tokens": ["Des", "Augs", "ver\u00b7r\u00e4\u00b7the\u00b7risch", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gab mir zum Weichen nicht mehr Zeit.", "tokens": ["Gab", "mir", "zum", "Wei\u00b7chen", "nicht", "mehr", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da, als er auszuholen wagte,", "tokens": ["Da", ",", "als", "er", "aus\u00b7zu\u00b7ho\u00b7len", "wag\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Flo\u00df zwischen uns ein heller Schein;", "tokens": ["Flo\u00df", "zwi\u00b7schen", "uns", "ein", "hel\u00b7ler", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es sank die Hand; ich aber sagte:", "tokens": ["Es", "sank", "die", "Hand", ";", "ich", "a\u00b7ber", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbdas wird vielleicht dein Engel sein!\u00ab", "tokens": ["\u00bb", "das", "wird", "viel\u00b7leicht", "dein", "En\u00b7gel", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es lag die Bibel aufgeschlagen,", "tokens": ["Es", "lag", "die", "Bi\u00b7bel", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und der Verleumder stand dabei,", "tokens": ["Und", "der", "Ver\u00b7leum\u00b7der", "stand", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um auf das heilge Buch zu sagen,", "tokens": ["Um", "auf", "das", "heil\u00b7ge", "Buch", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df seine L\u00fcge Wahrheit sei.", "tokens": ["Da\u00df", "sei\u00b7ne", "L\u00fc\u00b7ge", "Wahr\u00b7heit", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da war ein fremder Ton zu h\u00f6ren,", "tokens": ["Da", "war", "ein", "frem\u00b7der", "Ton", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie \u00fcberirdisch, warnend, fein.", "tokens": ["Wie", "\u00fc\u00b7be\u00b7rir\u00b7disch", ",", "war\u00b7nend", ",", "fein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Mann schrie auf: \u00bbIch will nicht schw\u00f6ren,", "tokens": ["Der", "Mann", "schrie", "auf", ":", "\u00bb", "Ich", "will", "nicht", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn das, das wird mein Engel sein!\u00ab", "tokens": ["Denn", "das", ",", "das", "wird", "mein", "En\u00b7gel", "sein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bin ich dereinst bereit zum Scheiden,", "tokens": ["Bin", "ich", "de\u00b7reinst", "be\u00b7reit", "zum", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihr steht weinend um mich her,", "tokens": ["Und", "ihr", "steht", "wei\u00b7nend", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mag es Tr\u00f6stung euch bereiten,", "tokens": ["So", "mag", "es", "Tr\u00f6s\u00b7tung", "euch", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich zur\u00fcck zum Vater kehr.", "tokens": ["Da\u00df", "ich", "zu\u00b7r\u00fcck", "zum", "Va\u00b7ter", "kehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Habt Acht auf einen lichten Schimmer,", "tokens": ["Habt", "Acht", "auf", "ei\u00b7nen", "lich\u00b7ten", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf einen Ton, ersterbend lind,", "tokens": ["Auf", "ei\u00b7nen", "Ton", ",", "ers\u00b7ter\u00b7bend", "lind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "ADJD", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und trifft es ein, so zweifelt nimmer,", "tokens": ["Und", "trifft", "es", "ein", ",", "so", "zwei\u00b7felt", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df dies dann meine Engel sind!", "tokens": ["Da\u00df", "dies", "dann", "mei\u00b7ne", "En\u00b7gel", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich sa\u00df im lieben, trauten St\u00fcbchen,", "tokens": ["Ich", "sa\u00df", "im", "lie\u00b7ben", ",", "trau\u00b7