{"dta.poem.6532": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An einem heitern Tag, wenn \u00f6ffters St\u00fcrm' entstehn,", "tokens": ["An", "ei\u00b7nem", "hei\u00b7tern", "Tag", ",", "wenn", "\u00f6ff\u00b7ters", "St\u00fcrm'", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erblickt man finstrer Wolcken Decken,", "tokens": ["Er\u00b7blickt", "man", "finst\u00b7rer", "Wol\u00b7cken", "De\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie den hellen Glantz der Sonnen offt verstecken:", "tokens": ["Wie", "sie", "den", "hel\u00b7len", "Glantz", "der", "Son\u00b7nen", "offt", "ver\u00b7ste\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es kan ihr Funckeln nicht zu uns hinunter gehn,", "tokens": ["Es", "kan", "ihr", "Fun\u00b7ckeln", "nicht", "zu", "uns", "hin\u00b7un\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ohn\u2019 Krafft und ohn\u2019 Gebrauch ist ihrer Strahlen Heer,", "tokens": ["Ohn'", "Krafft", "und", "ohn'", "Ge\u00b7brauch", "ist", "ih\u00b7rer", "Strah\u00b7len", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man siehet fast kein Licht und keine Sonne mehr.", "tokens": ["Man", "sie\u00b7het", "fast", "kein", "Licht", "und", "kei\u00b7ne", "Son\u00b7ne", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "KON", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}