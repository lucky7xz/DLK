{"textgrid.poem.66790": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er war von Mutterleib der Brave", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er war von Mutterleib der Brave", "tokens": ["Er", "war", "von", "Mut\u00b7ter\u00b7leib", "der", "Bra\u00b7ve"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und hat sich trefflich aufgef\u00fchrt,", "tokens": ["Und", "hat", "sich", "treff\u00b7lich", "auf\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er war das zahmste aller Schafe,", "tokens": ["Er", "war", "das", "zahms\u00b7te", "al\u00b7ler", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem stets der Demut Kranz geb\u00fchrt.", "tokens": ["Dem", "stets", "der", "De\u00b7mut", "Kranz", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er war gehorsam in der Schule,", "tokens": ["Er", "war", "ge\u00b7hor\u00b7sam", "in", "der", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Bakel lehrt' ihn seine Pflicht,", "tokens": ["Der", "Ba\u00b7kel", "lehrt'", "ihn", "sei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er tr\u00e4umte nie vom fernen Thule,", "tokens": ["Er", "tr\u00e4um\u00b7te", "nie", "vom", "fer\u00b7nen", "Thu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn unvern\u00fcnftig war er nicht.", "tokens": ["Denn", "un\u00b7ver\u00b7n\u00fcnf\u00b7tig", "war", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit Glorie macht' er sein Examen,", "tokens": ["Mit", "Glo\u00b7rie", "macht'", "er", "sein", "E\u00b7xa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward vielbeflissener Jurist,", "tokens": ["Ward", "viel\u00b7be\u00b7flis\u00b7se\u00b7ner", "Ju\u00b7rist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Er schw\u00e4rmte sehr f\u00fcr leichte Damen", "tokens": ["Er", "schw\u00e4rm\u00b7te", "sehr", "f\u00fcr", "leich\u00b7te", "Da\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und war ein k\u00f6nigstreuer Christ.", "tokens": ["Und", "war", "ein", "k\u00f6\u00b7nigs\u00b7treu\u00b7er", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er hob urdeutscher Sitte Fahne", "tokens": ["Er", "hob", "ur\u00b7deut\u00b7scher", "Sit\u00b7te", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und br\u00fcllte weidlich mit im Schwarm,", "tokens": ["Und", "br\u00fcll\u00b7te", "weid\u00b7lich", "mit", "im", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Macht' seinen Schatz zur Kurtisane", "tokens": ["Macht'", "sei\u00b7nen", "Schatz", "zur", "Kur\u00b7ti\u00b7sa\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und zahlt' sie aus \u2013 da\u00df Gott erbarm!", "tokens": ["Und", "zahlt'", "sie", "aus", "\u2013", "da\u00df", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$(", "KOUS", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er war ein eleganter, feiner,", "tokens": ["Er", "war", "ein", "e\u00b7leg\u00b7an\u00b7ter", ",", "fei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein h\u00f6chst charmanter, junger Held,", "tokens": ["Ein", "h\u00f6chst", "char\u00b7man\u00b7ter", ",", "jun\u00b7ger", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", 