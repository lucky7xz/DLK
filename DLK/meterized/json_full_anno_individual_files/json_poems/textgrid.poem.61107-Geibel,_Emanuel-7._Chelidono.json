{"textgrid.poem.61107": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7. Chelidono", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die Platane sich riesig erhebt im Schatten der Waldschlucht,", "tokens": ["Wo", "die", "Pla\u00b7ta\u00b7ne", "sich", "rie\u00b7sig", "er\u00b7hebt", "im", "Schat\u00b7ten", "der", "Wald\u00b7schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "---+--+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ragt in Tr\u00fcmmer bereits fallend das Kloster empor;", "tokens": ["Ragt", "in", "Tr\u00fcm\u00b7mer", "be\u00b7reits", "fal\u00b7lend", "das", "Klos\u00b7ter", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "L\u00e4ngst ist der M\u00f6nche Gesang in der Kirche verhallt, und es duftet", "tokens": ["L\u00e4ngst", "ist", "der", "M\u00f6n\u00b7che", "Ge\u00b7sang", "in", "der", "Kir\u00b7che", "ver\u00b7hallt", ",", "und", "es", "duf\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weihrauch nimmer, des Chors ewige Lampe verlosch;", "tokens": ["Weih\u00b7rauch", "nim\u00b7mer", ",", "des", "Chors", "e\u00b7wi\u00b7ge", "Lam\u00b7pe", "ver\u00b7losch", ";"], "token_info": ["word", "word", "punct", "