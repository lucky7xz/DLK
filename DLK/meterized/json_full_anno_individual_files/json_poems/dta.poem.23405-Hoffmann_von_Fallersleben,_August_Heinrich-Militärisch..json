{"dta.poem.23405": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Milit\u00e4risch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eha! was eilt die Stra\u00df entlang?", "tokens": ["\u201e", "ha", "!", "was", "eilt", "die", "Stra\u00df", "ent\u00b7lang", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie's da blitzt im Sonnenglanz!", "tokens": ["Wie's", "da", "blitzt", "im", "Son\u00b7nen\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trommelwirbel, Pfeifenklang!", "tokens": ["Trom\u00b7mel\u00b7wir\u00b7bel", ",", "Pfei\u00b7fen\u00b7klang", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig, hei\u00dfa! wie zum Tanz.\u201c", "tokens": ["Lus\u00b7tig", ",", "hei\u00b7\u00dfa", "!", "wie", "zum", "Tanz", ".", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "ITJ", "$.", "PWAV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind Soldaten, ziehn herein,", "tokens": ["Sind", "Sol\u00b7da\u00b7ten", ",", "ziehn", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen vom Begr\u00e4bni\u00df her,", "tokens": ["Kom\u00b7men", "vom", "Be\u00b7gr\u00e4b\u00b7ni\u00df", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcssen jetzo lustig sein,", "tokens": ["M\u00fcs\u00b7sen", "jet\u00b7zo", "lus\u00b7tig", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wenn nichts passieret w\u00e4r.", "tokens": ["Als", "wenn", "nichts", "pas\u00b7sie\u00b7ret", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sind Soldaten, liebes Kind,", "tokens": ["Sind", "Sol\u00b7da\u00b7ten", ",", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die nicht Tod und Teufel scheu'n,", "tokens": ["Die", "nicht", "Tod", "und", "Teu\u00b7fel", "scheu'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf Commando traurig sind", "tokens": ["Auf", "Com\u00b7man\u00b7do", "trau\u00b7rig", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJD", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sich auf Commando freu'n.", "tokens": ["Und", "sich", "auf", "Com\u00b7man\u00b7do", "freu'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PRF", "APPR", "NE", "NE", "NE"], "meter": "---+--+", "measure": "iambic.di.chol"}}}}}