lagen;", "tokens": ["Was", "hilft", "dir", "al\u00b7les", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein mehr als \u00e4ngstlich Klagen", "tokens": ["Dein", "mehr", "als", "\u00e4ngst\u00b7lich", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "PIAT", "KOKOM", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Kann nicht dein Retter seyn.", "tokens": ["Kann", "nicht", "dein", "Ret\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der so die Welt erschaffen,", "tokens": ["Der", "so", "die", "Welt", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nimmt dich in seinen Schutz.", "tokens": ["Nimmt", "dich", "in", "sei\u00b7nen", "Schutz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wacht wenn du wilst schlafen,", "tokens": ["Er", "wacht", "wenn", "du", "wilst", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beut allen Feinden trutz.", "tokens": ["Beut", "al\u00b7len", "Fein\u00b7den", "trutz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn der sich will erbarmen,", "tokens": ["Wenn", "der", "sich", "will", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So leide was du kannst,", "tokens": ["So", "lei\u00b7de", "was", "du", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil du in seinen Armen,", "tokens": ["Weil", "du", "in", "sei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In deines Vaters Armen", "tokens": ["In", "dei\u00b7nes", "Va\u00b7ters", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das wahre Heyl erlangst.", "tokens": ["Das", "wah\u00b7re", "Heyl", "er\u00b7langst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er hat vor l\u00e4ngst gesehen", "tokens": ["Er", "hat", "vor", "l\u00e4ngst", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was gut und n\u00fctzlich ist.", "tokens": ["Was", "gut", "und", "n\u00fctz\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesetzt er l\u00e4\u00dft geschehen", "tokens": ["Ge\u00b7setzt", "er", "l\u00e4\u00dft", "ge\u