{"textgrid.poem.36121": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "18. Auf den un\u00fcberwindlichsten K\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wird in k\u00fcnftig denn wol ewer ", "tokens": ["Was", "wird", "in", "k\u00fcnf\u00b7tig", "denn", "wol", "e\u00b7wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ADJD", "KON", "ADV", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der vnbeherzte Mann? sagt ", "tokens": ["Der", "vn\u00b7be\u00b7herz\u00b7te", "Mann", "?", "sagt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich nahme zu der Hand den Rei\u00dfzeug vnd Papier,", "tokens": ["Ich", "nah\u00b7me", "zu", "der", "Hand", "den", "Rei\u00df\u00b7zeug", "vnd", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Malt einen Siegesherrn mit allen Siegessachen.", "tokens": ["Malt", "ei\u00b7nen", "Sie\u00b7ges\u00b7herrn", "mit", "al\u00b7len", "Sie\u00b7ges\u00b7sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Haupt Achilles war, wenn er die Feinde schluge,", "tokens": ["Das", "Haupt", "A\u00b7chil\u00b7les", "war", ",", "wenn", "er", "die", "Fein\u00b7de", "schlu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd nach verrichter Schlacht zu Hause kommen war.", "tokens": ["Vnd", "nach", "ver\u00b7rich\u00b7ter", "Schlacht", "zu", "Hau\u00b7se", "kom\u00b7men", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich wandt ihm gr\u00fcnes Laub vnd Gold in seine Haar,", "tokens": ["Ich", "wandt", "ihm", "gr\u00fc\u00b7nes", "Laub", "vnd", "Gold", "in", "sei\u00b7ne", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd in der rechten Hand er einen Scepter truge.", "tokens": ["Vnd", "in", "der", "rech\u00b7ten", "Hand", "er", "ei\u00b7nen", "Scep\u00b7ter", "tru\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zur Rechten prangten her viel Palm- vnd Lorbeersprossen,", "tokens": ["Zur", "Rech\u00b7ten", "prang\u00b7ten", "her", "viel", "Pal\u00b7m", "vnd", "Lor\u00b7beer\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADV", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Vnd die erworbne Beut' hier auf der linken Hand,", "tokens": ["Vnd", "die", "er\u00b7worb\u00b7ne", "Beut'", "hier", "auf", "der", "lin\u00b7ken", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Viel palmgeformbte R\u00f6ck' vnd steingesticktes Wand,", "tokens": ["Viel", "palm\u00b7ge\u00b7formb\u00b7te", "R\u00f6ck'", "vnd", "stein\u00b7ge\u00b7stick\u00b7tes", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auch Waffen, die noch frisch vom Staub vnd Blute flossen.", "tokens": ["Auch", "Waf\u00b7fen", ",", "die", "noch", "frisch", "vom", "Staub", "vnd", "Blu\u00b7te", "flos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "ADV", "ADJD", "APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sonst schrieb' ich oben an die Thaten vnd die Kriege,", "tokens": ["Sonst", "schrieb'", "ich", "o\u00b7ben", "an", "die", "Tha\u00b7ten", "vnd", "die", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Namen vnd das Volk, das nieder war gemacht.", "tokens": ["Die", "Na\u00b7men", "vnd", "das", "Volk", ",", "das", "nie\u00b7der", "war", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "K