{"dta.poem.3011": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sich ein Hausherr freut zu sehn ein Kinderpaar", "tokens": ["Wie", "sich", "ein", "Haus\u00b7herr", "freut", "zu", "sehn", "ein", "Kin\u00b7der\u00b7paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Daseyns froh und froh auch die Gesindeschaar;", "tokens": ["Des", "Da\u00b7seyns", "froh", "und", "froh", "auch", "die", "Ge\u00b7sin\u00b7de\u00b7schaar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "ADV", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Er freut sich, wenn sie treu ihr Tagwerk freudig thun,", "tokens": ["Er", "freut", "sich", ",", "wenn", "sie", "treu", "ihr", "Tag\u00b7werk", "freu\u00b7dig", "thun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mehr noch, wenn vergn\u00fcgt sie vom gethanen ruhn:", "tokens": ["Und", "mehr", "noch", ",", "wenn", "ver\u00b7gn\u00fcgt", "sie", "vom", "ge\u00b7tha\u00b7nen", "ruhn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie m\u00fc\u00dfte sich erst freun ein F\u00fcrst, der ebenso,", "tokens": ["Wie", "m\u00fc\u00df\u00b7te", "sich", "erst", "freun", "ein", "F\u00fcrst", ",", "der", "e\u00b7ben\u00b7so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im weitern Kreise nur, s\u00e4h' all die Seinen froh;", "tokens": ["Im", "wei\u00b7tern", "Krei\u00b7se", "nur", ",", "s\u00e4h'", "all", "die", "Sei\u00b7nen", "froh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,", "VVFIN", "PIAT", "ART", "PPOSS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn auch dem Landesherrn Gott wie dem Hausherrn g\u00f6nnte,", "tokens": ["Wenn", "auch", "dem", "Lan\u00b7des\u00b7herrn", "Gott", "wie", "dem", "Haus\u00b7herrn", "g\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df jeden Wunsch er so zufrieden stellen k\u00f6nnte!", "tokens": ["Da\u00df", "je\u00b7den", "Wunsch", "er", "so", "zu\u00b7frie\u00b7den", "stel\u00b7len", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Darum ist selig nur der h\u00f6chste Herr im Himmel,", "tokens": ["Da\u00b7rum", "ist", "se\u00b7lig", "nur", "der", "h\u00f6chs\u00b7te", "Herr", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er beseligen kann alles Weltgewimmel.", "tokens": ["Weil", "er", "be\u00b7se\u00b7li\u00b7gen", "kann", "al\u00b7les", "Welt\u00b7ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}