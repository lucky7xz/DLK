{"textgrid.poem.47318": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6pfung ist zur Ruh' gegangen, o wach in mir!", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "ist", "zur", "Ruh'", "ge\u00b7gan\u00b7gen", ",", "o", "wach", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es will der Schlaf auch mich befangen, o wach in mir!", "tokens": ["Es", "will", "der", "Schlaf", "auch", "mich", "be\u00b7fan\u00b7gen", ",", "o", "wach", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PPER", "VVPP", "$,", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du Auge, das am Himmel wachet mit Sternenblick,", "tokens": ["Du", "Au\u00b7ge", ",", "das", "am", "Him\u00b7mel", "wa\u00b7chet", "mit", "Ster\u00b7nen\u00b7blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn mir die Augen zugegangen, o wach in mir!", "tokens": ["Wenn", "mir", "die", "Au\u00b7gen", "zu\u00b7ge\u00b7gan\u00b7gen", ",", "o", "wach", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du Licht, im \u00c4ther h\u00f6her strahlend als Sonn' und Mond;", "tokens": ["Du", "Licht", ",", "im", "\u00c4\u00b7ther", "h\u00f6\u00b7her", "strah\u00b7lend", "als", "Sonn'", "und", "Mond", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPRART", "NN", "ADJD", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wenn Sonn' und Mond ist ausgegangen, o wach in mir!", "tokens": ["Wenn", "Sonn'", "und", "Mond", "ist", "aus\u00b7ge\u00b7gan\u00b7gen", ",", "o", "wach", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VAFIN", "VVPP", "$,", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wenn sich der Sinne Thor geschlossen der Au\u00dfenwelt,", "tokens": ["Wenn", "sich", "der", "Sin\u00b7ne", "Thor", "ge\u00b7schlos\u00b7sen", "der", "Au\u00b7\u00dfen\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "So la\u00df die Seel' in sich nicht bangen, o wach in mir!", "tokens": ["So", "la\u00df", "die", "Seel'", "in", "sich", "nicht", "ban\u00b7gen", ",", "o", "wach", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "APPR", "PRF", "PTKNEG", "VVINF", "$,", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "La\u00df nicht die Macht der Finsternisse, das Grau'n der Nacht", "tokens": ["La\u00df", "nicht", "die", "Macht", "der", "Fins\u00b7ter\u00b7nis\u00b7se", ",", "das", "Grau'n", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Sieg \u00fcbers innre Licht erlangen, o wach in mir!", "tokens": ["Sieg", "\u00fc\u00b7bers", "inn\u00b7re", "Licht", "er\u00b7lan\u00b7gen", ",", "o", "wach", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVINF", "$,", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "O la\u00df im feuchten Hauch der N\u00e4chte, im Schattenduft", "tokens": ["O", "la\u00df", "im", "feuch\u00b7ten", "Hauch", "der", "N\u00e4ch\u00b7te", ",", "im", "Schat\u00b7ten\u00b7duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Nicht sprossen s\u00fcndiges Verlangen, o wach in mir!", "tokens": ["Nicht", "spros\u00b7sen", "s\u00fcn\u00b7di\u00b7ges", "Ver\u00b7lan\u00b7gen", ",", "o", "wach", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADJA", "NN", "$,", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "La\u00df aus dem Duft von Edens Zweigen in meinem Traum", "tokens": ["La\u00df", "aus", "dem", "Duft", "von", "E\u00b7dens", "Zwei\u00b7gen", "in", "mei\u00b7nem", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "NN", "APPR", "NE", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Die Frucht des Lebens niederhangen o wach in mir!", "tokens": ["Die", "Frucht", "des", "Le\u00b7bens", "nie\u00b7der\u00b7han\u00b7gen", "o", "wach", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "O zeige mir, mich zu erquicken, im Traum das Werk", "tokens": ["O", "zei\u00b7ge", "mir", ",", "mich", "zu", "er\u00b7qui\u00b7cken", ",", "im", "Traum", "das", "Werk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.16": {"text": "Geendet, das ich angefangen, o wach in mir!", "tokens": ["Ge\u00b7en\u00b7det", ",", "das", "ich", "an\u00b7ge\u00b7fan\u00b7gen", ",", "o", "wach", "in", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPER", "VVPP", "$,", "FM", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "In deinem Scho\u00dfe will ich schlummern, bis neu mich weckt", "tokens": ["In", "dei\u00b7nem", "Scho\u00b7\u00dfe", "will", "ich", "schlum\u00b7mern", ",", "bis", "neu", "mich", "weckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,", "KOU