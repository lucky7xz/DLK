{"dta.poem.1314": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Thomas-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Thomas sieht und gl\u00e4ubt: Hilff/ da\u00df ich ohne Sehen", "tokens": ["Dein", "Tho\u00b7mas", "sieht", "und", "gl\u00e4ubt", ":", "Hilff", "/", "da\u00df", "ich", "oh\u00b7ne", "Se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VVFIN", "KON", "VVFIN", "$.", "NN", "$(", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir willig gl\u00e4uben mag. Es zeigt dein N\u00e4gel-Mahl/", "tokens": ["Dir", "wil\u00b7lig", "gl\u00e4u\u00b7ben", "mag.", "Es", "zeigt", "dein", "N\u00e4\u00b7gel\u00b7Mahl", "/"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich geschrieben bin in deiner Kinder Zahl/", "tokens": ["Da\u00df", "ich", "ge\u00b7schrie\u00b7ben", "bin", "in", "dei\u00b7ner", "Kin\u00b7der", "Zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vermerckt in deiner Hand; es zeigt der Seiten-Stich/", "tokens": ["Ver\u00b7merckt", "in", "dei\u00b7ner", "Hand", ";", "es", "zeigt", "der", "Sei\u00b7ten\u00b7Stich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df auch dein heilges Blutt vergossen sey f\u00fcr mich.", "tokens": ["Da\u00df", "auch", "dein", "heil\u00b7ges", "Blutt", "ver\u00b7gos\u00b7sen", "sey", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du wirst den Hertzens-Wunsch/ o Heyland/ nicht verschm\u00e4-", "tokens": ["Du", "wirst", "den", "Hert\u00b7zens\u00b7Wunsch", "/", "o", "Hey\u00b7land", "/", "nicht", "ver\u00b7schm\u00e4"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "FM", "NN", "$(", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "hen:", "tokens": ["hen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Vom Trauen hilff zum Schann/ vom Glauben hilff zum Se-", "tokens": ["Vom", "Trau\u00b7en", "hilff", "zum", "Schann", "/", "vom", "Glau\u00b7ben", "hilff", "zum", "Se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$(", "APPRART", "NN", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "hen.", "tokens": ["hen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}