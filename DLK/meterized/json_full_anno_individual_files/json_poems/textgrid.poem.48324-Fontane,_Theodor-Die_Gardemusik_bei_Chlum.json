{"textgrid.poem.48324": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Gardemusik bei Chlum", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas fechten kann, r\u00fcckt vor auf Chlum,", "tokens": ["\u00bb", "was", "fech\u00b7ten", "kann", ",", "r\u00fcckt", "vor", "auf", "Chlum", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVINF", "VMFIN", "$,", "VVFIN", "APPR", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unsre Garde d\u00fcrstet nach neuem Ruhm,", "tokens": ["Uns\u00b7re", "Gar\u00b7de", "d\u00fcrs\u00b7tet", "nach", "neu\u00b7em", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sie zieht ", "tokens": ["Sie", "zieht"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Wir schleichen ", "tokens": ["Wir", "schlei\u00b7chen"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Musik ist nie so recht dabei,", "tokens": ["Mu\u00b7sik", "ist", "nie", "so", "recht", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenig Wolle und viel Geschrei.\u00ab", "tokens": ["We\u00b7nig", "Wol\u00b7le", "und", "viel", "Ge\u00b7schrei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Kapellmeister spricht's. Da blitzt es drunt'", "tokens": ["Ka\u00b7pell\u00b7meis\u00b7ter", "spricht'", "s.", "Da", "blitzt", "es", "drunt'"], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "VVIMP", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Aus staubiger Wolke, golden und bunt.", "tokens": ["Aus", "stau\u00b7bi\u00b7ger", "Wol\u00b7ke", ",", "gol\u00b7den", "und", "bunt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "KON", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbung'rische Husaren, wenn recht ich seh';", "tokens": ["\u00bb", "un\u00b7g'\u00b7ri\u00b7sche", "Hu\u00b7sa\u00b7ren", ",", "wenn", "recht", "ich", "seh'", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ihr Chok gilt ", "tokens": ["Ihr", "Chok", "gilt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Karree. Da springen, ohn' Unterschied,", "tokens": ["Kar\u00b7ree", ".", "Da", "sprin\u00b7gen", ",", "ohn'", "Un\u00b7ter\u00b7schied", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "All die gro\u00dfen Bl\u00e4ser ins erste Glied,", "tokens": ["All", "die", "gro\u00b7\u00dfen", "Bl\u00e4\u00b7ser", "ins", "ers\u00b7te", "Glied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Janitschar und Pauke schlie\u00dfen sich an,", "tokens": ["Ja\u00b7nit\u00b7schar", "und", "Pau\u00b7ke", "schlie\u00b7\u00dfen", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-++-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Obo, Klarinette, Mann f\u00fcr Mann,", "tokens": ["O\u00b7bo", ",", "Kla\u00b7ri\u00b7net\u00b7te", ",", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Fagott und Tuba \u2013 mehr, immer mehr,", "tokens": ["Fa\u00b7gott", "und", "Tu\u00b7ba", "\u2013", "mehr", ",", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$(", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und nun Kommando: \u00bbF\u00e4llt das Gewehr!\u00ab", "tokens": ["Und", "nun", "Kom\u00b7man\u00b7do", ":", "\u00bb", "F\u00e4llt", "das", "Ge\u00b7wehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Und die Ba\u00dfposaune, voll kriegrischem Zorn,", "tokens": ["Und", "die", "Ba\u00df\u00b7po\u00b7sau\u00b7ne", ",", "voll", "krie\u00b7gri\u00b7schem", "Zorn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Streckt ihre Z\u00fcge weithin nach vorn.", "tokens": ["Streckt", "ih\u00b7re", "Z\u00fc\u00b7ge", "weit\u00b7hin", "nach", "vorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Zu rechter Zeit. Denn schon sind sie da.", "tokens": ["Zu", "rech\u00b7ter", "Zeit", ".", "Denn", "schon", "sind", "sie", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "KON", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbergib dich, preu\u00dfische Musika!\u00ab", "tokens": ["\u00bb", "er\u00b7gib", "dich", ",", "preu\u00b7\u00dfi\u00b7sche", "Mu\u00b7si\u00b7ka", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kapellmeister aber winkt ab und spricht:", "tokens": ["Ka\u00b7pell\u00b7meis\u00b7ter", "a\u00b7ber", "winkt", "ab", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u00bbdie Gardemusik ergibt sich nicht.\u00ab", "tokens": ["\u00bb", "die", "Gar\u00b7de\u00b7mu\u00b7sik", "er\u00b7gibt", "sich", "nicht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und keiner wankt und keiner weicht,", "tokens": ["Und", "kei\u00b7ner", "wankt", "und", "kei\u00b7ner", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Posaun' und Tuba, die zwingt man nicht leicht,", "tokens": ["Po\u00b7saun'", "und", "Tu\u00b7ba", ",", "die", "zwingt", "man", "nicht", "leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,", "PRELS", "VVFIN", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Auch die Pauke h\u00e4lt sich wie ein Turm,", "tokens": ["Auch", "die", "Pau\u00b7ke", "h\u00e4lt", "sich", "wie", "ein", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "VVFIN", "PRF", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und siehe, vor\u00fcber braust der Sturm.", "tokens": ["Und", "sie\u00b7he", ",", "vor\u00b7\u00fc\u00b7ber", "braust", "der", "Sturm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da hebt sich unsres Kapellmeisters Brust:", "tokens": ["Da", "hebt", "sich", "uns\u00b7res", "Ka\u00b7pell\u00b7meis\u00b7ters", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwer ist gefalln? Wie steht der Verlust?\u00ab", "tokens": ["\u00bb", "wer", "ist", "ge\u00b7falln", "?", "Wie", "steht", "der", "Ver\u00b7lust", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "VVPP", "$.", "PWAV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bbgefallen keiner; leicht zerhaun", "tokens": ["\u00bb", "ge\u00b7fal\u00b7len", "kei\u00b7ner", ";", "leicht", "zer\u00b7haun"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVPP", "PIS", "$.", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Pauke, Tuba und Posaun',", "tokens": ["Sind", "Pau\u00b7ke", ",", "Tu\u00b7ba", "und", "Po\u00b7saun'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gestreift, geschrammt blo\u00df, sonst intakt,", "tokens": ["Ge\u00b7streift", ",", "ge\u00b7schrammt", "blo\u00df", ",", "sonst", "in\u00b7takt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nur das Fagott ist wie zerhackt!\u00ab", "tokens": ["Und", "nur", "das", "Fa\u00b7gott", "ist", "wie", "zer\u00b7hackt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "KOKOM", "VVPP", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbdrei leicht, einer schwer, der Rest gesund \u2013", "tokens": ["\u00bb", "drei", "leicht", ",", "ei\u00b7ner", "schwer", ",", "der", "Rest", "ge\u00b7sund", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "ADJD", "$,", "ART", "ADJD", "$,", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und fehlt uns auch unser brav Fagott,", "tokens": ["Und", "fehlt", "uns", "auch", "un\u00b7ser", "brav", "Fa\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wir blasen ", "tokens": ["Wir", "bla\u00b7sen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und blasen es durch und blasen es ganz,", "tokens": ["Und", "bla\u00b7sen", "es", "durch", "und", "bla\u00b7sen", "es", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und zum Schlusse: \u203aHeil dir im Siegerkranz.\u2039\u00ab", "tokens": ["Und", "zum", "Schlus\u00b7se", ":", "\u203a", "Heil", "dir", "im", "Sie\u00b7ger\u00b7kranz", ".", "\u2039", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "$.", "$(", "NN", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbwas fechten kann, r\u00fcckt vor auf Chlum,", "tokens": ["\u00bb", "was", "fech\u00b7ten", "kann", ",", "r\u00fcckt", "vor", "auf", "Chlum", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVINF", "VMFIN", "$,", "VVFIN", "APPR", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unsre Garde d\u00fcrstet nach neuem Ruhm,", "tokens": ["Uns\u00b7re", "Gar\u00b7de", "d\u00fcrs\u00b7tet", "nach", "neu\u00