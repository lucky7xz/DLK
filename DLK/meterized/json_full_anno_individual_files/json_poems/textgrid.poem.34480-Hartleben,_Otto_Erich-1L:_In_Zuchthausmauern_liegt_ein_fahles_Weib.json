{"textgrid.poem.34480": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Zuchthausmauern liegt ein fahles Weib", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Zuchthausmauern liegt ein fahles Weib", "tokens": ["In", "Zucht\u00b7haus\u00b7mau\u00b7ern", "liegt", "ein", "fah\u00b7les", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "auf seinem Bett. Es schl\u00e4ft, es tr\u00e4umt, es fiebert.", "tokens": ["auf", "sei\u00b7nem", "Bett", ".", "Es", "schl\u00e4ft", ",", "es", "tr\u00e4umt", ",", "es", "fie\u00b7bert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und heiser und mit trocknen Lippen fl\u00fcsterts:", "tokens": ["Und", "hei\u00b7ser", "und", "mit", "trock\u00b7nen", "Lip\u00b7pen", "fl\u00fcs\u00b7terts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "\u2013 Auf einem Mondstrahl .. muss es wieder kommen ...", "tokens": ["\u2013", "Auf", "ei\u00b7nem", "Mond\u00b7strahl", "..", "muss", "es", "wie\u00b7der", "kom\u00b7men", "..."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Mond mit einem schmalen, hellen Strahl", "tokens": ["Der", "Mond", "mit", "ei\u00b7nem", "schma\u00b7len", ",", "hel\u00b7len", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "spielt oben an der weissget\u00fcnchten Wand,", "tokens": ["spielt", "o\u00b7ben", "an", "der", "weiss\u00b7ge\u00b7t\u00fcnch\u00b7ten", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dem Bette gegen\u00fcber. Langsam r\u00fcckt er", "tokens": ["dem", "Bet\u00b7te", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ".", "Lang\u00b7sam", "r\u00fcckt", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "herab. Er gleitet auf den Arbeitstisch. \u2013", "tokens": ["her\u00b7ab", ".", "Er", "glei\u00b7tet", "auf", "den", "Ar\u00b7beits\u00b7tisch", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt liegt er auf dem Boden. Eine Leiter,", "tokens": ["Jetzt", "liegt", "er", "auf", "dem", "Bo\u00b7den", ".", "Ei\u00b7ne", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "hineingestellt vom hohen, kleinen Fenster,", "tokens": ["hin\u00b7ein\u00b7ge\u00b7stellt", "vom", "ho\u00b7hen", ",", "klei\u00b7nen", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "so steht er in der Zelle. Langsam, langsam,", "tokens": ["so", "steht", "er", "in", "der", "Zel\u00b7le", ".", "Lang\u00b7sam", ",", "lang\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "doch stetig weiter kriecht er. Immer n\u00e4her", "tokens": ["doch", "ste\u00b7tig", "wei\u00b7ter", "kriecht", "er", ".", "Im\u00b7mer", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "und n\u00e4her kommt er. \u2013 Auf dem Bette w\u00e4lzt", "tokens": ["und", "n\u00e4\u00b7her", "kommt", "er", ".", "\u2013", "Auf", "dem", "Bet\u00b7te", "w\u00e4lzt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "$(", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "unruhig sich das fieberkranke Weib,", "tokens": ["un\u00b7ru\u00b7hig", "sich", "das", "fie\u00b7ber\u00b7kran\u00b7ke", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "und immer wilder, banger r\u00f6chelt sie:", "tokens": ["und", "im\u00b7mer", "wil\u00b7der", ",", "ban\u00b7ger", "r\u00f6\u00b7chelt", "sie", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "$,", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u2013 Auf einem Mondstrahl .. muss es wieder kommen ...", "tokens": ["\u2013", "Auf", "ei\u00b7nem", "Mond\u00b7strahl", "..", "muss", "es", "wie\u00b7der", "kom\u00b7men", "..."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da, pl\u00f6tzlich schreit sie auf: Das Kind! Das Kind! \u2013", "tokens": ["Da", ",", "pl\u00f6tz\u00b7lich", "schreit", "sie", "auf", ":", "Das", "Kind", "!", "Das", "Kind", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wieder im Mond! Und toll springt sie empor. \u2013", "tokens": ["Wie\u00b7der", "im", "Mond", "!", "Und", "toll", "springt", "sie", "em\u00b7por", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Mond klimmt langsam an den Linnen auf", "tokens": ["Der", "Mond", "klimmt", "lang\u00b7sam", "an", "den", "Lin\u00b7nen", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und tastet auf die Kissen \u2013 weiter \u2013 weiter.", "tokens": ["und", "tas\u00b7tet", "auf", "die", "Kis\u00b7sen", "\u2013", "wei\u00b7ter", "\u2013", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "ADV", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wahnsinnig presst das Weib sich in die Ecke \u2013", "tokens": ["Wahn\u00b7sin\u00b7nig", "presst", "das", "Weib", "sich", "in", "die", "E\u00b7cke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "krampfhaft zerrt sie das Hemd um ihre Glieder,", "tokens": ["krampf\u00b7haft", "zerrt", "sie", "das", "Hemd", "um", "ih\u00b7re", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "die Z\u00e4hne klappern \u2013 und da h\u00f6rt sie gellen:", "tokens": ["die", "Z\u00e4h\u00b7ne", "klap\u00b7pern", "\u2013", "und", "da", "h\u00f6rt", "sie", "gel\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "KON", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt willst du fl\u00fcchten, fliehn: ich glaub es wohl!", "tokens": ["Jetzt", "willst", "du", "fl\u00fcch\u00b7ten", ",", "fliehn", ":", "ich", "glaub", "es", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,", "VVINF", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umsonst! Die Mauern halten dich, die Mauern!", "tokens": ["Um\u00b7sonst", "!", "Die", "Mau\u00b7ern", "hal\u00b7ten", "dich", ",", "die", "Mau\u00b7ern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hart sind die Mauern, hart und kalt und stumm,", "tokens": ["Hart", "sind", "die", "Mau\u00b7ern", ",", "hart", "und", "kalt", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und unerbittlich n\u00e4her kriecht der Mond \u2013", "tokens": ["und", "un\u00b7er\u00b7bitt\u00b7lich", "n\u00e4\u00b7her", "kriecht", "der", "Mond", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u2013 Auf einem Mondstrahl .. muss es wieder kommen ...", "tokens": ["\u2013", "Auf", "ei\u00b7nem", "Mond\u00b7strahl", "..", "muss", "es", "wie\u00b7der", "kom\u00b7men", "..."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein wildes Heulen weckt die W\u00e4rterin.", "tokens": ["Ein", "wil\u00b7des", "Heu\u00b7len", "weckt", "die", "W\u00e4r\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-