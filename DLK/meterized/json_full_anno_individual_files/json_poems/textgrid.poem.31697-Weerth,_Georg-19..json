{"textgrid.poem.31697": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Berge klang ein Klagelied:", "tokens": ["\u00dc\u00b7ber", "die", "Ber\u00b7ge", "klang", "ein", "Kla\u00b7ge\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Schwalbe war's, die von der Heimat schied.", "tokens": ["Die", "Schwal\u00b7be", "wa\u00b7r's", ",", "die", "von", "der", "Hei\u00b7mat", "schied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie hob sich hoch empor im Abendsonnenbrand \u2013", "tokens": ["Sie", "hob", "sich", "hoch", "em\u00b7por", "im", "A\u00b7bend\u00b7son\u00b7nen\u00b7brand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu schaun noch einmal ihrer Jugend Land.", "tokens": ["Zu", "schaun", "noch", "ein\u00b7mal", "ih\u00b7rer", "Ju\u00b7gend", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da war verwelkt der Auen frischer Flor,", "tokens": ["Da", "war", "ver\u00b7welkt", "der", "Au\u00b7en", "fri\u00b7scher", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verdorrt die Rebe \u00fcber grauem Tor,", "tokens": ["Ver\u00b7dorrt", "die", "Re\u00b7be", "\u00fc\u00b7ber", "grau\u00b7em", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entlaubt der Linden lustiges Gezweig,", "tokens": ["Ent\u00b7laubt", "der", "Lin\u00b7den", "lus\u00b7ti\u00b7ges", "Ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verweht die Rose in des Gartens Reich.", "tokens": ["Ver\u00b7weht", "die", "Ro\u00b7se", "in", "des", "Gar\u00b7tens", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kalt blies der Nachtwind durch des Dorfes Raum,", "tokens": ["Kalt", "blies", "der", "Nacht\u00b7wind", "durch", "des", "Dor\u00b7fes", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Und vor dem Haus, wo an des Daches Saum", "tokens": ["Und", "vor", "dem", "Haus", ",", "wo", "an", "des", "Da\u00b7ches", "Saum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text"