{"dta.poem.20950": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  \n Grafens  Flisco.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Das Hau\u00df von ", "tokens": ["Das", "Hau\u00df", "von"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Es solte dessen Fall die Herrschafft mir erwerben.", "tokens": ["Es", "sol\u00b7te", "des\u00b7sen", "Fall", "die", "Herr\u00b7schafft", "mir", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Himmel der verlacht der Menschen stoltzen", "tokens": ["Der", "Him\u00b7mel", "der", "ver\u00b7lacht", "der", "Men\u00b7schen", "stolt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schlu\u00df/", "tokens": ["Schlu\u00df", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und macht da\u00df b\u00f6ser Rath zu Wasser werden mu\u00df.", "tokens": ["Und", "macht", "da\u00df", "b\u00f6\u00b7ser", "Rath", "zu", "Was\u00b7ser", "wer\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "ADJA", "NN", "APPR", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}