{"dta.poem.8118": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Posaunrufen der Heerlager, die ernstanbetend", "tokens": ["Po\u00b7saun\u00b7ru\u00b7fen", "der", "Heer\u00b7la\u00b7ger", ",", "die", "erns\u00b7tan\u00b7be\u00b7tend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "--+--+---+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fortzogen, umscholl wehdrohend der Palmstadt Th\u00fcrme:", "tokens": ["Fort\u00b7zo\u00b7gen", ",", "um\u00b7scholl", "weh\u00b7dro\u00b7hend", "der", "Palm\u00b7stadt", "Th\u00fcr\u00b7me", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Todstag kam dunkel, und des Herrn Heer zog", "tokens": ["Der", "Tods\u00b7tag", "kam", "dun\u00b7kel", ",", "und", "des", "Herrn", "Heer", "zog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "KON", "ART", "NN", "NN", "VVFIN"], "meter": "-+--+-+-++-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und es sank f\u00fcrchterlich aufdonnernd Jericho!", "tokens": ["Und", "es", "sank", "f\u00fcrch\u00b7ter\u00b7lich", "auf\u00b7don\u00b7nernd", "Je\u00b7ri\u00b7cho", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVPP", "NE", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}