{"textgrid.poem.32875": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, ja, ich weis es, glaube mir,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, ja, ich weis es, glaube mir,", "tokens": ["Ja", ",", "ja", ",", "ich", "weis", "es", ",", "glau\u00b7be", "mir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "PTKVZ", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sah es selbst, da\u00df Damon hier", "tokens": ["Ich", "sah", "es", "selbst", ",", "da\u00df", "Da\u00b7mon", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vertraulich mit dir sprach:", "tokens": ["Ver\u00b7trau\u00b7lich", "mit", "dir", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich merkt es auch, da\u00df er dich pries,", "tokens": ["Ich", "merkt", "es", "auch", ",", "da\u00df", "er", "dich", "pries", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sah es, da er dich verlie\u00df,", "tokens": ["Und", "sah", "es", ",", "da", "er", "dich", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "