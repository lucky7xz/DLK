{"dta.poem.10928": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wankelmuth und Unachtsamkeit  \n der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie oft und vielmal w\u00fcnschten wir,", "tokens": ["Wie", "oft", "und", "viel\u00b7mal", "w\u00fcnschten", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als uns der Winter alle Zier", "tokens": ["Als", "uns", "der", "Win\u00b7ter", "al\u00b7le", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gr\u00fcnenden Natur beraubet,", "tokens": ["Der", "gr\u00fc\u00b7nen\u00b7den", "Na\u00b7tur", "be\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kraut erstickt, die B\u00e4um entlaubet;", "tokens": ["Das", "Kraut", "er\u00b7stickt", ",", "die", "B\u00e4um", "ent\u00b7lau\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da Feld und Wald mit Schnee bedeckt;", "tokens": ["Da", "Feld", "und", "Wald", "mit", "Schnee", "be\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da uns der rauhe Nordwind schreckt:", "tokens": ["Da", "uns", "der", "rau\u00b7he", "Nord\u00b7wind", "schreckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach wenn der Fr\u00fchling wiederk\u00e4me,", "tokens": ["Ach", "wenn", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der\u00b7k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und die Beschwerde von uns n\u00e4hme!", "tokens": ["Und", "die", "Be\u00b7schwer\u00b7de", "von", "uns", "n\u00e4h\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ach wenn die L\u00fcfte klar und rein,", "tokens": ["Ach", "wenn", "die", "L\u00fcf\u00b7te", "klar", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erw\u00e4rmt vom hellen Sonnenschein,", "tokens": ["Er\u00b7w\u00e4rmt", "vom", "hel\u00b7len", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und lau, und sanft, und lieblich w\u00e4ren!", "tokens": ["Und", "lau", ",", "und", "sanft", ",", "und", "lieb\u00b7lich", "w\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ach k\u00f6nnte man, in hellen Ch\u00f6ren,", "tokens": ["Ach", "k\u00f6nn\u00b7te", "man", ",", "in", "hel\u00b7len", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die bunten V\u00f6gel wieder h\u00f6ren!", "tokens": ["Die", "bun\u00b7ten", "V\u00f6\u00b7gel", "wie\u00b7der", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ach s\u00e4hen wir die gelben Aehren", "tokens": ["Ach", "s\u00e4\u00b7hen", "wir", "die", "gel\u00b7ben", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "So angenehm als ungemein,", "tokens": ["So", "an\u00b7ge\u00b7nehm", "als", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bey sanfter K\u00fchlung, lieblich wallen!", "tokens": ["Bey", "sanf\u00b7ter", "K\u00fch\u00b7lung", ",", "lieb\u00b7lich", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ach m\u00f6cht, in fliessenden Krystallen,", "tokens": ["Ach", "m\u00f6cht", ",", "in", "flies\u00b7sen\u00b7den", "Krys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Des gr\u00fcnen Ufers Wiederschein", "tokens": ["Des", "gr\u00fc\u00b7nen", "U\u00b7fers", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Zu sehen seyn!", "tokens": ["Zu", "se\u00b7hen", "seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAINF", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.20": {"text": "Wie wollten wir uns denn nicht freuen!", "tokens": ["Wie", "woll\u00b7ten", "wir", "uns", "denn", "nicht", "freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wir wollten Lob und Dank erneuen.", "tokens": ["Wir", "woll\u00b7ten", "Lob", "und", "Dank", "er\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Jetzt seht und h\u00f6rt ihr alles das,", "tokens": ["Jetzt", "seht", "und", "h\u00f6rt", "ihr", "al\u00b7les", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "PIS", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Was ihr gew\u00fcnscht. Es ist erschienen", "tokens": ["Was", "ihr", "ge\u00b7w\u00fcnscht", ".", "Es", "ist", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$.", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die Zeit, die so erw\u00fcnschte Zeit.", "tokens": ["Die", "Zeit", ",", "die", "so", "er\u00b7w\u00fcnschte", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Die Welt ist voller Lieblichkeit;", "tokens": ["Die", "Welt", "ist", "vol\u00b7ler", "Lieb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Es stehet jetzt, durch Laub und Gras,", "tokens": ["Es", "ste\u00b7het", "jetzt", ",", "durch", "Laub", "und", "Gras", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "So Feld, als Wald, im holden Gr\u00fcnen.", "tokens": ["So", "Feld", ",", "als", "Wald", ",", "im", "hol\u00b7den", "Gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Es sind die L\u00fcfte klar und rein,", "tokens": ["Es", "sind", "die", "L\u00fcf\u00b7te", "klar", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Erw\u00e4rmt, vom heitern Sonnenschein.", "tokens": ["Er\u00b7w\u00e4rmt", ",", "vom", "hei\u00b7tern", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Jhr k\u00f6nnet jetzt, in hellen Ch\u00f6ren,", "tokens": ["Ihr", "k\u00f6n\u00b7net", "jetzt", ",", "in", "hel\u00b7len", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Die bunten V\u00f6gel singen h\u00f6ren.", "tokens": ["Die", "bun\u00b7ten", "V\u00f6\u00b7gel", "sin\u00b7gen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Jhr sehet jetzt die gelben Aehren,", "tokens": ["Ihr", "se\u00b7het", "jetzt", "die", "gel\u00b7ben", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "So angenehm als ungemein,", "tokens": ["So", "an\u00b7ge\u00b7nehm", "als", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Bey sanfter K\u00fchlung, lieblich wallen.", "tokens": ["Bey", "sanf\u00b7ter", "K\u00fch\u00b7lung", ",", "lieb\u00b7lich", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Jhr seht, in fliessenden Krystallen,", "tokens": ["Ihr", "seht", ",", "in", "flies\u00b7sen\u00b7den", "Krys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Des gr\u00fcnen Ufers Wiederschein;", "tokens": ["Des", "gr\u00fc\u00b7nen", "U\u00b7fers", "Wie\u00b7der\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Wie? wollt ihr euch denn nicht erfreuen,", "tokens": ["Wie", "?", "wollt", "ihr", "euch", "denn", "nicht", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und Preis und Lob und Dank erneuen?", "tokens": ["Und", "Preis", "und", "Lob", "und", "Dank", "er\u00b7neu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Warum wollt ihr es nicht geniessen?", "tokens": ["Wa\u00b7rum", "wollt", "ihr", "es", "nicht", "ge\u00b7nies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und lasset diesen Tag verfliessen,", "tokens": ["Und", "las\u00b7set", "die\u00b7sen", "Tag", "ver\u00b7flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Als wie den gestrigen? War\u00fcm", "tokens": ["Als", "wie", "den", "ge\u00b7stri\u00b7gen", "?", "Wa\u00b7r\u00fcm"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "KOKOM", "ART", "VVPP", "$.", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.42": {"text": "Vernehmt ihr nicht die S\u00fc\u00dfigkeiten,", "tokens": ["Ver\u00b7nehmt", "ihr", "nicht", "die", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Der sch\u00f6nen Creaturen Stimm?", "tokens": ["Der", "sch\u00f6\u00b7nen", "Crea\u00b7tu\u00b7ren", "Stimm", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Die euch in diesen sch\u00f6nen Zeiten,", "tokens": ["Die", "euch", "in", "die\u00b7sen", "sch\u00f6\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Durch jeden Sinn, in vielen Ch\u00f6ren", "tokens": ["Durch", "je\u00b7den", "Sinn", ",", "in", "vie\u00b7len", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Euch reizt, den Sch\u00f6pfer zu verehren?", "tokens": ["Euch", "reizt", ",", "den", "Sch\u00f6p\u00b7fer", "zu", "ver\u00b7eh\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Jhr seht unachtsam vor euch nieder;", "tokens": ["Ihr", "seht", "u\u00b7nacht\u00b7sam", "vor", "euch", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Man sieht an Minen und Gesicht,", "tokens": ["Man", "sieht", "an", "Mi\u00b7nen", "und", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Da\u00df etwas euch ergetzet, nicht.", "tokens": ["Da\u00df", "et\u00b7was", "euch", "er\u00b7get\u00b7zet", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Man h\u00f6rt nicht Dank-nicht Lobes-Lieder.", "tokens": ["Man", "h\u00f6rt", "nicht", "Dank\u00b7nicht", "Lo\u00b7bes\u00b7Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Wie schnell verfliegt die schnelle Zeit!", "tokens": ["Wie", "schnell", "ver\u00b7fliegt", "die", "schnel\u00b7le", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Wollt ihr mit eurer Lust denn warten,", "tokens": ["Wollt", "ihr", "mit", "eu\u00b7rer", "Lust", "denn", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Bis Wiesen, Feld, und Wald, und Garten", "tokens": ["Bis", "Wie\u00b7sen", ",", "Feld", ",", "und", "Wald", ",", "und", "Gar\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "KON", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Entlaubt, verw