{"dta.poem.10479": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch, verspotte nicht den Teufel,", "tokens": ["Mensch", ",", "ver\u00b7spot\u00b7te", "nicht", "den", "Teu\u00b7fel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kurz ist ja die Lebensbahn,", "tokens": ["Kurz", "ist", "ja", "die", "Le\u00b7bens\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die ewige Verdammni\u00df", "tokens": ["Und", "die", "e\u00b7wi\u00b7ge", "Ver\u00b7damm\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist kein blo\u00dfer P\u00f6belwahn.", "tokens": ["Ist", "kein", "blo\u00b7\u00dfer", "P\u00f6\u00b7bel\u00b7wahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mensch, bezahle deine Schulden,", "tokens": ["Mensch", ",", "be\u00b7zah\u00b7le", "dei\u00b7ne", "Schul\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lang ist ja die Lebensbahn,", "tokens": ["Lang", "ist", "ja", "die", "Le\u00b7bens\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du mu\u00dft noch manchmal borgen,", "tokens": ["Und", "du", "mu\u00dft", "noch", "manch\u00b7mal", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie du es so oft gethan.", "tokens": ["Wie", "du", "es", "so", "oft", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}