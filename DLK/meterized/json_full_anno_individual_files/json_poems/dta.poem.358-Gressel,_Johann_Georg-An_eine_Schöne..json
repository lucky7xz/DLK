{"dta.poem.358": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Durch deiner Augen-Gluth bin ich in Brand gesetzet/", "tokens": ["Durch", "dei\u00b7ner", "Au\u00b7gen\u00b7Gluth", "bin", "ich", "in", "Brand", "ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wunder-sch\u00f6ne Brust hat Geist und Hertz verletzet.", "tokens": ["Die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Brust", "hat", "Geist", "und", "Hertz", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Mienen schliessen mich in Band\u2019 und Ketten ein", "tokens": ["Die", "Mie\u00b7nen", "schlies\u00b7sen", "mich", "in", "Band'", "und", "Ket\u00b7ten", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Leibeigen mu\u00df mein Sinn dem sch\u00f6nen Wesen seyn.", "tokens": ["Leib\u00b7ei\u00b7gen", "mu\u00df", "mein", "Sinn", "dem", "sch\u00f6\u00b7nen", "We\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil ich nun deine bin/ und dir gantz zugeh\u00f6re/", "tokens": ["Weil", "ich", "nun", "dei\u00b7ne", "bin", "/", "und", "dir", "gantz", "zu\u00b7ge\u00b7h\u00f6\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "VAFIN", "$(", "KON", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So nimm das Opffer an/ damit ich dich verehre/", "tokens": ["So", "nimm", "das", "Opf\u00b7fer", "an", "/", "da\u00b7mit", "ich", "dich", "ver\u00b7eh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PTKVZ", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist mein Hertz so dir/ mein Schatz gantz zugethan/", "tokens": ["Es", "ist", "mein", "Hertz", "so", "dir", "/", "mein", "Schatz", "gantz", "zu\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "PPER", "$(", "PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gib deines wieder her/ den Wechsel geh\u2019 ich an.", "tokens": ["Gib", "dei\u00b7nes", "wie\u00b7der", "her", "/", "den", "Wech\u00b7sel", "geh'", "ich", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADV", "PTKVZ", "$(", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}