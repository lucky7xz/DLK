{"dta.poem.14271": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der junge Ehemann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Hier unter dieser Linde", "tokens": ["Hier", "un\u00b7ter", "die\u00b7ser", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df ich viel tausendmal", "tokens": ["Sa\u00df", "ich", "viel", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schaut' nach meinem Kinde", "tokens": ["Und", "schaut'", "nach", "mei\u00b7nem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinunter in das Thal,", "tokens": ["Hin\u00b7un\u00b7ter", "in", "das", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis da\u00df die Sterne standen", "tokens": ["Bis", "da\u00df", "die", "Ster\u00b7ne", "stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hell \u00fcber ihrem Haus", "tokens": ["Hell", "\u00fc\u00b7ber", "ih\u00b7rem", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und weit in den stillen Landen", "tokens": ["Und", "weit", "in", "den", "stil\u00b7len", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Alle Lichter l\u00f6schten aus.", "tokens": ["Al\u00b7le", "Lich\u00b7ter", "l\u00f6schten", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}