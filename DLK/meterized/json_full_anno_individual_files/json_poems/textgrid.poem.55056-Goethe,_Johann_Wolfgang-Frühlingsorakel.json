{"textgrid.poem.55056": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsorakel", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du prophet'scher Vogel du,", "tokens": ["Du", "pro\u00b7phet'\u00b7scher", "Vo\u00b7gel", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fctens\u00e4nger, o Coucou!", "tokens": ["Bl\u00fc\u00b7ten\u00b7s\u00e4n\u00b7ger", ",", "o", "Cou\u00b7cou", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bitten eines jungen Paares", "tokens": ["Bit\u00b7ten", "ei\u00b7nes", "jun\u00b7gen", "Paa\u00b7res"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der sch\u00f6nsten Zeit des Jahres", "tokens": ["In", "der", "sch\u00f6ns\u00b7ten", "Zeit", "des", "Jah\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6re, liebster Vogel du;", "tokens": ["H\u00f6\u00b7re", ",", "liebs\u00b7ter", "Vo\u00b7gel", "du", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann es hoffen, ruf ihm zu", "tokens": ["Kann", "es", "hof\u00b7fen", ",", "ruf", "ihm", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dein Coucou, dein Coucou,", "tokens": ["Dein", "Cou\u00b7cou", ",", "dein", "Cou\u00b7cou", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Immer mehr Coucou, Coucou.", "tokens": ["Im\u00b7mer", "mehr", "Cou\u00b7cou", ",", "Cou\u00b7cou", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6rst du! ein verliebtes Paar", "tokens": ["H\u00f6rst", "du", "!", "ein", "ver\u00b7lieb\u00b7tes", "Paar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehnt sich herzlich zum Altar;", "tokens": ["Sehnt", "sich", "herz\u00b7lich", "zum", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es ist bei seiner Jugend", "tokens": ["Und", "es", "ist", "bei", "sei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Voller Treue, voller Tugend.", "tokens": ["Vol\u00b7ler", "Treu\u00b7e", ",", "vol\u00b7ler", "Tu\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist die Stunde denn noch nicht voll?", "tokens": ["Ist", "die", "Stun\u00b7de", "denn", "noch", "nicht", "voll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sag, wie lange es warten soll?", "tokens": ["Sag", ",", "wie", "lan\u00b7ge", "es", "war\u00b7ten", "soll", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Horch! Coucou! Horch! Coucou!", "tokens": ["Horch", "!", "Cou\u00b7cou", "!", "Horch", "!", "Cou\u00b7cou", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$.", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Immer stille! Nichts hinzu!", "tokens": ["Im\u00b7mer", "stil\u00b7le", "!", "Nichts", "hin\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist es doch nicht unsre Schuld!", "tokens": ["Ist", "es", "doch", "nicht", "uns\u00b7re", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur zwei Jahre noch Geduld!", "tokens": ["Nur", "zwei", "Jah\u00b7re", "noch", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, wenn wir uns genommen,", "tokens": ["A\u00b7ber", ",", "wenn", "wir", "uns", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden Pa-pa-papas kommen?", "tokens": ["Wer\u00b7den", "Pa\u00b7pa\u00b7pa\u00b7pas", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wisse, da\u00df du uns erfreust,", "tokens": ["Wis\u00b7se", ",", "da\u00df", "du", "uns", "er\u00b7freust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn du viele prophezeist.", "tokens": ["Wenn", "du", "vie\u00b7le", "pro\u00b7phe\u00b7zeist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eins! Coucou! Zwei! Coucou!", "tokens": ["Eins", "!", "Cou\u00b7cou", "!", "Zwei", "!", "Cou\u00b7cou", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NE", "$.", "CARD", "$.", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Immer weiter Coucou, Coucou, Cou.", "tokens": ["Im\u00b7mer", "wei\u00b7ter", "Cou\u00b7cou", ",", "Cou\u00b7cou", ",", "Cou", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Haben wir wohl recht gez\u00e4hlt,", "tokens": ["Ha\u00b7ben", "wir", "wohl", "recht", "ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenig am Halbdutzend fehlt.", "tokens": ["We\u00b7nig", "am", "Halb\u00b7dut\u00b7zend", "fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir gute Worte geben,", "tokens": ["Wenn", "wir", "gu\u00b7te", "Wor\u00b7te", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagst du wohl, wie lang wir leben?", "tokens": ["Sagst", "du", "wohl", ",", "wie", "lang", "wir", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freilich, wir gestehen dir's,", "tokens": ["Frei\u00b7lich", ",", "wir", "ge\u00b7ste\u00b7hen", "dir's", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gern zum l\u00e4ngsten trieben wir's.", "tokens": ["Gern", "zum", "l\u00e4ngs\u00b7ten", "trie\u00b7ben", "wir'", "s."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPRART", "ADJA", "VVFIN", "VAFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Cou Coucou, Cou Coucou,", "tokens": ["Cou", "Cou\u00b7cou", ",", "Cou", "Cou\u00b7cou", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "$,", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Cou, Cou, Cou, Cou, Cou, Cou, Cou, Cou, Cou.", "tokens": ["Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word