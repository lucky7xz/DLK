unct"], "pos": ["VAFIN", "ADV", "PIS", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Cou Coucou, Cou Coucou :/:", "tokens": ["Cou", "Cou\u00b7cou", ",", "Cou", "Cou\u00b7cou", ":/", ":"], "token_info": ["word", "word", "punct", "word", "word", "emoticon", "punct"], "pos": ["FM.fr", "FM.fr", "$,", "FM.fr", "FM.fr", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Cou, Cou, Cou, Cou, Cou, Cou, Cou, Cou, Cou!", "tokens": ["Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", ",", "Cou", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NE", "$,", "NE", "$,", "NE", "$,", "NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "(mit Grazie in infinitum)", "tokens": ["(", "mit", "Gra\u00b7zie", "in", "in\u00b7fi\u00b7ni\u00b7tum", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du prophet'scher Vogel du,", "tokens": ["Du", "pro\u00b7phet'\u00b7scher", "Vo\u00b7gel", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fctens\u00e4nger, o Coucou!", "tokens": ["Bl\u00fc\u00b7ten\u00b7s\u00e4n\u00b7ger", ",", "o", "Cou\u00b7cou", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bitten eines jungen Paares", "tokens": ["Bit\u00b7ten", "ei\u00b7nes", "jun\u00b7gen", "Paa\u00b7res"], "token_info": ["word", "word", "word"