{"textgrid.poem.41332": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Mann", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bestimmter w\u00e4hlt ein Mann, nach Zweifeln und Verdacht:", "tokens": ["Be\u00b7stimm\u00b7ter", "w\u00e4hlt", "ein", "Mann", ",", "nach", "Zwei\u00b7feln", "und", "Ver\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm l\u00e4chelt nur die Welt, die ihm zuvor gelacht,", "tokens": ["Ihm", "l\u00e4\u00b7chelt", "nur", "die", "Welt", ",", "die", "ihm", "zu\u00b7vor", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Tanzplatz j\u00fcngrer Lust. Nun richtet er die Kr\u00e4fte", "tokens": ["Der", "Tanz\u00b7platz", "j\u00fcng\u00b7rer", "Lust", ".", "Nun", "rich\u00b7tet", "er", "die", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erhabner auf den Zweck versorgender Gesch\u00e4fte.", "tokens": ["Er\u00b7hab\u00b7ner", "auf", "den", "Zweck", "ver\u00b7sor\u00b7gen\u00b7der", "Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun unterwirft er sich: ihn z\u00e4hmt ein fremder Zwang:", "tokens": ["Nun", "un\u00b7ter\u00b7wirft", "er", "sich", ":", "ihn", "z\u00e4hmt", "ein", "frem\u00b7der", "Zwang", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nun wirbt sein kluger Flei\u00df um Ansehn, Amt und Rang.", "tokens": ["Nun", "wirbt", "sein", "klu\u00b7ger", "Flei\u00df", "um", "An\u00b7sehn", ",", "Amt", "und", "Rang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Damit er weiter nicht mit theuren K\u00fcssen buhle,", "tokens": ["Da\u00b7mit", "er", "wei\u00b7ter", "nicht", "mit", "theu\u00b7ren", "K\u00fcs\u00b7sen", "buh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schickt ihn der Eigennutz dem Ehstand in die Schule:", "tokens": ["Schickt", "ihn", "der", "Ei\u00b7gen\u00b7nutz", "dem", "Eh\u00b7stand", "in", "die", "Schu\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Ordnung Heiligthum, und, durch des Himmels Gunst,", "tokens": ["Der", "Ord\u00b7nung", "Hei\u00b7lig\u00b7thum", ",", "und", ",", "durch", "des", "Him\u00b7mels", "Gunst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem Sitz geweihter Treu' und sch\u00e4rfrer Rechenkunst.", "tokens": ["Dem", "Sitz", "ge\u00b7weih\u00b7ter", "Treu'", "und", "sch\u00e4r\u00b7frer", "Re\u00b7chen\u00b7kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So mehrt er Stamm und Gut, ist achtsam und verschwiegen,", "tokens": ["So", "mehrt", "er", "Stamm", "und", "Gut", ",", "ist", "acht\u00b7sam", "und", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "ADJD", "$,", "VAFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Scharfsinnig im Beruf, gesetzlich im Vergn\u00fcgen,", "tokens": ["Scharf\u00b7sin\u00b7nig", "im", "Be\u00b7ruf", ",", "ge\u00b7setz\u00b7lich", "im", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und w\u00fcnscht, wenn ihn kein Weib des Lebens Lust verg\u00e4llt,", "tokens": ["Und", "w\u00fcnscht", ",", "wenn", "ihn", "kein", "Weib", "des", "Le\u00b7bens", "Lust", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPE