{"dta.poem.4004": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Eduard .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Euch alten Freunde droben, unsterbliches", "tokens": ["Euch", "al\u00b7ten", "Freun\u00b7de", "dro\u00b7ben", ",", "uns\u00b7terb\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "ADV", "$,", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gestirn! euch frag' ich, Helden! woher es ist,", "tokens": ["Ge\u00b7stirn", "!", "euch", "frag'", "ich", ",", "Hel\u00b7den", "!", "wo\u00b7her", "es", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "$,", "NN", "$.", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich so unterthan ihm bin, und", "tokens": ["Da\u00df", "ich", "so", "un\u00b7ter\u00b7than", "ihm", "bin", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VAFIN", "$,", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So der Gewaltige sein mich nennet?", "tokens": ["So", "der", "Ge\u00b7wal\u00b7ti\u00b7ge", "sein", "mich", "nen\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "PPER", "VVFIN", "$."], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Denn wenig kann ich bieten, nur weniges", "tokens": ["Denn", "we\u00b7nig", "kann", "ich", "bie\u00b7ten", ",", "nur", "we\u00b7ni\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$,", "ADV", "PIS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kann ich verlieren, aber ein liebes Gl\u00fcck,", "tokens": ["Kann", "ich", "ver\u00b7lie\u00b7ren", ",", "a\u00b7ber", "ein", "lie\u00b7bes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein einziges, zum Angedenken", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", ",", "zum", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPRART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Reicherer Tage zur\u00fcck geblieben;", "tokens": ["Rei\u00b7che\u00b7rer", "Ta\u00b7ge", "zu\u00b7r\u00fcck", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Und so er mir's geb\u00f6te, dies Eine noch,", "tokens": ["Und", "so", "er", "mir's", "ge\u00b7b\u00f6\u00b7te", ",", "dies", "Ei\u00b7ne", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "NE", "VVFIN", "$,", "PDS", "ART", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Saitenspiel, ich wagt' es, wohin er wollt',", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", ",", "ich", "wagt'", "es", ",", "wo\u00b7hin", "er", "wollt'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und mit Gesange folgt' ich, selbst in's", "tokens": ["Und", "mit", "Ge\u00b7san\u00b7ge", "folgt'", "ich", ",", "selbst", "in's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "$,", "ADV", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ende der Tapferen ihm hinunter.", "tokens": ["En\u00b7de", "der", "Tap\u00b7fe\u00b7ren", "ihm", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "\u201edie Wolke\u201c \u2014 s\u00e4ng' ich \u2014 \u201etr\u00e4nket mit Regen", "tokens": ["\u201e", "die", "Wol\u00b7ke", "\u201c", "s\u00e4ng'", "ich", "\u201e", "tr\u00e4n\u00b7ket", "mit", "Re\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$(", "$(", "VVIMP", "PPER", "$(", "$(", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich,", "tokens": ["Dich", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "\u201edu Mutterboden! aber mit Blut der Mensch;", "tokens": ["\u201e", "du", "Mut\u00b7ter\u00b7bo\u00b7den", "!", "a\u00b7ber", "mit", "Blut", "der", "Mensch", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$.", "ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201dso ruht, so k\u00fchlt die Liebe sich, die", "tokens": ["\"", "so", "ruht", ",", "so", "k\u00fchlt", "die", "Lie\u00b7be", "sich", ",", "die"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PRF", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ddroben und drunten nicht Gleiches findet.", "tokens": ["\"", "dro\u00b7ben", "und", "drun\u00b7ten", "nicht", "Glei\u00b7ches", "fin\u00b7det", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KON", "ADV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "\u201ewo ist am Tag ihr Zeichen? wo spricht das Herz", "tokens": ["\u201e", "wo", "ist", "am", "Tag", "ihr", "Zei\u00b7chen", "?", "wo", "spricht", "das", "Herz"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201esich aus? o wann im Leben, wann ist es frei,", "tokens": ["\u201e", "sich", "aus", "?", "o", "wann", "im", "Le\u00b7ben", ",", "wann", "ist", "es", "frei", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRF", "PTKVZ", "$.", "FM", "PWAV", "APPRART", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201dwas unser Wort nicht nennt, wann wird, was", "tokens": ["\"", "was", "un\u00b7ser", "Wort", "nicht", "nennt", ",", "wann", "wird", ",", "was"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PWS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,", "PWAV", "VAFIN", "$,", "PWS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201dtrauert, gebannt in die Nacht, sein Wunsch", "tokens": ["\"", "trau\u00b7ert", ",", "ge\u00b7bannt", "in", "die", "Nacht", ",", "sein", "Wunsch"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "ihm? &#8212;", "tokens": ["ihm", "?", "&#8212;"], "token_info": ["word", "punct", "XML_entity"], "pos": ["PPER", "$.", "$("], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "\u201ejetzt, wann die Opfer fallen, ihr Freunde! jetzt!", "tokens": ["\u201e", "jetzt", ",", "wann", "die", "Op\u00b7fer", "fal\u00b7len", ",", "ihr", "Freun\u00b7de", "!", "jetzt", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$,", "PPOSAT", "NN", "$.", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201eschon tritt hinzu der festliche Zug, schon blinkt", "tokens": ["\u201e", "schon", "tritt", "hin\u00b7zu", "der", "fest\u00b7li\u00b7che", "Zug", ",", "schon", "blinkt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201dder Stahl, die Wolke dampft, sie fallen, und es", "tokens": ["\"", "der", "Stahl", ",", "die", "Wol\u00b7ke", "dampft", ",", "sie", "fal\u00b7len", ",", "und", "es"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "PPER", "VVINF", "$,", "KON", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201dhallt in der Luft, und die Erde r\u00fchmt es!&#8220;", "tokens": ["\"", "hallt", "in", "der", "Luft", ",", "und", "die", "Er\u00b7de", "r\u00fchmt", "es", "!", "&#8220;"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Wenn ich so singend fiele, dann r\u00e4chtest Du", "tokens": ["Wenn", "ich", "so", "sin\u00b7gend", "fie\u00b7le", ",", "dann", "r\u00e4ch\u00b7test", "Du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mich, mein Achill! und spr\u00e4chest: \u201eer lebte doch", "tokens": ["Mich", ",", "mein", "A\u00b7chill", "!", "und", "spr\u00e4\u00b7chest", ":", "\u201e", "er", "leb\u00b7te", "doch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$.", "KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ADV"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "\u201dtreu bis zuletzt!&#8220; das ernste Wort, das", "tokens": ["\"", "treu", "bis", "zu\u00b7letzt", "!", "&#8220;", "das", "erns\u00b7te", "Wort", ",", "das"], "token_info": ["punct", "word", "word", "word", "punct", "XML_entity", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADJD", "APPR", "ADV", "$.", "$(", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Spr\u00e4che mein Feind und der Todtenrichter!", "tokens": ["Spr\u00e4\u00b7che", "mein", "Feind", "und", "der", "Tod\u00b7ten\u00b7rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Doch weilen wir in Ruhe, Du Lieber, noch;", "tokens": ["Doch", "wei\u00b7len", "wir", "in", "Ru\u00b7he", ",", "Du", "Lie\u00b7ber", ",", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,", "PPER", "ADJD", "$,", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Uns birgt der Wald, es h\u00e4lt das Gebirge dort", "tokens": ["Uns", "birgt", "der", "Wald", ",", "es", "h\u00e4lt", "das", "Ge\u00b7bir\u00b7ge", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das m\u00fctterliche, noch die beiden", "tokens": ["Das", "m\u00fct\u00b7ter\u00b7li\u00b7che", ",", "noch", "die", "bei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "ART", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Br\u00fcder in sicherem Arm gefangen.", "tokens": ["Br\u00fc\u00b7der", "in", "si\u00b7che\u00b7rem", "Arm", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Uns ist die Weisheit Wiegengesang; sie webt", "tokens": ["Uns", "ist", "die", "Weis\u00b7heit", "Wie\u00b7gen\u00b7ge\u00b7sang", ";", "sie", "webt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um's Aug' ihr heilig Dunkel; doch \u00f6fters k\u00f6mmt", "tokens": ["Um's", "Aug'", "ihr", "hei\u00b7lig", "Dun\u00b7kel", ";", "doch", "\u00f6f\u00b7ters", "k\u00f6mmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "PPER", "ADJD", "NN", "$.", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus ferne t\u00f6nendem Gew\u00f6lk die", "tokens": ["Aus", "fer\u00b7ne", "t\u00f6\u00b7nen\u00b7dem", "Ge\u00b7w\u00f6lk", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mahnende Flamme des Zeitengottes.", "tokens": ["Mah\u00b7nen\u00b7de", "Flam\u00b7me", "des", "Zei\u00b7ten\u00b7got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Es regt sein Sturm die Schwingen Dir auf; Dich", "tokens": ["Es", "regt", "sein", "Sturm", "die", "Schwin\u00b7gen", "Dir", "auf", ";", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PPER", "PTKVZ", "$.", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ruft,", "tokens": ["ruft", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dich nimmt der m\u00e4cht'ge Vater hinauf; o nimm", "tokens": ["Dich", "nimmt", "der", "m\u00e4cht'\u00b7ge", "Va\u00b7ter", "hin\u00b7auf", ";", "o", "nimm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "FM", "FM"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mich Du, und trage Deine leichte", "tokens": ["Mich", "Du", ",", "und", "tra\u00b7ge", "Dei\u00b7ne", "leich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "$,", "KON", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Beute dem l\u00e4chelnden Gott entgegen!", "tokens": ["Beu\u00b7te", "dem", "l\u00e4\u00b7cheln\u00b7den", "Gott", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}