{"dta.poem.7182": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zitt\u2019re denn, o mein Gem\u00fcthe!", "tokens": ["Zitt'\u00b7re", "denn", ",", "o", "mein", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber nein, erzitt\u2019re nicht!", "tokens": ["A\u00b7ber", "nein", ",", "er\u00b7zitt'\u00b7re", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Gr\u00f6ss\u2019 ist nichts als G\u00fcte,", "tokens": ["Die\u00b7se", "Gr\u00f6ss'", "ist", "nichts", "als", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PIS", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Glantz ein Gnaden-Licht.", "tokens": ["Die\u00b7ser", "Glantz", "ein", "Gna\u00b7den\u00b7Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Z\u00e4rtlichkeit und Neigungs-Triebe,", "tokens": ["Z\u00e4rt\u00b7lich\u00b7keit", "und", "Nei\u00b7gungs\u00b7Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sanfftmuth, Huld, Geduld und Liebe", "tokens": ["Sanfft\u00b7muth", ",", "Huld", ",", "Ge\u00b7duld", "und", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sind des Sch\u00f6pffers Eigenschafft,", "tokens": ["Sind", "des", "Sch\u00f6pf\u00b7fers", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die W\u00fcrckung Seiner Krafft.", "tokens": ["Und", "die", "W\u00fcr\u00b7ckung", "Sei\u00b7ner", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}