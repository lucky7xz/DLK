{"textgrid.poem.25961": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbla\u00dft sch\u00f6nstes Wetter zu mir kommen\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbla\u00dft sch\u00f6nstes Wetter zu mir kommen!\u00ab", "tokens": ["\u00bb", "la\u00dft", "sch\u00f6ns\u00b7tes", "Wet\u00b7ter", "zu", "mir", "kom\u00b7men", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ADJA", "NN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach heute fr\u00fch Frau Dauthendey,", "tokens": ["Sprach", "heu\u00b7te", "fr\u00fch", "Frau", "Dau\u00b7then\u00b7dey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Sonnenschirm hat sie genommen,", "tokens": ["Den", "Son\u00b7nen\u00b7schirm", "hat", "sie", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ward der blau'ste Tag im Mai.", "tokens": ["Da", "ward", "der", "blau'\u00b7ste", "Tag", "im", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bball die Verliebten sind geladen!\u00ab", "tokens": ["\u00bb", "all", "die", "Ver\u00b7lieb\u00b7ten", "sind", "ge\u00b7la\u00b7den", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT",