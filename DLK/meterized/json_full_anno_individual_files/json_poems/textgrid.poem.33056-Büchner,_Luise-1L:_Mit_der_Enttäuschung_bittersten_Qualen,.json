{"textgrid.poem.33056": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit der Entt\u00e4uschung bittersten Qualen,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der Entt\u00e4uschung bittersten Qualen,", "tokens": ["Mit", "der", "Ent\u00b7t\u00e4u\u00b7schung", "bit\u00b7ters\u00b7ten", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wiege dich Hoffnung auch noch so sch\u00f6n,", "tokens": ["Wie\u00b7ge", "dich", "Hoff\u00b7nung", "auch", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "T\u00fcckisch wird sie doch untergeh'n!", "tokens": ["T\u00fc\u00b7ckisch", "wird", "sie", "doch", "un\u00b7ter\u00b7geh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was deine brennende Sehnsucht begehret:", "tokens": ["Was", "dei\u00b7ne", "bren\u00b7nen\u00b7de", "Sehn\u00b7sucht", "be\u00b7ge\u00b7hret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ob auch aus weinender Seele er quillt,", "tokens": ["Ob", "auch", "aus", "wei\u00b7nen\u00b7der", "See\u00b7le", "er", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nie sich der rettende Wunsch erf\u00fcllt. \u2013", "tokens": ["Nie", "sich", "der", "ret\u00b7ten\u00b7de", "Wunsch", "er\u00b7f\u00fcllt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Nimmer dein hei\u00dfes Bangen und Sehnen,", "tokens": ["Nim\u00b7mer", "dein", "hei\u00b7\u00dfes", "Ban\u00b7gen", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Wehr' ihres Strom's unb\u00e4nd'ger Gewalt,", "tokens": ["Wehr'", "ih\u00b7res", "Strom's", "un\u00b7b\u00e4n\u00b7d'\u00b7ger", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Geh' deines Weges ruhig und kalt.", "tokens": ["Geh'", "dei\u00b7nes", "We\u00b7ges", "ru\u00b7hig", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "D'rauf du voll Groll dein Leid kannst w\u00e4lzen,", "tokens": ["D'\u00b7rauf", "du", "voll", "Groll", "dein", "Leid", "kannst", "w\u00e4l\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Vor seiner Schwere dein Stolz sinkt hin,", "tokens": ["Vor", "sei\u00b7ner", "Schwe\u00b7re", "dein", "Stolz", "sinkt", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Brich' du ihn selbst mit duldendem Sinn!", "tokens": ["Brich'", "du", "ihn", "selbst", "mit", "dul\u00b7den\u00b7dem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Bis uns umschwebet der ewige Frieden;", "tokens": ["Bis", "uns", "um\u00b7schwe\u00b7bet", "der", "e\u00b7wi\u00b7ge", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Ob auch das Leid dich erdr\u00fccken will \u2013", "tokens": ["Ob", "auch", "das", "Leid", "dich", "er\u00b7dr\u00fc\u00b7cken", "will", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dulde du, schweig' und halte dich still!", "tokens": ["Dul\u00b7de", "du", ",", "schweig'", "und", "hal\u00b7te", "dich", "still", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Mit der Entt\u00e4uschung bittersten Qualen,", "tokens": ["Mit", "der", "Ent\u00b7t\u00e4u\u00b7schung", "bit\u00b7ters\u00b