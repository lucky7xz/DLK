{"dta.poem.443": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Fast desgleichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe nimmt so zu/ und f\u00e4llt auch wie das Meer/", "tokens": ["Die", "Lie\u00b7be", "nimmt", "so", "zu", "/", "und", "f\u00e4llt", "auch", "wie", "das", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKZU", "$(", "KON", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Wunder/ denn sie schreibt da ihren Ursprung her.", "tokens": ["Kein", "Wun\u00b7der", "/", "denn", "sie", "schreibt", "da", "ih\u00b7ren", "Ur\u00b7sprung", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr Sinn ist wandelbahr/ man stellt sie zum Jrr-Sternen/", "tokens": ["Ihr", "Sinn", "ist", "wan\u00b7del\u00b7bahr", "/", "man", "stellt", "sie", "zum", "Jrr\u00b7S\u00b7ter\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "PIS", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht unters ", "tokens": ["Nicht", "un\u00b7ters"], "token_info": ["word", "word"], "pos": ["PTKNEG", "APPRART"], "meter": "-+-", "measure": "amph