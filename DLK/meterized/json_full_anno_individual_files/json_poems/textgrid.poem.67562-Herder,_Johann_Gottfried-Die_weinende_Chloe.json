{"textgrid.poem.67562": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die weinende Chloe", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du weinest, liebste Chloe? Sieh, o sieh", "tokens": ["Du", "wei\u00b7nest", ",", "liebs\u00b7te", "Chloe", "?", "Sieh", ",", "o", "sieh"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$.", "NE", "$,", "FM", "FM"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rings um Dich die Natur in Sympathie!", "tokens": ["Rings", "um", "Dich", "die", "Na\u00b7tur", "in", "Sym\u00b7pa\u00b7thie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die kleinen s\u00fc\u00dfen V\u00f6gel \u2013 sieh, sie singen", "tokens": ["Die", "klei\u00b7nen", "s\u00fc\u00b7\u00dfen", "V\u00f6\u00b7gel", "\u2013", "sieh", ",", "sie", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "VVIMP", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht l\u00e4nger, neigen Haupt und Schwingen", "tokens": ["Nicht", "l\u00e4n\u00b7ger", ",", "nei\u00b7gen", "Haupt", "und", "Schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So traurig nieder! \u2013 Jene Wolken wollen weinen,", "tokens": ["So", "trau\u00b7rig", "nie\u00b7der", "!", "\u2013", "Je\u00b7ne", "Wol\u00b7ken", "wol\u00b7len", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$.", "$(", "PDAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Abneigend, schauernd, mischend zu den Deinen", "tokens": ["Ab\u00b7nei\u00b7gend", ",", "schau\u00b7ernd", ",", "mi\u00b7schend", "zu", "den", "Dei\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVPP", "APPR", "ART", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auch ihre Thr\u00e4nen! Jene B\u00e4che flie\u00dfen", "tokens": ["Auch", "ih\u00b7re", "Thr\u00e4\u00b7nen", "!", "Je\u00b7ne", "B\u00e4\u00b7che", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$.", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So trauriger! So sanfter murmelnd gie\u00dfen", "tokens": ["So", "trau\u00b7ri\u00b7ger", "!", "So", "sanf\u00b7ter", "mur\u00b7melnd", "gie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "ADV", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sie Seufzer in Dein Ach! Sieh Sch\u00e4ferinnen", "tokens": ["Sie", "Seuf\u00b7zer", "in", "Dein", "Ach", "!", "Sieh", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$.", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und Sch\u00e4fer, wie von ihren Wangen rinnen", "tokens": ["Und", "Sch\u00e4\u00b7fer", ",", "wie", "von", "ih\u00b7ren", "Wan\u00b7gen", "rin\u00b7nen"], "token_info": ["word", "word", 