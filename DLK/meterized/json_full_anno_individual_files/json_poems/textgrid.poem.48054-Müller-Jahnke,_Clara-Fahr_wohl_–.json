{"textgrid.poem.48054": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Fahr wohl \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus rastlosem Ringen nach Schein und Schaum", "tokens": ["Aus", "rast\u00b7lo\u00b7sem", "Rin\u00b7gen", "nach", "Schein", "und", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "in ewige Ruh gerettet \u2013", "tokens": ["in", "e\u00b7wi\u00b7ge", "Ruh", "ge\u00b7ret\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "fahr wohl, mein sch\u00f6ner Jugendtraum,", "tokens": ["fahr", "wohl", ",", "mein", "sch\u00f6\u00b7ner", "Ju\u00b7gend\u00b7traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie haben dich sanft gebettet.", "tokens": ["sie", "ha\u00b7ben", "dich", "sanft", "ge\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie haben des Friedens Palmenzweig", "tokens": ["Sie", "ha\u00b7ben", "des", "Frie\u00b7dens", "Pal\u00b7men\u00b7zweig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "um deine Stirn geschlungen,", "tokens": ["um", "dei\u00b7ne", "Stirn", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "den Sarg geschm\u00fcckt mit Lorbeer reich,", "tokens": ["den", "Sarg", "ge\u00b7schm\u00fcckt", "mit", "Lor\u00b7beer", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den lebend du nicht errungen.", "tokens": ["den", "le\u00b7bend", "du", "nicht", "er\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "PTKNEG", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Und wenn der Abend sinkt herab,", "tokens": ["Und", "wenn", "der", "A\u00b7bend", "sinkt", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von fern die Glocken klingen,", "tokens": ["von", "fern", "die", "Glo\u00b7cken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "dann will ich schleichen an dein Grab", "tokens": ["dann", "will", "ich", "schlei\u00b7chen", "an", "dein", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und dir die Rose bringen.", "tokens": ["und", "dir", "die", "Ro\u00b7se", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$."], "meter