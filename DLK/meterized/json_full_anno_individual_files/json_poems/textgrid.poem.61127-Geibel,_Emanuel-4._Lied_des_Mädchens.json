{"textgrid.poem.61127": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4. Lied des M\u00e4dchens", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Mond, mein leuchtendheller Mond im klaren Lichtgewande,", "tokens": ["O", "Mond", ",", "mein", "leuch\u00b7tend\u00b7hel\u00b7ler", "Mond", "im", "kla\u00b7ren", "Licht\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Der du dort oben ziehst im Blau, und der du niederschauest,", "tokens": ["Der", "du", "dort", "o\u00b7ben", "ziehst", "im", "Blau", ",", "und", "der", "du", "nie\u00b7der\u00b7schau\u00b7est", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "APPRART", "NN", "$,", "KON", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "O sahst du meine Liebe nicht, den vielgeliebten J\u00fcngling?", "tokens": ["O", "sahst", "du", "mei\u00b7ne", "Lie\u00b7be", "nicht", ",", "den", "viel\u00b7ge\u00b7lieb\u00b7ten", "J\u00fcng\u00b7ling", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "In welchem Schlosse sitzt er nun, in welchem Schlosse trinkt er?", "tokens": ["In", "wel\u00b7chem", "Schlos\u00b7se", "sitzt", "er", "nun", ",", "in", "wel\u00b7chem", "Schlos\u00b7se", "trinkt", "er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "ADV", "$,", "APPR", "PWAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Wes H\u00e4nde schenken ihm den Wein? - und ach, die meinen rasten.", "tokens": ["Wes", "H\u00e4n\u00b7de", "schen\u00b7ken", "ihm", "den", "Wein", "?", "und", "ach", ",", "die", "mei\u00b7nen", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "KON", "XY", "$,", "PRELS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Wes Augen schaun ihn an mit Lust? - und meine sind voll Tr\u00e4nen.", "tokens": ["Wes", "Au\u00b7gen", "schaun", "ihn", "an", "mit", "Lust", "?", "und", "mei\u00b7ne", "sind", "voll", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPR", "APPR", "NN", "$.", "$(", "KON", "PPOSAT", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "An wessen Tische ruht er aus? - und meiner steht verlassen.", "tokens": ["An", "wes\u00b7sen", "Ti\u00b7sche", "ruht", "er", "aus", "?", "und", "mei\u00b7ner", "steht", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "KON", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Wes Lippe k\u00fc\u00dft und kost mit ihm? - und meine brennt in Sehnsucht!", "tokens": ["Wes", "Lip\u