{"textgrid.poem.40556": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das sind die Fluren gottgesegnet,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das sind die Fluren gottgesegnet,", "tokens": ["Das", "sind", "die", "Flu\u00b7ren", "gott\u00b7ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist der alte deutsche Rhein!", "tokens": ["Das", "ist", "der", "al\u00b7te", "deut\u00b7sche", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von der Gef\u00e4hrten Lippen regnet", "tokens": ["Von", "der", "Ge\u00b7f\u00e4hr\u00b7ten", "Lip\u00b7pen", "reg\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein andrer Reim als Wein und Wein!", "tokens": ["Kein", "an\u00b7drer", "Reim", "als", "Wein", "und", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie kommt's, da\u00df diesen nun ich f\u00e4nde,", "tokens": ["Wie", "kommt's", ",", "da\u00df", "die\u00b7sen", "nun", "ich", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PDAT", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den h\u00e4rt'sten von den Reimen all?", "tokens": ["Den", "h\u00e4rt'\u00b7sten", "von", "den", "Rei\u00b7men", "all", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "PIAT", "$."], "meter": "+++--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich vom gr\u00fcnen Rebgel\u00e4nde", "tokens": ["Da\u00df", "ich", "vom", "gr\u00fc\u00b7nen", "Reb\u00b7ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "R\u00fcckschau' zum grauen Festungswall?", "tokens": ["R\u00fcck\u00b7schau'", "zum", "grau\u00b7en", "Fes\u00b7tungs\u00b7wall", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Dort mu\u00dft' ich bl\u00fch'nde Rosenwangen", "tokens": ["Dort", "mu\u00dft'", "ich", "bl\u00fch'n\u00b7de", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umrahmt von Kerkergittern sehn,", "tokens": ["Um\u00b7rahmt", "von", "Ker\u00b7ker\u00b7git\u00b7tern", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort sah aus schwarzen Eisenstangen", "tokens": ["Dort", "sah", "aus", "schwar\u00b7zen", "Ei\u00b7sen\u00b7stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein blondes J\u00fcnglingshaupt ich sp\u00e4hn!", "tokens": ["Ein", "blon\u00b7des", "J\u00fcng\u00b7lings\u00b7haupt", "ich", "sp\u00e4hn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohl meint' ich, da\u00df am Fensterrande", "tokens": ["Wohl", "meint'", "ich", ",", "da\u00df", "am", "Fens\u00b7ter\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein s\u00fc\u00dfer Blumenstrau\u00df erbl\u00fcht,", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Blu\u00b7men\u00b7strau\u00df", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich ahnte nicht, da\u00df hier zu Lande", "tokens": ["Ich", "ahn\u00b7te", "nicht", ",", "da\u00df", "hier", "zu", "Lan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Kerkern Jugend man erzieht!", "tokens": ["In", "Ker\u00b7kern", "Ju\u00b7gend", "man", "er\u00b7zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo Fesseln J\u00fcnglingsh\u00e4nde dr\u00fccken,", "tokens": ["Wo", "Fes\u00b7seln", "J\u00fcng\u00b7lings\u00b7h\u00e4n\u00b7de", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df schlimm es mit den Alten stehn!", "tokens": ["Mu\u00df", "schlimm", "es", "mit", "den", "Al\u00b7ten", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach deren Armen m\u00f6cht' ich blicken,", "tokens": ["Nach", "de\u00b7ren", "Ar\u00b7men", "m\u00f6cht'", "ich", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ob Kettenspur nicht dran zu sehn?", "tokens": ["Ob", "Ket\u00b7ten\u00b7spur", "nicht", "dran", "zu", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was hat das junge Volk verbrochen?", "tokens": ["Was", "hat", "das", "jun\u00b7ge", "Volk", "ver\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Fehler selbst ist sch\u00f6nheitreich!", "tokens": ["Sein", "Feh\u00b7ler", "selbst", "ist", "sch\u00f6n\u00b7hei\u00b7treich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vulkanen gleich, die Laven kochen,", "tokens": ["Vul\u00b7ka\u00b7nen", "gleich", ",", "die", "La\u00b7ven", "ko\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sturzb\u00e4chen, alpentquollnen, gleich.", "tokens": ["Sturz\u00b7b\u00e4\u00b7chen", ",", "al\u00b7pent\u00b7quoll\u00b7nen", ",", "gleich", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADV", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Staunt im Vesuve Gottes Wunder,", "tokens": ["Staunt", "im", "Ve\u00b7su\u00b7ve", "Got\u00b7tes", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Pflanzt dran der s\u00fc\u00dfen Reben Zaun!", "tokens": ["Pflanzt", "dran", "der", "s\u00fc\u00b7\u00dfen", "Re\u00b7ben", "Zaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wer hie\u00df euch, so nah dem Zunder,", "tokens": ["Doch", "wer", "hie\u00df", "euch", ",", "so", "nah", "dem", "Zun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rings eure morschen H\u00fctten baun?", "tokens": ["Rings", "eu\u00b7re", "mor\u00b7schen", "H\u00fct\u00b7ten", "baun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sonnt euch in Sturzbachs Farbenbogen!", "tokens": ["Sonnt", "euch", "in", "Sturz\u00b7bachs", "Far\u00b7ben\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch euch zum Bade dient er schlecht;", "tokens": ["Doch", "euch", "zum", "Ba\u00b7de", "dient", "er", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht da\u00df einst im Thal die Wogen", "tokens": ["Viel\u00b7leicht", "da\u00df", "einst", "im", "Thal", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Bad und R\u00e4dertrieb gerecht!", "tokens": ["Zu", "Bad", "und", "R\u00e4\u00b7der\u00b7trieb", "ge\u00b7recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Kann \u00bbFreiheit, Vaterland!\u00ab euch schrecken,", "tokens": ["Kann", "\u00bb", "Frei\u00b7heit", ",", "Va\u00b7ter\u00b7land", "!", "\u00ab", "euch", "schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$(", "NN", "$,", "NN", "$.", "$(", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gejauchzt aus voller J\u00fcnglingsbrust?", "tokens": ["Ge\u00b7jauchzt", "aus", "vol\u00b7ler", "J\u00fcng\u00b7lings\u00b7brust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Riesengeist ist's, den zu wecken,", "tokens": ["Der", "Rie\u00b7sen\u00b7geist", "ist's", ",", "den", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nicht zu bannen ihr gewu\u00dft!", "tokens": ["Doch", "nicht", "zu", "ban\u00b7nen", "ihr", "ge\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Traun, wo die Jugend will entwenden", "tokens": ["Traun", ",", "wo", "die", "Ju\u00b7gend", "will", "ent\u00b7wen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Alten Degen, scharf und blank,", "tokens": ["Der", "Al\u00b7ten", "De\u00b7gen", ",", "scharf", "und", "blank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wankt, statt des Schwerts, in greisen H\u00e4nden", "tokens": ["Wankt", ",", "statt", "des", "Schwerts", ",", "in", "grei\u00b7sen", "H\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUI", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gewi\u00df ein Binsenzepter schwank!", "tokens": ["Ge\u00b7wi\u00df", "ein", "Bin\u00b7sen\u00b7zep\u00b7ter", "schwank", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und wo die Jugend, Rath zu halten,", "tokens": ["Und", "wo", "die", "Ju\u00b7gend", ",", "Rath", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich dr\u00e4ngt zum Senatorenstuhl,", "tokens": ["Sich", "dr\u00e4ngt", "zum", "Se\u00b7na\u00b7to\u00b7ren\u00b7stuhl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da machten sich's gewi\u00df die Alten", "tokens": ["Da", "mach\u00b7ten", "sich's", "ge\u00b7wi\u00df", "die", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vorerst bequem im Lotterpfuhl!", "tokens": ["Vor\u00b7erst", "be\u00b7quem", "im", "Lot\u00b7ter\u00b7pfuhl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Und wenn von steilen Bergesspitzen", "tokens": ["Und", "wenn", "von", "stei\u00b7len", "Ber\u00b7ges\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jugend Wort das Volk ermannt,", "tokens": ["Der", "Ju\u00b7gend", "Wort", "das", "Volk", "er\u00b7mannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verkrochen l\u00e4ngst in Thalespf\u00fctzen", "tokens": ["Ver\u00b7kro\u00b7chen", "l\u00e4ngst", "in", "Tha\u00b7les\u00b7pf\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Alten sich vorm Sonnenbrand.", "tokens": ["Die", "Al\u00b7ten", "sich", "vorm", "Son\u00b7nen\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Drum scheint's, da\u00df f\u00fcr der Alten S\u00fcnden", "tokens": ["Drum", "scheint's", ",", "da\u00df", "f\u00fcr", "der", "Al\u00b7ten", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Jugend fromm die Kette nahm:", "tokens": ["Die", "Ju\u00b7gend", "fromm", "die", "Ket\u00b7te", "nahm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Kerker m\u00fc\u00dften Greis' erblinden,", "tokens": ["Im", "Ker\u00b7ker", "m\u00fc\u00df\u00b7ten", "Greis'", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Erz br\u00e4ch' ihre H\u00e4nde lahm!", "tokens": ["Das", "Erz", "br\u00e4ch'", "ih\u00b7re", "H\u00e4n\u00b7de", "lahm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Drum tragt, ihr J\u00fcngling', ohne Schelten", "tokens": ["Drum", "tragt", ",", "ihr", "J\u00fcng\u00b7ling'", ",", "oh\u00b7ne", "Schel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-", "mea