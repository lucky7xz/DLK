{"dta.poem.1224": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6nen aber gef\u00e4hrlichen Fr\u00fcchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Zwey Aepffel sind die Br\u00fcst/ Erdbeeren ihre H\u00f6hen:", "tokens": ["Zwey", "A\u00b7e\u00b7pf\u00b7fel", "sind", "die", "Br\u00fcst", "/", "Erd\u00b7bee\u00b7ren", "ih\u00b7re", "H\u00f6\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "$(", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Hier mu\u00df der Schnee verschwarzt/ erblast die Rose stehen.", "tokens": ["Hier", "mu\u00df", "der", "Schnee", "ver\u00b7schwarzt", "/", "er\u00b7blast", "die", "Ro\u00b7se", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVPP", "$(", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie jene Frucht bald fault/ so m\u00fcssen die vergehen:", "tokens": ["Wie", "je\u00b7ne", "Frucht", "bald", "fault", "/", "so", "m\u00fcs\u00b7sen", "die", "ver\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "VVFIN", "$(", "ADV", "VMFIN", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Naschen bringt Gefahr: drum la\u00df die Fr\u00fcchte stehen!", "tokens": ["Das", "Na\u00b7schen", "bringt", "Ge\u00b7fahr", ":", "drum", "l