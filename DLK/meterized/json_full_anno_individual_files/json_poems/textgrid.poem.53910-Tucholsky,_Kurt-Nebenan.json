{"textgrid.poem.53910": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Nebenan", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es raschelt so im Nebenzimmer", "tokens": ["Es", "ra\u00b7schelt", "so", "im", "Ne\u00b7ben\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "im zweiten Stock, 310 \u2013", "tokens": ["im", "zwei\u00b7ten", "Stock", ",", "310", "\u2013"], "token_info": ["word", "word", "word", "punct", "number", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "CARD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ich sehe einen gelben Schimmer,", "tokens": ["ich", "se\u00b7he", "ei\u00b7nen", "gel\u00b7ben", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ich h\u00f6re, doch ich kann nichts sehn.", "tokens": ["ich", "h\u00f6\u00b7re", ",", "doch", "ich", "kann", "nichts", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lacht eine Frau? spricht da ein Mann?", "tokens": ["Lacht", "ei\u00b7ne", "Frau", "?", "spricht", "da", "ein", "Mann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich halte meinen Atem an \u2013", "tokens": ["ich", "hal\u00b7te", "mei\u00b7nen", "A\u00b7tem", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sind das da zwei? was die wohl sagen?", "tokens": ["Sind", "das", "da", "zwei", "?", "was", "die", "wohl", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "CARD", "$.", "PWS", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "ich sp\u00fcre Uhrgetick und Pulse schlagen . . .", "tokens": ["ich", "sp\u00fc\u00b7re", "Uhr\u00b7ge\u00b7tick", "und", "Pul\u00b7se", "schla\u00b7gen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ohr an die Wand. Was h\u00f6r ich dann", "tokens": ["Ohr", "an", "die", "Wand", ".", "Was", "h\u00f6r", "ich", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "von nebenan \u2013?", "tokens": ["von", "ne\u00b7be\u00b7nan", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "$(", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Knackt da ein Bett? Rauscht da ein Kissen?", "tokens": ["Knackt", "da", "ein", "Bett", "?", "Rauscht", "da", "ein", "Kis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$.", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist das mein Atem oder der", "tokens": ["Ist", "das", "mein", "A\u00b7tem", "o\u00b7der", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "KON", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "von jenen . . . alles will ich wissen!", "tokens": ["von", "je\u00b7nen", ".", ".", ".", "al\u00b7les", "will", "ich", "wis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$.", "$.", "$.", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gib, Gott, den Lautverst\u00e4rker her \u2013!", "tokens": ["Gib", ",", "Gott", ",", "den", "Laut\u00b7ver\u00b7st\u00e4r\u00b7ker", "her", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "PTKVZ", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein St\u00f6hnen; hab ichs nicht gewu\u00dft . . .", "tokens": ["Ein", "St\u00f6h\u00b7nen", ";", "hab", "ichs", "nicht", "ge\u00b7wu\u00dft", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "PIS", "PTKNEG", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich zecke an der fremden Lust;", "tokens": ["Ich", "ze\u00b7cke", "an", "der", "frem\u00b7den", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ich will sie voller Graun beneiden", "tokens": ["ich", "will", "sie", "vol\u00b7ler", "Graun", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "um jenes Dritte, \u00fcber beiden,", "tokens": ["um", "je\u00b7nes", "Drit\u00b7te", ",", "\u00fc\u00b7ber", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "das weder sie noch er empfinden kann . . .", "tokens": ["das", "we\u00b7der", "sie", "noch", "er", "emp\u00b7fin\u00b7den", "kann", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "KON", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbmarie \u2013!\u00ab", "tokens": ["\u00bb", "ma\u00b7rie", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "$(", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Zerplatzt.", "tokens": ["Zer\u00b7platzt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Ein Stubenm\u00e4dchen war nur nebenan.", "tokens": ["Ein", "Stu\u00b7ben\u00b7m\u00e4d\u00b7chen", "war", "nur", "ne\u00b7be\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "War ich als Kind wo eingeladen \u2013:", "tokens": ["War", "ich", "als", "Kind", "wo", "ein\u00b7ge\u00b7la\u00b7den", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "NN", "PWAV", "VVPP", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "nur ausw\u00e4rts schmeckt das Essen sch\u00f6n,", "tokens": ["nur", "aus\u00b7w\u00e4rts", "schmeckt", "das", "Es\u00b7sen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei andern siehst du die Fassaden,", "tokens": ["Bei", "an\u00b7dern", "siehst", "du", "die", "Fas\u00b7sa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "h\u00f6rst nur Musik und Lustgest\u00f6hn.", "tokens": ["h\u00f6rst", "nur", "Mu\u00b7sik", "und", "Lust\u00b7ge\u00b7st\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ich auch! ich auch! es greift die Hand", "tokens": ["Ich", "auch", "!", "ich", "auch", "!", "es", "greift", "die", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$.", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "nach einem nicht vorhandenen Land:", "tokens": ["nach", "ei\u00b7nem", "nicht", "vor\u00b7han\u00b7de\u00b7nen", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ja, da \u2013! strahlt warmer Lampenschimmer.", "tokens": ["Ja", ",", "da", "\u2013", "!", "strahlt", "war\u00b7mer", "Lam\u00b7pen\u00b7schim\u00b7mer", "."], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "$(", "$.", "VVFIN", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ja, da ist Heimat und das Gl\u00fcck.", "tokens": ["Ja", ",", "da", "ist", "Hei\u00b7mat", "und", "das", "Gl\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In jeder Stra\u00dfe l\u00e4\u00dft du immer", "tokens": ["In", "je\u00b7der", "Stra\u00b7\u00dfe", "l\u00e4\u00dft", "du", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "ein kleines St\u00fcckchen Herz zur\u00fcck.", "tokens": ["ein", "klei\u00b7nes", "St\u00fcck\u00b7chen", "Herz", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Darfst nie der eigenen Schw\u00e4che fluchen;", "tokens": ["Darfst", "nie", "der", "ei\u00b7ge\u00b7nen", "Schw\u00e4\u00b7che", "flu\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "mu\u00dft immer nach einem Dolchsto\u00df suchen.", "tokens": ["mu\u00dft", "im\u00b7mer", "nach", "ei\u00b7nem", "Dolch\u00b7sto\u00df", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Ja, da k\u00f6nnt ich in Ruhe schreiben!", "tokens": ["Ja", ",", "da", "k\u00f6nnt", "ich", "in", "Ru\u00b7he", "schrei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ja, hier \u2013! hier m\u00f6cht ich immer bleiben,", "tokens": ["Ja", ",", "hier", "\u2013", "!", "hier", "m\u00f6cht", "ich", "im\u00b7mer", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$(", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "in dieser Landschaft, wo wir stehn,", "tokens": ["in", "die\u00b7ser", "Land\u00b7schaft", ",", "wo", "wir", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "und ich m\u00f6chte nie mehr nach Hause gehn.", "tokens": ["und", "ich", "m\u00f6ch\u00b7te", "nie", "mehr", "nach", "Hau\u00b7se", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n ist nur, was niemals dein.", "tokens": ["Sch\u00f6n", "ist", "nur", ",", "was", "nie\u00b7mals", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "$,", "PRELS", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es ist heiter, zu reisen, und schrecklich, zu sein.", "tokens": ["Es", "ist", "hei\u00b7ter", ",", "zu", "rei\u00b7sen", ",", "und", "schreck\u00b7lich", ",", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PTKZU", "VVINF", "$,", "KON", "ADJD", "$,", "PTKZU", "VAINF", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Ewiger, ewiger Wandersmann", "tokens": ["E\u00b7wi\u00b7ger", ",", "e\u00b7wi\u00b7ger", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "um das kleine Zimmer nebenan.", "tokens": ["um", "das", "klei\u00b7ne", "Zim\u00b7mer", "ne\u00b7be\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "--+-+---+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Es raschelt so im Nebenzimmer", "tokens": ["Es", "ra\u00b7schelt", "so", "im", "Ne\u00b7ben\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "im zweiten Stock, 310 \u2013", "tokens": ["im", "zwei\u00b7ten", "Stock", ",", "310", "\u2013"], "token_info": ["word", "word", "word", "punct", "number", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "CARD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "ich sehe einen gelben Schimmer,", "tokens": ["ich", "se\u00b7he", "ei\u00b7nen", "gel\u00b7ben", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ich h\u00f6re, doch ich kann nichts sehn.", "tokens": ["ich", "h\u00f6\u00b7re", ",", "doch", "ich", "kann", "nichts", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lacht eine Frau? spricht da ein Mann?", "tokens": ["Lacht", "ei\u00b7ne", "Frau", "?", "spricht", "da", "ein", "Mann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich halte meinen Atem an \u2013", "tokens": ["ich", "hal\u00b7te", "mei\u00b7nen", "A\u00b7tem", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sind das da zwei? was die wohl sagen?", "tokens": ["Sind", "das", "da", "zwei", "?", "was", "die", "wohl", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "CARD", "$.", "PWS", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "ich sp\u00fcre Uhrgetick und Pulse schlagen . . .", "tokens": ["ich", "sp\u00fc\u00b7re", "Uhr\u00b7ge\u00b7tick", "und", "Pul\u00b7se", "schla\u00b7gen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ohr an die Wand. Was h\u00f6r ich dann", "tokens": ["Ohr", "an", "die", "Wand", ".", "Was", "h\u00f6r", "ich", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "von nebenan \u2013?", "tokens": ["von", "ne\u00b7be\u00b7nan", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "$(", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Knackt da ein Bett? Rauscht da ein Kissen?", "tokens": ["Knackt", "da", "ein", "Bett", "?", "Rauscht", "da", "ein", "Kis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$.", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist das mein Atem oder der", "tokens": ["Ist", "das", "mein", "A\u00b7tem", "o\u00b7der", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "KON", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "von jenen . . . alles will ich wissen!", "tokens": ["von", "je\u00b7nen", ".", ".", ".", "al\u00b7les", "will", "ich", "wis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$.", "$.", "$.", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gib, Gott, den Lautverst\u00e4rker her \u2013!", "tokens": ["Gib", ",", "Gott", ",", "den", "Laut\u00b7ver\u00b7st\u00e4r\u00b7ker", "her", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "PTKVZ", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein St\u00f6hnen; hab ichs nicht gewu\u00dft . . .", "tokens": ["Ein", "St\u00f6h\u00b7nen", ";", "hab", "ichs", "nicht", "ge\u00b7wu\u00dft", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "PIS", "PTKNEG", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich zecke an der fremden Lust;", "tokens": ["Ich", "ze\u00b7cke", "an", "der", "frem\u00b7den", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ich will sie voller Graun beneiden", "tokens": ["ich", "will", "sie", "vol\u00b7ler", "Graun", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "um jenes Dritte, \u00fcber beiden,", "tokens": ["um", "je\u00b7nes", "Drit\u00b7te", ",", "\u00fc\u00b7ber", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "das weder sie noch er empfinden kann . . .", "tokens": ["das", "we\u00b7der", "sie", "noch", "er", "emp\u00b7fin\u00b7den", "kann", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "KON", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbmarie \u2013!\u00ab", "tokens": ["\u00bb", "ma\u00b7rie", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "$(", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Zerplatzt.", "tokens": ["Zer\u00b7platzt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Ein Stubenm\u00e4dchen war nur nebenan.", "tokens": ["Ein", "Stu\u00b7ben\u00b7m\u00e4d\u00b7chen", "war", "nur", "ne\u00b7be\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-