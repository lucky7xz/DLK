{"dta.poem.19071": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LiI.   Auf den muthigen J\u00fcngling, Nicol.  \n Wilibald, Frey-Herrn von Ger\u00dfdorf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Der Christen wahrer Helden-Muth", "tokens": ["Der", "Chris\u00b7ten", "wah\u00b7rer", "Hel\u00b7den\u00b7Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4st sich nicht tr\u00e4ge finden,", "tokens": ["L\u00e4st", "sich", "nicht", "tr\u00e4\u00b7ge", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sein hochgebohrnes F\u00fcrsten-Blut,", "tokens": ["Sein", "hoch\u00b7ge\u00b7bohr\u00b7nes", "F\u00fcrs\u00b7ten\u00b7Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will immer \u00fcberwinden,", "tokens": ["Will", "im\u00b7mer", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er l\u00e4st die Kinder dieser Welt", "tokens": ["Er", "l\u00e4st", "die", "Kin\u00b7der", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich um das Eitle k\u00fcmmern,", "tokens": ["Sich", "um", "das", "Eit\u00b7le", "k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn der Feind sie \u00fcberf\u00e4llt,", "tokens": ["Und", "wenn", "der", "Feind", "sie", "\u00fc\u00b7berf\u00b7\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Banden H\u00fclf-lo\u00df wimmern.", "tokens": ["In", "Ban\u00b7den", "H\u00fcl\u00b7flo\u00df", "wim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sein edel-muthigs Angesicht,", "tokens": ["Sein", "e\u00b7del\u00b7muthigs", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wei\u00df zwar mit Lieblichkeiten,", "tokens": ["Wei\u00df", "zwar", "mit", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die, so sich wider ihn gericht,", "tokens": ["Die", ",", "so", "sich", "wi\u00b7der", "ihn", "ge\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Freunde zu erbeuten;", "tokens": ["Dem", "Freun\u00b7de", "zu", "er\u00b7beu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein die unsichtbare Kraft", "tokens": ["Al\u00b7lein", "die", "un\u00b7sicht\u00b7ba\u00b7re", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Des Reichs der Finsternissen", "tokens": ["Des", "Reichs", "der", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird seiner tapfern Ritterschaft", "tokens": ["Wird", "sei\u00b7ner", "tap\u00b7fern", "Rit\u00b7ter\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Schau-Spiel hingerissen.", "tokens": ["Zum", "Schau\u00b7Spiel", "hin\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Muth und Unerschrockenheit", "tokens": ["Der", "Muth", "und", "Un\u00b7er\u00b7schro\u00b7cken\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Christi zarten Jugend", "tokens": ["Von", "Chris\u00b7ti", "zar\u00b7ten", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vermischt mit wahrer Freundlichkeit,", "tokens": ["Ver\u00b7mischt", "mit", "wah\u00b7rer", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey deine Streiter-Tugend,", "tokens": ["Sey", "dei\u00b7ne", "Strei\u00b7ter\u00b7Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein freundlich Wesen mache hie,", "tokens": ["Sein", "freund\u00b7lich", "We\u00b7sen", "ma\u00b7che", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Menschen ihm zu Freunden,", "tokens": ["Die", "Men\u00b7schen", "ihm", "zu", "Freun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Muth gesiege spat und fr\u00fch,", "tokens": ["Sein", "Muth", "ge\u00b7sie\u00b7ge", "spat", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Geistern deinen Feinden.", "tokens": ["Den", "Geis\u00b7tern", "dei\u00b7nen", "Fein\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}