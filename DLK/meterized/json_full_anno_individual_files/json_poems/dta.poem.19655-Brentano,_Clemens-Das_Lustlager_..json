{"dta.poem.19655": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das Lustlager .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Reiter. H\u00f6r Bauer, was ich sage,               ", "tokens": ["Rei\u00b7ter", ".", "H\u00f6r", "Bau\u00b7er", ",", "was", "ich", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "NE", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Quartier und das ist aus,", "tokens": ["Das", "Quar\u00b7tier", "und", "das", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PDS", "VAFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn du'n Trompeter h\u00f6rst blasen;", "tokens": ["Wenn", "du'n", "Trom\u00b7pe\u00b7ter", "h\u00f6rst", "bla\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "So komm und weck mich auf.", "tokens": ["So", "komm", "und", "weck", "mich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sattle mir mein Pferdchen", "tokens": ["Und", "satt\u00b7le", "mir", "mein", "Pferd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und leg zur Hand mein Schwerdt,", "tokens": ["Und", "leg", "zur", "Hand", "mein", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Mantel thu drauf binden,", "tokens": ["Den", "Man\u00b7tel", "thu", "drauf", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich bald fertig werd.", "tokens": ["Da\u00df", "ich", "bald", "fer\u00b7tig", "werd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "M\u00e4dchen. Wer stehet drau\u00dfen vor meinem Fenster,               ", "tokens": ["M\u00e4d\u00b7chen", ".", "Wer", "ste\u00b7het", "drau\u00b7\u00dfen", "vor", "mei\u00b7nem", "Fens\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wer steht drau\u00dfen vor meiner Th\u00fcr?", "tokens": ["Wer", "steht", "drau\u00b7\u00dfen", "vor", "mei\u00b7ner", "Th\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ist es der Sch\u00f6nste, der Angenehmste,", "tokens": ["Ist", "es", "der", "Sch\u00f6ns\u00b7te", ",", "der", "An\u00b7ge\u00b7nehms\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der noch heute will von hier?", "tokens": ["Der", "noch", "heu\u00b7te", "will", "von", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VMFIN", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Reiter. Jungfrau, ich bitt sie ganz unterth\u00e4nig,               ", "tokens": ["Rei\u00b7ter", ".", "Jung\u00b7frau", ",", "ich", "bitt", "sie", "ganz", "un\u00b7ter\u00b7th\u00e4\u00b7nig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-++--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ach er\u00f6ffnen sie mir die Th\u00fcr,", "tokens": ["Ach", "er\u00b7\u00f6ff\u00b7nen", "sie", "mir", "die", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dieweil nunmehr die Zeit verflossen,", "tokens": ["Die\u00b7weil", "nun\u00b7mehr", "die", "Zeit", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich abscheiden mu\u00df von hier.", "tokens": ["Und", "ich", "ab\u00b7schei\u00b7den", "mu\u00df", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00e4dchen. Mein Vater liegt im obern Zimmer,               ", "tokens": ["M\u00e4d\u00b7chen", ".", "Mein", "Va\u00b7ter", "liegt", "im", "o\u00b7bern", "Zim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "In sein Schlafk\u00e4mmerlein ruhet er,", "tokens": ["In", "sein", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", "ru\u00b7het", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Er hat ein Brieflein in seiner Tasche,", "tokens": ["Er", "hat", "ein", "Brie\u00b7flein", "in", "sei\u00b7ner", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Antwort steht geschrieben darin.", "tokens": ["Die", "Ant\u00b7wort", "steht", "ge\u00b7schrie\u00b7ben", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Reiter. Tr\u00fcbe Wolken an dem Himmel,               ", "tokens": ["Rei\u00b7ter", ".", "Tr\u00fc\u00b7be", "Wol\u00b7ken", "an", "dem", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tausend Seufzer schick ich zu dir,", "tokens": ["Tau\u00b7send", "Seuf\u00b7zer", "schick", "ich", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dieweil ich mu\u00df fort an einen andern Ort,", "tokens": ["Die\u00b7weil", "ich", "mu\u00df", "fort", "an", "ei\u00b7nen", "an\u00b7dern", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Lebe wohl zu tausend guter Nacht.", "tokens": ["Le\u00b7be", "wohl", "zu", "tau\u00b7send", "gu\u00b7ter", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "M\u00e4dchen. Ich trage Ketten mein ganzes Leben,               ", "tokens": ["M\u00e4d\u00b7chen", ".", "Ich", "tra\u00b7ge", "Ket\u00b7ten", "mein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wer mich kann retten aus meiner Qual,", "tokens": ["Wer", "mich", "kann", "ret\u00b7ten", "aus", "mei\u00b7ner", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem will ich zeigen, da\u00df ich sein eigen,", "tokens": ["Dem", "will", "ich", "zei\u00b7gen", ",", "da\u00df", "ich", "sein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ihm getreu will seyn bis an mein Grab.", "tokens": ["Und", "ihm", "ge\u00b7treu", "will", "seyn", "bis", "an", "mein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VMFIN", "PPOSAT", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Reiter. Sterbe nicht mein Kind, das bitt ich dich,               ", "tokens": ["Rei\u00b7ter", ".", "Ster\u00b7be", "nicht", "mein", "Kind", ",", "das", "bitt", "ich", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "PTKNEG", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sonst ist verlohren all mein Freud,", "tokens": ["Sonst", "ist", "ver\u00b7loh\u00b7ren", "all", "mein", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Alle Berge und Th\u00e4ler zusammenfallen,", "tokens": ["Al\u00b7le", "Ber\u00b7ge", "und", "Th\u00e4\u00b7ler", "zu\u00b7sam\u00b7men\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Eh ich dir mein Kind untreu will seyn.", "tokens": ["Eh", "ich", "dir", "mein", "Kind", "un\u00b7treu", "will", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Der Tag kommt hergeschlichen,", "tokens": ["Der", "Tag", "kommt", "her\u00b7ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonne blickt herf\u00fcr,", "tokens": ["Die", "Son\u00b7ne", "blickt", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nachdem die Nacht verstrichen,", "tokens": ["Nach\u00b7dem", "die", "Nacht", "ver\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Bauer tritt an die Th\u00fcr.", "tokens": ["Der", "Bau\u00b7er", "tritt", "an", "die", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.10": {"line.1": {"text": "Bauer. Sie blasen wacker drauf,               ", "tokens": ["Bau\u00b7er", ".", "Sie", "bla\u00b7sen", "wa\u00b7cker", "drauf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Herr mein Soldat! steh auf,", "tokens": ["Herr", "mein", "Sol\u00b7dat", "!", "steh", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Pferd ist schon gesattelt,", "tokens": ["Das", "Pferd", "ist", "schon", "ge\u00b7sat\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Mantel gebunden drauf.", "tokens": ["Der", "Man\u00b7tel", "ge\u00b7bun\u00b7den", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Das Pferdchen mu\u00df ihn tragen", "tokens": ["Das", "Pferd\u00b7chen", "mu\u00df", "ihn", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl vor das hohe Haus,", "tokens": ["Wohl", "vor", "das", "ho\u00b7he", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihren schwarzen Augen", "tokens": ["Mit", "ih\u00b7ren", "schwar\u00b7zen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schaut Liebchen zum Fenster n'aus.", "tokens": ["Schaut", "Lieb\u00b7chen", "zum", "Fens\u00b7ter", "n'\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Was th\u00e4t er ihr zu Ehren?", "tokens": ["Was", "th\u00e4t", "er", "ihr", "zu", "Eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Scho\u00df Pulver in die Luft,", "tokens": ["Scho\u00df", "Pul\u00b7ver", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man den Knall th\u00e4t h\u00f6ren,", "tokens": ["Da\u00df", "man", "den", "Knall", "th\u00e4t", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ein Pistole pufft.", "tokens": ["Wie", "ein", "Pis\u00b7to\u00b7le", "pufft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Reiter. H\u00f6r Pferdchen, was ich sage,               ", "tokens": ["Rei\u00b7ter", ".", "H\u00f6r", "Pferd\u00b7chen", ",", "was", "ich", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "H\u00f6r Pferdchen, was ich sag,", "tokens": ["H\u00f6r", "Pferd\u00b7chen", ",", "was", "ich", "sag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heut Nacht must du mich tragen", "tokens": ["Heut", "Nacht", "must", "du", "mich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur\u00fcck vor Liebchens Th\u00fcr.", "tokens": ["Zu\u00b7r\u00fcck", "vor", "Lieb\u00b7chens", "Th\u00fcr."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}}}}