rem dunklen Augenpaar!", "tokens": ["Aus", "ih\u00b7rem", "dunk\u00b7len", "Au\u00b7gen\u00b7paar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich trat hinein, sie sah mich an,", "tokens": ["Ich", "trat", "hin\u00b7ein", ",", "sie", "sah", "mich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hatte sie mir's angetan:", "tokens": ["Da", "hat\u00b7te", "sie", "mir's", "an\u00b7ge\u00b7tan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein brennend Pfeiflein ging mir aus,", "tokens": ["Mein", "bren\u00b7nend", "Pfei\u00b7flein", "ging", "mir", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich trug ein brennend Herz nach Haus!", "tokens": ["Ich", "trug", "ein", "bren\u00b7nend", "Herz", "nach", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich trat hinein zur K\u00fcchent\u00fcr", "tokens": ["Ich", "trat", "hin\u00b7ein", "zur", "K\u00fc\u00b7chen\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und brannte an mein Pfeifchen mir,", "tokens": ["Und", "brann\u00b7te",