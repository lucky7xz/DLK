{"textgrid.poem.32623": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Ich trat hinein zur K\u00fcchent\u00fcr ...", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trat hinein zur K\u00fcchent\u00fcr", "tokens": ["Ich", "trat", "hin\u00b7ein", "zur", "K\u00fc\u00b7chen\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und brannte an mein Pfeifchen mir,", "tokens": ["Und", "brann\u00b7te", "an", "mein", "Pfei\u00b7fchen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das hei\u00dft, ich h\u00e4tt' es angebrannt,", "tokens": ["Das", "hei\u00dft", ",", "ich", "h\u00e4tt'", "es", "an\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4tt' es nicht ohnehin gebrannt.", "tokens": ["H\u00e4tt'", "es", "nicht", "oh\u00b7ne\u00b7hin", "ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Pfeifchen, ei das brannte fein,", "tokens": ["Mein", "Pfei\u00b7fchen", ",", "ei", "das", "brann\u00b7te", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "XY", "PDS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.