{"textgrid.poem.48400": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14. Der holdselige Name Jesus", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ists, das mich bestrahlt, da\u00df ich so rede frei?", "tokens": ["Was", "ists", ",", "das", "mich", "be\u00b7strahlt", ",", "da\u00df", "ich", "so", "re\u00b7de", "frei", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer gehet mir denn vor, dem ich so folg' ohn' Scheu?", "tokens": ["Wer", "ge\u00b7het", "mir", "denn", "vor", ",", "dem", "ich", "so", "fol\u00b7g'", "ohn'", "Scheu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+----+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wer, wer folget mir? Welch' eine laute Rede,", "tokens": ["Und", "wer", ",", "wer", "fol\u00b7get", "mir", "?", "Welch'", "ei\u00b7ne", "lau\u00b7te", "Re\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "PWS", "VVFIN", "PPER", "$.", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so hellen Glanzes voll, die mich so machet bl\u00f6de?", "tokens": ["so", "hel\u00b7len", "Glan\u00b7zes", "voll", ",", "die", "mich", "so", "ma\u00b7chet", "bl\u00f6\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$,", "PRELS", "PPER", "ADV", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer ist der neue Nam', als den der Herr selbst nennt?", "tokens": ["Wer", "ist", "der", "neu\u00b7e", "Nam'", ",", "als", "den", "der", "Herr", "selbst", "nennt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kom, meine Seel', und schau, schau den an, der dich kennt,", "tokens": ["Kom", ",", "mei\u00b7ne", "Seel'", ",", "und", "schau", ",", "schau", "den", "an", ",", "der", "dich", "kennt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "PTKVZ", "$,", "ADJD", "ART", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "den du liebst und er dich! Er ist herf\u00fcr geschossen,", "tokens": ["den", "du", "liebst", "und", "er", "dich", "!", "Er", "ist", "her\u00b7f\u00fcr", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "PPER", "PRF", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "gleich wie am Libanon ein ungekr\u00fcmmter Sprossen,", "tokens": ["gleich", "wie", "am", "Li\u00b7ba\u00b7non", "ein", "un\u00b7ge\u00b7kr\u00fcmm\u00b7ter", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "der mit der Wurzel recht bis in den Abgrund reicht", "tokens": ["der", "mit", "der", "Wur\u00b7zel", "recht", "bis", "in", "den", "Ab\u00b7grund", "reicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "APPR", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und einen Gipfel hat, der sich dem Himmel gleicht.", "tokens": ["und", "ei\u00b7nen", "Gip\u00b7fel", "hat", ",", "der", "sich", "dem", "Him\u00b7mel", "gleicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ists, das mich bestrahlt, da\u00df ich so rede frei?", "tokens": ["Was", "ists", ",", "das", "mich", "be\u00b7strahlt", ",", "da\u00df", "ich", "so", "re\u00b7de", "frei", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer gehet mir denn vor, dem ich so folg' ohn' Scheu?", "tokens": ["Wer", "ge\u00b7het", "mir", "denn", "vor", ",", "dem", "ich", "so", "fol\u00b7g'", "ohn'", "Scheu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+----+--+", "measure": "iambic.