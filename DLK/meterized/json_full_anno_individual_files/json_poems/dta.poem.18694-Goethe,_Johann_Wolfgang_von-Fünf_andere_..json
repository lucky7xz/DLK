{"dta.poem.18694": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnf andere .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Was verk\u00fcrzt mir die Zeit?", "tokens": ["Was", "ver\u00b7k\u00fcrzt", "mir", "die", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Th\u00e4tigkeit!", "tokens": ["Th\u00e4\u00b7tig\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Was macht sie unertr\u00e4glich lang?", "tokens": ["Was", "macht", "sie", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "lang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00fcssiggang!", "tokens": ["M\u00fcs\u00b7sig\u00b7gang", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Was bringt in Schulden?", "tokens": ["Was", "bringt", "in", "Schul\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Harren und Dulden!", "tokens": ["Har\u00b7ren", "und", "Dul\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Was macht Gewinnen?", "tokens": ["Was", "macht", "Ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Nicht lange besinnen!", "tokens": ["Nicht", "lan\u00b7ge", "be\u00b7sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Was bringt zu Ehren?", "tokens": ["Was", "bringt", "zu", "Eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Sich wehren!", "tokens": ["Sich", "weh\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}