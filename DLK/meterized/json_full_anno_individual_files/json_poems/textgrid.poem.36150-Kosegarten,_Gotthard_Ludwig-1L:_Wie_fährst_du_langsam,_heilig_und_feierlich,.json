{"textgrid.poem.36150": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie f\u00e4hrst du langsam, heilig und feierlich,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie f\u00e4hrst du langsam, heilig und feierlich,", "tokens": ["Wie", "f\u00e4hrst", "du", "lang\u00b7sam", ",", "hei\u00b7lig", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du goldner Himmelswagen, in blauer Luft!", "tokens": ["Du", "gold\u00b7ner", "Him\u00b7mels\u00b7wa\u00b7gen", ",", "in", "blau\u00b7er", "Luft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich h\u00f6re deiner R\u00e4der Rasseln,", "tokens": ["Ich", "h\u00f6\u00b7re", "dei\u00b7ner", "R\u00e4\u00b7der", "Ras\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6re das Wiehern von deinen Rossen!", "tokens": ["H\u00f6\u00b7re", "das", "Wie\u00b7hern", "von", "dei\u00b7nen", "Ros\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sieh! wie sie Flammen schnauben, die Wieherer!", "tokens": ["Sieh", "!", "wie", "sie", "Flam\u00b7men", "schnau\u00b7ben", ",", "die", "Wie\u00b7he\u00b7rer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "NN", "VVINF", "$,", "ART", "NN", "$."], "meter": "++-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sieh! wie sie Funken schlagen, die Schnaubenden!", "tokens": ["Sieh", "!", "wie", "sie", "Fun\u00b7ken", "schla\u00b7gen", ",", "die", "Schnau\u00b7ben\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "NN", "VVINF", "$,", "ART", "NN", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Horch! wie die hohe Himmelstra\u00dfe", "tokens": ["Horch", "!", "wie", "die", "ho\u00b7he", "Him\u00b7mel\u00b7stra\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hallt von dem Stampfen der Demanthufe.", "tokens": ["Hallt", "von", "dem", "Stamp\u00b7fen", "der", "De\u00b7mant\u00b7hu\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich liebe dich. Ich habe dich l\u00e4ngst geliebt,", "tokens": ["Ich", "lie\u00b7be", "dich", ".", "Ich", "ha\u00b7be", "dich", "l\u00e4ngst", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du k\u00f6niglicher Wagen! Mein Knabenblick", "tokens": ["Du", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Wa\u00b7gen", "!", "Mein", "Kna\u00b7ben\u00b7blick"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hat oft in tiefen Mittern\u00e4chten", "tokens": ["Hat", "oft", "in", "tie\u00b7fen", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ahndend und staunend an dir gehangen!", "tokens": ["Ahn\u00b7dend", "und", "stau\u00b7nend", "an", "dir", "ge\u00b7han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Nun lieb' ich dich noch hei\u00dfer, du Herrlicher!", "tokens": ["Nun", "lieb'", "ich", "dich", "noch", "hei\u00b7\u00dfer", ",", "du", "Herr\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJA", "$,", "PPER", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich seh' dich oft mit inniger Wehmuth an,", "tokens": ["Ich", "seh'", "dich", "oft", "mit", "in\u00b7ni\u00b7ger", "Weh\u00b7muth", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und eine leise Thr\u00e4ne bebt mir", "tokens": ["Und", "ei\u00b7ne", "lei\u00b7se", "Thr\u00e4\u00b7ne", "bebt", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem entbrannteren J\u00fcnglingsauge.", "tokens": ["In", "dem", "ent\u00b7brann\u00b7te\u00b7ren", "J\u00fcng\u00b7lings\u00b7au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein sanftes Thr\u00e4nchen weint' ich an Jinny's Brust,", "tokens": ["Ein", "sanf\u00b7tes", "Thr\u00e4n\u00b7chen", "weint'", "ich", "an", "Jin\u00b7ny's", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein sanftes Thr\u00e4nchen weinte die herrliche,", "tokens": ["Ein", "sanf\u00b7tes", "Thr\u00e4n\u00b7chen", "wein\u00b7te", "die", "herr\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als wir uns im vertrauten Garten", "tokens": ["Als", "wir", "uns", "im", "ver\u00b7trau\u00b7ten", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Busen an Busen umschlungen hielten!", "tokens": ["Bu\u00b7sen", "an", "Bu\u00b7sen", "um\u00b7schlun\u00b7gen", "hiel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Tiefe Mitternacht war um uns. Der bl\u00fchende", "tokens": ["Tie\u00b7fe", "Mit\u00b7ter\u00b7nacht", "war", "um", "uns", ".", "Der", "bl\u00fc\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "PPER", "$.", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jasmin der Laube duftete um uns her,", "tokens": ["Jas\u00b7min", "der", "Lau\u00b7be", "duf\u00b7te\u00b7te", "um", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Blinkten uns an durch das Gr\u00fcn der Laube.", "tokens": ["Blink\u00b7ten", "uns", "an", "durch", "das", "Gr\u00fcn", "der", "Lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Da fuhr mir durch die Seele ein d\u00fcsterer,", "tokens": ["Da", "fuhr", "mir", "durch", "die", "See\u00b7le", "ein", "d\u00fcs\u00b7te\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Wehgedanke, dumpf wie ein Unkenruf,", "tokens": ["Ein", "Weh\u00b7ge\u00b7dan\u00b7ke", ",", "dumpf", "wie", "ein", "Un\u00b7ken\u00b7ruf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und scharf wie Schwert in M\u00f6rderh\u00e4nden \u2013", "tokens": ["Und", "scharf", "wie", "Schwert", "in", "M\u00f6r\u00b7der\u00b7h\u00e4n\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! ein Gedanke von nahem Scheiden.", "tokens": ["Ach", "!", "ein", "Ge\u00b7dan\u00b7ke", "von", "na\u00b7hem", "Schei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Ich dr\u00fcckte hei\u00df mein holdiges Kind an mich.", "tokens": ["Ich", "dr\u00fcck\u00b7te", "hei\u00df", "mein", "hol\u00b7di\u00b7ges", "Kind", "an", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich ri\u00df mich ungeduldig von ihrer Brust", "tokens": ["Ich", "ri\u00df", "mich", "un\u00b7ge\u00b7dul\u00b7dig", "von", "ih\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Empor, und sah des Himmelswagens", "tokens": ["Em\u00b7por", ",", "und", "sah", "des", "Him\u00b7mels\u00b7wa\u00b7gens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Goldene Deichsel das Laub durchfunkeln.", "tokens": ["Gol\u00b7de\u00b7ne", "Deich\u00b7sel", "das", "Laub", "durch\u00b7fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Ich f\u00fchlte dunkle Wonne mit Weh gemischt.", "tokens": ["Ich", "f\u00fchl\u00b7te", "dunk\u00b7le", "Won\u00b7ne", "mit", "Weh", "ge\u00b7mischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich sprach zu meines Herzens erw\u00e4hlter Braut:", "tokens": ["Ich", "sprach", "zu", "mei\u00b7nes", "Her\u00b7zens", "er\u00b7w\u00e4hl\u00b7ter", "Braut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbsiehst du des hohen Himmelswagens", "tokens": ["\u00bb", "siehst", "du", "des", "ho\u00b7hen", "Him\u00b7mels\u00b7wa\u00b7gens"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Goldene Deichsel? Sie sey uns heilig!", "tokens": ["Gol\u00b7de\u00b7ne", "Deich\u00b7sel", "?", "Sie", "sey", "uns", "hei\u00b7lig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Wenn ich mich um dich gr\u00e4me in \u00f6der Fern',", "tokens": ["Wenn", "ich", "mich", "um", "dich", "gr\u00e4\u00b7me", "in", "\u00f6\u00b7der", "Fern'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn du dich um mich gr\u00e4mest in \u00f6der Fern',", "tokens": ["Wenn", "du", "dich", "um", "mich", "gr\u00e4\u00b7mest", "in", "\u00f6\u00b7der", "Fern'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "So wollen wir bei diesen Sternen", "tokens": ["So", "wol\u00b7len", "wir", "bei", "die\u00b7sen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Inniger einer des andern denken!\u00ab", "tokens": ["In\u00b7ni\u00b7ger", "ei\u00b7ner", "des", "an\u00b7dern", "den\u00b7ken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ART", "ADJA", "VVINF", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Ich sprach's, und eine Thr\u00e4ne der Wehmuth rann", "tokens": ["Ich", "sprach's", ",", "und", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "der", "Weh\u00b7muth", "rann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Jinny's Rosenwange. Sie rinnt seit dem", "tokens": ["Von", "Jin\u00b7ny's", "Ro\u00b7sen\u00b7wan\u00b7ge", ".", "Sie", "rinnt", "seit", "dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "$.", "PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr oft die Wang' hinab, wenn's dunkel", "tokens": ["Ihr", "oft", "die", "Wang'", "hin\u00b7ab", ",", "wenn's", "dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rund um sie ist, und die Sterne funkeln!", "tokens": ["Rund", "um", "sie", "ist", ",", "und", "die", "Ster\u00b7ne", "fun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VAFIN", "$,", "KON", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Der du den weltenwimmelnden Sternenplan", "tokens": ["Der", "du", "den", "wel\u00b7ten\u00b7wim\u00b7meln\u00b7den", "Ster\u00b7nen\u00b7plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit spiegelblauem Marmor gepflastert hast \u2013", "tokens": ["Mit", "spie\u00b7gel\u00b7blau\u00b7em", "Mar\u00b7mor", "ge\u00b7pflas\u00b7tert", "hast", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der du die goldgem\u00e4hnten Rosse", "tokens": ["Der", "du", "die", "gold\u00b7ge\u00b7m\u00e4hn\u00b7ten", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schirrst an die Deichsel des Flammenwagens,", "tokens": ["Schirrst", "an", "die", "Deich\u00b7sel", "des", "Flam\u00b7men\u00b7wa\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "O h\u00f6re, was ich flehe, Allliebender!", "tokens": ["O", "h\u00f6\u00b7re", ",", "was", "ich", "fle\u00b7he", ",", "A\u00b7lllie\u00b7ben\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O rei\u00df mich nicht von meiner erw\u00e4hlten Braut!", "tokens": ["O", "rei\u00df", "mich", "nicht", "von", "mei\u00b7ner", "er\u00b7w\u00e4hl\u00b7ten", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "La\u00df meine Jinny mir! La\u00df Jinny,", "tokens": ["La\u00df", "mei\u00b7ne", "Jin\u00b7ny", "mir", "!", "La\u00df", "Jin\u00b7ny", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "$.", "VVIMP", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Jinny am Arm mir durchs Leben wandeln!", "tokens": ["Jin\u00b7ny", "am", "Arm", "mir", "durchs", "Le\u00b7ben", "wan\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Von ihrem Engell\u00e4cheln zu That entflammt,", "tokens": ["Von", "ih\u00b7rem", "En\u00b7gel\u00b7l\u00e4\u00b7cheln", "zu", "That", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von ihrem keuschen Kusse mit Kraft beseelt,", "tokens": ["Von", "ih\u00b7rem", "keu\u00b7schen", "Kus\u00b7se", "mit", "Kraft", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Werd' ich mit Adlerflug der Tugend", "tokens": ["Werd'", "ich", "mit", "Ad\u00b7ler\u00b7flug", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonnigste, schwindelndste H\u00f6he erfliegen!", "tokens": ["Son\u00b7nigs\u00b7te", ",", "schwin\u00b7delnds\u00b7te", "H\u00f6\u00b7he", "er\u00b7flie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.15": {"line.1": {"text": "Wie f\u00e4hrst du langsam, heilig und feierlich,", "tokens": ["Wie", "f\u00e4hrst", "du", "lang\u00b7sam", ",", "hei\u00b7lig", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du goldner Himmelswagen, in blauer Luft!", "tokens": ["Du", "gold\u00b7ner", "Him\u00b7mels\u00b7wa\u00b7gen", ",", "in", "blau\u00b7er", "Luft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich h\u00f6re deiner R\u00e4der Rasseln,", "tokens": ["Ich", "h\u00f6\u00b7re", "dei\u00b7ner", "R\u00e4\u00b7der", "Ras\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6re das Wiehern von deinen Rossen!", "tokens": ["H\u00f6\u00b7re", "das", "Wie\u00b7hern", "von", "dei\u00b7nen", "Ros\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.16": {"line.1": {"text": "Sieh! wie sie Flammen schnauben, die Wieherer!", "tokens": ["Sieh", "!", "wie", "sie", "Flam\u00b7men", "schnau\u00b7ben", ",", "die", "Wie\u00b7he\u00b7rer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "NN", "VVINF", "$,", "ART", "NN", "$."], "meter": "++-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sieh! wie sie Funken schlagen, die Schnaubenden!", "tokens": ["Sieh", "!", "wie", "sie", "Fun\u00b7ken", "schla\u00b7gen", ",", "die", "Schnau\u00b7ben\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "NN", "VVINF", "$,", "ART", "NN", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Horch! wie die hohe Himmelstra\u00dfe", "tokens": ["Horch", "!", "wie", "die", "ho\u00b7he", "Him\u00b7mel\u00b7stra\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hallt von dem Stampfen der Demanthufe.", "tokens": ["Hallt", "von", "dem", "Stamp\u00b7fen", "der", "De\u00b7mant\u00b7hu\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Ich liebe dich. Ich habe dich l\u00e4ngst geliebt,", "tokens": ["Ich", "lie\u00b7be", "dich", ".", "Ich", "ha\u00b7be", "dich", "l\u00e4ngst", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du k\u00f6niglicher Wagen! Mein Knabenblick", "tokens": ["Du", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Wa\u00b7gen", "!", "Mein", "Kna\u00b7ben\u00b7blick"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hat oft in tiefen Mittern\u00e4chten", "tokens": ["Hat", "oft", "in", "tie\u00b7fen", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ahndend und staunend an dir gehangen!", "tokens": ["Ahn\u00b7dend", "und", "stau\u00b7nend", "an", "dir", "ge\u00b7han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.18": {"line.1": {"text": "Nun lieb' ich dich noch hei\u00dfer, du Herrlicher!", "tokens": ["Nun", "lieb'", "ich", "dich", "noch", "hei\u00b7\u00dfer", ",", "du", "Herr\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJA", "$,", "PPER", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich seh' dich oft mit inniger Wehmuth an,", "tokens": ["Ich", "seh'", "dich", "oft", "mit", "in\u00b7ni\u00b7ger", "Weh\u00b7muth", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und eine leise Thr\u00e4ne bebt mir", "tokens": ["Und", "ei\u00b7ne", "lei\u00b7se", "Thr\u00e4\u00b7ne", "bebt", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem entbrannteren J\u00fcnglingsauge.", "tokens": ["In", "dem", "ent\u00b7brann\u00b7te\u00b7ren", "J\u00fcng\u00b7lings\u00b7au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Ein sanftes Thr\u00e4nchen weint' ich an Jinny's Brust,", "tokens": ["Ein", "sanf\u00b7tes", "Thr\u00e4n\u00b7chen", "weint'", "ich", "an", "Jin\u00b7ny's", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein sanftes Thr\u00e4nchen weinte die herrliche,", "tokens": ["Ein", "sanf\u00b7tes", "Thr\u00e4n\u00b7chen", "wein\u00b7te", "die", "herr\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als wir uns im vertrauten Garten", "tokens": ["Als", "wir", "uns", "im", "ver\u00b7trau\u00b7ten", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Busen an Busen umschlungen hielten!", "tokens": ["Bu\u00b7sen", "an", "Bu\u00b7sen", "um\u00b7schlun\u00b7gen", "hiel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.20": {"line.1": {"text": "Tiefe Mitternacht war um uns. Der bl\u00fchende", "tokens": ["Tie\u00b7fe", "Mit\u00b7ter\u00b7nacht", "war", "um", "uns", ".", "Der", "bl\u00fc\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "PPER", "$.", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jasmin der Laube duftete um uns her,", "tokens": ["Jas\u00b7min", "der", "Lau\u00b7be", "duf\u00b7te\u00b7te", "um", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Blinkten uns an durch das Gr\u00fcn der Laube.", "tokens": ["Blink\u00b7ten", "uns", "an", "durch", "das", "Gr\u00fcn", "der", "Lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.21": {"line.1": {"text": "Da fuhr mir durch die Seele ein d\u00fcsterer,", "tokens": ["Da", "fuhr", "mir", "durch", "die", "See\u00b7le", "ein", "d\u00fcs\u00b7te\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Wehgedanke, dumpf wie ein Unkenruf,", "tokens": ["Ein", "Weh\u00b7ge\