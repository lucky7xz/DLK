e.4": {"text": "Schirrst an die Deichsel des Flammenwagens,", "tokens": ["Schirrst", "an", "die", "Deich\u00b7sel", "des", "Flam\u00b7men\u00b7wa\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.27": {"line.1": {"text": "O h\u00f6re, was ich flehe, Allliebender!", "tokens": ["O", "h\u00f6\u00b7re", ",", "was", "ich", "fle\u00b7he", ",", "A\u00b7lllie\u00b7ben\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O rei\u00df mich nicht von meiner erw\u00e4hlten Braut!", "tokens": ["O", "rei\u00df", "mich", "nicht", "von", "mei\u00b7ner", "er\u00b7w\u00e4hl\u00b7ten", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "La\u00df meine Jinny mir! La\u00df Jinny,", "tokens": ["La\u00df", "mei\u00b7ne", "Jin\u00b7ny", "mir", "!", "La\u00df", "Jin\u00b7ny", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "$.", "VVIMP", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Jinny am Arm mir durchs Leben wandeln!", "tokens": ["Jin\u00b7ny", "am", "Arm", "mir", "durchs", "Le\u00b7ben", "wan\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.28": {"line.1": {"text": "Von ihrem Engell\u00e4cheln zu That entflammt,", "