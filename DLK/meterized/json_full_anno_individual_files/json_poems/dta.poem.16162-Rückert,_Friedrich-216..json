{"dta.poem.16162": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "216.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gezogen ist ein Kreis, lang eh du tritst darein,", "tokens": ["Ge\u00b7zo\u00b7gen", "ist", "ein", "Kreis", ",", "lang", "eh", "du", "tritst", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "ADJD", "KOUS", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worin der Tummelplatz soll deiner Kr\u00e4fte seyn.", "tokens": ["Wo\u00b7rin", "der", "Tum\u00b7mel\u00b7platz", "soll", "dei\u00b7ner", "Kr\u00e4f\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erweitern kanst du selbst ihn weder noch verengern,", "tokens": ["Er\u00b7wei\u00b7tern", "kanst", "du", "selbst", "ihn", "we\u00b7der", "noch", "ver\u00b7en\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PPER", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht deine Bahn darin verk\u00fcrzen noch verl\u00e4ngern.", "tokens": ["Nicht", "dei\u00b7ne", "Bahn", "da\u00b7rin", "ver\u00b7k\u00fcr\u00b7zen", "noch", "ver\u00b7l\u00e4n\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "PAV", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zufrieden kanst du seyn bei jedem Schritt darinn,", "tokens": ["Zu\u00b7frie\u00b7den", "kanst", "du", "seyn", "bei", "je\u00b7dem", "Schritt", "da\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "APPR", "PIAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df, statt nach deiner Wahl, es geht nach Gottes Sinn.", "tokens": ["Da\u00df", ",", "statt", "nach", "dei\u00b7ner", "Wahl", ",", "es", "geht", "nach", "Got\u00b7tes", "Sinn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUI", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}