{"dta.poem.6865": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In diesen leichten Bildern nun,", "tokens": ["In", "die\u00b7sen", "leich\u00b7ten", "Bil\u00b7dern", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den uns ein Traum formirt, la\u00dft uns die Spur", "tokens": ["Den", "uns", "ein", "Traum", "for\u00b7mirt", ",", "la\u00dft", "uns", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,", "VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von der geheimen Kunst der w\u00fcrckenden Natur,", "tokens": ["Von", "der", "ge\u00b7hei\u00b7men", "Kunst", "der", "w\u00fcr\u00b7cken\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Besehen und erwegen.", "tokens": ["Be\u00b7se\u00b7hen", "und", "er\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Thun Seel und C\u00f6rper gleich zusammen, was sie thun;", "tokens": ["Thun", "Seel", "und", "C\u00f6r\u00b7per", "gleich", "zu\u00b7sam\u00b7men", ",", "was", "sie", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "PTKVZ", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Lasst uns die Regeln doch besonders \u00fcberlegen.", "tokens": ["Lasst", "uns", "die", "Re\u00b7geln", "doch", "be\u00b7son\u00b7ders", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indem sie Winckel macht,", "tokens": ["In\u00b7dem", "sie", "Win\u00b7ckel", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und Linien formiret", "tokens": ["Und", "Li\u00b7ni\u00b7en", "for\u00b7mi\u00b7ret"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}