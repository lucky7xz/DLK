{"dta.poem.9932": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzund w\u00e4scht un\u0303 schneidt man mich/ bald werd ich geprest/ ge-", "tokens": ["Je\u00b7tzund", "w\u00e4scht", "u\u00f1", "schneidt", "man", "mich", "/", "bald", "werd", "ich", "ge\u00b7prest", "/", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "PPER", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$(", "TRUNC"], "meter": "-+--+--++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jtzund ziert man mich mit gold/ bald mu\u00df ich was schwartzes", "tokens": ["Jt\u00b7zund", "ziert", "man", "mich", "mit", "gold", "/", "bald", "mu\u00df", "ich", "was", "schwart\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "NN", "$(", "ADV", "VMFIN", "PPER", "PIS", "ADJA"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "tragen.", "tokens": ["tra\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Ich mag doch kein s\u00e4uffer heissen/ ob ich schon viel trincke\u0303 mu\u00df/", "tokens": ["Ich", "mag", "doch", "kein", "s\u00e4uf\u00b7fer", "heis\u00b7sen", "/", "ob", "ich", "schon", "viel", "trin\u00b7ck\u1ebd", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "ADJD", "VVINF", "$(", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "VMFIN", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Bacchus hat mir niemals freude/ aber wohl gemacht verdru\u00df.", "tokens": ["Bac\u00b7chus", "hat", "mir", "nie\u00b7mals", "freu\u00b7de", "/", "a\u00b7ber", "wohl", "ge\u00b7macht", "ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "NN", "$(", "ADV", "ADV", "VVPP", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}