{"textgrid.poem.48077": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Morgenandacht", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein scheues Ahnen, das sein Haupt verh\u00fcllt,", "tokens": ["Ein", "scheu\u00b7es", "Ah\u00b7nen", ",", "das", "sein", "Haupt", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein tiefes Sehnen, das sich nie erf\u00fcllt,", "tokens": ["ein", "tie\u00b7fes", "Seh\u00b7nen", ",", "das", "sich", "nie", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ein blasser Mondstrahl der vertr\u00e4umten Nacht,", "tokens": ["ein", "blas\u00b7ser", "Mond\u00b7strahl", "der", "ver\u00b7tr\u00e4um\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "so irrt dein Bild durch diese Morgenpracht.", "tokens": ["so", "irrt", "dein", "Bild", "durch", "die\u00b7se", "Mor\u00b7gen\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus feuchten Nebeln steigt der klare Tag,", "tokens": ["Aus", "feuch\u00b7ten", "Ne\u00b7beln", "steigt", "der", "kla\u00b7re", "Tag", ","], "token_info": [