{"textgrid.poem.62023": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[als ich heut so bitterlich]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich heut so bitterlich", "tokens": ["Als", "ich", "heut", "so", "bit\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief vor Gott geweinet,", "tokens": ["Tief", "vor", "Gott", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da \u2013 ein kleines V\u00f6gelein", "tokens": ["Da", "\u2013", "ein", "klei\u00b7nes", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KOUS", "$(", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinem Schmerz sich einet;", "tokens": ["Mei\u00b7nem", "Schmerz", "sich", "ei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Flog zu mir bis an den Sims", "tokens": ["Flog", "zu", "mir", "bis", "an", "den", "Sims"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Fensters treulich:", "tokens": ["Mei\u00b7nes", "Fens\u00b7ters", "treu\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbweine nicht, Du Herzensma