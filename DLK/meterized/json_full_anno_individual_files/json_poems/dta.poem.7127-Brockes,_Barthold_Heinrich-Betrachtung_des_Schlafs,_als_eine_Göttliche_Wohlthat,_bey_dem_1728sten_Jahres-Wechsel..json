{"dta.poem.7127": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00f6nnte wol auf dieser Erden,", "tokens": ["Was", "k\u00f6nn\u00b7te", "wol", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo mehrentheils die Lust vermenget mit Beschwerden,", "tokens": ["Wo", "meh\u00b7ren\u00b7theils", "die", "Lust", "ver\u00b7men\u00b7get", "mit", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo nicht allein der Leib matt und geschw\u00e4chet wird,", "tokens": ["Wo", "nicht", "al\u00b7lein", "der", "Leib", "matt", "und", "ge\u00b7schw\u00e4\u00b7chet", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo auch die Geister tr\u00e4g, unf\u00e4hig, stumpff, verirrt,", "tokens": ["Wo", "auch", "die", "Geis\u00b7ter", "tr\u00e4g", ",", "un\u00b7f\u00e4\u00b7hig", ",", "stumpff", ",", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "---+--+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bey steten Wachen sind; wol ansersonnen werden,", "tokens": ["Bey", "ste\u00b7ten", "Wa\u00b7chen", "sind", ";", "wol", "an\u00b7ser\u00b7son\u00b7nen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$.", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das, wie der sanffte Schlaf, auf einmahl Leib und Geist", "tokens": ["Das", ",", "wie", "der", "sanff\u00b7te", "Schlaf", ",", "auf", "ein\u00b7mahl", "Leib", "und", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "APPR", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verwunderlich erquickt, der L\u00e4\u00dfigkeit entreisst,", "tokens": ["Ver\u00b7wun\u00b7der\u00b7lich", "er\u00b7quickt", ",", "der", "L\u00e4\u00b7\u00dfig\u00b7keit", "en\u00b7treisst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und beyd\u2019 aufs neu belebt. Was kann ein traurigs Hertz,", "tokens": ["Und", "beyd'", "aufs", "neu", "be\u00b7lebt", ".", "Was", "kann", "ein", "trau\u00b7rigs", "Hertz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJD", "VVPP", "$.", "PWS", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Sorg und Gram beklemmt, wenn auch sein bittrer", "tokens": ["Das", "Sorg", "und", "Gram", "be\u00b7klemmt", ",", "wenn", "auch", "sein", "bit\u00b7trer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "KOUS", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Schmertz", "tokens": ["Schmertz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Am unertr\u00e4glichsten, und seine Noth am gr\u00f6sten,", "tokens": ["Am", "un\u00b7er\u00b7tr\u00e4g\u00b7lichs\u00b7ten", ",", "und", "sei\u00b7ne", "Noth", "am", "gr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KON", "PPOSAT", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So, da\u00df er fast erliegt, erfrischen, st\u00e4rcken, tr\u00f6sten", "tokens": ["So", ",", "da\u00df", "er", "fast", "er\u00b7liegt", ",", "er\u00b7fri\u00b7schen", ",", "st\u00e4r\u00b7cken", ",", "tr\u00f6s\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als wie ein fester Schlaf? Verdru\u00df, Furcht, Pein und Leid", "tokens": ["Als", "wie", "ein", "fes\u00b7ter", "Schlaf", "?", "Ver\u00b7dru\u00df", ",", "Furcht", ",", "Pein", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vergehen, h\u00f6ren auf. In der Vergessenheit", "tokens": ["Ver\u00b7ge\u00b7hen", ",", "h\u00f6\u00b7ren", "auf", ".", "In", "der", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Tief unergr\u00fcndlichs Meer, ist alle Noth versencket.", "tokens": ["Tief", "un\u00b7er\u00b7gr\u00fcnd\u00b7lichs", "Meer", ",", "ist", "al\u00b7le", "Noth", "ver\u00b7sen\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Indem er sie nicht f\u00fchlt, und nicht daran gedencket;", "tokens": ["In\u00b7dem", "er", "sie", "nicht", "f\u00fchlt", ",", "und", "nicht", "da\u00b7ran", "ge\u00b7den\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,", "KON", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So ist sie w\u00fcrcklich nicht. Was man von Lethens Fluth", "tokens": ["So", "ist", "sie", "w\u00fcrck\u00b7lich", "nicht", ".", "Was", "man", "von", "Le\u00b7thens", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKNEG", "$.", "PWS", "PIS", "APPR", "NE", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.18": {"text": "Wol ehe fabulirt, das trifft hier gleichsam ein.", "tokens": ["Wol", "e\u00b7he", "fa\u00b7bu\u00b7lirt", ",", "das", "trifft", "hier", "gleich\u00b7sam", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "VVPP", "$,", "PDS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Scheint ein Betr\u00fcbter nicht, so bald er schl\u00e4fft und ruht,", "tokens": ["Scheint", "ein", "Be\u00b7tr\u00fcb\u00b7ter", "nicht", ",", "so", "bald", "er", "schl\u00e4fft", "und", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,", "ADV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit diesem holden Na\u00df getr\u00e4nckt zu seyn?", "tokens": ["Mit", "die\u00b7sem", "hol\u00b7den", "Na\u00df", "ge\u00b7tr\u00e4nckt", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Wer Ehre, Gut und Blut, Haus, Weib und Kind mu\u00df", "tokens": ["Wer", "Eh\u00b7re", ",", "Gut", "und", "Blut", ",", "Haus", ",", "Weib", "und", "Kind", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "ADJD", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "missen,", "tokens": ["mis\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Wer elend, kranck und arm, im schwartzen Kercker steckt;", "tokens": ["Wer", "e\u00b7lend", ",", "kranck", "und", "arm", ",", "im", "schwart\u00b7zen", "Ker\u00b7cker", "steckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wen ein schon naher Tod mit grausem Blick erschreckt,", "tokens": ["Wen", "ein", "schon", "na\u00b7her", "Tod", "mit", "grau\u00b7sem", "Blick", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der ist gleich seiner Angst, so bald er schl\u00e4fft, entrissen,", "tokens": ["Der", "ist", "gleich", "sei\u00b7ner", "Angst", ",", "so", "bald", "er", "schl\u00e4fft", ",", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und wird man ihn mit Recht, so lang er schl\u00e4fft, nicht k\u00f6nnen", "tokens": ["Und", "wird", "man", "ihn", "mit", "Recht", ",", "so", "lang", "er", "schl\u00e4fft", ",", "nicht", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PIS", "PPER", "APPR", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "PTKNEG", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Gefangen, elend, arm und ungl\u00fcckseelig nennen.", "tokens": ["Ge\u00b7fan\u00b7gen", ",", "e\u00b7lend", ",", "arm", "und", "un\u00b7gl\u00fcck\u00b7see\u00b7lig", "nen\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}