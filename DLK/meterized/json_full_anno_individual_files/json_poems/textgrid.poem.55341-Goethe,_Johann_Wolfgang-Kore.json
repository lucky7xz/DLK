{"textgrid.poem.55341": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Kore", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob Mutter? Tochter? Schwester? Enkelin?", "tokens": ["Ob", "Mut\u00b7ter", "?", "Toch\u00b7ter", "?", "Schwes\u00b7ter", "?", "En\u00b7ke\u00b7lin", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "$.", "NN", "$.", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Helios gezeugt? Von wer geboren?", "tokens": ["Von", "He\u00b7li\u00b7os", "ge\u00b7zeugt", "?", "Von", "wer", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$.", "APPR", "PWS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohin gewandert? Wo versteckt? Verloren?", "tokens": ["Wo\u00b7hin", "ge\u00b7wan\u00b7dert", "?", "Wo", "ver\u00b7steckt", "?", "Ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVPP", "$.", "PWAV", "VVPP", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gefunden? \u2013 R\u00e4tsel ist's dem K\u00fcnstlersinn.", "tokens": ["Ge\u00b7fun\u00b7den", "?", "\u2013", "R\u00e4t\u00b7sel", "ist's", "dem", "K\u00fcnst\u00b7ler\u00b7sinn", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ruhte sie verh\u00fcllt in d\u00fcstre Schleier,", "tokens": ["Und", "ruh\u00b7te", "sie", "ver\u00b7h\u00fcllt", "in", "d\u00fcst\u00b7re", "Schlei\u00b7er", ",