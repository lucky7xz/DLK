{"textgrid.poem.53321": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Bey wolseeligem Hintrit der HochEdelgebohrnen Frawen Helenen von Pr\u00f6hken, Seiner HochEdl. Gestr. Herrl. Herrn Ahasverus von Brandten, Churfl. Durchl. zu Brandenb. etc. Ober- vnd RegimentsRahts vnd Ober-Marschallen in Preussen etc. Hertzlich geliebten Gemahlin", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt aus mein Hertz, vnd sey bescheiden,", "tokens": ["Halt", "aus", "mein", "Hertz", ",", "vnd", "sey", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "$,", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schilt auff die Zucht des Herren nicht,", "tokens": ["Schilt", "auff", "die", "Zucht", "des", "Her\u00b7ren", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der wahren Gottes Kinder Pflicht", "tokens": ["Der", "wah\u00b7ren", "Got\u00b7tes", "Kin\u00b7der", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist alle Straff in Demuth leiden,", "tokens": ["Ist", "al\u00b7le", "Straff", "in", "De\u00b7muth", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd wiss, da\u00df Christus Sch\u00e4felein", "tokens": ["Vnd", "wiss", ",", "da\u00df", "Chris\u00b7tus", "Sch\u00e4\u00b7fe\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Blohs durch das Creutz gezeichnet seyn!", "tokens": ["Blohs", "durch", "das", "Creutz", "ge\u00b7zeich\u00b7net", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Druckt dich des Alters Last ohn massen,", "tokens": ["Druckt", "dich", "des", "Al\u00b7ters", "Last", "ohn", "mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4sst Kranckheit niemals von dir ab,", "tokens": ["L\u00e4sst", "Kran\u00b7ck\u00b7heit", "nie\u00b7mals", "von", "dir", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Eilt deiner Seelen Pfandt ins Grab,", "tokens": ["Eilt", "dei\u00b7ner", "See\u00b7len", "Pfandt", "ins", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du lebest einsam vnd verlassen", "tokens": ["Du", "le\u00b7best", "ein\u00b7sam", "vnd", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd w\u00fcnschest dir in andrer Noht", "tokens": ["Vnd", "w\u00fcn\u00b7schest", "dir", "in", "an\u00b7drer", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vieleicht aus Vngedult den Tod:", "tokens": ["Vie\u00b7leicht", "aus", "Vn\u00b7ge\u00b7dult", "den", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott z\u00fcchtigt nicht, ohn nur zum gutten,", "tokens": ["Gott", "z\u00fcch\u00b7tigt", "nicht", ",", "ohn", "nur", "zum", "gut\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "$,", "KOUI", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd die, so Ihm am liebsten sind;", "tokens": ["Vnd", "die", ",", "so", "Ihm", "am", "liebs\u00b7ten", "sind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "PPER", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein frommer Vater h\u00e4lt sein Kind", "tokens": ["Ein", "from\u00b7mer", "Va\u00b7ter", "h\u00e4lt", "sein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am meisten unter scharffen Ruten,", "tokens": ["Am", "meis\u00b7ten", "un\u00b7ter", "scharf\u00b7fen", "Ru\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein FeldHerr unter hartem Streit", "tokens": ["Ein", "Feld", "Herr", "un\u00b7ter", "har\u00b7tem", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die er wil kr\u00f6hnen mit der Zeit.", "tokens": ["Die", "er", "wil", "kr\u00f6h\u00b7nen", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer, Christo \u00e4hnlich, dort wil erben,", "tokens": ["Wer", ",", "Chris\u00b7to", "\u00e4hn\u00b7lich", ",", "dort", "wil", "er\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NE", "ADJD", "$,", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der thue es Ihm hie erstlich nach", "tokens": ["Der", "thue", "es", "Ihm", "hie", "erst\u00b7lich", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In aller M\u00fch vnd Vngemach", "tokens": ["In", "al\u00b7ler", "M\u00fch", "vnd", "Vn\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd suche mit Ihm auch zu sterben;", "tokens": ["Vnd", "su\u00b7che", "mit", "Ihm", "auch", "zu", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Noch keinem ist der Himmels-Standt", "tokens": ["Noch", "kei\u00b7nem", "ist", "der", "Him\u00b7mels\u00b7Standt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Schertz vnd Kurtzweil zuerkant.", "tokens": ["Durch", "Schertz", "vnd", "Kurt\u00b7zweil", "zu\u00b7er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ist es doch hie Vngl\u00fcck haben,", "tokens": ["Was", "ist", "es", "doch", "hie", "Vn\u00b7gl\u00fcck", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Creutz vnd Vnmuth seyn beschwehrt,", "tokens": ["Mit", "Creutz", "vnd", "Vn\u00b7muth", "seyn", "be\u00b7schwehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang dies kurtze Leben wehrt,", "tokens": ["So", "lang", "dies", "kurt\u00b7ze", "Le\u00b7ben", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd daf\u00fcr dort sich ewig laben", "tokens": ["Vnd", "da\u00b7f\u00fcr", "dort", "sich", "e\u00b7wig", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ADV", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In solchen Frewden, die kein Mann,", "tokens": ["In", "sol\u00b7chen", "Frew\u00b7den", ",", "die", "kein", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie tieff er sinnt, ersinnen kan?", "tokens": ["Wie", "tieff", "er", "sinnt", ",", "er\u00b7sin\u00b7nen", "kan", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vmb welcher willen viel in S\u00e4cken", "tokens": ["Vmb", "wel\u00b7cher", "wil\u00b7len", "viel", "in", "S\u00e4\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr F\u00fcrst- vnd K\u00f6niglichen Pracht", "tokens": ["F\u00fcr", "F\u00fcr\u00b7st", "vnd", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihr gantzes Leben zugebracht,", "tokens": ["Ihr", "gant\u00b7zes", "Le\u00b7ben", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebt f\u00fcr Schl\u00f6sser wilde Hecken,", "tokens": ["Ge\u00b7liebt", "f\u00fcr", "Schl\u00f6s\u00b7ser", "wil\u00b7de", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gesch\u00e4tzt Schwerdt, Marter, Strick vnd Glut", "tokens": ["Ge\u00b7sch\u00e4tzt", "Schwerdt", ",", "Mar\u00b7ter", ",", "Strick", "vnd", "Glut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr dieses Lebens h\u00f6chstes Gut.", "tokens": ["F\u00fcr", "die\u00b7ses", "Le\u00b7bens", "h\u00f6chs\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wir sehen, was die Hand des Herren", "tokens": ["Wir", "se\u00b7hen", ",", "was", "die", "Hand", "des", "Her\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Wolthat t\u00e4glich vns erzeigt,", "tokens": ["F\u00fcr", "Wolt\u00b7hat", "t\u00e4g\u00b7lich", "vns", "er\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob dann vnd wann sich Creutz er\u00e4ugt,", "tokens": ["Ob", "dann", "vnd", "wann", "sich", "Creutz", "er\u00b7\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "PWAV", "PRF", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer hat dawieder sich zu sperren?", "tokens": ["Wer", "hat", "da\u00b7wie\u00b7der", "sich", "zu", "sper\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PAV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Voraus, weil der sehr \u00fcbel steht", "tokens": ["Vo\u00b7raus", ",", "weil", "der", "sehr", "\u00fc\u00b7bel", "steht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "ia