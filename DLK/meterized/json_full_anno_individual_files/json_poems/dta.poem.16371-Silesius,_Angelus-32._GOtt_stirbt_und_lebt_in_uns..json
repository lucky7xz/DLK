{"dta.poem.16371": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "32.  GOtt stirbt und lebt in uns.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sterb\u2019 und leb\u2019 auch nicht: ", "tokens": ["Ich", "sterb'", "und", "leb'", "auch", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "in mir:", "tokens": ["in", "mir", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und was ich leben sol/ (b) lebt Er auch f\u00fcr und f\u00fcr.", "tokens": ["Und", "was", "ich", "le\u00b7ben", "sol", "/", "(", "b", ")", "lebt", "Er", "auch", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$(", "$(", "XY", "$(", "VVFIN", "PPER", "ADV", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}