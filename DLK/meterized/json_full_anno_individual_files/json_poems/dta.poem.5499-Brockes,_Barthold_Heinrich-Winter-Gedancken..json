], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Und kurtz: man sp\u00fchrt, zur Winters-Zeit,", "tokens": ["Und", "kurtz", ":", "man", "sp\u00fchrt", ",", "zur", "Win\u00b7ter\u00b7sZeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PIS", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu unsers weisen Sch\u00f6pfers Preise,", "tokens": ["Zu", "un\u00b7sers", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie auch, so gar im Schnee und Eise,", "tokens": ["Wie", "auch", ",", "so", "gar", "im", "Schnee", "und", "Ei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "ADV", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt ein frommes Aug\u2019 erfreut.", "tokens": ["Die", "Welt", "ein", "from\u00b7mes", "Aug'", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Ach s\u00e4he denn doch jederman,", "tokens": ["Ach", "s\u00e4\u00b7he", "denn", "doch", "je\u00b7der\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zumahl der, den der Frost nicht dr\u00fccket,", "tokens": ["Zu\u00b7mahl", "der", ",", "den", "der", "Frost", "nicht", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "w