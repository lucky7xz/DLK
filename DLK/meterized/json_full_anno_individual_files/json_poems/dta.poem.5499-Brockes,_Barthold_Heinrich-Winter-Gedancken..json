{"dta.poem.5499": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOtt! das Feuer w\u00e4rmet mich", "tokens": ["Mein", "Gott", "!", "das", "Feu\u00b7er", "w\u00e4r\u00b7met", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht nicht nur, da\u00df ich nicht friere;", "tokens": ["Und", "macht", "nicht", "nur", ",", "da\u00df", "ich", "nicht", "frie\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich im Frost auch Anmuth sp\u00fchre,", "tokens": ["Da\u00df", "ich", "im", "Frost", "auch", "An\u00b7muth", "sp\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daf\u00fcr erheb\u2019 und preis\u2019 ich dich!", "tokens": ["Da\u00b7f\u00fcr", "er\u00b7heb'", "und", "preis'", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchl\u2019 ietzt einen Trieb in mir,", "tokens": ["Ich", "f\u00fchl'", "ietzt", "ei\u00b7nen", "Trieb", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Winter-Opfer dir zu bringen,", "tokens": ["Ein", "Win\u00b7ter\u00b7Op\u00b7fer", "dir", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und deine Wunder zu besingen,", "tokens": ["Und", "dei\u00b7ne", "Wun\u00b7der", "zu", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ich, auch selbst im Frost, versp\u00fchr.", "tokens": ["Die", "ich", ",", "auch", "selbst", "im", "Frost", ",", "ver\u00b7sp\u00fchr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "ADV", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die d\u00fcstern Tag\u2019 erhellt der Schnee,", "tokens": ["Die", "d\u00fcs\u00b7tern", "Tag'", "er\u00b7hellt", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der jetzt die dunckle Welt bedecket,", "tokens": ["Der", "jetzt", "die", "dunck\u00b7le", "Welt", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mehr vergn\u00fcgt und n\u00fctzt, als schrecket;", "tokens": ["Und", "mehr", "ver\u00b7gn\u00fcgt", "und", "n\u00fctzt", ",", "als", "schre\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "KON", "VVFIN", "$,", "KOUS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So da\u00df ich ihn mit Anmuth seh.", "tokens": ["So", "da\u00df", "ich", "ihn", "mit", "An\u00b7muth", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht ohne Regung unsrer Brust", "tokens": ["Nicht", "oh\u00b7ne", "Re\u00b7gung", "uns\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erblickt man weisse weite Felder.", "tokens": ["Er\u00b7blickt", "man", "weis\u00b7se", "wei\u00b7te", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wipfel der beschneiten W\u00e4lder", "tokens": ["Die", "Wip\u00b7fel", "der", "be\u00b7schnei\u00b7ten", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erregen uns besondre Lust.", "tokens": ["Er\u00b7re\u00b7gen", "uns", "be\u00b7sond\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Indem die schwartze Dunckelheit", "tokens": ["In\u00b7dem", "die", "schwart\u00b7ze", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Aeste, welche nicht beklebet,", "tokens": ["Der", "A\u00b7es\u00b7te", ",", "wel\u00b7che", "nicht", "be\u00b7kle\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den weissen Schnee noch mehr erhebet,", "tokens": ["Den", "weis\u00b7sen", "Schnee", "noch", "mehr", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Gegensatz und Unterscheid.", "tokens": ["Jm", "Ge\u00b7gen\u00b7satz", "und", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Desgleichen wircken hier und dort", "tokens": ["Des\u00b7glei\u00b7chen", "wir\u00b7cken", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verstreut- und halb-beschneite Reiser.", "tokens": ["Ver\u00b7streut", "und", "halb\u00b7be\u00b7schnei\u00b7te", "Rei\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Gipfel der bemoosten H\u00e4user", "tokens": ["Die", "Gip\u00b7fel", "der", "be\u00b7moos\u00b7ten", "H\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind gleichfals sch\u00f6n an manchem Ort.", "tokens": ["Sind", "gleich\u00b7fals", "sch\u00f6n", "an", "man\u00b7chem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So lassen auch, nicht minder sch\u00f6n,", "tokens": ["So", "las\u00b7sen", "auch", ",", "nicht", "min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die regel-rechten Ziegel-D\u00e4cher", "tokens": ["Die", "re\u00b7gel\u00b7rech\u00b7ten", "Zie\u00b7gel\u00b7D\u00e4\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Schnee die nett-gevierten F\u00e4cher", "tokens": ["Jm", "Schnee", "die", "net\u00b7tge\u00b7vier\u00b7ten", "F\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel deutlicher, als sonsten, sehn.", "tokens": ["Viel", "deut\u00b7li\u00b7cher", ",", "als", "sons\u00b7ten", ",", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Durchs Wassers Blau, wenn noch kein Eis", "tokens": ["Durchs", "Was\u00b7sers", "Blau", ",", "wenn", "noch", "kein", "Eis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fluht mit Schollen \u00fcberbr\u00fccket,", "tokens": ["Die", "Fluht", "mit", "Schol\u00b7len", "\u00fc\u00b7berb\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird der gefallne Schnee geschm\u00fccket,", "tokens": ["Wird", "der", "ge\u00b7fall\u00b7ne", "Schnee", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es macht sein Wei\u00df noch einst so wei\u00df.", "tokens": ["Es", "macht", "sein", "Wei\u00df", "noch", "einst", "so", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Zumahl wenn in dem Wieder-Schein", "tokens": ["Zu\u00b7mahl", "wenn", "in", "dem", "Wie\u00b7der\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Ufers wei\u00df beschneite H\u00f6hen,", "tokens": ["Des", "U\u00b7fers", "wei\u00df", "be\u00b7schnei\u00b7te", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dunckler Fl\u00e4che hell zu sehen", "tokens": ["Auf", "dunck\u00b7ler", "Fl\u00e4\u00b7che", "hell", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wei\u00df und blau gemischet seyn.", "tokens": ["Und", "wei\u00df", "und", "blau", "ge\u00b7mi\u00b7schet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Seht wie uns, selbst der Dorn vergn\u00fcgt,", "tokens": ["Seht", "wie", "uns", ",", "selbst", "der", "Dorn", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn, nach der wei\u00df-beschneiten Speise,", "tokens": ["Wenn", ",", "nach", "der", "wei\u00df\u00b7be\u00b7schnei\u00b7ten", "Spei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch ihn, zusammt der bunten Meise,", "tokens": ["Durch", "ihn", ",", "zu\u00b7sammt", "der", "bun\u00b7ten", "Mei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Z\u00e4une kleiner K\u00f6nig fliegt.", "tokens": ["Der", "Z\u00e4u\u00b7ne", "klei\u00b7ner", "K\u00f6\u00b7nig", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Des welcken Schilffes gelber Schein", "tokens": ["Des", "wel\u00b7cken", "Schilf\u00b7fes", "gel\u00b7ber", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird auch nicht ohne Lust versp\u00fchret;", "tokens": ["Wird", "auch", "nicht", "oh\u00b7ne", "Lust", "ver\u00b7sp\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es unterbricht es schm\u00fcckt, und zieret", "tokens": ["Es", "un\u00b7ter\u00b7bricht", "es", "schm\u00fcckt", ",", "und", "zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das weisse, das sonst allgemein.", "tokens": ["Das", "weis\u00b7se", ",", "das", "sonst", "all\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Jmgleichen theilt und unterbricht", "tokens": ["Jm\u00b7glei\u00b7chen", "theilt", "und", "un\u00b7ter\u00b7bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seiner Striche duncklen L\u00e4nge,", "tokens": ["Mit", "sei\u00b7ner", "Stri\u00b7che", "dunck\u00b7len", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der tieffen Wasser-Graben Menge", "tokens": ["Der", "tief\u00b7fen", "Was\u00b7ser\u00b7Gra\u00b7ben", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom weissen Schnee das weisse Licht.", "tokens": ["Vom", "weis\u00b7sen", "Schnee", "das", "weis\u00b7se", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Wenn hier ein Gr\u00e4schen, dort ein Straus", "tokens": ["Wenn", "hier", "ein", "Gr\u00e4sc\u00b7hen", ",", "dort", "ein", "Straus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Schnee, worin es meist verstecket,", "tokens": ["Aus", "Schnee", ",", "wo\u00b7rin", "es", "meist", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein gr\u00fcnes Spitzgen eintzeln strecket,", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Spitz\u00b7gen", "eint\u00b7zeln", "stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht es nicht minder lieblich aus.", "tokens": ["Sieht", "es", "nicht", "min\u00b7der", "lieb\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Jmgleichen, wenn das glatte Gr\u00fcn", "tokens": ["Jm\u00b7glei\u00b7chen", ",", "wenn", "das", "glat\u00b7te", "Gr\u00fcn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Buxbaums, der im Garten gl\u00e4ntzet", "tokens": ["Des", "Bux\u00b7baums", ",", "der", "im", "Gar\u00b7ten", "gl\u00e4nt\u00