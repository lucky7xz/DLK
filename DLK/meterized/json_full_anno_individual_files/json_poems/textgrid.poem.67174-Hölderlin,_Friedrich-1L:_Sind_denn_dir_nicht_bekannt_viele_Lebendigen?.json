, "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "PIS", "VVPP", "PPER", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sei zur Freude gereimt, oder was k\u00f6nnte denn", "tokens": ["Sei", "zur", "Freu\u00b7de", "ge\u00b7reimt", ",", "o\u00b7der", "was", "k\u00f6nn\u00b7te", "denn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,", "KON", "PWS", "VMFIN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dich beleidigen, Herz, was", "tokens": ["Dich", "be\u00b7lei\u00b7di\u00b7gen", ",", "Herz", ",", "was"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PWS"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da begegnen, wohin du sollst?", "tokens": ["Da", "be\u00b7geg\u00b7nen", ",", "wo\u00b7hin", "du", "sollst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Denn, seit Himmlischen gleich Menschen, ein einsam Wild,", "tokens": ["Denn", ",", "seit", "Himm\u00b7li\u00b7schen", "gleich", "Men\u00b7schen", ",", "ein", "ein\u00b7sam", "Wild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "ADV", "NN", "$,", "ART", "ADJD", "NE", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und die Himmlischen selbst f\u00fchret, der Einkehr zu,", "tokens": ["Und", "die", "Himm\u00b7li\u00b7schen", "selbst", "f\u00fch\u00b7ret", ",", "der", "Ein\u00b7kehr", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"