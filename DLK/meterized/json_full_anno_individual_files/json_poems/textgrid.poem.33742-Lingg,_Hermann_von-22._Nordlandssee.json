{"textgrid.poem.33742": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "22. Nordlandssee", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Norden liegt ein See, gebirgumschlossen", "tokens": ["Im", "Nor\u00b7den", "liegt", "ein", "See", ",", "ge\u00b7bir\u00b7gum\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fast das ganze Jahr bedeckt vom Eise.", "tokens": ["Und", "fast", "das", "gan\u00b7ze", "Jahr", "be\u00b7deckt", "vom", "Ei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Fr\u00fchling, wenn er kommt, geht hier so leise,", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "wenn", "er", "kommt", ",", "geht", "hier", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df nur die kleinsten Blumen ihm entsprossen.", "tokens": ["Da\u00df", "nur", "die", "kleins\u00b7ten", "Blu\u00b7men", "ihm", "ent\u00b7spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann kommt wohl auch ein Quell vom Berg ergossen,", "tokens": ["Dann", "kommt", "wohl", "auch", "ein", "Quell", "vom", "Berg", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Birke gr\u00fcnt, die leicht beschwingte Meise", "tokens": ["Die", "Bir\u00b7ke", "gr\u00fcnt", ",", "die", "leicht", "be\u00b7schwing\u00b7te", "Mei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Singt im Vor\u00fcberflug auf ihrer Reise,", "tokens": ["Singt", "im", "Vor\u00b7\u00fc\u00b7berf\u00b7lug", "auf", "ih\u00b7rer", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch diese Sommerzeit ist bald verflossen.", "tokens": ["Doch", "die\u00b7se", "Som\u00b7mer\u00b7zeit", "ist", "bald", "ver\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Welle, noch vom Wind gekr\u00e4uselt eben,", "tokens": ["Die", "Wel\u00b7le", ",", "noch", "vom", "Wind", "ge\u00b7kr\u00e4u\u00b7selt", "e\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erstarrt urpl\u00f6tzlich, vom Gestad' verschwindet", "tokens": ["Er\u00b7starrt", "ur\u00b7pl\u00f6tz\u00b7lich", ",", "vom", "Ge\u00b7stad'", "ver\u00b7schwin\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das zarte Gr\u00fcn, die letzte Spur vom Leben.", "tokens": ["Das", "zar\u00b7te", "Gr\u00fcn", ",", "die", "letz\u00b7te", "Spur", "vom", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Ruhe, die nun alles wieder bindet,", "tokens": ["Die", "Ru\u00b7he", ",", "die", "nun", "al\u00b7les", "wie\u00b7der", "bin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist ohne Gl\u00fcck, und keinen Trost mag geben", "tokens": ["Ist", "oh\u00b7ne", "Gl\u00fcck", ",", "und", "kei\u00b7nen", "Trost", "mag", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "$,", "KON", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Einsamkeit, die hier das Herz empfindet.", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", ",", "die", "hier", "das", "Herz", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Im Norden liegt ein See, gebirgumschlossen", "tokens": ["Im", "Nor\u00b7den", "liegt", "ein", "See", ",", "ge\u00b7bir\u00b7gum\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "wor