{"dta.poem.13230": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur als Elisabeth von Kron' und Zepter sprach,", "tokens": ["Nur", "als", "E\u00b7lis\u00b7a\u00b7beth", "von", "Kron'", "und", "Zep\u00b7ter", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NE", "APPR", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lie\u00df die Munterkeit desselben Anblicks nach.", "tokens": ["So", "lie\u00df", "die", "Mun\u00b7ter\u00b7keit", "des\u00b7sel\u00b7ben", "An\u00b7blicks", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich nahme wahr, da\u00df er mit solchen Lobes-Spr\u00fcchen", "tokens": ["Ich", "nah\u00b7me", "wahr", ",", "da\u00df", "er", "mit", "sol\u00b7chen", "Lo\u00b7bes\u00b7Spr\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich weder innerlich noch \u00e4usserlich verglichen.", "tokens": ["Sich", "we\u00b7der", "in\u00b7ner\u00b7lich", "noch", "\u00e4us\u00b7ser\u00b7lich", "ver\u00b7gli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ADJD", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}