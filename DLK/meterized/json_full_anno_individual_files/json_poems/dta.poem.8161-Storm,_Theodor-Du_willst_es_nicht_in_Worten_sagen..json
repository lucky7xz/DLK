{"dta.poem.8161": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Du willst es nicht in Worten sagen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.85", "fr:0.14"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Doch legst du's brennend Mund auf Mund,", "tokens": ["Doch", "legst", "du's", "bren\u00b7nend", "Mund", "auf", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deiner Pulse tiefes Schlagen", "tokens": ["Und", "dei\u00b7ner", "Pul\u00b7se", "tie\u00b7fes", "Schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Thut liebliches Geheimni\u00df kund.", "tokens": ["Thut", "lieb\u00b7li\u00b7ches", "Ge\u00b7heim\u00b7ni\u00df", "kund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du fliehst vor mir, du scheue Taube,", "tokens": ["Du", "fliehst", "vor", "mir", ",", "du", "scheu\u00b7e", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dr\u00fcckst dich fest an meine Brust;", "tokens": ["Und", "dr\u00fcckst", "dich", "fest", "an", "mei\u00b7ne", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist der Liebe schon zum Raube,", "tokens": ["Du", "bist", "der", "Lie\u00b7be", "schon", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bist dir kaum des Worts bewu\u00dft.", "tokens": ["Und", "bist", "dir", "kaum", "des", "Worts", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du biegst den schlanken Leib mir ferne,", "tokens": ["Du", "biegst", "den", "schlan\u00b7ken", "Leib", "mir", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Inde\u00df dein rother Mund mich k\u00fc\u00dft;", "tokens": ["In\u00b7de\u00df", "dein", "ro\u00b7ther", "Mund", "mich", "k\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Behalten m\u00f6chtest du dich gerne,", "tokens": ["Be\u00b7hal\u00b7ten", "m\u00f6ch\u00b7test", "du", "dich", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da du doch ganz verloren bist.", "tokens": ["Da", "du", "doch", "ganz", "ver\u00b7lo\u00b7ren", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du f\u00fchlst, wir k\u00f6nnen nicht verzichten;", "tokens": ["Du", "f\u00fchlst", ",", "wir", "k\u00f6n\u00b7nen", "nicht", "ver\u00b7zich\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum zu geben scheust du noch?", "tokens": ["Wa\u00b7rum", "zu", "ge\u00b7ben", "scheust", "du", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du mu\u00dft die ganze Schuld entrichten,", "tokens": ["Du", "mu\u00dft", "die", "gan\u00b7ze", "Schuld", "ent\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du mu\u00dft, gewi\u00df, du mu\u00dft es doch.", "tokens": ["Du", "mu\u00dft", ",", "ge\u00b7wi\u00df", ",", "du", "mu\u00dft", "es", "doch", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "$,", "PPER", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In Sehnen halb und halb in Bangen,", "tokens": ["In", "Seh\u00b7nen", "halb", "und", "halb", "in", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Ende rinnt die Schaale voll;", "tokens": ["Am", "En\u00b7de", "rinnt", "die", "Schaa\u00b7le", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die holde Schaam ist nur empfangen,", "tokens": ["Die", "hol\u00b7de", "Schaam", "ist", "nur", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie in Liebe sterben soll.", "tokens": ["Da\u00df", "sie", "in", "Lie\u00b7be", "ster\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}