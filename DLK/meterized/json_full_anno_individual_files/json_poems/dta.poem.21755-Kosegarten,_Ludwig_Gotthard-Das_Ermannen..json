{"dta.poem.21755": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Ermannen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ermanne dich, mein Geist! Entraffe dich der", "tokens": ["Er\u00b7man\u00b7ne", "dich", ",", "mein", "Geist", "!", "E\u00b7ntraf\u00b7fe", "dich", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nacht,", "tokens": ["Nacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die bleyern dich bedeckt. Der Schwermuth Trauer-", "tokens": ["Die", "bley\u00b7ern", "dich", "be\u00b7deckt", ".", "Der", "Schwer\u00b7muth", "Trau\u00b7e\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$.", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Besch\u00e4mt das Rosenroth von Gottes sch\u00f6ner Welt.", "tokens": ["Be\u00b7sch\u00e4mt", "das", "Ro\u00b7sen\u00b7roth", "von", "Got\u00b7tes", "sch\u00f6\u00b7ner", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Helden sey ein Held!", "tokens": ["Mit", "Hel\u00b7den", "sey", "ein", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bist gegen Tausend du gleich klein und na-", "tokens": ["Bist", "ge\u00b7gen", "Tau\u00b7send", "du", "gleich", "klein", "und", "na"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "CARD", "PPER", "ADV", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist du vor Tausenden doch herrlich auch und gross,", "tokens": ["Bist", "du", "vor", "Tau\u00b7sen\u00b7den", "doch", "herr\u00b7lich", "auch", "und", "gross", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bist vor Zehntausenden trotz aller Quaal und Noth", "tokens": ["Bist", "vor", "Zehn\u00b7tau\u00b7sen\u00b7den", "trotz", "al\u00b7ler", "Qua\u00b7al", "und", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beg\u00fcnstigt von dem Gott.", "tokens": ["Be\u00b7g\u00fcns\u00b7tigt", "von", "dem", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Spannt deine Muskel nicht noch ungeschw\u00e4ch-", "tokens": ["Spannt", "dei\u00b7ne", "Mus\u00b7kel", "nicht", "noch", "un\u00b7ge\u00b7schw\u00e4ch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schwellt deine Adern nicht die Woge Leiden-", "tokens": ["Schwellt", "dei\u00b7ne", "A\u00b7dern", "nicht", "die", "Wo\u00b7ge", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Giebt dem Begeisterten der Sch\u00f6nheit Genius", "tokens": ["Giebt", "dem", "Be\u00b7geis\u00b7ter\u00b7ten", "der", "Sch\u00f6n\u00b7heit", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NE"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nicht manchen Liebeskuss?", "tokens": ["Nicht", "man\u00b7chen", "Lie\u00b7bes\u00b7kuss", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Liegt offen nicht vor dir des Wissens Blumen-", "tokens": ["Liegt", "of\u00b7fen", "nicht", "vor", "dir", "des", "Wis\u00b7sens", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKNEG", "APPR", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dr\u00fcckt an ihr Herz dich nicht erbarmend die", "tokens": ["Dr\u00fcckt", "an", "ihr", "Herz", "dich", "nicht", "er\u00b7bar\u00b7mend", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PTKNEG", "VVPP", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Natur?", "tokens": ["Na\u00b7tur", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Nimmt dich die Freundschaft nicht in jedem Le-", "tokens": ["Nimmt", "dich", "die", "Freund\u00b7schaft", "nicht", "in", "je\u00b7dem", "Le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sanfttr\u00f6stend in den Arm?", "tokens": ["Sanft\u00b7tr\u00f6s\u00b7tend", "in", "den", "Arm", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ward nicht zur Letze dir das s\u00fcsse Lied ver-", "tokens": ["Ward", "nicht", "zur", "Let\u00b7ze", "dir", "das", "s\u00fcs\u00b7se", "Lied", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPRART", "NN", "PPER", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Braust nicht dein Hochgesang daher gewitterk\u00fchn?", "tokens": ["Braust", "nicht", "dein", "Hoch\u00b7ge\u00b7sang", "da\u00b7her", "ge\u00b7wit\u00b7ter\u00b7k\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schmelzt nicht dein leisres Lied, das warm vom", "tokens": ["Schmelzt", "nicht", "dein", "leis\u00b7res", "Lied", ",", "das", "warm", "vom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herzen kam,", "tokens": ["Her\u00b7zen", "kam", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Das Herz in s\u00fcssen Gram?", "tokens": ["Das", "Herz", "in", "s\u00fcs\u00b7sen", "Gram", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gelang im Dunkeln dir nicht manche bessre", "tokens": ["Ge\u00b7lang", "im", "Dun\u00b7keln", "dir", "nicht", "man\u00b7che", "bess\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "PPER", "PTKNEG", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "That,", "tokens": ["That", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die keine Zeugen hier, die Zeugen droben hat?", "tokens": ["Die", "kei\u00b7ne", "Zeu\u00b7gen", "hier", ",", "die", "Zeu\u00b7gen", "dro\u00b7ben", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "$,", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hast du der ernsten Pflicht, die kalt der Neigung", "tokens": ["Hast", "du", "der", "erns\u00b7ten", "Pflicht", ",", "die", "kalt", "der", "Nei\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "lacht,", "tokens": ["lacht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Nicht Opfer gnug gebracht?", "tokens": ["Nicht", "Op\u00b7fer", "gnug", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sind dir nicht nah und fern die Guten hold", "tokens": ["Sind", "dir", "nicht", "nah", "und", "fern", "die", "Gu\u00b7ten", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und freund?", "tokens": ["und", "freund", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Schm\u00e4ht dich der P\u00f6bel nicht? Sind Schurken dir", "tokens": ["Schm\u00e4ht", "dich", "der", "P\u00f6\u00b7bel", "nicht", "?", "Sind", "Schur\u00b7ken", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$.", "VAFIN", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "nicht feind?", "tokens": ["nicht", "feind", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und schliesst nicht manches Herz, das nirgends", "tokens": ["Und", "schliesst", "nicht", "man\u00b7ches", "Herz", ",", "das", "nir\u00b7gends"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "PIAT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "halten kann,", "tokens": ["hal\u00b7ten", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sich liebend an dich an?", "tokens": ["Sich", "lie\u00b7bend", "an", "dich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ermanne dich, mein Geist! Entraffe dich der", "tokens": ["Er\u00b7man\u00b7ne", "dich", ",", "mein", "Geist", "!", "E\u00b7ntraf\u00b7fe", "dich", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nacht,", "tokens": ["Nacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die bleyern auf dir dr\u00fcckt. Der Schwermuth", "tokens": ["Die", "bley\u00b7ern", "auf", "dir", "dr\u00fcckt", ".", "Der", "Schwer\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trauertracht", "tokens": ["Trau\u00b7er\u00b7tracht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Lass jenem, dem der Born der Hoffnung gar ver-", "tokens": ["Lass", "je\u00b7nem", ",", "dem", "der", "Born", "der", "Hoff\u00b7nung", "gar", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PDAT", "$,", "PRELS", "ART", "NN", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du aber, sey ein Mann!", "tokens": ["Du", "a\u00b7ber", ",", "sey", "ein", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nicht w\u00fcrdig deines Grams ist diese Spanne Zeit.", "tokens": ["Nicht", "w\u00fcr\u00b7dig", "dei\u00b7nes", "Grams", "ist", "die\u00b7se", "Span\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht deines Schmachtens werth ist die Verg\u00e4ng-", "tokens": ["Nicht", "dei\u00b7nes", "Schmach\u00b7tens", "werth", "ist", "die", "Ver\u00b7g\u00e4ng"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADJD", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wen Erdenlust entz\u00fcckt, wen Erdengram verzehrt,", "tokens": ["Wen", "Er\u00b7den\u00b7lust", "ent\u00b7z\u00fcckt", ",", "wen", "Er\u00b7den\u00b7gram", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "PWS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist Hohngel\u00e4chter werth!", "tokens": ["Ist", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter", "werth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Im Strom der Jahre schmilzt des Busens hoher", "tokens": ["Im", "Strom", "der", "Jah\u00b7re", "schmilzt", "des", "Bu\u00b7sens", "ho\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schnee;", "tokens": ["Schnee", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Zu gelbem Krokos welkt des Halses Lilie;", "tokens": ["Zu", "gel\u00b7bem", "Kro\u00b7kos", "welkt", "des", "Hal\u00b7ses", "Li\u00b7lie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Der Lippen Rosenkelch wird ein verschrumpftes", "tokens": ["Der", "Lip\u00b7pen", "Ro\u00b7sen\u00b7kelch", "wird", "ein", "ver\u00b7schrumpf\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Blatt;", "tokens": ["Blatt", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Des Auges Blitz wird matt.", "tokens": ["Des", "Au\u00b7ges", "Blitz", "wird", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Erde Ruhm ist Hauch, der durch die Luft", "tokens": ["Der", "Er\u00b7de", "Ruhm", "ist", "Hauch", ",", "der", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verwallt;", "tokens": ["ver\u00b7wallt", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der Erde Freundschaft Schall, der hohlem Fass'", "tokens": ["Der", "Er\u00b7de", "Freund\u00b7schaft", "Schall", ",", "der", "hoh\u00b7lem", "Fass'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "enthallt;", "tokens": ["ent\u00b7hallt", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Der Erde Ewigkeit w\u00e4hrt Wendung einer Hand;", "tokens": ["Der", "Er\u00b7de", "E\u00b7wig\u00b7keit", "w\u00e4hrt", "Wen\u00b7dung", "ei\u00b7ner", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "N