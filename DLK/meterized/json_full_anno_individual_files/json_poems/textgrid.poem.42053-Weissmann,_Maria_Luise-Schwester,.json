{"textgrid.poem.42053": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Schwester,", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer sind die dunkeln Abenteuer", "tokens": ["Im\u00b7mer", "sind", "die", "dun\u00b7keln", "A\u00b7bent\u00b7eu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zwischen uns, wir k\u00f6nnen oft", "tokens": ["Zwi\u00b7schen", "uns", ",", "wir", "k\u00f6n\u00b7nen", "oft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PPER", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keines der vielen blauen Worte finden,", "tokens": ["Kei\u00b7nes", "der", "vie\u00b7len", "blau\u00b7en", "Wor\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die uns geschenkt sind.", "tokens": ["Die", "uns", "ge\u00b7schenkt", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Dann, wenn ich die schmalen Krystalle", "tokens": ["Dann", ",", "wenn", "ich", "die", "schma\u00b7len", "Krys\u00b7tal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meines wei\u00dfen Traumes Dir bringe,", "tokens": ["Mei\u00b7nes", "wei\u00b7\u00dfen", "Trau\u00b7mes", "Dir", "brin\u00b7g