{"dta.poem.10601": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma  \n  An die Nacht vnd das Gestirn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dv schwartze Nacht/ die du die Welt vmbfangen", "tokens": ["Dv", "schwart\u00b7ze", "Nacht", "/", "die", "du", "die", "Welt", "vmb\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast vberal mit Forcht vnd Dunckelheit/", "tokens": ["Hast", "vbe\u00b7ral", "mit", "Forcht", "vnd", "Dun\u00b7ckel\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schemstu dich nicht/ wann jhre rote Wangen", "tokens": ["Schems\u00b7tu", "dich", "nicht", "/", "wann", "jhre", "ro\u00b7te", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mein Augentrost lest sehen weit vnd breit?", "tokens": ["Mein", "Au\u00b7gen\u00b7trost", "lest", "se\u00b7hen", "weit", "vnd", "breit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Sternen auch d\u00f6rfft jhr von oben schawen/", "tokens": ["Ihr", "Ster\u00b7nen", "auch", "d\u00f6rfft", "jhr", "von", "o\u00b7ben", "scha\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Vnd lenger stehn/ da\u00df jhr euch nicht verwendt/", "tokens": ["Vnd", "len\u00b7ger", "stehn", "/", "da\u00df", "jhr", "euch", "nicht", "ver\u00b7wendt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$(", "KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wann jhr das Liecht der sch\u00f6nesten Jungfrawen/", "tokens": ["Wann", "jhr", "das", "Liecht", "der", "sch\u00f6\u00b7nes\u00b7ten", "Jung\u00b7fra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So bi\u00df zu euch in Himmel reicht/ erkent?", "tokens": ["So", "bi\u00df", "zu", "euch", "in", "Him\u00b7mel", "reicht", "/", "er\u00b7kent", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "APPR", "NN", "VVFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie m\u00f6get jhr nicht also bald verbleichen/", "tokens": ["Wie", "m\u00f6\u00b7get", "jhr", "nicht", "al\u00b7so", "bald", "ver\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn jhr Gesicht als eine Rose bl\u00fcht?", "tokens": ["Wenn", "jhr", "Ge\u00b7sicht", "als", "ei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Aurora selbst die pfleget jhr zu weichen/", "tokens": ["Au\u00b7ro\u00b7ra", "selbst", "die", "pfle\u00b7get", "jhr", "zu", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So da\u00df sie auch f\u00fcr Scham blutroth au\u00dfsieht.", "tokens": ["So", "da\u00df", "sie", "auch", "f\u00fcr", "Scham", "blut\u00b7roth", "au\u00df\u00b7sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}