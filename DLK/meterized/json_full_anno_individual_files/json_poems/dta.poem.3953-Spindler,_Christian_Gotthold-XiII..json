{"dta.poem.3953": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Da Herr N. die Jungfer N. als die dritte", "tokens": ["Da", "Herr", "N.", "die", "Jung\u00b7fer", "N.", "als", "die", "drit\u00b7te"], "token_info": ["word", "word", "abbreviation", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["KOUS", "NN", "NE", "ART", "NN", "NE", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Braut gek\u00fc\u00dft,", "tokens": ["Braut", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Zeigte eine treue Feder, da\u00df die Liebe redlich ist.", "tokens": ["Zeig\u00b7te", "ei\u00b7ne", "treu\u00b7e", "Fe\u00b7der", ",", "da\u00df", "die", "Lie\u00b7be", "red\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dieses Blat wird deutlich weisen, wie ich ohne", "tokens": ["Die\u00b7ses", "Blat", "wird", "deut\u00b7lich", "wei\u00b7sen", ",", "wie", "ich", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "VVINF", "$,", "PWAV", "PPER", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Heucheley", "tokens": ["Heu\u00b7che\u00b7ley"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Diesem Paar der Neu-Verlobten eigen stets ver-", "tokens": ["Die\u00b7sem", "Paar", "der", "Neu\u00b7Ver\u00b7lob\u00b7ten", "ei\u00b7gen", "stets", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN", "ADJD", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "bunden sey.", "tokens": ["bun\u00b7den", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}