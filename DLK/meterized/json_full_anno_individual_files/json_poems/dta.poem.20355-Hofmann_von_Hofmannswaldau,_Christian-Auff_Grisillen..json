{"dta.poem.20355": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff Grisillen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grisillens tochter kam zu ihren reiffen jahren/", "tokens": ["Gri\u00b7sil\u00b7lens", "toch\u00b7ter", "kam", "zu", "ih\u00b7ren", "reif\u00b7fen", "jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcnschte hertzlich sich mit einem mann zu paaren/", "tokens": ["Und", "w\u00fcnschte", "hertz\u00b7lich", "sich", "mit", "ei\u00b7nem", "mann", "zu", "paa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die freundschafft war alsbald auff guten rath bedacht.", "tokens": ["Die", "freund\u00b7schafft", "war", "als\u00b7bald", "auff", "gu\u00b7ten", "rath", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eilff schneider sassen da zusammen wie die ziegen/", "tokens": ["Eilff", "schnei\u00b7der", "sas\u00b7sen", "da", "zu\u00b7sam\u00b7men", "wie", "die", "zie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "ADV", "ADV", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wolten mit gewalt den ausspruch so verf\u00fcgen/", "tokens": ["Und", "wol\u00b7ten", "mit", "ge\u00b7walt", "den", "aus\u00b7spruch", "so", "ver\u00b7f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ihr ein schneider w\u00fcrd zum manne zugebracht.", "tokens": ["Da\u00df", "ihr", "ein", "schnei\u00b7der", "w\u00fcrd", "zum", "man\u00b7ne", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie waren gantz verpicht auff ihren ernsten willen/", "tokens": ["Sie", "wa\u00b7ren", "gantz", "ver\u00b7picht", "auff", "ih\u00b7ren", "erns\u00b7ten", "wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und suchten mit gewalt das dutzend auszuf\u00fcllen.", "tokens": ["Und", "such\u00b7ten", "mit", "ge\u00b7walt", "das", "dut\u00b7zend", "aus\u00b7zu\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur einer fehlte noch/ das solt ein schneider seyn;", "tokens": ["Nur", "ei\u00b7ner", "fehl\u00b7te", "noch", "/", "das", "solt", "ein", "schnei\u00b7der", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "$(", "PDS", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Allein es war umsonst/ des himmels schlu\u00df sprach: nein.", "tokens": ["Al\u00b7lein", "es", "war", "um\u00b7sonst", "/", "des", "him\u00b7mels", "schlu\u00df", "sprach", ":", "nein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$(", "ART", "NN", "NN", "VVFIN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als man das dutzend nun nicht konte voll bekommen/", "tokens": ["Als", "man", "das", "dut\u00b7zend", "nun", "nicht", "kon\u00b7te", "voll", "be\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJD", "ADV", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat an des schneiders statt man einen bock", "tokens": ["Hat", "an", "des", "schnei\u00b7ders", "statt", "man", "ei\u00b7nen", "bock"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "APPR", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}