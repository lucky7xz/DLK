{"dta.poem.12703": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines kauffmanns.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der tod kam vor dem marckt, dem zahlt ich seinen rest,", "tokens": ["Der", "tod", "kam", "vor", "dem", "marckt", ",", "dem", "zahlt", "ich", "sei\u00b7nen", "rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nach der messe wird die letzte schuld auf erden", "tokens": ["Und", "nach", "der", "mes\u00b7se", "wird", "die", "letz\u00b7te", "schuld", "auf", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch einen trauer-gang mir abgestattet werden:", "tokens": ["Durch", "ei\u00b7nen", "trau\u00b7e\u00b7rgang", "mir", "ab\u00b7ge\u00b7stat\u00b7tet", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wohl! da\u00df der himmel mich den wechsel ziehen l\u00e4st.", "tokens": ["Wohl", "!", "da\u00df", "der", "him\u00b7mel", "mich", "den", "wech\u00b7sel", "zie\u00b7hen", "l\u00e4st", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "ART", "NN",