{"textgrid.poem.63519": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sauset und es brauset,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sauset und es brauset,", "tokens": ["Es", "sau\u00b7set", "und", "es", "brau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es geht ein k\u00fchler Wind.", "tokens": ["Es", "geht", "ein", "k\u00fch\u00b7ler", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da drunten auf der Heide,", "tokens": ["Da", "drun\u00b7ten", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da steht ein sch\u00f6nes Kind.", "tokens": ["Da", "steht", "ein", "sch\u00f6\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit ihren wei\u00dfen Armen", "tokens": ["Mit", "ih\u00b7ren", "wei\u00b7\u00dfen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie winkt mir Gottwillkomm,", "tokens": ["Sie", "winkt", "mir", "Gott\u00b7will\u00b7komm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihren schwarzen Augen", "tokens": ["Mit", "ih\u00b7ren", "schwar\u00b7zen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie lacht mich an so fromm.", "tokens": ["Sie", "lacht", "mich", "an", "so", "fromm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dein Winken und dein Gr\u00fc\u00dfen,", "tokens": ["Dein", "Win\u00b7ken", "und", "dein", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach Sch\u00e4tzlein, hilft dir nicht:", "tokens": ["Ach", "Sch\u00e4tz\u00b7lein", ",", "hilft", "dir", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df zur Welt 'nein fahren,", "tokens": ["Ich", "mu\u00df", "zur", "Welt", "'n\u00b7ein", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "NE", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein vogelfreier Wicht.", "tokens": ["Ein", "vo\u00b7gel\u00b7frei\u00b7er", "Wicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und willst einen Liebsten haben,", "tokens": ["Und", "willst", "ei\u00b7nen", "Liebs\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter":