{"textgrid.poem.42852": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Enge K\u00fcnstlerschaft", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie wissen alle was, was sie nicht sagen,", "tokens": ["Sie", "wis\u00b7sen", "al\u00b7le", "was", ",", "was", "sie", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PWS", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sie nach ihrer Meinung vorw\u00e4rtstrug.", "tokens": ["Was", "sie", "nach", "ih\u00b7rer", "Mei\u00b7nung", "vor\u00b7w\u00e4rts\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie n\u00fctzen ihren engen Weg und wagen", "tokens": ["Sie", "n\u00fct\u00b7zen", "ih\u00b7ren", "en\u00b7gen", "Weg", "und", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht, wissen nichts vom freien Flug.", "tokens": ["Nicht", ",", "wis\u00b7sen", "nichts", "vom", "frei\u00b7en", "Flug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "VVFIN", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als w\u00e4r nicht Raum genug in Welt und Leben,", "tokens": ["Als", "w\u00e4r", "nicht", "Raum", "ge\u00b7nug", "in", "Welt", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PTKNEG", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo alle echten Menschen K\u00fcnstler sind.", "tokens": ["Wo", "al\u00b7le", "ech\u00b7ten", "Men\u00b7schen", "K\u00fcnst\u00b7ler", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4re doch mit dem kontr\u00e4ren Wind", "tokens": ["Und", "w\u00e4\u00b7re", "doch", "mit", "dem", "kont\u00b7r\u00e4\u00b7ren", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jedem ganz unersch\u00f6pflich viel gegeben.", "tokens": ["Je\u00b7dem", "ganz", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "viel", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr Lachen schwitzt, ihr St\u00fcrmen ist ein Schleichen.", "tokens": ["Ihr", "La\u00b7chen", "schwitzt", ",", "ihr", "St\u00fcr\u00b7men", "ist", "ein", "Schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Untereinander hocken sie vertraut", "tokens": ["Un\u00b7ter\u00b7ein\u00b7an\u00b7der", "ho\u00b7cken", "sie", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und tuscheln gegen Au\u00dfenseiter laut,", "tokens": ["Und", "tu\u00b7scheln", "ge\u00b7gen", "Au\u00b7\u00dfen\u00b7sei\u00b7ter", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Derweil sie selber giftig sich vergleichen.", "tokens": ["Der\u00b7weil", "sie", "sel\u00b7ber", "gif\u00b7tig", "sich", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kristallisiert zum legitimen Gr\u00fcppchen,", "tokens": ["Kris\u00b7tal\u00b7li\u00b7siert", "zum", "le\u00b7gi\u00b7ti\u00b7men", "Gr\u00fcpp\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird ihr Charakter pl\u00f6tzlich fest bestimmt.", "tokens": ["Wird", "ihr", "Cha\u00b7rak\u00b7ter", "pl\u00f6tz\u00b7lich", "fest", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von den Idealen bleibt ein S\u00fcppchen,", "tokens": ["Von", "den", "I\u00b7dea\u00b7len", "bleibt", "ein", "S\u00fcpp\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darin ein Titel oder Goldnes schwimmt.", "tokens": ["Da\u00b7rin", "ein", "Ti\u00b7tel", "o\u00b7der", "Gold\u00b7nes", "schwimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie pochen all auf was, was gar nicht klingt,", "tokens": ["Sie", "po\u00b7chen", "all", "auf", "was", ",", "was", "gar", "nicht", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPR", "PRELS", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Obwohl es hohl ist. Dennoch nehmen", "tokens": ["Ob\u00b7wohl", "es", "hohl", "ist", ".", "Den\u00b7noch", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$.", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie andre auf, doch wieder nur bedingt,", "tokens": ["Sie", "and\u00b7re", "auf", ",", "doch", "wie\u00b7der", "nur", "be\u00b7dingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKVZ", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Kleinen oder Gro\u00dfen, doch Bequemen.", "tokens": ["Die", "Klei\u00b7nen", "o\u00b7der", "Gro\u00b7\u00dfen", ",", "doch", "Be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und k\u00f6nnte doch f\u00fcr sie und jedermann", "tokens": ["Und", "k\u00f6nn\u00b7te", "doch", "f\u00fcr", "sie", "und", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PPER", "KON", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Alles