{"textgrid.poem.36201": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus tiefem Herzen wollte Liebe dringen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus tiefem Herzen wollte Liebe dringen,", "tokens": ["Aus", "tie\u00b7fem", "Her\u00b7zen", "woll\u00b7te", "Lie\u00b7be", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Gr\u00fcn der Jugend flammte hoch der Mut", "tokens": ["Im", "Gr\u00fcn", "der", "Ju\u00b7gend", "flamm\u00b7te", "hoch", "der", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch lichte Kraft die Sterne zu erringen.", "tokens": ["Durch", "lich\u00b7te", "Kraft", "die", "Ster\u00b7ne", "zu", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Doch brannte bald der Geist in eigner Glut,", "tokens": ["Doch", "brann\u00b7te", "bald", "der", "Geist", "in", "eig\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verachtend wandt' er sich von allen Dingen,", "tokens": ["Ver\u00b7ach\u00b7tend", "wandt'", "er", "sich", "von", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Raub gegeben seiner Sehnsucht Wut.", "tokens": ["Zum", "Raub", "ge\u00b7ge\u00b7ben", "sei\u00b7ner", "Sehn\u00b7sucht", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da klang der dunkeln Tugend Lichtgebot:", "tokens": ["Da", "klang", "der", "dun\u00b7keln", "Tu\u00b7gend", "Licht\u00b7ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Befrei' dich Freier selbst, durch heil'gen Tod.", "tokens": ["Be\u00b7frei'", "dich", "Frei\u00b7er", "selbst", ",", "durch", "heil'\u00b7gen", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kraft dieses Strahls ward ich mir neu gegeben.", "tokens": ["Kraft", "die\u00b7ses", "Strahls", "ward", "ich", "mir", "neu", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "VAFIN", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des Todes Liebe heilt des Lebens Wunde,", "tokens": ["Des", "To\u00b7des", "Lie\u00b7be", "heilt", "des", "Le\u00b7bens", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus der Vernichtung blitzt das h\u00f6chste Leben.", "tokens": ["Aus", "der", "Ver\u00b7nich\u00b7tung", "blitzt", "das", "h\u00f6chs\u00b7te", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die gro\u00dfe Bildung wuchs auf sicherm Grunde;", "tokens": ["Die", "gro\u00b7\u00dfe", "Bil\u00b7dung", "wuchs", "auf", "si\u00b7cherm", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was herrlich war und sein wird, fa\u00dft' im Streben", "tokens": ["Was", "herr\u00b7lich", "war", "und", "sein", "wird", ",", "fa\u00dft'", "im", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "VAINF", "VAFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kunstlieb' und Heldenstolz im festen Bunde.", "tokens": ["Kunst\u00b7lieb'", "und", "Hel\u00b7den\u00b7stolz", "im", "fes\u00b7ten", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Wissenschaften Geist in Einem Bilde", "tokens": ["Der", "Wis\u00b7sen\u00b7schaf\u00b7ten", "Geist", "in", "Ei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erschien dem Zauberrufe sch\u00f6n und milde.", "tokens": ["Er\u00b7schien", "dem", "Zau\u00b7ber\u00b7ru\u00b7fe", "sch\u00f6n", "und", "mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da wird ein Feuer aus den alten Funken.", "tokens": ["Da", "wird", "ein", "Feu\u00b7er", "aus", "den", "al\u00b7ten", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Br\u00fcder, die mich schonend oft ertragen,", "tokens": ["Die", "Br\u00fc\u00b7der", ",", "die", "mich", "scho\u00b7nend", "oft", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn in der Freundschaft Urbild ich versunken,", "tokens": ["Wenn", "in", "der", "Freund\u00b7schaft", "Ur\u00b7bild", "ich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So grenzenlos begehrt' ohn' es zu sagen,", "tokens": ["So", "gren\u00b7zen\u00b7los", "be\u00b7gehrt'", "ohn'", "es", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie sind mit mir von gleicher Liebe trunken;", "tokens": ["Sie", "sind", "mit", "mir", "von", "glei\u00b7cher", "Lie\u00b7be", "trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wir alle hoffen, es soll g\u00f6ttlich tagen.", "tokens": ["Wir", "al\u00b7le", "hof\u00b7fen", ",", "es", "soll", "g\u00f6tt\u00b7lich", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "$,", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zum Scherz belebt den Kreis der Frauen G\u00fcte,", "tokens": ["Zum", "Scherz", "be\u00b7lebt", "den", "Kreis", "der", "Frau\u00b7en", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auch mich erfreut des Witzes zarte Bl\u00fcte.", "tokens": ["Auch", "mich", "er\u00b7freut", "des", "Wit\u00b7zes", "zar\u00b7te", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du warst mir Morgensonne, Heliodora!", "tokens": ["Du", "warst", "mir", "Mor\u00b7gen\u00b7son\u00b7ne", ",", "He\u00b7li\u00b7o\u00b7do\u00b7ra", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus deinem Lichte sog ich neue Glut;", "tokens": ["Aus", "dei\u00b7nem", "Lich\u00b7te", "sog", "ich", "neu\u00b7e", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist mir Lebensquelle, Heliodora!", "tokens": ["Du", "bist", "mir", "Le\u00b7bens\u00b7quel\u00b7le", ",", "He\u00b7li\u00b7o\u00b7do\u00b7ra", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Durch deren Kraft der alte Schmerz nun ruht;", "tokens": ["Durch", "de\u00b7ren", "Kraft", "der", "al\u00b7te", "Schmerz", "nun", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bl\u00fch' auf, du Wunderblume, Heliodora!", "tokens": ["Bl\u00fch'", "auf", ",", "du", "Wun\u00b7der\u00b7blu\u00b7me", ",", "He\u00b7li\u00b7o\u00b7do\u00b7ra", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zur ew'gen Poesie hauch' ew'gen Mut.", "tokens": ["Zur", "ew'\u00b7gen", "Poe\u00b7sie", "hauch'", "ew'\u00b7gen", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ich will nicht l\u00e4nger mit dem Schicksal rechten,", "tokens": ["Ich", "will", "nicht", "l\u00e4n\u00b7ger", "mit", "dem", "Schick\u00b7sal", "rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu sch\u00f6nem Kranz nur sch\u00f6ne Zweige flechten.", "tokens": ["Zu", "sch\u00f6\u00b7nem", "Kranz", "nur", "sch\u00f6\u00b7ne", "Zwei\u00b7ge", "flech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch wollen mit Vernunft wir vorw\u00e4rts schreiten;", "tokens": ["Doch", "wol\u00b7len", "mit", "Ver\u00b7nunft", "wir", "vor\u00b7w\u00e4rts", "schrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstand erkenne, was die Lust begonnen.", "tokens": ["Ver\u00b7stand", "er\u00b7ken\u00b7ne", ",", "was", "die", "Lust", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Klugheit seh' ich selbst die besten gleiten,", "tokens": ["Durch", "Klug\u00b7heit", "seh'", "ich", "selbst", "die", "bes\u00b7ten", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verworrne List ist gar zu bald zerronnen;", "tokens": ["Ver\u00b7worr\u00b7ne", "List", "ist", "gar", "zu", "bald", "zer\u00b7ron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie irren von sich selbst in ferne Weiten", "tokens": ["Sie", "ir\u00b7ren", "von", "sich", "selbst", "in", "fer\u00b7ne", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und haben nichts als ihre M\u00fch' gewonnen.", "tokens": ["Und", "ha\u00b7ben", "nichts", "als", "ih\u00b7re", "M\u00fch'", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "KOKOM", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zeigt Weisheit sich in t\u00f6richtem Gewande,", "tokens": ["Zeigt", "Weis\u00b7heit", "sich", "in", "t\u00f6\u00b7rich\u00b7tem", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "So kommt der Dumme leichtlich von Verstande.", "tokens": ["So", "kommt", "der", "Dum\u00b7me", "leicht\u00b7lich", "von", "Ver\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die schwangre Zukunft rauscht mit m\u00e4cht'gem Fl\u00fcgel,", "tokens": ["Die", "schwang\u00b7re", "Zu\u00b7kunft", "rauscht", "mit", "m\u00e4cht'\u00b7gem", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich \u00f6ffne meiner Lebensbahn die Schranken,", "tokens": ["Ich", "\u00f6ff\u00b7ne", "mei\u00b7ner", "Le\u00b7bens\u00b7bahn", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schau in des klaren Geistes tiefsten Spiegel;", "tokens": ["Schau", "in", "des", "kla\u00b7ren", "Geis\u00b7tes", "tiefs\u00b7ten", "Spie\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Da k\u00e4mpf' ich, Werke bildend sonder Wanken,", "tokens": ["Da", "k\u00e4mpf'", "ich", ",", "Wer\u00b7ke", "bil\u00b7dend", "son\u00b7der", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entrei\u00dfe jeder Wissenschaft das Siegel,", "tokens": ["Ent\u00b7rei\u00b7\u00dfe", "je\u00b7der", "Wis\u00b7sen\u00b7schaft", "das", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verk\u00fcnd'ge Freunden heilige Gedanken,", "tokens": ["Ver\u00b7k\u00fcn\u00b7d'\u00b7ge", "Freun\u00b7den", "hei\u00b7li\u00b7ge", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und stifte allen K\u00fcnsten einen Tempel,", "tokens": ["Und", "stif\u00b7te", "al\u00b7len", "K\u00fcns\u00b7ten", "ei\u00b7nen", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "i