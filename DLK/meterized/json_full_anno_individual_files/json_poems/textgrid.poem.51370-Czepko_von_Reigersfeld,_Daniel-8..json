{"textgrid.poem.51370": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herberg einer sch\u00f6nen Hand,", "tokens": ["Her\u00b7berg", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zarter Finger Wohn Geba\u00fcde,", "tokens": ["Zar\u00b7ter", "Fin\u00b7ger", "Wohn", "Ge\u00b7ba\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnenschild, mein Ehr und Freude,", "tokens": ["Son\u00b7nen\u00b7schild", ",", "mein", "Ehr", "und", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welch ich diese Nacht erkannt.", "tokens": ["Welch", "ich", "die\u00b7se", "Nacht", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Handschuch, weil du diese Nacht", "tokens": ["Hand\u00b7schuch", ",", "weil", "du", "die\u00b7se", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Anfechtung bist gewesen,", "tokens": ["Mein", "An\u00b7fech\u00b7tung", "bist", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VAPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hast du besser zu genesen", "tokens": ["Hast", "du", "bes\u00b7ser", "zu", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In die Heimath dich gemacht.", "tokens": ["In", "die", "Hei\u00b7math", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wann du heimkommst, sprich zu ihr,", "tokens": ["Wann", "du", "heim\u00b7kommst", ",", "sprich", "zu", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fra\u00fclein, nehmt mich an in Gnaden,", "tokens": ["Fra\u00fc\u00b7lein", ",", "nehmt", "mich", "an", "in", "Gna\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieng es euch, w\u00e4r es ohn Schaden,", "tokens": ["Gieng", "es", "euch", ",", "w\u00e4r", "es", "ohn", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Gleich auch diese Nacht, wie mir.", "tokens": ["Gleich", "auch", "die\u00b7se", "Nacht", ",", "wie", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "NN", "$,", "PWAV", "PPER