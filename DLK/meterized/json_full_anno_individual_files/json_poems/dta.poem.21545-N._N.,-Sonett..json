{"dta.poem.21545": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Sonett.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "In meine Kammer f\u00e4llt nur tr\u00fcbes Licht \u2014", "tokens": ["In", "mei\u00b7ne", "Kam\u00b7mer", "f\u00e4llt", "nur", "tr\u00fc\u00b7bes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie lang ist\u2019s her, da\u00df ich des Gl\u00fccks entbehrte \u2014", "tokens": ["Wie", "lang", "ist's", "her", ",", "da\u00df", "ich", "des", "Gl\u00fccks", "ent\u00b7behr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur der Verstand ist mir im Leid Gef\u00e4hrte,", "tokens": ["Nur", "der", "Ver\u00b7stand", "ist", "mir", "im", "Leid", "Ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ewig gr\u00e4mliche und tr\u00fcbe Wicht.", "tokens": ["Der", "e\u00b7wig", "gr\u00e4m\u00b7li\u00b7che", "und", "tr\u00fc\u00b7be", "Wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Stets d\u00fcst\u2019rer wards, es schwand das letzte Licht,", "tokens": ["Stets", "d\u00fcst'\u00b7rer", "wards", ",", "es", "schwand", "das", "letz\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als sich von mir das sapphirblaue, werthe", "tokens": ["Als", "sich", "von", "mir", "das", "sap\u00b7phir\u00b7blau\u00b7e", ",", "wert\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PRF", "APPR", "PPER", "PDS", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Huldauge meiner Jugendliebe kehrte \u2026", "tokens": ["Huld\u00b7au\u00b7ge", "mei\u00b7ner", "Ju\u00b7gend\u00b7lie\u00b7be", "kehr\u00b7te", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dunkel blieb\u2019s, tiefschwarz, und tagte nicht \u2026", "tokens": ["Und", "dun\u00b7kel", "blie\u00b7b's", ",", "tief\u00b7schwarz", ",", "und", "tag\u00b7te", "nicht", "\u2026"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADJD", "$,", "KON", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Nur manchmal tritt in s\u00fc\u00dfem Glorienschein,", "tokens": ["Nur", "manch\u00b7mal", "tritt", "in", "s\u00fc\u00b7\u00dfem", "Glo\u00b7ri\u00b7en\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein wunderlieblich Weib zu mir", "tokens": ["Ein", "wun\u00b7der\u00b7lieb\u00b7lich", "Weib", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein guter Engel ist es, gottgesandt:", "tokens": ["Ein", "gu\u00b7ter", "En\u00b7gel", "ist", "es", ",", "gott\u00b7ge\u00b7sandt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann weicht vom Blick das Bild der Schmerzmeduse,", "tokens": ["Dann", "weicht", "vom", "Blick", "das", "Bild", "der", "Schmerz\u00b7me\u00b7du\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Leis\u2019 legt auf\u2019s Haupt sich eine weiche Hand:", "tokens": ["Leis'", "legt", "auf's", "Haupt", "sich", "ei\u00b7ne", "wei\u00b7che", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann herzt und k\u00fc\u00dft mich liebreich meine Muse . .", "tokens": ["Dann", "herzt", "und", "k\u00fc\u00dft", "mich", "lieb\u00b7reich", "mei\u00b7ne", "Mu\u00b7se", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVPP", "KON", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}