{"dta.poem.4141": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "December.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meld' ich mich dir, und sch\u00fcttle wei\u00dfe Flocken", "tokens": ["Meld'", "ich", "mich", "dir", ",", "und", "sch\u00fctt\u00b7le", "wei\u00b7\u00dfe", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PPER", "PPER", "$,", "KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch alle Stra\u00dfen hin aus meinen Locken:", "tokens": ["Durch", "al\u00b7le", "Stra\u00b7\u00dfen", "hin", "aus", "mei\u00b7nen", "Lo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich, hoff' ich, macht das Ungeth\u00fcm nicht bange.", "tokens": ["Dich", ",", "hoff'", "ich", ",", "macht", "das", "Un\u00b7ge\u00b7th\u00fcm", "nicht", "ban\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es schnaubt der Renner an des Schlittens Stange,", "tokens": ["Es", "schnaubt", "der", "Ren\u00b7ner", "an", "des", "Schlit\u00b7tens", "Stan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das blanke Halsband sch\u00fctteln deine Doggen,", "tokens": ["Das", "blan\u00b7ke", "Hals\u00b7band", "sch\u00fct\u00b7teln", "dei\u00b7ne", "Dog\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Dame h\u00fcllt in warme Flaumensocken", "tokens": ["Die", "Da\u00b7me", "h\u00fcllt", "in", "war\u00b7me", "Flau\u00b7men\u00b7so\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den zarten Fu\u00df, und denkt: Er bleibt so lange.", "tokens": ["Den", "zar\u00b7ten", "Fu\u00df", ",", "und", "denkt", ":", "Er", "bleibt", "so", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was zauderst du? Sitz' auf, mein Freund, geschwinde!", "tokens": ["Was", "zau\u00b7derst", "du", "?", "Sitz'", "auf", ",", "mein", "Freund", ",", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sei mir auf der Fahrt nicht zu verwegen,", "tokens": ["Und", "sei", "mir", "auf", "der", "Fahrt", "nicht", "zu", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df ich im Namen deiner Sch\u00f6nen bitten.", "tokens": ["Mu\u00df", "ich", "im", "Na\u00b7men", "dei\u00b7ner", "Sch\u00f6\u00b7nen", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den s\u00fc\u00dfen, warmen Odem wehn die Winde", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", ",", "war\u00b7men", "O\u00b7dem", "wehn", "die", "Win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und manche weiche Locke dir entgegen:", "tokens": ["Und", "man\u00b7che", "wei\u00b7che", "Lo\u00b7cke", "dir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Halt kurz das Ro\u00df, und sieh auf deinen Schlitten!", "tokens": ["Halt", "kurz", "das", "Ro\u00df", ",", "und", "sieh", "auf", "dei\u00b7nen", "Schlit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,", "KON", "VVIMP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}