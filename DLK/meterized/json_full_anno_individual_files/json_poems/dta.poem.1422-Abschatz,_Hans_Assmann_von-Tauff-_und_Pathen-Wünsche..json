{"dta.poem.1422": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tag Best\u00e4ndigkeit legt dir den gutten Grund", "tokens": ["Der", "Tag", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "legt", "dir", "den", "gut\u00b7ten", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu wahrem Christenthum/ schreibt dich in Lebens-", "tokens": ["Zu", "wah\u00b7rem", "Chris\u00b7ten\u00b7thum", "/", "schreibt", "dich", "in", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "VVFIN", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bund/", "tokens": ["Bund", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wir nehmen di\u00df f\u00fcr dich zur Vorbedeutung an/", "tokens": ["Wir", "neh\u00b7men", "di\u00df", "f\u00fcr", "dich", "zur", "Vor\u00b7be\u00b7deu\u00b7tung", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "APPR", "PPER", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df dir best\u00e4ndig Gl\u00fcck und Heyl nicht mangeln kan.", "tokens": ["Da\u00df", "dir", "be\u00b7st\u00e4n\u00b7dig", "Gl\u00fcck", "und", "Heyl", "nicht", "man\u00b7geln", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "KON", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denck in Best\u00e4ndigkeit an dieses Tages Nahmen/", "tokens": ["Denck", "in", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "an", "die\u00b7ses", "Ta\u00b7ges", "Nah\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "APPR", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So spricht der Himmel selbst zu meinem W\u00fcnschen Amen.", "tokens": ["So", "spricht", "der", "Him\u00b7mel", "selbst", "zu", "mei\u00b7nem", "W\u00fcn\u00b7schen", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}