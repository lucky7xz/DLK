{"textgrid.poem.47623": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nicht W\u00fcrde, Macht und Hoheit]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht W\u00fcrde, Macht und Hoheit,", "tokens": ["Nicht", "W\u00fcr\u00b7de", ",", "Macht", "und", "Ho\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Goldsch\u00e4tze nicht, noch Ruhm und Ehre will ich;", "tokens": ["Gold\u00b7sch\u00e4t\u00b7ze", "nicht", ",", "noch", "Ruhm", "und", "Eh\u00b7re", "will", "ich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ADV", "NN", "KON", "NN", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur einen trauten Busen", "tokens": ["Nur", "ei\u00b7nen", "trau\u00b7ten", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum \u00fcberreichen Heilbescheere will ich.", "tokens": ["Zum", "\u00fc\u00b7berr\u00b7ei\u00b7chen", "Heil\u00b7be\u00b7schee\u00b7re", "will", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In's Paradies der Liebe", "tokens": ["In's", "Pa\u00b7ra\u00b7dies", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Strebt meiner Sehnsucht nie gesenkte Schwinge;", "tokens": ["Strebt"