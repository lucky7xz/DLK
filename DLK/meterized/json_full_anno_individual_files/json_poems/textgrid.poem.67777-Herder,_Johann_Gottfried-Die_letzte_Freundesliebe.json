{"textgrid.poem.67777": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die letzte Freundesliebe", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Jesus nun zum letzten Kampf", "tokens": ["Als", "Je\u00b7sus", "nun", "zum", "letz\u00b7ten", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ging in Gethsemane,", "tokens": ["Ging", "in", "Geth\u00b7se\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auf seiner Stirne brach schon Angst,", "tokens": ["Auf", "sei\u00b7ner", "Stir\u00b7ne", "brach", "schon", "Angst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Herzen Ach und Weh!", "tokens": ["Im", "Her\u00b7zen", "Ach", "und", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum Vater wallt' er sehnend hin,", "tokens": ["Zum", "Va\u00b7ter", "wallt'", "er", "seh\u00b7nend", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu beten und zu flehn;", "tokens": ["Zu", "be\u00b7ten", "und", "zu", "flehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbihr Br\u00fcder,\u00ab sprach er, \u00bbharret hie,", "tokens": ["\u00bb", "ihr", "Br\u00fc\u00b7der", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "har\u00b7ret", "hie", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dort will ich beten gehn.\u00ab", "tokens": ["Dort", "will", "ich", "be\u00b7ten", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drei liebe Freunde nahm er hin.", "tokens": ["Drei", "lie\u00b7be", "Freun\u00b7de", "nahm", "er", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbihr,\u00ab sprach er, \u00bbwachet hier!", "tokens": ["\u00bb", "ihr", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "wa\u00b7chet", "hier", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz ist traurig in den Tod;", "tokens": ["Mein", "Herz", "ist", "trau\u00b7rig", "in", "den", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Br\u00fcder, wacht mit mir!\u00ab", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "wacht", "mit", "mir", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ging hin ein Wenig f\u00fcrder, fiel", "tokens": ["Ging", "hin", "ein", "We\u00b7nig", "f\u00fcr\u00b7der", ",", "fiel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ART", "ADV", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dahin aufs Angesicht", "tokens": ["Da\u00b7hin", "aufs", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PAV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und betete und zitterte;", "tokens": ["Und", "be\u00b7te\u00b7te", "und", "zit\u00b7ter\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Br\u00fcder wachten nicht.", "tokens": ["Die", "Br\u00fc\u00b7der", "wach\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Vater,\u00ab sprach er, \u00bbkann es sein,", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "kann", "es", "sein", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So geh' der Kelch von mir!", "tokens": ["So", "geh'", "der", "Kelch", "von", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht ich, wie Du willst! wie Du willst,", "tokens": ["Nicht", "ich", ",", "wie", "Du", "willst", "!", "wie", "Du", "willst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$.", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Wille folget Dir!\u00ab", "tokens": ["Mein", "Wil\u00b7le", "fol\u00b7get", "Dir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er kam; sie schliefen. \u00bbTraget Ihr", "tokens": ["Er", "kam", ";", "sie", "schlie\u00b7fen", ".", "\u00bb", "Tra\u00b7get", "Ihr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Kreuz mir also nach?", "tokens": ["Mein", "Kreuz", "mir", "al\u00b7so", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach, wacht und betet! Willig ist", "tokens": ["Ach", ",", "wacht", "und", "be\u00b7tet", "!", "Wil\u00b7lig", "ist"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "KON", "VVFIN", "$.", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Geist, das Fleisch ist schwach!\u00ab", "tokens": ["Der", "Geist", ",", "das", "Fleisch", "ist", "schwach", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ging hin zum andern Male, fiel", "tokens": ["Ging", "hin", "zum", "an\u00b7dern", "Ma\u00b7le", ",", "fiel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dahin aufs Angesicht", "tokens": ["Da\u00b7hin", "aufs", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PAV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zagte tiefer, zitterte;", "tokens": ["Und", "zag\u00b7te", "tie\u00b7fer", ",", "zit\u00b7ter\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Br\u00fcder wachten nicht.", "tokens": ["Die", "Br\u00fc\u00b7der", "wach\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbkann nicht, o Vater, kann er nicht,", "tokens": ["\u00bb", "kann", "nicht", ",", "o", "Va\u00b7ter", ",", "kann", "er", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PTKNEG", "$,", "FM", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Kelch, vor\u00fcbergehn?", "tokens": ["Der", "Kelch", ",", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich soll ihn trinken; nun wolan,", "tokens": ["Ich", "soll", "ihn", "trin\u00b7ken", ";", "nun", "wo\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$.", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Wille soll geschehn!\u00ab", "tokens": ["Dein", "Wil\u00b7le", "soll", "ge\u00b7schehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er kam; sie schliefen; ri\u00df sich hin", "tokens": ["Er", "kam", ";", "sie", "schlie\u00b7fen", ";", "ri\u00df", "sich", "hin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "VVFIN", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fleht' zum dritten Mal;", "tokens": ["Und", "fleht'", "zum", "drit\u00b7ten", "Mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Angstschwei\u00df troff von seiner Stirn", "tokens": ["Der", "A\u00b7ngstschwei\u00df", "troff", "von", "sei\u00b7ner", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Tropfen ohne Zahl.", "tokens": ["In", "Trop\u00b7fen", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Engel kam, zu st\u00e4rken ihn;", "tokens": ["Ein", "En\u00b7gel", "kam", ",", "zu", "st\u00e4r\u00b7ken", "ihn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PTKZU", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er betet' \u00e4ngster, rang", "tokens": ["Er", "be\u00b7tet'", "\u00e4ngs\u00b7ter", ",", "rang"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Schon mit dem Tode, da\u00df sein Blut", "tokens": ["Schon", "mit", "dem", "To\u00b7de", ",", "da\u00df", "sein", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Durch alle Adern drang.", "tokens": ["Durch", "al\u00b7le", "A\u00b7dern", "drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Stand auf und suchte seine Drei,", "tokens": ["Stand", "auf", "und", "such\u00b7te", "sei\u00b7ne", "Drei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PPOSAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fand alle schlummern sie.", "tokens": ["Fand", "al\u00b7le", "schlum\u00b7mern", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach, wollt Ihr schlafen nun und ruhn?", "tokens": ["\u00bb", "ach", ",", "wollt", "Ihr", "schla\u00b7fen", "nun", "und", "ruhn", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VMFIN", "PPER", "VVFIN", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stund, die Stund ist hie,", "tokens": ["Die", "Stund", ",", "die", "Stund", "ist", "hie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da 's Menschen Sohn in S\u00fcnderhand", "tokens": ["Da", "'s", "Men\u00b7schen", "Sohn", "in", "S\u00fcn\u00b7der\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "NN", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Zum Tode von Euch geht;", "tokens": ["Zum", "To\u00b7de", "von", "Euch", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wacht auf! steht auf! er ist schon da,", "tokens": ["Wacht", "auf", "!", "steht", "auf", "!", "er", "ist", "schon", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist da, der mich verr\u00e4th.\u00ab", "tokens": ["Ist", "da", ",", "der", "mich", "ver\u00b7r\u00e4\u00b7th", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie nahmen ihn; der J\u00fcnger Hauf'", "tokens": ["Sie", "nah\u00b7men", "ihn", ";", "der", "J\u00fcn\u00b7ger", "Hauf'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerstob von ihm im Nu.", "tokens": ["Zer\u00b7stob", "von", "ihm", "im", "Nu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo warst Du nun, Du liebes Drei?", "tokens": ["Wo", "warst", "Du", "nun", ",", "Du", "lie\u00b7bes", "Drei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Ferne schwindest Du!", "tokens": ["In", "Fer\u00b7ne", "schwin\u00b7dest", "Du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der nimmer ihn verlassen wollt'", "tokens": ["Der", "nim\u00b7mer", "ihn", "ver\u00b7las\u00b7sen", "wollt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor aller J\u00fcnger Zahl,", "tokens": ["Vor", "al\u00b7ler", "J\u00fcn\u00b7ger", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der heute mit ihm sterben wollt',", "tokens": ["Der", "heu\u00b7te", "mit", "ihm", "ster\u00b7ben", "wollt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verleugnet' ihn dreimal.", "tokens": ["Ver\u00b7leug\u00b7net'", "ihn", "drei\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrac