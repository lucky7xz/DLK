 "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der rauhste Tagruf gellender Trompeten:", "tokens": ["Der", "rauhs\u00b7te", "Tag\u00b7ruf", "gel\u00b7len\u00b7der", "Trom\u00b7pe\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie gelten jetzo mehr, o ihr Propheten!", "tokens": ["Sie", "gel\u00b7ten", "jet\u00b7zo", "mehr", ",", "o", "ihr", "Pro\u00b7phe\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als alles, was wir stolz und eitel sangen.", "tokens": ["Als", "al\u00b7les", ",", "was", "wir", "stolz", "und", "ei\u00b7tel", "san\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der letzte schlichte W\u00e4chter vor dem Heere,", "tokens": ["Der", "letz\u00b7te", "schlich\u00b7te", "W\u00e4ch\u00b7ter", "vor", "dem", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der, Glut und Kraft im Herzen, hat getragen", "tokens": ["Der", ",", "Glut", "und", "Kraft", "im", "Her\u00b7zen", ",", "hat", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "NN", "KON", "NN", "A