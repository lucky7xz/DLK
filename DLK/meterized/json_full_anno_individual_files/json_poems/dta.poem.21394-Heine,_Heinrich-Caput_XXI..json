{"dta.poem.21394": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und der Dreckwall, wo ist der Dreckwall hin?", "tokens": ["Und", "der", "Dreck\u00b7wall", ",", "wo", "ist", "der", "Dreck\u00b7wall", "hin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich kann ihn vergeblich suchen!", "tokens": ["Ich", "kann", "ihn", "ver\u00b7geb\u00b7lich", "su\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo ist der Pavillon, wo ich", "tokens": ["Wo", "ist", "der", "Pa\u00b7vil\u00b7lon", ",", "wo", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gegessen so manchen Kuchen?", "tokens": ["Ge\u00b7ges\u00b7sen", "so", "man\u00b7chen", "Ku\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo ist das Rathhaus, worin der Senat", "tokens": ["Wo", "ist", "das", "Rath\u00b7haus", ",", "wo\u00b7rin", "der", "Se\u00b7nat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und die B\u00fcrgerschaft gethronet?", "tokens": ["Und", "die", "B\u00fcr\u00b7ger\u00b7schaft", "ge\u00b7thro\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Raub der Flammen! Die Flamme hat", "tokens": ["Ein", "Raub", "der", "Flam\u00b7men", "!", "Die", "Flam\u00b7me", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Heiligste nicht verschonet.", "tokens": ["Das", "Hei\u00b7ligs\u00b7te", "nicht", "ver\u00b7scho\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Leute seufzten noch vor Angst,", "tokens": ["Die", "Leu\u00b7te", "seufz\u00b7ten", "noch", "vor", "Angst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit wehm\u00fcth\u2019gem Gesichte", "tokens": ["Und", "mit", "weh\u00b7m\u00fcth'\u00b7gem", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erz\u00e4hlten sie mir vom gro\u00dfen Brand", "tokens": ["Er\u00b7z\u00e4hl\u00b7ten", "sie", "mir", "vom", "gro\u00b7\u00dfen", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die schreckliche Geschichte:", "tokens": ["Die", "schreck\u00b7li\u00b7che", "Ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ees brannte an allen Ecken zugleich,", "tokens": ["\u201e", "es", "brann\u00b7te", "an", "al\u00b7len", "E\u00b7cken", "zu\u00b7gleich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Man sah nur Rauch und Flammen!", "tokens": ["Man", "sah", "nur", "Rauch", "und", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kirchenth\u00fcrme loderten auf", "tokens": ["Die", "Kir\u00b7chent\u00b7h\u00fcr\u00b7me", "lo\u00b7der\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und st\u00fcrzten krachend zusammen.", "tokens": ["Und", "st\u00fcrz\u00b7ten", "kra\u00b7chend", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}