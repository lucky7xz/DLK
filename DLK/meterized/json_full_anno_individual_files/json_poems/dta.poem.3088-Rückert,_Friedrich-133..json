{"dta.poem.3088": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "133.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um Neujahr hattest du, wie mir dein B\u00fcchlein sagt,", "tokens": ["Um", "Neu\u00b7jahr", "hat\u00b7test", "du", ",", "wie", "mir", "dein", "B\u00fcch\u00b7lein", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gedanken, die mich auch um jene Zeit geplagt;", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "die", "mich", "auch", "um", "je\u00b7ne", "Zeit", "ge\u00b7plagt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur mit dem Unterschied: du hast daraus ersonnen", "tokens": ["Nur", "mit", "dem", "Un\u00b7ter\u00b7schied", ":", "du", "hast", "da\u00b7raus", "er\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "PAV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Lehrgeb\u00e4ud' und ich nur Lieder draus gesponnen.", "tokens": ["Ein", "Lehr\u00b7ge\u00b7b\u00e4ud'", "und", "ich", "nur", "Lie\u00b7der", "draus", "ge\u00b7spon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "ADV", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun aber find' ich, da\u00df bei dir gar wirr und kraus", "tokens": ["Nun", "a\u00b7ber", "find'", "ich", ",", "da\u00df", "bei", "dir", "gar", "wirr", "und", "kraus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPER", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das aussieht, was bei mir sich nimmt ganz menschlich aus.", "tokens": ["Das", "aus\u00b7sieht", ",", "was", "bei", "mir", "sich", "nimmt", "ganz", "menschlich", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Warum? Du hast umsonst gesucht Zusammenhang", "tokens": ["Wa\u00b7rum", "?", "Du", "hast", "um\u00b7sonst", "ge\u00b7sucht", "Zu\u00b7sam\u00b7men\u00b7hang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "ADV", "VVPP", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Sinns, wo mir gen\u00fcgt des Tons Zusammenklang.", "tokens": ["Des", "Sinns", ",", "wo", "mir", "ge\u00b7n\u00fcgt", "des", "Tons", "Zu\u00b7sam\u00b7men\u00b7klang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}