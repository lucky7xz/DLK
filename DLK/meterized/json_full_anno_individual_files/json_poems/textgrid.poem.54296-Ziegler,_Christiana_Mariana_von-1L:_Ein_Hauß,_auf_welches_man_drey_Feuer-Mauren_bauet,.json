{"textgrid.poem.54296": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Hau\u00df, auf welches man drey Feuer-Mauren bauet,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hau\u00df, auf welches man drey Feuer-Mauren bauet,", "tokens": ["Ein", "Hau\u00df", ",", "auf", "wel\u00b7ches", "man", "drey", "Feu\u00b7er\u00b7Mau\u00b7ren", "bau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PIS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Heist mehr als zu gemein, weil man es t\u00e4glich schauet,", "tokens": ["Heist", "mehr", "als", "zu", "ge\u00b7mein", ",", "weil", "man", "es", "t\u00e4g\u00b7lich", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KOKOM", "PTKA", "ADJD", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wer sein Auge l\u00e4st nach Bauer-Stiefeln gehn,", "tokens": ["Und", "wer", "sein", "Au\u00b7ge", "l\u00e4st", "nach", "Bau\u00b7e\u00b7rS\u00b7tie\u00b7feln", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der sieht an selbgen mehr als eine Falte stehn.", "tokens": ["Der", "sieht", "an", "selb\u00b7gen", "mehr", "als", "ei\u00b7ne", "Fal\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADV", "PIAT", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum wei\u00df ich w\u00fcrklich nicht, woher es doch geschehen,", "tokens": ["Drum", "wei\u00df", "ich", "w\u00fcrk\u00b7lich", "nicht", ",", "wo\u00b7her", "es", "doch", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df wir an einen Kinn drey Unterkehlen sehen.", "tokens": ["Da\u00df", "wir", "an", "ei\u00b7nen", "Kinn", "drey", "Un\u00b7ter\u00b7keh\u00b7len", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wann sonsten die Natur an ihre Werckstatt trit,", "tokens": ["Wann", "sons\u00b7ten", "die", "Na\u00b7tur", "an", "ih\u00b7re", "Wercks\u00b7tatt", "trit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So theilt sie insgemein nicht mehr als eine mit.", "tokens": ["So", "theilt", "sie", "ins\u00b7ge\u00b7mein", "nicht", "mehr", "als", "ei\u00b7ne", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "KOKOM", "ART", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du bist die einge nur, so diesen Schatz besitzet;", "tokens": ["Du", "bist", "die", "ein\u00b7ge", "nur", ",", "so", "die\u00b7sen", "Schatz", "be\u00b7sit\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADV", "$,", "ADV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Warum? weil die Natur dich obenhin geschnitzet,", "tokens": ["Wa\u00b7rum", "?", "weil", "die", "Na\u00b7tur", "dich", "o\u00b7ben\u00b7hin", "ge\u00b7schnit\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und nicht viel artiges aus deinen C\u00f6rper lacht,", "tokens": ["Und", "nicht", "viel", "ar\u00b7ti\u00b7ges", "aus", "dei\u00b7nen", "C\u00f6r\u00b7per", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJA", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat sie in diesen St\u00fcck den Mangel eingebracht.", "tokens": ["Hat", "sie", "in", "die\u00b7sen", "St\u00fcck", "den", "Man\u00b7gel", "ein\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Amando l\u00e4ufft inde\u00df nach dir zu vielen Stunden,", "tokens": ["A\u00b7man\u00b7do", "l\u00e4ufft", "in\u00b7de\u00df", "nach", "dir", "zu", "vie\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Und meynt, er h\u00e4tt an dir die Venus selbst gefunden.", "tokens": ["Und", "meynt", ",", "er", "h\u00e4tt", "an", "dir", "die", "Ve\u00b7nus", "selbst", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was hilfft ihn aber di\u00df, wann er dich \u00e4ngstlich sucht?", "tokens": ["Was", "hilfft", "ihn", "a\u00b7ber", "di\u00df", ",", "wann", "er", "dich", "\u00e4ngst\u00b7lich", "sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PDS", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil ihm verbothen ist der Kern der Liebes-Frucht.", "tokens": ["Weil", "ihm", "ver\u00b7bo\u00b7then", "ist", "der", "Kern", "der", "Lie\u00b7bes\u00b7Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch dein Galan wird nicht nach leeren Schalen rennen,", "tokens": ["Doch", "dein", "Ga\u00b7lan", "wird", "nicht", "nach", "lee\u00b7ren", "Scha\u00b7len", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du wirst ihm, denck ich wol, etwas zu naschen g\u00f6nnen.", "tokens": ["Du", "wirst", "ihm", ",", "denck", "ich", "wol", ",", "et\u00b7was", "zu", "na\u00b7schen", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "$,", "PIS", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er ist, dem Ansehn nach, kein tummer Coridon,", "tokens": ["Er", "ist", ",", "dem", "An\u00b7sehn", "nach", ",", "kein", "tum\u00b7mer", "Co\u00b7ri\u00b7don", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "PTKVZ", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mir ist, als tr\u00fcg er gnug Genu\u00df und Nip\u00df davon.", "tokens": ["Mir", "ist", ",", "als", "tr\u00fcg", "er", "gnug", "Ge\u00b7nu\u00df", "und", "Nip\u00df", "da\u00b7von", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJD", "PPER", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ihr werdet warlich euch darzu nicht leuchten lassen,", "tokens": ["Ihr", "wer\u00b7det", "war\u00b7lich", "euch", "dar\u00b7zu", "nicht", "leuch\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "PAV", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein schlau verliebtes Paar wird Licht und Zeugen hassen;", "tokens": ["Ein", "schlau", "ver\u00b7lieb\u00b7tes", "Paar", "wird", "Licht", "und", "Zeu\u00b7gen", "has\u00b7sen", ";"], "token_info": ["word