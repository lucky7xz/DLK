ord", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch dein Galan wird nicht nach leeren Schalen rennen,", "tokens": ["Doch", "dein", "Ga\u00b7lan", "wird", "nicht", "nach", "lee\u00b7ren", "Scha\u00b7len", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du wirst ihm, denck ich wol, etwas zu naschen g\u00f6nnen.", "tokens": ["Du", "wirst", "ihm", ",", "denck", "ich", "wol", ",", "et\u00b7was", "zu", "na\u00b7schen", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "$,", "PIS", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er ist, dem Ansehn nach, kein tummer Coridon,", "tokens": ["Er", "ist", ",", "dem", "An\u00b7sehn", "nach", ",", "kein", "tum\u00b7mer", "Co\u00b7ri\u00b7don", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "PTKVZ", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mir ist, als tr\u00fcg er gnug Genu\u00df und Nip\u00df davon.", "tokens": ["Mir", "ist", ",", "als", "tr\u00fcg", "er", "gnug", "Ge\u00b7nu\u00df", "und", "Nip\u00df", "da\u00b7von", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJD", "PPER", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ihr werdet warlich euch darzu nicht leuchten lassen,", "tokens": ["Ihr", "wer\u00b7det", "war\u00b7lich", "euch", "dar\u00b7zu", "nicht", "leuch\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "PAV", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein schlau verliebtes Paar wird Licht und Zeugen hassen;", "tokens": ["Ein", "schlau", "ver\u00b7lieb\u00b7tes", "Paar", "wird", "Licht", "und", "Zeu\u00b7gen", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und also macht ihrs auch, weil Eulen insgemein,", "tokens": ["Und", "al\u00b7so", "macht", "ihrs", "auch", ",", "weil", "Eu\u00b7len", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn sie zusammen gehn, im Finstern wollen seyn.", "tokens": ["Wenn", "sie", "zu\u00b7sam\u00b7men", "gehn", ",", "im", "Fins\u00b7tern", "wol\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$,", "APPRART", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}