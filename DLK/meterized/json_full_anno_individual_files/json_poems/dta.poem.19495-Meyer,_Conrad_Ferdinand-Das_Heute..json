{"dta.poem.19495": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Heute.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Das Heut ist einem jungen Weibe gleich.", "tokens": ["Das", "Heut", "ist", "ei\u00b7nem", "jun\u00b7gen", "Wei\u00b7be", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schlag Mitternacht wird ihm die Wange bleich.", "tokens": ["Schlag", "Mit\u00b7ter\u00b7nacht", "wird", "ihm", "die", "Wan\u00b7ge", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es schaudert. Einen vollen Becher fa\u00dft", "tokens": ["Es", "schau\u00b7dert", ".", "Ei\u00b7nen", "vol\u00b7len", "Be\u00b7cher", "fa\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es gierig noch und schl\u00fcrft in toller Hast.", "tokens": ["Es", "gie\u00b7rig", "noch", "und", "schl\u00fcrft", "in", "tol\u00b7ler", "Hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der \u00fcpp'ge Mund, indem er lechzt und trinkt,", "tokens": ["Der", "\u00fcpp'\u00b7ge", "Mund", ",", "in\u00b7dem", "er", "lechzt", "und", "trinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Entf\u00e4rbt sich und verwelkt. Der Becher sinkt.", "tokens": ["Ent\u00b7f\u00e4rbt", "sich", "und", "ver\u00b7welkt", ".", "Der", "Be\u00b7cher", "sinkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVPP", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Langsam zieht es den Kranz sich aus dem Haar.", "tokens": ["Lang\u00b7sam", "zieht", "es", "den", "Kranz", "sich", "aus", "dem", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Das Haar ergraut, das eben braun noch war.", "tokens": ["Das", "Haar", "er\u00b7graut", ",", "das", "e\u00b7ben", "braun", "noch", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ADV", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Tiefrunzelt sich das sch\u00f6ne schuld'ge Haupt.", "tokens": ["Tie\u00b7frun\u00b7zelt", "sich", "das", "sch\u00f6\u00b7ne", "schuld'\u00b7ge", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "Zusammenbricht das Knie, der Kraft beraubt.", "tokens": ["Zu\u00b7sam\u00b7men\u00b7bricht", "das", "Knie", ",", "der", "Kraft", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Horen kleiden dicht in Schleier ein", "tokens": ["Die", "Ho\u00b7ren", "klei\u00b7den", "dicht", "in", "Schlei\u00b7er", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und f\u00fchren weg ein greises M\u00fctterlein.", "tokens": ["Und", "f\u00fch\u00b7ren", "weg", "ein", "grei\u00b7ses", "M\u00fct\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}