{"textgrid.poem.47064": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Blumen, die ihr, weil der Winter schauert,", "tokens": ["O", "Blu\u00b7men", ",", "die", "ihr", ",", "weil", "der", "Win\u00b7ter", "schau\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schnee auf der Au und Eis liegt auf dem Bronne,", "tokens": ["Schnee", "auf", "der", "Au", "und", "Eis", "liegt", "auf", "dem", "Bron\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "An eines Ofens W\u00e4rm' anstatt der Sonne", "tokens": ["An", "ei\u00b7nes", "O\u00b7fens", "W\u00e4rm'", "an\u00b7statt", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Euch m\u00fc\u00dft erschlie\u00dfen, o wie ihr mich dauert;", "tokens": ["Euch", "m\u00fc\u00dft", "er\u00b7schlie\u00b7\u00dfen", ",", "o", "wie", "ihr", "mich", "dau\u00b7ert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "FM", "KON", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die ihr vergebens auf Erl\u00f6sung lauert,", 