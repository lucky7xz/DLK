{"dta.poem.7581": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von beiden Welten wenn ich sollt' entbehren eine,", "tokens": ["Von", "bei\u00b7den", "Wel\u00b7ten", "wenn", "ich", "sollt'", "ent\u00b7beh\u00b7ren", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KOUS", "PPER", "VMFIN", "VVINF", "ART", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gro\u00dfe drau\u00dfen w\u00e4rs, und nicht in mir die kleine.", "tokens": ["Die", "gro\u00b7\u00dfe", "drau\u00b7\u00dfen", "w\u00e4rs", ",", "und", "nicht", "in", "mir", "die", "klei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "$,", "KON", "PTKNEG", "APPR", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du wirst die Welt in dir nicht mehr die kleine nennen,", "tokens": ["Du", "wirst", "die", "Welt", "in", "dir", "nicht", "mehr", "die", "klei\u00b7ne", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du das g\u00f6ttliche im Menschen wirst erkennen.", "tokens": ["Wenn", "du", "das", "g\u00f6tt\u00b7li\u00b7che", "im", "Men\u00b7schen", "wirst", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "APPRART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Klein ist und eng, was Zeit und Raum nennt seine Schranke,", "tokens": ["Klein", "ist", "und", "eng", ",", "was", "Zeit", "und", "Raum", "nennt", "sei\u00b7ne", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KON", "ADJD", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur g\u00f6ttlich weit ist ein Gottfassender Gedanke.", "tokens": ["Nur", "g\u00f6tt\u00b7lich", "weit", "ist", "ein", "Gott\u00b7fas\u00b7sen\u00b7der", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Viel leichter macht der Mensch von jedem ird'schen Band", "tokens": ["Viel", "leich\u00b7ter", "macht", "der", "Mensch", "von", "je\u00b7dem", "ird'\u00b7schen", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich los als von dem Zug nach einem h\u00f6hern Land.", "tokens": ["Sich", "los", "als", "von", "dem", "Zug", "nach", "ei\u00b7nem", "h\u00f6\u00b7hern", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KOKOM", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Sinnennahrung kannst du selber ehr entbehren,", "tokens": ["Der", "Sin\u00b7nen\u00b7nah\u00b7rung", "kannst", "du", "sel\u00b7ber", "ehr", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als des Gedankens, der den Gott in dir mu\u00df n\u00e4hren.", "tokens": ["Als", "des", "Ge\u00b7dan\u00b7kens", ",", "der", "den", "Gott", "in", "dir", "mu\u00df", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Begl\u00fcckt, wenn dir ein Hauch der Fantasie mit Kunst", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wenn", "dir", "ein", "Hauch", "der", "Fan\u00b7ta\u00b7sie", "mit", "Kunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die beiden Welten schmelzt in Eine rein von Dunst.", "tokens": ["Die", "bei\u00b7den", "Wel\u00b7ten", "schmelzt", "in", "Ei\u00b7ne", "rein", "von", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "ART", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das Unsichtbare siehst du klar im Sichtbarn nur,", "tokens": ["Das", "Un\u00b7sicht\u00b7ba\u00b7re", "siehst", "du", "klar", "im", "Sicht\u00b7barn", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nichts im Sichtbarn als des Unsichtbaren Spur.", "tokens": ["Und", "nichts", "im", "Sicht\u00b7barn", "als", "des", "Un\u00b7sicht\u00b7ba\u00b7ren", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "NN", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Umringt von einer Welt verk\u00f6rperter Gedanken,", "tokens": ["Um\u00b7ringt", "von", "ei\u00b7ner", "Welt", "ver\u00b7k\u00f6r\u00b7per\u00b7ter", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empfindest schrankenlos du dich in K\u00f6rperschranken.", "tokens": ["Emp\u00b7fin\u00b7dest", "schran\u00b7ken\u00b7los", "du", "dich", "in", "K\u00f6r\u00b7per\u00b7schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}