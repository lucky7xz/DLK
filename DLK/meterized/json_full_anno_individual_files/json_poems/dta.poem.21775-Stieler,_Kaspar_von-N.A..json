{"dta.poem.21775": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Treues Kleebat dreyer Hirten/", "tokens": ["Treu\u00b7es", "Klee\u00b7bat", "drey\u00b7er", "Hir\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die der Zesyr au\u00dfgesezzt/", "tokens": ["die", "der", "Ze\u00b7syr", "au\u00df\u00b7ge\u00b7sezzt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die kein Nordwind ferner hezzt", "tokens": ["die", "kein", "Nord\u00b7wind", "fer\u00b7ner", "hezzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "an des Amors falsche Syrten.", "tokens": ["an", "des", "A\u00b7mors", "fal\u00b7sche", "Syr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden meine Venus-Grillen/", "tokens": ["Wer\u00b7den", "mei\u00b7ne", "Ve\u00b7nus\u00b7Gril\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "meiner Liebe Wiederwillen/", "tokens": ["mei\u00b7ner", "Lie\u00b7be", "Wie\u00b7der\u00b7wil\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "welch\u2019 ihr in der Ruh verlacht", "tokens": ["welch'", "ihr", "in", "der", "Ruh", "ver\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "PPOSAT", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "auch bey Euch was sein geacht?", "tokens": ["auch", "bey", "Euch", "was", "sein", "ge\u00b7acht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PIS", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was? geachtt? ich mu\u00df ja schreiben", "tokens": ["Was", "?", "ge\u00b7achtt", "?", "ich", "mu\u00df", "ja", "schrei\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "$.", "VVPP", "$.", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was die k\u00fchne Feder will", "tokens": ["was", "die", "k\u00fch\u00b7ne", "Fe\u00b7der", "will"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "besser was/ als in der Still\u2019", "tokens": ["bes\u00b7ser", "was", "/", "als", "in", "der", "Still'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PWS", "$(", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "allzeit um Vakunen bleiben.", "tokens": ["all\u00b7zeit", "um", "Va\u00b7ku\u00b7nen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Wenn die donnernde Melpose", "tokens": ["Wenn", "die", "don\u00b7nern\u00b7de", "Mel\u00b7po\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "treibt au\u00df ihrem ernsten Mose/", "tokens": ["treibt", "au\u00df", "ih\u00b7rem", "erns\u00b7ten", "Mo\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "mu\u00df au\u00df Venus Kanzeley", "tokens": ["mu\u00df", "au\u00df", "Ve\u00b7nus", "Kan\u00b7ze\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "nehmen seine Schreiberey.", "tokens": ["neh\u00b7men", "sei\u00b7ne", "Schrei\u00b7be\u00b7rey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo mich kan ein Beyspiel sch\u00fczzen/", "tokens": ["Wo", "mich", "kan", "ein", "Bey\u00b7spiel", "sch\u00fcz\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zieh\u2019 ich die Poeten an/", "tokens": ["zieh'", "ich", "die", "Po\u00b7et\u00b7en", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die dergleichen auch gethan", "tokens": ["die", "derg\u00b7lei\u00b7chen", "auch", "ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mit Ergezzen und mit N\u00fczzen.", "tokens": ["mit", "Er\u00b7gez\u00b7zen", "und", "mit", "N\u00fcz\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer die allzugrossen Lasten", "tokens": ["Wer", "die", "all\u00b7zu\u00b7gros\u00b7sen", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mag es tuhn. Ich bin zu schwach/", "tokens": ["mag", "es", "tuhn", ".", "Ich", "bin", "zu", "schwach", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "PPER", "VAFIN", "PTKA", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "meine Schultern geben nach.", "tokens": ["mei\u00b7ne", "Schul\u00b7tern", "ge\u00b7ben", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieben/ das geprei\u00dfte Lieben", "tokens": ["Lie\u00b7ben", "/", "das", "ge\u00b7prei\u00df\u00b7te", "Lie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJA", "$(", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wekket meine Musen auff.", "tokens": ["wek\u00b7ket", "mei\u00b7ne", "Mu\u00b7sen", "auff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Amatusens Myrten-Hauff\u2019", "tokens": ["A\u00b7ma\u00b7tu\u00b7sens", "Myr\u00b7ten\u00b7Hauff'"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mein Feuer auffgetrieben.", "tokens": ["Hat", "mein", "Feu\u00b7er", "auff\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "darmit straal\u2019 ich. Nennt es D\u00fcnste/", "tokens": ["dar\u00b7mit", "straal'", "ich", ".", "Nennt", "es", "D\u00fcns\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nennt es Thorheit oder K\u00fcnste/", "tokens": ["nennt", "es", "Thor\u00b7heit", "o\u00b7der", "K\u00fcns\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "g\u00f6nnt mir Venus einen Tanz:", "tokens": ["g\u00f6nnt", "mir", "Ve\u00b7nus", "ei\u00b7nen", "Tanz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wol! fahr hin du Lorbeer-Kranz!", "tokens": ["Wol", "!", "fahr", "hin", "du", "Lor\u00b7beer\u00b7Kranz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo Jhr noch nicht zu den Alten/", "tokens": ["Wo", "Ihr", "noch", "nicht", "zu", "den", "Al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "alte Freunde/ seyd gezehlt/", "tokens": ["al\u00b7te", "Freun\u00b7de", "/", "seyd", "ge\u00b7zehlt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wo ihr Freude noch erwehlt", "tokens": ["wo", "ihr", "Freu\u00b7de", "noch", "er\u00b7wehlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und vor wilde nicht zuhalten", "tokens": ["und", "vor", "wil\u00b7de", "nicht", "zu\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wird Euch/ was ich von dem Lieben", "tokens": ["wird", "Euch", "/", "was", "ich", "von", "dem", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$(", "PWS", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "hab\u2019 in diesem Zehn geschrieben/", "tokens": ["hab'", "in", "die\u00b7sem", "Zehn", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "CARD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "eben so genehme sein/", "tokens": ["e\u00b7ben", "so", "ge\u00b7neh\u00b7me", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "als f\u00fchrt\u2019 ich was ernstlichs ein.", "tokens": ["als", "f\u00fchrt'", "ich", "was", "ernst\u00b7lichs", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PWS", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freyer Geister freye Sinn", "tokens": ["Frey\u00b7er", "Geis\u00b7ter", "frey\u00b7e", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ein Scherz und denn ein Schmau\u00df", "tokens": ["Denn", "ein", "Scherz", "und", "denn", "ein", "Schmau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "m\u00fcssen uns die Zeit gewinnen.", "tokens": ["m\u00fcs\u00b7sen", "uns", "die", "Zeit", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den hat Ammon nicht gemachet/", "tokens": ["Den", "hat", "Am\u00b7mon", "nicht", "ge\u00b7ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "der nicht auch zuweilen lachet.", "tokens": ["der", "nicht", "auch", "zu\u00b7wei\u00b7len", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Besser ist es/ nie gelebt", "tokens": ["Bes\u00b7ser", "ist", "es", "/", "nie", "ge\u00b7lebt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$(", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "als in Sturm-sehn stets geschwebt.", "tokens": ["als", "in", "Stur\u00b7msehn", "stets", "ge\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kenn\u2019 ich noch das alte Herze", "tokens": ["Kenn'", "ich", "noch", "das", "al\u00b7te", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df ihr habt vor dehm gef\u00fchrt/", "tokens": ["da\u00df", "ihr", "habt", "vor", "dehm", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wie auch Euch offt Lust ger\u00fchrt", "tokens": ["wie", "auch", "Euch", "offt", "Lust", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "ADV", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und wie ehmals Jhr im Scherze", "tokens": ["und", "wie", "eh\u00b7mals", "Ihr", "im", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PPOSAT", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "manches Liebes-Lied gesungen", "tokens": ["man\u00b7ches", "Lie\u00b7bes\u00b7Lied", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df die H\u00fctten wieder klungen:", "tokens": ["da\u00df", "die", "H\u00fct\u00b7ten", "wie\u00b7der", "klun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bild\u2019 ich mir beglaubet ein/", "tokens": ["Bild'", "ich", "mir", "be\u00b7glau\u00b7bet", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VVFIN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "di\u00df werd\u2019 Euch nicht wiedrig sein.", "tokens": ["di\u00df", "werd'", "Euch", "nicht", "wied\u00b7rig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nehmt derhalben/ Liebste/ nehmet", "tokens": ["Nehmt", "der\u00b7hal\u00b7ben", "/", "Liebs\u00b7te", "/", "neh\u00b7met"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PDAT", "$(", "NN", "$(", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dieses frohe Venus-Werk/", "tokens": ["die\u00b7ses", "fro\u00b7he", "Ve\u00b7nus\u00b7Werk", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "als ein kleines Gunst-gemerk.", "tokens": ["als", "ein", "klei\u00b7nes", "Gunst\u00b7ge\u00b7merk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Venus wird ja nicht besch\u00e4met/", "tokens": ["Ve\u00b7nus", "wird", "ja", "nicht", "be\u00b7sch\u00e4\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df sie frische Rosen tr\u00e4get/", "tokens": ["da\u00df", "sie", "fri\u00b7sche", "Ro\u00b7sen", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die der Floren Garte heget.", "tokens": ["Die", "der", "Flo\u00b7ren", "Gar\u00b7te", "he\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein betr\u00fcbter Amarant", "tokens": ["Ein", "be\u00b7tr\u00fcb\u00b7ter", "A\u00b7ma\u00b7rant"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "ist der Venus unbekant.", "tokens": ["ist", "der", "Ve\u00b7nus", "un\u00b7be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}