{"dta.poem.13014": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Weisheit fieng auch an, doch blieb sie wieder still", "tokens": ["Die", "Weis\u00b7heit", "fi\u00b7eng", "auch", "an", ",", "doch", "blieb", "sie", "wie\u00b7der", "still"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sprach nur Seitw\u00e4rts die\u00df: \u201eDie wei\u00df nicht, was sie will:", "tokens": ["Und", "sprach", "nur", "Seit\u00b7w\u00e4rts", "die\u00df", ":", "\u201e", "Die", "wei\u00df", "nicht", ",", "was", "sie", "will", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PDS", "$.", "$(", "PDS", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "35\u201dJhr Eigensinn vermeint, man soll nur sie verehren;", "tokens": ["\"", "Ihr", "Ei\u00b7gen\u00b7sinn", "ver\u00b7meint", ",", "man", "soll", "nur", "sie", "ver\u00b7eh\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "$,", "PIS", "VMFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edas w\u00e4r ja die Vernunft in blinden Wiz verkehren.", "tokens": ["\u201e", "das", "w\u00e4r", "ja", "die", "Ver\u00b7nunft", "in", "blin\u00b7den", "Wiz", "ver\u00b7keh\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die welche von dem Gl\u00fcck so redte, lie\u00df nicht nach,", "tokens": ["Die", "wel\u00b7che", "von", "dem", "Gl\u00fcck", "so", "red\u00b7te", ",", "lie\u00df", "nicht", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRELS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie suchte mit Get\u00f6\u00df den Vorzug, da sie sprach:", "tokens": ["Sie", "such\u00b7te", "mit", "Ge\u00b7t\u00f6\u00df", "den", "Vor\u00b7zug", ",", "da", "sie", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201est\u00fcnd ich von euch entfernt, so w\u00e4r der Abgrund offen,", "tokens": ["\u201e", "st\u00fcnd", "ich", "von", "euch", "ent\u00b7fernt", ",", "so", "w\u00e4r", "der", "Ab\u00b7grund", "of\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPER", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "40\u201dUnd ihr verschmachtetet f\u00fcr unfruchtbarem Hoffen.", "tokens": ["\"", "Und", "ihr", "ver\u00b7schmach\u00b7te\u00b7tet", "f\u00fcr", "un\u00b7frucht\u00b7ba\u00b7rem", "Hof\u00b7fen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewie manches Mahl verlohr der Feind so Ma\u00df als Ziel?", "tokens": ["\u201e", "wie", "man\u00b7ches", "Mahl", "ver\u00b7lohr", "der", "Feind", "so", "Ma\u00df", "als", "Ziel", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "VVFIN", "ART", "NN", "ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewarum? ich machte mir aus seinem Krieg ein Spiel.", "tokens": ["\u201e", "wa\u00b7rum", "?", "ich", "mach\u00b7te", "mir", "aus", "sei\u00b7nem", "Krieg", "ein", "Spiel", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ehat er nicht oft zu fruh, zu sp\u00e4t, umsonst gewachet,", "tokens": ["\u201e", "hat", "er", "nicht", "oft", "zu", "fruh", ",", "zu", "sp\u00e4t", ",", "um\u00b7sonst", "ge\u00b7wa\u00b7chet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "ADV", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.12": {"text": "\u201ewarum? weil ihr mit mir von euern Waffen sprachet;", "tokens": ["\u201e", "wa\u00b7rum", "?", "weil", "ihr", "mit", "mir", "von", "eu\u00b7ern", "Waf\u00b7fen", "spra\u00b7chet", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "KOUS", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "45\u201dIch ware stets mit euch, mit euch hielt' ich mich auf,", "tokens": ["\"", "Ich", "wa\u00b7re", "stets", "mit", "euch", ",", "mit", "euch", "hielt'", "ich", "mich", "auf", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "APPR", "PPER", "$,", "APPR", "PPER", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.14": {"text": "\u201eund lenckte das Geschick in einen treuen Lauf.", "tokens": ["\u201e", "und", "lenck\u00b7te", "das", "Ge\u00b7schick", "in", "ei\u00b7nen", "treu\u00b7en", "Lauf", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ewann euer Feind von mir nur einen Blick versp\u00fchrte,", "tokens": ["\u201e", "wann", "eu\u00b7er", "Feind", "von", "mir", "nur", "ei\u00b7nen", "Blick", "ver\u00b7sp\u00fchr\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eso war es, wann ich ihn zu dem Verderben f\u00fchrte.", "tokens": ["\u201e", "so", "war", "es", ",", "wann", "ich", "ihn", "zu", "dem", "Ver\u00b7der\u00b7ben", "f\u00fchr\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201ehalff nicht oft ein Morast, ein Regen, oder Frost,", "tokens": ["\u201e", "halff", "nicht", "oft", "ein", "Mo\u00b7rast", ",", "ein", "Re\u00b7gen", ",", "o\u00b7der", "Frost", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "NN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "50\u201dAuch selbst der Feinde Zwist? der war euch oft zum Trost.", "tokens": ["\"", "Auch", "selbst", "der", "Fein\u00b7de", "Zwist", "?", "der", "war", "euch", "oft", "zum", "Trost", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "NN", "$.", "ART", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eerweget, wie das Meer so gar sich einst erweckte,", "tokens": ["\u201e", "er\u00b7we\u00b7get", ",", "wie", "das", "Meer", "so", "gar", "sich", "einst", "er\u00b7weck\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eund durch der Winde Grimm der Feinde Stolz erschreckte;", "tokens": ["\u201e", "und", "durch", "der", "Win\u00b7de", "Grimm", "der", "Fein\u00b7de", "Stolz", "er\u00b7schreck\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ART", "NN", "NE", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201ewem schreibt ihr dieses zu? spracht ihr nicht: das Geschick,", "tokens": ["\u201e", "wem", "schreibt", "ihr", "die\u00b7ses", "zu", "?", "spracht", "ihr", "nicht", ":", "das", "Ge\u00b7schick", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PDS", "PTKVZ", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "\u201eder Umstand, dieser Fall ist unsrer Waffen Gl\u00fcck!", "tokens": ["\u201e", "der", "Um\u00b7stand", ",", "die\u00b7ser", "Fall", "ist", "uns\u00b7rer", "Waf\u00b7fen", "Gl\u00fcck", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PDAT", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "55\u201dJhr selber rieffet auf: des Feindes Ungl\u00fccks-F\u00e4lle!", "tokens": ["\"", "Ihr", "sel\u00b7ber", "rief\u00b7fet", "auf", ":", "des", "Fein\u00b7des", "Un\u00b7gl\u00fccks\u00b7F\u00e4l\u00b7le", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VVFIN", "PTKVZ", "$.", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201ewarum benanntet ihr sie nicht des Gl\u00fcckes Quelle?", "tokens": ["\u201e", "wa\u00b7rum", "be\u00b7nann\u00b7tet", "ihr", "sie", "nicht", "des", "Gl\u00fc\u00b7ckes", "Quel\u00b7le", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}