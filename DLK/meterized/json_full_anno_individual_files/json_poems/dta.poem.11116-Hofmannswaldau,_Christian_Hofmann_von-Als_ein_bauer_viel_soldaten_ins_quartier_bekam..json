{"dta.poem.11116": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als ein bauer viel soldaten ins quartier  \n bekam.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlag die capaunen todt, so wirst du nicht geschlagen,", "tokens": ["Schlag", "die", "ca\u00b7pau\u00b7nen", "todt", ",", "so", "wirst", "du", "nicht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "La\u00df wildpret auf den tisch mit wein und biere tragen,", "tokens": ["La\u00df", "wild\u00b7pret", "auf", "den", "tisch", "mit", "wein", "und", "bie\u00b7re", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Setz hecht und karpen vor aufs beste zugericht;", "tokens": ["Setz", "hecht", "und", "kar\u00b7pen", "vor", "aufs", "bes\u00b7te", "zu\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "APPR", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die teuffel dieser art vertreibt kein fasten nicht.", "tokens": ["Die", "teuf\u00b7fel", "die\u00b7ser", "art", "ver\u00b7treibt", "kein", "fas\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN",