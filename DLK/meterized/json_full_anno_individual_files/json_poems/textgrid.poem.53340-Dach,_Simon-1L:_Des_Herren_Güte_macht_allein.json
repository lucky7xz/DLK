ns": ["Vnd", "bey", "Ihm", "su\u00b7chen", "Schutz", "und", "Rhu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist ein k\u00f6stlich Ding in Pein", "tokens": ["Es", "ist", "ein", "k\u00f6st\u00b7lich", "Ding", "in", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bescheiden und gedultig seyn", "tokens": ["Be\u00b7schei\u00b7den", "und", "ge\u00b7dul\u00b7tig", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd auff des Herren H\u00fclffe hoffen;", "tokens": ["Vnd", "auff", "des", "Her\u00b7ren", "H\u00fclf\u00b7fe", "hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist sehr k\u00f6stlich einem Mann,", "tokens": ["Es", "ist", "sehr", "k\u00f6st\u00b7lich", "ei\u00b7nem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df er das Leid, so jhn getroffen,", "tokens": ["Da\u00df", "er", "das", "Leid", ",", "so", "jhn", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In seiner Jugend tragen kan,", "tokens": ["In", "sei\u00b7ner", "Ju\u00b7gend", "tra\