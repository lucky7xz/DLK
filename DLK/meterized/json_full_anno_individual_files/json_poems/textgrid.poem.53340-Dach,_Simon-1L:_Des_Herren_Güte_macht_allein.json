iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Denn Gott verst\u00f6sst nicht ewiglich,", "tokens": ["Denn", "Gott", "ver\u00b7st\u00f6sst", "nicht", "e\u00b7wig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er z\u00fcchtigt und erbarmet sich,", "tokens": ["Er", "z\u00fcch\u00b7tigt", "und", "er\u00b7bar\u00b7met", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schl\u00e4gt und heilet unsre Schmertzen", "tokens": ["Er", "schl\u00e4gt", "und", "hei\u00b7let", "uns\u00b7re", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach seiner G\u00fcte, die Er \u00fcbt,", "tokens": ["Nach", "sei\u00b7ner", "G\u00fc\u00b7te", ",", "die", "Er", "\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Er doch nimmermehr von Hertzen", "tokens": ["Denn", "Er", "doch", "nim\u00b7mer\u00b7mehr", "von", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Menschen plaget und betr\u00fcbt.", "tokens": ["Die", "Men\u00b7schen", "pla\u00b7get", "und", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}