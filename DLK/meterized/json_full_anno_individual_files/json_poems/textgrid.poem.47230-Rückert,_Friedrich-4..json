{"textgrid.poem.47230": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckt die Pflanze, die im Spiel der L\u00fcfte", "tokens": ["Be\u00b7gl\u00fcckt", "die", "Pflan\u00b7ze", ",", "die", "im", "Spiel", "der", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still in des Zeitlaufs fester Ordnung lebet,", "tokens": ["Still", "in", "des", "Zeit\u00b7laufs", "fes\u00b7ter", "Ord\u00b7nung", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An ihrem heimischen Boden ruhig klebet", "tokens": ["An", "ih\u00b7rem", "hei\u00b7mi\u00b7schen", "Bo\u00b7den", "ru\u00b7hig", "kle\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und doch zum Himmel aufhaucht ihre D\u00fcfte.", "tokens": ["Und", "doch", "zum", "Him\u00b7mel", "auf\u00b7haucht", "ih\u00b7re", "D\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Begl\u00fcckt der Strom auch, der im Waldgekl\u00fcfte,", "tokens": ["Be\u00b7gl\u00fcckt", "der", "Strom", "auch", ",", "der", "im", "Wald\u00b7ge\u00b7kl\u00fcf\u00b7te