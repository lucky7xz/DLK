{"textgrid.poem.62690": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Unterth\u00e4nigstes Sendschreiben an Ihro Hochf\u00fcrstl. Durchl. zu Sachsen Weymar", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? leb ich? oder nicht?", "tokens": ["Wie", "?", "leb", "ich", "?", "o\u00b7der", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$.", "KON", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie? wach ich? oder ists ein sch\u00f6nes Nacht-Gesicht", "tokens": ["Wie", "?", "wach", "ich", "?", "o\u00b7der", "ists", "ein", "sch\u00f6\u00b7nes", "Nacht\u00b7Ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$.", "KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ist bey Tage seh? Was will mein Auge blenden?", "tokens": ["Das", "ist", "bey", "Ta\u00b7ge", "seh", "?", "Was", "will", "mein", "Au\u00b7ge", "blen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVFIN", "$.", "PWS", "VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem ich einen Brief von ", "tokens": ["In\u00b7dem", "ich", "ei\u00b7nen", "Brief", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der nur mit F\u00fcrsten-Huld und Gnade angef\u00fcllt", "tokens": ["Der", "nur", "mit", "F\u00fcrs\u00b7ten\u