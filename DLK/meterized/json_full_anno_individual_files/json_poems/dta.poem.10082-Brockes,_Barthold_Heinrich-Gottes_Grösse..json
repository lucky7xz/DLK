{"dta.poem.10082": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ein Menschen-Leib an Gr\u00f6sse bey der Erden C\u00f6r-", "tokens": ["Was", "ein", "Men\u00b7schen\u00b7Leib", "an", "Gr\u00f6s\u00b7se", "bey", "der", "Er\u00b7den", "C\u00f6\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "per ist,", "tokens": ["per", "ist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ist die Erde, wenn man sie gegen alle Himmel misst,", "tokens": ["Ist", "die", "Er\u00b7de", ",", "wenn", "man", "sie", "ge\u00b7gen", "al\u00b7le", "Him\u00b7mel", "misst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Noch bey weitem nicht einmahl. Gegen GOtt, dem HErrn", "tokens": ["Noch", "bey", "wei\u00b7tem", "nicht", "ein\u00b7mahl", ".", "Ge\u00b7gen", "Gott", ",", "dem", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PIS", "PTKNEG", "ADV", "$.", "APPR", "NN", "$,", "ART", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "des Lichts,", "tokens": ["des", "Lichts", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Wird der Himmel Himmel Gr\u00f6sse nicht nur klein, nein,", "tokens": ["Wird", "der", "Him\u00b7mel", "Him\u00b7mel", "Gr\u00f6s\u00b7se", "nicht", "nur", "klein", ",", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "NN", "PTKNEG", "ADV", "ADJD", "$,", "PTKANT", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "gar zu nichts.", "tokens": ["gar", "zu", "nichts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "PIS", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}