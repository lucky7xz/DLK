{"dta.poem.12540": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "L.  \n  Der Kunstprotektor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.57", "no:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ealles kann ein Pinsel adlen,", "tokens": ["\u201e", "al\u00b7les", "kann", "ein", "Pin\u00b7sel", "ad\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles macht ein Pinsel eben,", "tokens": ["Al\u00b7les", "macht", "ein", "Pin\u00b7sel", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Satyr kann ein deutscher", "tokens": ["Ei\u00b7nen", "Sa\u00b7tyr", "kann", "ein", "deut\u00b7scher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pinsel zum Apoll erheben.", "tokens": ["Pin\u00b7sel", "zum", "A\u00b7poll", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Darum, nur mit Andacht trete", "tokens": ["Da\u00b7rum", ",", "nur", "mit", "An\u00b7dacht", "tre\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "man vor Meine Pinsel hin:", "tokens": ["man", "vor", "Mei\u00b7ne", "Pin\u00b7sel", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber vor dem Allergr\u00f6\u00dften", "tokens": ["A\u00b7ber", "vor", "dem", "Al\u00b7ler\u00b7gr\u00f6\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "sollen Meine Bauern knie'n.", "tokens": ["sol\u00b7len", "Mei\u00b7ne", "Bau\u00b7ern", "knie'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie gewinnen, wenn des Landes", "tokens": ["Sie", "ge\u00b7win\u00b7nen", ",", "wenn", "des", "Lan\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Vater f\u00fcr die Pinsel brennt:", "tokens": ["Va\u00b7ter", "f\u00fcr", "die", "Pin\u00b7sel", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn die Schweine m\u00fcssen steigen", "tokens": ["Denn", "die", "Schwei\u00b7ne", "m\u00fcs\u00b7sen", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "durch solch borstig Regiment.\u201c", "tokens": ["durch", "solch", "bors\u00b7tig", "Re\u00b7gi\u00b7ment", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}