{"dta.poem.7084": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nichts von einem seel\u2019gen Leben", "tokens": ["Wenn", "nichts", "von", "ei\u00b7nem", "seel'\u00b7gen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir einigen Begriff sonst f\u00e4hig w\u00e4r zu geben;", "tokens": ["Mir", "ei\u00b7ni\u00b7gen", "Be\u00b7griff", "sonst", "f\u00e4\u00b7hig", "w\u00e4r", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00fcrde dieses gantz allein", "tokens": ["So", "w\u00fcr\u00b7de", "die\u00b7ses", "gantz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon einigen Begriff von der Vollkommenheit", "tokens": ["Schon", "ei\u00b7ni\u00b7gen", "Be\u00b7griff", "von", "der", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Sch\u00f6pffers, und zugleich von einer Seeligkeit,", "tokens": ["Des", "Sch\u00f6pf\u00b7fers", ",", "und", "zu\u00b7gleich", "von", "ei\u00b7ner", "See\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mir zu erwecken, kr\u00e4fftig seyn.", "tokens": ["Mir", "zu", "er\u00b7we\u00b7cken", ",", "kr\u00e4ff\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}