: "trochaic.penta"}, "line.10": {"text": "(dir verhehl' ich's nicht: kleinm\u00fcthig neid' ich", "tokens": ["(", "dir", "ver\u00b7hehl'", "ich's", "nicht", ":", "klein\u00b7m\u00fct\u00b7hig", "neid'", "ich"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PIS", "PTKNEG", "$.", "ADJD", "VVFIN", "PPER"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Die entseelten Todten; also zittert", "tokens": ["Die", "ent\u00b7seel\u00b7ten", "Tod\u00b7ten", ";", "al\u00b7so", "zit\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Vor mir meine Seele!) reiche Du mir,", "tokens": ["Vor", "mir", "mei\u00b7ne", "See\u00b7le", "!", ")", "rei\u00b7che", "Du", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$.", "$(", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Du aus fernem, mir in fernem Lande", "tokens": ["Du", "aus", "fer\u00b7nem", ",", "mir", "in", "fer\u00b7nem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "$,", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Deine milden Arme und entrei\u00dfe", "tokens": ["Dei\u00b7ne", "mil\u00b7den", "Ar\u00b7me", "und", "ent\u00b7rei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Mich mir selbst und mache mich \u2013 was Du willst!", "tokens": ["Mich", "mir", "selbst", "und", "ma\u00b7che", "mich", "\u2013", "was", "Du", "willst