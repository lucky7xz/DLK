{"textgrid.poem.56984": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Bim, Bam, Bum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Glockenton fliegt durch die Nacht,", "tokens": ["Ein", "Glo\u00b7cken\u00b7ton", "fliegt", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "als h\u00e4tt er Vogelfl\u00fcgel,", "tokens": ["als", "h\u00e4tt", "er", "Vo\u00b7gel\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "er fliegt in r\u00f6mischer Kirchentracht", "tokens": ["er", "fliegt", "in", "r\u00f6\u00b7mi\u00b7scher", "Kir\u00b7chen\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "wohl \u00fcber Tal und H\u00fcgel.", "tokens": ["wohl", "\u00fc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sucht die Glockent\u00f6nin BIM,", "tokens": ["Er", "sucht", "die", "Glo\u00b7cken\u00b7t\u00f6\u00b7nin", "BiM", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die ihm vorausgeflogen;", "tokens": ["die", "ihm", "vor\u00b7aus\u00b7ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "wor