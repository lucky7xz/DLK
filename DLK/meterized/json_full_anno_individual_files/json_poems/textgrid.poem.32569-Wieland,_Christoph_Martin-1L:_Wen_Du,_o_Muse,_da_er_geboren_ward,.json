{"textgrid.poem.32569": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wen Du, o Muse, da er geboren ward,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen Du, o Muse, da er geboren ward,", "tokens": ["Wen", "Du", ",", "o", "Mu\u00b7se", ",", "da", "er", "ge\u00b7bo\u00b7ren", "ward", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "FM", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "L\u00e4chelnd umfingest, den wird kein Siegesgeschrei", "tokens": ["L\u00e4\u00b7chelnd", "um\u00b7fin\u00b7gest", ",", "den", "wird", "kein", "Sie\u00b7ges\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "ART", "VAFIN", "PIAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Mit Lorbeern, die vom Blut der Feinde", "tokens": ["Mit", "Lor\u00b7beern", ",", "die", "vom", "Blut", "der", "Fein\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tauen, bekr\u00f6nt, durch das Schlachtfeld f\u00fchren.", "tokens": ["Tau\u00b7en", ",", "be\u00b7kr\u00f6nt", ",", "durch", "das", "Schlacht\u00b7feld", "f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Da\u00df ihm die Nachwelt bei den Erobrern seh,", "tokens": ["Da\u00df", "ihm", "die", "Nach\u00b7welt", "bei", "den", "E\u00b7ro\u00b7brern", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Klagt keine Mutter, eilt aus des M\u00e4dchens Arm,", "tokens": ["Klagt", "kei\u00b7ne", "Mut\u00b7ter", ",", "eilt", "aus", "des", "M\u00e4d\u00b7chens", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Das ihm nachweint, kein J\u00fcngling", "tokens": ["Das", "ihm", "nach\u00b7weint", ",", "kein", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hin in die Schlacht, sein Blut zu str\u00f6men.", "tokens": ["Hin", "in", "die", "Schlacht", ",", "sein", "Blut", "zu", "str\u00f6\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch da er kindisch sich um den Busen schmiegt,", "tokens": ["Noch", "da", "er", "kin\u00b7disch", "sich", "um", "den", "Bu\u00b7sen", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oft mit Entz\u00fcckung m\u00fctterlich angeweint,", "tokens": ["Oft", "mit", "Ent\u00b7z\u00fc\u00b7ckung", "m\u00fct\u00b7ter\u00b7lich", "an\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "F\u00fchlt er schon mehr als andre, t\u00f6net", "tokens": ["F\u00fchlt", "er", "schon", "mehr", "als", "and\u00b7re", ",", "t\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KOUS", "PIS", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Etwas harmonisches in seinem Weinen.", "tokens": ["Et\u00b7was", "har\u00b7mo\u00b7ni\u00b7sches", "in", "sei\u00b7nem", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Da schon empfindt er l\u00e4chelnder Blicke Kraft,", "tokens": ["Da", "schon", "emp\u00b7findt", "er", "l\u00e4\u00b7cheln\u00b7der", "Bli\u00b7cke", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da wird sein Herz in ihren Umarmungen", "tokens": ["Da", "wird", "sein", "Herz", "in", "ih\u00b7ren", "Um\u00b7ar\u00b7mun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach dem Herzen der Mutter gebildet,", "tokens": ["Nach", "dem", "Her\u00b7zen", "der", "Mut\u00b7ter", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Z\u00e4rtlich und sch\u00f6n, wie ein junger Amor.", "tokens": ["Z\u00e4rt\u00b7lich", "und", "sch\u00f6n", ",", "wie", "ein", "jun\u00b7ger", "A\u00b7mor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Oft stehst du denn in Stunden der Mitternacht,", "tokens": ["Oft", "stehst", "du", "denn", "in", "Stun\u00b7den", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erato, bei ihm; wenn itzt die Mutter m\u00fcd", "tokens": ["E\u00b7ra\u00b7to", ",", "bei", "ihm", ";", "wenn", "itzt", "die", "Mut\u00b7ter", "m\u00fcd"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPER", "$.", "KOUS", "ADV", "ART", "NN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ihn zu umarmen, eingeschlummert,", "tokens": ["Ihn", "zu", "um\u00b7ar\u00b7men", ",", "ein\u00b7ge\u00b7schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihn in Tr\u00e4umen schon gl\u00fccklich siehet.", "tokens": ["Und", "ihn", "in", "Tr\u00e4u\u00b7men", "schon", "gl\u00fcck\u00b7lich", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Da stehst du bei ihm, wehst in sein offnes Herz", "tokens": ["Da", "stehst", "du", "bei", "ihm", ",", "wehst", "in", "sein", "off\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Zephyrlippen deine Empfindungen.", "tokens": ["Mit", "Ze\u00b7phyr\u00b7lip\u00b7pen", "dei\u00b7ne", "Emp\u00b7fin\u00b7dun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Formst in der w\u00e4chsernen Brust des S\u00e4uglings", "tokens": ["Formst", "in", "der", "w\u00e4ch\u00b7ser\u00b7nen", "Brust", "des", "S\u00e4ug\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Jeglichen Trieb, dir einst nachzuf\u00fchlen.", "tokens": ["Jeg\u00b7li\u00b7chen", "Trieb", ",", "dir", "einst", "nach\u00b7zu\u00b7f\u00fch\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "ADV", "VVIZU", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ihn tr\u00e4gt die hohe einsame Bahn dereinst", "tokens": ["Ihn", "tr\u00e4gt", "die", "ho\u00b7he", "ein\u00b7sa\u00b7me", "Bahn", "de\u00b7reinst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf Flaccus Spuren, oder den Hainen zu,", "tokens": ["Auf", "Flac\u00b7cus", "Spu\u00b7ren", ",", "o\u00b7der", "den", "Hai\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo dein unsterbliches Lied, o Maro,", "tokens": ["Wo", "dein", "uns\u00b7terb\u00b7li\u00b7ches", "Lied", ",", "o", "Ma\u00b7ro", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "FM", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch die Homerischen Lorbeern rauschet;", "tokens": ["Durch", "die", "Ho\u00b7me\u00b7ri\u00b7schen", "Lor\u00b7beern", "rau\u00b7schet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Oder in T\u00e4ler hin zu dem Silberquell,", "tokens": ["O\u00b7der", "in", "T\u00e4\u00b7ler", "hin", "zu", "dem", "Sil\u00b7ber\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der, wie dein Trinklied, S\u00e4nger von Teos, flie\u00dft,", "tokens": ["Der", ",", "wie", "dein", "Trin\u00b7klied", ",", "S\u00e4n\u00b7ger", "von", "Te\u00b7os", ",", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPOSAT", "NN", "$,", "NN", "APPR", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oder wie deines M\u00e4dchens K\u00fcsse,", "tokens": ["O\u00b7der", "wie", "dei\u00b7nes", "M\u00e4d\u00b7chens", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du, von ihnen berauscht, geraset.", "tokens": ["Wenn", "du", ",", "von", "ih\u00b7nen", "be\u00b7rauscht", ",", "ge\u00b7ra\u00b7set", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Denn lehrt ein M\u00e4dchen, welches der Doris gleicht,", "tokens": ["Denn", "lehrt", "ein", "M\u00e4d\u00b7chen", ",", "wel\u00b7ches", "der", "Do\u00b7ris", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So sch\u00f6n, mit Augen, die so begeisternd sind,", "tokens": ["So", "sch\u00f6n", ",", "mit", "Au\u00b7gen", ",", "die", "so", "be\u00b7geis\u00b7ternd", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihn umarmend, die Liebe singen,", "tokens": ["Ihn", "um\u00b7ar\u00b7mend", ",", "die", "Lie\u00b7be", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und dich, o Unschuld, der Liebe Schwester.", "tokens": ["Und", "dich", ",", "o", "Un\u00b7schuld", ",", "der", "Lie\u00b7be", "Schwes\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Er \u00fcbt die Haine, wenn sie der Morgen gr\u00fc\u00dft,", "tokens": ["Er", "\u00fcbt", "die", "Hai\u00b7ne", ",", "wenn", "sie", "der", "Mor\u00b7gen", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oder wenn auf den Auen der Fr\u00fchling schl\u00e4ft,", "tokens": ["O\u00b7der", "wenn", "auf", "den", "Au\u00b7en", "der", "Fr\u00fch\u00b7ling", "schl\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Ruhm der Tugend nachzuhallen.", "tokens": ["Den", "Ruhm", "der", "Tu\u00b7gend", "nach\u00b7zu\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Menschen sind taub, doch ihm horcht der Seraph!", "tokens": ["Men\u00b7schen", "sind", "taub", ",", "doch", "ihm", "horcht", "der", "Se\u00b7raph", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Ihn wird die Nachwelt, wenn seine Stimme schon", "tokens": ["Ihn", "wird", "die", "Nach\u00b7welt", ",", "wenn", "sei\u00b7ne", "Stim\u00b7me", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich den Ges\u00e4ngen Englischer Harfen mischt,", "tokens": ["Sich", "den", "Ge\u00b7s\u00e4n\u00b7gen", "Eng\u00b7li\u00b7scher", "Har\u00b7fen", "mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "H\u00f6ren, ihn segnen, seine Hymnen", "tokens": ["H\u00f6\u00b7ren", ",", "ihn", "seg\u00b7nen", ",", "sei\u00b7ne", "Hym\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVINF", "$,", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vor dem nachahmenden J\u00fcngling spielen,", "tokens": ["Vor", "dem", "nac\u00b7hah\u00b7men\u00b7den", "J\u00fcng\u00b7ling", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Welcher, sie h\u00f6rend, sich hingez\u00fccket f\u00fchlt,", "tokens": ["Wel\u00b7cher", ",", "sie", "h\u00f6\u00b7rend", ",", "sich", "hin\u00b7ge\u00b7z\u00fc\u00b7cket", "f\u00fchlt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "PPER", "VVPP", "$,", "PRF", "VVFIN", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "G\u00f6ttlich zu singen. Auch weint bei seinem Lied", "tokens": ["G\u00f6tt\u00b7lich", "zu", "sin\u00b7gen", ".", "Auch", "weint", "bei", "sei\u00b7nem", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Einst manch jugendlich weiches M\u00e4dchen,", "tokens": ["Einst", "manch", "ju\u00b7gend\u00b7lich", "wei\u00b7ches", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Z\u00e4rtlich wie Doris, und liebt den S\u00e4nger.", "tokens": ["Z\u00e4rt\u00b7lich", "wie", "Do\u00b7ris", ",", "und", "liebt", "den", "S\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NE", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Wen Du, o Muse, da er geboren ward,", "tokens": ["Wen", "Du", ",", "o", "Mu\u00b7se", ",", "da", "er", "ge\u00b7bo\u00b7ren", "ward", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "FM", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "L\u00e4chelnd umfingest, den wird kein Siegesgeschrei", "tokens": ["L\u00e4\u00b7chelnd", "um\u00b7fin\u00b7gest", ",", "den", "wird", "kein", "Sie\u00b7ges\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "ART", "VAFIN", "PIAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Mit Lorbeern, die vom Blut der Feinde", "tokens": ["Mit", "Lor\u00b7beern", ",", "die", "vom", "Blut", "der", "Fein\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tauen, bekr\u00f6nt, durch das Schlachtfeld f\u00fchren.", "tokens": ["Tau\u00b7en", ",", "be\u00b7kr\u00f6nt", ",", "durch", "das", "Schlacht\u00b7feld", "f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Da\u00df ihm die Nachwelt bei den Erobrern seh,", "tokens": ["Da\u00df", "ihm", "die", "Nach\u00b7welt", "bei", "den", "E\u00b7ro\u00b7brern", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Klagt keine Mutter, eilt aus des M\u00e4dchens Arm,", "tokens": ["Klagt", "kei\u00b7ne", "Mut\u00b7ter", ",", "eilt", "aus", "des", "M\u00e4d\u00b7chens", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Das ihm nachweint, kein J\u00fcngling", "tokens": ["Das", "ihm", "nach\u00b7weint", ",", "kein", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hin in die Schlacht, sein Blut zu str\u00f6men.", "tokens": ["Hin", "in", "die", "Schlacht", ",", "sein", "Blut", "zu", "str\u00f6\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Noch da er kindisch sich um den Busen schmiegt,", "tokens": ["Noch", "da", "er", "kin\u00b7disch", "sich", "um", "den", "Bu\u00b7sen", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oft mit Entz\u00fcckung m\u00fctterlich angeweint,", "tokens": ["Oft", "mit", "Ent\u00b7z\u00fc\u00b7ckung", "m\u00fct\u00b7ter\u00b7lich", "an\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "F\u00fchlt er schon mehr als andre, t\u00f6net", "tokens": ["F\u00fchlt", "er", "schon", "mehr", "als", "and\u00b7re", ",", "t\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KOUS", "PIS", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Etwas harmonisches in seinem Weinen.", "tokens": ["Et\u00b7was", "har\u00b7mo\u00b7ni\u00b7sches", "in", "sei\u00b7nem", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.16": {"line.1": {"text": "Da schon empfindt er l\u00e4chelnder Blicke Kraft,", "tokens": ["Da", "schon", "emp\u00b7findt", "er", "l\u00e4\u00b7cheln\u00b7der", "Bli\u00b7cke", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da wird sein Herz in ihren Umarmungen", "tokens": ["Da", "wird", "sein", "Herz", "in", "ih\u00b7ren", "Um\u00b7ar\u00b7mun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach dem Herzen der Mutter gebildet,", "tokens": ["Nach", "dem", "Her\u00b7zen", "der", "Mut\u00b7ter", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Z\u00e4rtlich und sch\u00f6n, wie ein junger Amor.", "tokens": ["Z\u00e4rt\u00b7lich", "und", "sch\u00f6n", ",", "wie", "ein", "jun\u00b7ger", "A\u00b7mor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Oft stehst du denn in Stunden der Mitternacht,", "tokens": ["Oft", "stehst", "du", "denn", "in", "Stun\u00b7den", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erato, bei ihm; wenn itzt die Mutter m\u00fcd", "tokens": ["E\u00b7ra\u00b7to", ",", "bei", "ihm", ";", "wenn", "itzt", "die", "Mut\u00b7ter", "m\u00fcd"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPER", "$.", "KOUS", "ADV", "ART", "NN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ihn zu umarmen, eingeschlummert,", "tokens": ["Ihn", "zu", "um\u00b7ar\u00b7men", ",", "ein\u00b7ge\u00b7schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihn in Tr\u00e4umen schon gl\u00fccklich siehet.", "tokens": ["Und", "ihn", "in", "Tr\u00e4u\u00b7men", "schon", "gl\u00fcck\u00b7lich", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Da stehst du bei ihm, wehst in sein offnes Herz", "tokens": ["Da", "stehst", "du", "bei", "ihm", ",", "wehst", "in", "sein", "off\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Zephyrlippen deine Empfindungen.", "tokens": ["Mit", "Ze\u00b7phyr\u00b7lip\u00b7pen", "dei\u00b7ne", "Emp\u00b7fin\u00b7dun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Formst in der w\u00e4chsernen Brust des S\u00e4uglings", "tokens": ["Formst", "in", "der", "w\u00e4ch\u00b7ser\u00b7nen", "Brust", "des", "S\u00e4ug\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Jeglichen Trieb, dir einst nachzuf\u00fchlen.", "tokens": ["Jeg\u00b7li\u00b7chen", "Trieb", ",", "dir", "einst", "nach\u00b7zu\u00b7f\u00fch\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "ADV", "VVIZU", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "Ihn tr\u00e4gt die hohe einsame Bahn dereinst", "tokens": ["Ihn", "tr\u00e4gt", "die", "ho\u00b7he", "ein\u00b7sa\u00b7me", "Bahn", "de\u00b7reinst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf Flaccus Spuren, oder den Hainen zu,", "tokens": ["Auf", "Flac\u00b7cus", "Spu\u00b7ren", ",", "o\u00b7der", "den", "Hai\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo dein unsterbliches Lied, o Maro,", "tokens": ["Wo", "dein", "uns\u00b7terb\u00b7li\u00b7ches", "Lied", ",", "o", "Ma\u00b7ro", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "FM", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch die Homerischen Lorbeern rauschet;", "tokens": ["Durch", "die", "Ho\u00b7me\u00b7ri\u00b7schen", "Lor\u00b7beern", "rau\u00b7schet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.20": {"line.1": {"text": "Oder in T\u00e4ler hin zu dem Silberquell,", "tokens": ["O\u00b7der", "in", "T\u00e4\u00b7ler", "hin", "zu", "dem", "Sil\u00b7ber\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der, wie dein Trinklied, S\u00e4nger von Teos, flie\u00dft,", "tokens": ["Der", ",", "wie", "dein", "Trin\u00b7klied", ",", "S\u00e4n\u00b7ger", "von", "Te\u00b7os", ",", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPOSAT", "NN", "$,", "NN", "APPR", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oder wie deines M\u00e4dchens K\u00fcsse,", "tokens": ["O\u00b7der", "wie", "dei\u00b7nes", "M\u00e4d\u00b7chens", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du, von ihnen berauscht, geraset.", "tokens": ["Wenn", "du", ",", "von", "ih\u00b7nen", "be\u00b7rauscht", ",", "ge\u00b7ra\u00b7set", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.21": {"line.1": {"text": "Denn lehrt ein M\u00e4dchen, welches der Doris gleicht,", "tokens": ["Denn", "lehrt", "ein", "M\u00e4d\u00b7chen", ",", "wel\u00b7ches", "der", "Do\u00b7ris", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So sch\u00f6n, mit Augen, die so begeisternd sind,", "tokens": ["So", "sch\u00f6n", ",", "mit", "Au\u00b7gen", ",", "die", "so", "be\u00b7geis\u00b7ternd", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihn umarmend, die Liebe singen,", "tokens": ["Ihn", "um\u00b7ar\u00b7mend", ",", "die", "Lie\u00b7be", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und dich, o Unschuld, der Liebe Schwester.", "tokens": ["Und", "dich", ",", "o", "Un\u00b7schuld", ",", "der", "Lie\u00b7be", "Schwes\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "Er \u00fcbt die Haine, wenn sie der Morgen gr\u00fc\u00dft,", "tokens": ["Er", "\u00fcbt", "die", "Hai\u00b7ne", ",", "wenn", "sie", "der", "Mor\u00b7gen", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oder wenn auf den Auen der Fr\u00fchling schl\u00e4ft,", "tokens": ["O\u00b7der", "wenn", "auf", "den", "Au\u00b7en", "der", "Fr\u00fch\u00b7ling", "schl\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Ruhm der Tugend nachzuhallen.", "tokens": ["Den", "Ruhm", "der", "Tu\u00b7gend", "nach\u00b7zu\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Menschen sind taub, doch ihm horcht der Seraph!", "tokens": ["Men\u00b7schen", "sind", "taub", ",", "doch", "ihm", "horcht", "der", "Se\u00b7raph", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.23": {"line.1": {"text": "Ihn wird die Nachwelt, wenn seine Stimme schon", "tokens": ["Ihn", "wird", "die", "Nach\u00b7welt", ",", "wenn", "sei\u00b7ne", "Stim\u00b7me", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich den Ges\u00e4ngen Englischer Harfen mischt,", "tokens": ["Sich", "den", "Ge\u00b7s\u00e4n\u00b7gen", "Eng\u00b7li\u00b7scher", "Har\u00b7fen", "mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "H\u00f6ren, ihn segnen, seine Hymnen", "tokens": ["H\u00f6\u00b7ren", ",", "ihn", "seg\u00b7nen", ",", "sei\u00b7ne", "Hym\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVINF", "$,", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Vor dem nachahmenden J\u00fcngling spielen,", "tokens": ["Vor", "dem", "nac\u00b7hah\u00b7men\u00b7den", "J\u00fcng\u00b7ling", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "Welcher, sie h\u00f6rend, sich hingez\u00fccket f\u00fchlt,", "tokens": ["Wel\u00b7cher", ",", "sie", "h\u00f6\u00b7rend", ",", "sich", "hin\u00b7ge\u00b7z\u00fc\u00b7cket", "f\u00fchlt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "PPER", "VVPP", "$,", "PRF", "VVFIN", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "G\u00f6ttlich zu singen. Auch weint bei seinem Lied", "tokens": ["G\u00f6tt\u00b7lich", "zu", "sin\u00b7gen", ".", "Auch", "weint", "bei", "sei\u00b7nem", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Einst manch jugendlich weiches M\u00e4dchen,", "tokens": ["Einst", "manch", "ju\u00b7gend\u00b7lich", "wei\u00b7ches", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Z\u00e4rtlich wie Doris, und liebt den S\u00e4nger.", "tokens": ["Z\u00e4rt\u00b7lich", "wie", "Do\u00b7ris", ",", "und", "liebt", "den", "S\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NE", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}