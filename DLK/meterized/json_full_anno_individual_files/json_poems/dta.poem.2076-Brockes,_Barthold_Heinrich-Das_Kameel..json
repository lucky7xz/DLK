{"dta.poem.2076": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Kameel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist dieses Thier bey uns zwar nicht sonderlich be-", "tokens": ["Es", "ist", "die\u00b7ses", "Thier", "bey", "uns", "zwar", "nicht", "son\u00b7der\u00b7lich", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "APPR", "PPER", "ADV", "PTKNEG", "ADJD", "TRUNC"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "kannt,", "tokens": ["kannt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Sondern meist in Asia: Dennoch wird in seinem Haare,", "tokens": ["Son\u00b7dern", "meist", "in", "A\u00b7sia", ":", "Den\u00b7noch", "wird", "in", "sei\u00b7nem", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NE", "$.", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Als in einer n\u00fctzlichen und sehr vortheilhaften Waare,", "tokens": ["Als", "in", "ei\u00b7ner", "n\u00fctz\u00b7li\u00b7chen", "und", "sehr", "vor\u00b7theil\u00b7haf\u00b7ten", "Waa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Uns ein rechter Schatz im Handel aus der Ferne zugesandt.", "tokens": ["Uns", "ein", "rech\u00b7ter", "Schatz", "im", "Han\u00b7del", "aus", "der", "Fer\u00b7ne", "zu\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Dieses Haar soll es zum \u00f6ftern, meist in einem jeden", "tokens": ["Die\u00b7ses", "Haar", "soll", "es", "zum", "\u00f6f\u00b7tern", ",", "meist", "in", "ei\u00b7nem", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "APPRART", "VVINF", "$,", "ADV", "APPR", "ART", "PIAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Jahre,", "tokens": ["Jah\u00b7re", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und zwar in sehr wenig Tagen, da\u00df man es bequemer", "tokens": ["Und", "zwar", "in", "sehr", "we\u00b7nig", "Ta\u00b7gen", ",", "da\u00df", "man", "es", "be\u00b7que\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADV", "PIAT", "NN", "$,", "KOUS", "PIS", "PPER", "ADJD"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "fassen,", "tokens": ["fas\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Sammeln und gebrauchen kann, fast auf einmal fallen", "tokens": ["Sam\u00b7meln", "und", "ge\u00b7brau\u00b7chen", "kann", ",", "fast", "auf", "ein\u00b7mal", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$,", "ADV", "APPR", "ADV", "VVINF"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "lassen.", "tokens": ["las\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Dem, der es zum ersten sieht, k\u00f6mmt gemeiniglich die\u00df", "tokens": ["Dem", ",", "der", "es", "zum", "ers\u00b7ten", "sieht", ",", "k\u00f6mmt", "ge\u00b7mei\u00b7nig\u00b7lich", "die\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPER", "APPRART", "ADJA", "VVFIN", "$,", "VVFIN", "ADV", "PDS"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Thier", "tokens": ["Thier"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "An Gestalt fast ungeformt, plump und grob und h\u00e4\u00df-", "tokens": ["An", "Ge\u00b7stalt", "fast", "un\u00b7ge\u00b7formt", ",", "plump", "und", "grob", "und", "h\u00e4\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "KON", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "lich f\u00fcr:", "tokens": ["lich", "f\u00fcr", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "APPR", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Dennoch hat es seine Gleichmaa\u00df, und es sind die starken", "tokens": ["Den\u00b7noch", "hat", "es", "sei\u00b7ne", "Gleich\u00b7maa\u00df", ",", "und", "es", "sind", "die", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Glieder", "tokens": ["Glie\u00b7der"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Wunderw\u00fcrdig eingerichtet. Sich zum Tragen gut zu", "tokens": ["Wun\u00b7der\u00b7w\u00fcr\u00b7dig", "ein\u00b7ge\u00b7rich\u00b7tet", ".", "Sich", "zum", "Tra\u00b7gen", "gut", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "PRF", "APPRART", "NN", "ADJD", "PTKZU"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.19": {"text": "schicken,", "tokens": ["schi\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Zeigt sich ein sehr starker Buckel in der Mitt\u2019 auf seinem", "tokens": ["Zeigt", "sich", "ein", "sehr", "star\u00b7ker", "Bu\u00b7ckel", "in", "der", "Mitt'", "auf", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADV", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "R\u00fccken.", "tokens": ["R\u00fc\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Wenn man es beladen will, legt es sich von selbsten nie-", "tokens": ["Wenn", "man", "es", "be\u00b7la\u00b7den", "will", ",", "legt", "es", "sich", "von", "selbs\u00b7ten", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "TRUNC"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "der,", "tokens": ["der", ","], "token_info": ["word", "punct"], "pos": ["ART", "$,"], "meter": "-", "measure": "single.down"}, "line.24": {"text": "Da es denn, wenn es g\u2019nug hat, selber in die H\u00f6he", "tokens": ["Da", "es", "denn", ",", "wenn", "es", "g'\u00b7nug", "hat", ",", "sel\u00b7ber", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.25": {"text": "springt.", "tokens": ["springt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Nicht beschreiblich ist der Nutzen, welchen das Kameel-", "tokens": ["Nicht", "be\u00b7schreib\u00b7lich", "ist", "der", "Nut\u00b7zen", ",", "wel\u00b7chen", "das", "Ka\u00b7meel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,", "PWAT", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.27": {"text": "thier bringt,", "tokens": ["thier", "bringt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.28": {"text": "Da es durch die d\u00fcrren W\u00fcsten, mit sehr schlechter Kost", "tokens": ["Da", "es", "durch", "die", "d\u00fcr\u00b7ren", "W\u00fcs\u00b7ten", ",", "mit", "sehr", "schlech\u00b7ter", "Kost"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.29": {"text": "vergn\u00fcget,", "tokens": ["ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Und sehr lange sonder Trank Lasten schleppt; Auch, wenn", "tokens": ["Und", "sehr", "lan\u00b7ge", "son\u00b7der", "Trank", "Las\u00b7ten", "schleppt", ";", "Auch", ",", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "NN", "VVFIN", "$.", "ADV", "$,", "KOUS"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "man krieget,", "tokens": ["man", "krie\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}}}}