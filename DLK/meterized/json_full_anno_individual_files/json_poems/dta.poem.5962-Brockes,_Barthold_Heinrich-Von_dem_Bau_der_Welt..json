{"dta.poem.5962": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die C\u00f6rper nun, wovon in solchem Unterscheid", "tokens": ["Die", "C\u00f6r\u00b7per", "nun", ",", "wo\u00b7von", "in", "sol\u00b7chem", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein jeglicher weit von einander stehet;", "tokens": ["Ein", "jeg\u00b7li\u00b7cher", "weit", "von", "ein\u00b7an\u00b7der", "ste\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sind, in verschiedner Daur der Zeit,", "tokens": ["Sind", ",", "in", "ver\u00b7schied\u00b7ner", "Daur", "der", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Wirbeln, die verschiedlich gro\u00df, gedrehet.", "tokens": ["In", "Wir\u00b7beln", ",", "die", "ver\u00b7schied\u00b7lich", "gro\u00df", ",", "ge\u00b7dre\u00b7het", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Erden-Wirbel zieht, in seinem Creise,", "tokens": ["Der", "Er\u00b7den\u00b7Wir\u00b7bel", "zieht", ",", "in", "sei\u00b7nem", "Crei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Mondes kleine Mass\u2019 und Kugel mit sich fort.", "tokens": ["Des", "Mon\u00b7des", "klei\u00b7ne", "Mass'", "und", "Ku\u00b7gel", "mit", "sich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verschiedne Sterne sind auf gleiche Weise", "tokens": ["Ver\u00b7schied\u00b7ne", "Ster\u00b7ne", "sind", "auf", "glei\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch andre mitgef\u00fchrt von ihrem Ort.", "tokens": ["Durch", "and\u00b7re", "mit\u00b7ge\u00b7f\u00fchrt", "von", "ih\u00b7rem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Saturnus hat verschiedene Trabanten,", "tokens": ["Sa\u00b7tur\u00b7nus", "hat", "ver\u00b7schie\u00b7de\u00b7ne", "Tra\u00b7ban\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ingleichen Jupiter, in jener tieffen Weite,", "tokens": ["In\u00b7glei\u00b7chen", "Ju\u00b7pi\u00b7ter", ",", "in", "je\u00b7ner", "tief\u00b7fen", "Wei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zu ihrem stetigen Geleite.", "tokens": ["Zu", "ih\u00b7rem", "ste\u00b7ti\u00b7gen", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}