{"textgrid.poem.39663": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du alte stolze Rom, die, was der Erdkrei\u00df fa\u00dft,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du alte stolze Rom, die, was der Erdkrei\u00df fa\u00dft,", "tokens": ["Du", "al\u00b7te", "stol\u00b7ze", "Rom", ",", "die", ",", "was", "der", "Erd\u00b7krei\u00df", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NE", "$,", "PRELS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Prunk herbeigeschleppt vom Rheine, wie vom Nile;", "tokens": ["Zum", "Prunk", "her\u00b7bei\u00b7ge\u00b7schleppt", "vom", "Rhei\u00b7ne", ",", "wie", "vom", "Ni\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "APPRART", "NE", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die du des Herrschers Kraft, des Herrschers Hochgef\u00fchle", "tokens": ["Die", "du", "des", "Herr\u00b7schers", "Kraft", ",", "des", "Herr\u00b7schers", "Hoch\u00b7ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf deiner Werke Stirn so hell gestempelt hast.", "tokens": ["Auf", "dei\u00b7ner", "Wer\u00b7ke", "Stirn", "so", "hell", "ge\u00b7stem\u00b7pelt", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["AP