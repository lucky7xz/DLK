{"dta.poem.3281": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Weinlese.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, Weinles, unsre Freude!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Wein\u00b7les", ",", "uns\u00b7re", "Freu\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sey ewig unser grosses Fest!", "tokens": ["Sey", "e\u00b7wig", "un\u00b7ser", "gros\u00b7ses", "Fest", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie jauchzen wir, nach langem Leide,", "tokens": ["Wie", "jauch\u00b7zen", "wir", ",", "nach", "lan\u00b7gem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Bacchus uns nicht gar verl\u00e4\u00dft!", "tokens": ["Da\u00df", "Bac\u00b7chus", "uns", "nicht", "gar", "ver\u00b7l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du schenkest uns das Mark der Reben,", "tokens": ["Du", "schen\u00b7kest", "uns", "das", "Mark", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Greis und J\u00fcngling zu erfreun.", "tokens": ["Den", "Greis", "und", "J\u00fcng\u00b7ling", "zu", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, ja! nun mag ich wieder leben:", "tokens": ["Ja", ",", "ja", "!", "nun", "mag", "ich", "wie\u00b7der", "le\u00b7ben", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was ist ein Leben ohne Wein?", "tokens": ["Was", "ist", "ein", "Le\u00b7ben", "oh\u00b7ne", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Erdkreis drohte zu vergehen:", "tokens": ["Der", "Erd\u00b7kreis", "droh\u00b7te", "zu", "ver\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn, ach! die Rebe stund betr\u00fcbt.", "tokens": ["Denn", ",", "ach", "!", "die", "Re\u00b7be", "stund", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun flie\u00dft ihr Necktar auf den H\u00f6hen,", "tokens": ["Nun", "flie\u00dft", "ihr", "Neck\u00b7tar", "auf", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der allem neues Leben giebt.", "tokens": ["Der", "al\u00b7lem", "neu\u00b7es", "Le\u00b7ben", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erfrorne Dichter, singt nun wieder!", "tokens": ["Er\u00b7fror\u00b7ne", "Dich\u00b7ter", ",", "singt", "nun", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will keine Muse g\u00fcnstig seyn?", "tokens": ["Will", "kei\u00b7ne", "Mu\u00b7se", "g\u00fcns\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ly\u00e4us lehret bessre Lieder:", "tokens": ["Ly\u00b7\u00e4us", "leh\u00b7ret", "bess\u00b7re", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nichts ist so sinnreich, als der Wein.", "tokens": ["Nichts", "ist", "so", "sinn\u00b7reich", ",", "als", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verschmachtend lag mit schlaffem Bogen", "tokens": ["Ver\u00b7schmach\u00b7tend", "lag", "mit", "schlaf\u00b7fem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die matte Liebe hingestreckt.", "tokens": ["Die", "mat\u00b7te", "Lie\u00b7be", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie muthig ist sie aufgeflogen,", "tokens": ["Wie", "mut\u00b7hig", "ist", "sie", "auf\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem sie jungen Wein geschmeckt!", "tokens": ["Nach\u00b7dem", "sie", "jun\u00b7gen", "Wein", "ge\u00b7schmeckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er hilft ihr feine Freunde kr\u00f6nen:", "tokens": ["Er", "hilft", "ihr", "fei\u00b7ne", "Freun\u00b7de", "kr\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist bequem, ihr Weib zu seyn:", "tokens": ["Es", "ist", "be\u00b7quem", ",", "ihr", "Weib", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie k\u00fcssen immer treue Sch\u00f6nen;", "tokens": ["Sie", "k\u00fcs\u00b7sen", "im\u00b7mer", "treu\u00b7e", "Sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So \u00fcberredend ist ihr Wein!", "tokens": ["So", "\u00fc\u00b7berr\u00b7e\u00b7dend", "ist", "ihr", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jsmenen qu\u00e4lt ein tr\u00e4ger Gatte,", "tokens": ["Js\u00b7me\u00b7nen", "qu\u00e4lt", "ein", "tr\u00e4\u00b7ger", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der ganze N\u00e4chte schlafen kann.", "tokens": ["Der", "gan\u00b7ze", "N\u00e4ch\u00b7te", "schla\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil Amor nicht geholfen hatte,", "tokens": ["Weil", "A\u00b7mor", "nicht", "ge\u00b7hol\u00b7fen", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ruft sie Vater Bacchum an.", "tokens": ["So", "ruft", "sie", "Va\u00b7ter", "Bac\u00b7chum", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Alte zecht, wird los\u2019 und herzet,", "tokens": ["Der", "Al\u00b7te", "zecht", ",", "wird", "los'", "und", "her\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schl\u00e4ft erst sp\u00e4t und k\u00fcssend ein.", "tokens": ["Und", "schl\u00e4ft", "erst", "sp\u00e4t", "und", "k\u00fcs\u00b7send", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df der mit halber Jugend scherzet;", "tokens": ["Da\u00df", "der", "mit", "hal\u00b7ber", "Ju\u00b7gend", "scher\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Wunder! thut es nicht der Wein?", "tokens": ["O", "Wun\u00b7der", "!", "thut", "es", "nicht", "der", "Wein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Wein kann alles m\u00f6glich machen:", "tokens": ["Der", "Wein", "kann", "al\u00b7les", "m\u00f6g\u00b7lich", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Wein, sey dieser Tag geweiht!", "tokens": ["Dir", ",", "Wein", ",", "sey", "die\u00b7ser", "Tag", "ge\u00b7weiht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es herrsche Scherz, Gesang und Lachen;", "tokens": ["Es", "herr\u00b7sche", "Scherz", ",", "Ge\u00b7sang", "und", "La\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man zech\u2019 aus frommer Dankbarkeit!", "tokens": ["Man", "zech'", "aus", "from\u00b7mer", "Dank\u00b7bar\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was fehlt? Jhr Freunde, nur noch eines!", "tokens": ["Was", "fehlt", "?", "Ihr", "Freun\u00b7de", ",", "nur", "noch", "ei\u00b7nes", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PPOSAT", "NN", "$,", "ADV", "ADV", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den frohen Amor ladet ein:", "tokens": ["Den", "fro\u00b7hen", "A\u00b7mor", "la\u00b7det", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn Amor ist ein Freund des Weines,", "tokens": ["Denn", "A\u00b7mor", "ist", "ein", "Freund", "des", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ohne K\u00fcsse schmeckt kein Wein.", "tokens": ["Und", "oh\u00b7ne", "K\u00fcs\u00b7se", "schmeckt", "kein", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}