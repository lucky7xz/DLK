{"textgrid.poem.33806": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Der schwarze Tod", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erzittre Welt, ich bin die Pest,", "tokens": ["Er\u00b7zitt\u00b7re", "Welt", ",", "ich", "bin", "die", "Pest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich komm' in alle Lande", "tokens": ["Ich", "komm'", "in", "al\u00b7le", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und richte mir ein gro\u00dfes Fest,", "tokens": ["Und", "rich\u00b7te", "mir", "ein", "gro\u00b7\u00dfes", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Blick ist Fieber, feuerfest", "tokens": ["Mein", "Blick", "ist", "Fie\u00b7ber", ",", "feu\u00b7er\u00b7fest"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schwarz ist mein Gewande.", "tokens": ["Und", "schwarz", "ist", "mein", "Ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich komme von \u00c4gyptenland", "tokens": ["Ich", "kom\u00b7me", "von", "\u00c4\u00b7gyp\u00b7ten\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In roten Nebelschleiern,", "tokens": ["In", "ro\u00b7ten", "Ne\u00b7bel\u00b7schlei\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Nilusstrand im gelben Sand", "tokens": ["Am", "Ni\u00b7lus\u00b7strand", "im", "gel\u00b7ben", "Sand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entsog ich Gift dem W\u00fcstenbrand", "tokens": ["Ent\u00b7sog", "ich", "Gift", "dem", "W\u00fcs\u00b7ten\u00b7brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und Gift aus Dracheneiern.", "tokens": ["Und", "Gift", "aus", "Dra\u00b7che\u00b7nei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Tal ein und aus, bergauf und ab,", "tokens": ["Tal", "ein", "und", "aus", ",", "ber\u00b7gauf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$,", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich m\u00e4h' zur \u00f6den Heide", "tokens": ["Ich", "m\u00e4h'", "zur", "\u00f6\u00b7den", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt mit meinem Wanderstab,", "tokens": ["Die", "Welt", "mit", "mei\u00b7nem", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich setz' vor jedes Haus ein Grab", "tokens": ["Ich", "setz'", "vor", "je\u00b7des", "Haus", "ein", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und eine Trauerweide.", "tokens": ["Und", "ei\u00b7ne", "Trau\u00b7er\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich bin der gro\u00dfe V\u00f6lkertod,", "tokens": ["Ich", "bin", "der", "gro\u00b7\u00dfe", "V\u00f6l\u00b7ker\u00b7tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin das gro\u00dfe Sterben.", "tokens": ["Ich", "bin", "das", "gro\u00b7\u00dfe", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es geht vor mir die Wassernot,", "tokens": ["Es", "geht", "vor", "mir", "die", "Was\u00b7ser\u00b7not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bringe mit das teure Brot,", "tokens": ["Ich", "brin\u00b7ge", "mit", "das", "teu\u00b7re", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Krieg tu' ich beerben.", "tokens": ["Den", "Krieg", "tu'", "ich", "be\u00b7er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es hilft euch nichts, wie weit ihr floh't,", "tokens": ["Es", "hilft", "euch", "nichts", ",", "wie", "weit", "ihr", "floh't", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin ein schneller Schreiter,", "tokens": ["Ich", "bin", "ein", "schnel\u00b7ler", "Schrei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin der schnelle schwarze Tod,", "tokens": ["Ich", "bin", "der", "schnel\u00b7le", "schwar\u00b7ze", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich \u00fcberhol' das schnellste Boot", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7hol'", "das", "schnells\u00b7te", "Boot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und auch den schnellsten Reiter.", "tokens": ["Und", "auch", "den", "schnells\u00b7ten", "Rei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dem Kaufmann tr\u00e4gt man mich ins Haus", "tokens": ["Dem", "Kauf\u00b7mann", "tr\u00e4gt", "man", "mich", "ins", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zugleich mit seiner Ware;", "tokens": ["Zu\u00b7gleich", "mit", "sei\u00b7ner", "Wa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er freut sich hoch, er lacht beim Schmaus,", "tokens": ["Er", "freut", "sich", "hoch", ",", "er", "lacht", "beim", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich steig' aus seinem Schatz heraus", "tokens": ["Ich", "steig'", "aus", "sei\u00b7nem", "Schatz", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und streck' ihn auf die Bahre.", "tokens": ["Und", "streck'", "ihn", "auf", "die", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mir ist auf hohem Felsvorsprung", "tokens": ["Mir", "ist", "auf", "ho\u00b7hem", "Fels\u00b7vor\u00b7sprung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Schlo\u00df zu hoch, ich komme;", "tokens": ["Kein", "Schlo\u00df", "zu", "hoch", ",", "ich", "kom\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist kein junges Blut zu jung,", "tokens": ["Mir", "ist", "kein", "jun\u00b7ges", "Blut", "zu", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Leib ist mir gesund genung,", "tokens": ["Kein", "Leib", "ist", "mir", "ge\u00b7sund", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir ist kein Herz zu fromme.", "tokens": ["Mir", "ist", "kein", "Herz", "zu", "from\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wem ich nur schau' ins Aug' hinein,", "tokens": ["Wem", "ich", "nur", "schau'", "ins", "Aug'", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mag kein Licht mehr sehen;", "tokens": ["Der", "mag", "kein", "Licht", "mehr", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wem ich gesegnet Brot und Wein,", "tokens": ["Wem", "ich", "ge\u00b7seg\u00b7net", "Brot", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den hungert nur nach Staub allein,", "tokens": ["Den", "hun\u00b7gert", "nur", "nach", "Staub", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den durstet's, heimzugehen.", "tokens": ["Den", "durs\u00b7tet's", ",", "heim\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Im Osten starb der gro\u00dfe Chan,", "tokens": ["Im", "Os\u00b7ten", "starb", "der", "gro\u00b7\u00dfe", "Chan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Indiens Zimmetinseln", "tokens": ["Auf", "In\u00b7di\u00b7ens", "Zim\u00b7me\u00b7tin\u00b7seln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Starb Negerf\u00fcrst und Muselmann,", "tokens": ["Starb", "Ne\u00b7ger\u00b7f\u00fcrst", "und", "Mu\u00b7sel\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Man h\u00f6rt auch Nachts in Ispahan", "tokens": ["Man", "h\u00f6rt", "auch", "Nachts", "in", "Is\u00b7pa\u00b7han"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beim Aas die Hunde winseln.", "tokens": ["Beim", "Aas", "die", "Hun\u00b7de", "win\u00b7seln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Byzanz war eine sch\u00f6ne Stadt,", "tokens": ["By\u00b7zanz", "war", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bl\u00fchend lag Venedig;", "tokens": ["Und", "bl\u00fc\u00b7hend", "lag", "Ve\u00b7ne\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NE", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Nun liegt das Volk wie welkes Blatt,", "tokens": ["Nun", "liegt", "das", "Volk", "wie", "wel\u00b7kes", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wer das Laub zu sammeln hat,", "tokens": ["Und", "wer", "das", "Laub", "zu", "sam\u00b7meln", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird auch der M\u00fche ledig.", "tokens": ["Wird", "auch", "der", "M\u00fc\u00b7he", "le\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "An Nordlands letztem Felsenriff", "tokens": ["An", "Nord\u00b7lands", "letz\u00b7tem", "Fel\u00b7sen\u00b7riff"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einen kleinen Hafen", "tokens": ["In", "ei\u00b7nen", "klei\u00b7nen", "Ha\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Warf ich ein ausgestorbnes Schiff,", "tokens": ["Warf", "ich", "ein", "aus\u00b7ge\u00b7storb\u00b7nes", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles, was mein Hauch ergriff,", "tokens": ["Und", "al\u00b7les", ",", "was", "mein", "Hauch", "er\u00b7griff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das mu\u00dfte schlafen, schlafen.", "tokens": ["Das", "mu\u00df\u00b7te", "schla\u00b7fen", ",", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie liegen in der Stadt umher:", "tokens": ["Sie", "lie\u00b7gen", "in", "der", "Stadt", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob Tag' und Monde schwinden,", "tokens": ["Ob", "Tag'", "und", "Mon\u00b7de", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es z\u00e4hlt kein Mensch die Stunden mehr \u2013", "tokens": ["Es", "z\u00e4hlt", "kein", "Mensch", "die", "Stun\u00b7den", "mehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Jahren wird man \u00f6d' und leer", "tokens": ["Nach", "Jah\u00b7ren", "wird", "man", "\u00f6d'", "und", "leer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PIS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Stadt der Toten finden.", "tokens": ["Die", "Stadt", "der", "To\u00b7ten", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Erzittre Welt, ich bin die Pest,", "tokens": ["Er\u00b7zitt\u00b7re", "Welt", ",", "ich", "bin", "die", "Pest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich komm' in alle Lande", "tokens": ["Ich", "komm'", "in", "al\u00b7le", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und richte mir ein gro\u00dfes Fest,", "tokens": ["Und", "rich\u00b7te", "mir", "ein", "gro\u00b7\u00dfes", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Blick ist Fieber, feuerfest", "tokens": ["Mein", "Blick", "ist", "Fie\u00b7ber", ",", "feu\u00b7er\u00b7fest"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schwarz ist mein Gewande.", "tokens": ["Und",