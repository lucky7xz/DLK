{"dta.poem.9137": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n An eben dieselbe/ als er ihrer gunst versichert ward.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein m\u00e4dgen ist mir gut!", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ob sie gleich zu zeiten/", "tokens": ["Und", "ob", "sie", "gleich", "zu", "zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Absonderlich bey leuten/", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "bey", "leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein bi\u00dfgen spr\u00f6de thut;", "tokens": ["Ein", "bi\u00df\u00b7gen", "spr\u00f6\u00b7de", "thut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So macht ihr wort-gez\u00e4ncke", "tokens": ["So", "macht", "ihr", "wor\u00b7tge\u00b7z\u00e4n\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir gleichwohl einen muth/", "tokens": ["Mir", "gleich\u00b7wohl", "ei\u00b7nen", "muth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich im hertzen dencke/", "tokens": ["Da\u00df", "ich", "im", "hert\u00b7zen", "den\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein m\u00e4dgen ist mir gut.", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Mein m\u00e4dgen ist mir gut.", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Manch jahr ist ja vergangen/", "tokens": ["Manch", "jahr", "ist", "ja", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seyt mein geneigt verlangen", "tokens": ["Seyt", "mein", "ge\u00b7neigt", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ihrer freundschafft ruht/", "tokens": ["In", "ih\u00b7rer", "freund\u00b7schafft", "ruht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie solt ich dann nicht wissen", "tokens": ["Wie", "solt", "ich", "dann", "nicht", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Indem sie anders thut/", "tokens": ["In\u00b7dem", "sie", "an\u00b7ders", "thut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie weit ich k\u00f6nne schliessen/", "tokens": ["Wie", "weit", "ich", "k\u00f6n\u00b7ne", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein m\u00e4dgen sey mir gut.", "tokens": ["Mein", "m\u00e4d\u00b7gen", "sey", "mir", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Mein m\u00e4dgen ist mir gut!", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein lassen und beginnen/", "tokens": ["Mein", "las\u00b7sen", "und", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ihren zarten sinnen", "tokens": ["Das", "ih\u00b7ren", "zar\u00b7ten", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar nichts zu wider thut/", "tokens": ["Gar", "nichts", "zu", "wi\u00b7der", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKZU", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bewegt mir auch das hertze", "tokens": ["Be\u00b7wegt", "mir", "auch", "das", "hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie eine sanffte fluth/", "tokens": ["Wie", "ei\u00b7ne", "sanff\u00b7te", "fluth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich mit freuden schertze/", "tokens": ["Da\u00df", "ich", "mit", "freu\u00b7den", "schert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein m\u00e4dgen ist mir gut.", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Mein m\u00e4dgen ist mir gut!", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie macht ihr amts-gesichte", "tokens": ["Sie", "macht", "ihr", "amts\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vi\u00dfweilen noch zu nichte/", "tokens": ["Vi\u00df\u00b7wei\u00b7len", "noch", "zu", "nich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "PIS", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und r\u00fchret mir das blut", "tokens": ["Und", "r\u00fch\u00b7ret", "mir", "das", "blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch liebgesinnte blicke:", "tokens": ["Durch", "lieb\u00b7ge\u00b7sinn\u00b7te", "bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum spricht mein freyer muth/", "tokens": ["Drum", "spricht", "mein", "frey\u00b7er", "muth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich habe noch das gl\u00fccke/", "tokens": ["Ich", "ha\u00b7be", "noch", "das", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein m\u00e4dgen ist mir gut.", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Mein m\u00e4dgen ist mir gut!", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein ander mag sich kr\u00e4ncken/", "tokens": ["Ein", "an\u00b7der", "mag", "sich", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und voller furcht gedencken/", "tokens": ["Und", "vol\u00b7ler", "furcht", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was seine Nymfe thut;", "tokens": ["Was", "sei\u00b7ne", "Nym\u00b7fe", "thut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kan die sorgen brechen/", "tokens": ["Ich", "kan", "die", "sor\u00b7gen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Denn mein vergn\u00fcgter muth", "tokens": ["Denn", "mein", "ver\u00b7gn\u00fcg\u00b7ter", "muth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kan in der warheit sprechen/", "tokens": ["Kan", "in", "der", "war\u00b7heit", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein m\u00e4dgen ist mir gut.", "tokens": ["Mein", "m\u00e4d\u00b7gen", "ist", "mir", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}