{"textgrid.poem.52901": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00fc\u00df, verehrter Kabinetts-Minister,", "tokens": ["Wie", "s\u00fc\u00df", ",", "ver\u00b7ehr\u00b7ter", "Ka\u00b7bi\u00b7netts\u00b7Mi\u00b7nis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Klingt, im Vergleich zu P\u00f6beltums-Gekreisch,", "tokens": ["Klingt", ",", "im", "Ver\u00b7gleich", "zu", "P\u00f6\u00b7bel\u00b7tums\u00b7Ge\u00b7kreisch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6chst-Ihrer Laute liebliches Geflister", "tokens": ["H\u00f6chst\u00b7Ih\u00b7rer", "Lau\u00b7te", "lieb\u00b7li\u00b7ches", "Ge\u00b7flis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Tee mit Butterbrot und Hammelfleisch!", "tokens": ["Zu", "Tee", "mit", "But\u00b7ter\u00b7brot", "und", "Ham\u00b7mel\u00b7fleisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mag auch die Aftermuse mit Ger\u00e4usch", "tokens": ["Mag", "auch", "die", "Af\u00b7ter\u00b7mu\u00b7se", "mit", "Ge\u00b7r\u00e4usch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Losziehen wider Schlendrian und Philister,", "tokens": ["Los\u00b7zie\u00b7hen", "wi\u00b7der", "Schlen\u00b7dri\u00b7an", "und", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Ihrige bleibt mild, loyal und keusch,", "tokens": ["Die", "Ih\u00b7ri\u00b7ge", "bleibt", "mild", ",", "lo\u00b7yal", "und", "keusch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie und der Schlaf sind leibliche Geschwister.", "tokens": ["Sie", "und", "der", "Schlaf", "sind", "leib\u00b7li\u00b7che", "Ge\u00b7schwis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gebenedeit das Land vor allen L\u00e4ndern,", "tokens": ["Ge\u00b7be\u00b7ne\u00b7deit", "das", "Land", "vor", "al\u00b7len", "L\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das R\u00e4t' und Richter schm\u00fcckt mit Lorbeerzweigen", "tokens": ["Das", "R\u00e4t'", "und", "Rich\u00b7ter", "schm\u00fcckt", "mit", "Lor\u00b7beer\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Dichter mit Minister-Ordens-B\u00e4ndern!", "tokens": ["Und", "Dich\u00b7ter", "mit", "Mi\u00b7nis\u