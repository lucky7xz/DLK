{"textgrid.poem.64275": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Zum Ged\u00e4chtnis Kaiser Wilhelms I", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Kaiser Wilhelm, wir gedenken dein!", "tokens": ["O", "Kai\u00b7ser", "Wil\u00b7helm", ",", "wir", "ge\u00b7den\u00b7ken", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "PPER", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Liebe f\u00fcllt und Wehmut, Stolz und Trauer", "tokens": ["Und", "Lie\u00b7be", "f\u00fcllt", "und", "Weh\u00b7mut", ",", "Stolz", "und", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zugleich das Herz uns. \u2013 Eine ganze Welt", "tokens": ["Zu\u00b7gleich", "das", "Herz", "uns", ".", "\u2013", "Ei\u00b7ne", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPER", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Steigt vor uns auf, die mit dir sank ins Grab.", "tokens": ["Steigt", "vor", "uns", "auf", ",", "die", "mit", "dir", "sank", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir sehn dich unverstanden, viel geschm\u00e4ht,", "tokens": ["Wir", "sehn", "dich", "un\u00b7ver\u00b7stan\u00b7den", ",", "viel", "ge\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ehrne R\u00fcstzeug schmieden deinem Preu\u00dfen;", "tokens": ["Das", "ehr\u00b7ne", "R\u00fcst\u00b7zeug", "schmie\u00b7den", "dei\u00b7nem", "Preu\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sehn das Schwert dich dreimal ziehn \u2013 stets z\u00f6gernd \u2013", "tokens": ["Wir", "sehn", "das", "Schwert", "dich", "drei\u00b7mal", "ziehn", "\u2013", "stets", "z\u00f6\u00b7gernd", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$(", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stets zum Sieg. \u2013 Wir sehn den Herzenstraum \u2013", "tokens": ["Und", "stets", "zum", "Sieg", ".", "\u2013", "Wir", "sehn", "den", "Her\u00b7zen\u00b7straum", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Traum vom Reich! \u2013 glorreich erf\u00fcllt durch dich!", "tokens": ["Den", "Traum", "vom", "Reich", "!", "\u2013", "glor\u00b7reich", "er\u00b7f\u00fcllt", "durch", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$(", "ADJD", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Seitdem fast wie ein K\u00f6nigsbild der Vorzeit,", "tokens": ["Seit\u00b7dem", "fast", "wie", "ein", "K\u00f6\u00b7nigs\u00b7bild", "der", "Vor\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Standst, Barbablanca, du in dieser Welt:", "tokens": ["Standst", ",", "Bar\u00b7bab\u00b7lan\u00b7ca", ",", "du", "in", "die\u00b7ser", "Welt", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Sage schlang schon um den Lebenden", "tokens": ["Die", "Sa\u00b7ge", "schlang", "schon", "um", "den", "Le\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Leis' fl\u00fcsternd hin ihr efeu-gr\u00fcn Gerank;", "tokens": ["Leis'", "fl\u00fcs\u00b7ternd", "hin", "ihr", "e\u00b7feu\u00b7gr\u00fcn", "Ge\u00b7rank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als seinen Vater liebte dich dein Volk", "tokens": ["Als", "sei\u00b7nen", "Va\u00b7ter", "lieb\u00b7te", "dich", "dein", "Volk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und unter deinem starken Schilde barg", "tokens": ["Und", "un\u00b7ter", "dei\u00b7nem", "star\u00b7ken", "Schil\u00b7de", "barg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Europas Friede das bedrohte Haupt. \u2013 \u2013", "tokens": ["Eu\u00b7ro\u00b7pas", "Frie\u00b7de", "das", "be\u00b7droh\u00b7te", "Haupt", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und dies geweihte Haupt, das heil'ger noch", "tokens": ["Und", "dies", "ge\u00b7weih\u00b7te", "Haupt", ",", "das", "heil'\u00b7ger", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN", "$,", "ART", "ADJA", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Kronengold des Alters Silber sch\u00fctzte,", "tokens": ["Als", "Kro\u00b7nen\u00b7gold", "des", "Al\u00b7ters", "Sil\u00b7ber", "sch\u00fctz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Mordfaust hat's bedroht, es flo\u00df dein Blut.", "tokens": ["Die", "Mord\u00b7faust", "hat's", "be\u00b7droht", ",", "es", "flo\u00df", "dein", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und was war deine Rache? \u2013 W\u00e4rmer noch,", "tokens": ["Und", "was", "war", "dei\u00b7ne", "Ra\u00b7che", "?", "\u2013", "W\u00e4r\u00b7mer", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPOSAT", "NN", "$.", "$(", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als je, werkt\u00e4t'ger liebtest du dein Volk", "tokens": ["Als", "je", ",", "werk\u00b7t\u00e4t'\u00b7ger", "lieb\u00b7test", "du", "dein", "Volk"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und zogst die Armen, Notbeladenen", "tokens": ["Und", "zogst", "die", "Ar\u00b7men", ",", "Not\u00b7be\u00b7la\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Erbarmend an dein gro\u00dfes K\u00f6nigsherz!", "tokens": ["Er\u00b7bar\u00b7mend", "an", "dein", "gro\u00b7\u00dfes", "K\u00f6\u00b7nigs\u00b7herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So woll'n auch wir tun, wenn das Scheu\u00dfliche,", "tokens": ["So", "woll'n", "auch", "wir", "tun", ",", "wenn", "das", "Scheu\u00df\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "VVINF", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das wir erleben, uns ergrimmen will,", "tokens": ["Das", "wir", "er\u00b7le\u00b7ben", ",", "uns", "er\u00b7grim\u00b7men", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn viele, viele Tausende im Volk \u2013", "tokens": ["Wenn", "vie\u00b7le", ",", "vie\u00b7le", "Tau\u00b7sen\u00b7de", "im", "Volk", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PIAT", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kaum zwei Jahrzehnte nur nach solchen Taten! \u2013", "tokens": ["Kaum", "zwei", "Jahr\u00b7zehn\u00b7te", "nur", "nach", "sol\u00b7chen", "Ta\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "CARD", "NN", "ADV", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zertr\u00fcmmern woll'n dies Reich, sich selber sch\u00e4nden", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mern", "woll'n", "dies", "Reich", ",", "sich", "sel\u00b7ber", "sch\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PDS", "NN", "$,", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und die Germania auf dem Niederwald", "tokens": ["Und", "die", "Ger\u00b7ma\u00b7nia", "auf", "dem", "Nie\u00b7der\u00b7wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Zuliebe den Franzosen nieder rei\u00dfen!", "tokens": ["Zu\u00b7lie\u00b7be", "den", "Fran\u00b7zo\u00b7sen", "nie\u00b7der", "rei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn Ekel uns und Abscheu fassen will", "tokens": ["Wenn", "E\u00b7kel", "uns", "und", "Ab\u00b7scheu", "fas\u00b7sen", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "KON", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor solchem Undank, solcher Niedertracht,", "tokens": ["Vor", "sol\u00b7chem", "Un\u00b7dank", ",", "sol\u00b7cher", "Nie\u00b7der\u00b7tracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann woll'n wir dein, o Kaiser Wilhelm, denken!", "tokens": ["Dann", "woll'n", "wir", "dein", ",", "o", "Kai\u00b7ser", "Wil\u00b7helm", ",", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "$,", "FM", "NN", "NE", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als zweimal dich die M\u00f6rderhand bedroht,", "tokens": ["Als", "zwei\u00b7mal", "dich", "die", "M\u00f6r\u00b7der\u00b7hand", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hast du dich nicht mit Abscheu abgewandt,", "tokens": ["Hast", "du", "dich", "nicht", "mit", "Ab\u00b7scheu", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hast dich von deinem Thron herabgeneigt", "tokens": ["Hast", "dich", "von", "dei\u00b7nem", "Thron", "her\u00b7ab\u00b7ge\u00b7neigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und schirmend deine H\u00e4nde ausgestreckt.", "tokens": ["Und", "schir\u00b7mend", "dei\u00b7ne", "H\u00e4n\u00b7de", "aus\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wem unter uns ward weh getan wie dir?", "tokens": ["Wem", "un\u00b7ter", "uns", "ward", "weh", "ge\u00b7tan", "wie", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VAFIN", "ADV", "VVPP", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nein, dein Ged\u00e4chtnis ehren wir am treusten,", "tokens": ["Nein", ",", "dein", "Ge\u00b7d\u00e4cht\u00b7nis", "eh\u00b7ren", "wir", "am", "treus\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn wir \u2013 gleich dir! \u2013 im Ringen nicht erm\u00fcden", "tokens": ["Wenn", "wir", "\u2013", "gleich", "dir", "!", "\u2013", "im", "Rin\u00b7gen", "nicht", "er\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "ADV", "PPER", "$.", "$(", "APPRART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr dies bet\u00f6rte Volk.", "tokens": ["F\u00fcr", "dies", "be\u00b7t\u00f6r\u00b7te", "Volk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Kaiser Wilhelm, wir gedenken dein!", "tokens": ["O", "Kai\u00b7ser", "Wil\u00b7helm", ",", "wir", "ge\u00b