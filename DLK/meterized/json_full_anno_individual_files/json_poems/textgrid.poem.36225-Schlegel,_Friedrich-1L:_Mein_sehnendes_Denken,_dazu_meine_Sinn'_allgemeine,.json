{"textgrid.poem.36225": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein sehnendes Denken, dazu meine Sinn' allgemeine,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein sehnendes Denken, dazu meine Sinn' allgemeine,", "tokens": ["Mein", "seh\u00b7nen\u00b7des", "Den\u00b7ken", ",", "da\u00b7zu", "mei\u00b7ne", "Sinn'", "all\u00b7ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PAV", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf Eines sich lenken, besorgen einzig das Eine,", "tokens": ["Auf", "Ei\u00b7nes", "sich", "len\u00b7ken", ",", "be\u00b7sor\u00b7gen", "ein\u00b7zig", "das", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PRF", "VVINF", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie ich Ihr bescheine,", "tokens": ["Wie", "ich", "Ihr", "be\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df ich schon lange mit Sange sie meine,", "tokens": ["Da\u00df", "ich", "schon", "lan\u00b7ge", "mit", "San\u00b7ge", "sie", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In stetem Mute, sie gute, sie reine.", "tokens": ["In", "ste\u00b7tem", "Mu\u00b7te", ",", "sie", "gu\u00b7te", ",", "sie", "rei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "ADJA", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Selig in Freuden ich w\u00e4re, der reichste an Gute,", "tokens": ["Se\u00b7lig", "in", "Freu\u00b7den", "ich", "w\u00e4\u00b7re", ",", "der", "reichs\u00b7te", "an", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PPER", "VAFIN", "$,", "ART", "ADJA", "APPR", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Wollte mein Leiden bedenken die wohlgemute,", "tokens": ["Woll\u00b7te", "mein", "Lei\u00b7den", "be\u00b7den\u00b7ken", "die", "wohl\u00b7ge\u00b7mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "ART", "ADJA", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Vor Falschem behute;", "tokens": ["Vor", "Fal\u00b7schem", "be\u00b7hu\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und m\u00f6cht' es gelingen mit Singen dem Mute,", "tokens": ["Und", "m\u00f6cht'", "es", "ge\u00b7lin\u00b7gen", "mit", "Sin\u00b7gen", "dem", "Mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVPP", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da\u00df sie mein h\u00fcte mit G\u00fcte, sie liebe, sie gute.", "tokens": ["Da\u00df", "sie", "mein", "h\u00fc\u00b7te", "mit", "G\u00fc\u00b7te", ",", "sie", "lie\u00b7be", ",", "sie", "