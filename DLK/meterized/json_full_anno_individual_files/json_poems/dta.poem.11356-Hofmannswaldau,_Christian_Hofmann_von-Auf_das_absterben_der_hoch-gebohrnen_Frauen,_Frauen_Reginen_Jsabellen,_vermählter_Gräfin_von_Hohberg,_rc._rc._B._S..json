{"dta.poem.11356": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben der hoch-gebohrnen  \n Frauen, Frauen Reginen Jsabellen,  \n verm\u00e4hlter Gr\u00e4fin von  \n Hohberg, rc. rc.  \n B. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bethr\u00e4nter F\u00fcrstenstein! es schallet noch die stimme,", "tokens": ["Be\u00b7thr\u00e4n\u00b7ter", "F\u00fcrs\u00b7ten\u00b7stein", "!", "es", "schal\u00b7let", "noch", "die", "stim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dir vor kurtzer zeit ein harter bote war.", "tokens": ["Die", "dir", "vor", "kurt\u00b7zer", "zeit", "ein", "har\u00b7ter", "bo\u00b7te", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es zittert dein pallast noch vor des todes grimme,", "tokens": ["Es", "zit\u00b7tert", "dein", "pal\u00b7last", "noch", "vor", "des", "to\u00b7des", "grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entsetzen, furcht und angst stellt dir ein Nebo dar.", "tokens": ["Ent\u00b7set\u00b7zen", ",", "furcht", "und", "angst", "stellt", "dir", "ein", "Ne\u00b7bo", "dar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJD", "VVFIN", "PPER", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch deine herrlichkeit ward durch die trauer-wolcke", "tokens": ["Auch", "dei\u00b7ne", "herr\u00b7lich\u00b7keit", "ward", "durch", "die", "trau\u00b7e\u00b7rwol\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In tieffstes leid verh\u00fcllt, in eitel nacht versenckt.", "tokens": ["In", "tieffs\u00b7tes", "leid", "ver\u00b7h\u00fcllt", ",", "in", "ei\u00b7tel", "nacht", "ver\u00b7senckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein gipffel, der zuvor mit fr\u00f6lichkeit getr\u00e4nckt,", "tokens": ["Dein", "gipf\u00b7fel", ",", "der", "zu\u00b7vor", "mit", "fr\u00f6\u00b7lich\u00b7keit", "ge\u00b7tr\u00e4nckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ward bald in thr\u00e4nenna\u00df; ja du mit deinem volcke", "tokens": ["Ward", "bald", "in", "thr\u00e4\u00b7nen\u00b7na\u00df", ";", "ja", "du", "mit", "dei\u00b7nem", "vol\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NE", "$.", "PTKANT", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Stundst, wie durch einen blitz verstarret und ger\u00fchrt,", "tokens": ["Stundst", ",", "wie", "durch", "ei\u00b7nen", "blitz", "ver\u00b7star\u00b7ret", "und", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil dir der fr\u00fche tod ach! gar zu viel entf\u00fchrt.", "tokens": ["Weil", "dir", "der", "fr\u00fc\u00b7he", "tod", "ach", "!", "gar", "zu", "viel", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ITJ", "$.", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man h\u00f6rt das echo noch in deinen th\u00e4lern weinen:", "tokens": ["Man", "h\u00f6rt", "das", "ec\u00b7ho", "noch", "in", "dei\u00b7nen", "th\u00e4\u00b7lern", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O schmertzlicher verlust nach kurtzen sonnen-scheinen!", "tokens": ["O", "schmertz\u00b7li\u00b7cher", "ver\u00b7lust", "nach", "kurt\u00b7zen", "son\u00b7nen\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Hohberg satzte sich tieff in den staub darnieder;", "tokens": ["Dein", "Hoh\u00b7berg", "satz\u00b7te", "sich", "tieff", "in", "den", "staub", "dar\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ART", "VVFIN", "PAV", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Bey dieser leiche starb, was vor sein leben hie\u00df.", "tokens": ["Bey", "die\u00b7ser", "lei\u00b7che", "starb", ",", "was", "vor", "sein", "le\u00b7ben", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "VVFIN", "$,", "PRELS", "APPR", "PPOSAT", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der schmertz gieng bi\u00df ins hertz durch die erstarrten glieder;", "tokens": ["Der", "schmertz", "gieng", "bi\u00df", "ins", "hertz", "durch", "die", "er\u00b7starr\u00b7ten", "glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es ward ein thr\u00e4nen-feld, wo vor ein paradies.", "tokens": ["Es", "ward", "ein", "thr\u00e4\u00b7nen\u00b7feld", ",", "wo", "vor", "ein", "pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und welche feder mag so eine schrifft verfassen,", "tokens": ["Und", "wel\u00b7che", "fe\u00b7der", "mag", "so", "ei\u00b7ne", "schrifft", "ver\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VMFIN", "ADV", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ob blut und z\u00e4hren gleich zur dinte worden seyn,", "tokens": ["Ob", "blut", "und", "z\u00e4h\u00b7ren", "gleich", "zur", "din\u00b7te", "wor\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "ADV", "APPRART", "NN", "VAPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die uns ein solches leid und solche hertzens-pein", "tokens": ["Die", "uns", "ein", "sol\u00b7ches", "leid", "und", "sol\u00b7che", "hert\u00b7zens\u00b7pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "PIAT", "ADJD", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nach ihrer hefftigkeit kan deutlich lesen lassen?", "tokens": ["Nach", "ih\u00b7rer", "heff\u00b7tig\u00b7keit", "kan", "deut\u00b7lich", "le\u00b7sen", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Schreib an, o F\u00fcrstenstein! den tag als einen tag,", "tokens": ["Schreib", "an", ",", "o", "F\u00fcrs\u00b7ten\u00b7stein", "!", "den", "tag", "als", "ei\u00b7nen", "tag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$.", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den keine schwartze schrifft nicht gnug bezeichnen mag.", "tokens": ["Den", "kei\u00b7ne", "schwart\u00b7ze", "schrifft", "nicht", "gnug", "be\u00b7zeich\u00b7nen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PIAT", "ADJA", "VVFIN", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wie solte nicht ein flor um deine scheitel hangen,", "tokens": ["Wie", "sol\u00b7te", "nicht", "ein", "flor", "um", "dei\u00b7ne", "schei\u00b7tel", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "PTKVZ", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil deine sonne dir im mittag untergangen?", "tokens": ["Weil", "dei\u00b7ne", "son\u00b7ne", "dir", "im", "mit\u00b7tag", "un\u00b7ter\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie Welschland itzt erstaunt der berge ri\u00df beschauet,", "tokens": ["Wie", "Wel\u00b7schland", "itzt", "er\u00b7staunt", "der", "ber\u00b7ge", "ri\u00df", "be\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie Napoli best\u00fcrtzt die felsen beben h\u00f6rt,", "tokens": ["Wie", "Na\u00b7po\u00b7li", "be\u00b7st\u00fcrtzt", "die", "fel\u00b7sen", "be\u00b7ben", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Wenn dorten Gibellin nur pech und schwefel thauet,", "tokens": ["Wenn", "dor\u00b7ten", "Gi\u00b7bel\u00b7lin", "nur", "pech", "und", "schwe\u00b7fel", "thau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und seines nachbars land mit asch und graus zerst\u00f6rt:", "tokens": ["Und", "sei\u00b7nes", "nach\u00b7bars", "land", "mit", "asch", "und", "graus", "zer\u00b7st\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "So sieht man auch um dich der unterthauen h\u00fctten", "tokens": ["So", "sieht", "man", "auch", "um", "dich", "der", "un\u00b7ter\u00b7thau\u00b7en", "h\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Voll angst und traurigkeit, da so ein ri\u00df geschieht,", "tokens": ["Voll", "angst", "und", "trau\u00b7rig\u00b7keit", ",", "da", "so", "ein", "ri\u00df", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "NN", "$,", "KOUS", "ADV", "ART", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "In den man ihren schatz mit leid verborgen sieht.", "tokens": ["In", "den", "man", "ih\u00b7ren", "schatz", "mit", "leid", "ver\u00b7bor\u00b7gen", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PPOSAT", "NN", "APPR", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der himmel ward ein fels, und lie\u00df sich nicht erbitten,", "tokens": ["Der", "him\u00b7mel", "ward", "ein", "fels", ",", "und", "lie\u00df", "sich", "nicht", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der schlu\u00df war einmahl da: Du soltest, F\u00fcrstenstein!", "tokens": ["Der", "schlu\u00df", "war", "ein\u00b7mahl", "da", ":", "Du", "sol\u00b7test", ",", "F\u00fcrs\u00b7ten\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$.", "PPER", "VMFIN", "$,", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.34": {"text": "Ein schau-platz kurtzer lust und langen traurens seyn,", "tokens": ["Ein", "schau\u00b7platz", "kurt\u00b7zer", "lust", "und", "lan\u00b7gen", "trau\u00b7rens", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein stein, den k\u00fcnfftig man zum marmor solte setzen,", "tokens": ["Ein", "stein", ",", "den", "k\u00fcnff\u00b7tig", "man", "zum", "mar\u00b7mor", "sol\u00b7te", "set\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PIS", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und nichts als ach! und ach! darein zum denckmahl \u00e4tzen.", "tokens": ["Und", "nichts", "als", "ach", "!", "und", "ach", "!", "da\u00b7rein", "zum", "denck\u00b7mahl", "\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ITJ", "$.", "KON", "XY", "$.", "PAV", "APPRART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch wo verfall ich hin? Hinweg mit den gedancken!", "tokens": ["Doch", "wo", "ver\u00b7fall", "ich", "hin", "?", "Hin\u00b7weg", "mit", "den", "ge\u00b7dan\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die wie ein neuer ri\u00df in frische wunden gehn.", "tokens": ["Die", "wie", "ein", "neu\u00b7er", "ri\u00df", "in", "fri\u00b7sche", "wun\u00b7den", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Beflorter F\u00fcrstenstein! enth\u00fclle deine schrancken,", "tokens": ["Be\u00b7flor\u00b7ter", "F\u00fcrs\u00b7ten\u00b7stein", "!", "ent\u00b7h\u00fcl\u00b7le", "dei\u00b7ne", "schran\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und lasse nicht dein haupt in solchem nebel stehn!", "tokens": ["Und", "las\u00b7se", "nicht", "dein", "haupt", "in", "sol\u00b7chem", "ne\u00b7bel", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es ist ja freylich so: Dein schmuck ist asche worden,", "tokens": ["Es", "ist", "ja", "frey\u00b7lich", "so", ":", "Dein", "schmuck", "ist", "asc\u00b7he", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$.", "PPOSAT", "ADJD", "VAFIN", "ADJA", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "In deinen th\u00e4lern rauscht gerechter thr\u00e4nen bach,", "tokens": ["In", "dei\u00b7nen", "th\u00e4\u00b7lern", "rauscht", "ge\u00b7rech\u00b7ter", "thr\u00e4\u00b7nen", "bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Gar billig rufft ein berg dem anderen mit ach!", "tokens": ["Gar", "bil\u00b7lig", "rufft", "ein", "berg", "dem", "an\u00b7de\u00b7ren", "mit", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "APPR", "ITJ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.44": {"text": "Und wer nur um dich wohnt, erwehlt den jammer-orden,", "tokens": ["Und", "wer", "nur", "um", "dich", "wohnt", ",", "er\u00b7wehlt", "den", "jam\u00b7mer\u00b7or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Doch wie dein scheitel auch bi\u00df an die wolcken steht,", "tokens": ["Doch", "wie", "dein", "schei\u00b7tel", "auch", "bi\u00df", "an", "die", "wol\u00b7cken", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und also dein gesicht stets nach der h\u00f6he geht;", "tokens": ["Und", "al\u00b7so", "dein", "ge\u00b7sicht", "stets", "nach", "der", "h\u00f6\u00b7he", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJD", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So dencke, da\u00df der schlag, der dich so hart zerspalten,", "tokens": ["So", "den\u00b7cke", ",", "da\u00df", "der", "schlag", ",", "der", "dich", "so", "hart", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Von niemand, als von GOtt, den nachdruck hat erhalten.", "tokens": ["Von", "nie\u00b7mand", ",", "als", "von", "Gott", ",", "den", "nach\u00b7druck", "hat", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "APPR", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Der himmel hatte dir ein kleinod anvertrauet,", "tokens": ["Der", "him\u00b7mel", "hat\u00b7te", "dir", "ein", "klei\u00b7nod", "an\u00b7ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die gr\u00e4fin, die man stets vor unvergleichlich hielt,", "tokens": ["Die", "gr\u00e4\u00b7fin", ",", "die", "man", "stets", "vor", "un\u00b7ver\u00b7gleich\u00b7lich", "hielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "An welcher die natur gantz nichts umsonst gebauet,", "tokens": ["An", "wel\u00b7cher", "die", "na\u00b7tur", "gantz", "nichts", "um\u00b7sonst", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Die crone des geschlechts, der tugend ebenbild.", "tokens": ["Die", "cro\u00b7ne", "des", "ge\u00b7schlechts", ",", "der", "tu\u00b7gend", "e\u00b7ben\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "In ihr war fr\u00f6mmigkeit, und ein rechtschaffen wesen:", "tokens": ["In", "ihr", "war", "fr\u00f6m\u00b7mig\u00b7keit", ",", "und", "ein", "recht\u00b7schaf\u00b7fen", "we\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "$,", "KON", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Das hielte sie vor schmuck, was sie in Christo fand,", "tokens": ["Das", "hiel\u00b7te", "sie", "vor", "schmuck", ",", "was", "sie", "in", "Chris\u00b7to", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ADJD", "$,", "PRELS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Jhr hertze war wohl recht der sanfftmuth vaterland.", "tokens": ["Ihr", "hert\u00b7ze", "war", "wohl", "recht", "der", "sanfft\u00b7muth", "va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die gr\u00e4fin hatte dir dein Conrad auserlesen,", "tokens": ["Die", "gr\u00e4\u00b7fin", "hat\u00b7te", "dir", "dein", "Con\u00b7rad", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Du nahmst auch diesen schatz wie einen engel an,", "tokens": ["Du", "nahmst", "auch", "die\u00b7sen", "schatz", "wie", "ei\u00b7nen", "en\u00b7gel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "NN", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Die h\u00fcgel h\u00fcpffeten, und auch dein unterthan,", "tokens": ["Die", "h\u00fc\u00b7gel", "h\u00fcpf\u00b7fe\u00b7ten", ",", "und", "auch", "dein", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Die h\u00e4nde sahe man von allen unterlegen,", "tokens": ["Die", "h\u00e4n\u00b7de", "sa\u00b7he", "man", "von", "al\u00b7len", "un\u00b7ter\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Begl\u00fcckter F\u00fcrstenstein! so rieff man allerwegen.", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "F\u00fcrs\u00b7ten\u00b7stein", "!", "so", "rieff", "man", "al\u00b7ler\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Ach aber! wie so bald ward hertz und schatz begraben!", "tokens": ["Ach", "a\u00b7ber", "!", "wie", "so", "bald", "ward", "hertz", "und", "schatz", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PWAV", "ADV", "ADV", "VAFIN", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Du sahest kaum dein licht, so war die nacht schon da.", "tokens": ["Du", "sa\u00b7hest", "kaum", "dein", "licht", ",", "so", "war", "die", "nacht", "schon", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Wiewohl, besinne dich! Es ist der H\u00f6chste ja,", "tokens": ["Wie\u00b7wohl", ",", "be\u00b7sin\u00b7ne", "dich", "!", "Es", "ist", "der", "H\u00f6chs\u00b7te", "ja", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ART", "ADJA", "PTKANT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Der dir die gr\u00e4fin gab, und auch will wieder haben.", "tokens": ["Der", "dir", "die", "gr\u00e4\u00b7fin", "gab", ",", "und", "auch", "will", "wie\u00b7der", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Das, was sie itzt besitzt, ist auch ihr F\u00fcrstenstein,", "tokens": ["Das", ",", "was", "sie", "itzt", "be\u00b7sitzt", ",", "ist", "auch", "ihr", "F\u00fcrs\u00b7ten\u00b7stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Den eckstein hat sie l\u00e4ngst im glauben lieb gewonnen,", "tokens": ["Den", "ecks\u00b7tein", "hat", "sie", "l\u00e4ngst", "im", "glau\u00b7ben", "lieb", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPRART", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Auf diesem steine schlief sie Jacobitisch ein;", "tokens": ["Auf", "die\u00b7sem", "stei\u00b7ne", "schlief", "sie", "Ja\u00b7co\u00b7bi\u00b7tisch", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Noch n\u00e4her, als bey dir, ist sie nun bey der sonnen.", "tokens": ["Noch", "n\u00e4\u00b7her", ",", "als", "bey", "dir", ",", "ist", "sie", "nun", "bey", "der", "son\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "PPER", "$,", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.69": {"text": "Bedencke diesen zug! Zwar du verliehrest viel;", "tokens": ["Be\u00b7den\u00b7cke", "die\u00b7sen", "zug", "!", "Zwar", "du", "ver\u00b7lieh\u00b7rest", "viel", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$.", "ADV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Doch du verliehrest nichts, weil GOtt dich tr\u00f6sten will.", "tokens": ["Doch", "du", "ver\u00b7lieh\u00b7rest", "nichts", ",", "weil", "Gott", "dich", "tr\u00f6s\u00b7ten", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "$,", "KOUS", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Dein Hohberg darff nur hin nach hohen bergen sehen,", "tokens": ["Dein", "Hoh\u00b7berg", "darff", "nur", "hin", "nach", "ho\u00b7hen", "ber\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Gott l\u00e4st ihm zwar sehr weh, doch auch zum wohl, geschehen.", "tokens": ["Gott", "l\u00e4st", "ihm", "zwar", "sehr", "weh", ",", "doch", "auch", "zum", "wohl", ",", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "ADV", "ADV", "APPRART", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Soll dir ein denckmahl seyn in deinen fels gegraben?", "tokens": ["Soll", "dir", "ein", "denck\u00b7mahl", "seyn", "in", "dei\u00b7nen", "fels", "ge\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "So merck ihr ende nur, das ungemeine war.", "tokens": ["So", "merck", "ihr", "en\u00b7de", "nur", ",", "das", "un\u00b7ge\u00b7mei\u00b7ne", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "ADV", "$,", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Hier wirst du balsam gnung auch von der wehmuth haben,", "tokens": ["Hier", "wirst", "du", "bal\u00b7sam", "gnung", "auch", "von", "der", "weh\u00b7muth", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Es stellt sich selbst der tod zu deinem troste dar.", "tokens": ["Es", "stellt", "sich", "selbst", "der", "tod", "zu", "dei\u00b7nem", "tros\u00b7te", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Wie freudig war der geist, der recht nach Zion leckte!", "tokens": ["Wie", "freu\u00b7dig", "war", "der", "geist", ",", "der", "recht", "nach", "Zi\u00b7on", "leck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Wie willig ri\u00df das hertz sich von der erden los!", "tokens": ["Wie", "wil\u00b7lig", "ri\u00df", "das", "hertz", "sich", "von", "der", "er\u00b7den", "los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.79": {"text": "Was auf der zunge lag, das war ihr JEsus blos,", "tokens": ["Was", "auf", "der", "zun\u00b7ge", "lag", ",", "das", "war", "ihr", "Je\u00b7sus", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Vor dessen s\u00fc\u00dfigkeit sie keine schmertzen schmeckte:", "tokens": ["Vor", "des\u00b7sen", "s\u00fc\u00b7\u00dfig\u00b7keit", "sie", "kei\u00b7ne", "schmert\u00b7zen", "schmeck\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "PIAT", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Die schl\u00e4ge hie\u00df sie gunst, die GOtt zu f\u00fchlen gab:", "tokens": ["Die", "schl\u00e4\u00b7ge", "hie\u00df", "sie", "gunst", ",", "die", "Gott", "zu", "f\u00fch\u00b7len", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Jhr siech-bett GOttes hand, und ihre lust das grab:", "tokens": ["Ihr", "siech\u00b7bett", "Got\u00b7tes", "hand", ",", "und", "ih\u00b7re", "lust", "das", "grab", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "NN", "$,", "KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Jhr mund troff stets von blut, das JEsus lassen fliessen:", "tokens": ["Ihr", "mund", "troff", "stets", "von", "blut", ",", "das", "Je\u00b7sus", "las\u00b7sen", "flies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$,", "PRELS", "NE", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Von diesem wolte sie, und sonst von gar nichts wissen.", "tokens": ["Von", "die\u00b7sem", "wol\u00b7te", "sie", ",", "und", "sonst", "von", "gar", "nichts", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "$,", "KON", "ADV", "APPR", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "So war sie himmlisch schon, ob sie gleich noch auf erden,", "tokens": ["So", "war", "sie", "himm\u00b7lisch", "schon", ",", "ob", "sie", "gleich", "noch", "auf", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.86": {"text": "Jhr Conrad schien ihr wohl ein zug noch in der welt;", "tokens": ["Ihr", "Con\u00b7rad", "schien", "ihr", "wohl", "ein", "zug", "noch", "in", "der", "welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Sie letzte sich mit ihm durch sehnliche geb\u00e4rden,", "tokens": ["Sie", "letz\u00b7te", "sich", "mit", "ihm", "durch", "sehn\u00b7li\u00b7che", "ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.88": {"text": "Die lippen segneten, wie thau vom himmel f\u00e4llt.", "tokens": ["Die", "lip\u00b7pen", "seg\u00b7ne\u00b7ten", ",", "wie", "thau", "vom", "him\u00b7mel", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Doch \u00fcberlie\u00df sie ihn zuletzt aus ihren armen,", "tokens": ["Doch", "\u00fc\u00b7ber\u0