{"dta.poem.13660": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Vor mir liegen Deine Zeilen,", "tokens": ["Vor", "mir", "lie\u00b7gen", "Dei\u00b7ne", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind nicht Worte, Schriften nicht,", "tokens": ["Sind", "nicht", "Wor\u00b7te", ",", "Schrif\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfeile, die verwundend heilen,", "tokens": ["Pfei\u00b7le", ",", "die", "ver\u00b7wun\u00b7dend", "hei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundes-Augen, treu und schlicht.", "tokens": ["Freun\u00b7des\u00b7Au\u00b7gen", ",", "treu", "und", "schlicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}