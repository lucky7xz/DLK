, "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deinem Jauchzen, das verk\u00fcndigt", "tokens": ["Dei\u00b7nem", "Jauch\u00b7zen", ",", "das", "ver\u00b7k\u00fcn\u00b7digt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine starke, tiefe Brust!", "tokens": ["Ei\u00b7ne", "star\u00b7ke", ",", "tie\u00b7fe", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf den zauberischen Wogen", "tokens": ["Auf", "den", "zau\u00b7be\u00b7ri\u00b7schen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutscher T\u00e4nze schwebtest du,", "tokens": ["Deut\u00b7scher", "T\u00e4n\u00b7ze", "schweb\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wetter kamen schw\u00fcl gezogen,", "tokens": ["Wet\u00b7ter", "ka\u00b7men", "schw\u00fcl", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schelmisch logst du \u00fcppige Ruh;", "tokens": ["Schel\u00b7misch", "logst", "du", "\u00fcp\u00b7pi\u00b7ge", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Eisgrau sa\u00dfen tote W\u00e4chter", "tokens": ["Eis\u00b7gr