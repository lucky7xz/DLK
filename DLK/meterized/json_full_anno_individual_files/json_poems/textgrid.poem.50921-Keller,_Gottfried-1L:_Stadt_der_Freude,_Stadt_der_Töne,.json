tetra"}, "line.4": {"text": "Auf der morgenroten Bahn!", "tokens": ["Auf", "der", "mor\u00b7gen\u00b7ro\u00b7ten", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "F\u00fchr ein deutsches Kaiserbild,", "tokens": ["F\u00fchr", "ein", "deut\u00b7sches", "Kai\u00b7ser\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Reich zu schaun im Goldgewande,", "tokens": ["Reich", "zu", "schaun", "im", "Gold\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wir gr\u00fc\u00dfen fromm und mild!", "tokens": ["Und", "wir", "gr\u00fc\u00b7\u00dfen", "fromm", "und", "mild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Dieser Traum wird auch verwehen", "tokens": ["Die\u00b7ser", "Traum", "wird", "auch", "ver\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und am alten Sternenzelt", "tokens": ["Und", "am", "al\u00b7ten", "Ster\u00b7nen\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich unter die Sterne gehen", "tokens": ["End\u00b7lich", "un\u00b7ter", "die", "Ster\u00b7ne", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Zu der toten G\u00f6tterwelt;", "tokens": ["Zu", "der", "to\u00b7ten", "G\u00f6t\u00b7ter\u00b7welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wo flimmernd Schwan und Leier", "tokens": ["Und", "wo", "flim\u00b7mernd", "Schwan", "und", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und das Bild des Kreuzes spr\u00fchn,", "tokens": ["Und", "das", "Bild", "des", "Kreu\u00b7zes", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird dereinst in sch\u00f6nem Feuer", "tokens": ["Wird", "de\u00b7reinst", "in", "sch\u00f6\u00b7nem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Caroli Magni Krone gl\u00fchn!", "tokens": ["Ca\u00b7ro\u00b7li", "Mag\u00b7ni", "Kro\u00b7ne", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber dann in tausend Wiegen,", "tokens": ["A\u00b7ber", "dann", "in", "tau\u00b7send", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier in Gold und dort in Holz,", "tokens": ["Hier", "in", "Gold", "und", "dort", "in", "Holz", ","], "token_info": ["word", "word", "word", "word"