{"dta.poem.5435": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An statt des grossen Gebers G\u00fcte und Macht und Wei\u00df-", "tokens": ["An", "statt", "des", "gros\u00b7sen", "Ge\u00b7bers", "G\u00fc\u00b7te", "und", "Macht", "und", "Wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "NN", "KON", "NN", "KON", "TRUNC"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "heit zu entdecken;", "tokens": ["heit", "zu", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "An statt, f\u00fcr die empfangnen G\u00fcter, erkenntlich froh und", "tokens": ["An", "statt", ",", "f\u00fcr", "die", "emp\u00b7fang\u00b7nen", "G\u00fc\u00b7ter", ",", "er\u00b7kennt\u00b7lich", "froh", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "ADJD", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "fromm zu seyn;", "tokens": ["fromm", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So schlingen wir, ohn Danck und Anmuth, was uns ge-", "tokens": ["So", "schlin\u00b7gen", "wir", ",", "ohn", "Danck", "und", "An\u00b7muth", ",", "was", "uns", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "NN", "KON", "NN", "$,", "PRELS", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "schenckt stets hungrig ein,", "tokens": ["schenckt", "stets", "hung\u00b7rig", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Weil wir, in unterlassnem Dencken, nicht h\u00f6ren, sehen,", "tokens": ["Weil", "wir", ",", "in", "un\u00b7ter\u00b7lass\u00b7nem", "Den\u00b7cken", ",", "nicht", "h\u00f6\u00b7ren", ",", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "PTKNEG", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "f\u00fchlen, schmecken.", "tokens": ["f\u00fch\u00b7len", ",", "schme\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}