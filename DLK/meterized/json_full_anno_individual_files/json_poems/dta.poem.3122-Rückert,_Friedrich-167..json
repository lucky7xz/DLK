{"dta.poem.3122": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "167.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wesen unter sich sind stets im Widerstreit,", "tokens": ["Die", "We\u00b7sen", "un\u00b7ter", "sich", "sind", "stets", "im", "Wi\u00b7der\u00b7streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben, eins in Gott, ist au\u00dfer ihm entzweit.", "tokens": ["Das", "Le\u00b7ben", ",", "eins", "in", "Gott", ",", "ist", "au\u00b7\u00dfer", "ihm", "ent\u00b7zweit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "APPR", "NN", "$,", "VAFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In Gott sind wir geeint, und au\u00dfer ihm geschieden;", "tokens": ["In", "Gott", "sind", "wir", "ge\u00b7eint", ",", "und", "au\u00b7\u00dfer", "ihm", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,", "KON", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' ihn ist ew'ger Krieg, und durch ihn ew'ger Frieden.", "tokens": ["Ohn'", "ihn", "ist", "ew'\u00b7ger", "Krieg", ",", "und", "durch", "ihn", "ew'\u00b7ger", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPE