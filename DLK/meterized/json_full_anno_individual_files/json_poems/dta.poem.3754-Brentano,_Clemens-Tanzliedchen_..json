{"dta.poem.3754": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzliedchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bin ich nit ein B\u00fcrschlein               ", "tokens": ["Bin", "ich", "nit", "ein", "B\u00fcr\u00b7schlein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "In der Welt?", "tokens": ["In", "der", "Welt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Spring ja wie ein Hirschlein,", "tokens": ["Spring", "ja", "wie", "ein", "Hirsc\u00b7hlein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In dem Feld?", "tokens": ["In", "dem", "Feld", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "In dem Feld, im gr\u00fcnen Holz,", "tokens": ["In", "dem", "Feld", ",", "im", "gr\u00fc\u00b7nen", "Holz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Begegnet mir ein Jungfrau stolz.", "tokens": ["Be\u00b7geg\u00b7net", "mir", "ein", "Jung\u00b7frau", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Guten Morgen, Jungfrau!", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", ",", "Jung\u00b7frau", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Mach geschwind,", "tokens": ["Mach", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Du sollst mit mir tanzen,", "tokens": ["Du", "sollst", "mit", "mir", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Munter Kind!", "tokens": ["Mun\u00b7ter", "Kind", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Bischen auf und abgeschwenkt,", "tokens": ["Bi\u00b7schen", "auf", "und", "ab\u00b7ge\u00b7schwenkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und ein Gl\u00e4schen eingeschenkt.", "tokens": ["Und", "ein", "Gl\u00e4s\u00b7chen", "ein\u00b7ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Sch\u00f6ne Musikanten", "tokens": ["Sch\u00f6\u00b7ne", "Mu\u00b7si\u00b7kan\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Spielet auf!", "tokens": ["Spie\u00b7let", "auf", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Spielet mir ein T\u00e4nzlein", "tokens": ["Spie\u00b7let", "mir", "ein", "T\u00e4nz\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Oben drauf;", "tokens": ["O\u00b7ben", "drauf", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Aufgepuzt, eingeschn\u00fcrt,", "tokens": ["Auf\u00b7ge\u00b7puzt", ",", "ein\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Lustig dann zum Tanz gef\u00fchrt.", "tokens": ["Lus\u00b7tig", "dann", "zum", "Tanz", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Heisasa.", "tokens": ["Hei\u00b7sa\u00b7sa", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--", "measure": "dactylic.init"}}}}}