", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "ADJD", "KOKOM", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um die sich marmorn Mann und M\u00e4dchen ranken,", "tokens": ["Um", "die", "sich", "mar\u00b7morn", "Mann", "und", "M\u00e4d\u00b7chen", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PRF", "NE", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit vollen Zweigen und zertretnem Kraut,", "tokens": ["Mit", "vol\u00b7len", "Zwei\u00b7gen", "und", "zer\u00b7tret\u00b7nem", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schweigende Form! du rufst in uns Gedanken,", "tokens": ["Schwei\u00b7gen\u00b7de", "Form", "!", "du", "rufst", "in", "uns", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wie Ewigkeit es tut: kalt Sch\u00e4ferspiel!", "tokens": ["Wie", "E\u00b7wig\u00b7keit", "es", "tut", ":", "kalt", "Sch\u00e4\u00b7fer\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "$.", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sind wir mit unserm Leid dahin, so findest", "tokens": ["Sind", "wir", "mit", "un\u00b7serm", "Leid", "da\u00b7hin", ",", "so", "fin\u00b7dest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", 