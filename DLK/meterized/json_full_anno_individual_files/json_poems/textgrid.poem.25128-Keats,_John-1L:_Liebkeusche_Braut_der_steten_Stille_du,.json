{"textgrid.poem.25128": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebkeusche Braut der steten Stille du,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebkeusche Braut der steten Stille du,", "tokens": ["Lieb\u00b7keu\u00b7sche", "Braut", "der", "ste\u00b7ten", "Stil\u00b7le", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du Pflegekind von Tag und Tag und Schweigen!", "tokens": ["Du", "Pfle\u00b7ge\u00b7kind", "von", "Tag", "und", "Tag", "und", "Schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welch blumiges Waldgeschichtchen schilderst du \u2013", "tokens": ["Welch", "blu\u00b7mi\u00b7ges", "Wald\u00b7ge\u00b7schicht\u00b7chen", "schil\u00b7derst", "du", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sagst es s\u00fc\u00dfer als ein Reimereigen?", "tokens": ["Und", "sagst", "es", "s\u00fc\u00b7\u00dfer", "als", "ein", "Rei\u00b7mer\u00b7ei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Welch blattumrankte M\u00e4r umstreicht dein Rund", "tokens": ["Welch", "blat\u00b7tum\u00b7rank\u00b7te", "M\u00e4r", "um\u00b7streicht", "dein", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von G\u00f6ttern oder Menschen oder beiden", "tokens": ["Von", "G\u00f6t\u00b7tern", "o\u00b7der", "Men\u00b7schen", "o\u00b7der", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Tempe oder in Arkadiens H\u00e4ngen?", "tokens": ["In", "Tem\u00b7pe", "o\u00b7der", "in", "Ar\u00b7ka\u00b7di\u00b7ens", "H\u00e4n\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wer sind sie, die an M\u00e4dchenangst sich weiden?", "tokens": ["Wer", "sind", "sie", ",", "die", "an", "M\u00e4d\u00b7chen\u00b7angst", "sich", "wei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Was jagt so toll? Was ringt und flieht so bunt?", "tokens": ["Was", "jagt", "so", "toll", "?", "Was", "ringt", "und", "flieht", "so", "bunt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$.", "PWS", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Welch Fl\u00f6tenlied? Welch lustberauschtes Dr\u00e4ngen?", "tokens": ["Welch", "Fl\u00f6\u00b7ten\u00b7lied", "?", "Welch", "lust\u00b7be\u00b7rauschtes", "Dr\u00e4n\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Geh\u00f6rtes Lied ist s\u00fc\u00df, doch s\u00fc\u00dfer ist", "tokens": ["Ge\u00b7h\u00f6r\u00b7tes", "Lied", "ist", "s\u00fc\u00df", ",", "doch", "s\u00fc\u00b7\u00dfer", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein ungeh\u00f6rtes: sanfte Fl\u00f6te, weiter!", "tokens": ["Ein", "un\u00b7ge\u00b7h\u00f6r\u00b7tes", ":", "sanf\u00b7te", "Fl\u00f6\u00b7te", ",", "wei\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$.", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O wie du, klanglos, mehr als k\u00f6stlich bist,", "tokens": ["O", "wie", "du", ",", "klang\u00b7los", ",", "mehr", "als", "k\u00f6st\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "$,", "ADJD", "$,", "PIAT", "KOKOM", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du geisterhaft-lautlosen Lieds Begleiter!", "tokens": ["Du", "geis\u00b7ter\u00b7haft\u00b7laut\u00b7lo\u00b7sen", "Lieds", "Be\u00b7glei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nie kannst du, Jugend, lassen von dem Sang,", "tokens": ["Nie", "kannst", "du", ",", "Ju\u00b7gend", ",", "las\u00b7sen", "von", "dem", "Sang", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie nie die B\u00e4ume hier ihr Laub verlieren;", "tokens": ["Wie", "nie", "die", "B\u00e4u\u00b7me", "hier", "ihr", "Laub", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du keck Verliebter, nie, nie kannst du k\u00fcssen,", "tokens": ["Du", "keck", "Ver\u00b7lieb\u00b7ter", ",", "nie", ",", "nie", "kannst", "du", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "ADV", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So nah du auch dem Ziel \u2013 doch sei nicht bang:", "tokens": ["So", "nah", "du", "auch", "dem", "Ziel", "\u2013", "doch", "sei", "nicht", "bang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "NN", "$(", "ADV", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nie welkt sie! Wirst du auch entbehren m\u00fcssen,", "tokens": ["Nie", "welkt", "sie", "!", "Wirst", "du", "auch", "ent\u00b7beh\u00b7ren", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wird Liebe dich und Sch\u00f6nheit sie stets zieren.", "tokens": ["Wird", "Lie\u00b7be", "dich", "und", "Sch\u00f6n\u00b7heit", "sie", "stets", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "KON", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gl\u00fccklicher Baum in ewiger Fr\u00fchlingszeit,", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "Baum", "in", "e\u00b7wi\u00b7ger", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nie sinken deiner Zweige Bl\u00e4tter nieder.", "tokens": ["Nie", "sin\u00b7ken", "dei\u00b7ner", "Zwei\u00b7ge", "Bl\u00e4t\u00b7ter", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gl\u00fccklicher S\u00e4nger, ohne M\u00fcdigkeit", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "S\u00e4n\u00b7ger", ",", "oh\u00b7ne", "M\u00fc\u00b7dig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUI", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "F\u00fcr immer fl\u00f6tend immer neue Lieder!", "tokens": ["F\u00fcr", "im\u00b7mer", "fl\u00f6\u00b7tend", "im\u00b7mer", "neu\u00b7e", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Liebe, Liebe, voll von gr\u00f6\u00dferem Gl\u00fcck:", "tokens": ["Und", "Lie\u00b7be", ",", "Lie\u00b7be", ",", "voll", "von", "gr\u00f6\u00b7\u00dfe\u00b7rem", "Gl\u00fcck", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "F\u00fcr immer hei\u00df und der Erf\u00fcllung harrend,", "tokens": ["F\u00fcr", "im\u00b7mer", "hei\u00df", "und", "der", "Er\u00b7f\u00fcl\u00b7lung", "har\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du immer jagende, du immer junge!", "tokens": ["Du", "im\u00b7mer", "ja\u00b7gen\u00b7de", ",", "du", "im\u00b7mer", "jun\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "$,", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Wie steht vor dir lebendige Gier zur\u00fcck,", "tokens": ["Wie", "steht", "vor", "dir", "le\u00b7ben\u00b7di\u00b7ge", "Gier", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Die Herzen satt macht, im Genu\u00df erstarrend,", "tokens": ["Die", "Her\u00b7zen", "satt", "macht", ",", "im", "Ge\u00b7nu\u00df", "er\u00b7star\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Die Hirn erhitzt und d\u00fcrr versengt die Zunge!", "tokens": ["Die", "Hirn", "er\u00b7hitzt", "und", "d\u00fcrr", "ver\u00b7sengt", "die", "Zun\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wer sind diese mit dem Priester hier", "tokens": ["Und", "wer", "sind", "die\u00b7se", "mit", "dem", "Pries\u00b7ter", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jener F\u00e4rse? Welcher Gottheit danken", "tokens": ["Und", "je\u00b7ner", "F\u00e4r\u00b7se", "?", "Wel\u00b7cher", "Got\u00b7theit", "dan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$.", "PWAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Gr\u00fcnen sie mit sch\u00f6nstem Opfertier,", "tokens": ["Im", "Gr\u00fc\u00b7nen", "sie", "mit", "sch\u00f6ns\u00b7tem", "Op\u00b7fer\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Kr\u00e4nze bl\u00fchen um die seidnen Flanken?", "tokens": ["Dem", "Kr\u00e4n\u00b7ze", "bl\u00fc\u00b7hen", "um", "die", "seid\u00b7nen", "Flan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRELS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Welch kleine Stadt an Flu\u00df, in Bergeshain,", "tokens": ["Welch", "klei\u00b7ne", "Stadt", "an", "Flu\u00df", ",", "in", "Ber\u00b7ges\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An Seestrand, Stadt mit Burg zu Wehr und Frieden.", "tokens": ["An", "Sees\u00b7t