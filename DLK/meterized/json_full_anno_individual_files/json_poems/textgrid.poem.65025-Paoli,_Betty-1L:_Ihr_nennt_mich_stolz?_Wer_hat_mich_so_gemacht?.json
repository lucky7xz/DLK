{"textgrid.poem.65025": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr nennt mich stolz? Wer hat mich so gemacht?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr nennt mich stolz? Wer hat mich so gemacht?", "tokens": ["Ihr", "nennt", "mich", "stolz", "?", "Wer", "hat", "mich", "so", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr selbst, die mich betrogen und verrathen!", "tokens": ["Ihr", "selbst", ",", "die", "mich", "be\u00b7tro\u00b7gen", "und", "ver\u00b7ra\u00b7then", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Regung, die ihr schm\u00e4ht, ist erst erwacht,", "tokens": ["Die", "Re\u00b7gung", ",", "die", "ihr", "schm\u00e4ht", ",", "ist", "erst", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ich mein Thun verglich mit euern Thaten!", "tokens": ["Als", "ich", "mein", "Thun", "ver\u00b7glich", "mit", "eu\u00b7ern", "Tha\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr n