{"textgrid.poem.33213": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2252.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wir immer suchten und nimmer funden, treffen wir endlich an in den wunden des Gottes Lamms.", "tokens": ["Was", "wir", "im\u00b7mer", "such\u00b7ten", "und", "nim\u00b7mer", "fun\u00b7den", ",", "tref\u00b7fen", "wir", "end\u00b7lich", "an", "in", "den", "wun\u00b7den", "des", "Got\u00b7tes", "Lamms", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "APPR", "ART", "ADJA", "ART", "NN", "NE", "$."], "meter": "--+-+--+-+-+--+-+--+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Was der k\u00f6nig David kaum heimlich wu\u00dfte, wenn er am s\u00fcnder seyn lernen mu\u00dfte, ist d\u00e4cher werk.", "tokens": ["Was", "der", "k\u00f6\u00b7nig", "Da\u00b7vid", "kaum", "heim\u00b7lich", "wu\u00df\u00b7te", ",", "wenn", "er", "am", "s\u00fcn\u00b7der", "seyn", "ler\u00b7nen", "mu\u00df\u00b7te", ",", "ist", "d\u00e4\u00b7cher", "werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NE", "ADV", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "VAINF", "VVINF", "VMFIN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+--+--+-+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Was die tempel-priester im geiste rochen, damit durchr\u00e4uchern itzt alle b'rochen das sch\u00e4cher-volk.", "tokens": ["Was", "die", "tem\u00b7pel\u00b7pries\u00b7ter", "im", "geis\u00b7te", "ro\u00b7chen", ",", "da\u00b7mit", "durch\u00b7r\u00e4u\u00b7chern", "itzt", "al\u00b7le", "ro\u00b7che", "das", "sch\u00e4\u00b7cher\u00b7volk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "ADJA", "ADJA", "$,", "PAV", "VVFIN", "ADV", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+--+-+-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Der geist, den der knabe zu f\u00fchlen kriegte, als der prophete sich zu ihm f\u00fcgte, geht siebenfach.", "tokens": ["Der", "geist", ",", "den", "der", "kna\u00b7be", "zu", "f\u00fch\u00b7len", "krieg\u00b7te", ",", "als", "der", "pro\u00b7phe\u00b7te", "sich", "zu", "ihm", "f\u00fcg\u00b7te", ",", "geht", "sie\u00b7ben\u00b7fach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,", "KOUS", "ART", "VVFIN", "PRF", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+-+--+-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Euer angedenken an Satans fallen und unser s\u00fcnden-gef\u00fchl macht schallen: Eleison!", "tokens": ["Eu\u00b7er", "an\u00b7ge\u00b7den\u00b7ken", "an", "Sa\u00b7tans", "fal\u00b7len", "und", "un\u00b7ser", "s\u00fcn\u00b7den\u00b7ge\u00b7f\u00fchl", "macht", "schal\u00b7len", ":", "E\u00b7lei\u00b7son", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "KON", "PPOSAT", "NN", "VVFIN", "VVINF", "$.", "NE", "$."], "meter": "+-+-+--+-+--+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Und weil wir in Christo nur einer werden, schauet ihr t\u00e4glich bey unsern heerden zur arche nein.", "tokens": ["Und", "weil", "wir", "in", "Chris\u00b7to", "nur", "ei\u00b7ner", "wer\u00b7den", ",", "schau\u00b7et", "ihr", "t\u00e4g\u00b7lich", "bey", "un\u00b7sern", "heer\u00b7den", "zur", "ar\u00b7che", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NE", "ADV", "PIS", "VAINF", "$,", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "PTKANT", "$."], "meter": "-+-+-+-+-+-+--+--+-+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Was in k\u00fcnftgen tagen vom Salems gl\u00fckke noch zu gewarten, habt ihr im blikke, und lobet Gott.", "tokens": ["Was", "in", "k\u00fcnft\u00b7gen", "ta\u00b7gen", "vom", "Sa\u00b7lems", "gl\u00fck\u00b7ke", "noch", "zu", "ge\u00b7war\u00b7ten", ",", "habt", "ihr", "im", "blik\u00b7ke", ",", "und", "lo\u00b7bet", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "APPRART", "VVFIN", "$,", "KON", "VVFIN", "NN", "$."], "meter": "--+-+--+-+--+-+-+--+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Unsre Litaneyen und eure fugen, helffen zusammen den geistlich klugen zur Lammes-lehr.", "tokens": ["Uns\u00b7re", "Li\u00b7ta\u00b7ne\u00b7yen", "und", "eu\u00b7re", "fu\u00b7gen", ",", "helf\u00b7fen", "zu\u00b7sam\u00b7men", "den", "geist\u00b7lich", "klu\u00b7gen", "zur", "Lam\u00b7mes\u00b7lehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "ART", "ADJD", "ADJA", "APPRART", "NN", "$."], "meter": "---+-+-+-+-+--+--+-+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Eure ewigkeiten und unsre jahre, ja, was noch mehr, der einf\u00e4ltigen haare sind registrirt.", "tokens": ["Eu\u00b7re", "e\u00b7wig\u00b7kei\u00b7ten", "und", "uns\u00b7re", "jah\u00b7re", ",", "ja", ",", "was", "noch", "mehr", ",", "der", "ein\u00b7f\u00e4l\u00b7ti\u00b7gen", "haa\u00b7re", "sind", "re\u00b7gis\u00b7trirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "PPOSAT", "NN", "$,", "PTKANT", "$,", "PRELS", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+--+-+-+--+--+--+-++-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Unsers Lammes d\u00fcrsten, unser gesehne augen zu \u00f6fnen, und eu'r geth\u00f6ne, sind concertirt.", "tokens": ["Un\u00b7sers", "Lam\u00b7mes", "d\u00fcrs\u00b7ten", ",", "un\u00b7ser", "ge\u00b7seh\u00b7ne", "au\u00b7gen", "zu", "\u00f6f\u00b7nen", ",", "und", "eu'r", "ge\u00b7th\u00f6\u00b7ne", ",", "sind", "con\u00b7cer\u00b7tirt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,", "KON", "PPOSAT", "ADJA", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+--+-+--+--+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Was wir immer suchten und nimmer funden, treffen wir endlich an in den wunden des Gottes Lamms.", "tokens": ["Was", "wir", "im\u00b7mer", "such\u00b7ten", "und", "nim\u00b7mer", "fun\u00b7den", ",", "tref\u00