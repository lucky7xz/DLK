{"dta.poem.912": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "VoRSTADT IM F\u00d6HN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Am Abend liegt die St\u00e4tte \u00f6d und braun,", "tokens": ["Am", "A\u00b7bend", "liegt", "die", "St\u00e4t\u00b7te", "\u00f6d", "und", "braun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Luft von gr\u00e4ulichem Gestank durchzogen.", "tokens": ["Die", "Luft", "von", "gr\u00e4u\u00b7li\u00b7chem", "Ge\u00b7stank", "durch\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Donnern eines Zugs vom Br\u00fcckenbogen \u2014", "tokens": ["Das", "Don\u00b7nern", "ei\u00b7nes", "Zugs", "vom", "Br\u00fc\u00b7cken\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Spatzen flattern \u00fcber Busch und Zaun.", "tokens": ["Und", "Spat\u00b7zen", "flat\u00b7tern", "\u00fc\u00b7ber", "Busch", "und", "Zaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geduckte H\u00fctten, Pfade wirr verstreut,", "tokens": ["Ge\u00b7duck\u00b7te", "H\u00fct\u00b7ten", ",", "Pfa\u00b7de", "wirr", "ver\u00b7streut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In G\u00e4rten Durcheinander und Bewegung,", "tokens": ["In", "G\u00e4r\u00b7ten", "Durch\u00b7ein\u00b7an\u00b7der", "und", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bisweilen schwillt Geheul aus dumpfer Regung,", "tokens": ["Bis\u00b7wei\u00b7len", "schwillt", "Ge\u00b7heul", "aus", "dum\u00b7pfer", "Re\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einer Kinderschar fliegt rot ein Kleid.", "tokens": ["In", "ei\u00b7ner", "Kin\u00b7der\u00b7schar", "fliegt", "rot", "ein", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Kehricht pfeift verliebt ein Rattenchor.", "tokens": ["Am", "Keh\u00b7richt", "pfeift", "ver\u00b7liebt", "ein", "Rat\u00b7ten\u00b7chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In K\u00f6rben tragen Frauen Eingeweide,", "tokens": ["In", "K\u00f6r\u00b7ben", "tra\u00b7gen", "Frau\u00b7en", "Ein\u00b7ge\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein ekelhafter Zug voll Schmutz und R\u00e4ude,", "tokens": ["Ein", "e\u00b7kel\u00b7haf\u00b7ter", "Zug", "voll", "Schmutz", "und", "R\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kommen sie aus der D\u00e4mmerung hervor.", "tokens": ["Kom\u00b7men", "sie", "aus", "der", "D\u00e4m\u00b7me\u00b7rung", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ein Kanal speit pl\u00f6tzlich feistes Blut", "tokens": ["Und", "ein", "Ka\u00b7nal", "speit", "pl\u00f6tz\u00b7lich", "feis\u00b7tes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Schlachthaus in den stillen Flu\u00df hinunter.", "tokens": ["Vom", "Schlacht\u00b7haus", "in", "den", "stil\u00b7len", "Flu\u00df", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die F\u00f6hne f\u00e4rben karge Stauden bunter", "tokens": ["Die", "F\u00f6h\u00b7ne", "f\u00e4r\u00b7ben", "kar\u00b7ge", "Stau\u00b7den", "bun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und langsam kriecht die R\u00f6te durch die Flut.", "tokens": ["Und", "lang\u00b7sam", "kriecht", "die", "R\u00f6\u00b7te", "durch", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Fl\u00fcstern, das in tr\u00fcbem Schlaf ertrinkt.", "tokens": ["Ein", "Fl\u00fcs\u00b7tern", ",", "das", "in", "tr\u00fc\u00b7bem", "Schlaf", "er\u00b7trinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gebilde gaukeln auf aus Wassergr\u00e4ben,", "tokens": ["Ge\u00b7bil\u00b7de", "gau\u00b7keln", "auf", "aus", "Was\u00b7ser\u00b7gr\u00e4\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht Erinnerung an ein fr\u00fcheres Leben,", "tokens": ["Viel\u00b7leicht", "E\u00b7rin\u00b7ne\u00b7rung", "an", "ein", "fr\u00fc\u00b7he\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die mit den warmen Winden steigt und sinkt.", "tokens": ["Die", "mit", "den", "war\u00b7men", "Win\u00b7den", "steigt", "und", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Aus Wolken tauchen schimmernde Alleen,", "tokens": ["Aus", "Wol\u00b7ken", "tau\u00b7chen", "schim\u00b7mern\u00b7de", "Al\u00b7leen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erf\u00fcllt von sch\u00f6nen W\u00e4gen, k\u00fchnen Reitern.", "tokens": ["Er\u00b7f\u00fcllt", "von", "sch\u00f6\u00b7nen", "W\u00e4\u00b7gen", ",", "k\u00fch\u00b7nen", "Rei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann sieht man auch ein Schiff auf Klippen scheitern", "tokens": ["Dann", "sieht", "man", "auch", "ein", "Schiff", "auf", "Klip\u00b7pen", "schei\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und manchmal rosenfarbene Moscheen.", "tokens": ["Und", "manch\u00b7mal", "ro\u00b7sen\u00b7far\u00b7be\u00b7ne", "Mo\u00b7scheen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}