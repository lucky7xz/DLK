{"textgrid.poem.65971": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hab' erst einen gro\u00dfen Anlauf genommen,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab' erst einen gro\u00dfen Anlauf genommen,", "tokens": ["Hab'", "erst", "ei\u00b7nen", "gro\u00b7\u00dfen", "An\u00b7lauf", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wollt' gern eine Strecke vorw\u00e4rts kommen,", "tokens": ["Wollt'", "gern", "ei\u00b7ne", "Stre\u00b7cke", "vor\u00b7w\u00e4rts", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In Spr\u00fcngen das hohe Ziel erreichen,", "tokens": ["In", "Spr\u00fcn\u00b7gen", "das", "ho\u00b7he", "Ziel", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das winkte mit seinen leuchtenden Zeichen.", "tokens": ["Das", "wink\u00b7te", "mit", "sei\u00b7nen", "leuch\u00b7ten\u00b7den", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da blieb ich verschnaufend einmal stehn,", "tokens": ["Da", "blieb", "ich", "ver\u00b7schnau\u00b7fend", "ein\u00b7mal", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Pr\u00fcfend vor- und r\u00fcckw\u00e4rts zu sehn.", "tokens": ["Pr\u00fc\u00b7fend", "vor", "und", "r\u00fcck\u00b7w\u00e4rts", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "TRUNC", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Gleich sank der Mut mir. Sei nicht dumm,", "tokens": ["Gleich", "sank", "der", "Mut", "mir", ".", "Sei", "nicht", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$.", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dacht' ich, und kehre wieder um.", "tokens": ["Dacht'", "ich", ",", "und", "keh\u00b7re", "wie\u00b7der", "um", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Soll's deinen kurzen Beinen gelingen,", "tokens": ["Soll's", "dei\u00b7nen", "kur\u00b7zen", "Bei\u00b7nen", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Musst all dein Lebtag laufen und springen.", "tokens": ["Musst", "all", "dein", "Leb\u00b7tag", "lau\u00b7fen", "und", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "PPOSAT", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Schone die Lunge und gehe h\u00fcbsch sacht,", "tokens": ["Scho\u00b7ne", "die", "Lun\u00b7ge", "und", "ge\u00b7he", "h\u00fcbsch", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Wie es der gro\u00dfe Haufe macht.", "tokens": ["Wie", "es", "der", "gro\u00b7\u00dfe", "Hau\u00b7fe", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auf einmal aber f\u00e4ngt's an zu zwicken,", "tokens": ["Auf", "ein\u00b7mal", "a\u00b7ber", "f\u00e4ngt's", "an", "zu", "zwi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Zu rei\u00dfen, zu zerren wie mit Stricken,", "tokens": ["Zu", "rei\u00b7\u00dfen", ",", "zu", "zer\u00b7ren", "wie", "mit", "Stri\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KOKOM", "APPR", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Dass ich wieder, als g\u00e4lt's mein Heil,", "tokens": ["Dass", "ich", "wie\u00b7der", ",", "als", "g\u00e4lt's", "mein", "Heil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Mit allem Eifer vorw\u00e4rts eil'.", "tokens": ["Mit", "al\u00b7lem", "Ei\u00b7fer", "vor\u00b7w\u00e4rts", "eil'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So hab' ich's denn noch weiter getrieben,", "tokens": ["So", "hab'", "ich's", "denn", "noch", "wei\u00b7ter", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und bin ich manchmal stehen geblieben,", "tokens": ["Und", "bin", "ich", "manch\u00b7mal", "ste\u00b7hen", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Stets hat ein inneres Zerren und Rei\u00dfen", "tokens": ["Stets", "hat", "ein", "in\u00b7ne\u00b7res", "Zer\u00b7ren", "und", "Rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich von der Stelle packen hei\u00dfen.", "tokens": ["Mich", "von", "der", "Stel\u00b7le", "pa\u00b7cken", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bin ich denn, alles in allem genommen,", "tokens": ["So", "bin", "ich", "denn", ",", "al\u00b7les", "in", "al\u00b7lem", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PIS", "APPR", "PIS", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Eine gute Strecke vorw\u00e4rts gekommen,", "tokens": ["Ei\u00b7ne", "gu\u