{"dta.poem.9742": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines bequemen mannes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mjr hat das trincken mehr/ als lieben/ freude bracht/", "tokens": ["Mjr", "hat", "das", "trin\u00b7cken", "mehr", "/", "als", "lie\u00b7ben", "/", "freu\u00b7de", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDS", "VVFIN", "ADV", "$(", "KOKOM", "VVFIN", "$(", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil dieses kr\u00e4ffte nahm/ und jenes st\u00e4rcke macht:", "tokens": ["Weil", "die\u00b7ses", "kr\u00e4ff\u00b7te", "nahm", "/", "und", "je\u00b7nes", "st\u00e4r\u00b7cke", "macht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$(", "KON", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch ist mein weib dabey nicht unvergn\u00fcgt geblieben/", "tokens": ["Doch", "ist", "mein", "weib", "da\u00b7bey", "nicht", "un\u00b7ver\u00b7gn\u00fcgt", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PAV", "PTKNEG", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie lie\u00df mir frey den trunck/ ich lie\u00df ihr frey das lieben.", "tokens": ["Sie", "lie\u00df", "mir", "frey", "den", "trunck", "/", "ich", "lie\u00df", "ihr", "frey", "das", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}