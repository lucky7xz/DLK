{"dta.poem.18870": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Nun aber sind gemordet von mir", "tokens": ["Nun", "a\u00b7ber", "sind", "ge\u00b7mor\u00b7det", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "APPR", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Hudseiliten mit tiefen Wunden.", "tokens": ["Die", "Hud\u00b7sei\u00b7li\u00b7ten", "mit", "tie\u00b7fen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "M\u00fcrbe macht mich nicht das Ungl\u00fcck,", "tokens": ["M\u00fcr\u00b7be", "macht", "mich", "nicht", "das", "Un\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es selbst wird m\u00fcrbe.", "tokens": ["Es", "selbst", "wird", "m\u00fcr\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}