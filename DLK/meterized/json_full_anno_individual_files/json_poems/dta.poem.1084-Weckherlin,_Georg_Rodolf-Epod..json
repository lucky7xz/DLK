{"dta.poem.1084": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jn welches r\u00fchmliche land", "tokens": ["In", "wel\u00b7ches", "r\u00fchm\u00b7li\u00b7che", "land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kan sich ein man wol vergehen/", "tokens": ["Kan", "sich", "ein", "man", "wol", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "PIS", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alda Er deinen wolstand", "tokens": ["Al\u00b7da", "Er", "dei\u00b7nen", "wol\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Vnd namen nicht m\u00f6g verstehen?", "tokens": ["Vnd", "na\u00b7men", "nicht", "m\u00f6g", "ver\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Taub ist der/ dessen geh\u00f6r", "tokens": ["Taub", "ist", "der", "/", "des\u00b7sen", "ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ART", "$(", "PRELAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Die weit-vmb fliegende mehr", "tokens": ["Die", "weit\u00b7\u00b7vmb", "flie\u00b7gen\u00b7de", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Deiner Wei\u00dfheit nicht vernommen;", "tokens": ["Dei\u00b7ner", "Wei\u00df\u00b7heit", "nicht", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd wer da will h\u00f6flichkeit/", "tokens": ["Vnd", "wer", "da", "will", "h\u00f6f\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VMFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Freundlichkeit vnd zierlichkeit", "tokens": ["Freund\u00b7lich\u00b7keit", "vnd", "zier\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sehen/ der mu\u00df zu dir kommen.", "tokens": ["Se\u00b7hen", "/", "der", "mu\u00df", "zu", "dir", "kom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ART", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}