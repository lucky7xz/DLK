{"dta.poem.23821": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Als den Abend vorher am Bu\u00df-Tage  \n drey Ma\u00dfqvirte Damen sich bey Hofe  \n eingefunden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Als gestern unsre Stadt/ wie ehmahls Ninive/", "tokens": ["Als", "ge\u00b7stern", "uns\u00b7re", "Stadt", "/", "wie", "eh\u00b7mahls", "Ni\u00b7ni\u00b7ve", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$(", "KOKOM", "ADV", "NE", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Im Sack und Asche lag/ und ihre Fasten hielte/", "tokens": ["Im", "Sack", "und", "A\u00b7sche", "lag", "/", "und", "ih\u00b7re", "Fas\u00b7ten", "hiel\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geschah\u2019 es bey der Nacht/ da\u00df zwischen Ach und Weh/", "tokens": ["Ge\u00b7scha\u00b7h'", "es", "bey", "der", "Nacht", "/", "da\u00df", "zwi\u00b7schen", "Ach", "und", "Weh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$(", "KOUS", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das schon betr\u00fcbte Volck ein neues Schrecken f\u00fchlte.", "tokens": ["Das", "schon", "be\u00b7tr\u00fcb\u00b7te", "Volck", "ein", "neu\u00b7es", "Schre\u00b7cken", "f\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drey Ma\u00dfqven liessen sich in fremdem Zierrath sehn/", "tokens": ["Drey", "Ma\u00df\u00b7qven", "lies\u00b7sen", "sich", "in", "frem\u00b7dem", "Zier\u00b7rath", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df nicht ob sie uns vielleicht zum Trost erschiene\u0303.", "tokens": ["Ich", "wei\u00df", "nicht", "ob", "sie", "uns", "viel\u00b7leicht", "zum", "Trost", "er\u00b7schie\u00b7n\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie sahen denen gleich/ die hin zum Paris gehn/", "tokens": ["Sie", "sa\u00b7hen", "de\u00b7nen", "gleich", "/", "die", "hin", "zum", "Pa\u00b7ris", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "$(", "ART", "ADV", "APPRART", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch seinen Richter-Spruch den Apffel zu verdienen.", "tokens": ["Durch", "sei\u00b7nen", "Rich\u00b7ter\u00b7Spruch", "den", "Apf\u00b7fel", "zu", "ver\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Propheteten die ihr sonst die Geister pr\u00fcfen k\u00f6nnt/", "tokens": ["Pro\u00b7phe\u00b7te\u00b7ten", "die", "ihr", "sonst", "die", "Geis\u00b7ter", "pr\u00fc\u00b7fen", "k\u00f6nnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "ADV", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ob es solche sind die GOtt den HErren loben/", "tokens": ["Und", "ob", "es", "sol\u00b7che", "sind", "die", "Gott", "den", "Her\u00b7ren", "lo\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr die ihr jedes Ding bey seinen Nahmen nennt", "tokens": ["Ihr", "die", "ihr", "je\u00b7des", "Ding", "bey", "sei\u00b7nen", "Nah\u00b7men", "nennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPOSAT", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sagt kamen diese drey von unten oder oben!", "tokens": ["Sagt", "ka\u00b7men", "die\u00b7se", "drey", "von", "un\u00b7ten", "o\u00b7der", "o\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PDAT", "CARD", "APPR", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}