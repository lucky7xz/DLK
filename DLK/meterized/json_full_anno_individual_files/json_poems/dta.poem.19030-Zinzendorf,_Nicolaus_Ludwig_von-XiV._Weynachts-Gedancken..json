{"dta.poem.19030": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  Weynachts-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Rath, Krafft, und Held und Wunderbar!", "tokens": ["Rath", ",", "Krafft", ",", "und", "Held", "und", "Wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Nahm ist meiner Seelen klar", "tokens": ["Dein", "Nahm", "ist", "mei\u00b7ner", "See\u00b7len", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die du mit deinem Blut erkaufft,", "tokens": ["Die", "du", "mit", "dei\u00b7nem", "Blut", "er\u00b7kaufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit der Liebes-Glut getaufft,", "tokens": ["Und", "mit", "der", "Lie\u00b7bes\u00b7Glut", "ge\u00b7taufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Br\u00e4utigam, an meiner Stirne brennt", "tokens": ["Mein", "Br\u00e4u\u00b7ti\u00b7gam", ",", "an", "mei\u00b7ner", "Stir\u00b7ne", "brennt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein Nahm und Creutz, seit dem ich dich erkennt.", "tokens": ["Dein", "Nahm", "und", "Creutz", ",", "seit", "dem", "ich", "dich", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich, mit allem meinem Flei\u00df,", "tokens": ["Wenn", "ich", ",", "mit", "al\u00b7lem", "mei\u00b7nem", "Flei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir nimmermehr zu rathen wei\u00df,", "tokens": ["Mir", "nim\u00b7mer\u00b7mehr", "zu", "ra\u00b7then", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Ohnmacht Unverstand", "tokens": ["Und", "mei\u00b7ne", "Ohn\u00b7macht", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schwachheit kr\u00e4fftiglich erkandt;", "tokens": ["Und", "Schwach\u00b7heit", "kr\u00e4ff\u00b7tig\u00b7lich", "er\u00b7kandt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bist du ja der unerforschte Mann,", "tokens": ["So", "bist", "du", "ja", "der", "un\u00b7er\u00b7forschte", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der allen meinen Sachen rathen kan.", "tokens": ["Der", "al\u00b7len", "mei\u00b7nen", "Sa\u00b7chen", "ra\u00b7then", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Fehlt mirs an aller Lebens-Krafft,", "tokens": ["Fehlt", "mirs", "an", "al\u00b7ler", "Le\u00b7bens\u00b7Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat meine Rebe keinen Safft,", "tokens": ["Hat", "mei\u00b7ne", "Re\u00b7be", "kei\u00b7nen", "Safft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sincke ich vor Mattigkeit", "tokens": ["Und", "sin\u00b7cke", "ich", "vor", "Mat\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey nahe hin zu mancher Zeit;", "tokens": ["Bey", "na\u00b7he", "hin", "zu", "man\u00b7cher", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist dein kr\u00e4fftiges Gef\u00fchl in mir,", "tokens": ["So", "ist", "dein", "kr\u00e4ff\u00b7ti\u00b7ges", "Ge\u00b7f\u00fchl", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das h\u00e4lt mir starcke Helden-Kr\u00e4ffte f\u00fcr.", "tokens": ["Das", "h\u00e4lt", "mir", "star\u00b7cke", "Hel\u00b7den\u00b7Kr\u00e4ff\u00b7te", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn ich im schweren Glaubens-Kampf", "tokens": ["Wenn", "ich", "im", "schwe\u00b7ren", "Glau\u00b7bens\u00b7Kampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch manchen dicken Rauch und Dampf,", "tokens": ["Durch", "man\u00b7chen", "di\u00b7cken", "Rauch", "und", "Dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch manche Leibs- und Geists-Gefahr,", "tokens": ["Durch", "man\u00b7che", "Leibs", "und", "Geist\u00b7s\u00b7Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "TRUNC", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mich drenge zu der Sieges-Schaar;", "tokens": ["Mich", "dren\u00b7ge", "zu", "der", "Sie\u00b7ges\u00b7Schaar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bist dus, unbezwungner Wunder-Held,", "tokens": ["So", "bist", "dus", ",", "un\u00b7be\u00b7zwung\u00b7ner", "Wun\u00b7der\u00b7Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der meinetwegen alle Feinde f\u00e4llt.", "tokens": ["Der", "mei\u00b7net\u00b7we\u00b7gen", "al\u00b7le", "Fein\u00b7de", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn sich mein Senff-Korns-Glaube regt,", "tokens": ["Wenn", "sich", "mein", "Senf\u00b7fKorns\u00b7Glau\u00b7be", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kindlich dir zu F\u00fcssen legt,", "tokens": ["Und", "kind\u00b7lich", "dir", "zu", "F\u00fcs\u00b7sen", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mag der Feinde Hohn-Geschrey", "tokens": ["So", "mag", "der", "Fein\u00b7de", "Hohn\u00b7Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erth\u00f6nen: da\u00df ich th\u00f6rig sey.", "tokens": ["Er\u00b7th\u00f6\u00b7nen", ":", "da\u00df", "ich", "th\u00f6\u00b7rig", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}