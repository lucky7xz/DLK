{"textgrid.poem.57395": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aufgeschwungen hattest du einst hoch \u00fcber der Menschheit", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufgeschwungen hattest du einst hoch \u00fcber der Menschheit", "tokens": ["Auf\u00b7ge\u00b7schwun\u00b7gen", "hat\u00b7test", "du", "einst", "hoch", "\u00fc\u00b7ber", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gr\u00f6sse dich, Gallien, hattest, dem Krieg' entsagend, der Erde", "tokens": ["Gr\u00f6s\u00b7se", "dich", ",", "Gal\u00b7li\u00b7en", ",", "hat\u00b7test", ",", "dem", "Krieg'", "ent\u00b7sa\u00b7gend", ",", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "NE", "$,", "VAFIN", "$,", "ART", "NN", "VVPP", "$,", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "V\u00f6lker alle besiegt, des Alterthums, und die sp\u00e4tern,", "tokens": ["V\u00f6l\u00b7ker", "al\u00b7le", "be\u00b7siegt", ",", "des", "Al\u00b7ter\u00b7thums", ",", "und", "die", "sp\u00e4\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "VVPP", "$,", "ART", "NN", "$,", "KON", "ART", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Durch das melodische, himlische Wort.", "tokens": ["Durch", "das", "me\u00b7lo\u00b7di\u00b7sche", ",", "him\u00b7li\u00b7sche", "Wort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie war das noch gewesen; und ward! Doch des hehren Gesetzes", "tokens": ["Nie", "war", "das", "noch", "ge\u00b7we\u00b7sen", ";", "und", "ward", "!", "Doch", "des", "heh\u00b7ren", "Ge\u00b7set\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDS", "ADV", "VAPP", "$.", "KON", "VAFIN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Tafeln hast du zerschlagen, zerschlagen! deckender Staub macht", "tokens": ["Ta\u00b7feln", "hast", "du", "zer\u00b7schla\u00b7gen", ",", "zer\u00b7schla\u00b7gen", "!", "de\u00b7cken\u00b7der", "Staub", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,", "VVPP", "$.", "ART", "NN", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Selbst die Tr\u00fcmmern unkenbar! und du, (Jetzt mag ich nicht nennen", "tokens": ["Selbst", "die", "Tr\u00fcm\u00b7mern", "un\u00b7ken\u00b7bar", "!", "und", "du", ",", "(", "Jetzt", "mag", "ich", "nicht", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD", "$.", "KON", "PPER", "$,", "$(", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.4": {"text": "Deinen Namen) du liegest nun da,", "tokens": ["Dei\u00b7nen", "Na\u00b7men", ")", "du", "lie\u00b7gest", "nun", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie von der H\u00f6h du dich st\u00fcrzetest, wie die Maale der Schmach dich", "tokens": ["Wie", "von", "der", "H\u00f6h", "du", "dich", "st\u00fcr\u00b7ze\u00b7test", ",", "wie", "die", "Maa\u00b7le", "der", "Schmach", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "PRF", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "PPER"], "meter": "---+--+--+-+-+++", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "R\u00f6theten, der vor allen: Was jetzo du w\u00e4rst! was Europa's", "tokens": ["R\u00f6\u00b7the\u00b7ten", ",", "der", "vor", "al\u00b7len", ":", "Was", "jet\u00b7zo", "du", "w\u00e4rst", "!", "was", "Eu\u00b7ro\u00b7pa's"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PIAT", "$.", "PWS", "ADV", "PPER", "VAFIN", "$.", "PWS", "NE"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "V\u00f6lker w\u00e4ren; h\u00e4ttest du Hochverrath nicht begangen", "tokens": ["V\u00f6l\u00b7ker", "w\u00e4\u00b7ren", ";", "h\u00e4t\u00b7test", "du", "Hoch\u00b7ver\u00b7rath", "nicht", "be\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "VAFIN", "PPER", "NN", "PTKNEG", "VVPP"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Gegen das heilverk\u00fcndende Wort!", "tokens": ["Ge\u00b7gen", "das", "heil\u00b7ver\u00b7k\u00fcn\u00b7den\u00b7de", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Wer den h\u00f6chsten Gipfel erstieg, (O darf sich auch wenden", "tokens": ["Wer", "den", "h\u00f6chs\u00b7ten", "Gip\u00b7fel", "er\u00b7stieg", ",", "(", "O", "darf", "sich", "auch", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "$(", "NE", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "--+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "An die Nazion die Stimme des einzelnen? Freude", "tokens": ["An", "die", "Na\u00b7zi\u00b7on", "die", "Stim\u00b7me", "des", "ein\u00b7zel\u00b7nen", "?", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "$.", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "War's mir, mein neues Vaterland, dass du B\u00fcrger mich nantest;", "tokens": ["Wa\u00b7r's", "mir", ",", "mein", "neu\u00b7es", "Va\u00b7ter\u00b7land", ",", "dass", "du", "B\u00fcr\u00b7ger", "mich", "nan\u00b7test", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wonne, h\u00f6rst du mich, wird es mir seyn!)", "tokens": ["Won\u00b7ne", ",", "h\u00f6rst", "du", "mich", ",", "wird", "es", "mir", "seyn", "!", ")"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$,", "VAFIN", "PPER", "PPER", "VAINF", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer den h\u00f6chsten Gipfel erstieg, ist zu stolz auf dem zweyten", "tokens": ["Wer", "den", "h\u00f6chs\u00b7ten", "Gip\u00b7fel", "er\u00b7stieg", ",", "ist", "zu", "stolz", "auf", "dem", "zwey\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "VAFIN", "PTKA", "ADJD", "APPR", "ART", "ADJA"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich zu zeigen: und doch w\u00e4r's Nachweltruhm, wenn zu diesem", "tokens": ["Sich", "zu", "zei\u00b7gen", ":", "und", "doch", "w\u00e4r's", "Nach\u00b7welt\u00b7ruhm", ",", "wenn", "zu", "die\u00b7sem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "$.", "KON", "ADV", "VAFIN", "NN", "$,", "KOUS", "APPR", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Du dich erh\u00fcbst, da der erste dir nun unersteiglich ist. Edel", "tokens": ["Du", "dich", "er\u00b7h\u00fcbst", ",", "da", "der", "ers\u00b7te", "dir", "nun", "un\u00b7er\u00b7steig\u00b7lich", "ist", ".", "E\u00b7del"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PRF", "VVFIN", "$,", "KOUS", "ART", "ADJA", "PPER", "ADV", "ADJD", "VAFIN", "$.", "NN"], "meter": "-+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "W\u00e4r nicht der hier noch zweifelnde Stolz.", "tokens": ["W\u00e4r", "nicht", "der", "hier", "noch", "zwei\u00b7feln\u00b7de", "Stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Hab' ich Wahrheit geredet; so ist's thatfodernde Wahrheit,", "tokens": ["Hab'", "ich", "Wahr\u00b7heit", "ge\u00b7re\u00b7det", ";", "so", "ist's", "that\u00b7fo\u00b7dern\u00b7de", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "VVPP", "$.", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und so schwingest du, Gallien, dich auf den zweyten der Gipfel:", "tokens": ["Und", "so", "schwin\u00b7gest", "du", ",", "Gal\u00b7li\u00b7en", ",", "dich", "auf", "den", "zwey\u00b7ten", "der", "Gip\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "NE", "$,", "PRF", "APPR", "ART", "ADJA", "ART", "NN", "$."], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "H\u00e4ltst von dem Bunde des Friedens an, von dem frohesten Tage", "tokens": ["H\u00e4ltst", "von", "dem", "Bun\u00b7de", "des", "Frie\u00b7dens", "an", ",", "von", "dem", "fro\u00b7hes\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Unsers Jahrhunderts, was einst du versprachst.", "tokens": ["Un\u00b7sers", "Jahr\u00b7hun\u00b7derts", ",", "was", "einst", "du", "ver\u00b7sprachst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Kom, o Vergessung dann de Erduldeten, l\u00f6sche die Bilder", "tokens": ["Kom", ",", "o", "Ver\u00b7ges\u00b7sung", "dann", "de", "E\u00b7rdul\u00b7de\u00b7ten", ",", "l\u00f6\u00b7sche", "die", "Bil\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "FM", "NN", "ADV", "NE", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Jenes Jammers aus, den allgegenw\u00e4rtig das bleiche", "tokens": ["Je\u00b7nes", "Jam\u00b7mers", "aus", ",", "den", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "das", "blei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Scheusal, das thierische, graus' ergoss, das w\u00fcrfelnde, kalte", "tokens": ["Scheu\u00b7sal", ",", "das", "thie\u00b7ri\u00b7sche", ",", "graus'", "er\u00b7goss", ",", "das", "w\u00fcr\u00b7feln\u00b7de", ",", "kal\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "FM.fr", "FM.fr", "$,", "PDS", "VAFIN", "$,", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Scheusal, der Menschheit Schande, der Krieg,", "tokens": ["Scheu\u00b7sal", ",", "der", "Menschheit", "Schan\u00b7de", ",", "der", "Krieg", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Ha die sch\u00e4ndlichere, seit seine Tilgung verheissen", "tokens": ["Ha", "die", "sch\u00e4nd\u00b7li\u00b7che\u00b7re", ",", "seit", "sei\u00b7ne", "Til\u00b7gung", "ver\u00b7heis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "$,", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ward, und gehoft! Lass heller, Vergessung, der Weinenden Auge", "tokens": ["Ward", ",", "und", "ge\u00b7hoft", "!", "Lass", "hel\u00b7ler", ",", "Ver\u00b7ges\u00b7sung", ",", "der", "Wei\u00b7nen\u00b7den", "Au\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "KON", "VVPP", "$.", "NE", "ADJD", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Werden, es l\u00e4nger nicht bluten; lass um den todten Geliebten", "tokens": ["Wer\u00b7den", ",", "es", "l\u00e4n\u00b7ger", "nicht", "blu\u00b7ten", ";", "lass", "um", "den", "tod\u00b7ten", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PPER", "ADJD", "PTKNEG", "VVINF", "$.", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "L\u00e4nger vor Wehmuth nicht jammern die Braut;", "tokens": ["L\u00e4n\u00b7ger", "vor", "Weh\u00b7muth", "nicht", "jam\u00b7mern", "die", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "L\u00e4nder um den entrissenen Freund den liebenden Freund nicht", "tokens": ["L\u00e4n\u00b7der", "um", "den", "ent\u00b7ris\u00b7se\u00b7nen", "Freund", "den", "lie\u00b7ben\u00b7den", "Freund", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Klagen, ihn gehn, und Blumen ihn auf des ruhenden Grab streun,", "tokens": ["Kla\u00b7gen", ",", "ihn", "gehn", ",", "und", "Blu\u00b7men", "ihn", "auf", "des", "ru\u00b7hen\u00b7den", "Grab", "streun", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVINF", "$,", "KON", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Welches, von der Zipresse beschattet, dem Wanderer winket,", "tokens": ["Wel\u00b7ches", ",", "von", "der", "Zip\u00b7res\u00b7se", "be\u00b7schat\u00b7tet", ",", "dem", "Wan\u00b7de\u00b7rer", "win\u00b7ket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPR", "ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Oder von weinenden Weiden gek\u00fchlt.", "tokens": ["O\u00b7der", "von", "wei\u00b7nen\u00b7den", "Wei\u00b7den", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbha was tr\u00f6stest du, sprichst von der Ruhe dess, den ich liebte!", "tokens": ["\u00bb", "ha", "was", "tr\u00f6s\u00b7test", "du", ",", "sprichst", "von", "der", "Ru\u00b7he", "dess", ",", "den", "ich", "lieb\u00b7te", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "PWS", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und von der Weide, die um ihn weinet. Er ist mir verschwunden!", "tokens": ["Und", "von", "der", "Wei\u00b7de", ",", "die", "um", "ihn", "wei\u00b7net", ".", "Er", "ist", "mir", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "---+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ist mir verschwunden! mir bringet ihn nicht, die Zipresse, die mitklagt,", "tokens": ["Ist", "mir", "ver\u00b7schwun\u00b7den", "!", "mir", "brin\u00b7get", "ihn", "nicht", ",", "die", "Zip\u00b7res\u00b7se", ",", "die", "mit\u00b7klagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+--+--+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Nicht die Thr\u00e4ne der Weide zur\u00fcck.", "tokens": ["Nicht", "die", "Thr\u00e4\u00b7ne", "der", "Wei\u00b7de", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.11": {"line.1": {"text": "Helden, Helden! wie gross seyd ihr! Wer giebt mir der sch\u00f6nsten", "tokens": ["Hel\u00b7den", ",", "Hel\u00b7den", "!", "wie", "gross", "seyd", "ihr", "!", "Wer", "giebt", "mir", "der", "sch\u00f6ns\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sprosse genug, dass ich geh, und Lorberw\u00e4lder euch pflanze!", "tokens": ["Spros\u00b7se", "ge\u00b7nug", ",", "dass", "ich", "geh", ",", "und", "Lor\u00b7ber\u00b7w\u00e4l\u00b7der", "euch", "pflan\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Aber auch, verzeiht! von den Wolfsgesichtern darunter,", "tokens": ["A\u00b7ber", "auch", ",", "ver\u00b7zeiht", "!", "von", "den", "Wolfs\u00b7ge\u00b7sich\u00b7tern", "da\u00b7run\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "$.", "APPR", "ART", "NN", "PAV", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und von den L\u00f6wenz\u00e4hnen, verzeiht!\u00ab", "tokens": ["Und", "von", "den", "L\u00f6\u00b7wen\u00b7z\u00e4h\u00b7nen", ",", "ver\u00b7zeiht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Aufgeschwungen hattest du einst hoch \u00fcber der Menschheit", "tokens": ["Auf\u00b7ge\u00b7schwun\u00b7gen", "hat\u00b7test", "du", "einst", "hoch", "\u00fc\u00b7ber", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gr\u00f6sse dich, Gallien, hattest, dem Krieg' entsagend, der Erde", "tokens": ["Gr\u00f6s\u00b7se", "dich", ",", "Gal\u00b7li\u00b7en", ",", "hat\u00b7test", ",", "dem", "Krieg'", "ent\u00b7sa\u00b7gend", ",", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "NE", "$,", "VAFIN", "$,", "ART", "NN", "VVPP", "$,", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "V\u00f6lker alle besiegt, des Alterthums, und die sp\u00e4tern,", "tokens": ["V\u00f6l\u00b7ker", "al\u00b7le", "be\u00b7siegt", ",", "des", "Al\u00b7ter\u00b7thums", ",", "und", "die", "sp\u00e4\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "VVPP", "$,", "ART", "NN", "$,", "KON", "ART", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Durch das melodische, himlische Wort.", "tokens": ["Durch", "das", "me\u00b7lo\u00b7di\u00b7sche", ",", "him\u00b7li\u00b7sche", "Wort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.13": {"line.1": {"text": "Nie war das noch gewesen; und ward! Doch des hehren Gesetzes", "tokens": ["Nie", "war", "das", "noch", "ge\u00b7we\u00b7sen", ";", "und", "ward", "!", "Doch", "des", "heh\u00b7ren", "Ge\u00b7set\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDS", "ADV", "VAPP", "$.", "KON", "VAFIN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Tafeln hast du zerschlagen, zerschlagen! deckender Staub macht", "tokens": ["Ta\u00b7feln", "hast", "du", "zer\u00b7schla\u00b7gen", ",", "zer\u00b7schla\u00b7gen", "!", "de\u00b7cken\u00b7der", "Staub", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$,", "VVPP", "$.", "ART", "NN", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Selbst die Tr\u00fcmmern unkenbar! und du, (Jetzt mag ich nicht nennen", "tokens": ["Selbst", "die", "Tr\u00fcm\u00b7mern", "un\u00b7ken\u00b7bar", "!", "und", "du", ",", "(", "Jetzt", "mag", "ich", "nicht", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD", "$.", "KON", "PPER", "$,", "$(", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.4": {"text": "Deinen Namen) du liegest nun da,", "tokens": ["Dei\u00b7nen", "Na\u00b7men", ")", "du", "lie\u00b7gest", "nun", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Wie von der H\u00f6h du dich st\u00fcrzetest, wie die Maale der Schmach dich", "tokens": ["Wie", "von", "der", "H\u00f6h", "du", "dich", "st\u00fcr\u00b7ze\u00b7test", ",", "wie", "die", "Maa\u00b7le", "der", "Schmach", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "PRF", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "PPER"], "meter": "---+--+--+-+-+++", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "R\u00f6theten, der vor allen: Was jetzo du w\u00e4rst! was Europa's", "tokens": ["R\u00f6\u00b7the\u00b7ten", ",", "der", "vor", "al\u00b7len", ":", "Was", "jet\u00b7zo", "du", "w\u00e4rst", "!", "was", "Eu\u00b7ro\u00b7pa's"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PIAT", "$.", "PWS", "ADV", "PPER", "VAFIN", "$.", "PWS", "NE"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "V\u00f6lker w\u00e4ren; h\u00e4ttest du Hochverrath nicht begangen", "tokens": ["V\u00f6l\u00b7ker", "w\u00e4\u00b7ren", ";", "h\u00e4t\u00b7test", "du", "Hoch\u00b7ver\u00b7rath", "nicht", "be\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "VAFIN", "PPER", "NN", "PTKNEG", "VVPP"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Gegen das heilverk\u00fcndende Wort!", "tokens": ["Ge\u00b7gen", "das", "heil\u00b7ver\u00b7k\u00fcn\u00b7den\u00b7de", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.15": {"line.1": {"text": "Wer den h\u00f6chsten Gipfel erstieg, (O darf sich auch wenden", "tokens": ["Wer", "den", "h\u00f6chs\u00b7ten", "Gip\u00b7fel", "er\u00b7stieg", ",", "(", "O", "darf", "sich", "auch", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "$(", "NE", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "--+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "An die Nazion die Stimme des einzelnen? Freude", "tokens": ["An", "die", "Na\u00b7zi\u00b7on", "die", "Stim\u00b7me", "des", "ein\u00b7zel\u00b7nen", "?", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "$.", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "War's mir, mein neues Vaterland, dass du B\u00fcrger mich nantest;", "tokens": ["Wa\u00b7r's", "mir", ",", "mein", "neu\u00b7es", "Va\u00b7ter\u00b7land", ",", "dass", "du", "B\u00fcr\u00b7ger", "mich", "nan\u00b7test", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wonne, h\u00f6rst du mich, wird es mir seyn!)", "tokens": ["Won\u00b7ne", ",", "h\u00f6rst", "du", "mich", ",", "wird", "es", "mir", "seyn", "!", ")"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$,", "VAFIN", "PPER", "PPER", "VAINF", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Wer den h\u00f6chsten Gipfel erstieg, ist zu stolz auf dem zweyten", "tokens": ["Wer", "den", "h\u00f6chs\u00b7ten", "Gip\u00b7fel", "er\u00b7stieg", ",", "ist", "zu", "stolz", "auf", "dem", "zwey\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "VAFIN", "PTKA", "ADJD", "APPR", "ART", "ADJA"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich zu zeigen: und doch w\u00e4r's Nachweltruhm, wenn zu diesem", "tokens": ["Sich", "zu", "zei\u00b7gen", ":", "und", "doch", "w\u00e4r's", "Nach\u00b7welt\u00b7ruhm", ",", "wenn", "zu", "die\u00b7sem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "$.", "KON", "ADV", "VAFIN", "NN", "$,", "KOUS", "APPR", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Du dich erh\u00fcbst, da der erste dir nun unersteiglich ist. Edel", "tokens": ["Du", "dich", "er\u00b7h\u00fcbst", ",", "da", "der", "ers\u00b7te", "dir", "nun", "un\u00b7er\u00b7steig\u00b7lich", "ist", ".", "E\u00b7del"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PRF", "VVFIN", "$,", "KOUS", "ART", "ADJA", "PPER", "ADV", "ADJD", "VAFIN", "$.", "NN"], "meter": "-+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "W\u00e4r nicht der hier noch zweifelnde Stolz.", "tokens": ["W\u00e4r", "nicht", "der", "hier", "noch", "zwei\u00b7feln\u00b7de", "Stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}, "stanza.17": {"line.1": {"text": "Hab' ich Wahrheit geredet; so ist's thatfodernde Wahrheit,", "tokens": ["Hab'", "ich", "Wahr\u00b7heit", "ge\u00b7re\u00b7det", ";", "so", "ist's", "that\u00b7fo\u00b7dern\u00b7de", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "VVPP", "$.", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und so schwingest du, Gallien, dich auf den zweyten der Gipfel:", "tokens": ["Und", "so", "schwin\u00b7gest", "du", ",", "Gal\u00b7li\u00b7en", ",", "dich", "auf", "den", "zwey\u00b7ten", "der", "Gip\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "NE", "$,", "PRF", "APPR", "ART", "ADJA", "ART", "NN", "$."], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "H\u00e4ltst von dem Bunde des Friedens an, von dem frohesten Tage", "tokens": ["H\u00e4ltst", "von", "dem", "Bun\u00b7de", "des", "Frie\u00b7dens", "an", ",", "von", "dem", "fro\u00b7hes\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Unsers Jahrhunderts, was einst 