{"dta.poem.4122": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "8.  Entschuldigung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Recht wie Bursch' in Saus und Braus,", "tokens": ["Recht", "wie", "Bur\u00b7sch'", "in", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schauen Augen, blau' und graue,", "tokens": ["Schau\u00b7en", "Au\u00b7gen", ",", "blau'", "und", "grau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKVZ", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarz' und braun' aus manchem Haus.", "tokens": ["Schwa\u00b7rz'", "und", "braun'", "aus", "man\u00b7chem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich lass' die Blicke schweifen", "tokens": ["Und", "ich", "lass'", "die", "Bli\u00b7cke", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Fenster hin und her,", "tokens": ["Durch", "die", "Fens\u00b7ter", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast als wollt' ich Eine suchen,", "tokens": ["Fast", "als", "wollt'", "ich", "Ei\u00b7ne", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "VMFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mir die Allerliebste w\u00e4r'.", "tokens": ["Die", "mir", "die", "Al\u00b7ler\u00b7liebs\u00b7te", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch wei\u00df ich, da\u00df die Eine", "tokens": ["Und", "doch", "wei\u00df", "ich", ",", "da\u00df", "die", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohnt viel Meilen weit von mir,", "tokens": ["Wohnt", "viel", "Mei\u00b7len", "weit", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch kann ich's Schaun nicht lassen", "tokens": ["Und", "doch", "kann", "ich's", "Schaun", "nicht", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVFIN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den schmucken Jungfern hier.", "tokens": ["Nach", "den", "schmu\u00b7cken", "Jung\u00b7fern", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Liebchen, woll' dich nicht betr\u00fcben,", "tokens": ["Lieb\u00b7chen", ",", "woll'", "dich", "nicht", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dir Eins die Kunde bringt,", "tokens": ["Wenn", "dir", "Eins", "die", "Kun\u00b7de", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da\u00df dich's nicht \u00fcberrasche,", "tokens": ["Und", "da\u00df", "dich's", "nicht", "\u00fc\u00b7berr\u00b7a\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Lied der Wandrer singt.", "tokens": ["Die\u00b7ses", "Lied", "der", "Wand\u00b7rer", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}