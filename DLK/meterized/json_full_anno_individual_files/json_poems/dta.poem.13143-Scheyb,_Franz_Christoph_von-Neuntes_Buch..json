{"dta.poem.13143": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eein grauer Nebel-Dunst hem\u0303t oft der Sonne Strahlen;", "tokens": ["\u201e", "ein", "grau\u00b7er", "Ne\u00b7bel\u00b7Dunst", "hem\u0303t", "oft", "der", "Son\u00b7ne", "Strah\u00b7len", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso pflegt die Welt mich auch mit Wolcken zu bemahlen.", "tokens": ["\u201e", "so", "pflegt", "die", "Welt", "mich", "auch", "mit", "Wol\u00b7cken", "zu", "be\u00b7mah\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "165\u201dDer Sonne scharffer Blick verdringet und durchbricht", "tokens": ["\"", "Der", "Son\u00b7ne", "scharf\u00b7fer", "Blick", "ver\u00b7drin\u00b7get", "und", "durch\u00b7bricht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie vorgezogne Wand; ihr heiteres Gesicht", "tokens": ["\u201e", "die", "vor\u00b7ge\u00b7zog\u00b7ne", "Wand", ";", "ihr", "hei\u00b7te\u00b7res", "Ge\u00b7sicht"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201esteigt ungeschw\u00e4cht empor; so bleib\u2019 ich unverdunckelt.", "tokens": ["\u201e", "steigt", "un\u00b7ge\u00b7schw\u00e4cht", "em\u00b7por", ";", "so", "bleib'", "ich", "un\u00b7ver\u00b7dun\u00b7ckelt", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eso wird die Finsternis von mir auch durchgefunckelt;", "tokens": ["\u201e", "so", "wird", "die", "Fins\u00b7ter\u00b7nis", "von", "mir", "auch", "durch\u00b7ge\u00b7fun\u00b7ckelt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ebi\u00df sie sich unvermerckt fr\u00fch oder sp\u00e4t verschleicht,", "tokens": ["\u201e", "bi\u00df", "sie", "sich", "un\u00b7ver\u00b7merckt", "fr\u00fch", "o\u00b7der", "sp\u00e4t", "ver\u00b7schleicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "ADJD", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "170\u201dUnd meine Wesenheit der hellen Sonne gleicht.", "tokens": ["\"", "Und", "mei\u00b7ne", "We\u00b7sen\u00b7heit", "der", "hel\u00b7len", "Son\u00b7ne", "gleicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eso wirckt der Schatten nichts, der meinen Glanz beflecket;", "tokens": ["\u201e", "so", "wirckt", "der", "Schat\u00b7ten", "nichts", ",", "der", "mei\u00b7nen", "Glanz", "be\u00b7fle\u00b7cket", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edann endlich bin ich doch wie reines Gold entdecket.", "tokens": ["\u201e", "dann", "end\u00b7lich", "bin", "ich", "doch", "wie", "rei\u00b7nes", "Gold", "ent\u00b7de\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VAFIN", "PPER", "ADV", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edurch meiner Tugend Licht, durch meine Gegenwart", "tokens": ["\u201e", "durch", "mei\u00b7ner", "Tu\u00b7gend", "Licht", ",", "durch", "mei\u00b7ne", "Ge\u00b7gen\u00b7wart"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ewird das Verborgenste der Welt geoffenbart.", "tokens": ["\u201e", "wird", "das", "Ver\u00b7bor\u00b7gens\u00b7te", "der", "Welt", "ge\u00b7of\u00b7fen\u00b7bart", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "175\u201dMithin sag' ich beherzt, frey, deutlich, ungeschmincket,", "tokens": ["\"", "Mi\u00b7thin", "sag'", "ich", "be\u00b7herzt", ",", "frey", ",", "deut\u00b7lich", ",", "un\u00b7ge\u00b7schmin\u00b7cket", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ewas mich von euerm Frie\u00df und von dem Streit bed\u00fcncket.", "tokens": ["\u201e", "was", "mich", "von", "eu\u00b7erm", "Frie\u00df", "und", "von", "dem", "Streit", "be\u00b7d\u00fcn\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "APPR", "PPOSAT", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}