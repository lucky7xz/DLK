{"dta.poem.13070": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso seyd ihr insgesamt des Vorschlags \u00fcberzeugt,", "tokens": ["\u201e", "so", "seyd", "ihr", "ins\u00b7ge\u00b7samt", "des", "Vor\u00b7schlags", "\u00fc\u00b7berz\u00b7eugt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ezu wessen Wircklichkeit sich meine Rede neigt.", "tokens": ["\u201e", "zu", "wes\u00b7sen", "Wir\u00b7ck\u00b7lich\u00b7keit", "sich", "mei\u00b7ne", "Re\u00b7de", "neigt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKZU", "VVINF", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u201ewas hei\u00dft es, selber uns mit Helden-S\u00e4ulen ehren?", "tokens": ["\u201e", "was", "hei\u00dft", "es", ",", "sel\u00b7ber", "uns", "mit", "Hel\u00b7den\u00b7S\u00e4u\u00b7len", "eh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "ADV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso gar uns um den Rang in einen Streit emp\u00f6ren?", "tokens": ["\u201e", "so", "gar", "uns", "um", "den", "Rang", "in", "ei\u00b7nen", "Streit", "em\u00b7p\u00f6\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "685\u201dSie beyde seynd schon eins, best\u00e4ndig eins zu seyn,", "tokens": ["\"", "Sie", "bey\u00b7de", "seynd", "schon", "eins", ",", "be\u00b7st\u00e4n\u00b7dig", "eins", "zu", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PIS", "VAFIN", "ADV", "PIS", "$,", "ADJD", "PIS", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ethron, Zepter, Kron und Ruhm besizen sie gemein;", "tokens": ["\u201e", "thron", ",", "Zep\u00b7ter", ",", "Kron", "und", "Ruhm", "be\u00b7si\u00b7zen", "sie", "ge\u00b7mein", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso d\u00e4rffen wir nicht mehr in solchem Zweifel stehen,", "tokens": ["\u201e", "so", "d\u00e4rf\u00b7fen", "wir", "nicht", "mehr", "in", "sol\u00b7chem", "Zwei\u00b7fel", "ste\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201egenug: ", "tokens": ["\u201e", "ge\u00b7nug", ":"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "\u201ees bleibt bey meinem Saz: er ist die sch\u00f6nste Wahl,", "tokens": ["\u201e", "es", "bleibt", "bey", "mei\u00b7nem", "Saz", ":", "er", "ist", "die", "sch\u00f6ns\u00b7te", "Wahl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "690\u201dWir weihen ihm allein des Frieses Ehren-Mahl!", "tokens": ["\"", "Wir", "wei\u00b7hen", "ihm", "al\u00b7lein", "des", "Frie\u00b7ses", "Eh\u00b7ren\u00b7Mahl", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}