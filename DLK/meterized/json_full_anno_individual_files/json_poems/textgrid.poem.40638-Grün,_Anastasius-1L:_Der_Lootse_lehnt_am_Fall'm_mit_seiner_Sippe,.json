{"textgrid.poem.40638": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Lootse lehnt am Fall'm mit seiner Sippe,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lootse lehnt am Fall'm mit seiner Sippe,", "tokens": ["Der", "Loot\u00b7se", "lehnt", "am", "Fall'm", "mit", "sei\u00b7ner", "Sip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Theergewand, nicht regend Arm' und Beine,", "tokens": ["Im", "Theer\u00b7ge\u00b7wand", ",", "nicht", "re\u00b7gend", "Arm'", "und", "Bei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKNEG", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So fahl und starr wie Stein von diesem Steine,", "tokens": ["So", "fahl", "und", "starr", "wie", "Stein", "von", "die\u00b7sem", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur wachen Blicks, doch redescheuer Lippe.", "tokens": ["Nur", "wa\u00b7chen", "Blicks", ",", "doch", "re\u00b7de\u00b7scheu\u00b7er", "Lip\u00b7pe", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So liegt der Robbe wohl auf fahler Klippe", "tokens": ["So", "liegt", "der", "Rob\u00b7be", "wohl", "auf", "fah\u00b7ler", "Klip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit klugen Aeuglein tr\u00e4g im Sonnenscheine,", "tokens": ["Mit", "klu\u00b7gen", "A\u00b7e\u00b7u\u00b7glein", "tr\u00e4g", "im", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lautlos und unbeweglich, da\u00df man meine,", "tokens": ["Laut\u00b7los", "und", "un\u00b7be\u00b7weg\u00b7lich", ",", "da\u00df", "man", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er sei ein St\u00fcck nur dieser Felsenrippe.", "tokens": ["Er", "sei", "ein", "St\u00fcck", "nur", "die\u00b7ser", "Fel\u00b7sen\u00b7rip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da rauscht der Sturm und l\u00f6st ihn aus dem Banne!", "tokens": ["Da", "rauscht", "der", "Sturm", "und", "l\u00f6st", "ihn", "aus", "dem", "Ban\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht entzaubernd \u2013 wie in alten M\u00e4hren", "tokens": ["Viel\u00b7leicht", "ent\u00b7zau\u00b7bernd", "\u2013", "w