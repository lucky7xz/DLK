{"dta.poem.18320": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Schwarz in Schwarz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt streich ich's dick mit Rothstift an:", "tokens": ["Jetzt", "streich", "ich's", "dick", "mit", "Roth\u00b7stift", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der bibelgeborne Christengott", "tokens": ["Der", "bi\u00b7bel\u00b7ge\u00b7bor\u00b7ne", "Chris\u00b7ten\u00b7gott"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ist nie und nimmermehr mein Mann!", "tokens": ["Ist", "nie", "und", "nim\u00b7mer\u00b7mehr", "mein", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sch\u00f6pfung war einst sein erster Witz", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "war", "einst", "sein", "ers\u00b7ter", "Witz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dieser Witz war herzlich schlecht,", "tokens": ["Und", "die\u00b7ser", "Witz", "war", "herz\u00b7lich", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn oft schon traf es mich wie ein Blitz:", "tokens": ["Denn", "oft", "schon", "traf", "es", "mich", "wie", "ein", "Blitz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "PRF", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Despotie hat leider Recht!", "tokens": ["Die", "Des\u00b7po\u00b7tie", "hat", "lei\u00b7der", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Volk, das heut nicht auf Pr\u00fcgel h\u00f6rt,", "tokens": ["Ein", "Volk", ",", "das", "heut", "nicht", "auf", "Pr\u00fc\u00b7gel", "h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und eine Unschuld beim Ballett,", "tokens": ["Und", "ei\u00b7ne", "Un\u00b7schuld", "beim", "Bal\u00b7lett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein solches Erz-Ph\u00e4nomen geh\u00f6rt", "tokens": ["Ein", "sol\u00b7ches", "Er\u00b7zPh\u00e4\u00b7no\u00b7men", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ins Naturalienkabinett!", "tokens": ["Ins", "Na\u00b7tu\u00b7ra\u00b7li\u00b7en\u00b7ka\u00b7bi\u00b7nett", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}