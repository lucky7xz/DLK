{"textgrid.poem.57543": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Ballade", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Her kam er gefahren auf rollendem Rad,", "tokens": ["Her", "kam", "er", "ge\u00b7fah\u00b7ren", "auf", "rol\u00b7len\u00b7dem", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie war er so stattlich und sch\u00f6n,", "tokens": ["Wie", "war", "er", "so", "statt\u00b7lich", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Fort ist er gefahren auf rollendem Rad,", "tokens": ["Fort", "ist", "er", "ge\u00b7fah\u00b7ren", "auf", "rol\u00b7len\u00b7dem", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich hab' ihn nie wieder gesehn.", "tokens": ["Ich", "hab'", "ihn", "nie", "wie\u00b7der", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Luft war so warm und der Himmel so klar,", "tokens": ["Die", "Luft", "war", "so", "warm", "und", "der", "Him\u00b7mel", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den Weg durch die Felder ich ging,", "tokens": ["Den", "Weg", "durch", "die", "Fel\u00b7der", "ich", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich stand damals grade im sechzehnten Jahr", "tokens": ["Ich", "stand", "da\u00b7mals", "gra\u00b7de", "im", "sech\u00b7zehn\u00b7ten", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und war ein recht lustiges Ding;", "tokens": ["Und", "war", "ein", "recht", "lus\u00b7ti\u00b7ges", "Ding", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Noch war nicht die Liebe im Herzen erwacht,", "tokens": ["Noch", "war", "nicht", "die", "Lie\u00b7be", "im", "Her\u00b7zen", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die Liebe, die selig und unselig macht,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "se\u00b7lig", "und", "un\u00b7se\u00b7lig", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Herzbl\u00fcten zeitigt und Herzbl\u00fcten knickt \u2013", "tokens": ["Die", "Herz\u00b7bl\u00fc\u00b7ten", "zei\u00b7tigt", "und", "Herz\u00b7bl\u00fc\u00b7ten", "knickt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$("], "meter": "-++-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Ach h\u00e4tt' ich ihn niemals erblickt!", "tokens": ["Ach", "h\u00e4tt'", "ich", "ihn", "nie\u00b7mals", "er\u00b7blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Grad als ich den Seitenweg einschlagen wollt',", "tokens": ["Grad", "als", "ich", "den", "Sei\u00b7ten\u00b7weg", "ein\u00b7schla\u00b7gen", "wollt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da klirrte es hinter mir laut,", "tokens": ["Da", "klirr\u00b7te", "es", "hin\u00b7ter", "mir", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da kam er auf blitzendem Zweirad gerollt", "tokens": ["Da", "kam", "er", "auf", "blit\u00b7zen\u00b7dem", "Zwei\u00b7rad", "ge\u00b7rollt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und gr\u00fc\u00dfte so freundlich und traut;", "tokens": ["Und", "gr\u00fc\u00df\u00b7te", "so", "freund\u00b7lich", "und", "traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Er sprang aus dem Sattel, ging neben mir her", "tokens": ["Er", "sprang", "aus", "dem", "Sat\u00b7tel", ",", "ging", "ne\u00b7ben", "mir", "her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "PPER", "APZR"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und fragte nach Wasser; ihn d\u00fcrste so sehr, \u2013", "tokens": ["Und", "frag\u00b7te", "nach", "Was\u00b7ser", ";", "ihn", "d\u00fcrs\u00b7te", "so", "sehr", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "PPER", "VMFIN", "ADV", "ADV", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ich sagte, ein Spring w\u00e4re ganz in der N\u00e4h', \u2013", "tokens": ["Ich", "sag\u00b7te", ",", "ein", "Spring", "w\u00e4\u00b7re", "ganz", "in", "der", "N\u00e4h'", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "O Quell, dir entrauschte mein Weh.", "tokens": ["O", "Quell", ",", "dir", "ent\u00b7rauschte", "mein", "Weh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Am Springe, wo Kresse und Otterwurz bl\u00fcht,", "tokens": ["Am", "Sprin\u00b7ge", ",", "wo", "Kres\u00b7se", "und", "Ot\u00b7ter\u00b7wurz", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "KON", "NE", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da war es so duftig und k\u00fchl,", "tokens": ["Da", "war", "es", "so", "duf\u00b7tig", "und", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch in meinem Herzen da hat es gegl\u00fcht,", "tokens": ["Doch", "in", "mei\u00b7nem", "Her\u00b7zen", "da", "hat", "es", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mir war so beklommen und schw\u00fcl.", "tokens": ["Mir", "war", "so", "be\u00b7klom\u00b7men", "und", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wo bist du, o Stunde, zerronnen so schnell", "tokens": ["Wo", "bist", "du", ",", "o", "Stun\u00b7de", ",", "zer\u00b7ron\u00b7nen", "so", "schnell"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wie sprudelndes Wasser von murmelndem Quell,", "tokens": ["Wie", "spru\u00b7deln\u00b7des", "Was\u00b7ser", "von", "mur\u00b7meln\u00b7dem", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Du Stunde voll Liebe, voll Lust und voll Gl\u00fcck,", "tokens": ["Du", "Stun\u00b7de", "voll", "Lie\u00b7be", ",", "voll", "Lust", "und", "voll", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "$,", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Ach, kehre noch einmal zur\u00fcck!", "tokens": ["Ach", ",", "keh\u00b7re", "noch", "ein\u00b7mal", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O bittere Wonne, er zog mich ans Herz", "tokens": ["O", "bit\u00b7te\u00b7re", "Won\u00b7ne", ",", "er", "zog", "mich", "ans", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sagte: \u00bbLieb M\u00e4dchen, ade!\u00ab", "tokens": ["Und", "sag\u00b7te", ":", "\u00bb", "Lieb", "M\u00e4d\u00b7chen", ",", "a\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "NN", "$,", "ADV", "$.", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ich glaube, er lachte \u2013 er sah nicht den Schmerz,", "tokens": ["Ich", "glau\u00b7be", ",", "er", "lach\u00b7te", "\u2013", "er", "sah", "nicht", "den", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er sah nicht das schneidende Weh.", "tokens": ["Er", "sah", "nicht", "das", "schnei\u00b7den\u00b7de", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Bis hinten zum Walde noch sah ich ihm nach,", "tokens": ["Bis", "hin\u00b7ten", "zum", "Wal\u00b7de", "noch", "sah", "ich", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Dann warf ich mich neben den rieselnden Bach:", "tokens": ["Dann", "warf", "ich", "mich", "ne\u00b7ben", "den", "rie\u00b7seln\u00b7den", "Bach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Vor\u00fcber, vor\u00fcber, vorbei, ach vorbei,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7bei", ",", "ach", "vor\u00b7bei", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PTKVZ", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da wu\u00dft' ich, was Liebhaben sei!", "tokens": ["Da", "wu\u00dft'", "ich", ",", "was", "Lieb\u00b7ha\u00b7ben", "sei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Her kam er gefahren auf rollendem Rad,", "tokens": ["Her", "kam", "er", "ge\u00b7fah\u