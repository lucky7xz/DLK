{"dta.poem.19347": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wolfeiler kanst du nicht den Fordernden abspeisen,", "tokens": ["Wol\u00b7fei\u00b7ler", "kanst", "du", "nicht", "den", "For\u00b7dern\u00b7den", "ab\u00b7spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+--++-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Als ihm, da\u00df er schon was er fordert hat, beweisen.", "tokens": ["Als", "ihm", ",", "da\u00df", "er", "schon", "was", "er", "for\u00b7dert", "hat", ",", "be\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADV", "PWS", "PPER", "VVPP", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In Ruh genie\u00dfest du den Ueberflu\u00df der Gaben,", "tokens": ["In", "Ruh", "ge\u00b7nie\u00b7\u00dfest", "du", "den", "Ue\u00b7berf\u00b7lu\u00df", "der", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du uns glauben machst, da\u00df wir die F\u00fclle haben. \u2014", "tokens": ["Wenn", "du", "uns", "glau\u00b7ben", "machst", ",", "da\u00df", "wir", "die", "F\u00fcl\u00b7le", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "\u201ewas fechten Niedere der H\u00f6hern Vorrecht an?", "tokens": ["\u201e", "was", "fech\u00b7ten", "Nie\u00b7de\u00b7re", "der", "H\u00f6\u00b7hern", "Vor\u00b7recht", "an", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJA", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein eigen Vorrecht hat auch der gemeine Mann.", "tokens": ["Sein", "ei\u00b7gen", "Vor\u00b7recht", "hat", "auch", "der", "ge\u00b7mei\u00b7ne", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "\u201ehat nicht der Bettelmann den Vorzug vor dem Reichen?", "tokens": ["\u201e", "hat", "nicht", "der", "Bet\u00b7tel\u00b7mann", "den", "Vor\u00b7zug", "vor", "dem", "Rei\u00b7chen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er nimmt Almosen an, und dieser mu\u00df es reichen.", "tokens": ["Er", "nimmt", "Al\u00b7mo\u00b7sen", "an", ",", "und", "die\u00b7ser", "mu\u00df", "es", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "KON", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u201edu hast, was er dir gab, den Reichen hat die Habe;", "tokens": ["\u201e", "du", "hast", ",", "was", "er", "dir", "gab", ",", "den", "Rei\u00b7chen", "hat", "die", "Ha\u00b7be", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es geht kein Herrscherstab vor deinem Bettelstabe.", "tokens": ["Es", "geht", "kein", "Herr\u00b7scher\u00b7stab", "vor", "dei\u00b7nem", "Bet\u00b7tel\u00b7sta\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "\u201edir stihlt, weil er ist leer, kein Dieb den Bettelsack;", "tokens": ["\u201e", "dir", "stihlt", ",", "weil", "er", "ist", "leer", ",", "kein", "Dieb", "den", "Bet\u00b7tel\u00b7sack", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "$,", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leicht trag ihn, und entbehr den schweren Sorgenpack.", "tokens": ["Leicht", "trag", "ihn", ",", "und", "ent\u00b7behr", "den", "schwe\u00b7ren", "Sor\u00b7gen\u00b7pack", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "\u201eschwer h\u00e4lt dem ird'schen Sinn des Irdischen Entschlagung;", "tokens": ["\u201e", "schwer", "h\u00e4lt", "dem", "ird'\u00b7schen", "Sinn", "des", "Ir\u00b7di\u00b7schen", "Ent\u00b7schla\u00b7gung", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leicht f\u00e4llt der Hauptgewinn des Lebens dir, Entsagung.\u201c \u2014", "tokens": ["Leicht", "f\u00e4llt", "der", "Haupt\u00b7ge\u00b7winn", "des", "Le\u00b7bens", "dir", ",", "Ent\u00b7sa\u00b7gung", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "$,", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ein lust'ger Bettler mag so tr\u00f6sten seinen Sohn,", "tokens": ["Ein", "lust'\u00b7ger", "Bett\u00b7ler", "mag", "so", "tr\u00f6s\u00b7ten", "sei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch in des Reichen Mund klingt dieser Trost wie Hohn.", "tokens": ["Doch", "in", "des", "Rei\u00b7chen", "Mund", "klingt", "die\u00b7ser", "Trost", "wie", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PDAT", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}