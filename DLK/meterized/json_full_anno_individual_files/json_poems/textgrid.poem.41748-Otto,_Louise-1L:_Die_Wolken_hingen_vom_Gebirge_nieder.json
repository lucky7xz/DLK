{"textgrid.poem.41748": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Wolken hingen vom Gebirge nieder", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken hingen vom Gebirge nieder", "tokens": ["Die", "Wol\u00b7ken", "hin\u00b7gen", "vom", "Ge\u00b7bir\u00b7ge", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gespenstig ziehend um den finstern Wald,", "tokens": ["Ge\u00b7spens\u00b7tig", "zie\u00b7hend", "um", "den", "fins\u00b7tern", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dampfende Nebel dehnten Riesenglieder", "tokens": ["Damp\u00b7fen\u00b7de", "Ne\u00b7bel", "dehn\u00b7ten", "Rie\u00b7sen\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "In grau und schwarz mit seltsamer Gestalt;", "tokens": ["In", "grau", "und", "schwarz", "mit", "selt\u00b7sa\u00b7mer", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch hob sich draus auf waldumkr\u00e4nzter H\u00f6he", "tokens": ["Doch", "hob", "sich", "draus", "auf", "wald\u00b7um\u00b7kr\u00e4nz\u00b7ter", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PAV", "APPR", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Die alte Veste stolz und k\u00fchn hervor,", "tokens": ["Die", "al\u00b7te", "Ves\u00b7te", "stolz", "und", "k\u00fchn", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df sie die Wolken sich zu F\u00fc\u00dfen sehe", "tokens": ["Da\u00df", "sie", "die", "Wol\u00b7ken", "sich", "zu", "F\u00fc\u00b7\u00dfen", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als Weihrauch sie des Nebels Ziehn erkor.", "tokens": ["Als", "Weih\u00b7rauch", "sie", "des", "Ne\u00b7bels", "Ziehn", "er\u00b7kor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und durch die Nebel schritt ich ihr entgegen", "tokens": ["Und", "durch", "die", "Ne\u00b7bel", "schritt", "ich", "ihr", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und durch die Wolken eilte ich ihr zu", "tokens": ["Und", "durch", "die", "Wol\u00b7ken", "eil\u00b7te", "ich", "ihr", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf feuchten moosbedeckten Waldeswegen", "tokens": ["Auf", "feuch\u00b7ten", "moos\u00b7be\u00b7deck\u00b7ten", "Wal\u00b7des\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu des Gebirges stiller Totenruh.", "tokens": ["Zu", "des", "Ge\u00b7bir\u00b7ges", "stil\u00b7ler", "To\u00b7ten\u00b7ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald klomm ich zu des Kynast h\u00f6chstem Walle", "tokens": ["Bald", "klomm", "ich", "zu", "des", "Ky\u00b7nast", "h\u00f6chs\u00b7tem", "Wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und lie\u00df die Blicke schweifen in die Runde \u2013", "tokens": ["Und", "lie\u00df", "die", "Bli\u00b7cke", "schwei\u00b7fen", "in", "die", "Run\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da fuhr ich auf von eines Seufzers Schalle", "tokens": ["Da", "fuhr", "ich", "auf", "von", "ei\u00b7nes", "Seuf\u00b7zers", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und vor mir stand sie \u2013 ", "tokens": ["Und", "vor", "mir", "stand", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "\u00bbviel Ritter kamen einst um mich zu werben,", "tokens": ["\u00bb", "viel", "Rit\u00b7ter", "ka\u00b7men", "einst", "um", "mich", "zu", "wer\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil meine Sch\u00f6nheit, weil mein Gold sie zog;", "tokens": ["Weil", "mei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "weil", "mein", "Gold", "sie", "zog", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber wollt als freie Jungfrau sterben,", "tokens": ["Ich", "a\u00b7ber", "wollt", "als", "frei\u00b7e", "Jung\u00b7frau", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn nicht die Lieb mir mehr als Freiheit wog", "tokens": ["Wenn", "nicht", "die", "Lieb", "mir", "mehr", "als", "Frei\u00b7heit", "wog"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "ADV", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drum sann ich, mich der Werber zu entschlagen,", "tokens": ["Drum", "sann", "ich", ",", "mich", "der", "Wer\u00b7ber", "zu", "ent\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein listiges ein finstres Mittel aus \u2013", "tokens": ["Ein", "lis\u00b7ti\u00b7ges", "ein", "finst\u00b7res", "Mit\u00b7tel", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein Leben dacht ich w\u00fcrde keiner wagen,", "tokens": ["Sein", "Le\u00b7ben", "dacht", "ich", "w\u00fcr\u00b7de", "kei\u00b7ner", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr mich nicht wagen einen blut'gen Strau\u00df.", "tokens": ["F\u00fcr", "mich", "nicht", "wa\u00b7gen", "ei\u00b7nen", "blut'\u00b7gen", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "VVINF", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch kamen sie um Ruhm sich zu erringen,", "tokens": ["Doch", "ka\u00b7men", "sie", "um", "Ruhm", "sich", "zu", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Ritt zu wagen um des Walles Ring.", "tokens": ["Den", "Ritt", "zu", "wa\u00b7gen", "um", "des", "Wal\u00b7les", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch konnte keinem je die That gelingen", "tokens": ["Doch", "konn\u00b7te", "kei\u00b7nem", "je", "die", "That", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und einer nach dem andern unterging.", "tokens": ["Und", "ei\u00b7ner", "nach", "dem", "an\u00b7dern", "un\u00b7ter\u00b7ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da kam der eine, der mein Herz bezwungen,", "tokens": ["Da", "kam", "der", "ei\u00b7ne", ",", "der", "mein", "Herz", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df es f\u00fcr ihn in hei\u00dfer Liebe schlug", "tokens": ["Da\u00df", "es", "f\u00fcr", "ihn", "in", "hei\u00b7\u00dfer", "Lie\u00b7be", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich rief und hielt sein Knie ihm fest umschlungen", "tokens": ["Ich", "rief", "und", "hielt", "sein", "Knie", "ihm", "fest", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u203ahier meine Hand \u2013 Halt ein! es ist genug!\u2039", "tokens": ["\u203a", "hier", "mei\u00b7ne", "Hand", "\u2013", "Halt", "ein", "!", "es", "ist", "ge\u00b7nug", "!", "\u2039"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "$(", "VVIMP", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er aber stie\u00df mich fort und sprengt zum Rande", "tokens": ["Er", "a\u00b7ber", "stie\u00df", "mich", "fort", "und", "sprengt", "zum", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihm gelang der unheilvolle Ritt \u2013", "tokens": ["Und", "ihm", "ge\u00b7lang", "der", "un\u00b7heil\u00b7vol\u00b7le", "Ritt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann h\u00f6hnt er mich, \u203aDas that ich dir zur Schande,", "tokens": ["Dann", "h\u00f6hnt", "er", "mich", ",", "\u203a", "Das", "that", "ich", "dir", "zur", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "$(", "PDS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Rache jedem, der hier Tod erlitt!\u2039", "tokens": ["Zur", "Ra\u00b7che", "je\u00b7dem", ",", "der", "hier", "Tod", "er\u00b7litt", "!", "\u2039"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PIAT", "$,", "PRELS", "ADV", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Zorne sch\u00f6n noch wie ein Rachegott,", "tokens": ["Im", "Zor\u00b7ne", "sch\u00f6n", "noch", "wie", "ein", "Ra\u00b7che\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So sprach er es mit heldenstolzen Trieben \u2013", "tokens": ["So", "sprach", "er", "es", "mit", "hel\u00b7den\u00b7stol\u00b7zen", "Trie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da trug ich still der Andern Hohn und Spott,", "tokens": ["Da", "trug", "ich", "still", "der", "An\u00b7dern", "Hohn", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch trug ich nimmer das verratne Lieben!", "tokens": ["Doch", "trug", "ich", "nim\u00b7mer", "das", "ver\u00b7rat\u00b7ne", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wo der andren Ritter Leichen lagen,", "tokens": ["Und", "wo", "der", "an\u00b7dren", "Rit\u00b7ter", "Lei\u00b7chen", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da eilt ich selber mir das Grab zu betten \u2013", "tokens": ["Da", "eilt", "ich", "sel\u00b7ber", "mir", "das", "Grab", "zu", "bet\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun mu\u00df ich n\u00e4chtlich umgehn noch und klagen", "tokens": ["Nun", "mu\u00df", "ich", "n\u00e4cht\u00b7lich", "um\u00b7gehn", "noch", "und", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Fl\u00fcche h\u00f6ren an den \u00f6den St\u00e4tten;", "tokens": ["Und", "Fl\u00fc\u00b7che", "h\u00f6\u00b7ren", "an", "den", "\u00f6\u00b7den", "St\u00e4t\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und war es doch mein einziges Verbrechen,", "tokens": ["Und", "war", "es", "doch", "mein", "ein\u00b7zi\u00b7ges", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nicht ohne Lieb zur Sklavin mich zu machen! \u2013", "tokens": ["Nicht", "oh\u00b7ne", "Lieb", "zur", "Skla\u00b7vin", "mich", "zu", "ma\u00b7chen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPR", "NN", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das wollten nur die stolzen M\u00e4nner r\u00e4chen,", "tokens": ["Das", "woll\u00b7ten", "nur", "die", "stol\u00b7zen", "M\u00e4n\u00b7ner", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das ist's, was sie noch heut an mir verlachen!\u00ab", "tokens": ["Das", "ist's", ",", "was", "sie", "noch", "heut", "an", "mir", "ver\u00b7la\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das ist's rief ich, das wird noch heut beschworen \u2013", "tokens": ["Das", "ist's", "rief", "ich", ",", "das", "wird", "noch", "heut", "be\u00b7schwo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sind ja nichts \u2013 sie sind die Herrn der Welt.", "tokens": ["Wir", "sind", "ja", "nichts", "\u2013", "sie", "sind", "die", "Herrn", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "$(", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wird das Weib zur Sklavin nur geboren.", "tokens": ["Es", "wird", "das", "Weib", "zur", "Skla\u00b7vin", "nur", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So hei\u00dft der Spruch, das Urteil ist gef\u00e4llt.", "tokens": ["So", "hei\u00dft", "der", "Spruch", ",", "das", "Ur\u00b7teil", "ist", "ge\u00b7f\u00e4llt", "."], "token_info