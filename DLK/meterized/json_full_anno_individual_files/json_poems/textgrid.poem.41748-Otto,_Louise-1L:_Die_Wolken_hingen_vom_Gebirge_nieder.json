"token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und frei nach einem andern Ziele strebt,", "tokens": ["Und", "frei", "nach", "ei\u00b7nem", "an\u00b7dern", "Zie\u00b7le", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An einem andern Altar zu geloben", "tokens": ["An", "ei\u00b7nem", "an\u00b7dern", "Al\u00b7tar", "zu", "ge\u00b7lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein h\u00f6hres F\u00fchlen, das sein Herz durchbebt.", "tokens": ["Ein", "h\u00f6h\u00b7res", "F\u00fch\u00b7len", ",", "das", "sein", "Herz", "durch\u00b7bebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und weh dem Weibe, das mit festen Schritten", "tokens": ["Und", "weh", "dem", "Wei\u00b7be", ",", "das", "mit", "fes\u00b7ten", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich ob der Knechtschaft Schranken stolz erhebt \u2013", "tokens": ["Sich", "ob", "der", "Knecht\u00b7schaft", "Schran\u00b7ken", "stolz", "er\u00b7hebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "ART", "NN", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}, "stanza.9": {"line.1": {"text": "Die Wolken hingen vom Gebirge nieder", "tokens": ["Die", "Wol\u00b7ken", "hin\u00b7gen", "vom", "Ge\u00b7bir\u00b7ge", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gespenstig ziehend um den finstern Wald,", "tokens": ["Ge\u00b7spens\u00b7tig", "zie\u00b7hend", "um", "den", "fins\u00b7tern", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dampfende Nebel dehnten Riesenglieder", "tokens": ["Damp\u00b7fen\u00b7de", "Ne\u00b7bel", "dehn\u00b7ten", "Rie\u00b7sen\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "In grau und schwarz mit seltsamer Gestalt;", "tokens": ["In", "grau", "und", "schwarz", "mit", "selt\u00b7sa\u00b7mer", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch hob sich draus auf waldumkr\u00e4nzter H\u00f6he", "tokens": ["Doch", "hob", "sich", "draus", "auf", "wald\u00b7um\u00b7kr\u00e4nz\u00b7ter", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PAV", "APPR", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Die alte Veste stolz und k\u00fchn hervor,", "tokens": ["Die", "al\u00b7te", "Ves\u00b7te", "stolz", "und", "k\u00fchn", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df sie die Wolken sich zu F\u00fc\u00dfen sehe", "tokens": ["Da\u00df", "sie", "die", "Wol\u00b7ken", "sich", "zu", "F\u00fc\u00b7\u00dfen", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als Weihrauch sie des Nebels Ziehn erkor.", "tokens": ["Als", "Weih\u00b7rauch", "sie", "des", "Ne\u00b7bels", "Ziehn", "er\u00b7kor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und durch die Nebel schritt ich ihr entgegen", "tokens": ["Und", "durch", "die", "Ne\u00b7bel", "schritt", "ich", "ihr", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und durch die Wolken eilte ich ihr zu", "tokens": ["Und", "durch", "die", "Wol\u00b7ken", "eil\u00b7te", "ich", "ihr", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf feuchten moosbedeckten Waldeswegen", "tokens": ["Auf", "feuch\u00b7ten", "moos\u00b7be