{"dta.poem.11369": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen, welcher sich nach hof und in  \n f\u00fcrstliche dienste begeben  \n solte.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh hin, gepriesner mann! wohin des gl\u00fcckes huld", "tokens": ["Geh", "hin", ",", "ge\u00b7pri\u00b7es\u00b7ner", "mann", "!", "wo\u00b7hin", "des", "gl\u00fc\u00b7ckes", "huld"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ADJA", "NN", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dich heist aus deinem lager r\u00fccken!", "tokens": ["Dich", "heist", "aus", "dei\u00b7nem", "la\u00b7ger", "r\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bequem dich aber vor zur h\u00f6flichen gedult,", "tokens": ["Be\u00b7quem", "dich", "a\u00b7ber", "vor", "zur", "h\u00f6f\u00b7li\u00b7chen", "ge\u00b7dult", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und lern dich in das g\u00fcldne joch und in die s\u00fcssen schmertzen", "tokens": ["Und", "lern", "dich", "in", "das", "g\u00fcld\u00b7ne", "joch", "und", "in", "die", "s\u00fcs\u00b7sen", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "schicken!", "tokens": ["schi\u00b7cken", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Der hof ist eine see;", "tokens": ["Der", "hof", "ist", "ei\u00b7ne", "see", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df deiner hoffnung schiff nicht weit vom ufer schweben!", "tokens": ["La\u00df", "dei\u00b7ner", "hoff\u00b7nung", "schiff", "nicht", "weit", "vom", "u\u00b7fer", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer stillen tieffen traut, und auf des meeres h\u00f6h", "tokens": ["Wer", "stil\u00b7len", "tief\u00b7fen", "traut", ",", "und", "auf", "des", "mee\u00b7res", "h\u00f6h"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADJD", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sich will mit gantzem wind und vollen segeln heben,", "tokens": ["Sich", "will", "mit", "gant\u00b7zem", "wind", "und", "vol\u00b7len", "se\u00b7geln", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "APPR", "ADJA", "NN", "KON", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00e4llt durch verleumdung offt in harte klippen ein,", "tokens": ["F\u00e4llt", "durch", "ver\u00b7leum\u00b7dung", "offt", "in", "har\u00b7te", "klip\u00b7pen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "APPR", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und st\u00f6sset an des neiders stein", "tokens": ["Und", "st\u00f6s\u00b7set", "an", "des", "nei\u00b7ders", "stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit schrecken seinen kahn in st\u00fccken.", "tokens": ["Mit", "schre\u00b7cken", "sei\u00b7nen", "kahn", "in", "st\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Dann schwimmt der gantze bau zu deiner schande fort,", "tokens": ["Dann", "schwimmt", "der", "gant\u00b7ze", "bau", "zu", "dei\u00b7ner", "schan\u00b7de", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und jeder lacht dich aus, da\u00df du von deinem port", "tokens": ["Und", "je\u00b7der", "lacht", "dich", "aus", ",", "da\u00df", "du", "von", "dei\u00b7nem", "port"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So th\u00f6richt abgeschifft: Der mast-baum liegt darnieder,", "tokens": ["So", "th\u00f6\u00b7richt", "ab\u00b7ge\u00b7schifft", ":", "Der", "mast\u00b7baum", "liegt", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und kommet selten mehr zu seinem herren wieder.", "tokens": ["Und", "kom\u00b7met", "sel\u00b7ten", "mehr", "zu", "sei\u00b7nem", "her\u00b7ren", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bleht dich die ruhmsucht auf? so setz ihr maas und ziel!", "tokens": ["Bleht", "dich", "die", "ruhm\u00b7sucht", "auf", "?", "so", "setz", "ihr", "maas", "und", "ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die ehre pflegt ein feiges hertz zu hassen,", "tokens": ["Die", "eh\u00b7re", "pflegt", "ein", "fei\u00b7ges", "hertz", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ein stoltzes zu verlassen.", "tokens": ["Ein", "stolt\u00b7zes", "zu", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Sey keinem andern hart; Dich strafe stets zuviel!", "tokens": ["Sey", "kei\u00b7nem", "an\u00b7dern", "hart", ";", "Dich", "stra\u00b7fe", "stets", "zu\u00b7viel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bem\u00fch dich nicht, die stirne zu verstellen,", "tokens": ["Be\u00b7m\u00fch", "dich", "nicht", ",", "die", "stir\u00b7ne", "zu", "ver\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und la\u00df dein augenlied von keiner hoffarth schwellen!", "tokens": ["Und", "la\u00df", "dein", "au\u00b7gen\u00b7lied", "von", "kei\u00b7ner", "hof\u00b7farth", "schwel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "APPR", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das gl\u00fcck ist wandelbar, und \u00e4ndert leicht den schein:", "tokens": ["Das", "gl\u00fcck", "ist", "wan\u00b7del\u00b7bar", ",", "und", "\u00e4n\u00b7dert", "leicht", "den", "schein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drum sorge blo\u00df dahin, wie du m\u00f6gst ehrlich seyn.", "tokens": ["Drum", "sor\u00b7ge", "blo\u00df", "da\u00b7hin", ",", "wie", "du", "m\u00f6gst", "ehr\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PAV", "$,", "PWAV", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nicht w\u00fcnsche, da\u00df es dir stets fr\u00f6lich m\u00f6ge gehen!", "tokens": ["Nicht", "w\u00fcn\u00b7sche", ",", "da\u00df", "es", "dir", "stets", "fr\u00f6\u00b7lich", "m\u00f6\u00b7ge", "ge\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der purpur, den du tr\u00e4gst, stirbt und verliehret sich:", "tokens": ["Der", "pur\u00b7pur", ",", "den", "du", "tr\u00e4gst", ",", "stirbt", "und", "ver\u00b7lieh\u00b7ret", "sich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PRF", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Der stern, der itzund scheint, kan bald in wolcken stehen:", "tokens": ["Der", "stern", ",", "der", "it\u00b7zund", "scheint", ",", "kan", "bald", "in", "wol\u00b7cken", "ste\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "PRELS", "ADV", "VVFIN", "$,", "VMFIN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und wie ist zeit und jahr nicht selbst ver\u00e4nderlich?", "tokens": ["Und", "wie", "ist", "zeit", "und", "jahr", "nicht", "selbst", "ver\u00b7\u00e4n\u00b7der\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "NN", "KON", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der ist der gl\u00fccklichste, der keinem gl\u00fccke trauet;", "tokens": ["Der", "ist", "der", "gl\u00fcck\u00b7lichs\u00b7te", ",", "der", "kei\u00b7nem", "gl\u00fc\u00b7cke", "trau\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wer, wenn ihn ein jeder schauet,", "tokens": ["Und", "wer", ",", "wenn", "ihn", "ein", "je\u00b7der", "schau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "KOUS", "PPER", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Sich doch im finstern w\u00fcnscht, der ist ein weiser mann.", "tokens": ["Sich", "doch", "im", "fins\u00b7tern", "w\u00fcnscht", ",", "der", "ist", "ein", "wei\u00b7ser", "mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "VVINF", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wie? weist du es noch nicht?", "tokens": ["Wie", "?", "weist", "du", "es", "noch", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Des hofes treppen sind von porcellan und glase zugericht:", "tokens": ["Des", "ho\u00b7fes", "trep\u00b7pen", "sind", "von", "por\u00b7cel\u00b7lan", "und", "gla\u00b7se", "zu\u00b7ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "APPR", "NE", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.34": {"text": "Man steigt, indem man abwerts geht, man f\u00e4llt, indem man", "tokens": ["Man", "steigt", ",", "in\u00b7dem", "man", "ab\u00b7werts", "geht", ",", "man", "f\u00e4llt", ",", "in\u00b7dem", "man"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,", "PIS", "VVFIN", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "aufwerts steiget,", "tokens": ["auf\u00b7werts", "stei\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.36": {"text": "Und wird im augenblick offt bi\u00df in staub gebeuget.", "tokens": ["Und", "wird", "im", "au\u00b7gen\u00b7blick", "offt", "bi\u00df", "in", "staub", "ge\u00b7beu\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "ADV", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Verachte dich stets selbst! Jm andern traue GOtt!", "tokens": ["Ver\u00b7ach\u00b7te", "dich", "stets", "selbst", "!", "Jm", "an\u00b7dern", "trau\u00b7e", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Vergebne sorge bringt dem hertzen nichts als plagen,", "tokens": ["Ver\u00b7geb\u00b7ne", "sor\u00b7ge", "bringt", "dem", "hert\u00b7zen", "nichts", "als", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PIS", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und unsre jammer-klagen", "tokens": ["Und", "uns\u00b7re", "jam\u00b7mer\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Vermehren nur die noth.", "tokens": ["Ver\u00b7meh\u00b7ren", "nur", "die", "noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Zeigt dir der himmel blitz? So leide seine schl\u00e4ge!", "tokens": ["Zeigt", "dir", "der", "him\u00b7mel", "blitz", "?", "So", "lei\u00b7de", "sei\u00b7ne", "schl\u00e4\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Lockt dich die wollust an? So weich ihr aus dem wege!", "tokens": ["Lockt", "dich", "die", "wol\u00b7lust", "an", "?", "So", "weich", "ihr", "aus", "dem", "we\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "ADV", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Liebkoset dir das gl\u00fcck? So setze dich zur ruh!", "tokens": ["Lieb\u00b7ko\u00b7set", "dir", "das", "gl\u00fcck", "?", "So", "set\u00b7ze", "dich", "zur", "ruh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Dr\u00e4ut dir ein schwacher feind? So lache nur darzu!", "tokens": ["Dr\u00e4ut", "dir", "ein", "schwa\u00b7cher", "feind", "?", "So", "la\u00b7che", "nur", "dar\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Bist du beliebt? So halt die gunst mit h\u00e4nd und f\u00fcssen!", "tokens": ["Bist", "du", "be\u00b7liebt", "?", "So", "halt", "die", "gunst", "mit", "h\u00e4nd", "und", "f\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Liegst du? So kanst du ja den m\u00fc\u00dfiggang genie\u00dfen:", "tokens": ["Liegst", "du", "?", "So", "kanst", "du", "ja", "den", "m\u00fc\u00b7\u00dfig\u00b7gang", "ge\u00b7nie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Steigst du? So la\u00df niemahls so grosse freude sehn!", "tokens": ["Steigst", "du", "?", "So", "la\u00df", "nie\u00b7mahls", "so", "gros\u00b7se", "freu\u00b7de", "sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VVIMP", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wirst du erniedriget? Wohl! la\u00df es auch geschehn!", "tokens": ["Wirst", "du", "er\u00b7nied\u00b7ri\u00b7get", "?", "Wohl", "!", "la\u00df", "es", "auch", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$.", "ADV", "$.", "VVIMP", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Trotzt dein verfolger dich? So zwinge dein gesicht!", "tokens": ["Trotzt", "dein", "ver\u00b7fol\u00b7ger", "dich", "?", "So", "zwin\u00b7ge", "dein", "ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "PPER", "$.", "ADV", "VVFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Thut es dir weh? Ey so beschwer dich nicht!", "tokens": ["Thut", "es", "dir", "weh", "?", "Ey", "so", "be\u00b7schwer", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$.", "NN", "ADV", "ADJD", "PPER", "PTKNEG", "$."], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}, "line.51": {"text": "Mehr wei\u00df ich dir nicht zu befehlen;", "tokens": ["Mehr", "wei\u00df", "ich", "dir", "nicht", "zu", "be\u00b7feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Durch tugend wirst du stehn:", "tokens": ["Durch", "tu\u00b7gend", "wirst", "du", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Die tugend kan allein erh\u00f6hn:", "tokens": ["Die", "tu\u00b7gend", "kan", "al\u00b7lein", "er\u00b7h\u00f6hn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Die tugend wird dich auch zu den begl\u00fcckten zehlen.", "tokens": ["Die", "tu\u00b7gend", "wird", "dich", "auch", "zu", "den", "be\u00b7gl\u00fcck\u00b7ten", "zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}