{"textgrid.poem.53862": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Opposition! Opposition!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt gehts aber los! Jetzt werden wir was erleben!", "tokens": ["Jetzt", "gehts", "a\u00b7ber", "los", "!", "Jetzt", "wer\u00b7den", "wir", "was", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jetzt wird sich eine Opposition erheben:", "tokens": ["Jetzt", "wird", "sich", "ei\u00b7ne", "Op\u00b7po\u00b7si\u00b7ti\u00b7on", "er\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da werden die M\u00e4uler aufgerissen!", "tokens": ["Da", "wer\u00b7den", "die", "M\u00e4u\u00b7ler", "auf\u00b7ge\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schl\u00e4gt das nationale Gewissen,", "tokens": ["Da", "schl\u00e4gt", "das", "na\u00b7ti\u00b7o\u00b7na\u00b7le", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "da sch\u00fctteln sich F\u00e4uste im ganzen Land,", "tokens": ["da", "sch\u00fct\u00b7teln", "sich", "F\u00e4us\u00b7te", "im", "gan\u00b7zen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "gegen Hindenburg! Da wackelt die Wand.", "tokens": ["ge\u00b7gen", "Hin\u00b7den\u00b7burg", "!", "Da", "wa\u00b7ckelt", "die", "Wand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Jetzt ist alles freiheitlich und sozial . . .", "tokens": ["Jetzt", "ist", "al\u00b7les", "frei\u00b7heit\u00b7lich", "und", "so\u00b7zi\u00b7al", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$.", "$.", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Auf einmal \u2013?", "tokens": ["Auf", "ein\u00b7mal", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Auf einmal: Verteilung des Steuergewichts?", "tokens": ["Auf", "ein\u00b7mal", ":", "Ver\u00b7tei\u00b7lung", "des", "Steu\u00b7er\u00b7ge\u00b7wichts", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf einmal taugt der Ge\u00dfler nichts?", "tokens": ["Auf", "ein\u00b7mal", "taugt", "der", "Ge\u00df\u00b7ler", "nichts", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf einmal: Freiheit der Denker und Dichter?", "tokens": ["Auf", "ein\u00b7mal", ":", "Frei\u00b7heit", "der", "Den\u00b7ker", "und", "Dich\u00b7ter", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Auf einmal: die Schande der deutschen Richter?", "tokens": ["Auf", "ein\u00b7mal", ":", "die", "Schan\u00b7de", "der", "deut\u00b7schen", "Rich\u00b7ter", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Hohn, Satire und Ironie?", "tokens": ["Hohn", ",", "Sa\u00b7ti\u00b7re", "und", "I\u00b7ro\u00b7nie", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das war doch noch nie . . .", "tokens": ["Das", "war", "doch", "noch", "nie", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADV", "$.", "$.", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "\u00bbf\u00fcr die Freiheit der Schule! Der Republik ein Spalier!\u00ab", "tokens": ["\u00bb", "f\u00fcr", "die", "Frei\u00b7heit", "der", "Schu\u00b7le", "!", "Der", "Re\u00b7pub\u00b7lik", "ein", "Spa\u00b7lier", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+--+-++-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Ausgerechnet ihr.", "tokens": ["Aus\u00b7ge\u00b7rech\u00b7net", "ihr", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Im Kampf gegen die Milit\u00e4rschweinerein", "tokens": ["Im", "Kampf", "ge\u00b7gen", "die", "Mi\u00b7li\u00b7t\u00e4r\u00b7schwei\u00b7ne\u00b7rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "standen wir jahrelang ganz allein.", "tokens": ["stan\u00b7den", "wir", "jah\u00b7re\u00b7lang", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Da war keiner von euch zu sehn.", "tokens": ["Da", "war", "kei\u00b7ner", "von", "euch", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Wann sind denn die schlimmsten Dinge geschehn?", "tokens": ["Wann", "sind", "denn", "die", "schlimms\u00b7ten", "Din\u00b7ge", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Als ihr an der Macht wart. Mit euern Leuten.", "tokens": ["Als", "ihr", "an", "der", "Macht", "wart", ".", "Mit", "eu\u00b7ern", "Leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.14": {"text": "Das hat auf einmal nichts zu bedeuten.", "tokens": ["Das", "hat", "auf", "ein\u00b7mal", "nichts", "zu", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-----+--+-", "measure": "iambic.di.relaxed"}, "line.15": {"text": "Jetzt, wo es in euern Parteikram pa\u00dft,", "tokens": ["Jetzt", ",", "wo", "es", "in", "eu\u00b7ern", "Par\u00b7tei\u00b7kram", "pa\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "tut ihr, als ob ihr mit uns ha\u00dft,", "tokens": ["tut", "ihr", ",", "als", "ob", "ihr", "mit", "uns", "ha\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "was hassenswert zwischen Rhein und Weichsel.", "tokens": ["was", "has\u00b7sens\u00b7wert", "zwi\u00b7schen", "Rhein", "und", "Weich\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Ihr hieltet dem Karren acht Jahre die Deichsel.", "tokens": ["Ihr", "hiel\u00b7tet", "dem", "Kar\u00b7ren", "acht", "Jah\u00b7re", "die", "Deich\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "CARD", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.19": {"text": "Ihr habt erst erm\u00f6glicht, was heute geschehn.", "tokens": ["Ihr", "habt", "erst", "er\u00b7m\u00f6g\u00b7licht", ",", "was", "heu\u00b7te", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.20": {"text": "Ihr la\u00dft Kinder in diese Schulen gehn.", "tokens": ["Ihr", "la\u00dft", "Kin\u00b7der", "in", "die\u00b7se", "Schu\u00b7len", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Ihr habt Arbeiterblut vergossen.", "tokens": ["Ihr", "habt", "Ar\u00b7bei\u00b7ter\u00b7blut", "ver\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ihr habt auf alles, was frei war, geschossen.", "tokens": ["Ihr", "habt", "auf", "al\u00b7les", ",", "was", "frei", "war", ",", "ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Die sich da die Macht erschoben:", "tokens": ["Die", "sich", "da", "die", "Macht", "er\u00b7scho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "ihr habt sie erst in den Sattel gehoben;", "tokens": ["ihr", "habt", "sie", "erst", "in", "den", "Sat\u00b7tel", "ge\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "die 